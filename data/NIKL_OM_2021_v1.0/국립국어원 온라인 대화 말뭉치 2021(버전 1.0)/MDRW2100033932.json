        },
        {
          "id": "MDRW2100033932.1.3",
          "form": "어떤 계절 가장 좋아해?",
          "original_form": "어떤 계절 가장 좋아해?",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033932.1.4",
          "form": "나는 겨울",
          "original_form": "나는 겨울",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033932.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033932.1.6",
          "form": "나는 가을",
          "original_form": "나는 가을",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033932.1.7",
          "form": "시원해서 좋아",
          "original_form": "시원해서 좋아",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033932.1.8",
          "form": "가을도 좋지",
          "original_form": "가을도 좋지",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033932.1.9",
          "form": "겨울은 안 좋아해?",
          "original_form": "겨울은 안 좋아해?",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033932.1.10",
          "form": "겨울도 좋아",
          "original_form": "겨울도 좋아",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100033932.1.11",
          "form": "겨울 냄새 되게 좋아해",
          "original_form": "겨울 냄새 되게 좋아해",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100033932.1.12",
          "form": "더위나 추위 타?",
          "original_form": "더위나 추위 타?",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100033932.1.13",
          "form": "둘 다 타ㅠㅠ",
          "original_form": "둘 다 타ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100033932.1.14",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100033932.1.15",
          "form": "나는 더위 많이 타",
          "original_form": "나는 더위 많이 타",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100033932.1.16",
          "form": "이번 여름에 너무 힘들었어",
          "original_form": "이번 여름에 너무 힘들었어",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100033932.1.17",
          "form": "진짜 너무 더웠지ㅠㅠ",
          "original_form": "진짜 너무 더웠지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100033932.1.18",
          "form": "에어컨 없이 살 수가 없었어",
          "original_form": "에어컨 없이 살 수가 없었어",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100033932.1.19",
          "form": "나도 낮에는 거의 틀었던 거 같아",
