      "utterance": [
        {
          "id": "MDRW2100014768.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014768.1.2",
          "form": "저는 집순이라 공연은 잘...",
          "original_form": "저는 집순이라 공연은 잘...",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014768.1.3",
          "form": "혹시 공연 자주 다니시나요?",
          "original_form": "혹시 공연 자주 다니시나요?",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014768.1.4",
          "form": "코로나 전에는요",
          "original_form": "코로나 전에는요",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014768.1.5",
          "form": "심할때는 금토일 다간적도",
          "original_form": "심할때는 금토일 다간적도",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014768.1.6",
          "form": "와 어떤 공연 좋아하시나요?",
          "original_form": "와 어떤 공연 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014768.1.7",
          "form": "전 힙합공연 좋아해요",
          "original_form": "전 힙합공연 좋아해요",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014768.1.8",
          "form": "마지막 콘도 쌈디콘이예요 코시국에 갔다왔어요",
          "original_form": "마지막 콘도 쌈디콘이예요 코시국에 갔다왔어요",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014768.1.9",
          "form": "마스크쓰고 장갑끼고 좌석으로 봐야했지만요",
          "original_form": "마스크쓰고 장갑끼고 좌석으로 봐야했지만요",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014768.1.10",
          "form": "요샌 공연 거의 못하는 거 같던데 아쉬우시겠네요",
          "original_form": "요샌 공연 거의 못하는 거 같던데 아쉬우시겠네요",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014768.1.11",
          "form": "힙합이면 되게 반응이 열렬하던데요",
          "original_form": "힙합이면 되게 반응이 열렬하던데요",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014768.1.12",
          "form": "네 심지어 티켓까지 있는데 취소된 공연도 있어요",
          "original_form": "네 심지어 티켓까지 있는데 취소된 공연도 있어요",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014768.1.13",
          "form": "코시국이라 ㅜㅜ",
          "original_form": "코시국이라 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014768.1.14",
          "form": "유툽으로 하는건 뭔가 심심하구요",
          "original_form": "유툽으로 하는건 뭔가 심심하구요",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014768.1.15",
          "form": "아무래도 콘서트는 공연하는 사람이랑 듣는 사람이 같이 호응하는 게 묘미인 것 같은데 유튜브는 좀 그 맛이 없는 거 같긴 해요",
          "original_form": "아무래도 콘서트는 공연하는 사람이랑 듣는 사람이 같이 호응하는 게 묘미인 것 같은데 유튜브는 좀 그 맛이 없는 거 같긴 해요",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014768.1.16",
          "form": "넹 가서 호흡하고 자리싸움도 하고 해야하는데",
          "original_form": "넹 가서 호흡하고 자리싸움도 하고 해야하는데",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014768.1.17",
          "form": "스탠딩 콘서트 한번 갔다오면 2키로 빠져요",
          "original_form": "스탠딩 콘서트 한번 갔다오면 2키로 빠져요",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014768.1.18",
          "form": "금방 다시 찌지만",
          "original_form": "금방 다시 찌지만",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014768.1.19",
          "form": "대단하네요 2키로나 빠진다니 되게 힘든 일인 것 같아요 즐거운 일이긴 하지만요",
          "original_form": "대단하네요 2키로나 빠진다니 되게 힘든 일인 것 같아요 즐거운 일이긴 하지만요",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014768.1.20",
          "form": "티켓팅부터 그 재미를 다시 맛보고 싶어요",
          "original_form": "티켓팅부터 그 재미를 다시 맛보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014768.1.21",
          "form": "티켓팅이 그렇게 치열하다면서요",
          "original_form": "티켓팅이 그렇게 치열하다면서요",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014768.1.22",
          "form": "몇분만에 매진되고 그렇다던데요",
          "original_form": "몇분만에 매진되고 그렇다던데요",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014768.1.23",
          "form": "분도 아니죠 초컷이예요",
          "original_form": "분도 아니죠 초컷이예요",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014768.1.24",
          "form": "근데 그게 다 진짜 가려는 사람 반",
          "original_form": "근데 그게 다 진짜 가려는 사람 반",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014768.1.25",
          "form": "되잘이하려는 사람 반이예요",
          "original_form": "되잘이하려는 사람 반이예요",
          "speaker_id": "1",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014768.1.26",
          "form": "되팔이요",
          "original_form": "되팔이요",
          "speaker_id": "1",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014768.1.27",
          "form": "혹시 티켓팅하시던 실력으로 꼬뜨게랑 가방을 겟하신 건가요?",
          "original_form": "혹시 티켓팅하시던 실력으로 꼬뜨게랑 가방을 겟하신 건가요?",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014768.1.28",
          "form": "그럴지도요",
          "original_form": "그럴지도요",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014768.1.29",
          "form": "결재가 빨리 빨리 되야하거든요",
          "original_form": "결재가 빨리 빨리 되야하거든요",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014768.1.30",
          "form": "그렇군요 제일 좋았던 공연은 누구의 공연이었나요?",
          "original_form": "그렇군요 제일 좋았던 공연은 누구의 공연이었나요?",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014768.1.31",
          "form": "제가 팬시차일드 좋아해서",
          "original_form": "제가 팬시차일드 좋아해서",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014768.1.32",
          "form": "크러쉬콘이랑 지코 콘이요",
          "original_form": "크러쉬콘이랑 지코 콘이요",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014768.1.33",
          "form": "크러쉬는 도깨비ost 부르신 분 맞죠? 저도 그분 목소리가 좋더라구요",
          "original_form": "크러쉬는 도깨비ost 부르신 분 맞죠? 저도 그분 목소리가 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014768.1.34",
          "form": "지코는 워낙 유명하고요",
          "original_form": "지코는 워낙 유명하고요",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014768.1.35",
          "form": "넹 넹.",
          "original_form": "넹 넹.",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014768.1.36",
          "form": "둘다 지금 군복무중",
          "original_form": "둘다 지금 군복무중",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014768.1.37",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014768.1.38",
          "form": "복귀쯤에는 코로롱이 사라지길",
          "original_form": "복귀쯤에는 코로롱이 사라지길",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014768.1.39",
          "form": "그러게요 꼭 콘서트 할 수 있었으면 좋겠네요",
          "original_form": "그러게요 꼭 콘서트 할 수 있었으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210825 21:48"
        }
      ]
