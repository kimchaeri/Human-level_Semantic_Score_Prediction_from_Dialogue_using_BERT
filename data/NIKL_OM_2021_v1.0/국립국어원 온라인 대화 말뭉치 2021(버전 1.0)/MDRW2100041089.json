      "utterance": [
        {
          "id": "MDRW2100041089.1.1",
          "form": "ㅋㅋ그니까여",
          "original_form": "ㅋㅋ그니까여",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041089.1.2",
          "form": "저녁먹기전에 산책한번해야겠어요",
          "original_form": "저녁먹기전에 산책한번해야겠어요",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041089.1.3",
          "form": "한번도나가지않아서..",
          "original_form": "한번도나가지않아서..",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041089.1.4",
          "form": "오왕 산책코스가 있으신가요",
          "original_form": "오왕 산책코스가 있으신가요",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041089.1.5",
          "form": "밥맛을 키워주는 산책",
          "original_form": "밥맛을 키워주는 산책",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041089.1.6",
          "form": "주말엔 원래 집콕 아닌가요",
          "original_form": "주말엔 원래 집콕 아닌가요",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041089.1.7",
          "form": "네 집앞 거북도사님이 계신곳을 돕니다ㅋㅋ",
          "original_form": "네 집앞 거북도사님이 계신곳을 돕니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041089.1.8",
          "form": "다녀와야 밥도맛나니까요!",
          "original_form": "다녀와야 밥도맛나니까요!",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041089.1.9",
          "form": "멋져요 저는 식곤증을 이겨낸 것만으로도 스스로를 칭찬했습니더",
          "original_form": "멋져요 저는 식곤증을 이겨낸 것만으로도 스스로를 칭찬했습니더",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041089.1.10",
          "form": "거북도사님과 만날 수 있으면 좋겠어요",
          "original_form": "거북도사님과 만날 수 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041089.1.11",
          "form": "ㅋㅋ충분히 칭찬받을만한 일입니다",
          "original_form": "ㅋㅋ충분히 칭찬받을만한 일입니다",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041089.1.12",
          "form": "그럼다녀오케옵",
          "original_form": "그럼다녀오케옵",
          "speaker_id": "2",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100041089.1.13",
          "form": "입맛을 위한 여정(?) 화이팅!!!",
          "original_form": "입맛을 위한 여정(?) 화이팅!!!",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041089.1.14",
          "form": "오늘은 그리팅몰이라는 데서 저녁을 사먹어봤습니다",
          "original_form": "오늘은 그리팅몰이라는 데서 저녁을 사먹어봤습니다",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100041089.1.15",
          "form": "배달도시락 맛있네요",
          "original_form": "배달도시락 맛있네요",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100041089.1.16",
          "form": "오 메뉴가 뭐였나요?",
          "original_form": "오 메뉴가 뭐였나요?",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100041089.1.17",
          "form": "오야코동이었어요",
          "original_form": "오야코동이었어요",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100041089.1.18",
          "form": "제법.. 그럴싸했는데 다 먹고 깨달았습니다",
          "original_form": "제법.. 그럴싸했는데 다 먹고 깨달았습니다",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100041089.1.19",
          "form": "소스가 맑다 싶던것이 계란이 없어서였단걸..",
          "original_form": "소스가 맑다 싶던것이 계란이 없어서였단걸..",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100041089.1.20",
          "form": "오야코동인걸 다 드시고 깨달으신거에요?ㅎㅎ",
          "original_form": "오야코동인걸 다 드시고 깨달으신거에요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100041089.1.21",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100041089.1.22",
          "form": "오야코동인건 알고 있었는데",
          "original_form": "오야코동인건 알고 있었는데",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100041089.1.23",
          "form": "계란은 알아서 넣어야했던건가여..",
          "original_form": "계란은 알아서 넣어야했던건가여..",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100041089.1.24",
          "form": "별생각없이 다 먹었어요",
          "original_form": "별생각없이 다 먹었어요",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100041089.1.25",
          "form": "ㅋㅋㅋ사진에.. 계란이 있더라고요?",
          "original_form": "ㅋㅋㅋ사진에.. 계란이 있더라고요?",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100041089.1.26",
          "form": "ㅋㅋㅋㅋㄱㅋㄱㄱ거기서 계란은 안챙겨주셯군여..",
          "original_form": "ㅋㅋㅋㅋㄱㅋㄱㄱ거기서 계란은 안챙겨주셯군여..",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100041089.1.27",
          "form": "하지만 깨끗이 맛있게 먹었으니 오케이입니다",
          "original_form": "하지만 깨끗이 맛있게 먹었으니 오케이입니다",
          "speaker_id": "1",
          "time": "20210907 18:55"
        }
      ]
