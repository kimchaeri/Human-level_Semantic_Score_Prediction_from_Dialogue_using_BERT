      "utterance": [
        {
          "id": "MDRW2100021461.1.1",
          "form": "안녕하세용 커피 좋아하세요?",
          "original_form": "안녕하세용 커피 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021461.1.2",
          "form": "건강 때문에 못 먹고 있어요",
          "original_form": "건강 때문에 못 먹고 있어요",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021461.1.3",
          "form": "커피 좋아하시나봐요?",
          "original_form": "커피 좋아하시나봐요?",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021461.1.4",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021461.1.5",
          "form": "아니요ㅋㅋㅋ 사실 저도 잘 못마셔요ㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ 사실 저도 잘 못마셔요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021461.1.6",
          "form": "그럼 건강식으로 뭐 드세요?",
          "original_form": "그럼 건강식으로 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021461.1.7",
          "form": "홍차는 1잔 정도만 마시고 주로 허브티 마셔요",
          "original_form": "홍차는 1잔 정도만 마시고 주로 허브티 마셔요",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100021461.1.8",
          "form": "좋아하는 허브티 있으신가요?",
          "original_form": "좋아하는 허브티 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100021461.1.9",
          "form": "앗 저도 홍차 넘 좋아해서 밀크티도 자주마시는데",
          "original_form": "앗 저도 홍차 넘 좋아해서 밀크티도 자주마시는데",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021461.1.10",
          "form": "허브티는 아직 시도를 못해봤네요ㅠㅠ",
          "original_form": "허브티는 아직 시도를 못해봤네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021461.1.11",
          "form": "진짜 홍차 좋아하시나봐요 밀크티도 자주 드시고",
          "original_form": "진짜 홍차 좋아하시나봐요 밀크티도 자주 드시고",
          "speaker_id": "2",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100021461.1.12",
          "form": "네네ㅋㅋㅋ 그런데",
          "original_form": "네네ㅋㅋㅋ 그런데",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021461.1.13",
          "form": "홍차가 치아 착색을 유발한다고해서",
          "original_form": "홍차가 치아 착색을 유발한다고해서",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021461.1.14",
          "form": "그 뒤로는 자제하고있어요ㅠ",
          "original_form": "그 뒤로는 자제하고있어요ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021461.1.15",
          "form": "빨대로 드세요 스케일링 잘 하시고요",
          "original_form": "빨대로 드세요 스케일링 잘 하시고요",
          "speaker_id": "2",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021461.1.16",
          "form": "저도 착색 때문에 치아 덜 닿으려고 노력해요",
          "original_form": "저도 착색 때문에 치아 덜 닿으려고 노력해요",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021461.1.17",
          "form": "오 그런방법이!",
          "original_form": "오 그런방법이!",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021461.1.18",
          "form": "빨대를 생각을 못했네요",
          "original_form": "빨대를 생각을 못했네요",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021461.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021461.1.20",
          "form": "한 번 사용해 보세요",
          "original_form": "한 번 사용해 보세요",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021461.1.21",
          "form": "실리콘 빨대 안그래도 음료 마실때 사려고 했는데",
          "original_form": "실리콘 빨대 안그래도 음료 마실때 사려고 했는데",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021461.1.22",
          "form": "홍차마실때 필요하니",
          "original_form": "홍차마실때 필요하니",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021461.1.23",
          "form": "당장 사야겠어요ㅋㅋ",
          "original_form": "당장 사야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021461.1.24",
          "form": "구매예정에 있으셨군요 빨대가 음료를 마실 때 참 좋아요",
          "original_form": "구매예정에 있으셨군요 빨대가 음료를 마실 때 참 좋아요",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100021461.1.25",
          "form": "맞아요ㅋㅋㅋ 혹시 배달음식 자주 사켜드세요?",
          "original_form": "맞아요ㅋㅋㅋ 혹시 배달음식 자주 사켜드세요?",
          "speaker_id": "1",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021461.1.26",
          "form": "아뇨 포장을 자주해요 오리덕님은요?",
          "original_form": "아뇨 포장을 자주해요 오리덕님은요?",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100021461.1.27",
          "form": "저도 배달비 오른 이후로는 주로 방문포장으로 사먹고 있어용",
          "original_form": "저도 배달비 오른 이후로는 주로 방문포장으로 사먹고 있어용",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021461.1.28",
          "form": "치킨 자주 사다먹어요ㅋㅋ",
          "original_form": "치킨 자주 사다먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021461.1.29",
          "form": "전 주로 샐러드 비빔밥 김밥 가끔 족발이요",
          "original_form": "전 주로 샐러드 비빔밥 김밥 가끔 족발이요",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100021461.1.30",
          "form": "우왕 건강한 음식 위주로 드시네요ㅋㅋㅋㅋ",
          "original_form": "우왕 건강한 음식 위주로 드시네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021461.1.31",
          "form": "저도 김밥 넘 좋아하는데",
          "original_form": "저도 김밥 넘 좋아하는데",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021461.1.32",
          "form": "어젠가 저희 동네에 단체 식중독이 터져서ㅠㅠ",
          "original_form": "어젠가 저희 동네에 단체 식중독이 터져서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021461.1.33",
          "form": "에구 요즘 김밥집 식중독이 기사 많이 나던데... 동네쪽이면 불안하시겠어요",
          "original_form": "에구 요즘 김밥집 식중독이 기사 많이 나던데... 동네쪽이면 불안하시겠어요",
          "speaker_id": "2",
          "time": "20210828 19:59"
        }
      ]
