      "utterance": [
        {
          "id": "MDRW2100003273.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 21:17"
        },
        {
          "id": "MDRW2100003273.1.2",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 21:17"
        },
        {
          "id": "MDRW2100003273.1.3",
          "form": "아뇨 좋아하진 않는데",
          "original_form": "아뇨 좋아하진 않는데",
          "speaker_id": "2",
          "time": "20210820 21:17"
        },
        {
          "id": "MDRW2100003273.1.4",
          "form": "그래도 꾸준히 달리기는 해요",
          "original_form": "그래도 꾸준히 달리기는 해요",
          "speaker_id": "2",
          "time": "20210820 21:17"
        },
        {
          "id": "MDRW2100003273.1.5",
          "form": "아침에 달리기 하시나요?",
          "original_form": "아침에 달리기 하시나요?",
          "speaker_id": "1",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003273.1.6",
          "form": "넵 아침 6시에 나가서 해요",
          "original_form": "넵 아침 6시에 나가서 해요",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003273.1.7",
          "form": "하시는 운동 있으세요?",
          "original_form": "하시는 운동 있으세요?",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003273.1.8",
          "form": "저는 운동 안하고있어요",
          "original_form": "저는 운동 안하고있어요",
          "speaker_id": "1",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003273.1.9",
          "form": "달리기는 어디서 하시나요?",
          "original_form": "달리기는 어디서 하시나요?",
          "speaker_id": "1",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003273.1.10",
          "form": "저희 집앞에서 그냥 바로해요",
          "original_form": "저희 집앞에서 그냥 바로해요",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003273.1.11",
          "form": "저희 동네가 반은 촌이라",
          "original_form": "저희 동네가 반은 촌이라",
          "speaker_id": "2",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003273.1.12",
          "form": "사람도 잘 없더라구요",
          "original_form": "사람도 잘 없더라구요",
          "speaker_id": "2",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003273.1.13",
          "form": "운동장 말고 도로를 뛰시나요?",
          "original_form": "운동장 말고 도로를 뛰시나요?",
          "speaker_id": "1",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003273.1.14",
          "form": "네 도로도 다니는데",
          "original_form": "네 도로도 다니는데",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003273.1.15",
          "form": "3분뛰면 농사짓는 밭쪽으로 뛰어요",
          "original_form": "3분뛰면 농사짓는 밭쪽으로 뛰어요",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003273.1.16",
          "form": "몇분정도 달리시나요?",
          "original_form": "몇분정도 달리시나요?",
          "speaker_id": "1",
          "time": "20210820 21:26"
        },
        {
          "id": "MDRW2100003273.1.17",
          "form": "한 3~40분 달리는거같아요",
          "original_form": "한 3~40분 달리는거같아요",
          "speaker_id": "2",
          "time": "20210820 21:27"
        },
        {
          "id": "MDRW2100003273.1.18",
          "form": "혹시 아프신곳 있어요?",
          "original_form": "혹시 아프신곳 있어요?",
          "speaker_id": "2",
          "time": "20210820 21:27"
        },
        {
          "id": "MDRW2100003273.1.19",
          "form": "아니요 건강합니다.",
          "original_form": "아니요 건강합니다.",
          "speaker_id": "1",
          "time": "20210820 21:29"
        },
        {
          "id": "MDRW2100003273.1.20",
          "form": "지병 있으신가요?",
          "original_form": "지병 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 21:29"
        },
        {
          "id": "MDRW2100003273.1.21",
          "form": "저도 어렸을때 천식있었던거 빼면 건강한 편이에요",
          "original_form": "저도 어렸을때 천식있었던거 빼면 건강한 편이에요",
          "speaker_id": "2",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003273.1.22",
          "form": "천식 힘들었겠네요",
          "original_form": "천식 힘들었겠네요",
          "speaker_id": "1",
          "time": "20210820 21:32"
        }
      ]
