      "utterance": [
        {
          "id": "MDRW2100002505.1.1",
          "form": "우리 아델 키워야하잖나",
          "original_form": "우리 아델 키워야하잖나",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002505.1.2",
          "form": "전에 무료쿠폰으로",
          "original_form": "전에 무료쿠폰으로",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.3",
          "form": "머리도 뽑고 눈도 뽑앗는데",
          "original_form": "머리도 뽑고 눈도 뽑앗는데",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.4",
          "form": "지금 방치중이야",
          "original_form": "지금 방치중이야",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.5",
          "form": "ㅜㅠ",
          "original_form": "ㅜㅠ",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.6",
          "form": "피시방도 코로나때문에",
          "original_form": "피시방도 코로나때문에",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.7",
          "form": "가기무섭",
          "original_form": "가기무섭",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.8",
          "form": "메이플하고싶다",
          "original_form": "메이플하고싶다",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.9",
          "form": "같이하면",
          "original_form": "같이하면",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.10",
          "form": "머든 꿀잼이자나",
          "original_form": "머든 꿀잼이자나",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.11",
          "form": "메이플 말고 모배하자",
          "original_form": "메이플 말고 모배하자",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.12",
          "form": "우리 요즘에 모배 너무안햇음",
          "original_form": "우리 요즘에 모배 너무안햇음",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.13",
          "form": "맞다",
          "original_form": "맞다",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.14",
          "form": "모배도해야제",
          "original_form": "모배도해야제",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002505.1.15",
          "form": "근데 역시 모배는",
          "original_form": "근데 역시 모배는",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.16",
          "form": "사녹이짱이야",
          "original_form": "사녹이짱이야",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.17",
          "form": "사녹 꿀잼",
          "original_form": "사녹 꿀잼",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.18",
          "form": "요즘엔 리빅도 재믹어",
          "original_form": "요즘엔 리빅도 재믹어",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.19",
          "form": "재밋어",
          "original_form": "재밋어",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.20",
          "form": "50명밖에 안내려서",
          "original_form": "50명밖에 안내려서",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.21",
          "form": "게임도 빨리끝나가지고조아",
          "original_form": "게임도 빨리끝나가지고조아",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.22",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.23",
          "form": "글거 맵좁은것도",
          "original_form": "글거 맵좁은것도",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.24",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.25",
          "form": "사람 찾기편하거등",
          "original_form": "사람 찾기편하거등",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.26",
          "form": "인졍",
          "original_form": "인졍",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.27",
          "form": "총소리 바로 들려",
          "original_form": "총소리 바로 들려",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002505.1.28",
          "form": "아 지금 모배하고싶네",
          "original_form": "아 지금 모배하고싶네",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.29",
          "form": "갑자기 얘기하니께",
          "original_form": "갑자기 얘기하니께",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.31",
          "form": "좀만 참아",
          "original_form": "좀만 참아",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.32",
          "form": "만나서 고고",
          "original_form": "만나서 고고",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.33",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.34",
          "form": "나도 집에있고싶다고",
          "original_form": "나도 집에있고싶다고",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.35",
          "form": "나왜 학교냐고",
          "original_form": "나왜 학교냐고",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.36",
          "form": "나도 겜하고싶어",
          "original_form": "나도 겜하고싶어",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.37",
          "form": "진정혀",
          "original_form": "진정혀",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.38",
          "form": "어차피 그래도 집에못오니깐",
          "original_form": "어차피 그래도 집에못오니깐",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.39",
          "form": "집오면 배그하자",
          "original_form": "집오면 배그하자",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002505.1.40",
          "form": "응.....",
          "original_form": "응.....",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002505.1.41",
          "form": "혼자 게임하지말기",
          "original_form": "혼자 게임하지말기",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002505.1.42",
          "form": "언제나 여름방학은 하고잇을게",
          "original_form": "언제나 여름방학은 하고잇을게",
          "speaker_id": "2",
          "time": "20210820 12:58"
        }
      ]
