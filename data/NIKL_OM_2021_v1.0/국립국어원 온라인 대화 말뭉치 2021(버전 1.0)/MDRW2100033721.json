        {
          "id": "MDRW2100033721.1.1",
          "form": "긍가",
          "original_form": "긍가",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033721.1.2",
          "form": "근데 니도 수업받으러왔는데",
          "original_form": "근데 니도 수업받으러왔는데",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033721.1.3",
          "form": "도움받아야하는골ㅜㅜ",
          "original_form": "도움받아야하는골ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033721.1.4",
          "form": "세상에 그러다보면 진도는 쭉쭉 나가고 있어요",
          "original_form": "세상에 그러다보면 진도는 쭉쭉 나가고 있어요",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033721.1.5",
          "form": "나는 패닉이고",
          "original_form": "나는 패닉이고",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033721.1.6",
          "form": "나의 상태",
          "original_form": "나의 상태",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033721.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033721.1.8",
          "form": "컴 바꿔죠",
          "original_form": "컴 바꿔죠",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033721.1.9",
          "form": "아니 컴터의 문제가 아니라",
          "original_form": "아니 컴터의 문제가 아니라",
          "speaker_id": "1",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033721.1.10",
          "form": "내가 미세한 실수를 한거임",
          "original_form": "내가 미세한 실수를 한거임",
          "speaker_id": "1",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033721.1.11",
          "form": "으헝헝",
          "original_form": "으헝헝",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033721.1.12",
          "form": "그걸 알러줘야 선생님이지",
          "original_form": "그걸 알러줘야 선생님이지",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033721.1.13",
          "form": "근데 중간에 안 끊은건 그렇다치고",
          "original_form": "근데 중간에 안 끊은건 그렇다치고",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033721.1.14",
          "form": "다른사람도 실수했을수도 있자낭",
          "original_form": "다른사람도 실수했을수도 있자낭",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033721.1.15",
          "form": "나중에 물어보면",
          "original_form": "나중에 물어보면",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033721.1.16",
          "form": "말투가 괜히 불친절하게 느껴짐",
          "original_form": "말투가 괜히 불친절하게 느껴짐",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033721.1.17",
          "form": "울name1님은 교육학 배운사람인데",
          "original_form": "울&name1&님은 교육학 배운사람인데",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033721.1.18",
          "form": "이거이거해봤어?",
          "original_form": "이거이거해봤어?",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033721.1.19",
          "form": "영 못가리키네",
          "original_form": "영 못가리키네",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033721.1.20",
          "form": "가리키는건 괜찮음",
          "original_form": "가리키는건 괜찮음",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033721.1.21",
          "form": "질문하면",
          "original_form": "질문하면",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033721.1.22",
          "form": "좀 친절하게 알려줬으면",
          "original_form": "좀 친절하게 알려줬으면",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033721.1.23",
          "form": "학생이 알게되야 잘가르치는거지",
          "original_form": "학생이 알게되야 잘가르치는거지",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033721.1.24",
          "form": "그래 바로 그거",
          "original_form": "그래 바로 그거",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033721.1.25",
          "form": "일부러 그러는건 아닌거 같고",
          "original_form": "일부러 그러는건 아닌거 같고",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033721.1.26",
          "form": "바빠서 긍가봄",
          "original_form": "바빠서 긍가봄",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033721.1.27",
          "form": "일부러 그러는게 아닌것 같은데",
          "original_form": "일부러 그러는게 아닌것 같은데",
