      "utterance": [
        {
          "id": "MDRW2100022668.1.1",
          "form": "여자가 원래 그 변호사한테 관심있었는데 뺏을라고 그 ㅈ?ㄹ 한거",
          "original_form": "여자가 원래 그 변호사한테 관심있었는데 뺏을라고 그 ㅈ?ㄹ 한거",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022668.1.2",
          "form": "아니 그 드라마에 나오는 남자들 다 그래?",
          "original_form": "아니 그 드라마에 나오는 남자들 다 그래?",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022668.1.3",
          "form": "뭐지",
          "original_form": "뭐지",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022668.1.4",
          "form": "오케이 광자매 드라마!",
          "original_form": "오케이 광자매 드라마!",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022668.1.5",
          "form": "드라마 음원도 가끔 좋은거나오는데",
          "original_form": "드라마 음원도 가끔 좋은거나오는데",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022668.1.6",
          "form": "갑자기 그 드라마 보고싶다",
          "original_form": "갑자기 그 드라마 보고싶다",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022668.1.7",
          "form": "언제 언제 해?",
          "original_form": "언제 언제 해?",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022668.1.8",
          "form": "토,일 kbs",
          "original_form": "토,일 kbs",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022668.1.9",
          "form": "근데  앞으로 3주뒤면 끝나",
          "original_form": "근데  앞으로 3주뒤면 끝나",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022668.1.10",
          "form": "엥 벌써..?",
          "original_form": "엥 벌써..?",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022668.1.11",
          "form": "그럼 대충 눈에 보이면 봐야겠다..",
          "original_form": "그럼 대충 눈에 보이면 봐야겠다..",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022668.1.12",
          "form": "각잡고 정주행하시오",
          "original_form": "각잡고 정주행하시오",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022668.1.13",
          "form": "오케오케",
          "original_form": "오케오케",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022668.1.14",
          "form": "나 지금 런닝맨 오랜만에 보는데",
          "original_form": "나 지금 런닝맨 오랜만에 보는데",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022668.1.15",
          "form": "공포 특집인가?",
          "original_form": "공포 특집인가?",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022668.1.16",
          "form": "재방송?",
          "original_form": "재방송?",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022668.1.17",
          "form": "본방송",
          "original_form": "본방송",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022668.1.18",
          "form": "지금 하고있어",
          "original_form": "지금 하고있어",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022668.1.19",
          "form": "오??",
          "original_form": "오??",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022668.1.20",
          "form": "누구나와",
          "original_form": "누구나와",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022668.1.21",
          "form": "원 멤버들끼리하고",
          "original_form": "원 멤버들끼리하고",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022668.1.22",
          "form": "인형의 집이래",
          "original_form": "인형의 집이래",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022668.1.23",
          "form": "런닝맨 인형의 집이 주제인 것 같은데",
          "original_form": "런닝맨 인형의 집이 주제인 것 같은데",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022668.1.24",
          "form": "무섭다",
          "original_form": "무섭다",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022668.1.25",
          "form": "인형에 귀신이 씌였대",
          "original_form": "인형에 귀신이 씌였대",
          "speaker_id": "1",
