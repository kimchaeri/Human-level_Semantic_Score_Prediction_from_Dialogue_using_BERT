      "utterance": [
        {
          "id": "MDRW2100014218.1.1",
          "form": "뮤지컬 좋아하시나봐여",
          "original_form": "뮤지컬 좋아하시나봐여",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014218.1.2",
          "form": "아ㅋㅋ 저 뮤지컬 커서는 기회가없어서 많이 못봤어요",
          "original_form": "아ㅋㅋ 저 뮤지컬 커서는 기회가없어서 많이 못봤어요",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014218.1.3",
          "form": "어릴 때 미녀와 야수 이런건 봤는데",
          "original_form": "어릴 때 미녀와 야수 이런건 봤는데",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014218.1.4",
          "form": "뮤지컬 좋아하시나요",
          "original_form": "뮤지컬 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014218.1.5",
          "form": "저도 어릴때 마리앙뚜아네트가 마지막이에요 ㅋㅌㅌㅌ",
          "original_form": "저도 어릴때 마리앙뚜아네트가 마지막이에요 ㅋㅌㅌㅌ",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014218.1.6",
          "form": "그 공연은 어땠나요?",
          "original_form": "그 공연은 어땠나요?",
          "speaker_id": "2",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014218.1.7",
          "form": "김소현...? 그분이 하셨는데",
          "original_form": "김소현...? 그분이 하셨는데",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014218.1.8",
          "form": "오졌어요",
          "original_form": "오졌어요",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014218.1.9",
          "form": "노래랑 연기가요?",
          "original_form": "노래랑 연기가요?",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014218.1.10",
          "form": "너무 궁금하네요",
          "original_form": "너무 궁금하네요",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014218.1.11",
          "form": "혹시 최근에 극장가서 보신 영화있으신가요",
          "original_form": "혹시 최근에 극장가서 보신 영화있으신가요",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014218.1.12",
          "form": "노래가 오졌어요ㄹㅇㄹㅇ!!!",
          "original_form": "노래가 오졌어요ㄹㅇㄹㅇ!!!",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014218.1.13",
          "form": "저는 음..",
          "original_form": "저는 음..",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014218.1.14",
          "form": "헐 생각해보니까 없어여 ㄷㄷㄷㄷㄷㄷ 코로나 이후로 갖적이..",
          "original_form": "헐 생각해보니까 없어여 ㄷㄷㄷㄷㄷㄷ 코로나 이후로 갖적이..",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014218.1.15",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014218.1.16",
          "form": "아무래도.. 코로나는 위험하니까요",
          "original_form": "아무래도.. 코로나는 위험하니까요",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014218.1.17",
          "form": "그럼 마지막으로 보신 영화 기억나시나요",
          "original_form": "그럼 마지막으로 보신 영화 기억나시나요",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014218.1.18",
          "form": "먼가 극장 돈아까워서 집에서 왓챠보게되네용",
          "original_form": "먼가 극장 돈아까워서 집에서 왓챠보게되네용",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014218.1.19",
          "form": "저는 인셉션봣어여",
          "original_form": "저는 인셉션봣어여",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014218.1.20",
          "form": "인셉션 너무 재밌죠",
          "original_form": "인셉션 너무 재밌죠",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014218.1.21",
          "form": "어떤점이 인상깊었나오",
          "original_form": "어떤점이 인상깊었나오",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014218.1.22",
          "form": "저 인셉션 너무 재밌게봐서 몇번 돌려봤는데",
          "original_form": "저 인셉션 너무 재밌게봐서 몇번 돌려봤는데",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014218.1.23",
          "form": "역시 명장면은 그 새로 들어온 여자애가",
          "original_form": "역시 명장면은 그 새로 들어온 여자애가",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014218.1.24",
          "form": "상상으로 도시 만드는 장면...?",
          "original_form": "상상으로 도시 만드는 장면...?",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014218.1.25",
          "form": "그거져",
          "original_form": "그거져",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014218.1.26",
          "form": "맞아요 그 장면 너무 좋았어요",
          "original_form": "맞아요 그 장면 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014218.1.27",
          "form": "봐도 봐도 잘만들었다 생각드는 장면이에요ㅎㅎ",
          "original_form": "봐도 봐도 잘만들었다 생각드는 장면이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:27"
        }
      ]
    }
  ]
