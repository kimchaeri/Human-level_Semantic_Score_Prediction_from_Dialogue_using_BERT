      "utterance": [
        {
          "id": "MDRW2100008992.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 20:15"
        },
        {
          "id": "MDRW2100008992.1.2",
          "form": "어떤 일 하세요?",
          "original_form": "어떤 일 하세요?",
          "speaker_id": "1",
          "time": "20210822 20:15"
        },
        {
          "id": "MDRW2100008992.1.3",
          "form": "유튜버요",
          "original_form": "유튜버요",
          "speaker_id": "2",
          "time": "20210822 20:15"
        },
        {
          "id": "MDRW2100008992.1.4",
          "form": "어떤일하세요?",
          "original_form": "어떤일하세요?",
          "speaker_id": "2",
          "time": "20210822 20:16"
        },
        {
          "id": "MDRW2100008992.1.5",
          "form": "저는 사무직이에요",
          "original_form": "저는 사무직이에요",
          "speaker_id": "1",
          "time": "20210822 20:16"
        },
        {
          "id": "MDRW2100008992.1.6",
          "form": "어떤 유투브 채널을 운영하시나요?",
          "original_form": "어떤 유투브 채널을 운영하시나요?",
          "speaker_id": "1",
          "time": "20210822 20:16"
        },
        {
          "id": "MDRW2100008992.1.7",
          "form": "말하기 곤란해요",
          "original_form": "말하기 곤란해요",
          "speaker_id": "2",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008992.1.8",
          "form": "개인정보여서요",
          "original_form": "개인정보여서요",
          "speaker_id": "2",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008992.1.9",
          "form": "아 채널명을 말하는 게 아니고 주제요",
          "original_form": "아 채널명을 말하는 게 아니고 주제요",
          "speaker_id": "1",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008992.1.10",
          "form": "취미랑 정보공유요",
          "original_form": "취미랑 정보공유요",
          "speaker_id": "2",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008992.1.11",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008992.1.12",
          "form": "어떤 점이 제일 힘드신가요?",
          "original_form": "어떤 점이 제일 힘드신가요?",
          "speaker_id": "1",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008992.1.13",
          "form": "취업한지 얼마나 되셨어요?",
          "original_form": "취업한지 얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008992.1.14",
          "form": "악플다는 사람들요",
          "original_form": "악플다는 사람들요",
          "speaker_id": "2",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100008992.1.15",
          "form": "저는 6년차입니다",
          "original_form": "저는 6년차입니다",
          "speaker_id": "1",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100008992.1.16",
          "form": "젤짜증나는 부류에요",
          "original_form": "젤짜증나는 부류에요",
          "speaker_id": "2",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100008992.1.17",
          "form": "어느정도 익숙해지셨겠어요",
          "original_form": "어느정도 익숙해지셨겠어요",
          "speaker_id": "2",
          "time": "20210822 20:21"
        },
        {
          "id": "MDRW2100008992.1.18",
          "form": "네 근데 직업 특성상 업무도 같이 일하는 사람도 자주 바뀌어서 매번 새로워요",
          "original_form": "네 근데 직업 특성상 업무도 같이 일하는 사람도 자주 바뀌어서 매번 새로워요",
          "speaker_id": "1",
          "time": "20210822 20:22"
        },
        {
          "id": "MDRW2100008992.1.19",
          "form": "좀더 자세히 여쭤봐도되나요?",
          "original_form": "좀더 자세히 여쭤봐도되나요?",
          "speaker_id": "2",
          "time": "20210822 20:23"
        },
        {
          "id": "MDRW2100008992.1.20",
          "form": "일반사기업이에요?",
          "original_form": "일반사기업이에요?",
          "speaker_id": "2",
          "time": "20210822 20:23"
        },
        {
          "id": "MDRW2100008992.1.21",
          "form": "아뇨 공무원입니다..",
          "original_form": "아뇨 공무원입니다..",
          "speaker_id": "1",
          "time": "20210822 20:23"
        },
        {
          "id": "MDRW2100008992.1.22",
          "form": "업무가 자주바뀌나요?",
          "original_form": "업무가 자주바뀌나요?",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008992.1.23",
          "form": "네 주기적으로 바뀌는 편이에요",
          "original_form": "네 주기적으로 바뀌는 편이에요",
          "speaker_id": "1",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008992.1.24",
          "form": "인사이동이 있어서",
          "original_form": "인사이동이 있어서",
          "speaker_id": "1",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008992.1.25",
          "form": "거기 빽으로 들어오는 사람들 많아요? 문정부 이후로 이런저런 자리 만든다고 말많던데요",
          "original_form": "거기 빽으로 들어오는 사람들 많아요? 문정부 이후로 이런저런 자리 만든다고 말많던데요",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008992.1.26",
          "form": "일반직 공무원은 대부분 시험을 봐서 들어옵니다",
          "original_form": "일반직 공무원은 대부분 시험을 봐서 들어옵니다",
          "speaker_id": "1",
          "time": "20210822 20:25"
        }
      ]
    }
