      "utterance": [
        {
          "id": "MDRW2100040317.1.1",
          "form": "제가 어제 보내드린 영상 봤나요",
          "original_form": "제가 어제 보내드린 영상 봤나요",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100040317.1.2",
          "form": "주키퍼 하는 영상인데",
          "original_form": "주키퍼 하는 영상인데",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100040317.1.3",
          "form": "수제 잘못 잡았다",
          "original_form": "수제 잘못 잡았다",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.4",
          "form": "해봤어요 죽기 퍼",
          "original_form": "해봤어요 죽기 퍼",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.5",
          "form": "자리를 옮겨서 없애는 게임이에요",
          "original_form": "자리를 옮겨서 없애는 게임이에요",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.6",
          "form": "피시 돈도 비슷해요",
          "original_form": "피시 돈도 비슷해요",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.7",
          "form": "허를 죽이진 마시고 주키퍼입니다",
          "original_form": "허를 죽이진 마시고 주키퍼입니다",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.8",
          "form": "또 미랑 똑같은 것 같아요",
          "original_form": "또 미랑 똑같은 것 같아요",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.9",
          "form": "아 그렇군요 금방 주키퍼 하고 왔는데 한 판에 30초에요",
          "original_form": "아 그렇군요 금방 주키퍼 하고 왔는데 한 판에 30초에요",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.10",
          "form": "피시도는 너무 오래 걸려요",
          "original_form": "피시도는 너무 오래 걸려요",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.11",
          "form": "안한지 오래 됐어요",
          "original_form": "안한지 오래 됐어요",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.12",
          "form": "떠나신 거 같아요 개인까지 하시구",
          "original_form": "떠나신 거 같아요 개인까지 하시구",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.13",
          "form": "주키퍼 - 30 초라서 빨리 끝낼 수 있어요",
          "original_form": "주키퍼 - 30 초라서 빨리 끝낼 수 있어요",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.14",
          "form": "부지런하신 거 같아요",
          "original_form": "부지런하신 거 같아요",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040317.1.15",
          "form": "이런 데서 만 부지런합니다",
          "original_form": "이런 데서 만 부지런합니다",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100040317.1.16",
          "form": "좋아하는 곳에서 부지런하면 됐죠",
          "original_form": "좋아하는 곳에서 부지런하면 됐죠",
          "speaker_id": "1",
          "time": "20210904 16:06"
        },
