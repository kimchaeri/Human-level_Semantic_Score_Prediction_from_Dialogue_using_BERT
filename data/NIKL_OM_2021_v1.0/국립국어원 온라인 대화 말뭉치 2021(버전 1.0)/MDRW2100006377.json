          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006377.1.3",
          "form": "전문 분야다",
          "original_form": "전문 분야다",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006377.1.4",
          "form": "조타",
          "original_form": "조타",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006377.1.5",
          "form": "크램이랑 왔다갔다 해라",
          "original_form": "크램이랑 왔다갔다 해라",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006377.1.6",
          "form": "추천 막 해봐라",
          "original_form": "추천 막 해봐라",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006377.1.7",
          "form": "맞 영업할란다",
          "original_form": "맞 영업할란다",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006377.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006377.1.9",
          "form": "아아니 당분간은 영업당하고 싶지않은데",
          "original_form": "아아니 당분간은 영업당하고 싶지않은데",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006377.1.10",
          "form": "어쩔수 없어 빨리 해봐",
          "original_form": "어쩔수 없어 빨리 해봐",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006377.1.11",
          "form": "엘리자베스 보세요 1시간마다 무료 이용권 준데용",
          "original_form": "엘리자베스 보세요 1시간마다 무료 이용권 준데용",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006377.1.12",
          "form": "아 웹툰은 안받아요",
          "original_form": "아 웹툰은 안받아요",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006377.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006377.1.14",
          "form": "소설은 이미 다 본거고",
          "original_form": "소설은 이미 다 본거고",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006377.1.15",
          "form": "웹툰은.. 좀 나중에..",
          "original_form": "웹툰은.. 좀 나중에..",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006377.1.16",
          "form": "아.. 이런..",
          "original_form": "아.. 이런..",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006377.1.17",
          "form": "그렇다면.. 뭐로 맞불놓지",
          "original_form": "그렇다면.. 뭐로 맞불놓지",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006377.1.18",
          "form": "소설만 받는다면",
          "original_form": "소설만 받는다면",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006377.1.19",
          "form": "문송안함 봅쉬다",
          "original_form": "문송안함 봅쉬다",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006377.1.20",
          "form": "아 3일 남았네",
          "original_form": "아 3일 남았네",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006377.1.21",
          "form": "아 이런",
          "original_form": "아 이런",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006377.1.22",
          "form": "!!!!",
          "original_form": "!!!!",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006377.1.23",
          "form": "아 망했어요 ~~..~~",
          "original_form": "아 망했어요 ~~..~~",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006377.1.24",
          "form": "ㅋㅋㅋㅋ 아 볼수 있어요",
          "original_form": "ㅋㅋㅋㅋ 아 볼수 있어요",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006377.1.25",
          "form": "충분히 합니다",
          "original_form": "충분히 합니다",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006377.1.26",
          "form": "50화 인걸요",
          "original_form": "50화 인걸요",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006377.1.27",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006377.1.28",
          "form": "좋아 일단 눈을 버린다고 생각하자",
          "original_form": "좋아 일단 눈을 버린다고 생각하자",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006377.1.29",
          "form": "ㅋㅋㅋㅋ 책읽는데 그정도는 감수해야지",
          "original_form": "ㅋㅋㅋㅋ 책읽는데 그정도는 감수해야지",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100006377.1.30",
          "form": "그쳐..",
          "original_form": "그쳐..",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006377.1.31",
          "form": "이런 이벤트 흔하게 오는 게 아니니ㅜ",
          "original_form": "이런 이벤트 흔하게 오는 게 아니니ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006377.1.32",
          "form": "당연한 말씀을",
          "original_form": "당연한 말씀을",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006377.1.33",
          "form": "저거 보시고",
          "original_form": "저거 보시고",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006377.1.34",
          "form": "저거 받고 다한아 보셔야죠",
          "original_form": "저거 받고 다한아 보셔야죠",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100006377.1.35",
          "form": "아 그르니까",
          "original_form": "아 그르니까",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006377.1.36",
          "form": "3일안에 문송 보고 또 그다음 4일안에 다한아 봐야해",
          "original_form": "3일안에 문송 보고 또 그다음 4일안에 다한아 봐야해",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006377.1.37",
          "form": "일주일안에 100편 뽀개기",
          "original_form": "일주일안에 100편 뽀개기",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006377.1.38",
          "form": "+지금 보고 있는 것도",
          "original_form": "+지금 보고 있는 것도",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006377.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋ\n 할 수 있다아아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ\n 할 수 있다아아",
