      "utterance": [
        {
          "id": "MDRW2100032488.1.1",
          "form": "넌 백신 맞았나?",
          "original_form": "넌 백신 맞았나?",
          "speaker_id": "1",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100032488.1.2",
          "form": "다담주!",
          "original_form": "다담주!",
          "speaker_id": "2",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100032488.1.3",
          "form": "추석전에 맞가로햇어요",
          "original_form": "추석전에 맞가로햇어요",
          "speaker_id": "2",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100032488.1.4",
          "form": "나는... 모레 맞아",
          "original_form": "나는... 모레 맞아",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.5",
          "form": "기절준비 철저히",
          "original_form": "기절준비 철저히",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.6",
          "form": "두렵니?",
          "original_form": "두렵니?",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.7",
          "form": "아니 별로",
          "original_form": "아니 별로",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.10",
          "form": "무서을 게 있나",
          "original_form": "무서을 게 있나",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.11",
          "form": "주사 안맞은지 넘 오래됨",
          "original_form": "주사 안맞은지 넘 오래됨",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.12",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.13",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.14",
          "form": "나 예방접종도 안했거둔 그간",
          "original_form": "나 예방접종도 안했거둔 그간",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.15",
          "form": "독감예방접종",
          "original_form": "독감예방접종",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100032488.1.16",
          "form": "나도 예방접종은 안하고 손목치료할 때 염증주사맞았네",
          "original_form": "나도 예방접종은 안하고 손목치료할 때 염증주사맞았네",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032488.1.17",
          "form": "손목",
          "original_form": "손목",
          "speaker_id": "2",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032488.1.18",
          "form": "독감 걸려본 역사가 없어서.",
          "original_form": "독감 걸려본 역사가 없어서.",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032488.1.19",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032488.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032488.1.21",
          "form": "화이자는 2차때가 아픈데",
          "original_form": "화이자는 2차때가 아픈데",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032488.1.22",
          "form": "모더나도 ㅇㅇ",
          "original_form": "모더나도 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032488.1.23",
          "form": "무섭네",
          "original_form": "무섭네",
          "speaker_id": "2",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032488.1.24",
          "form": "1차때 아프면 코로나에 한번 걸렸을 확률이 농후하대",
          "original_form": "1차때 아프면 코로나에 한번 걸렸을 확률이 농후하대",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032488.1.25",
          "form": "2차때도 안아프면 면역력이 일을 안하는 거라더라",
          "original_form": "2차때도 안아프면 면역력이 일을 안하는 거라더라",
          "speaker_id": "1",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032488.1.26",
          "form": "ㅎ헐 그럼",
          "original_form": "ㅎ헐 그럼",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032488.1.27",
          "form": "어찌해?",
          "original_form": "어찌해?",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032488.1.28",
          "form": "면역력일해",
          "original_form": "면역력일해",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032488.1.29",
          "form": "놀지마",
          "original_form": "놀지마",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100032488.1.30",
          "form": "나도 일하눈데 왜 넌 놀아",
          "original_form": "나도 일하눈데 왜 넌 놀아",
          "speaker_id": "2",
          "time": "20210905 19:31"
        },
