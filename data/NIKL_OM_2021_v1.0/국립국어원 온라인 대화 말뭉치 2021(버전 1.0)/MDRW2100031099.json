      "utterance": [
        {
          "id": "MDRW2100031099.1.1",
          "form": "운동은 숨쉬기랑 산책만 해요ㅎㅎ",
          "original_form": "운동은 숨쉬기랑 산책만 해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031099.1.2",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031099.1.3",
          "form": "아 ㅋㅋㅋㅋ 저도 걷기만요",
          "original_form": "아 ㅋㅋㅋㅋ 저도 걷기만요",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031099.1.4",
          "form": "산책 얼마나하세요?",
          "original_form": "산책 얼마나하세요?",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031099.1.5",
          "form": "건강을 위해서 해야하는데 잘은 안해요",
          "original_form": "건강을 위해서 해야하는데 잘은 안해요",
          "speaker_id": "2",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031099.1.6",
          "form": "한번 하면 한두시간?",
          "original_form": "한번 하면 한두시간?",
          "speaker_id": "2",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031099.1.7",
          "form": "한두시간이면 오래하시네요",
          "original_form": "한두시간이면 오래하시네요",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031099.1.8",
          "form": "저는 한시간해요",
          "original_form": "저는 한시간해요",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031099.1.9",
          "form": "자주 해야하는데 말이에요ㅎㅎ",
          "original_form": "자주 해야하는데 말이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100031099.1.10",
          "form": "그러게요 비오면 비 핑계로 안나가고 ᆢㅎ",
          "original_form": "그러게요 비오면 비 핑계로 안나가고 ᆢㅎ",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100031099.1.11",
          "form": "더우면 더워서 추우면 추워서 ㅎㅎ",
          "original_form": "더우면 더워서 추우면 추워서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100031099.1.12",
          "form": "일주일에 몇번 나가세요?",
          "original_form": "일주일에 몇번 나가세요?",
          "speaker_id": "1",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031099.1.13",
          "form": "일주일이면 한번 나갈랑 말랑? 주로 장보러 가요",
          "original_form": "일주일이면 한번 나갈랑 말랑? 주로 장보러 가요",
          "speaker_id": "2",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100031099.1.14",
          "form": "장은 안볼수없죠 ㅎ",
          "original_form": "장은 안볼수없죠 ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100031099.1.15",
          "form": "장보는것도 나름 많이 움직이니까요",
          "original_form": "장보는것도 나름 많이 움직이니까요",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100031099.1.16",
          "form": "맞아요ㅋㅋㅋ 힘도 써야하고",
          "original_form": "맞아요ㅋㅋㅋ 힘도 써야하고",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031099.1.17",
          "form": "저는 최근에 피부과 다녀왔어요",
          "original_form": "저는 최근에 피부과 다녀왔어요",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031099.1.18",
          "form": "무슨 시술 받으셨어요?",
          "original_form": "무슨 시술 받으셨어요?",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031099.1.19",
          "form": "시술은 아니고, 팔 안쪽에 각질처럼 피부가 일어나고",
          "original_form": "시술은 아니고, 팔 안쪽에 각질처럼 피부가 일어나고",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031099.1.20",
          "form": "반점이 생겨서요",
          "original_form": "반점이 생겨서요",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031099.1.21",
          "form": "병명은 모르겠는데",
          "original_form": "병명은 모르겠는데",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031099.1.22",
          "form": "약 먹고 바르니까 나았어요",
          "original_form": "약 먹고 바르니까 나았어요",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031099.1.23",
          "form": "헉 나았다니까 다행이네요",
          "original_form": "헉 나았다니까 다행이네요",
          "speaker_id": "1",
          "time": "20210904 18:08"
        }
      ]
