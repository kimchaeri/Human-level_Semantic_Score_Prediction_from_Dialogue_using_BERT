      "utterance": [
        {
          "id": "MDRW2100030681.1.1",
          "form": "이상형있으세요?",
          "original_form": "이상형있으세요?",
          "speaker_id": "1",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030681.1.2",
          "form": "어 저는 대화가 통화는 사람이요!!",
          "original_form": "어 저는 대화가 통화는 사람이요!!",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030681.1.3",
          "form": "이상형이 있으신가요?",
          "original_form": "이상형이 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030681.1.4",
          "form": "예전엔 외형적 이상형이었는데",
          "original_form": "예전엔 외형적 이상형이었는데",
          "speaker_id": "1",
          "time": "20210904 11:47"
        },
        {
          "id": "MDRW2100030681.1.5",
          "form": "요즘은 날 설레게 하는 사람 인것같아요",
          "original_form": "요즘은 날 설레게 하는 사람 인것같아요",
          "speaker_id": "1",
          "time": "20210904 11:47"
        },
        {
          "id": "MDRW2100030681.1.6",
          "form": "설레게 하는 사람을 만나기 쉽지않죠.. ㅠㅠ",
          "original_form": "설레게 하는 사람을 만나기 쉽지않죠.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100030681.1.7",
          "form": "어떤 포인트가 있으실까요?",
          "original_form": "어떤 포인트가 있으실까요?",
          "speaker_id": "2",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100030681.1.8",
          "form": "글쎄요 멘트나",
          "original_form": "글쎄요 멘트나",
          "speaker_id": "1",
          "time": "20210904 11:53"
        },
        {
          "id": "MDRW2100030681.1.9",
          "form": "아무렇지않게 하는 행동",
          "original_form": "아무렇지않게 하는 행동",
          "speaker_id": "1",
          "time": "20210904 11:53"
        },
        {
          "id": "MDRW2100030681.1.10",
          "form": "요즘은 그런 설레임도 없네요",
          "original_form": "요즘은 그런 설레임도 없네요",
          "speaker_id": "1",
          "time": "20210904 11:53"
        },
        {
          "id": "MDRW2100030681.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:53"
        },
        {
          "id": "MDRW2100030681.1.12",
          "form": "아직까지 없으셨나보네요",
          "original_form": "아직까지 없으셨나보네요",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030681.1.13",
          "form": "전 몇번의 소개팅으로 사람대사람으로 대화가 통하는 사람을 만나기가 어렵다는 ㄱㅓㄹ 알게 되어섴ㅋㄱㅋㅋㅋㅋ",
          "original_form": "전 몇번의 소개팅으로 사람대사람으로 대화가 통하는 사람을 만나기가 어렵다는 ㄱㅓㄹ 알게 되어섴ㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100030681.1.14",
          "form": "데이트는 주로 인도어? 아웃도어 ?",
          "original_form": "데이트는 주로 인도어? 아웃도어 ?",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030681.1.15",
          "form": "아웃도어 쪽이요ㅋㅋㅋㅋ",
          "original_form": "아웃도어 쪽이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030681.1.16",
          "form": "어느쪽이신가요?",
          "original_form": "어느쪽이신가요?",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030681.1.17",
          "form": "저두요 밖에나가서 실내로",
          "original_form": "저두요 밖에나가서 실내로",
          "speaker_id": "1",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030681.1.18",
          "form": "아웃도어쪽이 첨에는 할것도 많고 좋은거 같아요 ㅎㅎ",
          "original_form": "아웃도어쪽이 첨에는 할것도 많고 좋은거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030681.1.19",
          "form": "그쵸 다양하기도 하고",
          "original_form": "그쵸 다양하기도 하고",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030681.1.20",
          "form": "그럼 좀 이르지만 맘에 드는 상대가 나타나면 결혼하실건가요?",
          "original_form": "그럼 좀 이르지만 맘에 드는 상대가 나타나면 결혼하실건가요?",
          "speaker_id": "2",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100030681.1.21",
          "form": "글쎄요 비혼주의까지는 아니지만",
          "original_form": "글쎄요 비혼주의까지는 아니지만",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030681.1.22",
          "form": "결혼에 대한 필요성은 못 느껴서요",
          "original_form": "결혼에 대한 필요성은 못 느껴서요",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030681.1.23",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
