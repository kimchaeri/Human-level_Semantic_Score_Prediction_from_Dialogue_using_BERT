        {
          "id": "MDRW2100021656.1.1",
          "form": "중고거래 해봤어요?",
          "original_form": "중고거래 해봤어요?",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021656.1.2",
          "form": "네!! name2님은요?",
          "original_form": "네!! &name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021656.1.3",
          "form": "저도 해봤는데 좋았어요",
          "original_form": "저도 해봤는데 좋았어요",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021656.1.4",
          "form": "핸드폰 살 때 주는 기본 구성품 필요 없어서 팔았어요",
          "original_form": "핸드폰 살 때 주는 기본 구성품 필요 없어서 팔았어요",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021656.1.5",
          "form": "당근마켓 맞죠?ㅋㅋ",
          "original_form": "당근마켓 맞죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021656.1.6",
          "form": "중고나라에요 ㅎㅎ",
          "original_form": "중고나라에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021656.1.7",
          "form": "당근마켓은 아직 가입도 안해봤어요 ㅠ",
          "original_form": "당근마켓은 아직 가입도 안해봤어요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021656.1.8",
          "form": "오.. 용감하시네요 ㅋㅋ",
          "original_form": "오.. 용감하시네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021656.1.9",
          "form": "엥 왜 용감해용?ㅋㅋㅋ",
          "original_form": "엥 왜 용감해용?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021656.1.10",
          "form": "저는 중고나라 무서워서 ㅋㅋ",
          "original_form": "저는 중고나라 무서워서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021656.1.11",
          "form": "직거래로 하거든요 ㅋㅋㅋ",
          "original_form": "직거래로 하거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021656.1.12",
          "form": "아 ㅋㅋ 중고나라인데 직거래로 했어요",
          "original_form": "아 ㅋㅋ 중고나라인데 직거래로 했어요",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021656.1.13",
          "form": "구로역에서 만났네용",
          "original_form": "구로역에서 만났네용",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021656.1.14",
          "form": "아 그럼 다행이시네요 ㅋㅋㅋㅋ\r\n저는 지방이라 무조건 택배해야하거든요 ㅠ",
          "original_form": "아 그럼 다행이시네요 ㅋㅋㅋㅋ\r\n저는 지방이라 무조건 택배해야하거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021656.1.15",
          "form": "ㅠㅠ 지방이면 직거래도 어렵겠네요 그쵸?",
          "original_form": "ㅠㅠ 지방이면 직거래도 어렵겠네요 그쵸?",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021656.1.16",
          "form": "근데 택배거래도 되게 사기 많다면서요 ㅜㅜ",
          "original_form": "근데 택배거래도 되게 사기 많다면서요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021656.1.17",
          "form": "그래서 당근으로하죠 ㅋㅋㅋ\r\n당근에는 동네 사람이 진짜많아요",
          "original_form": "그래서 당근으로하죠 ㅋㅋㅋ\r\n당근에는 동네 사람이 진짜많아요",
          "speaker_id": "1",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021656.1.18",
          "form": "중고나라에는 잘 보이지도 않던데",
