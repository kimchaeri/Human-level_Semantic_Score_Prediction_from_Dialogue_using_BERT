      "utterance": [
        {
          "id": "MDRW2100004467.1.1",
          "form": "무슨 수업 좋아하시나요",
          "original_form": "무슨 수업 좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004467.1.2",
          "form": "창체요",
          "original_form": "창체요",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004467.1.3",
          "form": "창체면 뭐하나요?",
          "original_form": "창체면 뭐하나요?",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004467.1.4",
          "form": "저 학교다닐때는 검도했는데",
          "original_form": "저 학교다닐때는 검도했는데",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004467.1.5",
          "form": "영화보거나 등?",
          "original_form": "영화보거나 등?",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004467.1.6",
          "form": "오 재밌겠다",
          "original_form": "오 재밌겠다",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004467.1.7",
          "form": "그밖에 재밌는 일 없어요?",
          "original_form": "그밖에 재밌는 일 없어요?",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004467.1.8",
          "form": "저는 곧 수강신청인데",
          "original_form": "저는 곧 수강신청인데",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004467.1.9",
          "form": "큰일이네요 ^^",
          "original_form": "큰일이네요 ^^",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004467.1.10",
          "form": "저희는 화요일 학교 1.2교시 때마다",
          "original_form": "저희는 화요일 학교 1.2교시 때마다",
          "speaker_id": "2",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004467.1.11",
          "form": "배드민턴을 하는데",
          "original_form": "배드민턴을 하는데",
          "speaker_id": "2",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004467.1.12",
          "form": "최근 여름때 해가지고",
          "original_form": "최근 여름때 해가지고",
          "speaker_id": "2",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004467.1.13",
          "form": "한시간 만에 쓰러질뻔 했거든요",
          "original_form": "한시간 만에 쓰러질뻔 했거든요",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004467.1.14",
          "form": "물을 안가져와서",
          "original_form": "물을 안가져와서",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004467.1.15",
          "form": "그래서 친구 물 마실려고했는데",
          "original_form": "그래서 친구 물 마실려고했는데",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004467.1.16",
          "form": "친구가 물을 다마셨어요..",
          "original_form": "친구가 물을 다마셨어요..",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004467.1.17",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004467.1.18",
          "form": "...ㅜㅜ",
          "original_form": "...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004467.1.19",
          "form": "여름에는 진짜.. 몸조심해야해요 ㅠㅠ",
          "original_form": "여름에는 진짜.. 몸조심해야해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004467.1.20",
          "form": "학교에서 좀",
          "original_form": "학교에서 좀",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.21",
          "form": "에어컨을 빵빵허게 틀어주고",
          "original_form": "에어컨을 빵빵허게 틀어주고",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.22",
          "form": "맞어요",
          "original_form": "맞어요",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.23",
          "form": "실내에 있게햐야하는대",
          "original_form": "실내에 있게햐야하는대",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.24",
          "form": "체육관에서 했을땐",
          "original_form": "체육관에서 했을땐",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.25",
          "form": "정말..",
          "original_form": "정말..",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.26",
          "form": "안틀어줘요?",
          "original_form": "안틀어줘요?",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.27",
          "form": "천국이 따로없었어요",
          "original_form": "천국이 따로없었어요",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.28",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.29",
          "form": "그날씨에",
          "original_form": "그날씨에",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.30",
          "form": "밖에서 하는건",
          "original_form": "밖에서 하는건",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004467.1.31",
          "form": "진짜 너무 했내요",
          "original_form": "진짜 너무 했내요",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004467.1.32",
          "form": "동아리같은건 뭐 안해요?",
          "original_form": "동아리같은건 뭐 안해요?",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004467.1.33",
          "form": "쿠킹?",
          "original_form": "쿠킹?",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004467.1.34",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004467.1.35",
          "form": "그런거하면 뭐만들어요?",
          "original_form": "그런거하면 뭐만들어요?",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004467.1.36",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004467.1.37",
          "form": "쿠키 머핀 빵 등?",
          "original_form": "쿠키 머핀 빵 등?",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004467.1.38",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004467.1.39",
          "form": "지금 전 배우고싶어도 ㅠㅠ",
          "original_form": "지금 전 배우고싶어도 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004467.1.40",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004467.1.41",
          "form": "돈도 그렇고 장소도 마땅치가 않네요",
          "original_form": "돈도 그렇고 장소도 마땅치가 않네요",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004467.1.42",
          "form": "요리왕이 되겠다는 꿈은",
          "original_form": "요리왕이 되겠다는 꿈은",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004467.1.43",
          "form": "이룰수가 없어요",
          "original_form": "이룰수가 없어요",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004467.1.44",
          "form": "제가 따로 집에서 만들수 있는 간단한 요리가",
          "original_form": "제가 따로 집에서 만들수 있는 간단한 요리가",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004467.1.45",
          "form": "오므라이스 밖에 없어요",
          "original_form": "오므라이스 밖에 없어요",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004467.1.46",
          "form": "요리왕ㅋㅋ",
          "original_form": "요리왕ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004467.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004467.1.48",
          "form": "저도 별반 다르진 않넹ㅅ",
          "original_form": "저도 별반 다르진 않넹ㅅ",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004467.1.49",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004467.1.50",
          "form": "학교다닐때 더 배워보면 좋았을텐대",
          "original_form": "학교다닐때 더 배워보면 좋았을텐대",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004467.1.51",
          "form": "저는 공학계열이라 전혀 연관이 없거든요",
          "original_form": "저는 공학계열이라 전혀 연관이 없거든요",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004467.1.52",
          "form": "공학이라..ㅇㅅㅇ",
          "original_form": "공학이라..ㅇㅅㅇ",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004467.1.53",
          "form": "하기싫어서 안했다가",
          "original_form": "하기싫어서 안했다가",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004467.1.54",
          "form": "졸업학년돼서.. ㅎ",
          "original_form": "졸업학년돼서.. ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004467.1.55",
          "form": "꼭 동아리 하세요",
          "original_form": "꼭 동아리 하세요",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004467.1.56",
          "form": "ㅋㅋ네",
          "original_form": "ㅋㅋ네",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004467.1.57",
          "form": "재밌을거예요",
          "original_form": "재밌을거예요",
          "speaker_id": "1",
          "time": "20210821 17:07"
        }
      ]
