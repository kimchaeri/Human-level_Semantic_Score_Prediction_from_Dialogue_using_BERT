        },
        {
          "id": "MDRW2100018538.1.2",
          "form": "맞아뇨",
          "original_form": "맞아뇨",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100018538.1.3",
          "form": "비좀 그만 왔으면 좋겠네요",
