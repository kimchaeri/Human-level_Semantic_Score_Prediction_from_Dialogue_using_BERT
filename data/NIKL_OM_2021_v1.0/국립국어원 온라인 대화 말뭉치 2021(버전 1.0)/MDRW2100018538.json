          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100018538.1.4",
          "form": "지겨워요",
          "original_form": "지겨워요",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100018538.1.5",
          "form": "가을 장마 지겹네요",
