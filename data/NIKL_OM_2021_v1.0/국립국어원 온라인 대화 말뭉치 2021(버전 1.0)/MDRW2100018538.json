          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100018538.1.6",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100018538.1.7",
          "form": "밝은 해가 부럽네요",
          "original_form": "밝은 해가 부럽네요",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100018538.1.8",
          "form": "덥지만않으면",
          "original_form": "덥지만않으면",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100018538.1.9",
          "form": "가을이라 좀 선선해요",
          "original_form": "가을이라 좀 선선해요",
          "speaker_id": "2",
          "time": "20210828 09:03"
        },
        {
          "id": "MDRW2100018538.1.10",
          "form": "그렇죠",
