      "utterance": [
        {
          "id": "MDRW2100001463.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211014 17:51"
        },
        {
          "id": "MDRW2100001463.1.2",
          "form": "안녕하세요~ 혹시 백신 맞으셨나요?",
          "original_form": "안녕하세요~ 혹시 백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20211014 17:52"
        },
        {
          "id": "MDRW2100001463.1.3",
          "form": "1차만 맞았어요",
          "original_form": "1차만 맞았어요",
          "speaker_id": "1",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001463.1.4",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "1",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001463.1.5",
          "form": "화이자랑 모더나 중에 어떤걸로 맞으셨어요?",
          "original_form": "화이자랑 모더나 중에 어떤걸로 맞으셨어요?",
          "speaker_id": "2",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001463.1.6",
          "form": "화이자 1차맞았습니다",
          "original_form": "화이자 1차맞았습니다",
          "speaker_id": "1",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001463.1.7",
          "form": "아 화이자..",
          "original_form": "아 화이자..",
          "speaker_id": "2",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001463.1.8",
          "form": "혹시 부작용은 없으셨나요?",
          "original_form": "혹시 부작용은 없으셨나요?",
          "speaker_id": "2",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001463.1.9",
          "form": "실망하셨나요?",
          "original_form": "실망하셨나요?",
          "speaker_id": "1",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001463.1.10",
          "form": "몸좀 아프긴했는데 병원갔더니",
          "original_form": "몸좀 아프긴했는데 병원갔더니",
          "speaker_id": "1",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001463.1.11",
          "form": "괜찮다고 했어요",
          "original_form": "괜찮다고 했어요",
          "speaker_id": "1",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001463.1.12",
          "form": "실망했다뇨 ㅠㅠ ㅋㅋㅋㅋㅋ 아니에요",
          "original_form": "실망했다뇨 ㅠㅠ ㅋㅋㅋㅋㅋ 아니에요",
          "speaker_id": "2",
          "time": "20211014 17:55"
        },
        {
          "id": "MDRW2100001463.1.13",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20211014 17:55"
        },
        {
          "id": "MDRW2100001463.1.14",
          "form": "병원까지 가셨구나",
          "original_form": "병원까지 가셨구나",
          "speaker_id": "2",
          "time": "20211014 17:55"
        },
        {
          "id": "MDRW2100001463.1.15",
          "form": "뭔가 2차 맞기 겁나시겠어요",
          "original_form": "뭔가 2차 맞기 겁나시겠어요",
          "speaker_id": "2",
          "time": "20211014 17:56"
        },
        {
          "id": "MDRW2100001463.1.16",
          "form": "네 뭐 그렇긴한데 맞아야죠 뭐",
          "original_form": "네 뭐 그렇긴한데 맞아야죠 뭐",
          "speaker_id": "1",
          "time": "20211014 17:56"
        },
        {
          "id": "MDRW2100001463.1.17",
          "form": "19일날 맞을려구요",
          "original_form": "19일날 맞을려구요",
          "speaker_id": "1",
          "time": "20211014 17:56"
        },
        {
          "id": "MDRW2100001463.1.18",
          "form": "이번엔 맞고 괜찮으실거에요!!",
          "original_form": "이번엔 맞고 괜찮으실거에요!!",
          "speaker_id": "2",
          "time": "20211014 17:59"
        },
        {
          "id": "MDRW2100001463.1.19",
          "form": "20대 백신 접종율이 다른 연령대에 비해 낮긴 낮더라구요",
          "original_form": "20대 백신 접종율이 다른 연령대에 비해 낮긴 낮더라구요",
          "speaker_id": "2",
          "time": "20211014 17:59"
        },
        {
          "id": "MDRW2100001463.1.20",
          "form": "사망 뉴스도 들리니까",
          "original_form": "사망 뉴스도 들리니까",
          "speaker_id": "2",
          "time": "20211014 17:59"
        },
        {
          "id": "MDRW2100001463.1.21",
          "form": "겁날 거 같긴해요",
          "original_form": "겁날 거 같긴해요",
          "speaker_id": "2",
          "time": "20211014 18:00"
        },
        {
          "id": "MDRW2100001463.1.22",
          "form": "아 네 감사합니다.",
          "original_form": "아 네 감사합니다.",
          "speaker_id": "1",
          "time": "20211014 18:00"
        },
        {
          "id": "MDRW2100001463.1.23",
          "form": "뭐 그렇죠.....",
          "original_form": "뭐 그렇죠.....",
          "speaker_id": "1",
          "time": "20211014 18:00"
        },
        {
          "id": "MDRW2100001463.1.24",
          "form": "그래도 많이들 맞으시니까요",
          "original_form": "그래도 많이들 맞으시니까요",
          "speaker_id": "1",
          "time": "20211014 18:00"
        },
        {
          "id": "MDRW2100001463.1.25",
          "form": "개인의 자유기도 하고",
          "original_form": "개인의 자유기도 하고",
          "speaker_id": "1",
          "time": "20211014 18:00"
        },
        {
          "id": "MDRW2100001463.1.26",
          "form": "그쵸.. 빨리 치료제가 나와서",
          "original_form": "그쵸.. 빨리 치료제가 나와서",
          "speaker_id": "2",
          "time": "20211014 18:05"
        },
        {
          "id": "MDRW2100001463.1.27",
          "form": "코로나 걱정 그만하고싶네요 ㅠㅠ",
          "original_form": "코로나 걱정 그만하고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 18:05"
        },
        {
          "id": "MDRW2100001463.1.28",
          "form": "네 곧 나온다던데... 그때까지 힘내요!!",
          "original_form": "네 곧 나온다던데... 그때까지 힘내요!!",
