      "utterance": [
        {
          "id": "MDRW2100038572.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100038572.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100038572.1.3",
          "form": "점심 뭐드샸나요??",
          "original_form": "점심 뭐드샸나요??",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100038572.1.4",
          "form": "집에있는 반찬들에 계란후라이해서 간단하게 먹었어요ㅎㅎ",
          "original_form": "집에있는 반찬들에 계란후라이해서 간단하게 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038572.1.5",
          "form": "저는 즉석밥 먹었어요!",
          "original_form": "저는 즉석밥 먹었어요!",
          "speaker_id": "1",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100038572.1.6",
          "form": "저녁은 쌀국수요 ㅎㅎ",
          "original_form": "저녁은 쌀국수요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100038572.1.7",
          "form": "오 쌀국수드셨군요\r\n저는 저녁 된장찌개와 밑반찬들 먹을꺼 같아요ㅎㅎ",
          "original_form": "오 쌀국수드셨군요\r\n저는 저녁 된장찌개와 밑반찬들 먹을꺼 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038572.1.8",
          "form": "맛있겠네요!!",
          "original_form": "맛있겠네요!!",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100038572.1.9",
          "form": "저도 된장찌개가 먹고싶네요ㅜㅠ",
          "original_form": "저도 된장찌개가 먹고싶네요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100038572.1.10",
          "form": "된장찌개 드신지 오래되었나봐요",
          "original_form": "된장찌개 드신지 오래되었나봐요",
          "speaker_id": "2",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100038572.1.11",
          "form": "제가 한게 먹고싶어요 ㅋㅋㅋㅋ 청국장이랑 섞어서 끓인 것...",
          "original_form": "제가 한게 먹고싶어요 ㅋㅋㅋㅋ 청국장이랑 섞어서 끓인 것...",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038572.1.12",
          "form": "좀 돼긴 했네요ㅠㅠ 여름엔 불옆에 안가다보니...",
          "original_form": "좀 돼긴 했네요ㅠㅠ 여름엔 불옆에 안가다보니...",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100038572.1.13",
          "form": "된장찌개 끓일때 청국장과 섞어 끓이시는군요!",
          "original_form": "된장찌개 끓일때 청국장과 섞어 끓이시는군요!",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100038572.1.14",
          "form": "네! 제가 좀 걸쭉한 된장찌개를 좋아해서요!",
          "original_form": "네! 제가 좀 걸쭉한 된장찌개를 좋아해서요!",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100038572.1.15",
          "form": "짜지않은 청국장 넣고 된장 섞어 끓이면 맛있어요 ㅋㅋㅋㅋ",
          "original_form": "짜지않은 청국장 넣고 된장 섞어 끓이면 맛있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100038572.1.16",
          "form": "오 그렇군요!  새로운 조합을 알게 되었네요ㅎㅎ",
          "original_form": "오 그렇군요!  새로운 조합을 알게 되었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100038572.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 저는 된장에 들은 콩도 좋아하거등요..!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저는 된장에 들은 콩도 좋아하거등요..!",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038572.1.18",
          "form": "찌개에 들어있는게 좋아요...",
          "original_form": "찌개에 들어있는게 좋아요...",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038572.1.19",
          "form": "아! 그 콩 좋아하시는군요",
          "original_form": "아! 그 콩 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038572.1.20",
          "form": "저는 콩은 안 좋아해요ㅎㅎ",
          "original_form": "저는 콩은 안 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038572.1.21",
          "form": "저도 강낭콩은 안좋아해욬ㅋㅋㅋㅋ",
          "original_form": "저도 강낭콩은 안좋아해욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100038572.1.22",
          "form": "팥도 뭐... 별로...",
          "original_form": "팥도 뭐... 별로...",
          "speaker_id": "1",
          "time": "20210902 21:52"
        }
      ]
    }
