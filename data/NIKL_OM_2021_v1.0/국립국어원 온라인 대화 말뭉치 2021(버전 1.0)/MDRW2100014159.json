      "utterance": [
        {
          "id": "MDRW2100014159.1.1",
          "form": "오늘 저녁 뭐드세요",
          "original_form": "오늘 저녁 뭐드세요",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014159.1.2",
          "form": "어제끓여놓은 소고기 국 이요",
          "original_form": "어제끓여놓은 소고기 국 이요",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014159.1.3",
          "form": "뭐드실거에요?",
          "original_form": "뭐드실거에요?",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014159.1.4",
          "form": "저녁에 회아님 고기를 먹을까 생각중이에요!",
          "original_form": "저녁에 회아님 고기를 먹을까 생각중이에요!",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014159.1.5",
          "form": "저녁 몇시에 드세요?",
          "original_form": "저녁 몇시에 드세요?",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014159.1.6",
          "form": "6시30분쯤이요",
          "original_form": "6시30분쯤이요",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014159.1.7",
          "form": "회맛나겠어요",
          "original_form": "회맛나겠어요",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014159.1.8",
          "form": "네!! 연어를 먹을려고요",
          "original_form": "네!! 연어를 먹을려고요",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014159.1.9",
          "form": "회좋아하세요?",
          "original_form": "회좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014159.1.10",
          "form": "네 광어좋아해요 ㅎ",
          "original_form": "네 광어좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014159.1.11",
          "form": "회초밥도 먹고싶네요",
          "original_form": "회초밥도 먹고싶네요",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014159.1.12",
          "form": "우아 광어 ㅎㅎ 해산물 좋아하세요?",
          "original_form": "우아 광어 ㅎㅎ 해산물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014159.1.13",
          "form": "네 좋아해요\n연어도 맛있겠어요.. 연어는 양파와 흰소스랑 먹는게 맛있더라구요",
          "original_form": "네 좋아해요\n연어도 맛있겠어요.. 연어는 양파와 흰소스랑 먹는게 맛있더라구요",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014159.1.14",
          "form": "오늘 커피는 한잔하셨어요? 전 아메리카노 한잔중이에요",
          "original_form": "오늘 커피는 한잔하셨어요? 전 아메리카노 한잔중이에요",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100014159.1.15",
          "form": "앗 ! 저는 커피를 못마셔서 음료수 시켜먹어요 ㅋㅋ",
          "original_form": "앗 ! 저는 커피를 못마셔서 음료수 시켜먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100014159.1.16",
          "form": "어떤 음료드세요? 저는 주로 아메리카노만 마셔요",
          "original_form": "어떤 음료드세요? 저는 주로 아메리카노만 마셔요",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100014159.1.17",
          "form": "저는 녹차라떼나 초코 들어간거 좋아해요!!",
          "original_form": "저는 녹차라떼나 초코 들어간거 좋아해요!!",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100014159.1.18",
          "form": "아메리카노는 너무 쓰더라고요 ㅜ",
          "original_form": "아메리카노는 너무 쓰더라고요 ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100014159.1.19",
          "form": "아 그렇죠 커피안좋아하시면 맛없죠",
          "original_form": "아 그렇죠 커피안좋아하시면 맛없죠",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100014159.1.20",
          "form": "녹차라떼도 맛나죵 ㅎ",
          "original_form": "녹차라떼도 맛나죵 ㅎ",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100014159.1.21",
          "form": "맞아요!! 휘핑 올라간거 좋아하세요?",
          "original_form": "맞아요!! 휘핑 올라간거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100014159.1.22",
          "form": "오 단거 안좋아하세요?",
          "original_form": "오 단거 안좋아하세요?",
