      "utterance": [
        {
          "id": "MDRW2100040035.1.1",
          "form": "등극한지 얼마 되지도 않은 유리멘탈 신 하나를 못이겨야하냐그ㆍ",
          "original_form": "등극한지 얼마 되지도 않은 유리멘탈 신 하나를 못이겨야하냐그ㆍ",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100040035.1.2",
          "form": "아니 애초에 그럼 그때",
          "original_form": "아니 애초에 그럼 그때",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100040035.1.3",
          "form": "심상세계로는",
          "original_form": "심상세계로는",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100040035.1.4",
          "form": "왜 데려간건데",
          "original_form": "왜 데려간건데",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100040035.1.5",
          "form": "난 거기가 라이덴의 처형식장이라도",
          "original_form": "난 거기가 라이덴의 처형식장이라도",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100040035.1.6",
          "form": "되는줄 알았지",
          "original_form": "되는줄 알았지",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100040035.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100040035.1.8",
          "form": "그리고 진심",
          "original_form": "그리고 진심",
          "speaker_id": "1",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100040035.1.9",
          "form": "허뮤하게 끝남",
          "original_form": "허뮤하게 끝남",
          "speaker_id": "1",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100040035.1.10",
          "form": "검열당한거 아니냔 얘기도 도나봐",
          "original_form": "검열당한거 아니냔 얘기도 도나봐",
          "speaker_id": "1",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100040035.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100040035.1.12",
          "form": "어쩐지 소재가 너무 맵다 했어......",
          "original_form": "어쩐지 소재가 너무 맵다 했어......",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100040035.1.13",
          "form": "왜냐면 ㅋㅋㅋㅋ 사실 검열당할 수도 있을거같은게",
          "original_form": "왜냐면 ㅋㅋㅋㅋ 사실 검열당할 수도 있을거같은게",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040035.1.14",
          "form": "멘스를 다 보면 소재가....",
          "original_form": "멘스를 다 보면 소재가....",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040035.1.15",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040035.1.16",
          "form": "저게 억케 나왔지? 싶은게",
          "original_form": "저게 억케 나왔지? 싶은게",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040035.1.17",
          "form": "사실 몬드부터 그러긴",
          "original_form": "사실 몬드부터 그러긴",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040035.1.18",
          "form": "했어....",
          "original_form": "했어....",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040035.1.19",
          "form": "ㅇㅇ 그치 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 그치 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040035.1.20",
          "form": "근데 몬드나 리월은 그나마",
          "original_form": "근데 몬드나 리월은 그나마",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040035.1.21",
          "form": "벤티랑 종려가 다 착한 역할로 나오고",
          "original_form": "벤티랑 종려가 다 착한 역할로 나오고",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.22",
          "form": "최소 멍청하게 나오진 않아서 살아남은거같고",
          "original_form": "최소 멍청하게 나오진 않아서 살아남은거같고",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.23",
          "form": "대놓고 정부랑 저항군이랑 붙는게",
          "original_form": "대놓고 정부랑 저항군이랑 붙는게",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.24",
          "form": "검열당해서",
          "original_form": "검열당해서",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.25",
          "form": "저렇게 돌린건가?",
          "original_form": "저렇게 돌린건가?",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.26",
          "form": "이나즈마는 악역에 사람 억압하는 역할이라",
          "original_form": "이나즈마는 악역에 사람 억압하는 역할이라",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.27",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.28",
          "form": "반군이랑 저항군 문제가 아니고",
          "original_form": "반군이랑 저항군 문제가 아니고",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.29",
          "form": "멘스 더 보면",
          "original_form": "멘스 더 보면",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.30",
          "form": "엥??? 이게 이렇게 된다고???",
          "original_form": "엥??? 이게 이렇게 된다고???",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.31",
          "form": "ㄱ그ㄹㅐ......",
          "original_form": "ㄱ그ㄹㅐ......",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.32",
          "form": "갑자기??? 니가 왜???",
          "original_form": "갑자기??? 니가 왜???",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.33",
          "form": "일단 더 봐볼게",
          "original_form": "일단 더 봐볼게",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.34",
          "form": "이걸로 끝난다고?? 진짜??",
          "original_form": "이걸로 끝난다고?? 진짜??",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.35",
          "form": "하게 됨",
          "original_form": "하게 됨",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040035.1.36",
          "form": "ㄴㅏ 약간",
          "original_form": "ㄴㅏ 약간",
          "speaker_id": "2",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100040035.1.37",
          "form": "박치는데",
          "original_form": "박치는데",
          "speaker_id": "2",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100040035.1.38",
          "form": "이걸 억케 깨란거지......?",
          "original_form": "이걸 억케 깨란거지......?",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100040035.1.39",
          "form": "왜 ㅋㅋㅋㅋㅋ",
          "original_form": "왜 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100040035.1.40",
          "form": "아니 10대 안맞는건",
          "original_form": "아니 10대 안맞는건",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
