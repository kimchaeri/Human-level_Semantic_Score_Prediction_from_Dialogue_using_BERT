      "utterance": [
        {
          "id": "MDRW2100002634.1.1",
          "form": "요즘 하는 일 있어?",
          "original_form": "요즘 하는 일 있어?",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002634.1.2",
          "form": "요즘 하는 일이",
          "original_form": "요즘 하는 일이",
          "speaker_id": "2",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002634.1.3",
          "form": "그냥 집에서 부업 조금 하는정도?",
          "original_form": "그냥 집에서 부업 조금 하는정도?",
          "speaker_id": "2",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002634.1.4",
          "form": "나중에 군문제 해결하묜",
          "original_form": "나중에 군문제 해결하묜",
          "speaker_id": "2",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002634.1.5",
          "form": "취업하고 본격적으로 해야지",
          "original_form": "취업하고 본격적으로 해야지",
          "speaker_id": "2",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002634.1.6",
          "form": "뭐 할 생각이야?",
          "original_form": "뭐 할 생각이야?",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002634.1.7",
          "form": "물리치료사 자격증이 있으니",
          "original_form": "물리치료사 자격증이 있으니",
          "speaker_id": "2",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002634.1.8",
          "form": "물리치료사 해야지",
          "original_form": "물리치료사 해야지",
          "speaker_id": "2",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002634.1.9",
          "form": "오오~ 나중에 가면 막 너가 물리치료 해주고 그러는거야?",
          "original_form": "오오~ 나중에 가면 막 너가 물리치료 해주고 그러는거야?",
          "speaker_id": "1",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002634.1.10",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002634.1.11",
          "form": "분야는 완전힌 못정했는데",
          "original_form": "분야는 완전힌 못정했는데",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002634.1.12",
          "form": "아마 신경계나 기구쪽 할거같아",
          "original_form": "아마 신경계나 기구쪽 할거같아",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002634.1.13",
          "form": "그럼 물리치료사 말고 알바는 할 생각 있어?",
          "original_form": "그럼 물리치료사 말고 알바는 할 생각 있어?",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002634.1.14",
          "form": "알바를 과거에",
          "original_form": "알바를 과거에",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002634.1.15",
          "form": "편의점 3개월 해봤긴해",
          "original_form": "편의점 3개월 해봤긴해",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002634.1.16",
          "form": "근데 내 스타일은 아닌거같아",
          "original_form": "근데 내 스타일은 아닌거같아",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002634.1.17",
          "form": "사람과 사람은 가능은 하겠는데",
          "original_form": "사람과 사람은 가능은 하겠는데",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002634.1.18",
          "form": "리듬이 깨져서 몸이 자주 아프더라",
          "original_form": "리듬이 깨져서 몸이 자주 아프더라",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002634.1.19",
          "form": "편의점 알바는 할게 못되던데",
          "original_form": "편의점 알바는 할게 못되던데",
          "speaker_id": "1",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002634.1.20",
          "form": "진상은 없었어?",
          "original_form": "진상은 없었어?",
          "speaker_id": "1",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002634.1.21",
          "form": "진상이야 당연히",
          "original_form": "진상이야 당연히",
          "speaker_id": "2",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002634.1.22",
          "form": "있었지",
          "original_form": "있었지",
          "speaker_id": "2",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002634.1.23",
          "form": "어떤 사람이었는데?",
          "original_form": "어떤 사람이었는데?",
          "speaker_id": "1",
          "time": "20210820 14:21"
        },
        {
          "id": "MDRW2100002634.1.24",
          "form": "갑자기 문고리 잠구고 싶다고 때쓰는 사람이나",
          "original_form": "갑자기 문고리 잠구고 싶다고 때쓰는 사람이나",
          "speaker_id": "2",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002634.1.25",
          "form": "술취해서 나한테 욕하고 그냥 나가는 사람이나",
          "original_form": "술취해서 나한테 욕하고 그냥 나가는 사람이나",
          "speaker_id": "2",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002634.1.26",
          "form": "많았지 ㅋㅋㅋ",
          "original_form": "많았지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002634.1.27",
          "form": "진상은 어딜가나 있구나",
          "original_form": "진상은 어딜가나 있구나",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002634.1.28",
          "form": "꽤 힘들었겠네",
          "original_form": "꽤 힘들었겠네",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002634.1.29",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002634.1.30",
          "form": "진상은 어딜가나 있어",
          "original_form": "진상은 어딜가나 있어",
          "speaker_id": "2",
          "time": "20210820 14:25"
        }
      ]
    }
