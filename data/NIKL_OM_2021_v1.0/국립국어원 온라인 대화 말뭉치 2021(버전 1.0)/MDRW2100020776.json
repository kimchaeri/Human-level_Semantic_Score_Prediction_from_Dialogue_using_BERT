      "utterance": [
        {
          "id": "MDRW2100020776.1.1",
          "form": "쌀쌀하면 더 나가기 쉽지 않나요?",
          "original_form": "쌀쌀하면 더 나가기 쉽지 않나요?",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020776.1.2",
          "form": "전 더우니깐 나가기 너무 싫더라구요",
          "original_form": "전 더우니깐 나가기 너무 싫더라구요",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020776.1.3",
          "form": "그런가요? 제가 추위를 많이 타서 그런 것 같아요ㅋㅋ",
          "original_form": "그런가요? 제가 추위를 많이 타서 그런 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020776.1.4",
          "form": "더우면 운동하러 가는 길 자체가 고되더라구요 ㅋㅋㅋ",
          "original_form": "더우면 운동하러 가는 길 자체가 고되더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020776.1.5",
          "form": "ㅋㅋㅋ맞아요 가는 것 자체가 운동이에요",
          "original_form": "ㅋㅋㅋ맞아요 가는 것 자체가 운동이에요",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020776.1.6",
          "form": "사실 저는 추워도 나가기 싫더라구요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사실 저는 추워도 나가기 싫더라구요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020776.1.7",
          "form": "겨울앤 이불 속이 제일 좋아요",
          "original_form": "겨울앤 이불 속이 제일 좋아요",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020776.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020776.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020776.1.10",
          "form": "포근하게 있으면 행복하죠",
          "original_form": "포근하게 있으면 행복하죠",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020776.1.11",
          "form": "날 추워지니까",
          "original_form": "날 추워지니까",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020776.1.12",
          "form": "가을 옷 사고 싶어요 ㅋㅋㅋ",
          "original_form": "가을 옷 사고 싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020776.1.13",
          "form": "ㅋㅋㅋ저도 환절기마다 쇼핑 뽐뿌와요",
          "original_form": "ㅋㅋㅋ저도 환절기마다 쇼핑 뽐뿌와요",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020776.1.14",
          "form": "가을은 짧아서 맘ㅎ이 입지도 못 하는데 그렇게 탐이 나네요 ㅋㅋㅋ",
          "original_form": "가을은 짧아서 맘ㅎ이 입지도 못 하는데 그렇게 탐이 나네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020776.1.15",
          "form": "그쵸 가을은 잠깐이고 겨울 오니까 사기가 꺼려져요ㅋㅋ",
          "original_form": "그쵸 가을은 잠깐이고 겨울 오니까 사기가 꺼려져요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100020776.1.16",
          "form": "맞아요-",
          "original_form": "맞아요-",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100020776.1.17",
          "form": "어차피 겨울엔 코트나 패딩 안으로 들어가니까요ㅠ",
          "original_form": "어차피 겨울엔 코트나 패딩 안으로 들어가니까요ㅠ",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100020776.1.18",
          "form": "그쵸ㅋㅋ그땐 수면잠옷 입고 롱패딩 입어도 아무도 모르니까 좋아요",
          "original_form": "그쵸ㅋㅋ그땐 수면잠옷 입고 롱패딩 입어도 아무도 모르니까 좋아요",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100020776.1.19",
          "form": "내일은 떠 비가 온다네요",
          "original_form": "내일은 떠 비가 온다네요",
          "speaker_id": "2",
