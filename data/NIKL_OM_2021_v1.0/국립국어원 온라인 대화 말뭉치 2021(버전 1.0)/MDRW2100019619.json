        {
          "id": "MDRW2100019619.1.1",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019619.1.2",
          "form": "네 완전 좋아해요",
          "original_form": "네 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019619.1.3",
          "form": "콘솔 좋아하시는 편인가요?",
          "original_form": "콘솔 좋아하시는 편인가요?",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019619.1.4",
          "form": "콘솔게임도 좋아해요",
          "original_form": "콘솔게임도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019619.1.5",
          "form": "플스 ?스위치?. 어떤거하시나요?",
          "original_form": "플스 ?스위치?. 어떤거하시나요?",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019619.1.6",
          "form": "음.. 스위치 많이 했었어요",
          "original_form": "음.. 스위치 많이 했었어요",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019619.1.7",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019619.1.8",
          "form": "저는 본가에 플스있구요 지금 스위치산지 반년도 안되었어요",
          "original_form": "저는 본가에 플스있구요 지금 스위치산지 반년도 안되었어요",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019619.1.9",
          "form": "스위치 게임 추천가능하실까요...?",
          "original_form": "스위치 게임 추천가능하실까요...?",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019619.1.10",
          "form": "음... 젤다는 당연히 하셨을것같고",
          "original_form": "음... 젤다는 당연히 하셨을것같고",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019619.1.11",
          "form": "그죠!젤다기기니까요",
          "original_form": "그죠!젤다기기니까요",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019619.1.12",
          "form": "동물의 숲 해보셨나요?",
          "original_form": "동물의 숲 해보셨나요?",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019619.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 젤다기기죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 젤다기기죠",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019619.1.14",
          "form": "동숲도 하고있어요",
          "original_form": "동숲도 하고있어요",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019619.1.15",
          "form": "링피트도 하고있구요",
          "original_form": "링피트도 하고있구요",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019619.1.16",
          "form": "딱 3개있습니다",
          "original_form": "딱 3개있습니다",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019619.1.17",
          "form": "몬스터헌터 스토리즈 추천드려요",
          "original_form": "몬스터헌터 스토리즈 추천드려요",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019619.1.18",
          "form": "몬헌은 pc로 접해보셨으면",
          "original_form": "몬헌은 pc로 접해보셨으면",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019619.1.19",
          "form": "더 쉽게 접근할수있을거에요!",
          "original_form": "더 쉽게 접근할수있을거에요!",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019619.1.20",
          "form": "몬헌시리즈 좋아하시는구나",
          "original_form": "몬헌시리즈 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019619.1.21",
          "form": "다크소울처럼 매니아 게임이라던데",
          "original_form": "다크소울처럼 매니아 게임이라던데",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019619.1.22",
          "form": "시리즈 쭉 하고계신가요?",
          "original_form": "시리즈 쭉 하고계신가요?",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019619.1.23",
          "form": "아뇨 몬헌월드랑 아이스본만요 ㅎㅎ..",
          "original_form": "아뇨 몬헌월드랑 아이스본만요 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100019619.1.24",
          "form": "글도 충분히 몬헌 매력은 느껴봤어요",
          "original_form": "글도 충분히 몬헌 매력은 느껴봤어요",
