        {
          "id": "MDRW2100047504.1.1",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100047504.1.2",
          "form": "아니요 name2님은요?",
          "original_form": "아니요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100047504.1.3",
          "form": "저는 아이돌 좋아해요 ㅎㅎ",
          "original_form": "저는 아이돌 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100047504.1.4",
          "form": "오 누구요??",
          "original_form": "오 누구요??",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100047504.1.5",
          "form": "방탄소년단 좋아해요 ㅎㅎ !",
          "original_form": "방탄소년단 좋아해요 ㅎㅎ !",
          "speaker_id": "2",
          "time": "20210826 19:07"
