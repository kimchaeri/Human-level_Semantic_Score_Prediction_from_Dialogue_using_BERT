      "utterance": [
        {
          "id": "MDRW2100022972.1.1",
          "form": "초록색 소스는 담백하고 부드러운 맛이에요",
          "original_form": "초록색 소스는 담백하고 부드러운 맛이에요",
          "speaker_id": "1",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100022972.1.2",
          "form": "어느 나라 음식이예요?",
          "original_form": "어느 나라 음식이예요?",
          "speaker_id": "2",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022972.1.3",
          "form": "날카로운 질문이네요",
          "original_form": "날카로운 질문이네요",
          "speaker_id": "1",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022972.1.4",
          "form": "멕시코 음식이에요",
          "original_form": "멕시코 음식이에요",
          "speaker_id": "1",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022972.1.5",
          "form": "정답! 또띠아!",
          "original_form": "정답! 또띠아!",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100022972.1.6",
          "form": "맞나요!!!!!",
          "original_form": "맞나요!!!!!",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100022972.1.7",
          "form": "아니 초록소스가 들어가나..?",
          "original_form": "아니 초록소스가 들어가나..?",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100022972.1.8",
          "form": "또띠아는 음식 재료지 음식이 아니잖아요!",
          "original_form": "또띠아는 음식 재료지 음식이 아니잖아요!",
          "speaker_id": "1",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100022972.1.9",
          "form": "또띠야로 만든 음식이에요 좀만 더 힘내 보세요 ㅋㅋㅋㅋ",
          "original_form": "또띠야로 만든 음식이에요 좀만 더 힘내 보세요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100022972.1.10",
          "form": "뭐라고요 진짜요?!",
          "original_form": "뭐라고요 진짜요?!",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022972.1.11",
          "form": "헐 전 지금까지 또띠아가 음식 자체 이름인줄 알았어요",
          "original_form": "헐 전 지금까지 또띠아가 음식 자체 이름인줄 알았어요",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022972.1.12",
          "form": "아니에요 또띠아는 그냥 납작한 빵이에요",
          "original_form": "아니에요 또띠아는 그냥 납작한 빵이에요",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022972.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022972.1.14",
          "form": "제 상식에 충격을 금치 못하겠어요",
          "original_form": "제 상식에 충격을 금치 못하겠어요",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022972.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022972.1.16",
          "form": "컬쳐쇼크네요",
          "original_form": "컬쳐쇼크네요",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022972.1.17",
          "form": "음 초성힌트 찬스 씁니다",
          "original_form": "음 초성힌트 찬스 씁니다",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
