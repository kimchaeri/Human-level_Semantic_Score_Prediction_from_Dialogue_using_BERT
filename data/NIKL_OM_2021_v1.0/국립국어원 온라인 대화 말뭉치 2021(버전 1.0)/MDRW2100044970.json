      "utterance": [
        {
          "id": "MDRW2100044970.1.1",
          "form": "아 맞어 저 그래서 이마트 케익 ㅓㄱ었어요",
          "original_form": "아 맞어 저 그래서 이마트 케익 ㅓㄱ었어요",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044970.1.2",
          "form": "맛있어요?",
          "original_form": "맛있어요?",
          "speaker_id": "2",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044970.1.3",
          "form": "네 트레이더스 괜찮아요",
          "original_form": "네 트레이더스 괜찮아요",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044970.1.4",
          "form": "싼맛이긴 하지만 ㅋㅋㅋ",
          "original_form": "싼맛이긴 하지만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044970.1.5",
          "form": "전 주변에 이마트가 없어서",
          "original_form": "전 주변에 이마트가 없어서",
          "speaker_id": "2",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044970.1.6",
          "form": "나름 싼맛에 먹을만해요 담에 드셔봐요",
          "original_form": "나름 싼맛에 먹을만해요 담에 드셔봐요",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044970.1.7",
          "form": "그럴께요 ㅋㅋ",
          "original_form": "그럴께요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044970.1.8",
          "form": "케익은 무난하거",
          "original_form": "케익은 무난하거",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044970.1.9",
          "form": "쿠키가 맛있어요",
          "original_form": "쿠키가 맛있어요",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044970.1.10",
          "form": "쿠키 먹고싶다",
          "original_form": "쿠키 먹고싶다",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044970.1.11",
          "form": "살찌는맛 ㅋㅋ",
          "original_form": "살찌는맛 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044970.1.12",
          "form": "오늘 떡먹었어요",
          "original_form": "오늘 떡먹었어요",
