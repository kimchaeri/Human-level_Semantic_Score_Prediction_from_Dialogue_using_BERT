      "utterance": [
        {
          "id": "MDRW2100014777.1.1",
          "form": "안녕하세요 ",
          "original_form": "안녕하세요 ",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014777.1.2",
          "form": "안녕하세요 동호회하시나요?",
          "original_form": "안녕하세요 동호회하시나요?",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014777.1.3",
          "form": "운동동호회 이번에 시작했어요! 어떤동호회하세요?",
          "original_form": "운동동호회 이번에 시작했어요! 어떤동호회하세요?",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014777.1.4",
          "form": "오오 어떤운동이요?전 야구직관동호회요",
          "original_form": "오오 어떤운동이요?전 야구직관동호회요",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014777.1.5",
          "form": "저는 배구요! 월요일에 처음했어요!",
          "original_form": "저는 배구요! 월요일에 처음했어요!",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014777.1.6",
          "form": "우와 배구 멋있네요!",
          "original_form": "우와 배구 멋있네요!",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014777.1.7",
          "form": "올림픽보고 관심이생겨서요!근데 어렵더라구요 ㅎㅎ",
          "original_form": "올림픽보고 관심이생겨서요!근데 어렵더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014777.1.8",
          "form": "우와 추진력대박이시네요!  악 진짜 어려울거같은ㅜ",
          "original_form": "우와 추진력대박이시네요!  악 진짜 어려울거같은ㅜ",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014777.1.9",
          "form": "맞으면 아플거같아요",
          "original_form": "맞으면 아플거같아요",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014777.1.10",
          "form": "같이한친구는 팔뚝에 피멍들었어요 ㅎㅎ",
          "original_form": "같이한친구는 팔뚝에 피멍들었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014777.1.11",
          "form": "어우 듣기만해도 아프네요ㅜ",
          "original_form": "어우 듣기만해도 아프네요ㅜ",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014777.1.12",
          "form": "다른 모임이나 동호회 하시는거 있으세요?",
          "original_form": "다른 모임이나 동호회 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014777.1.13",
          "form": "아 걷기모임이랑 벽화봉사하는데 코로나때문에 잘안나가게됐네요",
          "original_form": "아 걷기모임이랑 벽화봉사하는데 코로나때문에 잘안나가게됐네요",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014777.1.14",
          "form": "다른거하시나요?",
          "original_form": "다른거하시나요?",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014777.1.15",
          "form": "저는 스터디 했었는데 요새 스터디카페도 시간제한이있어서 잘못하고있어요",
          "original_form": "저는 스터디 했었는데 요새 스터디카페도 시간제한이있어서 잘못하고있어요",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014777.1.16",
          "form": "오오 어떤거요?",
          "original_form": "오오 어떤거요?",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014777.1.17",
          "form": "그냥 자격증공부인데 일하면서 하는게 쉽지않네요ㅜ 벽화봉사는 벽화에 그림그리는건가요?!",
          "original_form": "그냥 자격증공부인데 일하면서 하는게 쉽지않네요ㅜ 벽화봉사는 벽화에 그림그리는건가요?!",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014777.1.18",
          "form": "아앗 맞아요ㅜ 일하면서는 힘들죠..",
          "original_form": "아앗 맞아요ㅜ 일하면서는 힘들죠..",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014777.1.19",
          "form": "아 아직 들어간지 얼마안되서 딱 한번갔는데",
          "original_form": "아 아직 들어간지 얼마안되서 딱 한번갔는데",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014777.1.20",
          "form": "그림은 베테랑들이 그려주고 저는 색칠만ㅋ",
          "original_form": "그림은 베테랑들이 그려주고 저는 색칠만ㅋ",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014777.1.21",
          "form": "색칠도 벽화에하려면 어렵지않나요?!",
          "original_form": "색칠도 벽화에하려면 어렵지않나요?!",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100014777.1.22",
          "form": "근데 저도 그럴줄알았는데 동호회에 베테랑분들이 계셔서 다도와주세요",
          "original_form": "근데 저도 그럴줄알았는데 동호회에 베테랑분들이 계셔서 다도와주세요",
          "speaker_id": "2",
          "time": "20210825 22:16"
        }
      ]
    }
