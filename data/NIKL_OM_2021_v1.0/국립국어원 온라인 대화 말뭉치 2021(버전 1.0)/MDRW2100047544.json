      "utterance": [
        {
          "id": "MDRW2100047544.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100047544.1.2",
          "form": "겨울을 잘나주면좋겠네요",
          "original_form": "겨울을 잘나주면좋겠네요",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100047544.1.3",
          "form": "아 맞네요 겨울에는 그럼 수온관리도 해줘야하나요??",
          "original_form": "아 맞네요 겨울에는 그럼 수온관리도 해줘야하나요??",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100047544.1.4",
          "form": "유투부를좀 찾아봐야겠어욯ㅎ 분양받은지 몇달안됐거든요",
          "original_form": "유투부를좀 찾아봐야겠어욯ㅎ 분양받은지 몇달안됐거든요",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100047544.1.5",
          "form": "앗 몇달 안되셨구나 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 몇달 안되셨구나 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100047544.1.6",
          "form": "제가 너무 이것저것 물어봤네욬ㅋㅋㅋ큐ㅠ",
          "original_form": "제가 너무 이것저것 물어봤네욬ㅋㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100047544.1.7",
          "form": "ㅋㅋ아니에요 저드 아는게 없어서 ㅋㅋ 공부해야해요",
          "original_form": "ㅋㅋ아니에요 저드 아는게 없어서 ㅋㅋ 공부해야해요",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100047544.1.8",
          "form": "name2님은 가볍게 머안키우시나요",
          "original_form": "&name2&님은 가볍게 머안키우시나요",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100047544.1.9",
          "form": "저는 아무것도... 안키워욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 아무것도... 안키워욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100047544.1.10",
          "form": "마리모라든지",
          "original_form": "마리모라든지",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100047544.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100047544.1.12",
          "form": "마리모도 안키워요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "마리모도 안키워요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100047544.1.13",
          "form": "예전에 다육식물 키웠었는데",
          "original_form": "예전에 다육식물 키웠었는데",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100047544.1.14",
          "form": "너무 무성하게 자라버려서...",
          "original_form": "너무 무성하게 자라버려서...",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100047544.1.15",
          "form": "다육이 제껀 아픈데",
          "original_form": "다육이 제껀 아픈데",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100047544.1.16",
          "form": "원인을모르겠어요 살려주세요",
          "original_form": "원인을모르겠어요 살려주세요",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100047544.1.17",
          "form": "앜ㅋㅋㅋㅋㅋ 어떻게 아픈가요...??",
          "original_form": "앜ㅋㅋㅋㅋㅋ 어떻게 아픈가요...??",
          "speaker_id": "2",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100047544.1.18",
          "form": "노랗게 말라가요 아래부터ㅜㅜㅜ",
          "original_form": "노랗게 말라가요 아래부터ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:04"
        },
        {
          "id": "MDRW2100047544.1.19",
          "form": "어... 그거 물을 며칠에 한번 주시나요???",
          "original_form": "어... 그거 물을 며칠에 한번 주시나요???",
          "speaker_id": "2",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100047544.1.20",
          "form": "흙이 물이 잘 안빠지는 흙이면",
          "original_form": "흙이 물이 잘 안빠지는 흙이면",
          "speaker_id": "2",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100047544.1.21",
          "form": "뿌리 썩어서 그럴수있어요...",
          "original_form": "뿌리 썩어서 그럴수있어요...",
          "speaker_id": "2",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100047544.1.22",
          "form": "흙을 갈아야할거같은데...",
          "original_form": "흙을 갈아야할거같은데...",
          "speaker_id": "2",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100047544.1.23",
          "form": "혹 그래요?",
          "original_form": "혹 그래요?",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100047544.1.24",
          "form": "전 물부족인가 싶어서..물을..줬는데..",
          "original_form": "전 물부족인가 싶어서..물을..줬는데..",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100047544.1.25",
          "form": "물을...너무 많이 주셨거나...",
          "original_form": "물을...너무 많이 주셨거나...",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100047544.1.26",
          "form": "흙이 물이 잘안빠지는 너무 고운 흙이어서 그럴수있어요",
          "original_form": "흙이 물이 잘안빠지는 너무 고운 흙이어서 그럴수있어요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100047544.1.27",
          "form": "아니면 화분이.. 아래에 구멍이 작아서 물이 안빠지거나...",
          "original_form": "아니면 화분이.. 아래에 구멍이 작아서 물이 안빠지거나...",
          "speaker_id": "2",
          "time": "20210826 20:16"
        }
      ]
