      "utterance": [
        {
          "id": "MDRW2100002450.1.1",
          "form": "점심 식사는 하셨나요?",
          "original_form": "점심 식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100002450.1.2",
          "form": "아뇨 아직이요",
          "original_form": "아뇨 아직이요",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002450.1.3",
          "form": "늦잠을 자서",
          "original_form": "늦잠을 자서",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002450.1.4",
          "form": "대충 씨리얼 먹었어요",
          "original_form": "대충 씨리얼 먹었어요",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002450.1.5",
          "form": "좀있다가 밥 제대로 먹으려구요",
          "original_form": "좀있다가 밥 제대로 먹으려구요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002450.1.6",
          "form": "씨리얼이 간단하게 때우기는 좋죠:)",
          "original_form": "씨리얼이 간단하게 때우기는 좋죠:)",
          "speaker_id": "1",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002450.1.7",
          "form": "제대로 드시는 메뉴는 뭐에요?",
          "original_form": "제대로 드시는 메뉴는 뭐에요?",
          "speaker_id": "1",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002450.1.8",
          "form": "샤브샤브 먹으려구요",
          "original_form": "샤브샤브 먹으려구요",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002450.1.9",
          "form": "본격적으로...",
          "original_form": "본격적으로...",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002450.1.10",
          "form": "오 샤브샤브라니!!!",
          "original_form": "오 샤브샤브라니!!!",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002450.1.11",
          "form": "저도 주말에 먹어야겠어요 ㅋ",
          "original_form": "저도 주말에 먹어야겠어요 ㅋ",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002450.1.12",
          "form": "ㅋㅋ 샤브샤브는 사랑입니다",
          "original_form": "ㅋㅋ 샤브샤브는 사랑입니다",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002450.1.13",
          "form": "오늘은 뭐드시나요",
          "original_form": "오늘은 뭐드시나요",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002450.1.14",
          "form": "평소에 야채 잘 안먹는데\n샤브샤브하면 많이 먹게되서ㅋ",
          "original_form": "평소에 야채 잘 안먹는데\n샤브샤브하면 많이 먹게되서ㅋ",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002450.1.15",
          "form": "저는 아직 못정했어여ㅠㅠㅋ",
          "original_form": "저는 아직 못정했어여ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002450.1.16",
          "form": "헉 이제 점심시간인데",
          "original_form": "헉 이제 점심시간인데",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002450.1.17",
          "form": "빨리 정하셔야겠어요",
          "original_form": "빨리 정하셔야겠어요",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002450.1.18",
          "form": "아침은 뭐드셨나요",
          "original_form": "아침은 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002450.1.19",
          "form": "저도 아침은 간단하게\n빵이랑 우유 먹었어요ㅋㅋ",
          "original_form": "저도 아침은 간단하게\n빵이랑 우유 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002450.1.20",
          "form": "근데 금요일은 열심히 산\n일주일에 대한 보상심리인지\n치킨을 먹어줘야 될거같은\n느낌이 드는건 저뿐인가여ㅋㅋ",
          "original_form": "근데 금요일은 열심히 산\n일주일에 대한 보상심리인지\n치킨을 먹어줘야 될거같은\n느낌이 드는건 저뿐인가여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002450.1.21",
          "form": "ㅋㅋㅋ 치킨은",
          "original_form": "ㅋㅋㅋ 치킨은",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002450.1.22",
          "form": "매일 먹고싶어요....",
          "original_form": "매일 먹고싶어요....",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002450.1.23",
          "form": "너무좋아여",
          "original_form": "너무좋아여",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002450.1.24",
          "form": "치킨도 사랑이쥬",
          "original_form": "치킨도 사랑이쥬{emoji:♡}",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002450.1.25",
          "form": "어디 치킨 좋아하시나여ㅋㅋ",
          "original_form": "어디 치킨 좋아하시나여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002450.1.26",
          "form": "음 다 좋은데....",
          "original_form": "음 다 좋은데....",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002450.1.27",
          "form": "후라이드는 비비큐",
          "original_form": "후라이드는 비비큐",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002450.1.28",
          "form": "양념은 처갓집슈프림양념치킨요",
          "original_form": "양념은 처갓집슈프림양념치킨요",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002450.1.29",
          "form": "홀 통했다통했다",
          "original_form": "홀 통했다통했다",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002450.1.30",
          "form": "비비큐 장난 아니져ㅜㅜㅋㅋ",
          "original_form": "비비큐 장난 아니져ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002450.1.31",
          "form": "비비큐 불매했었는데\n황금올리브 먹고는 비비큐만 먹어여ㅋㅋㅋㅋ",
          "original_form": "비비큐 불매했었는데\n황금올리브 먹고는 비비큐만 먹어여ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002450.1.32",
          "form": "ㅋㅋㅋ 황금올리브는 정말",
          "original_form": "ㅋㅋㅋ 황금올리브는 정말",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002450.1.33",
          "form": "클래스가달라요..ㅠ",
          "original_form": "클래스가달라요..ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002450.1.34",
          "form": "고소하고....",
          "original_form": "고소하고....",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002450.1.35",
          "form": "육즙이...",
          "original_form": "육즙이...",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002450.1.36",
          "form": "클라스가 진짜",
          "original_form": "클라스가 진짜{emoji:♡}{emoji:♡}",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002450.1.37",
          "form": "어느 부위 젤 좋아하세여?",
          "original_form": "어느 부위 젤 좋아하세여?",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002450.1.38",
          "form": "슈프림은 평이 넘 좋아서\n시켜봤었는데 저희 동네가\n잘못하는건지 제 입맛에는 쏘쏘...",
          "original_form": "슈프림은 평이 넘 좋아서\n시켜봤었는데 저희 동네가\n잘못하는건지 제 입맛에는 쏘쏘...",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002450.1.39",
          "form": "전 닭다리요 무조건",
          "original_form": "전 닭다리요 무조건",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002450.1.40",
          "form": "황올 천원만추가하면",
          "original_form": "황올 천원만추가하면",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002450.1.41",
          "form": "닭다리세트 가능해서",
          "original_form": "닭다리세트 가능해서",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002450.1.42",
          "form": "그렇게만 먹어요",
          "original_form": "그렇게만 먹어요",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002450.1.43",
          "form": "아.... 소오오오름ㅋㅋㅋㅋ",
          "original_form": "아.... 소오오오름ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002450.1.44",
          "form": "닉넴부터가 운명 같더라니ㅋㅋ",
          "original_form": "닉넴부터가 운명 같더라니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002450.1.45",
          "form": "ㅋㅋㅋㅋ 동지인가요",
          "original_form": "ㅋㅋㅋㅋ 동지인가요",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002450.1.46",
          "form": "입맛동지",
          "original_form": "입맛동지",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002450.1.47",
          "form": "그러게여ㅋㅋㅋㅋ\n같이 치키니는 못먹겠네여ㅋㅋ",
          "original_form": "그러게여ㅋㅋㅋㅋ\n같이 치키니는 못먹겠네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002450.1.48",
          "form": "퍽퍽이파들과 먹어야\n치키니는 평화로워서ㅋㅋ",
          "original_form": "퍽퍽이파들과 먹어야\n치키니는 평화로워서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002450.1.49",
          "form": "ㅋㅋ맞아요 가슴살 좋아하는 친구가",
          "original_form": "ㅋㅋ맞아요 가슴살 좋아하는 친구가",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002450.1.50",
          "form": "최고의 친구죠",
          "original_form": "최고의 친구죠",
          "speaker_id": "2",
          "time": "20210820 12:47"
        }
      ]
    }
