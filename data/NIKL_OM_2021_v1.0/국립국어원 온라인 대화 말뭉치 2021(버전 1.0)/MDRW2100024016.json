      "utterance": [
        {
          "id": "MDRW2100024016.1.1",
          "form": "name1님은 봉사나 동아리활동해보셨나요",
          "original_form": "&name1&님은 봉사나 동아리활동해보셨나요",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024016.1.2",
          "form": "저는 대학교때요 ㅎㅎ 사회복지전공해서 ㅎㅎ 봉사 매주다녔어요 ㅎㅎ",
          "original_form": "저는 대학교때요 ㅎㅎ 사회복지전공해서 ㅎㅎ 봉사 매주다녔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024016.1.3",
          "form": "와 사회복지사전공이셨군요",
          "original_form": "와 사회복지사전공이셨군요",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024016.1.4",
          "form": "좋은 학과네요 힘들지않으셨나요",
          "original_form": "좋은 학과네요 힘들지않으셨나요",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024016.1.5",
          "form": "네 ㅎㅎ 직장도 전공살려서 십년다녔는데 ㅎㅎ",
          "original_form": "네 ㅎㅎ 직장도 전공살려서 십년다녔는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024016.1.6",
          "form": "적성에 맞았어요 ㅎㅎ",
          "original_form": "적성에 맞았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024016.1.7",
          "form": "근데 결혼하고 그만뒀거든요 너무 한직장 오래 다녀서 약간 질리기도 했고 ㅎㅎ",
          "original_form": "근데 결혼하고 그만뒀거든요 너무 한직장 오래 다녀서 약간 질리기도 했고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024016.1.8",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024016.1.9",
          "form": "멋지시네요",
          "original_form": "멋지시네요",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024016.1.10",
          "form": "근데 듣다보면 name2님이 더 좋은일 많이 하시는것 같아요",
          "original_form": "근데 듣다보면 &name2&님이 더 좋은일 많이 하시는것 같아요",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024016.1.11",
          "form": "십년이나했으면 이제 질릴만두해요",
          "original_form": "십년이나했으면 이제 질릴만두해요",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024016.1.12",
          "form": "네 정말 쉬고 싶더라구요 ㅎㅎ",
          "original_form": "네 정말 쉬고 싶더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024016.1.13",
          "form": "전 그냥 인연닿는대로 해봤을뿐 크게 도움 안됐을거같아요",
          "original_form": "전 그냥 인연닿는대로 해봤을뿐 크게 도움 안됐을거같아요",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024016.1.14",
          "form": "아무것도 안하는 사람들도 있는걸요??",
          "original_form": "아무것도 안하는 사람들도 있는걸요??",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024016.1.15",
          "form": "그런 도움이 모여모여  근데 막상 받는 입장에서는",
          "original_form": "그런 도움이 모여모여  근데 막상 받는 입장에서는",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024016.1.16",
          "form": "정말 감사하고 도움되죠 ㅎㅎ",
          "original_form": "정말 감사하고 도움되죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024016.1.17",
          "form": "ㅋㅋ 그건 또 그들의 인연이죠 뭐",
          "original_form": "ㅋㅋ 그건 또 그들의 인연이죠 뭐",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024016.1.18",
          "form": "도움이 필요한곳에서",
          "original_form": "도움이 필요한곳에서",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024016.1.19",
          "form": "잘쓰이면 오히려 내가 좋더라구요",
          "original_form": "잘쓰이면 오히려 내가 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024016.1.20",
          "form": "맞아요 나누는 기쁨도 크죠",
          "original_form": "맞아요 나누는 기쁨도 크죠",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024016.1.21",
          "form": "그런 행복이 참 값진거 같아요",
          "original_form": "그런 행복이 참 값진거 같아요",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024016.1.22",
          "form": "요샌 코로나라  눈앞이 급하시지만 다들",
          "original_form": "요샌 코로나라  눈앞이 급하시지만 다들",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024016.1.23",
          "form": "형편어려운분들은 더 케어받지못하게된거 같아안타까워요",
