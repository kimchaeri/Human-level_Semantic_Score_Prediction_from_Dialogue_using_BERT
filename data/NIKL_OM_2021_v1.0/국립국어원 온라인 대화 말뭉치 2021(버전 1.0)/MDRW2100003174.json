        {
          "id": "MDRW2100003174.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 20:05"
        },
        {
          "id": "MDRW2100003174.1.2",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:05"
        },
        {
          "id": "MDRW2100003174.1.3",
          "form": "저 지금 피자헛 시켜 먹었어요 ㅋㅋ",
          "original_form": "저 지금 피자헛 시켜 먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:05"
        },
        {
          "id": "MDRW2100003174.1.4",
          "form": "저는 낮에 치킨을 시켜먹었는데 혼자라 다 못먹겠더라구요  피자도 맛있겠네요^^",
          "original_form": "저는 낮에 치킨을 시켜먹었는데 혼자라 다 못먹겠더라구요  피자도 맛있겠네요^^",
          "speaker_id": "1",
          "time": "20210820 20:06"
        },
        {
          "id": "MDRW2100003174.1.5",
          "form": "혼자는 힘들어요 ㅋㅋ",
          "original_form": "혼자는 힘들어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100003174.1.6",
          "form": "가족과 함께 드시나요?",
          "original_form": "가족과 함께 드시나요?",
          "speaker_id": "1",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100003174.1.7",
          "form": "피자를 씬으로 시켰더니 얇고 바삭해서 맛났어요",
          "original_form": "피자를 씬으로 시켰더니 얇고 바삭해서 맛났어요",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100003174.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100003174.1.9",
          "form": "저희도 많이 못먹어요",
          "original_form": "저희도 많이 못먹어요",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100003174.1.10",
          "form": "남겼네요 ㅜㅜ",
          "original_form": "남겼네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100003174.1.11",
          "form": "씬피자는 먹어보았는데 도우가얇아서 좋던데요?",
          "original_form": "씬피자는 먹어보았는데 도우가얇아서 좋던데요?",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100003174.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100003174.1.13",
          "form": "두꺼우면 버리게되서 싫어요",
          "original_form": "두꺼우면 버리게되서 싫어요",
          "speaker_id": "2",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100003174.1.14",
          "form": "저도 두꺼우면버리더라구요",
          "original_form": "저도 두꺼우면버리더라구요",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100003174.1.15",
          "form": "꽁다리 버리게 되니 씬이 최고죠 안버리고",
          "original_form": "꽁다리 버리게 되니 씬이 최고죠 안버리고",
          "speaker_id": "2",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100003174.1.16",
          "form": "역시 그렇죠",
          "original_form": "역시 그렇죠",
          "speaker_id": "2",
          "time": "20210820 20:09"
        },
        {
          "id": "MDRW2100003174.1.17",
          "form": "피자좋아하시나요?",
          "original_form": "피자좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 20:09"
        },
        {
          "id": "MDRW2100003174.1.18",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:09"
        },
        {
          "id": "MDRW2100003174.1.19",
          "form": "저는 원래 한식 좋아하는데 기프티콘이 곧 만료된다고해서 치킨을...",
          "original_form": "저는 원래 한식 좋아하는데 기프티콘이 곧 만료된다고해서 치킨을...",
          "speaker_id": "1",
          "time": "20210820 20:09"
        },
        {
          "id": "MDRW2100003174.1.20",
          "form": "아 기프티콘 꼭 만료전에 써요",
          "original_form": "아 기프티콘 꼭 만료전에 써요",
          "speaker_id": "2",
          "time": "20210820 20:10"
        },
        {
          "id": "MDRW2100003174.1.21",
          "form": "피자도 가족들 다같이 집에있으면 한번 먹어봐야겠어요^^",
          "original_form": "피자도 가족들 다같이 집에있으면 한번 먹어봐야겠어요^^",
          "speaker_id": "1",
          "time": "20210820 20:10"
        },
        {
          "id": "MDRW2100003174.1.22",
          "form": "저 그러다 하나 날렸어요 ㅜㅜ",
          "original_form": "저 그러다 하나 날렸어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 20:10"
        },
        {
          "id": "MDRW2100003174.1.23",
          "form": "네 그러세요 근데 세트 스파게티가 별로 였어요 ㅜㅜ",
          "original_form": "네 그러세요 근데 세트 스파게티가 별로 였어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 20:10"
        },
        {
          "id": "MDRW2100003174.1.24",
          "form": "기간이 너무 짧더라구요",
          "original_form": "기간이 너무 짧더라구요",
          "speaker_id": "1",
          "time": "20210820 20:10"
        },
        {
          "id": "MDRW2100003174.1.25",
          "form": "스파게티는 배달음식이 좀 별로인데가 많은거같아요",
          "original_form": "스파게티는 배달음식이 좀 별로인데가 많은거같아요",
          "speaker_id": "1",
          "time": "20210820 20:11"
        },
        {
          "id": "MDRW2100003174.1.26",
          "form": "맞아요 불기도 하고요",
          "original_form": "맞아요 불기도 하고요",
          "speaker_id": "2",
          "time": "20210820 20:11"
        },
        {
          "id": "MDRW2100003174.1.27",
          "form": "괜히 시켰어요",
          "original_form": "괜히 시켰어요",
          "speaker_id": "2",
          "time": "20210820 20:11"
        },
        {
          "id": "MDRW2100003174.1.28",
          "form": "면요리라 그런가봐요",
          "original_form": "면요리라 그런가봐요",
          "speaker_id": "1",
          "time": "20210820 20:12"
        },
        {
          "id": "MDRW2100003174.1.29",
          "form": "집에서 해먹는게 더 맛있는거같아요",
          "original_form": "집에서 해먹는게 더 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210820 20:12"
        },
        {
          "id": "MDRW2100003174.1.30",
          "form": "맞아요 근데 저보고 음식 실험하지 마래요 ㅋㅋ",
          "original_form": "맞아요 근데 저보고 음식 실험하지 마래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100003174.1.31",
          "form": "실험이라니ㅋㅋ 저도잘못하지만 그래도 요리는 해봐야 실력이 느는거 같아요",
          "original_form": "실험이라니ㅋㅋ 저도잘못하지만 그래도 요리는 해봐야 실력이 느는거 같아요",
          "speaker_id": "1",
          "time": "20210820 20:15"
        }
      ]
