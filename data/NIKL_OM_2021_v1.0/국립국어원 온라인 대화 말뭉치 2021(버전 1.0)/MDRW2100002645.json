      "utterance": [
        {
          "id": "MDRW2100002645.1.1",
          "form": "요새 게임 하는것 있나용 ㅋㅋ",
          "original_form": "요새 게임 하는것 있나용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002645.1.2",
          "form": "우리 전에 하던 게임은..ㅋ 오래가지 못했다 ㅋㅋ",
          "original_form": "우리 전에 하던 게임은..ㅋ 오래가지 못했다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002645.1.3",
          "form": "ㅋㅋㅋㅋㅋ그러하다....",
          "original_form": "ㅋㅋㅋㅋㅋ그러하다....",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002645.1.4",
          "form": "제2의 나라... 안 들어간지..",
          "original_form": "제2의 나라... 안 들어간지..",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002645.1.5",
          "form": "한달넘은듯...ㅠ",
          "original_form": "한달넘은듯...ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002645.1.6",
          "form": "웅.. ㅋㅋ 딱히 큰 재미가 없었다..",
          "original_form": "웅.. ㅋㅋ 딱히 큰 재미가 없었다..",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002645.1.7",
          "form": "우릴 잡아두지 못했숴..",
          "original_form": "우릴 잡아두지 못했숴..",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002645.1.8",
          "form": "ㅋㅋㅋㅋㅋ웅웅.. 뭔가 이쁘긴했는데..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ웅웅.. 뭔가 이쁘긴했는데..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002645.1.9",
          "form": "이쁘기만 했던듯한 느낌.ㅋㅋ",
          "original_form": "이쁘기만 했던듯한 느낌.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002645.1.10",
          "form": "쿠키런은 재밌게 하고 있나요??",
          "original_form": "쿠키런은 재밌게 하고 있나요??",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002645.1.11",
          "form": "웅 그냥 조금씩만ㅋㅋㅋ",
          "original_form": "웅 그냥 조금씩만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002645.1.12",
          "form": "ㅎㅎㅎ제 2의 나라보다 재밌는가 보네용~~",
          "original_form": "ㅎㅎㅎ제 2의 나라보다 재밌는가 보네용~~",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002645.1.13",
          "form": "웅 ㅋㅋㅋ 이게아직도 사람들 많이하기도 하고",
          "original_form": "웅 ㅋㅋㅋ 이게아직도 사람들 많이하기도 하고",
          "speaker_id": "1",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002645.1.14",
          "form": "컨텐츠도 넘 늘어나서;; 제대로하면 넘 힘드러ㅋㅋㅋ",
          "original_form": "컨텐츠도 넘 늘어나서;; 제대로하면 넘 힘드러ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002645.1.15",
          "form": "길드도있고해서 의리로 해야하는ㅋㅋ",
          "original_form": "길드도있고해서 의리로 해야하는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002645.1.16",
          "form": "ㅋㅋㅋ의리ㅋㅋ 끈끈하게 연결되어있군ㅋㅋ",
          "original_form": "ㅋㅋㅋ의리ㅋㅋ 끈끈하게 연결되어있군ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002645.1.17",
          "form": "ㅋㅋㅋ 요새 심슨..머시기 게임은 안하나용",
          "original_form": "ㅋㅋㅋ 요새 심슨..머시기 게임은 안하나용",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002645.1.18",
          "form": "아! 심즈프리플레이~ㅋㅋ",
          "original_form": "아! 심즈프리플레이~ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002645.1.19",
          "form": "생각날때 한번 들어가서 훑어주는데",
          "original_form": "생각날때 한번 들어가서 훑어주는데",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002645.1.20",
          "form": "언 들어간지 2달 넘은듯..ㅋㅋ",
          "original_form": "언 들어간지 2달 넘은듯..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002645.1.21",
          "form": "들어갈때마다 업데이트하랰ㅋㅋ",
