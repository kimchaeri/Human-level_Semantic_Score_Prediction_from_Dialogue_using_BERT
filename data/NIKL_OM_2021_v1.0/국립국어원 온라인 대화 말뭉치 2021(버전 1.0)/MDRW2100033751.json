      "utterance": [
        {
          "id": "MDRW2100033751.1.1",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100033751.1.2",
          "form": "오 네",
          "original_form": "오 네",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100033751.1.3",
          "form": "맘스터치",
          "original_form": "맘스터치",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100033751.1.4",
          "form": "저 자주시켜요",
          "original_form": "저 자주시켜요",
          "speaker_id": "2",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100033751.1.5",
          "form": "두툼하니 치킨 이며 버거 진짜 맛나요",
          "original_form": "두툼하니 치킨 이며 버거 진짜 맛나요",
          "speaker_id": "2",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100033751.1.6",
          "form": "다리도 왕크고 진짜 맛나죠",
          "original_form": "다리도 왕크고 진짜 맛나죠",
          "speaker_id": "2",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100033751.1.7",
          "form": "슈비버거는 안먹어봤어요",
          "original_form": "슈비버거는 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100033751.1.8",
          "form": "무슨맛인가요 ?",
          "original_form": "무슨맛인가요 ?",
          "speaker_id": "2",
          "time": "20210825 00:27"
        },
        {
          "id": "MDRW2100033751.1.9",
          "form": "전 맥도날드 맥모닝도 자주시켜요",
          "original_form": "전 맥도날드 맥모닝도 자주시켜요",
          "speaker_id": "2",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100033751.1.10",
          "form": "맞아요 싸이버거 처음먹고 5년전인가",
          "original_form": "맞아요 싸이버거 처음먹고 5년전인가",
          "speaker_id": "1",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100033751.1.11",
          "form": "진짜놀랐어요ㅋㅋㅋ",
          "original_form": "진짜놀랐어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100033751.1.12",
          "form": "슈비버거는 새우들어간건데 저는 버거킹 쉬림프와퍼보다",
          "original_form": "슈비버거는 새우들어간건데 저는 버거킹 쉬림프와퍼보다",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100033751.1.13",
          "form": "슈비버거가좋더라고요ㅋㅋㅋ",
          "original_form": "슈비버거가좋더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100033751.1.14",
          "form": "오우 맥모닝은 사랑이죠",
          "original_form": "오우 맥모닝은 사랑이죠",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100033751.1.15",
          "form": "오 그렇군료버거킹보다 맛나다니 도전요",
          "original_form": "오 그렇군료버거킹보다 맛나다니 도전요",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100033751.1.16",
          "form": "맥모닝 그 10시반전 달려서 먹는 기분 ㅋㅋ",
          "original_form": "맥모닝 그 10시반전 달려서 먹는 기분 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033751.1.17",
          "form": "머핀 넘좋아해요",
          "original_form": "머핀 넘좋아해요",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033751.1.18",
          "form": "맥모닝핫케이크는 딸이 젤좋아 해요 ㅎㅎ",
          "original_form": "맥모닝핫케이크는 딸이 젤좋아 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100033751.1.19",
          "form": "ㅋㅋㅋ 맥모닝 핫케익맛있죠",
          "original_form": "ㅋㅋㅋ 맥모닝 핫케익맛있죠",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100033751.1.20",
          "form": "먹는얘길하니",
          "original_form": "먹는얘길하니",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100033751.1.21",
          "form": "배고프네요ㅠㅠ",
          "original_form": "배고프네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100033751.1.22",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100033751.1.23",
          "form": "원래 먹는거보고 말하면 배고파요",
          "original_form": "원래 먹는거보고 말하면 배고파요",
          "speaker_id": "2",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100033751.1.24",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100033751.1.25",
          "form": "매운거잘드세요?",
          "original_form": "매운거잘드세요?",
          "speaker_id": "1",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100033751.1.26",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100033751.1.27",
          "form": "전 매운거 못막어요",
          "original_form": "전 매운거 못막어요",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100033751.1.28",
          "form": "닭발 좋아하는데 매워서 못시켜먹어료",
          "original_form": "닭발 좋아하는데 매워서 못시켜먹어료",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100033751.1.29",
          "form": "요새 너무 맵게만 만들어요 ㅜㅜ",
          "original_form": "요새 너무 맵게만 만들어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100033751.1.30",
          "form": "저도 매운걸못먹는답니다ㅠ",
          "original_form": "저도 매운걸못먹는답니다ㅠ",
          "speaker_id": "1",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100033751.1.31",
          "form": "엽떡은 드세요?",
          "original_form": "엽떡은 드세요?",
          "speaker_id": "1",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100033751.1.32",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100033751.1.33",
          "form": "저 진짜 못먹어요 ㅋㅋ",
          "original_form": "저 진짜 못먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100033751.1.34",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100033751.1.35",
          "form": "점점 더 음식들이 못먹게 매워져요 순한맛도 맵고요",
          "original_form": "점점 더 음식들이 못먹게 매워져요 순한맛도 맵고요",
          "speaker_id": "2",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100033751.1.36",
          "form": "순한맛도 맵긴해요ㅠ",
          "original_form": "순한맛도 맵긴해요ㅠ",
          "speaker_id": "1",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100033751.1.37",
          "form": "불닭도 못드시겠네요?!",
          "original_form": "불닭도 못드시겠네요?!",
          "speaker_id": "1",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100033751.1.38",
          "form": "네 못먹어요 ㅋㅋ",
          "original_form": "네 못먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100033751.1.39",
          "form": "넘매워요 혀가 얼얼",
          "original_form": "넘매워요 혀가 얼얼",
          "speaker_id": "2",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100033751.1.40",
          "form": "드세요?",
          "original_form": "드세요?",
          "speaker_id": "2",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100033751.1.41",
          "form": "ㅋㅋㅋ 저는 먹을수는있답니다",
          "original_form": "ㅋㅋㅋ 저는 먹을수는있답니다",
          "speaker_id": "1",
          "time": "20210825 01:17"
        },
        {
          "id": "MDRW2100033751.1.42",
          "form": "근데 맵긴해요ㅠㅠ",
          "original_form": "근데 맵긴해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 01:17"
        },
        {
          "id": "MDRW2100033751.1.43",
          "form": "오 능력자 세요",
          "original_form": "오 능력자 세요",
          "speaker_id": "2",
          "time": "20210825 01:17"
        },
        {
          "id": "MDRW2100033751.1.44",
          "form": "부럽네요 맛나지만 그림의 떡요",
          "original_form": "부럽네요 맛나지만 그림의 떡요",
          "speaker_id": "2",
          "time": "20210825 01:17"
        }
      ]
