      "utterance": [
        {
          "id": "MDRW2100025343.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 09:42"
        },
        {
          "id": "MDRW2100025343.1.2",
          "form": "연애에 관심이있으신가요?",
          "original_form": "연애에 관심이있으신가요?",
          "speaker_id": "1",
          "time": "20210901 09:42"
        },
        {
          "id": "MDRW2100025343.1.3",
          "form": "네 연애하고 싶은데 아직은 만나고 싶은 사람이 없네요",
          "original_form": "네 연애하고 싶은데 아직은 만나고 싶은 사람이 없네요",
          "speaker_id": "2",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025343.1.4",
          "form": "지금 연애중이신가요?",
          "original_form": "지금 연애중이신가요?",
          "speaker_id": "2",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025343.1.5",
          "form": "아~네~저는연애 중입니다아",
          "original_form": "아~네~저는연애 중입니다아",
          "speaker_id": "1",
          "time": "20210901 09:55"
        },
        {
          "id": "MDRW2100025343.1.6",
          "form": "오 얼마나 되셨어요",
          "original_form": "오 얼마나 되셨어요",
          "speaker_id": "2",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100025343.1.7",
          "form": "얼마안됬습니다아  학생시절연애 경험이 있으신가요오",
          "original_form": "얼마안됬습니다아  학생시절연애 경험이 있으신가요오",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025343.1.8",
          "form": "네 남녀공학 나와서 사귀어봤네요",
          "original_form": "네 남녀공학 나와서 사귀어봤네요",
          "speaker_id": "2",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025343.1.9",
          "form": "첫사랑은 언제이신가요",
          "original_form": "첫사랑은 언제이신가요",
          "speaker_id": "2",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025343.1.10",
          "form": "우아~역시 남녀 공학이네요!",
          "original_form": "우아~역시 남녀 공학이네요!",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025343.1.11",
          "form": "첫사랑은잘모르겠어요!",
          "original_form": "첫사랑은잘모르겠어요!",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025343.1.12",
          "form": "기준이 어려워서요",
          "original_form": "기준이 어려워서요",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025343.1.13",
          "form": "저는 초등학교때 좋아하는애 있었네요",
          "original_form": "저는 초등학교때 좋아하는애 있었네요",
          "speaker_id": "2",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025343.1.14",
          "form": "결혼은 몇살쯤 하고싶으세요",
          "original_form": "결혼은 몇살쯤 하고싶으세요",
          "speaker_id": "2",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025343.1.15",
          "form": "돈이 있으면 하고싶긴하네요",
          "original_form": "돈이 있으면 하고싶긴하네요",
          "speaker_id": "1",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025343.1.16",
          "form": "초등학생때...한번씩 있을법허죠",
          "original_form": "초등학생때...한번씩 있을법허죠",
          "speaker_id": "1",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025343.1.17",
          "form": "저두요 사실 돈없어도 하고싶은데 맘에 맞는 사람이 없어요",
          "original_form": "저두요 사실 돈없어도 하고싶은데 맘에 맞는 사람이 없어요",
          "speaker_id": "2",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025343.1.18",
          "form": "ㅋㅋㅋㅋ 다 그런거죠 돈이 최고긴해요...ㅎ",
          "original_form": "ㅋㅋㅋㅋ 다 그런거죠 돈이 최고긴해요...ㅎ",
          "speaker_id": "1",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025343.1.19",
          "form": "맘에 맞는 사람 빨리 찾고싶네요",
          "original_form": "맘에 맞는 사람 빨리 찾고싶네요",
          "speaker_id": "2",
          "time": "20210901 10:10"
        },
        {
          "id": "MDRW2100025343.1.20",
          "form": "소개팅앱같은거는 어떠세요?",
          "original_form": "소개팅앱같은거는 어떠세요?",
          "speaker_id": "1",
          "time": "20210901 10:10"
        }
