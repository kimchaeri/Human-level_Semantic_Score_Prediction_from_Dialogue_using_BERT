      "utterance": [
        {
          "id": "MDRW2100025088.1.1",
          "form": "그런가요....???",
          "original_form": "그런가요....???",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025088.1.2",
          "form": "웨이팅은",
          "original_form": "웨이팅은",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025088.1.3",
          "form": "긴 편인가요...",
          "original_form": "긴 편인가요...",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025088.1.4",
          "form": "식사때살짝피해가면 길지않아요 ㄱㅋㅋㄱㅋ",
          "original_form": "식사때살짝피해가면 길지않아요 ㄱㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025088.1.5",
          "form": "본점이 대학로인데 거기는 좀 심하고 강남점은 덜하더라구요",
          "original_form": "본점이 대학로인데 거기는 좀 심하고 강남점은 덜하더라구요",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025088.1.6",
          "form": "정돈유행하고 저런스타일돈가스집이 많아졌는데",
          "original_form": "정돈유행하고 저런스타일돈가스집이 많아졌는데",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025088.1.7",
          "form": "정돈따라올 집은 아직없는것 같아요",
          "original_form": "정돈따라올 집은 아직없는것 같아요",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025088.1.8",
          "form": "아 정돈이",
          "original_form": "아 정돈이",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025088.1.9",
          "form": "원조예요?!?!?@??!",
          "original_form": "원조예요?!?!?@??!",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025088.1.10",
          "form": "저 정말 가볼래요 저의 버킷리스트에 넣을 거여요",
          "original_form": "저 정말 가볼래요 저의 버킷리스트에 넣을 거여요",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025088.1.11",
          "form": "이런스타일 유행시킨건 정돈인거같아용!!!",
          "original_form": "이런스타일 유행시킨건 정돈인거같아용!!!",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025088.1.12",
          "form": "꼭꼭가보셔요",
          "original_form": "꼭꼭가보셔요",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025088.1.13",
          "form": "돈가스..",
          "original_form": "돈가스..",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025088.1.14",
          "form": "나의 소울푸드..",
          "original_form": "나의 소울푸드..",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025088.1.15",
          "form": "흑흑 넘맛있겠다",
          "original_form": "흑흑 넘맛있겠다",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025088.1.16",
          "form": "전 사실 저런 일식 돈까스보다",
          "original_form": "전 사실 저런 일식 돈까스보다",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025088.1.17",
          "form": "경양식을 더 좋아하거든요",
          "original_form": "경양식을 더 좋아하거든요",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025088.1.18",
          "form": "하지만 저 정도 비쥬얼이면 경양식에 비벼볼만 해요",
          "original_form": "하지만 저 정도 비쥬얼이면 경양식에 비벼볼만 해요",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025088.1.19",
          "form": "오 경양식도 맛있죠",
          "original_form": "오 경양식도 맛있죠",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025088.1.20",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025088.1.21",
          "form": "약간 그 뭐랄까 종류가다른느낌",
          "original_form": "약간 그 뭐랄까 종류가다른느낌",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025088.1.22",
          "form": "그치만 저긴 정말 저도 먹어보고 신세계였어요",
          "original_form": "그치만 저긴 정말 저도 먹어보고 신세계였어요",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025088.1.23",
          "form": "맞아요 라면에도",
          "original_form": "맞아요 라면에도",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025088.1.24",
          "form": "정통라면류가 있고 짜파게티가 있듯",
          "original_form": "정통라면류가 있고 짜파게티가 있듯",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025088.1.25",
          "form": "하지만 카레는시키지않으시는걸 추천",
          "original_form": "하지만 카레는시키지않으시는걸 추천",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025088.1.26",
          "form": "돈까스도 이제 그런 영역구분이 되어가고 있는 거죠",
          "original_form": "돈까스도 이제 그런 영역구분이 되어가고 있는 거죠",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025088.1.27",
          "form": "앗 카레는 왜요,",
          "original_form": "앗 카레는 왜요,",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025088.1.28",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025088.1.29",
          "form": "카레는 그냥 그런느낌이에요",
          "original_form": "카레는 그냥 그런느낌이에요",
          "speaker_id": "2",
