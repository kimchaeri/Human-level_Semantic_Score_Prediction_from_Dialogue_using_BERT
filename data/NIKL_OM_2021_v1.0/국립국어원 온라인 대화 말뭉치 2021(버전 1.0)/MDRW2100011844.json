        {
          "id": "MDRW2100011844.1.1",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011844.1.2",
          "form": "\"너무 튕기면 안된다\" 고들 하잖아요 ㅋㅋㅋ",
          "original_form": "\"너무 튕기면 안된다\" 고들 하잖아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011844.1.3",
          "form": "아 맞아욬ㅋㅋㅋㅋ",
          "original_form": "아 맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011844.1.4",
          "form": "튕긴다",
          "original_form": "튕긴다",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011844.1.5",
          "form": "싫고 좀 갑작스러워서 안하고싶은 의사를 표하면",
          "original_form": "싫고 좀 갑작스러워서 안하고싶은 의사를 표하면",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011844.1.6",
          "form": "튕기냐고 하죠",
          "original_form": "튕기냐고 하죠",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011844.1.7",
          "form": "무조건 스킨십이 좋다는 생각이 박혀있는 것 같아요",
          "original_form": "무조건 스킨십이 좋다는 생각이 박혀있는 것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011844.1.8",
          "form": "혹시 결혼식 가보셨나요?",
          "original_form": "혹시 결혼식 가보셨나요?",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011844.1.9",
          "form": "결혼식에서 우는 건 신부 가족들이나 신부인 경우가 많잖아요",
          "original_form": "결혼식에서 우는 건 신부 가족들이나 신부인 경우가 많잖아요",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011844.1.10",
          "form": "그것도 좀 이상하다고 생각했어요",
          "original_form": "그것도 좀 이상하다고 생각했어요",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011844.1.11",
          "form": "아 맞네요",
          "original_form": "아 맞네요",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011844.1.12",
          "form": "저 웨딩영상업체에서 일했어서",
          "original_form": "저 웨딩영상업체에서 일했어서",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011844.1.13",
          "form": "기기괴괴 진짜많이봣어요",
          "original_form": "기기괴괴 진짜많이봣어요",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011844.1.14",
          "form": "아 그렇겠네요",
          "original_form": "아 그렇겠네요",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011844.1.15",
          "form": "저도 지인이 결혼해서 가봤는데",
          "original_form": "저도 지인이 결혼해서 가봤는데",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011844.1.16",
          "form": "저 안경낀신부 한번도못봣어요",
          "original_form": "저 안경낀신부 한번도못봣어요",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011844.1.17",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011844.1.18",
          "form": "안경낀신랑은 69번 봤는데..",
          "original_form": "안경낀신랑은 69번 봤는데..",
          "speaker_id": "1",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011844.1.19",
          "form": "사실 다들 라식, 라섹 많이하죠",
          "original_form": "사실 다들 라식, 라섹 많이하죠",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011844.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011844.1.21",
          "form": "지인 결혼식 어떠셧나요?!",
          "original_form": "지인 결혼식 어떠셧나요?!",
          "speaker_id": "1",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011844.1.22",
          "form": "너무나 흥미진진해서 지금 폰을 손에서 못놓고잇어요",
          "original_form": "너무나 흥미진진해서 지금 폰을 손에서 못놓고잇어요",
          "speaker_id": "1",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011844.1.23",
          "form": "결혼식이 진짜",
          "original_form": "결혼식이 진짜",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011844.1.24",
          "form": "기기괴괴하다는 것 절정이라 생각해요",
          "original_form": "기기괴괴하다는 것 절정이라 생각해요",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011844.1.25",
          "form": "진짜 ㄹㅇ",
          "original_form": "진짜 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011844.1.26",
          "form": "다들 축하하는 분위기에",
          "original_form": "다들 축하하는 분위기에",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011844.1.27",
          "form": "돈도 돌고",
          "original_form": "돈도 돌고",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011844.1.28",
          "form": "식 전에 신부랑 기념사진찍잖아요",
          "original_form": "식 전에 신부랑 기념사진찍잖아요",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011844.1.29",
          "form": "무슨 기념품도아니고",
          "original_form": "무슨 기념품도아니고",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011844.1.30",
          "form": "신부 지인들 옷차림도 그렇고",
          "original_form": "신부 지인들 옷차림도 그렇고",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011844.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
