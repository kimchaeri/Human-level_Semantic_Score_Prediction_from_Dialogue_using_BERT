      "utterance": [
        {
          "id": "MDRW2100027786.1.1",
          "form": "그러니까욬ㅋㅋㅋㅋㅋㅋ미래 설계의 일환일까 싶기도 하고...",
          "original_form": "그러니까욬ㅋㅋㅋㅋㅋㅋ미래 설계의 일환일까 싶기도 하고...",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027786.1.2",
          "form": "혹시나이대가 어떻게 되세요?",
          "original_form": "혹시나이대가 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027786.1.3",
          "form": "전 20대 중 후반인데",
          "original_form": "전 20대 중 후반인데",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027786.1.4",
          "form": "벌써 이런 얘기를...^^..",
          "original_form": "벌써 이런 얘기를...^^..",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027786.1.5",
          "form": "다들 그러싱가요?",
          "original_form": "다들 그러싱가요?",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027786.1.6",
          "form": "앗 저도 이십대 후반이에요",
          "original_form": "앗 저도 이십대 후반이에요",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027786.1.7",
          "form": "부모님들은 슬슬... 하거 생각하시나봐요",
          "original_form": "부모님들은 슬슬... 하거 생각하시나봐요",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027786.1.8",
          "form": "다들.. 그러신가봐요",
          "original_form": "다들.. 그러신가봐요",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027786.1.9",
          "form": "그것두 넘 신기해요 뭔가..ㅎㅎ",
          "original_form": "그것두 넘 신기해요 뭔가..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027786.1.10",
          "form": "사회 통념상... 그런 시기라고 생각하니까요",
          "original_form": "사회 통념상... 그런 시기라고 생각하니까요",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027786.1.11",
          "form": "제 주변에듀 결혼하는 친구들이 생기더라구여",
          "original_form": "제 주변에듀 결혼하는 친구들이 생기더라구여",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027786.1.12",
          "form": "헐 저두 딱 한명 친구 결혼했어요",
          "original_form": "헐 저두 딱 한명 친구 결혼했어요",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027786.1.13",
          "form": "다들 좀 일찍했다구 하긴 하는데 다들 인정하는 분위기..?",
          "original_form": "다들 좀 일찍했다구 하긴 하는데 다들 인정하는 분위기..?",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027786.1.14",
          "form": "그러니까요 마치 여덟살이 되면 초등학교에 들어가는 것 처럼 으레 그 나이면 그러겠거니... 하는 게 있어서..",
          "original_form": "그러니까요 마치 여덟살이 되면 초등학교에 들어가는 것 처럼 으레 그 나이면 그러겠거니... 하는 게 있어서..",
          "speaker_id": "1",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100027786.1.15",
          "form": "맞아요.. 뮤엇보다 충격은 제가 느끼기에",
          "original_form": "맞아요.. 뮤엇보다 충격은 제가 느끼기에",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027786.1.16",
          "form": "저는 아직 아기같은데 다들 그렇게 생각하는게 충격이에용",
          "original_form": "저는 아직 아기같은데 다들 그렇게 생각하는게 충격이에용",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027786.1.17",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027786.1.18",
          "form": "그러니까요 제가 한 아이를 키울만한 사람이 아닌데... 싶어요ㅠㅠ",
          "original_form": "그러니까요 제가 한 아이를 키울만한 사람이 아닌데... 싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027786.1.19",
          "form": "저도 같은 생각이에여ㅠㅠ 저도 아짓 너무 어리고 저 하나 감당하기 힘든데",
          "original_form": "저도 같은 생각이에여ㅠㅠ 저도 아짓 너무 어리고 저 하나 감당하기 힘든데",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027786.1.20",
          "form": "어ㅖ 어린 아가를 제가..",
          "original_form": "어ㅖ 어린 아가를 제가..",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027786.1.21",
          "form": "애를 돌보신 적은 있으신가요?",
          "original_form": "애를 돌보신 적은 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027786.1.22",
          "form": "아뇨 전 주변에 사실 아직 아기가 없어서..",
          "original_form": "아뇨 전 주변에 사실 아직 아기가 없어서..",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027786.1.23",
          "form": "초등학생 애들 돌보는 봉사는 해본적이 이ㅛ는데",
          "original_form": "초등학생 애들 돌보는 봉사는 해본적이 이ㅛ는데",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027786.1.24",
          "form": "초등학생도 버겁더라고요 저에게..",
          "original_form": "초등학생도 버겁더라고요 저에게..",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027786.1.25",
          "form": "젤 체력 넘칠 때 잖아요ㅋㅋㅋㅋ",
          "original_form": "젤 체력 넘칠 때 잖아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027786.1.26",
          "form": "앗 그런가요?",
          "original_form": "앗 그런가요?",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027786.1.27",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027786.1.28",
          "form": "넘 힘들더라구요..",
          "original_form": "넘 힘들더라구요..",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027786.1.29",
          "form": "진짜 무슨 짐승 같은 느낌이라고 해여하나..",
          "original_form": "진짜 무슨 짐승 같은 느낌이라고 해여하나..",
          "speaker_id": "2",
          "time": "20210902 15:58"
        }
      ]
