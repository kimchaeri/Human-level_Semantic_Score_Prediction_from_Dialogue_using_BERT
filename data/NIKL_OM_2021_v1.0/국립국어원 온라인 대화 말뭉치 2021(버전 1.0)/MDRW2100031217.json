      "utterance": [
        {
          "id": "MDRW2100031217.1.1",
          "form": "지금 먹으러 가려구요!",
          "original_form": "지금 먹으러 가려구요!",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100031217.1.2",
          "form": "맛있게 드셨나요 ㅋㅋ",
          "original_form": "맛있게 드셨나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100031217.1.3",
          "form": "네넹, 잘 먹고 왔습니다",
          "original_form": "네넹, 잘 먹고 왔습니다",
          "speaker_id": "2",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031217.1.4",
          "form": "대게 먹었어요",
          "original_form": "대게 먹었어요",
          "speaker_id": "2",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031217.1.5",
          "form": "와 비싼거",
          "original_form": "와 비싼거",
          "speaker_id": "1",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100031217.1.6",
          "form": "맛잇겠네요",
          "original_form": "맛잇겠네요",
          "speaker_id": "1",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100031217.1.7",
          "form": "음식점에서 드셨어요?",
          "original_form": "음식점에서 드셨어요?",
          "speaker_id": "1",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100031217.1.8",
          "form": "네네, 대게만 취급하는 식당이었어요",
          "original_form": "네네, 대게만 취급하는 식당이었어요",
          "speaker_id": "2",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100031217.1.9",
          "form": "kg당 75,000원이더라구요",
          "original_form": "kg당 75,000원이더라구요",
          "speaker_id": "2",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100031217.1.10",
          "form": "금으로 된줄",
          "original_form": "금으로 된줄",
          "speaker_id": "2",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031217.1.11",
          "form": "역시 ᆢ 비싼음식",
          "original_form": "역시 ᆢ 비싼음식",
          "speaker_id": "1",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031217.1.12",
          "form": "오늘 기념일인가요?",
          "original_form": "오늘 기념일인가요?",
          "speaker_id": "1",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100031217.1.13",
          "form": "그건 아니구욬ㅋㅋㅋ",
          "original_form": "그건 아니구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031217.1.14",
          "form": "여행 온김에 근처에 있길래",
          "original_form": "여행 온김에 근처에 있길래",
          "speaker_id": "2",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031217.1.15",
          "form": "가봤습니다",
          "original_form": "가봤습니다",
          "speaker_id": "2",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031217.1.16",
          "form": "아 여행중이라고 하셨죠",
          "original_form": "아 여행중이라고 하셨죠",
          "speaker_id": "1",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031217.1.17",
          "form": "맛집 많이 다니세요 ㅎ",
          "original_form": "맛집 많이 다니세요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031217.1.18",
          "form": "그래야죠ㅎㅎ 먹는게 남는거!",
          "original_form": "그래야죠ㅎㅎ 먹는게 남는거!",
          "speaker_id": "2",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100031217.1.19",
          "form": "그쵸 먹는게 남는거예요",
          "original_form": "그쵸 먹는게 남는거예요",
          "speaker_id": "1",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031217.1.20",
          "form": "후식 드시나요?",
          "original_form": "후식 드시나요?",
          "speaker_id": "1",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031217.1.21",
          "form": "오늘은 패스요ㅎㅎ",
          "original_form": "오늘은 패스요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031217.1.22",
          "form": "어제는 팥빙수 먹었어요",
          "original_form": "어제는 팥빙수 먹었어요",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031217.1.23",
          "form": "팥빙수 맛있겠네요",
          "original_form": "팥빙수 맛있겠네요",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031217.1.24",
          "form": "이제 곧 가을인데 마지막으로 먹기 좋네요 ㅎ",
          "original_form": "이제 곧 가을인데 마지막으로 먹기 좋네요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031217.1.25",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031217.1.26",
          "form": "근데 겨울에 먹는 빙수도.별미예요ㅎㅎ",
          "original_form": "근데 겨울에 먹는 빙수도.별미예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031217.1.27",
          "form": "아 그쵸 겨울도 괜찮죠",
          "original_form": "아 그쵸 겨울도 괜찮죠",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031217.1.28",
          "form": "이냉치냉 좋네요",
          "original_form": "이냉치냉 좋네요",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031217.1.29",
          "form": "name1님은 어떤 음식 좋아하세요??",
          "original_form": "&name1&님은 어떤 음식 좋아하세요??",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100031217.1.30",
          "form": "저는 샤브샤브요",
          "original_form": "저는 샤브샤브요",
