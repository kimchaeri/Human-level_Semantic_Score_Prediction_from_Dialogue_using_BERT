      "utterance": [
        {
          "id": "MDRW2100039835.1.1",
          "form": "저는 매운거 회같은거",
          "original_form": "저는 매운거 회같은거",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039835.1.2",
          "form": "빼고 다 좋아합니다 ㅎ",
          "original_form": "빼고 다 좋아합니다 ㅎ",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039835.1.3",
          "form": "저도 매운음식은 싫어요",
          "original_form": "저도 매운음식은 싫어요",
          "speaker_id": "1",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039835.1.4",
          "form": "글쵸? ㅎㅎㅎ",
          "original_form": "글쵸? ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039835.1.5",
          "form": "매운 할매떡볶이 먹고 담날 화장실 문지방닳게",
          "original_form": "매운 할매떡볶이 먹고 담날 화장실 문지방닳게",
          "speaker_id": "2",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039835.1.6",
          "form": "왔다 갔다 하면서도 또 먹으러 가더라구요",
          "original_form": "왔다 갔다 하면서도 또 먹으러 가더라구요",
          "speaker_id": "2",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039835.1.7",
          "form": "저도 화장실땜에 아예 안먹어요",
          "original_form": "저도 화장실땜에 아예 안먹어요",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039835.1.8",
          "form": "회도 싫어 하나요 ?",
          "original_form": "회도 싫어 하나요 ?",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039835.1.9",
          "form": "네 날것은...좀",
          "original_form": "네 날것은...좀",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039835.1.10",
          "form": "이상해요 ㅎ",
          "original_form": "이상해요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039835.1.11",
          "form": "회 보다는 익히 생선이 저도 좋아요",
          "original_form": "회 보다는 익히 생선이 저도 좋아요",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039835.1.12",
          "form": "그래서 고기파예요 ㅎㅎㅎ",
          "original_form": "그래서 고기파예요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039835.1.13",
          "form": "전 삼겹살 매니아예요",
          "original_form": "전 삼겹살 매니아예요",
          "speaker_id": "1",
          "time": "20210904 07:51"
        },
        {
          "id": "MDRW2100039835.1.14",
          "form": "오 저두 고기는 삼겹살 ㅎ",
          "original_form": "오 저두 고기는 삼겹살 ㅎ",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100039835.1.15",
          "form": "소고기 머 이런저런해도",
          "original_form": "소고기 머 이런저런해도",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100039835.1.16",
          "form": "삼겹살이 젤이더라구요",
          "original_form": "삼겹살이 젤이더라구요",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100039835.1.17",
          "form": "소고기는 왠지 느끼해서 많이 못먹겠어요",
          "original_form": "소고기는 왠지 느끼해서 많이 못먹겠어요",
          "speaker_id": "1",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100039835.1.18",
          "form": "닭 오리는 어떤가요",
          "original_form": "닭 오리는 어떤가요",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100039835.1.19",
          "form": "닭은 치킨으로 ㅎ",
          "original_form": "닭은 치킨으로 ㅎ",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100039835.1.20",
          "form": "오리는 훈제 ㅎㅎ",
          "original_form": "오리는 훈제 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100039835.1.21",
          "form": "저도 똑같아요 ㅋㅋ ㅋ",
          "original_form": "저도 똑같아요 ㅋㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100039835.1.22",
          "form": "오리 백숙도 좋긴해요",
          "original_form": "오리 백숙도 좋긴해요",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100039835.1.23",
          "form": "아 백숙도 있군요",
          "original_form": "아 백숙도 있군요",
          "speaker_id": "2",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100039835.1.24",
          "form": "백숙은 자주 못먹으니 ㅎㅎ",
          "original_form": "백숙은 자주 못먹으니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:54"
        }
