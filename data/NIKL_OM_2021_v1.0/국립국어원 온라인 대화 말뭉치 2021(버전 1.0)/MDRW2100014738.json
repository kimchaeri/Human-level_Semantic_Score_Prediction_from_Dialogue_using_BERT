      "utterance": [
        {
          "id": "MDRW2100014738.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014738.1.2",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014738.1.3",
          "form": "스시 배민 시켜먹었어요",
          "original_form": "스시 배민 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014738.1.4",
          "form": "스시 좋아하시나봐요",
          "original_form": "스시 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014738.1.5",
          "form": "어떤 초밥좋아하세요",
          "original_form": "어떤 초밥좋아하세요",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014738.1.6",
          "form": "저는 먹는낙으로 살아서",
          "original_form": "저는 먹는낙으로 살아서",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014738.1.7",
          "form": "다~~~좋아해요ㅎㅎ",
          "original_form": "다~~~좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014738.1.8",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014738.1.9",
          "form": "저 닭갈비 먹었어요",
          "original_form": "저 닭갈비 먹었어요",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014738.1.10",
          "form": "저도 먹는 낙으로 사는 거 같아요",
          "original_form": "저도 먹는 낙으로 사는 거 같아요",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014738.1.11",
          "form": "초밥 드실 때 와사비 드시나요",
          "original_form": "초밥 드실 때 와사비 드시나요",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014738.1.12",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014738.1.13",
          "form": "약간 싸하고 찡한맛이 매력인거같아요",
          "original_form": "약간 싸하고 찡한맛이 매력인거같아요",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014738.1.14",
          "form": "락교 와사비",
          "original_form": "락교 와사비",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014738.1.15",
          "form": "ㅎㅎ다좋아요",
          "original_form": "ㅎㅎ다좋아요",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014738.1.16",
          "form": "락교는 무슨 맛잇가요? 약간 마늘처럼 생겨서 먹어본 적이 없는 거 같아요",
          "original_form": "락교는 무슨 맛잇가요? 약간 마늘처럼 생겨서 먹어본 적이 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014738.1.17",
          "form": "맞아요 마늘 간장에절인맛",
          "original_form": "맞아요 마늘 간장에절인맛",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014738.1.18",
          "form": "아하 그러시구나",
          "original_form": "아하 그러시구나",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014738.1.19",
          "form": "저는 요리에 똥손이라서",
          "original_form": "저는 요리에 똥손이라서",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014738.1.20",
          "form": "주로 배달",
          "original_form": "주로 배달",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014738.1.21",
          "form": "아니면",
          "original_form": "아니면",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014738.1.22",
          "form": "사먹어요",
          "original_form": "사먹어요",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014738.1.23",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014738.1.24",
          "form": "식비 많이들어욥",
          "original_form": "식비 많이들어욥",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014738.1.25",
          "form": "배달도 괜찮은 방법이에요ㅎㅎ",
