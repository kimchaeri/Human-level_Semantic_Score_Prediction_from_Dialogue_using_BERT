      "utterance": [
        {
          "id": "MDRW2100022822.1.1",
          "form": "오늘 날씨 더웠나?",
          "original_form": "오늘 날씨 더웠나?",
          "speaker_id": "1",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100022822.1.2",
          "form": "너 밖에 안나감? 밖에 더움",
          "original_form": "너 밖에 안나감? 밖에 더움",
          "speaker_id": "2",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022822.1.3",
          "form": "난 오늘 하루종일 집에 있었지",
          "original_form": "난 오늘 하루종일 집에 있었지",
          "speaker_id": "1",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022822.1.4",
          "form": "더 어디 나갔다왔어?",
          "original_form": "더 어디 나갔다왔어?",
          "speaker_id": "1",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022822.1.5",
          "form": "반팔 두개 겹쳐입었다가 땀샤워함",
          "original_form": "반팔 두개 겹쳐입었다가 땀샤워함",
          "speaker_id": "2",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022822.1.6",
          "form": "ㅋㅋㅋㅋ산책 간거야?",
          "original_form": "ㅋㅋㅋㅋ산책 간거야?",
          "speaker_id": "1",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022822.1.7",
          "form": "친구네 떡볶이 사들고감",
          "original_form": "친구네 떡볶이 사들고감",
          "speaker_id": "2",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022822.1.8",
          "form": "오늘 구름 이쁘더라",
          "original_form": "오늘 구름 이쁘더라",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022822.1.9",
          "form": "미세먼지없나봄",
          "original_form": "미세먼지없나봄",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022822.1.10",
          "form": "요즘 하늘이 이쁘긴 해",
          "original_form": "요즘 하늘이 이쁘긴 해",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022822.1.11",
          "form": "오늘 미세먼지 양호네",
          "original_form": "오늘 미세먼지 양호네",
          "speaker_id": "2",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022822.1.12",
          "form": "너도 산책하러 나와봐",
          "original_form": "너도 산책하러 나와봐",
          "speaker_id": "2",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022822.1.13",
          "form": "오 안 봤었는데",
          "original_form": "오 안 봤었는데",
          "speaker_id": "1",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022822.1.14",
          "form": "날씨 잘 챙겨보네",
          "original_form": "날씨 잘 챙겨보네",
          "speaker_id": "1",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022822.1.15",
          "form": "화요일부터 비온대",
          "original_form": "화요일부터 비온대",
          "speaker_id": "1",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022822.1.16",
          "form": "야 화수목 다 비오는데???",
          "original_form": "야 화수목 다 비오는데???",
          "speaker_id": "2",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022822.1.17",
          "form": "우산 챙겨다니셈",
          "original_form": "우산 챙겨다니셈",
          "speaker_id": "2",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022822.1.18",
          "form": "나 지난번에 우산 잃어버려서",
          "original_form": "나 지난번에 우산 잃어버려서",
          "speaker_id": "1",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100022822.1.19",
          "form": "우리집 우산 부족해",
          "original_form": "우리집 우산 부족해",
          "speaker_id": "1",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100022822.1.20",
          "form": "ㅋㄱㄱㅋㅋㄱㄱㄱㄱㅋㄱㄱㅋㄲㅋㅋ이제 날씨 추워지니까",
          "original_form": "ㅋㄱㄱㅋㅋㄱㄱㄱㄱㅋㄱㄱㅋㄲㅋㅋ이제 날씨 추워지니까",
          "speaker_id": "2",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100022822.1.21",
          "form": "겉옷을 우산 대용으로 써야되겠는데?",
          "original_form": "겉옷을 우산 대용으로 써야되겠는데?",
          "speaker_id": "2",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022822.1.22",
          "form": "그냥 비맞고 다니란 소리 아니냐곸ㅋㅋㅋㅋ",
          "original_form": "그냥 비맞고 다니란 소리 아니냐곸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100022822.1.23",
          "form": "화요일날 24도까지 떨어짐",
          "original_form": "화요일날 24도까지 떨어짐",
          "speaker_id": "2",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022822.1.24",
          "form": "지난번에 비 애매하게 오던 날 있었잖아",
          "original_form": "지난번에 비 애매하게 오던 날 있었잖아",
          "speaker_id": "1",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022822.1.25",
          "form": "언제지???",
          "original_form": "언제지???",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022822.1.26",
          "form": "날짜까지 그렇게 자세히 기억하지 못해",
          "original_form": "날짜까지 그렇게 자세히 기억하지 못해",
          "speaker_id": "1",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022822.1.27",
          "form": "그래 대충 저번주에 비 소나기같이 왔다 그쳤다가 했었음",
          "original_form": "그래 대충 저번주에 비 소나기같이 왔다 그쳤다가 했었음",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022822.1.28",
          "form": "근데 왜?",
          "original_form": "근데 왜?",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022822.1.29",
          "form": "어 맞아 그 날이었음",
          "original_form": "어 맞아 그 날이었음",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022822.1.30",
          "form": "아침에 운동가려고 우산 들고 나갔단 말이야",
          "original_form": "아침에 운동가려고 우산 들고 나갔단 말이야",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022822.1.31",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022822.1.32",
          "form": "쓸 정도로 내리는 건 아니라 우산을 쓸까 말까 고민하는데",
          "original_form": "쓸 정도로 내리는 건 아니라 우산을 쓸까 말까 고민하는데",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022822.1.33",
          "form": "반쯤 가다가 우산 썼다가 ㄱ우산 안에 고인물 맞았음ㅋㅋㅋㅋㅋ",
          "original_form": "반쯤 가다가 우산 썼다가 ㄱ우산 안에 고인물 맞았음ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022822.1.34",
          "form": "갔던 길 되돌아와서 옷 갈아입었잖아",
          "original_form": "갔던 길 되돌아와서 옷 갈아입었잖아",
          "speaker_id": "1",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022822.1.35",
          "form": "운동도 늦고.... 옷은 젖고....",
          "original_form": "운동도 늦고.... 옷은 젖고....",
          "speaker_id": "1",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022822.1.36",
          "form": "우산을 잘 말리고 다니도록 해봐",
          "original_form": "우산을 잘 말리고 다니도록 해봐",
          "speaker_id": "2",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022822.1.37",
          "form": "이제 추워지니까",
          "original_form": "이제 추워지니까",
          "speaker_id": "2",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022822.1.38",
          "form": "그 전날 태풍 때문에 고인물이라 어쩔 수 없었다고ㅠㅠ",
          "original_form": "그 전날 태풍 때문에 고인물이라 어쩔 수 없었다고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022822.1.39",
          "form": "후드티를 입고 이제 모자를 쓰자",
          "original_form": "후드티를 입고 이제 모자를 쓰자",
          "speaker_id": "2",
          "time": "20210829 18:22"
        },
        {
          "id": "MDRW2100022822.1.40",
          "form": "태풍 왔었나??",
          "original_form": "태풍 왔었나??",
          "speaker_id": "2",
          "time": "20210829 18:22"
        },
        {
          "id": "MDRW2100022822.1.41",
          "form": "진짜 \r\n우산 쓰지 말고 다니라고?",
          "original_form": "진짜 \r\n우산 쓰지 말고 다니라고?",
          "speaker_id": "1",
          "time": "20210829 18:22"
        },
        {
          "id": "MDRW2100022822.1.42",
          "form": "ㅇㅇ 전주였나?",
          "original_form": "ㅇㅇ 전주였나?",
          "speaker_id": "1",
          "time": "20210829 18:22"
        },
        {
          "id": "MDRW2100022822.1.43",
          "form": "다 소멸된거 아니었어?",
          "original_form": "다 소멸된거 아니었어?",
          "speaker_id": "2",
          "time": "20210829 18:22"
        },
        {
          "id": "MDRW2100022822.1.44",
          "form": "우리 인천인디",
          "original_form": "우리 인천인디",
          "speaker_id": "2",
          "time": "20210829 18:22"
        },
        {
          "id": "MDRW2100022822.1.45",
          "form": "하나 올라와서 인천까지 비 많이 온 날 있었어",
          "original_form": "하나 올라와서 인천까지 비 많이 온 날 있었어",
          "speaker_id": "1",
          "time": "20210829 18:22"
        },
        {
          "id": "MDRW2100022822.1.46",
          "form": "아마",
          "original_form": "아마",
          "speaker_id": "1",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.47",
          "form": "이번 여름 날씨 하도 비많이와서",
          "original_form": "이번 여름 날씨 하도 비많이와서",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.48",
          "form": "태풍인지 장마인지 구분도 안감",
          "original_form": "태풍인지 장마인지 구분도 안감",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.49",
          "form": "여름 장마는 무사히 지나갔었는데",
          "original_form": "여름 장마는 무사히 지나갔었는데",
          "speaker_id": "1",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.50",
          "form": "이제 가을이잖아",
          "original_form": "이제 가을이잖아",
          "speaker_id": "1",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.51",
          "form": "가을 장마랑 태풍이랑",
          "original_form": "가을 장마랑 태풍이랑",
          "speaker_id": "1",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.52",
          "form": "우리 벌써 가을이야?",
          "original_form": "우리 벌써 가을이야?",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.53",
          "form": "난 인정못해",
          "original_form": "난 인정못해",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.54",
          "form": "아직 반팔입는단말야",
          "original_form": "아직 반팔입는단말야",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.55",
          "form": "입추 지났으니까",
          "original_form": "입추 지났으니까",
          "speaker_id": "1",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022822.1.56",
          "form": "니가 인정 안하면 어쩔건뎈ㅋㅋㅋ",
          "original_form": "니가 인정 안하면 어쩔건뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022822.1.57",
          "form": "그러게 곧 백로네",
          "original_form": "그러게 곧 백로네",
          "speaker_id": "2",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022822.1.58",
          "form": "근데 나도 아직 반팔입어",
          "original_form": "근데 나도 아직 반팔입어",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022822.1.59",
          "form": "오 유식해보이는 말",
          "original_form": "오 유식해보이는 말",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022822.1.60",
          "form": "이제 새벽에 이슬이 맺히는 계절인것이다",
          "original_form": "이제 새벽에 이슬이 맺히는 계절인것이다",
          "speaker_id": "2",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022822.1.61",
          "form": "날씨 추워지니까 옷 따뜻하게 입고다녀",
          "original_form": "날씨 추워지니까 옷 따뜻하게 입고다녀",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022822.1.62",
          "form": "그래 너도 따듯하게 입고 다녀라",
          "original_form": "그래 너도 따듯하게 입고 다녀라",
          "speaker_id": "1",
          "time": "20210829 18:25"
        }
      ]
    }
