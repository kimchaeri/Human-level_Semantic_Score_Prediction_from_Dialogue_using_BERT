      "utterance": [
        {
          "id": "MDRW2100040216.1.1",
          "form": "밤되니 시원하고 좋네요",
          "original_form": "밤되니 시원하고 좋네요",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100040216.1.2",
          "form": "맞아요 너무 선선하고 좋네요",
          "original_form": "맞아요 너무 선선하고 좋네요",
          "speaker_id": "2",
          "time": "20210904 19:21"
        },
        {
          "id": "MDRW2100040216.1.3",
          "form": "오늘 날씨 진짜 대박이었던것 같아요",
          "original_form": "오늘 날씨 진짜 대박이었던것 같아요",
          "speaker_id": "2",
          "time": "20210904 19:21"
        },
        {
          "id": "MDRW2100040216.1.4",
          "form": "진짜 날씨가 오늘만 같았으면 너무 좋을거같아요",
          "original_form": "진짜 날씨가 오늘만 같았으면 너무 좋을거같아요",
          "speaker_id": "1",
          "time": "20210904 19:35"
        },
        {
          "id": "MDRW2100040216.1.5",
          "form": "캘리포니아가 날씨가 너무 좋아서 사람들 성격이 다 좋다고ㅎㅎ",
          "original_form": "캘리포니아가 날씨가 너무 좋아서 사람들 성격이 다 좋다고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:35"
        },
        {
          "id": "MDRW2100040216.1.6",
          "form": "으아아아 저도 그얘기 들은적있어요 ㅋㅋ 너무부러워요",
          "original_form": "으아아아 저도 그얘기 들은적있어요 ㅋㅋ 너무부러워요",
          "speaker_id": "2",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100040216.1.7",
          "form": "진짜 우리나라도 가을날씨만 같았다면 ㅋㅋㅋㅋ 더 좋을텐ㄷ0",
          "original_form": "진짜 우리나라도 가을날씨만 같았다면 ㅋㅋㅋㅋ 더 좋을텐ㄷ0",
          "speaker_id": "2",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100040216.1.8",
          "form": "짧아서 더 소중하다지만 이 날씨 진짜 너무 좋아요!!",
          "original_form": "짧아서 더 소중하다지만 이 날씨 진짜 너무 좋아요!!",
          "speaker_id": "1",
          "time": "20210904 20:06"
        },
        {
          "id": "MDRW2100040216.1.9",
          "form": "맞아요 ㅋㅋ 이제.가을옷 꺼내야할것같아요",
          "original_form": "맞아요 ㅋㅋ 이제.가을옷 꺼내야할것같아요",
          "speaker_id": "2",
          "time": "20210904 20:07"
        },
        {
          "id": "MDRW2100040216.1.10",
          "form": "트렌치의 계절",
          "original_form": "트렌치의 계절",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100040216.1.11",
          "form": "다가옵니다",
          "original_form": "다가옵니다",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100040216.1.12",
          "form": "ㅋㅋ맞아요 트렌치...!!",
          "original_form": "ㅋㅋ맞아요 트렌치...!!",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040216.1.13",
          "form": "해마다 계절을 맞이하는데",
          "original_form": "해마다 계절을 맞이하는데",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040216.1.14",
          "form": "해마다 뭐입을지 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "해마다 뭐입을지 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040216.1.15",
          "form": "그니까요 과연 작년의 나는 뭐입고 다녔나!!!",
          "original_form": "그니까요 과연 작년의 나는 뭐입고 다녔나!!!",
          "speaker_id": "1",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100040216.1.16",
          "form": "ㅋㅋ그니까요 ㅋㅋ 그리고 매해 겪는데 음 22도면 무슨옷을입어야하더라 18도면 무슨옷을 입어야하더라",
          "original_form": "ㅋㅋ그니까요 ㅋㅋ 그리고 매해 겪는데 음 22도면 무슨옷을입어야하더라 18도면 무슨옷을 입어야하더라",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100040216.1.17",
          "form": "이런것도 ㅋㅋㅋ고민되요",
          "original_form": "이런것도 ㅋㅋㅋ고민되요",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100040216.1.18",
          "form": "아 그거 이미지 많더라구요",
          "original_form": "아 그거 이미지 많더라구요",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100040216.1.19",
          "form": "몇도엔 자켓 몇도엔 패딩",
          "original_form": "몇도엔 자켓 몇도엔 패딩",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100040216.1.20",
          "form": "ㅋㅋㅋㅋ맞아요 저듀 그래서 해마다 그거 검색해서 보고 입어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맞아요 저듀 그래서 해마다 그거 검색해서 보고 입어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100040216.1.21",
          "form": "그러고 또 까먹고 ㅋㅋㅋㅋ",
          "original_form": "그러고 또 까먹고 ㅋㅋㅋㅋ",
