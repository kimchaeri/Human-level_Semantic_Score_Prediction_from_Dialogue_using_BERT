      "utterance": [
        {
          "id": "MDRW2100032459.1.1",
          "form": "계란국 맛있지 파송송 계란탁",
          "original_form": "계란국 맛있지 파송송 계란탁",
          "speaker_id": "2",
          "time": "20210905 18:24"
        },
        {
          "id": "MDRW2100032459.1.2",
          "form": "요즘에 빠진 거 있는데",
          "original_form": "요즘에 빠진 거 있는데",
          "speaker_id": "1",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100032459.1.3",
          "form": "뭔데 먹는 거>?",
          "original_form": "뭔데 먹는 거>?",
          "speaker_id": "2",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100032459.1.4",
          "form": "오이 고 추 된장 무침!",
          "original_form": "오이 고 추 된장 무침!",
          "speaker_id": "1",
          "time": "20210905 18:26"
        },
        {
          "id": "MDRW2100032459.1.5",
          "form": "고 추가 나쁜말이라고 나오네 ㅠ",
          "original_form": "고 추가 나쁜말이라고 나오네 ㅠ",
          "speaker_id": "1",
          "time": "20210905 18:26"
        },
        {
          "id": "MDRW2100032459.1.6",
          "form": "그러게... 오이고 추 된장 무침 맛있지",
          "original_form": "그러게... 오이고 추 된장 무침 맛있지",
          "speaker_id": "2",
          "time": "20210905 18:27"
        },
        {
          "id": "MDRW2100032459.1.7",
          "form": "오이땡땡에 된장 마늘 다진거 참기름 올리고당 넣으면 반찬으로 좋아",
          "original_form": "오이땡땡에 된장 마늘 다진거 참기름 올리고당 넣으면 반찬으로 좋아",
          "speaker_id": "1",
          "time": "20210905 18:28"
        },
        {
          "id": "MDRW2100032459.1.8",
          "form": "진짜 간단하네 재료도 다 있고",
          "original_form": "진짜 간단하네 재료도 다 있고",
          "speaker_id": "2",
          "time": "20210905 18:28"
        },
        {
          "id": "MDRW2100032459.1.9",
          "form": "감자조림도 간단하고",
          "original_form": "감자조림도 간단하고",
          "speaker_id": "1",
          "time": "20210905 18:28"
        },
        {
          "id": "MDRW2100032459.1.10",
          "form": "요리 자주 하나보다",
          "original_form": "요리 자주 하나보다",
          "speaker_id": "2",
          "time": "20210905 18:28"
        },
        {
          "id": "MDRW2100032459.1.11",
          "form": "요리라고 하기도 뭐한 게 간단해",
          "original_form": "요리라고 하기도 뭐한 게 간단해",
          "speaker_id": "1",
          "time": "20210905 18:28"
        },
        {
          "id": "MDRW2100032459.1.12",
          "form": "ㅎㅎ 멋쟁이 요리사!",
          "original_form": "ㅎㅎ 멋쟁이 요리사!",
          "speaker_id": "2",
          "time": "20210905 18:29"
        },
        {
          "id": "MDRW2100032459.1.13",
          "form": "굴소스도 하나 집에 있으면 좋아",
          "original_form": "굴소스도 하나 집에 있으면 좋아",
          "speaker_id": "1",
          "time": "20210905 18:29"
        },
        {
          "id": "MDRW2100032459.1.14",
          "form": "굴소스 있지 나 소스를 좋아해서 소스 종류 많아",
          "original_form": "굴소스 있지 나 소스를 좋아해서 소스 종류 많아",
          "speaker_id": "2",
          "time": "20210905 18:30"
        }
      ]
