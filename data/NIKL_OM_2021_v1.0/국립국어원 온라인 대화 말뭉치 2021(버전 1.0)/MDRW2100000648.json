      "utterance": [
        {
          "id": "MDRW2100000648.1.1",
          "form": "쁘걸 곡 스밍 좀 해주게",
          "original_form": "쁘걸 곡 스밍 좀 해주게",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000648.1.2",
          "form": "멜론 이벤트 하니까 가입해",
          "original_form": "멜론 이벤트 하니까 가입해",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000648.1.3",
          "form": "치맛바람 이라고~^^",
          "original_form": "치맛바람 이라고~^^",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000648.1.4",
          "form": "멜론 가입해서 쁘걸 음원 들어",
          "original_form": "멜론 가입해서 쁘걸 음원 들어",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000648.1.5",
          "form": "잠깐 1위했는데 요즘 방탄에 밀림",
          "original_form": "잠깐 1위했는데 요즘 방탄에 밀림",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000648.1.6",
          "form": "정말 반짝이야",
          "original_form": "정말 반짝이야",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000648.1.7",
          "form": "앨범 샀는데 뭔 멜론이여 ㅋㅋ",
          "original_form": "앨범 샀는데 뭔 멜론이여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000648.1.8",
          "form": "음원순위가 얼마나 자주 바뀌는데",
          "original_form": "음원순위가 얼마나 자주 바뀌는데",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000648.1.9",
          "form": "팬이라면 앨범을 사야지~",
          "original_form": "팬이라면 앨범을 사야지~",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000648.1.10",
          "form": "멜론에 가입해야 음원순위를 올려주지",
          "original_form": "멜론에 가입해야 음원순위를 올려주지",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000648.1.11",
          "form": "가끔 이벤트 한정판 앨범이나 사는거지 그래도 기본은 음원이야",
          "original_form": "가끔 이벤트 한정판 앨범이나 사는거지 그래도 기본은 음원이야",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000648.1.12",
          "form": "가수한테는 음원순위 음원수익이 제일 중요한겨",
          "original_form": "가수한테는 음원순위 음원수익이 제일 중요한겨",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000648.1.13",
          "form": "멜론에서 스밍돌릴 수 있는건가?",
          "original_form": "멜론에서 스밍돌릴 수 있는건가?",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000648.1.14",
          "form": "인지도 올리려면 음원차트에 들어야 하고",
          "original_form": "인지도 올리려면 음원차트에 들어야 하고",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000648.1.15",
          "form": "흠~ 설득당하고 있다 ㅋㅋ",
          "original_form": "흠~ 설득당하고 있다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000648.1.16",
          "form": "멜론 소리바다 지니 같은 데서",
          "original_form": "멜론 소리바다 지니 같은 데서",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000648.1.17",
          "form": "멜론 가입해야겠네~",
          "original_form": "멜론 가입해야겠네~",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000648.1.18",
          "form": "가입해서 계속 그 음원만 들어야해",
          "original_form": "가입해서 계속 그 음원만 들어야해",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000648.1.19",
          "form": "지금 2개월 100원 이벤트 하고 있으니까",
          "original_form": "지금 2개월 100원 이벤트 하고 있으니까",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000648.1.20",
          "form": "쁘걸 만세~^^",
          "original_form": "쁘걸 만세~^^",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000648.1.21",
          "form": "가입해서 좋아하는 노래 듣고 쁘걸 음원순위도 올려주고 해라",
          "original_form": "가입해서 좋아하는 노래 듣고 쁘걸 음원순위도 올려주고 해라",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000648.1.22",
          "form": "알겠네 ㅋㅋ",
          "original_form": "알겠네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000648.1.23",
          "form": "음원순위 변동은 순식간이지",
          "original_form": "음원순위 변동은 순식간이지",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000648.1.24",
          "form": "난 유튜브로만 봤는데 ㅋㅋ",
          "original_form": "난 유튜브로만 봤는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000648.1.25",
          "form": "맨날 유튜브만 보고!",
          "original_form": "맨날 유튜브만 보고!",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000648.1.26",
          "form": "유튜브도 보고 스밍도 하고 해야지",
          "original_form": "유튜브도 보고 스밍도 하고 해야지",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000648.1.27",
          "form": "이제 멜론 가입하러 간다 ㅋㅋ",
          "original_form": "이제 멜론 가입하러 간다 ㅋㅋ",
          "speaker_id": "2",
