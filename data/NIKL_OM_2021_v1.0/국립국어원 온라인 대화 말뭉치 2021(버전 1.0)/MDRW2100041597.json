      "utterance": [
        {
          "id": "MDRW2100041597.1.1",
          "form": "마자요 아빠어디가에 나와서 유명해졌죠",
          "original_form": "마자요 아빠어디가에 나와서 유명해졌죠",
          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041597.1.2",
          "form": "짜파구리도 맛있더라구요!",
          "original_form": "짜파구리도 맛있더라구요!",
          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041597.1.3",
          "form": "둘다 섞어서 만드는거죠??",
          "original_form": "둘다 섞어서 만드는거죠??",
          "speaker_id": "1",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041597.1.4",
          "form": "마자요!\r\n짜파게티만 먹으면  느끼할수있는데 합치니까 매콤해서 안느끼하고 좋더라구요",
          "original_form": "마자요!\r\n짜파게티만 먹으면  느끼할수있는데 합치니까 매콤해서 안느끼하고 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 12:07"
        },
        {
          "id": "MDRW2100041597.1.5",
          "form": "오오 저도 만들어봐야겟어여 ㅋㅋㅋㅋㅋ",
          "original_form": "오오 저도 만들어봐야겟어여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041597.1.6",
          "form": "짬짜면 생각나네요 ㅋㅋㅋ",
          "original_form": "짬짜면 생각나네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041597.1.7",
          "form": "아 짜파구리 안 드셔보셨나요?",
          "original_form": "아 짜파구리 안 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041597.1.8",
          "form": "아직 시도 안해봣어여 옛날에 맛만 봣던거 같아요 ㅋㅋ",
          "original_form": "아직 시도 안해봣어여 옛날에 맛만 봣던거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100041597.1.9",
          "form": "아 진짜요? 한번 만들어보세요 맛있어요",
          "original_form": "아 진짜요? 한번 만들어보세요 맛있어요",
          "speaker_id": "2",
          "time": "20210906 12:41"
        },
        {
          "id": "MDRW2100041597.1.10",
          "form": "그럼 컵라면도 너구리 좋아하세요?",
          "original_form": "그럼 컵라면도 너구리 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 12:42"
        },
        {
          "id": "MDRW2100041597.1.11",
          "form": "저는 왕뚜껑이나 오모리김치라면 좋아해요!!!",
          "original_form": "저는 왕뚜껑이나 오모리김치라면 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210906 13:13"
        },
        {
          "id": "MDRW2100041597.1.12",
          "form": "사실 컵라면은 다 맛있어요 ㅋㅋ",
          "original_form": "사실 컵라면은 다 맛있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:14"
        },
        {
          "id": "MDRW2100041597.1.13",
          "form": "오!오모리 김치찌개라면 진짜 맛있다그러더라구요",
          "original_form": "오!오모리 김치찌개라면 진짜 맛있다그러더라구요",
          "speaker_id": "2",
          "time": "20210906 13:31"
        },
        {
          "id": "MDRW2100041597.1.14",
          "form": "마자요 컵라면은 다 맛있죠!",
          "original_form": "마자요 컵라면은 다 맛있죠!",
          "speaker_id": "2",
          "time": "20210906 13:31"
        },
        {
          "id": "MDRW2100041597.1.15",
          "form": "네네!! 전자렌지 돌려서 먹으면",
          "original_form": "네네!! 전자렌지 돌려서 먹으면",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041597.1.16",
          "form": "국물이 찐해져요 ㅋㅋ",
          "original_form": "국물이 찐해져요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041597.1.17",
          "form": "오호 꿀팁이네요 완전!",
          "original_form": "오호 꿀팁이네요 완전!",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041597.1.18",
          "form": "네네 ㅋㅋ 근데 몸에 안좋나봐요 환경호르몬 ㅠㅠ",
          "original_form": "네네 ㅋㅋ 근데 몸에 안좋나봐요 환경호르몬 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041597.1.19",
          "form": "아 맞아요ㅠ 환경호르몬 많이 나오죠ㅠ",
          "original_form": "아 맞아요ㅠ 환경호르몬 많이 나오죠ㅠ",
