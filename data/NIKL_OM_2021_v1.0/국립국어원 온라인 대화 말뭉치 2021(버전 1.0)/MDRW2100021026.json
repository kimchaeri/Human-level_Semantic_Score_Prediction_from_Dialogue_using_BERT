        {
          "id": "MDRW2100021026.1.1",
          "form": "쇼핑 좋아하시나요",
          "original_form": "쇼핑 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100021026.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021026.1.3",
          "form": "어떤품목이요?",
          "original_form": "어떤품목이요?",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100021026.1.4",
          "form": "요샌 핸드폰 바꾸고 싶어요",
          "original_form": "요샌 핸드폰 바꾸고 싶어요",
          "speaker_id": "2",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021026.1.5",
          "form": "저도 폴드사고싶네여",
          "original_form": "저도 폴드사고싶네여",
          "speaker_id": "1",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100021026.1.6",
          "form": "저는 플립이요ㅋㅋ",
          "original_form": "저는 플립이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100021026.1.7",
          "form": "플립도 너무이쁘더라구요",
          "original_form": "플립도 너무이쁘더라구요",
          "speaker_id": "1",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100021026.1.8",
          "form": "예전 폴더폰 감성도 있고요",
          "original_form": "예전 폴더폰 감성도 있고요",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021026.1.9",
          "form": "마자요 잘나온것가타효",
          "original_form": "마자요 잘나온것가타효",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021026.1.10",
          "form": "색상도 다 예쁘던데 갖고싶네요ㅎ",
          "original_form": "색상도 다 예쁘던데 갖고싶네요ㅎ",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021026.1.11",
          "form": "최근 구매한거 있으세요?",
          "original_form": "최근 구매한거 있으세요?",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021026.1.12",
          "form": "신발샀어요",
          "original_form": "신발샀어요",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
