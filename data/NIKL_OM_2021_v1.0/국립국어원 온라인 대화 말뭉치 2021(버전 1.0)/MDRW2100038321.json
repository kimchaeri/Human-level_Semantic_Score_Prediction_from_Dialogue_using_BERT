      "utterance": [
        {
          "id": "MDRW2100038321.1.1",
          "form": "그 네이버 메인에 맨날 썸네일뜨는상품임",
          "original_form": "그 네이버 메인에 맨날 썸네일뜨는상품임",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038321.1.2",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038321.1.3",
          "form": "뭐 말하는지 알죵?",
          "original_form": "뭐 말하는지 알죵?",
          "speaker_id": "1",
          "time": "20210902 09:57"
        },
        {
          "id": "MDRW2100038321.1.4",
          "form": "광고비도 많이들어간",
          "original_form": "광고비도 많이들어간",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038321.1.5",
          "form": "비싼제품",
          "original_form": "비싼제품",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038321.1.6",
          "form": "왜 문구도 진짜 자극적이잖아요",
          "original_form": "왜 문구도 진짜 자극적이잖아요",
          "speaker_id": "1",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038321.1.7",
          "form": "뭔데 문구",
          "original_form": "뭔데 문구",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038321.1.8",
          "form": "이거하나면 다이어트 끝이런식으로",
          "original_form": "이거하나면 다이어트 끝이런식으로",
          "speaker_id": "1",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038321.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038321.1.10",
          "form": "비싼값하남?",
          "original_form": "비싼값하남?",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038321.1.11",
          "form": "네이버에 레디톡 48시간치면 나오는제품임",
          "original_form": "네이버에 레디톡 48시간치면 나오는제품임",
          "speaker_id": "1",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038321.1.12",
          "form": "효과는?",
          "original_form": "효과는?",
          "speaker_id": "2",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100038321.1.13",
          "form": "음 누가 산다고하면 도시락 싸 다니면서 말리고싶음",
          "original_form": "음 누가 산다고하면 도시락 싸 다니면서 말리고싶음",
          "speaker_id": "1",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038321.1.14",
          "form": "검색해서보면",
          "original_form": "검색해서보면",
          "speaker_id": "1",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038321.1.15",
          "form": "제품 디자인은 예쁨",
          "original_form": "제품 디자인은 예쁨",
          "speaker_id": "1",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038321.1.16",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038321.1.17",
          "form": "연예인이광고도하고",
          "original_form": "연예인이광고도하고",
          "speaker_id": "1",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038321.1.18",
          "form": "패키지도이쁘구만",
          "original_form": "패키지도이쁘구만",
          "speaker_id": "2",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038321.1.19",
          "form": "바이럴도엄청함 블로그후기 진짜많음",
          "original_form": "바이럴도엄청함 블로그후기 진짜많음",
          "speaker_id": "1",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038321.1.20",
          "form": "근데 저거 건강기능식품도 아님",
          "original_form": "근데 저거 건강기능식품도 아님",
          "speaker_id": "1",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100038321.1.21",
          "form": "나도사겠다ㅈ산다면",
          "original_form": "나도사겠다ㅈ산다면",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038321.1.22",
          "form": "다요트식품ㅈ이라며",
          "original_form": "다요트식품ㅈ이라며",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038321.1.23",
          "form": "그 흔한 녹차 카테킨성분이나 가르시니아도 안들음",
          "original_form": "그 흔한 녹차 카테킨성분이나 가르시니아도 안들음",
          "speaker_id": "1",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038321.1.24",
          "form": "막 설명은 화려하고 알록달록하지만",
          "original_form": "막 설명은 화려하고 알록달록하지만",
          "speaker_id": "1",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038321.1.25",
          "form": "사실 성분따져읽어보면",
          "original_form": "사실 성분따져읽어보면",
          "speaker_id": "1",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038321.1.26",
          "form": "자몽과즙",
          "original_form": "자몽과즙",
          "speaker_id": "1",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038321.1.27",
          "form": "비타민 정도임",
          "original_form": "비타민 정도임",
          "speaker_id": "1",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038321.1.28",
          "form": "엥?",
          "original_form": "엥?",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038321.1.29",
          "form": "공부많이ㅈ했구나",
          "original_form": "공부많이ㅈ했구나",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038321.1.30",
          "form": "근데 그럼과장광고아니가",
          "original_form": "근데 그럼과장광고아니가",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038321.1.31",
          "form": "아니 그냥 상품 뒤쪽에 식품종류 과채주스 적혀있는거보고",
          "original_form": "아니 그냥 상품 뒤쪽에 식품종류 과채주스 적혀있는거보고",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038321.1.32",
          "form": "어이털림",
          "original_form": "어이털림",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038321.1.33",
          "form": "그럼비타500이랑",
          "original_form": "그럼비타500이랑",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038321.1.34",
          "form": "다를게뭐지싶어서",
          "original_form": "다를게뭐지싶어서",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038321.1.35",
          "form": "돈버는 방식이지머",
          "original_form": "돈버는 방식이지머",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100038321.1.36",
          "form": "비싼비타오백이네",
          "original_form": "비싼비타오백이네",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100038321.1.37",
          "form": "차라리 야쿠르트 아줌마들이파는",
          "original_form": "차라리 야쿠르트 아줌마들이파는",
          "speaker_id": "1",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100038321.1.38",
          "form": "하루야채이런게 더 건강할듯",
          "original_form": "하루야채이런게 더 건강할듯",
          "speaker_id": "1",
          "time": "20210902 10:07"
        }
      ]
