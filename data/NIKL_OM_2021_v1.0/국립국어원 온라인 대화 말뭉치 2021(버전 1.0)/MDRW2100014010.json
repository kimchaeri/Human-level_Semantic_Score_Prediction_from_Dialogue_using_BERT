      "utterance": [
        {
          "id": "MDRW2100014010.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100014010.1.2",
          "form": "요새 수집하시거나 좋아하는 물건 있으세요?",
          "original_form": "요새 수집하시거나 좋아하는 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014010.1.3",
          "form": "음 영화 보고나면 포토티켓을 뽑아요",
          "original_form": "음 영화 보고나면 포토티켓을 뽑아요",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014010.1.4",
          "form": "포토티켓 쏠쏠히 모이면 은근히 뿌듯하더라구요",
          "original_form": "포토티켓 쏠쏠히 모이면 은근히 뿌듯하더라구요",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014010.1.5",
          "form": "예전에는 포스터 모았었는데 포토티켓이 더 컴팩트한거같아요",
          "original_form": "예전에는 포스터 모았었는데 포토티켓이 더 컴팩트한거같아요",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014010.1.6",
          "form": "맞아요 소장하기 편하고..",
          "original_form": "맞아요 소장하기 편하고..",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014010.1.7",
          "form": "수집하는 물건 있으세요?",
          "original_form": "수집하는 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014010.1.8",
          "form": "저는 블러셔에 좀 꽂혀서 색깔별로 모았었는데",
          "original_form": "저는 블러셔에 좀 꽂혀서 색깔별로 모았었는데",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014010.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014010.1.10",
          "form": "생각보다 제취향하고 저랑 맞는거랑은 또 다르더라구요ㅋㅋㅋㅋ",
          "original_form": "생각보다 제취향하고 저랑 맞는거랑은 또 다르더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014010.1.11",
          "form": "아그죠 좋아하는 거랑 자기랑 어울리는 거랑은 또 다르니까",
          "original_form": "아그죠 좋아하는 거랑 자기랑 어울리는 거랑은 또 다르니까",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014010.1.12",
          "form": "옷이 특히 그런 거 같아요",
          "original_form": "옷이 특히 그런 거 같아요",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014010.1.13",
          "form": "아 공감합니다",
          "original_form": "아 공감합니다",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014010.1.14",
          "form": "제 체형하고 맞는옷이 있고 안맞는 옷이 있는거같아요",
          "original_form": "제 체형하고 맞는옷이 있고 안맞는 옷이 있는거같아요",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014010.1.15",
          "form": "넥 라인도 체형을 타더라구요",
          "original_form": "넥 라인도 체형을 타더라구요",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014010.1.16",
          "form": "맞아요 저는 라운드넥이 답답해보여서 선호하진 않아요",
          "original_form": "맞아요 저는 라운드넥이 답답해보여서 선호하진 않아요",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014010.1.17",
          "form": "아 저도 라운드넥입으면 영 답답해보이더라구요",
          "original_form": "아 저도 라운드넥입으면 영 답답해보이더라구요",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014010.1.18",
          "form": "스퀘어넥 같은거 입으면 좀 시원해보이고",
          "original_form": "스퀘어넥 같은거 입으면 좀 시원해보이고",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014010.1.19",
          "form": "원피스도 스퀘어넥으로 구매하시나요?",
          "original_form": "원피스도 스퀘어넥으로 구매하시나요?",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014010.1.20",
          "form": "원피스를 잘 안입는데",
          "original_form": "원피스를 잘 안입는데",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014010.1.21",
          "form": "아마 구매한다면 스퀘어넥으로 살껏같아요",
          "original_form": "아마 구매한다면 스퀘어넥으로 살껏같아요",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014010.1.22",
