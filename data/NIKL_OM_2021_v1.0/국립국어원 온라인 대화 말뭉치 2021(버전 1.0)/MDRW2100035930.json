      "utterance": [
        {
          "id": "MDRW2100035930.1.1",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "1",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100035930.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100035930.1.3",
          "form": "오늘 계신곳의 날씨는 어때요?",
          "original_form": "오늘 계신곳의 날씨는 어때요?",
          "speaker_id": "1",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100035930.1.4",
          "form": "아까 전까지는 매우 화창하고 더웠는데",
          "original_form": "아까 전까지는 매우 화창하고 더웠는데",
          "speaker_id": "2",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100035930.1.5",
          "form": "지금은 흐리네요",
          "original_form": "지금은 흐리네요",
          "speaker_id": "2",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100035930.1.6",
          "form": "님 계신 곳은 어때요?",
          "original_form": "님 계신 곳은 어때요?",
          "speaker_id": "2",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100035930.1.7",
          "form": "제가 있는 곳도 흐리네요:)",
          "original_form": "제가 있는 곳도 흐리네요:)",
          "speaker_id": "1",
          "time": "20210830 11:57"
        },
        {
          "id": "MDRW2100035930.1.8",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "1",
          "time": "20210830 11:57"
        },
        {
          "id": "MDRW2100035930.1.9",
          "form": "날씨가 변덕이죠?",
          "original_form": "날씨가 변덕이죠?",
          "speaker_id": "2",
          "time": "20210830 11:57"
        },
        {
          "id": "MDRW2100035930.1.10",
          "form": "저는 가을 좋아해요 지금은 말고 거의 겨울 초입 전 선선한 가을요",
          "original_form": "저는 가을 좋아해요 지금은 말고 거의 겨울 초입 전 선선한 가을요",
          "speaker_id": "2",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035930.1.11",
          "form": "그러게요:)\r\n지난주부터 날씨가\r\n계속 변덕을 부리네요",
          "original_form": "그러게요:)\r\n지난주부터 날씨가\r\n계속 변덕을 부리네요",
          "speaker_id": "1",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100035930.1.12",
          "form": "저도 선선한 가을 바람 좋아해요",
          "original_form": "저도 선선한 가을 바람 좋아해요",
          "speaker_id": "1",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100035930.1.13",
          "form": "11월쯤 되면 그래도 괜찮으려나 ㅋㅋㅋ \r\n가을 바람 좋아서 어디 가기에도 좋아요 요즘은 아니지만ㅠ",
          "original_form": "11월쯤 되면 그래도 괜찮으려나 ㅋㅋㅋ \r\n가을 바람 좋아서 어디 가기에도 좋아요 요즘은 아니지만ㅠ",
          "speaker_id": "2",
          "time": "20210830 12:02"
        },
        {
          "id": "MDRW2100035930.1.14",
          "form": "그쵸ㅠ 코시국만 아니면\r\n가을엔 어디 놀러다니기도\r\n참 좋은 날씬데 아쉬워요",
          "original_form": "그쵸ㅠ 코시국만 아니면\r\n가을엔 어디 놀러다니기도\r\n참 좋은 날씬데 아쉬워요",
          "speaker_id": "1",
          "time": "20210830 12:03"
        },
        {
          "id": "MDRW2100035930.1.15",
          "form": "코시국만 아니면\r\n가을에 놀러다니기\r\n딱 좋은데 넘 아쉬워요ㅠ",
          "original_form": "코시국만 아니면\r\n가을에 놀러다니기\r\n딱 좋은데 넘 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:04"
        },
        {
          "id": "MDRW2100035930.1.16",
          "form": "맞아요 가을은 어디든 가기 좋은데...",
          "original_form": "맞아요 가을은 어디든 가기 좋은데...",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100035930.1.17",
          "form": "코로나가 아니었다면\r\n가을에 어딜 가고 싶으세요?",
          "original_form": "코로나가 아니었다면\r\n가을에 어딜 가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210830 14:02"
        },
        {
          "id": "MDRW2100035930.1.18",
          "form": "전 강원도쪽요!",
          "original_form": "전 강원도쪽요!",
          "speaker_id": "2",
          "time": "20210830 15:54"
        },
        {
          "id": "MDRW2100035930.1.19",
          "form": "그쪽은 가보질 않았거든요",
          "original_form": "그쪽은 가보질 않았거든요",
          "speaker_id": "2",
          "time": "20210830 15:54"
        },
        {
          "id": "MDRW2100035930.1.20",
          "form": "님은 어디 가고싶나요?",
          "original_form": "님은 어디 가고싶나요?",
          "speaker_id": "2",
          "time": "20210830 15:54"
        },
        {
          "id": "MDRW2100035930.1.21",
          "form": "저도 딱 코로나 시작되기전에\r\n가을에 강릉 갔었는데 그때 날씨\r\n정말 좋았어서 다시 가고싶어요^^",
          "original_form": "저도 딱 코로나 시작되기전에\r\n가을에 강릉 갔었는데 그때 날씨\r\n정말 좋았어서 다시 가고싶어요^^",
