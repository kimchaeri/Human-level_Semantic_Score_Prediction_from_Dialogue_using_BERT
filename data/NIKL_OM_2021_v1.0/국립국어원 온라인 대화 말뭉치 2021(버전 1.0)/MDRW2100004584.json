      "utterance": [
        {
          "id": "MDRW2100004584.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004584.1.2",
          "form": "아뇨 안했어요",
          "original_form": "아뇨 안했어요",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004584.1.3",
          "form": "결혼 하셨나요?",
          "original_form": "결혼 하셨나요?",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004584.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004584.1.5",
          "form": "오 배우자는 어떻게 만나셨나요?",
          "original_form": "오 배우자는 어떻게 만나셨나요?",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004584.1.6",
          "form": "친구 소개요",
          "original_form": "친구 소개요",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004584.1.7",
          "form": "연애는 얼마나 하시고 결혼했나요?",
          "original_form": "연애는 얼마나 하시고 결혼했나요?",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004584.1.8",
          "form": "1냔이요",
          "original_form": "1냔이요",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004584.1.9",
          "form": "서로 잘 맞았나 보네요",
          "original_form": "서로 잘 맞았나 보네요",
          "speaker_id": "2",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004584.1.10",
          "form": "배우자의 어떤 점이 제일 좋으셨나요?",
          "original_form": "배우자의 어떤 점이 제일 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004584.1.11",
          "form": "저랑 잘 맞더라구요",
          "original_form": "저랑 잘 맞더라구요",
          "speaker_id": "1",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004584.1.12",
          "form": "대화도",
          "original_form": "대화도",
          "speaker_id": "1",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004584.1.13",
          "form": "서로 공통관심사가 있었나요?",
          "original_form": "서로 공통관심사가 있었나요?",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004584.1.14",
          "form": "아님 같이하는 취미??",
          "original_form": "아님 같이하는 취미??",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004584.1.15",
          "form": "말이 잘 통해요",
          "original_form": "말이 잘 통해요",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004584.1.16",
          "form": "잘 맞는 상대를 만났으니 운이 좋으시네요",
          "original_form": "잘 맞는 상대를 만났으니 운이 좋으시네요",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004584.1.17",
          "form": "데이트는 주로 어디서 하셨나요?",
          "original_form": "데이트는 주로 어디서 하셨나요?",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004584.1.18",
          "form": "네 넘 행복해요",
          "original_form": "네 넘 행복해요",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004584.1.19",
          "form": "맛집 많이 다녔어요",
          "original_form": "맛집 많이 다녔어요",
          "speaker_id": "1",
          "time": "20210821 17:57"
        }
