      "utterance": [
        {
          "id": "MDRW2100008020.1.1",
          "form": "오늘 석경",
          "original_form": "오늘 석경",
          "speaker_id": "2",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100008020.1.2",
          "form": "맛집 고고",
          "original_form": "맛집 고고",
          "speaker_id": "2",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100008020.1.3",
          "form": "니는 오늘 메뉴뭐고?",
          "original_form": "니는 오늘 메뉴뭐고?",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100008020.1.4",
          "form": "국수먹을까싶은데",
          "original_form": "국수먹을까싶은데",
          "speaker_id": "1",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008020.1.5",
          "form": "하기가 귀찮네요.ㅜㅜㅋㅋ",
          "original_form": "하기가 귀찮네요.ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008020.1.6",
          "form": "국수좋지ㅋ 니가하나?",
          "original_form": "국수좋지ㅋ 니가하나?",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008020.1.7",
          "form": "난 날마다잔치집 좋아함",
          "original_form": "난 날마다잔치집 좋아함",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008020.1.8",
          "form": "점심먹었나?",
          "original_form": "점심먹었나?",
          "speaker_id": "2",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008020.1.9",
          "form": "날마다 잔치집  좋죠ㅋㅋ저도 좋아해요ㅋ",
          "original_form": "날마다 잔치집  좋죠ㅋㅋ저도 좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008020.1.10",
          "form": "편의점에서 삼각김밥이랑 컵커피, 우유사와서 급 캠핑왔어요ㅋ",
          "original_form": "편의점에서 삼각김밥이랑 컵커피, 우유사와서 급 캠핑왔어요ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008020.1.11",
          "form": "캠핑 좋으다ㅋ",
          "original_form": "캠핑 좋으다ㅋ",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008020.1.12",
          "form": "난 빽다방커피 원샷하고 얼음남은거 카누먹음",
          "original_form": "난 빽다방커피 원샷하고 얼음남은거 카누먹음",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008020.1.13",
          "form": "커피를하루에 몇잔마시는지",
          "original_form": "커피를하루에 몇잔마시는지",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008020.1.14",
          "form": "오오ㅋㅋ 빽다방커피도 맛있죠ㅋㅋ",
          "original_form": "오오ㅋㅋ 빽다방커피도 맛있죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008020.1.15",
          "form": "저는 스벅 컵커피ㅋㅋㅋ",
          "original_form": "저는 스벅 컵커피ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008020.1.16",
          "form": "편의점 사랑이쥐",
          "original_form": "편의점 사랑이쥐",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008020.1.17",
          "form": "그러니깐요,  진짜 ㅋㅋ 지금은 요플레랑 폴라레티먹고있어요ㅋㅋ",
          "original_form": "그러니깐요,  진짜 ㅋㅋ 지금은 요플레랑 폴라레티먹고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008020.1.18",
          "form": "난 오늘저녁은 요리해야된다",
          "original_form": "난 오늘저녁은 요리해야된다",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008020.1.19",
          "form": "미역국 끓여야재ㅋㅋ",
          "original_form": "미역국 끓여야재ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008020.1.20",
          "form": "고기사왔다",
          "original_form": "고기사왔다",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008020.1.21",
          "form": "오 미역국ㅋㅋ좋구만요ㅋㅋ",
          "original_form": "오 미역국ㅋㅋ좋구만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008020.1.22",
          "form": "우리는 저녁 뭘 먹을지 ㅜㅜㅋㅋ",
          "original_form": "우리는 저녁 뭘 먹을지 ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008020.1.23",
          "form": "난 미역국 끓이는중",
          "original_form": "난 미역국 끓이는중",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008020.1.24",
          "form": "디저트는 뭐먹지",
          "original_form": "디저트는 뭐먹지",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008020.1.25",
          "form": "디저트 고민까지ㅋㅋ",
          "original_form": "디저트 고민까지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008020.1.26",
          "form": "멋지네유ㅋㄲ",
          "original_form": "멋지네유ㅋㄲ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008020.1.27",
          "form": "저희는",
          "original_form": "저희는",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008020.1.28",
          "form": "첫째는 회먹고싶고ㅋㅋ",
          "original_form": "첫째는 회먹고싶고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008020.1.29",
          "form": "신랑은 고기먹고싶대요ㅋ",
          "original_form": "신랑은 고기먹고싶대요ㅋ",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008020.1.30",
          "form": "우리첫째도 회ㅋㅋ",
          "original_form": "우리첫째도 회ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008020.1.31",
          "form": "초밥도 좋아하고ㅋ",
          "original_form": "초밥도 좋아하고ㅋ",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008020.1.32",
          "form": "비슷하네요ㅋㅋ",
          "original_form": "비슷하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:38"
        }
      ]
    }
