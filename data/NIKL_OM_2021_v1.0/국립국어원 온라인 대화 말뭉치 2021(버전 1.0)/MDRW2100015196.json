      "utterance": [
        {
          "id": "MDRW2100015196.1.1",
          "form": "name1님은 종교가 있으신가요?",
          "original_form": "&name1&님은 종교가 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015196.1.2",
          "form": "전 종교 없어요! 근데 전에 교회를 몇년 다니긴 했어요",
          "original_form": "전 종교 없어요! 근데 전에 교회를 몇년 다니긴 했어요",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015196.1.3",
          "form": "저도 예전에 성당 다녔는데",
          "original_form": "저도 예전에 성당 다녔는데",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015196.1.4",
          "form": "이젠 안다녀요",
          "original_form": "이젠 안다녀요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015196.1.5",
          "form": "꾸준히 다니기 쉽지 않더라구요",
          "original_form": "꾸준히 다니기 쉽지 않더라구요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015196.1.6",
          "form": "맞아요 그리고 신앙심이라는게... 진짜 믿지 않으면 생기지가 않더라구요",
          "original_form": "맞아요 그리고 신앙심이라는게... 진짜 믿지 않으면 생기지가 않더라구요",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015196.1.7",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015196.1.8",
          "form": "전 어렸을때 다녔다가 말았던거라",
          "original_form": "전 어렸을때 다녔다가 말았던거라",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015196.1.9",
          "form": "그때부터 좀 아리송하긴 했어요",
          "original_form": "그때부터 좀 아리송하긴 했어요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015196.1.10",
          "form": "모태신앙이신건가요??",
          "original_form": "모태신앙이신건가요??",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015196.1.11",
          "form": "네 부모님이 다니셔서",
          "original_form": "네 부모님이 다니셔서",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015196.1.12",
          "form": "따라다니다가 관뒀어요",
          "original_form": "따라다니다가 관뒀어요",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015196.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015196.1.14",
          "form": "오ㅋㅋㅋㅋ 확실히 종교는 억지로는 몇번 가도 자발적으로 갈 맘이 있어야 가게 되는 거 같아요",
          "original_form": "오ㅋㅋㅋㅋ 확실히 종교는 억지로는 몇번 가도 자발적으로 갈 맘이 있어야 가게 되는 거 같아요",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015196.1.15",
          "form": "저는 모태신앙은 아니지만 초등학생 쯤에 교회 3년 정도 다녔는데도 습관이 안생기더라구요ㅋㅋㅋ",
          "original_form": "저는 모태신앙은 아니지만 초등학생 쯤에 교회 3년 정도 다녔는데도 습관이 안생기더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015196.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100015196.1.17",
          "form": "일주일에 하루를 투자하는게 어렵죠",
          "original_form": "일주일에 하루를 투자하는게 어렵죠",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100015196.1.18",
          "form": "이런걸 봐선 전 무교가 맞아요",
          "original_form": "이런걸 봐선 전 무교가 맞아요",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100015196.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ그러게욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러게욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100015196.1.20",
          "form": "그래도 저는 절로 템플스테이는 한 번 가보고 싶어요",
          "original_form": "그래도 저는 절로 템플스테이는 한 번 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100015196.1.21",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100015196.1.22",
          "form": "뭔가 힐링하는 느낌이라",
          "original_form": "뭔가 힐링하는 느낌이라",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100015196.1.23",
          "form": "차분하게 지내보고싶어요",
          "original_form": "차분하게 지내보고싶어요",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100015196.1.24",
          "form": "맞아요 아침에 일찍 일어나면서 이참에 생활패턴도 좀 바꿔보고요ㅋㅋㅋ",
          "original_form": "맞아요 아침에 일찍 일어나면서 이참에 생활패턴도 좀 바꿔보고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100015196.1.25",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100015196.1.26",
          "form": "공기 좋은 곳에서",
          "original_form": "공기 좋은 곳에서",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100015196.1.27",
          "form": "근데 그곳에 가서도 핸드폰 생각날까봐",
          "original_form": "근데 그곳에 가서도 핸드폰 생각날까봐",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100015196.1.28",
          "form": "좀 걱정되네요ㅋㅋㅋ",
          "original_form": "좀 걱정되네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100015196.1.29",
          "form": "아ㅋㅋㅋㅋ 그렇긴해요 ㅣㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ 그렇긴해요 ㅣㅋㅋㅋㅋ",
