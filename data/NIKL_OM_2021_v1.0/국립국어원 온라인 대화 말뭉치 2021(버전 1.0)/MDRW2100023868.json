      "utterance": [
        {
          "id": "MDRW2100023868.1.1",
          "form": "name2 배달 음식 자주 시켜드세요?!",
          "original_form": "&name2& 배달 음식 자주 시켜드세요?!",
          "speaker_id": "1",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100023868.1.2",
          "form": "한달에 한 두세번정도 먹는 것 같아요!",
          "original_form": "한달에 한 두세번정도 먹는 것 같아요!",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023868.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023868.1.4",
          "form": "저 자취할 때는 거의 일주일 내내 먹었던 것 같아요 ^.ㅠ",
          "original_form": "저 자취할 때는 거의 일주일 내내 먹었던 것 같아요 ^.ㅠ",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023868.1.5",
          "form": "아하 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023868.1.6",
          "form": "근데 자취하면 요리해도 재료가 남고 그러니까",
          "original_form": "근데 자취하면 요리해도 재료가 남고 그러니까",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023868.1.7",
          "form": "배달시켜먹게되는것같더라고요 다들 보니까",
          "original_form": "배달시켜먹게되는것같더라고요 다들 보니까",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023868.1.8",
          "form": "맞아요 ㅠ ㅠ 그렇다고 작게 파는 야채를 사자니",
          "original_form": "맞아요 ㅠ ㅠ 그렇다고 작게 파는 야채를 사자니",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023868.1.9",
          "form": "많이 파는 것보다 훨 비싸서",
          "original_form": "많이 파는 것보다 훨 비싸서",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023868.1.10",
          "form": "좀 부담스럽더라고요",
          "original_form": "좀 부담스럽더라고요",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023868.1.11",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023868.1.12",
          "form": "근데 배달도 배달대로 돈들고.. ㅋㅋㅋㅋ ㅜ",
          "original_form": "근데 배달도 배달대로 돈들고.. ㅋㅋㅋㅋ ㅜ",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023868.1.13",
          "form": "맞아요 건강도 엄청 나빠지고 ^.ㅠ",
          "original_form": "맞아요 건강도 엄청 나빠지고 ^.ㅠ",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023868.1.14",
          "form": "저 살도 엄청 쪘었어요 ㅠ",
          "original_form": "저 살도 엄청 쪘었어요 ㅠ",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023868.1.15",
          "form": "배달음식이 자극적인게 많아서",
          "original_form": "배달음식이 자극적인게 많아서",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023868.1.16",
          "form": "건강해치기 정말 쉬운것같아요 ㅜ",
          "original_form": "건강해치기 정말 쉬운것같아요 ㅜ",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023868.1.17",
          "form": "게다가 최소주문 금액 맞추면 양이 늘어서",
          "original_form": "게다가 최소주문 금액 맞추면 양이 늘어서",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023868.1.18",
          "form": "더 먹게 되더라고요 ㅠ.^",
          "original_form": "더 먹게 되더라고요 ㅠ.^",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023868.1.19",
          "form": "아 맞아요 ㅜ 배달비에 최소주문금액에.. 혼자 한끼먹는데 거의 이만원은 나가는 것 같아요",
          "original_form": "아 맞아요 ㅜ 배달비에 최소주문금액에.. 혼자 한끼먹는데 거의 이만원은 나가는 것 같아요",
          "speaker_id": "2",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100023868.1.20",
          "form": "네네 ㅠㅠ 가끔 고기라도 먹어보려하면 이만 오천원 훌쩍 넘더라고요 ㅜ.^",
          "original_form": "네네 ㅠㅠ 가끔 고기라도 먹어보려하면 이만 오천원 훌쩍 넘더라고요 ㅜ.^",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023868.1.21",
          "form": "그래도 이젠 잘 해먹는 편이에요!",
          "original_form": "그래도 이젠 잘 해먹는 편이에요!",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023868.1.22",
          "form": "name2도 요리 자주 하세요?",
          "original_form": "&name2&도 요리 자주 하세요?",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023868.1.23",
          "form": "자주 하진 않는데 그래도 가끔 해먹어요 ㅎㅎ",
          "original_form": "자주 하진 않는데 그래도 가끔 해먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023868.1.24",
          "form": "만만한 볶음밥이나 파스타 이런 거요 ㅋㅋㅋ",
          "original_form": "만만한 볶음밥이나 파스타 이런 거요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023868.1.25",
          "form": "헛 파스타!! 전 매번 할 때마다 실패하는 음식이에요",
          "original_form": "헛 파스타!! 전 매번 할 때마다 실패하는 음식이에요",
          "speaker_id": "1",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023868.1.26",
