      "utterance": [
        {
          "id": "MDRW2100002363.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210820 10:25"
        },
        {
          "id": "MDRW2100002363.1.2",
          "form": "요즘엔 정말 모임을 갖는게 힘드네요..ㅠ",
          "original_form": "요즘엔 정말 모임을 갖는게 힘드네요..ㅠ",
          "speaker_id": "1",
          "time": "20210820 10:25"
        },
        {
          "id": "MDRW2100002363.1.3",
          "form": "코르나때문에 일상생활이 멈춘거 같아요",
          "original_form": "코르나때문에 일상생활이 멈춘거 같아요",
          "speaker_id": "2",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002363.1.4",
          "form": "그러니까요..ㅠㅠ",
          "original_form": "그러니까요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002363.1.5",
          "form": "친구들도 못만나고",
          "original_form": "친구들도 못만나고",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002363.1.6",
          "form": "슬프네요",
          "original_form": "슬프네요",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002363.1.7",
          "form": "언제까지 계속 이럴지 힘드네요",
          "original_form": "언제까지 계속 이럴지 힘드네요",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002363.1.8",
          "form": "좀 잠잠해지나 했는데",
          "original_form": "좀 잠잠해지나 했는데",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002363.1.9",
          "form": "요즘 다시 심각해지고 잇어서...",
          "original_form": "요즘 다시 심각해지고 잇어서...",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002363.1.10",
          "form": "점점 희망이 안 보이네요..ㅠ",
          "original_form": "점점 희망이 안 보이네요..ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002363.1.11",
          "form": "모임하기에도 주변 눈치가 보여요",
          "original_form": "모임하기에도 주변 눈치가 보여요",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002363.1.12",
          "form": "맞아요.. 그냥 가볍게 얼굴만이라도 보고싶은데",
          "original_form": "맞아요.. 그냥 가볍게 얼굴만이라도 보고싶은데",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002363.1.13",
          "form": "그것또한 눈치보여서..ㅠ",
          "original_form": "그것또한 눈치보여서..ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002363.1.14",
          "form": "친구 얼굴도 까먹겠어요",
          "original_form": "친구 얼굴도 까먹겠어요",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002363.1.15",
          "form": "그러니까요~ MZ세대들은 줌으로 모인다고 하는데",
          "original_form": "그러니까요~ MZ세대들은 줌으로 모인다고 하는데",
          "speaker_id": "1",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002363.1.16",
          "form": "저는 아직 거기까지는 익숙치가 않더라고요",
          "original_form": "저는 아직 거기까지는 익숙치가 않더라고요",
          "speaker_id": "1",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002363.1.17",
          "form": "저두 신세대가 아니라서",
          "original_form": "저두 신세대가 아니라서",
          "speaker_id": "2",
          "time": "20210820 16:40"
        },
        {
          "id": "MDRW2100002363.1.18",
          "form": "ㅎㅎ 확실히 얼굴보고 얘기하는 게 좋더라고요~~",
          "original_form": "ㅎㅎ 확실히 얼굴보고 얘기하는 게 좋더라고요~~",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002363.1.19",
          "form": "맞아요 대화도 더 잘 듣게되고",
          "original_form": "맞아요 대화도 더 잘 듣게되고",
          "speaker_id": "2",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002363.1.20",
          "form": "저는 대화할때 눈보는 습관이 있어서.. 눈이 잘 안 보이면 답답하더라고요~",
