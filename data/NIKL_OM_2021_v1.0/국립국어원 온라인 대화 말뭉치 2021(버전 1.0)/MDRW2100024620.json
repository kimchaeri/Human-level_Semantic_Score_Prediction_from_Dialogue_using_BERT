      "utterance": [
        {
          "id": "MDRW2100024620.1.1",
          "form": "날씨 얘기해요ㅋㅋㅋ",
          "original_form": "날씨 얘기해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100024620.1.2",
          "form": "언니 hplc 찍는 자리에서도",
          "original_form": "언니 hplc 찍는 자리에서도",
          "speaker_id": "1",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100024620.1.3",
          "form": "퇴근할땐 안왔으면 좋겠는데",
          "original_form": "퇴근할땐 안왔으면 좋겠는데",
          "speaker_id": "2",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100024620.1.4",
          "form": "빗방울이 창문 두들기는 소리 들려요??",
          "original_form": "빗방울이 창문 두들기는 소리 들려요??",
          "speaker_id": "1",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024620.1.5",
          "form": "저도 그러기를 바라는 중이에요ㅠ",
          "original_form": "저도 그러기를 바라는 중이에요ㅠ",
          "speaker_id": "1",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024620.1.6",
          "form": "여기는 후드 소리나 그런게 커서",
          "original_form": "여기는 후드 소리나 그런게 커서",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024620.1.7",
          "form": "비소리는 잘 안들려요",
          "original_form": "비소리는 잘 안들려요",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024620.1.8",
          "form": "아하 그러겠네요!",
          "original_form": "아하 그러겠네요!",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100024620.1.9",
          "form": "언니도 장화 하나 장만하시는거 어때요?",
          "original_form": "언니도 장화 하나 장만하시는거 어때요?",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100024620.1.10",
          "form": "그러면 비 오는 날이",
          "original_form": "그러면 비 오는 날이",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100024620.1.11",
          "form": "40% 정도 괜찮아지더라구욬ㅋㅋㅋ",
          "original_form": "40% 정도 괜찮아지더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:13"
        },
        {
          "id": "MDRW2100024620.1.12",
          "form": "장화 있어요",
          "original_form": "장화 있어요",
          "speaker_id": "2",
          "time": "20210831 16:15"
        },
        {
          "id": "MDRW2100024620.1.13",
          "form": "근데 꼭 출근할땐 비가 많이 안오더라구요",
          "original_form": "근데 꼭 출근할땐 비가 많이 안오더라구요",
          "speaker_id": "2",
          "time": "20210831 16:15"
        },
        {
          "id": "MDRW2100024620.1.14",
          "form": "앗 있으시구나",
          "original_form": "앗 있으시구나",
          "speaker_id": "1",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100024620.1.15",
          "form": "흠 맞아요",
          "original_form": "흠 맞아요",
          "speaker_id": "1",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100024620.1.16",
          "form": "뭔가 출근길에 비가 와야 장화신을 마음이 생기긴하죠",
          "original_form": "뭔가 출근길에 비가 와야 장화신을 마음이 생기긴하죠",
          "speaker_id": "1",
          "time": "20210831 16:17"
        },
        {
          "id": "MDRW2100024620.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024620.1.18",
          "form": "마른땅엔 장화신으면 외려 불편해요",
          "original_form": "마른땅엔 장화신으면 외려 불편해요",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024620.1.19",
          "form": "맞아요! 발도 답답하고",
          "original_form": "맞아요! 발도 답답하고",
          "speaker_id": "1",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024620.1.20",
          "form": "통풍도 안되고 ㅎㅎ",
          "original_form": "통풍도 안되고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024620.1.21",
          "form": "이 기세라면 퇴근길에도 비가 올 것 같은데요....?",
          "original_form": "이 기세라면 퇴근길에도 비가 올 것 같은데요....?",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100024620.1.22",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100024620.1.23",
          "form": "그래도 지금 오는 정도라면.,..",
          "original_form": "그래도 지금 오는 정도라면.,..",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024620.1.24",
          "form": "아니네",
          "original_form": "아니네",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024620.1.25",
          "form": "좀 덜오면 좋겠네요",
          "original_form": "좀 덜오면 좋겠네요",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024620.1.26",
          "form": "네.. 비가 주룩주룩",
          "original_form": "네.. 비가 주룩주룩",
          "speaker_id": "1",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024620.1.27",
          "form": "생각해보니 약 1년 전에",
          "original_form": "생각해보니 약 1년 전에",
          "speaker_id": "1",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024620.1.28",
          "form": "저 여기 처음 왔을 때도 9월에 비가 많이 왔던 기억이 나네요,,",
          "original_form": "저 여기 처음 왔을 때도 9월에 비가 많이 왔던 기억이 나네요,,",
          "speaker_id": "1",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024620.1.29",
          "form": "그랬나요?",
          "original_form": "그랬나요?",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024620.1.30",
          "form": "출근하기 싫었겠어요 ㅋㅋ",
          "original_form": "출근하기 싫었겠어요 ㅋㅋ",
          "speaker_id": "2",
