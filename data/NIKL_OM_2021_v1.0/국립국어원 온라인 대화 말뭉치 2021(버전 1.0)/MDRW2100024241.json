      "utterance": [
        {
          "id": "MDRW2100024241.1.1",
          "form": "한강..",
          "original_form": "한강..",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024241.1.2",
          "form": "한번도 안봤는데..",
          "original_form": "한번도 안봤는데..",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024241.1.3",
          "form": "소년이온다는 읽었는디 채식주의자는 아직안읽어봄",
          "original_form": "소년이온다는 읽었는디 채식주의자는 아직안읽어봄",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024241.1.4",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024241.1.5",
          "form": "채식주의자",
          "original_form": "채식주의자",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024241.1.6",
          "form": "작가구나",
          "original_form": "작가구나",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024241.1.7",
          "form": "읽어봄?",
          "original_form": "읽어봄?",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024241.1.8",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024241.1.9",
          "form": "도올선생 딸",
          "original_form": "도올선생 딸",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024241.1.10",
          "form": "네 표지만",
          "original_form": "네 표지만",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024241.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024241.1.12",
          "form": "전 채식주의자가 아니라서요ㅋㅋ",
          "original_form": "전 채식주의자가 아니라서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024241.1.13",
          "form": "나도 빈그릇운동하지",
          "original_form": "나도 빈그릇운동하지",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024241.1.14",
          "form": "채식주의는아니지만 듣기로 82년생 김지영의 매운맛이라던데",
          "original_form": "채식주의는아니지만 듣기로 82년생 김지영의 매운맛이라던데",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024241.1.15",
          "form": "김지영책보다 더 앞에나옴",
          "original_form": "김지영책보다 더 앞에나옴",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024241.1.16",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024241.1.17",
          "form": "궁금하긴하네 도서관서 빌려봐야징",
          "original_form": "궁금하긴하네 도서관서 빌려봐야징",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024241.1.18",
          "form": "아 그 요한책 보셨어요?",
          "original_form": "아 그 요한책 보셨어요?",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024241.1.19",
          "form": "다봤는디 돌려드려야함",
          "original_form": "다봤는디 돌려드려야함",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024241.1.20",
          "form": "채식주의자는 보고 내용좀 알려주세요",
          "original_form": "채식주의자는 보고 내용좀 알려주세요",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024241.1.21",
          "form": "왠지 직접볼거 같진 않아서ㅋㅋ",
          "original_form": "왠지 직접볼거 같진 않아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024241.1.22",
          "form": "열심히 찾아봐야징",
          "original_form": "열심히 찾아봐야징",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024241.1.23",
          "form": "페미니즘 관련해서 책 추천해줄까요?",
          "original_form": "페미니즘 관련해서 책 추천해줄까요?",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100024241.1.24",
          "form": "조아요",
          "original_form": "조아요",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024241.1.25",
          "form": "페미니즘앞에선 그대",
          "original_form": "페미니즘앞에선 그대",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024241.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024241.1.27",
          "form": "안봤는데",
          "original_form": "안봤는데",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024241.1.28",
          "form": "내용 괜찮다더라구요",
          "original_form": "내용 괜찮다더라구요",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024241.1.29",
          "form": "보시고 추천해주셔야죠",
          "original_form": "보시고 추천해주셔야죠",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024241.1.30",
          "form": "그게 일단 전 살아남은 아이부터볼게요ㅋ",
          "original_form": "그게 일단 전 살아남은 아이부터볼게요ㅋ",
          "speaker_id": "2",
          "time": "20210830 23:54"
        }
      ]
    }
  ]
