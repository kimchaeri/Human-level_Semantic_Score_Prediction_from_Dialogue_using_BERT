        },
        {
          "id": "MDRW2100020154.1.10",
          "form": "째즞ㅇ",
          "original_form": "째즞ㅇ",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020154.1.11",
          "form": "난 녹스로 깔리지도않음",
          "original_form": "난 녹스로 깔리지도않음",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020154.1.12",
          "form": "왜그러지",
          "original_form": "왜그러지",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020154.1.13",
          "form": "그니깐",
          "original_form": "그니깐",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020154.1.14",
          "form": "지웟다 ㅋ",
          "original_form": "지웟다 ㅋ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020154.1.15",
          "form": "플디로 노래틀어놓고싶음..",
          "original_form": "플디로 노래틀어놓고싶음..",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020154.1.16",
          "form": "아 나도..",
          "original_form": "아 나도..",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020154.1.17",
          "form": "그림진전 개안나요",
          "original_form": "그림진전 개안나요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020154.1.18",
          "form": "노래틀어두고토크하멵",
          "original_form": "노래틀어두고토크하멵",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020154.1.19",
          "form": "좋겟구만..",
          "original_form": "좋겟구만..",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020154.1.20",
          "form": "저는 거의",
          "original_form": "저는 거의",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020154.1.21",
          "form": "일주일째 동결임",
          "original_form": "일주일째 동결임",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020154.1.22",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020154.1.23",
          "form": "클튜(반동결 ~ 며칠에 돌아옵니다)",
          "original_form": "클튜(반동결 ~ 며칠에 돌아옵니다)",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020154.1.24",
          "form": "나그래도 어제 조금",
          "original_form": "나그래도 어제 조금",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020154.1.25",
          "form": "따긴땃다",
          "original_form": "따긴땃다",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020154.1.26",
          "form": "하 좋걱다",
          "original_form": "하 좋걱다",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020154.1.27",
          "form": "나 아직도 라프임",
          "original_form": "나 아직도 라프임",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020154.1.28",
          "form": "오능 컨디션 좋아서",
          "original_form": "오능 컨디션 좋아서",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020154.1.29",
          "form": "저녁에 갈겨야지 햇는데",
          "original_form": "저녁에 갈겨야지 햇는데",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020154.1.30",
          "form": "영화보고 축 처쳦서",
          "original_form": "영화보고 축 처쳦서",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020154.1.31",
          "form": "아까 좀 자다옴 ㅎㅎ",
          "original_form": "아까 좀 자다옴 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
