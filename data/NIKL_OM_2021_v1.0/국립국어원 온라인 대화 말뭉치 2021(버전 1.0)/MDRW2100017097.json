      "utterance": [
        {
          "id": "MDRW2100017097.1.1",
          "form": "나 백신 맞았다고",
          "original_form": "나 백신 맞았다고",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017097.1.2",
          "form": "엄마 반차내고 오심",
          "original_form": "엄마 반차내고 오심",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017097.1.3",
          "form": "난 멀쩡한데...",
          "original_form": "난 멀쩡한데...",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017097.1.4",
          "form": "유난이여",
          "original_form": "유난이여",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017097.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017097.1.6",
          "form": "오늘 밤에 타이레놀 하나 먹고 자",
          "original_form": "오늘 밤에 타이레놀 하나 먹고 자",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017097.1.7",
          "form": "아프면 먹고 안아프면 안먹어야징",
          "original_form": "아프면 먹고 안아프면 안먹어야징",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017097.1.8",
          "form": "나 타이레놀에 대한 안좋은 기억있어서",
          "original_form": "나 타이레놀에 대한 안좋은 기억있어서",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017097.1.9",
          "form": "어 기본적으로는 그게 좋아",
          "original_form": "어 기본적으로는 그게 좋아",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017097.1.10",
          "form": "안 아프면 굳이 안 먹어도 됨",
          "original_form": "안 아프면 굳이 안 먹어도 됨",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017097.1.11",
          "form": "나도 미열 있을 때만 먹었슈",
          "original_form": "나도 미열 있을 때만 먹었슈",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017097.1.12",
          "form": "오오옹",
          "original_form": "오오옹",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017097.1.13",
          "form": "타이레놀",
          "original_form": "타이레놀",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017097.1.14",
          "form": "시러",
          "original_form": "시러",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017097.1.15",
          "form": "다른 약 먹어도 될 걸?",
          "original_form": "다른 약 먹어도 될 걸?",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017097.1.16",
          "form": "해열진통제면 괜찮지 않나",
          "original_form": "해열진통제면 괜찮지 않나",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017097.1.17",
          "form": "그런가?",
          "original_form": "그런가?",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017097.1.18",
          "form": "엉 아마두",
          "original_form": "엉 아마두",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017097.1.19",
          "form": "요즘에 동생은 어때?",
          "original_form": "요즘에 동생은 어때?",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017097.1.20",
          "form": "안 괴롭혀?",
          "original_form": "안 괴롭혀?",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017097.1.21",
          "form": "내가 방에만 툴어박혀서",
          "original_form": "내가 방에만 툴어박혀서",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017097.1.22",
          "form": "놀아가지고",
          "original_form": "놀아가지고",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017097.1.23",
          "form": "ㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017097.1.24",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017097.1.25",
          "form": "무슨 일 있으면 꼭 연락하고",
          "original_form": "무슨 일 있으면 꼭 연락하고",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017097.1.26",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017097.1.27",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017097.1.28",
          "form": "예에ㅔ",
          "original_form": "예에ㅔ",
          "speaker_id": "2",
          "time": "20210826 17:33"
        }
      ]
    }
  ]
