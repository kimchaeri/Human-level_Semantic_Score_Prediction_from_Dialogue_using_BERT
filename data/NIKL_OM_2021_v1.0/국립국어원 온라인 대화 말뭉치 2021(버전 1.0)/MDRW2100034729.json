      "utterance": [
        {
          "id": "MDRW2100034729.1.1",
          "form": "ㅋㅋㅋㅋ외국인들 관광오면 되게 좋아할 것 같은데 말이죠",
          "original_form": "ㅋㅋㅋㅋ외국인들 관광오면 되게 좋아할 것 같은데 말이죠",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034729.1.2",
          "form": "그러게요 ㅋㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034729.1.3",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034729.1.4",
          "form": "주말인데 뭐하세여",
          "original_form": "주말인데 뭐하세여",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034729.1.5",
          "form": "격하게 아무것도 안했어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "격하게 아무것도 안했어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034729.1.6",
          "form": "있는힘껏 뒹굴뒹굴 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "있는힘껏 뒹굴뒹굴 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034729.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034729.1.8",
          "form": "저도 진짜 오늘 침대밖을 안나갔어요",
          "original_form": "저도 진짜 오늘 침대밖을 안나갔어요",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100034729.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ침대에서 뒹굴뒹굴거릴때 그렇게 행복 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ침대에서 뒹굴뒹굴거릴때 그렇게 행복 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100034729.1.10",
          "form": "네 누워있는 삶 너무 편하고 좋아요",
          "original_form": "네 누워있는 삶 너무 편하고 좋아요",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034729.1.11",
          "form": "누워서 유튜브 보고 ㅋㅋㅋㅋㅋ",
          "original_form": "누워서 유튜브 보고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100034729.1.12",
          "form": "네 아예 업무 생각은 던져버리고",
          "original_form": "네 아예 업무 생각은 던져버리고",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034729.1.13",
          "form": "당연하죠 침대에서 그런 몹쓸생각 하는거 아니에요 ㅋㅋㅋㅋ",
          "original_form": "당연하죠 침대에서 그런 몹쓸생각 하는거 아니에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034729.1.14",
          "form": "저 꼭 학창시절에 시험기간에 책 다들고 가는 학생",
          "original_form": "저 꼭 학창시절에 시험기간에 책 다들고 가는 학생",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034729.1.15",
          "form": "업무해야되는 내용 양심상 침대위에 펼쳐는 놓는 사람이거근여",
          "original_form": "업무해야되는 내용 양심상 침대위에 펼쳐는 놓는 사람이거근여",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034729.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034729.1.17",
          "form": "안돼요 침대는 신성한 공간이라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "안돼요 침대는 신성한 공간이라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034729.1.18",
          "form": "양심이 또 한번 보긴봐라 하는 편이라 ㅋㅋㅌㅋㅌ 근데 오늘은 걍 다 던졌습니다",
          "original_form": "양심이 또 한번 보긴봐라 하는 편이라 ㅋㅋㅌㅋㅌ 근데 오늘은 걍 다 던졌습니다",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034729.1.19",
          "form": "잘하셧어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잘하셧어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034729.1.20",
          "form": "음악같은거 틀어두시나요 평소에",
          "original_form": "음악같은거 틀어두시나요 평소에",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034729.1.21",
          "form": "티비나 유튜브 틀어놓지 않고 잇을때 음악을 틀어요",
          "original_form": "티비나 유튜브 틀어놓지 않고 잇을때 음악을 틀어요",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034729.1.22",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034729.1.23",
          "form": "저두 방에 구글 스피커 잇어서",
          "original_form": "저두 방에 구글 스피커 잇어서",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034729.1.24",
          "form": "음악틀어줘~",
          "original_form": "음악틀어줘~",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034729.1.25",
          "form": "하고 배경음악 늘 틀어요",
          "original_form": "하고 배경음악 늘 틀어요",
          "speaker_id": "1",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100034729.1.26",
          "form": "주로 어떤 음악 틀어놓으세요? ㅋㅋ",
          "original_form": "주로 어떤 음악 틀어놓으세요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034729.1.27",
          "form": "구글이 지맘대로 트는데 보통은 우리나라 인기곡 탑백 틀어요",
          "original_form": "구글이 지맘대로 트는데 보통은 우리나라 인기곡 탑백 틀어요",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100034729.1.28",
          "form": "유튜브 프리미엄인데 이걸로 월드탑백 틀면 인도음악 나오더라구요..",
          "original_form": "유튜브 프리미엄인데 이걸로 월드탑백 틀면 인도음악 나오더라구요..",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100034729.1.29",
          "form": "인도음악 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "인도음악 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100034729.1.30",
          "form": "인도커리 먹을때 자주 흘러나오죠",
          "original_form": "인도커리 먹을때 자주 흘러나오죠",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100034729.1.31",
          "form": "커리먹으면서 흥미진진하게 뮤직비디오 봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "커리먹으면서 흥미진진하게 뮤직비디오 봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100034729.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ인도 음악 한참 인도 영화보고나서는 흥겹게 듣긴 했는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ인도 음악 한참 인도 영화보고나서는 흥겹게 듣긴 했는데",
          "speaker_id": "1",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100034729.1.33",
          "form": "아...방에 틀어놓을 정도까지는 아니더라구요ㅋㅋㅋㅋ",
          "original_form": "아...방에 틀어놓을 정도까지는 아니더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100034729.1.34",
          "form": "전 인도커리집에서 커리먹을때만 들어봣어요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 인도커리집에서 커리먹을때만 들어봣어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100034729.1.35",
          "form": "다 먹고 가게나서면 바로 까먹고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다 먹고 가게나서면 바로 까먹고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100034729.1.36",
          "form": "ㅋㅋㅋㅋㅋ뭔가 민요같잖아요",
          "original_form": "ㅋㅋㅋㅋㅋ뭔가 민요같잖아요",
          "speaker_id": "1",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100034729.1.37",
          "form": "전 들을때마다 뭔가 주문같은 기분이 들어요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 들을때마다 뭔가 주문같은 기분이 들어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100034729.1.38",
