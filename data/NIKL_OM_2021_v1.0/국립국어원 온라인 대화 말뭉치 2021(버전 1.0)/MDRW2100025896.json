      "utterance": [
        {
          "id": "MDRW2100025896.1.1",
          "form": "학교... 언제 다녔는지 기억두 안나네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "학교... 언제 다녔는지 기억두 안나네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025896.1.2",
          "form": "어머 광복전에 태어니셨나봐요",
          "original_form": "어머 광복전에 태어니셨나봐요",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025896.1.3",
          "form": "네 ㅋㅋㅋ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025896.1.4",
          "form": "광복전은 기억나요",
          "original_form": "광복전은 기억나요",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025896.1.5",
          "form": "전쟁통에 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전쟁통에 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025896.1.6",
          "form": "ㄴㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100025896.1.7",
          "form": "즐거웠나요?",
          "original_form": "즐거웠나요?",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100025896.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ네 즐거워욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ네 즐거워욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100025896.1.9",
          "form": "대학교때는 동아리활동 했나요?",
          "original_form": "대학교때는 동아리활동 했나요?",
          "speaker_id": "2",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100025896.1.10",
          "form": "아뇨 ㅋㅋ 안했어요",
          "original_form": "아뇨 ㅋㅋ 안했어요",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025896.1.11",
          "form": "아하 하고싶지않았나요?",
          "original_form": "아하 하고싶지않았나요?",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025896.1.12",
          "form": "음 집순이라 대학가기도싫었어요",
          "original_form": "음 집순이라 대학가기도싫었어요",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025896.1.13",
          "form": "안갈걸그랬나바요",
          "original_form": "안갈걸그랬나바요",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100025896.1.14",
          "form": "그랬군요",
          "original_form": "그랬군요",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100025896.1.15",
          "form": "넴 ㅠ",
          "original_form": "넴 ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100025896.1.16",
          "form": "동아리하셨어요?",
          "original_form": "동아리하셨어요?",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100025896.1.17",
          "form": "잠시 했어요 바빠서 짬이안나더라구요",
