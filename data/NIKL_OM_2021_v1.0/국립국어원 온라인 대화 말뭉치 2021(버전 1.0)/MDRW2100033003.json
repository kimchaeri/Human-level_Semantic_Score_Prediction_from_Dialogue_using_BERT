        {
          "id": "MDRW2100033003.1.1",
          "form": "돈까스가 맛없기가....",
          "original_form": "돈까스가 맛없기가....",
          "speaker_id": "1",
          "time": "20210906 12:47"
        },
        {
          "id": "MDRW2100033003.1.2",
          "form": "쉽지 않은데 ㅋㅋ",
          "original_form": "쉽지 않은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:47"
        },
        {
          "id": "MDRW2100033003.1.3",
          "form": "앗 돈까스 맛있기가 쉽지 않은거같은데요? ㅋㅋㅋㅋ",
          "original_form": "앗 돈까스 맛있기가 쉽지 않은거같은데요? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:47"
        },
        {
          "id": "MDRW2100033003.1.4",
          "form": "그 대충 튀기면 평타는 치잖아요ㄲㅋㅋ",
          "original_form": "그 대충 튀기면 평타는 치잖아요ㄲㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100033003.1.5",
          "form": "고기가맛없거나 소스가맛없거나 항상 둘중하나거나 둘다거나인거같아요 ㅋㅋ",
          "original_form": "고기가맛없거나 소스가맛없거나 항상 둘중하나거나 둘다거나인거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:50"
        },
        {
          "id": "MDRW2100033003.1.6",
          "form": "ㅋㅋ맛있는 곳에서 드셔야죠",
          "original_form": "ㅋㅋ맛있는 곳에서 드셔야죠",
          "speaker_id": "1",
          "time": "20210906 12:51"
        },
        {
          "id": "MDRW2100033003.1.7",
          "form": "별점 확인하시구",
          "original_form": "별점 확인하시구",
          "speaker_id": "1",
          "time": "20210906 12:51"
        },
        {
          "id": "MDRW2100033003.1.8",
          "form": "ㅋㅋㅋㅋ 꼼꼼하시군요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 꼼꼼하시군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:52"
        },
        {
          "id": "MDRW2100033003.1.9",
          "form": "ㅋㅋ막상 귀찮아서 잘 안봅니다",
          "original_form": "ㅋㅋ막상 귀찮아서 잘 안봅니다",
          "speaker_id": "1",
          "time": "20210906 12:53"
        },
        {
          "id": "MDRW2100033003.1.10",
          "form": "명절음식은 본가가서 드시죠?",
          "original_form": "명절음식은 본가가서 드시죠?",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100033003.1.11",
          "form": "아마 그러지 싶습니다 ㅋㅋ",
          "original_form": "아마 그러지 싶습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100033003.1.12",
          "form": "본가에선 보통 명절음식 뭐해드세요 ?",
          "original_form": "본가에선 보통 명절음식 뭐해드세요 ?",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100033003.1.13",
          "form": "동그랑땡?ㅋㅋ",
          "original_form": "동그랑땡?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033003.1.14",
          "form": "산적같은것도 있고 ㅎㅎ",
          "original_form": "산적같은것도 있고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033003.1.15",
          "form": "뭐해드시나요",
          "original_form": "뭐해드시나요",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033003.1.16",
          "form": "저희집은 명절차례 제사 안지냅니다 ㅋㅋㅋㅋ",
          "original_form": "저희집은 명절차례 제사 안지냅니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100033003.1.17",
          "form": "ㅋㅋ저희도 제사는 안지내는뎁쇼",
          "original_form": "ㅋㅋ저희도 제사는 안지내는뎁쇼",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100033003.1.18",
          "form": "그럼 명절음식 별로 안하지 않나요? ㅋㅋㅋ",
