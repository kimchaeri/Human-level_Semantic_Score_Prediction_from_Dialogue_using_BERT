      "utterance": [
        {
          "id": "MDRW2100016164.1.1",
          "form": "무슨동물 좋아하세요?",
          "original_form": "무슨동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100016164.1.2",
          "form": "저 모든 냥님과 멍님요...! ㅋㅋㅋㅋ",
          "original_form": "저 모든 냥님과 멍님요...! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100016164.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100016164.1.4",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016164.1.5",
          "form": "동물키우고싶어요",
          "original_form": "동물키우고싶어요",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016164.1.6",
          "form": "name1님도 반려동물 없으신가요?!",
          "original_form": "&name1&님도 반려동물 없으신가요?!",
          "speaker_id": "2",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100016164.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016164.1.8",
          "form": "햄스터를 키웠었는데",
          "original_form": "햄스터를 키웠었는데",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016164.1.9",
          "form": "6월에 죽었어요 ㅜ ㅜ",
          "original_form": "6월에 죽었어요 ㅜ ㅜ",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016164.1.10",
          "form": "앗ㅠㅠㅠ 많이 그리우시겠어요 ㅠㅠㅠㅠ",
          "original_form": "앗ㅠㅠㅠ 많이 그리우시겠어요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016164.1.11",
          "form": "햄스터별에서 잘 지내고 있을거에요...ㅠㅠ",
          "original_form": "햄스터별에서 잘 지내고 있을거에요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016164.1.12",
          "form": "네 ㅜ 아직도 슬퍼요",
          "original_form": "네 ㅜ 아직도 슬퍼요",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016164.1.13",
          "form": "귀여웠어요 착하고",
          "original_form": "귀여웠어요 착하고",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016164.1.14",
          "form": "ㅠㅠ햄스터가 멸치 야무지게 뼈발라먹은 사진 보고 심쿵했었는데 ㅠㅠ",
          "original_form": "ㅠㅠ햄스터가 멸치 야무지게 뼈발라먹은 사진 보고 심쿵했었는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016164.1.15",
          "form": "헛 그런사진이 있나요",
          "original_form": "헛 그런사진이 있나요",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016164.1.16",
          "form": "네!! 엄청 귀여워요ㅠㅠ 심장 뿌셔 지구뿌셔!!!",
          "original_form": "네!! 엄청 귀여워요ㅠㅠ 심장 뿌셔 지구뿌셔!!!",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016164.1.17",
          "form": "여기 사진 보낼 수 있으면 보내드렸을텐데 ㅠㅠㅠ",
          "original_form": "여기 사진 보낼 수 있으면 보내드렸을텐데 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016164.1.18",
          "form": "아 궁금해서 보고왔어요 ㅋㅋ",
          "original_form": "아 궁금해서 보고왔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016164.1.19",
          "form": "진짜 귀엽죠? ㅠㅠ",
          "original_form": "진짜 귀엽죠? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016164.1.20",
          "form": "전투적으로 먹었네요 ㅋㅋ",
          "original_form": "전투적으로 먹었네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016164.1.21",
          "form": "네 ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016164.1.22",
          "form": "ㅋㅋ진짜 야물딱져요 ㅠㅠ 포동포동한 볼도 어찌나 귀여운지",
          "original_form": "ㅋㅋ진짜 야물딱져요 ㅠㅠ 포동포동한 볼도 어찌나 귀여운지",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016164.1.23",
