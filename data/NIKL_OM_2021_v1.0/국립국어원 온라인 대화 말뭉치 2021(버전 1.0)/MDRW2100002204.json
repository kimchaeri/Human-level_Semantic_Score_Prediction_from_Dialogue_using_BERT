      "utterance": [
        {
          "id": "MDRW2100002204.1.1",
          "form": "사고싶은 물건 같은 거 있으신가요?",
          "original_form": "사고싶은 물건 같은 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002204.1.2",
          "form": "저는 요새 바람막이랑 러닝화 사고싶어요ㅎㅎ",
          "original_form": "저는 요새 바람막이랑 러닝화 사고싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002204.1.3",
          "form": "밖에서 뛰거나 걸을때 있으면 좋을거같더라구요",
          "original_form": "밖에서 뛰거나 걸을때 있으면 좋을거같더라구요",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002204.1.4",
          "form": "아 너무 좋죠",
          "original_form": "아 너무 좋죠",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002204.1.5",
          "form": "러닝하는 사람들도 많던데 런닝화 신고 해야잖아요!!",
          "original_form": "러닝하는 사람들도 많던데 런닝화 신고 해야잖아요!!",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002204.1.6",
          "form": "그니까요 런닝화 안신어서그런건지 몰라도 발이랑 발목 아프더라구요ㅠ",
          "original_form": "그니까요 런닝화 안신어서그런건지 몰라도 발이랑 발목 아프더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002204.1.7",
          "form": "요새 구입하신거나 사고싶은거 있으세요?",
          "original_form": "요새 구입하신거나 사고싶은거 있으세요?",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002204.1.8",
          "form": "전 요새 뜬금없지만 집에 있는 가전제품을 다 바꾸고싶어요",
          "original_form": "전 요새 뜬금없지만 집에 있는 가전제품을 다 바꾸고싶어요",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002204.1.9",
          "form": "오래되기도 했고 ㅎ 하지만 비용이 크니까...",
          "original_form": "오래되기도 했고 ㅎ 하지만 비용이 크니까...",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002204.1.10",
          "form": "마자요 비용 만만치 않죠\r\n저도 오래된거있으면 바꾸고싶더라구요",
          "original_form": "마자요 비용 만만치 않죠\r\n저도 오래된거있으면 바꾸고싶더라구요",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002204.1.11",
          "form": "요새 가전들이  다 이쁘게 잘 나와서 더 바꾸고싶어요",
          "original_form": "요새 가전들이  다 이쁘게 잘 나와서 더 바꾸고싶어요",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002204.1.12",
          "form": "그치요.... 정말 싹 다 바꾸고싶어요....",
          "original_form": "그치요.... 정말 싹 다 바꾸고싶어요....",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002204.1.13",
          "form": "엘지 예쁘더라구요",
          "original_form": "엘지 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002204.1.14",
          "form": "엘지꺼 예쁘죠\r\n근데 가전들이 고장도 잘 안나서 멀쩡한거 바꾸기도 애매해요ㅋㅋ",
          "original_form": "엘지꺼 예쁘죠\r\n근데 가전들이 고장도 잘 안나서 멀쩡한거 바꾸기도 애매해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002204.1.15",
          "form": "ㅋㅋ ㅋㅋㅋ 맞아요 바꾸고 나서 원래 가전들을 버리기도 아깝고..",
          "original_form": "ㅋㅋ ㅋㅋㅋ 맞아요 바꾸고 나서 원래 가전들을 버리기도 아깝고..",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002204.1.16",
          "form": "처리하는것도 일이죠 \r\n버릴때 돈들어가기도하고",
          "original_form": "처리하는것도 일이죠 \r\n버릴때 돈들어가기도하고",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002204.1.17",
          "form": "그니까요ㅋㅋㅋㅋㅋㅋㅠ 그래수 요즘 사람들은",
          "original_form": "그니까요ㅋㅋㅋㅋㅋㅋㅠ 그래수 요즘 사람들은",
          "speaker_id": "2",
          "time": "20210820 01:04"
        },
        {
          "id": "MDRW2100002204.1.18",
          "form": "당근에 올리더라구요,,",
          "original_form": "당근에 올리더라구요,,",
          "speaker_id": "2",
          "time": "20210820 01:04"
        },
        {
          "id": "MDRW2100002204.1.19",
          "form": "마자요 당근이 그래서 좋은거같아요",
          "original_form": "마자요 당근이 그래서 좋은거같아요",
          "speaker_id": "1",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002204.1.20",
          "form": "당근이 그런 메리트가 아쥬 넘치죠",
          "original_form": "당근이 그런 메리트가 아쥬 넘치죠",
          "speaker_id": "2",
          "time": "20210820 01:20"
        }
      ]
    }
