      "utterance": [
        {
          "id": "MDRW2100007628.1.1",
          "form": "학교 사람만나요?",
          "original_form": "학교 사람만나요?",
          "speaker_id": "1",
          "time": "20210822 08:42"
        },
        {
          "id": "MDRW2100007628.1.2",
          "form": "고등학교 동창들은 만나요",
          "original_form": "고등학교 동창들은 만나요",
          "speaker_id": "2",
          "time": "20210822 08:44"
        },
        {
          "id": "MDRW2100007628.1.3",
          "form": "학생인가요?",
          "original_form": "학생인가요?",
          "speaker_id": "2",
          "time": "20210822 08:44"
        },
        {
          "id": "MDRW2100007628.1.4",
          "form": "직장인이요",
          "original_form": "직장인이요",
          "speaker_id": "1",
          "time": "20210822 08:44"
        },
        {
          "id": "MDRW2100007628.1.5",
          "form": "일하니 만나기 쉽지 않네오",
          "original_form": "일하니 만나기 쉽지 않네오",
          "speaker_id": "1",
          "time": "20210822 08:44"
        },
        {
          "id": "MDRW2100007628.1.6",
          "form": "준비하셨던 시험이 있나요?",
          "original_form": "준비하셨던 시험이 있나요?",
          "speaker_id": "2",
          "time": "20210822 08:44"
        },
        {
          "id": "MDRW2100007628.1.7",
          "form": "자격증이요",
          "original_form": "자격증이요",
          "speaker_id": "1",
          "time": "20210822 08:45"
        },
        {
          "id": "MDRW2100007628.1.8",
          "form": "어떤자격증인가요?",
          "original_form": "어떤자격증인가요?",
          "speaker_id": "2",
          "time": "20210822 08:45"
        },
        {
          "id": "MDRW2100007628.1.9",
          "form": "전기기사요",
          "original_form": "전기기사요",
          "speaker_id": "1",
          "time": "20210822 08:46"
        },
        {
          "id": "MDRW2100007628.1.10",
          "form": "전기기사를 공부할때 뭐가 제일 힘드셨어요?",
          "original_form": "전기기사를 공부할때 뭐가 제일 힘드셨어요?",
          "speaker_id": "2",
          "time": "20210822 08:47"
        },
        {
          "id": "MDRW2100007628.1.11",
          "form": "전공인데도 생소한게 ㅎ",
          "original_form": "전공인데도 생소한게 ㅎ",
          "speaker_id": "1",
          "time": "20210822 08:47"
        },
        {
          "id": "MDRW2100007628.1.12",
          "form": "전공이 전기 관련 이였나요?",
          "original_form": "전공이 전기 관련 이였나요?",
          "speaker_id": "2",
          "time": "20210822 08:48"
        },
        {
          "id": "MDRW2100007628.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 08:49"
        },
        {
          "id": "MDRW2100007628.1.14",
          "form": "기사자격증은 실무경험이나 학사가 필요한 자격증이니 당연한이야기겠네요 어떤 과였나요",
          "original_form": "기사자격증은 실무경험이나 학사가 필요한 자격증이니 당연한이야기겠네요 어떤 과였나요",
          "speaker_id": "2",
          "time": "20210822 08:50"
        },
        {
          "id": "MDRW2100007628.1.15",
          "form": "전기요",
          "original_form": "전기요",
          "speaker_id": "1",
          "time": "20210822 08:50"
        }
