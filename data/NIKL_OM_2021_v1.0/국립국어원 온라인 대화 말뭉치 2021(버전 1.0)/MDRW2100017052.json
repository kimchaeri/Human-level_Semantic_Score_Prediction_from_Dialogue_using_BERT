        },
        {
          "id": "MDRW2100017052.1.2",
          "form": "오늘 온다 했었는데 안 온 듯",
          "original_form": "오늘 온다 했었는데 안 온 듯",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017052.1.3",
          "form": "그러게 뭔 장마라더니 틀리네",
          "original_form": "그러게 뭔 장마라더니 틀리네",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017052.1.4",
          "form": "그러게 너무 습하다",
          "original_form": "그러게 너무 습하다",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017052.1.5",
          "form": "옹 ㅠㅠ 습하기만 하고 시원하게 비가 안내리네",
          "original_form": "옹 ㅠㅠ 습하기만 하고 시원하게 비가 안내리네",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017052.1.6",
          "form": "그러게 좀 내리면 좋겠다",
          "original_form": "그러게 좀 내리면 좋겠다",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017052.1.7",
          "form": "옹 ㅠㅠ 답답",
          "original_form": "옹 ㅠㅠ 답답",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017052.1.8",
          "form": "내일은 오는 거 같더라",
          "original_form": "내일은 오는 거 같더라",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017052.1.9",
          "form": "오 좋넴",
