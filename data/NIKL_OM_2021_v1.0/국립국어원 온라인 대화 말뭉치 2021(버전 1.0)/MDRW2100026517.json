      "utterance": [
        {
          "id": "MDRW2100026517.1.1",
          "form": "여행조아하시나요",
          "original_form": "여행조아하시나요",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026517.1.2",
          "form": "너무 좋아하는데 못가고 있어요",
          "original_form": "너무 좋아하는데 못가고 있어요",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026517.1.3",
          "form": "좋아하는 여행지가 있으신가요?",
          "original_form": "좋아하는 여행지가 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026517.1.4",
          "form": "제주도요 ㅠㅋㅋㅋㅋ",
          "original_form": "제주도요 ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026517.1.5",
          "form": "가고싶어요ㅠㅠㅠㅠ",
          "original_form": "가고싶어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026517.1.6",
          "form": "저는 4월에 다녀왔는ㄷ0",
          "original_form": "저는 4월에 다녀왔는ㄷ0",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026517.1.7",
          "form": "지금은 조금 무서워서 못가겠어요",
          "original_form": "지금은 조금 무서워서 못가겠어요",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026517.1.8",
          "form": "제주 어디 좋아하세요?",
          "original_form": "제주 어디 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026517.1.9",
          "form": "요즘좀 그렇죠ㅠ",
          "original_form": "요즘좀 그렇죠ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026517.1.10",
          "form": "4월 부럽다 엄청조을때아닌가",
          "original_form": "4월 부럽다 엄청조을때아닌가",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026517.1.11",
          "form": "성산 함덕?",
          "original_form": "성산 함덕?",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026517.1.12",
          "form": "근데 오름에 못가봐서 오름가고싶네용",
          "original_form": "근데 오름에 못가봐서 오름가고싶네용",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026517.1.13",
          "form": "오 저는 성산에서 묵었는데",
          "original_form": "오 저는 성산에서 묵었는데",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026517.1.14",
          "form": "앗 어디용",
          "original_form": "앗 어디용",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026517.1.15",
          "form": "혹시골든튤립",
          "original_form": "혹시골든튤립",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026517.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026517.1.17",
          "form": "가을엔 산굼부리 추천드려요",
          "original_form": "가을엔 산굼부리 추천드려요",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026517.1.18",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026517.1.19",
          "form": "저장해놀게요",
          "original_form": "저장해놀게요",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026517.1.20",
          "form": "저는 휘닉스요",
          "original_form": "저는 휘닉스요",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026517.1.21",
          "form": "아하 ㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026517.1.22",
          "form": "골든튤립 핫한가요ㅋ",
          "original_form": "골든튤립 핫한가요ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026517.1.23",
          "form": "ㅋㅋㅋㅋ 많이나오더라구요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 많이나오더라구요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026517.1.24",
          "form": "싸고 ㅋㅋㅋ 가성비좋은",
          "original_form": "싸고 ㅋㅋㅋ 가성비좋은",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026517.1.25",
          "form": "제주도 요즘 렌트비 엄청 비싸졌드라구요",
          "original_form": "제주도 요즘 렌트비 엄청 비싸졌드라구요",
