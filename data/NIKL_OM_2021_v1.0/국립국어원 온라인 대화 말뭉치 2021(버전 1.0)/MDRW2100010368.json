      "utterance": [
        {
          "id": "MDRW2100010368.1.1",
          "form": "나 충전기가 고장난 것 겉아",
          "original_form": "나 충전기가 고장난 것 겉아",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010368.1.2",
          "form": "헉 그러면 어떡해?",
          "original_form": "헉 그러면 어떡해?",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010368.1.3",
          "form": "여행갈때 필수인데ㅠㅠ",
          "original_form": "여행갈때 필수인데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010368.1.4",
          "form": "아마 보조배터리를 빌릴 것 같아",
          "original_form": "아마 보조배터리를 빌릴 것 같아",
          "speaker_id": "1",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010368.1.5",
          "form": "내가 빌려주고싶다",
          "original_form": "내가 빌려주고싶다",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010368.1.6",
          "form": "그럼 빌려줄래??",
          "original_form": "그럼 빌려줄래??",
          "speaker_id": "1",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010368.1.7",
          "form": "아니면 삼성에서 근무하는 그 친구에게",
          "original_form": "아니면 삼성에서 근무하는 그 친구에게",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010368.1.8",
          "form": "기념품으로 뷰탁하는건 어때?",
          "original_form": "기념품으로 뷰탁하는건 어때?",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010368.1.9",
          "form": "너네집까지 받으러갈께!",
          "original_form": "너네집까지 받으러갈께!",
          "speaker_id": "1",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010368.1.10",
          "form": "흠 많이 곤란한 부탁이구나.",
          "original_form": "흠 많이 곤란한 부탁이구나.",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.11",
          "form": "흠 그것도 좋겠다",
          "original_form": "흠 그것도 좋겠다",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.12",
          "form": "5년만 더 고민해봐도 될까?",
          "original_form": "5년만 더 고민해봐도 될까?",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.13",
          "form": "전에 충전기없이 여행가봤는데",
          "original_form": "전에 충전기없이 여행가봤는데",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.14",
          "form": "사진도 못찍고 너무 힘들었어",
          "original_form": "사진도 못찍고 너무 힘들었어",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.15",
          "form": "나는 폰을 끄고살아야해서 힘들었어",
          "original_form": "나는 폰을 끄고살아야해서 힘들었어",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.16",
          "form": "너무 심심했어. 이동시간이",
          "original_form": "너무 심심했어. 이동시간이",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.17",
          "form": "너도 그랬던적있니??",
          "original_form": "너도 그랬던적있니??",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.18",
          "form": "충전기는 정말 여행필수품이야.",
          "original_form": "충전기는 정말 여행필수품이야.",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.19",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.20",
          "form": "맞아 여행 이동시간에 음악도 못들어",
          "original_form": "맞아 여행 이동시간에 음악도 못들어",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010368.1.21",
          "form": "휴대폰중독자라면 모두가 그럴거야.",
          "original_form": "휴대폰중독자라면 모두가 그럴거야.",
          "speaker_id": "2",
          "time": "20210823 23:32"
        }
      ]
