        {
          "id": "MDRW2100019870.1.1",
          "form": "날이 너무시원하네요",
          "original_form": "날이 너무시원하네요",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019870.1.2",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019870.1.3",
          "form": "에어컨요금 이제 안나오겠어ㅛ",
          "original_form": "에어컨요금 이제 안나오겠어ㅛ",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019870.1.4",
          "form": "대신 전기장판을 틀겠죠",
          "original_form": "대신 전기장판을 틀겠죠",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019870.1.5",
          "form": "내일은 뭘 입어야하려나",
          "original_form": "내일은 뭘 입어야하려나",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019870.1.6",
          "form": "예쁘겠어요",
          "original_form": "예쁘겠어요",
          "speaker_id": "1",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019870.1.7",
          "form": "청바지랑 가디건 입을까봐요",
          "original_form": "청바지랑 가디건 입을까봐요",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019870.1.8",
          "form": "예쁘겠어요",
          "original_form": "예쁘겠어요",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019870.1.9",
          "form": "코디해야지",
          "original_form": "코디해야지",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019870.1.10",
          "form": "저는 집콕이라",
          "original_form": "저는 집콕이라",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019870.1.11",
          "form": "가을옷도 사야겠어요",
          "original_form": "가을옷도 사야겠어요",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019870.1.12",
          "form": "저두 고민중이에요",
          "original_form": "저두 고민중이에요",
