      "utterance": [
        {
          "id": "MDRW2100041197.1.1",
          "form": "어디로 나들이하고 싶으세요?",
          "original_form": "어디로 나들이하고 싶으세요?",
          "speaker_id": "1",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100041197.1.2",
          "form": "강릉 예전에 한번 갔었는데 다시 가고싶네요",
          "original_form": "강릉 예전에 한번 갔었는데 다시 가고싶네요",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100041197.1.3",
          "form": "강릉 좋죠",
          "original_form": "강릉 좋죠",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100041197.1.4",
          "form": "저도 작년여름에 가봤는데",
          "original_form": "저도 작년여름에 가봤는데",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100041197.1.5",
          "form": "좋더라고요",
          "original_form": "좋더라고요",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100041197.1.6",
          "form": "순두부드셨어요?",
          "original_form": "순두부드셨어요?",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100041197.1.7",
          "form": "순두부도 먹고",
          "original_form": "순두부도 먹고",
          "speaker_id": "1",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100041197.1.8",
          "form": "꼬막비빔밥도 먹고",
          "original_form": "꼬막비빔밥도 먹고",
          "speaker_id": "1",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100041197.1.9",
          "form": "물회도 먹고요ㅎㅎ",
          "original_form": "물회도 먹고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100041197.1.10",
          "form": "숙박하셨군요!",
          "original_form": "숙박하셨군요!",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100041197.1.11",
          "form": "저는 당일치기라 한끼밖에 못먹었어요ㅜ",
          "original_form": "저는 당일치기라 한끼밖에 못먹었어요ㅜ",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100041197.1.12",
          "form": "네 저는 2박 3일이요",
          "original_form": "네 저는 2박 3일이요",
          "speaker_id": "1",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100041197.1.13",
          "form": "진짜 좋죠?ㅜㅠ",
          "original_form": "진짜 좋죠?ㅜㅠ",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100041197.1.14",
          "form": "강릉에서 바다는 어디 가셨어요?",
          "original_form": "강릉에서 바다는 어디 가셨어요?",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100041197.1.15",
          "form": "안목이었던거같아요",
          "original_form": "안목이었던거같아요",
          "speaker_id": "2",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100041197.1.16",
          "form": "아 저도 첫날엔 안목 갔는데",
          "original_form": "아 저도 첫날엔 안목 갔는데",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100041197.1.17",
          "form": "사천 쪽이 더 좋더라고요",
          "original_form": "사천 쪽이 더 좋더라고요",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100041197.1.18",
          "form": "마지막날 지나오면서 잠깐 들렀는데",
          "original_form": "마지막날 지나오면서 잠깐 들렀는데",
          "speaker_id": "1",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100041197.1.19",
          "form": "다음엔 사천쪽으로!!",
          "original_form": "다음엔 사천쪽으로!!",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100041197.1.20",
          "form": "사천이 더 한적하고 분위기 좋아서",
          "original_form": "사천이 더 한적하고 분위기 좋아서",
          "speaker_id": "1",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100041197.1.21",
          "form": "다음엔 사천쪽으로 숙소잡고 지내야겠다 생각했어요",
          "original_form": "다음엔 사천쪽으로 숙소잡고 지내야겠다 생각했어요",
          "speaker_id": "1",
          "time": "20210905 20:05"
        }
      ]
