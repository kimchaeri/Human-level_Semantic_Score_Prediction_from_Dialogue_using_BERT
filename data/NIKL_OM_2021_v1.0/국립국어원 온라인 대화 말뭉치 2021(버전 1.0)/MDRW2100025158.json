      "utterance": [
        {
          "id": "MDRW2100025158.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025158.1.2",
          "form": "네 안녕하세요~",
          "original_form": "네 안녕하세요~",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025158.1.3",
          "form": "요즘 하시는 운동 있으세요??",
          "original_form": "요즘 하시는 운동 있으세요??",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025158.1.4",
          "form": "요즘 비와서 홈트하고있어요~",
          "original_form": "요즘 비와서 홈트하고있어요~",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100025158.1.5",
          "form": "운동 어떤거 하세요?",
          "original_form": "운동 어떤거 하세요?",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100025158.1.6",
          "form": "저도 유튜브 보고 홈트 자주해요!",
          "original_form": "저도 유튜브 보고 홈트 자주해요!",
          "speaker_id": "1",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100025158.1.7",
          "form": "홈트 유튜브 보고 하세요?",
          "original_form": "홈트 유튜브 보고 하세요?",
          "speaker_id": "1",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025158.1.8",
          "form": "아 네 땅끄부부 주로 봐요~",
          "original_form": "아 네 땅끄부부 주로 봐요~",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025158.1.9",
          "form": "어떤거 보세요?",
          "original_form": "어떤거 보세요?",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025158.1.10",
          "form": "저도 땅끄부부 자주 봐요!",
          "original_form": "저도 땅끄부부 자주 봐요!",
          "speaker_id": "1",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025158.1.11",
          "form": "항상 웃으면서 하셔서 기분이 좋아져요ㅎㅎ",
          "original_form": "항상 웃으면서 하셔서 기분이 좋아져요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025158.1.12",
          "form": "맞아요 그리고 초보자가 하기에 쉬워서 좋아요!",
          "original_form": "맞아요 그리고 초보자가 하기에 쉬워서 좋아요!",
          "speaker_id": "2",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025158.1.13",
          "form": "다이어트 하고계신가요?",
          "original_form": "다이어트 하고계신가요?",
          "speaker_id": "2",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025158.1.14",
          "form": "다이어터는 아니고 유지어터입니다ㅎㅎ",
          "original_form": "다이어터는 아니고 유지어터입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025158.1.15",
          "form": "부러워요 전 아직 다이어트중이라ㅠㅠ",
          "original_form": "부러워요 전 아직 다이어트중이라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025158.1.16",
          "form": "목표체중에 도달하신건가요?",
          "original_form": "목표체중에 도달하신건가요?",
          "speaker_id": "2",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025158.1.17",
          "form": "저도 코로나때문에 살이 엄청 쪄서",
          "original_form": "저도 코로나때문에 살이 엄청 쪄서",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025158.1.18",
          "form": "겨우 원래 몸무게 회복했어요",
          "original_form": "겨우 원래 몸무게 회복했어요",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025158.1.19",
          "form": "저는 운동보다 식단이 어려운것같아요",
          "original_form": "저는 운동보다 식단이 어려운것같아요",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025158.1.20",
          "form": "아 저두요ㅜㅠ 세상에 맛있는게 너무많아서",
          "original_form": "아 저두요ㅜㅠ 세상에 맛있는게 너무많아서",
          "speaker_id": "2",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100025158.1.21",
          "form": "식단은 항상 숙제에요ㅜㅜ",
          "original_form": "식단은 항상 숙제에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100025158.1.22",
          "form": "단백질도 맛없고ㅠ",
          "original_form": "단백질도 맛없고ㅠ",
          "speaker_id": "2",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100025158.1.23",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025158.1.24",
          "form": "왜 맛있는건 칼로리가 높을까요??",
          "original_form": "왜 맛있는건 칼로리가 높을까요??",
          "speaker_id": "1",
          "time": "20210901 00:16"
        },
        {
          "id": "MDRW2100025158.1.25",
          "form": "그니까요ㅜㅠ 담엔 살안찌는 체질로 태어나고싶어요",
          "original_form": "그니까요ㅜㅠ 담엔 살안찌는 체질로 태어나고싶어요",
          "speaker_id": "2",
          "time": "20210901 00:19"
        }
      ]
    }
