      "utterance": [
        {
          "id": "MDRW2100034236.1.1",
          "form": "아니요 ㅜㅜ 해보셨어요?",
          "original_form": "아니요 ㅜㅜ 해보셨어요?",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034236.1.2",
          "form": "네 해보긴했는데 제 스타일은 아니더라구요",
          "original_form": "네 해보긴했는데 제 스타일은 아니더라구요",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034236.1.3",
          "form": "저는 자연스럽게 알아가는게 더 좋아서ㅠ",
          "original_form": "저는 자연스럽게 알아가는게 더 좋아서ㅠ",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034236.1.4",
          "form": "아 저두요! 자만추시군요 ㅋㅋㅋ",
          "original_form": "아 저두요! 자만추시군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034236.1.5",
          "form": "마자요! 자만추에요",
          "original_form": "마자요! 자만추에요",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034236.1.6",
          "form": "그래서 연애하기가 힘드네요ㅋㅋ",
          "original_form": "그래서 연애하기가 힘드네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034236.1.7",
          "form": "자연스럽게 만날곳이 딱히 없더라구요",
          "original_form": "자연스럽게 만날곳이 딱히 없더라구요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034236.1.8",
          "form": "맞아요 그런 사람들이 대부분 솔로더라구요 저처럼 ㅌㅋㅋㅋㅋ",
          "original_form": "맞아요 그런 사람들이 대부분 솔로더라구요 저처럼 ㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034236.1.9",
          "form": "ㅋㅋㅋㅋ제  얘기네요ㅋㅋ\r\n연애 많이 해보셨나요?",
          "original_form": "ㅋㅋㅋㅋ제  얘기네요ㅋㅋ\r\n연애 많이 해보셨나요?",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034236.1.10",
          "form": "별로 못해봤어요 ㅜㅜ 많이 해보셨어요?",
          "original_form": "별로 못해봤어요 ㅜㅜ 많이 해보셨어요?",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034236.1.11",
          "form": "아니요ㅋㅋ저두 많이 못해봤어요ㅋㅋ",
          "original_form": "아니요ㅋㅋ저두 많이 못해봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034236.1.12",
          "form": "엄청 금사빠긴한데ㅋㅋ",
          "original_form": "엄청 금사빠긴한데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034236.1.13",
          "form": "금사빠 오랜만에 들어요 ㅋㅋㅋ",
          "original_form": "금사빠 오랜만에 들어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034236.1.14",
          "form": "나중에 인연을 만나겠죠? 저희둘다 ㅋㅋㅋㅋ",
          "original_form": "나중에 인연을 만나겠죠? 저희둘다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034236.1.15",
          "form": "너무  올드했나요?ㅋㅋㅋ",
          "original_form": "너무  올드했나요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034236.1.16",
          "form": "마자요 언젠간 찾아오겠죠ㅋㅋ",
          "original_form": "마자요 언젠간 찾아오겠죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100034236.1.17",
          "form": "ㅋㅋㅋㅋ 아녀 추억돋는 말이었어요 약간 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 아녀 추억돋는 말이었어요 약간 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100034236.1.18",
          "form": "결혼생각은 있으세요?",
          "original_form": "결혼생각은 있으세요?",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100034236.1.19",
          "form": "아니요 연애는 하고싶은데 \r\n결혼은 별로 안하고싶어요",
          "original_form": "아니요 연애는 하고싶은데 \r\n결혼은 별로 안하고싶어요",
          "speaker_id": "1",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100034236.1.20",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100034236.1.21",
