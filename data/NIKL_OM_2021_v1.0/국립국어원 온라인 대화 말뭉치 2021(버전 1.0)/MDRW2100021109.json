      "utterance": [
        {
          "id": "MDRW2100021109.1.1",
          "form": "집안일해요??",
          "original_form": "집안일해요??",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100021109.1.2",
          "form": "집에서 잠깐 나왔어요",
          "original_form": "집에서 잠깐 나왔어요",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100021109.1.3",
          "form": "집안일하다 피곤해서 세라젬카페왔어요",
          "original_form": "집안일하다 피곤해서 세라젬카페왔어요",
          "speaker_id": "2",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100021109.1.4",
          "form": "잠깐 몸풀고 집에가서 저녁하려구여",
          "original_form": "잠깐 몸풀고 집에가서 저녁하려구여",
          "speaker_id": "2",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100021109.1.5",
          "form": "세라잼이 머죠?",
          "original_form": "세라잼이 머죠?",
          "speaker_id": "1",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100021109.1.6",
          "form": "요즘광고나오는 의료기여",
          "original_form": "요즘광고나오는 의료기여",
          "speaker_id": "2",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100021109.1.7",
          "form": "토요일에 가족들은 다있나요?",
          "original_form": "토요일에 가족들은 다있나요?",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100021109.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100021109.1.9",
          "form": "저희는 애들이 커서 저녁만 같이 먹으면되고 남편과 둘이 먹을때도 있어요",
          "original_form": "저희는 애들이 커서 저녁만 같이 먹으면되고 남편과 둘이 먹을때도 있어요",
          "speaker_id": "2",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100021109.1.10",
          "form": "주말이라 특별히 바쁘지는 않아요",
          "original_form": "주말이라 특별히 바쁘지는 않아요",
          "speaker_id": "2",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100021109.1.11",
          "form": "외롭죠?",
          "original_form": "외롭죠?",
          "speaker_id": "1",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100021109.1.12",
          "form": "아직은 편해요",
          "original_form": "아직은 편해요",
          "speaker_id": "2",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100021109.1.13",
          "form": "익숙하시겠네요",
          "original_form": "익숙하시겠네요",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100021109.1.14",
          "form": "집에 있으면 TV나 보니까 동네 마실 나온거예요",
          "original_form": "집에 있으면 TV나 보니까 동네 마실 나온거예요",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100021109.1.15",
          "form": "벗어난지 얼마안되서 지금은 즐겨요",
          "original_form": "벗어난지 얼마안되서 지금은 즐겨요",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100021109.1.16",
          "form": "부럽다요",
          "original_form": "부럽다요",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100021109.1.17",
          "form": "저녁은 뭐드시나요?",
          "original_form": "저녁은 뭐드시나요?",
          "speaker_id": "2",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100021109.1.18",
          "form": "생각중이요",
          "original_form": "생각중이요",
          "speaker_id": "1",
          "time": "20210828 16:16"
        }
      ]
