      "utterance": [
        {
          "id": "MDRW2100002127.1.1",
          "form": "싸이월드 하셨나요",
          "original_form": "싸이월드 하셨나요",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002127.1.2",
          "form": "싸이월드 세대에요",
          "original_form": "싸이월드 세대에요",
          "speaker_id": "2",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002127.1.3",
          "form": "저도 초딩때부터 햇어용 ㅎㅎ잼났는데",
          "original_form": "저도 초딩때부터 햇어용 ㅎㅎ잼났는데",
          "speaker_id": "1",
          "time": "20210820 08:06"
        },
        {
          "id": "MDRW2100002127.1.4",
          "form": "약간 사이버틱한 감성 ㅎㅎ 도토리 충전하고 포도알 모으고",
          "original_form": "약간 사이버틱한 감성 ㅎㅎ 도토리 충전하고 포도알 모으고",
          "speaker_id": "2",
          "time": "20210820 08:17"
        },
        {
          "id": "MDRW2100002127.1.5",
          "form": "맞아욬ㅋㅋㅋ 그시절 비쥐엠도 진짜 좋았는데!",
          "original_form": "맞아욬ㅋㅋㅋ 그시절 비쥐엠도 진짜 좋았는데!",
          "speaker_id": "1",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100002127.1.6",
          "form": "어떤 비쥐엠 깔아놓으셨어요",
          "original_form": "어떤 비쥐엠 깔아놓으셨어요",
          "speaker_id": "2",
          "time": "20210820 08:30"
        },
        {
          "id": "MDRW2100002127.1.7",
          "form": "전 프리스타일의 Y...",
          "original_form": "전 프리스타일의 Y...",
          "speaker_id": "2",
          "time": "20210820 08:30"
        },
        {
          "id": "MDRW2100002127.1.8",
          "form": "저는 막팝송이랑 스윗소로우ㅋㅋㅋㅋ",
          "original_form": "저는 막팝송이랑 스윗소로우ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:40"
        },
        {
          "id": "MDRW2100002127.1.9",
          "form": "너를 사랑해 사랑해 사랑해~ 이노래요?",
          "original_form": "너를 사랑해 사랑해 사랑해~ 이노래요?",
          "speaker_id": "2",
          "time": "20210820 08:40"
        },
        {
          "id": "MDRW2100002127.1.10",
          "form": "오 맞아요 ㅋㅋㅋㅋㅋ네미시스 솜사탕도 ㅋㅋ",
          "original_form": "오 맞아요 ㅋㅋㅋㅋㅋ네미시스 솜사탕도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:40"
        },
        {
          "id": "MDRW2100002127.1.11",
          "form": "아 진짜 추억이 샘솟네요ㅋㅋㅋㅋ \r\n최근 부활한다는데 어떠세요?",
          "original_form": "아 진짜 추억이 샘솟네요ㅋㅋㅋㅋ \r\n최근 부활한다는데 어떠세요?",
          "speaker_id": "2",
          "time": "20210820 08:41"
        },
        {
          "id": "MDRW2100002127.1.12",
          "form": "저는 그때 감성이랑 달라서",
          "original_form": "저는 그때 감성이랑 달라서",
          "speaker_id": "1",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100002127.1.13",
          "form": "지금하면 잼없을듯 ㅠㅠ",
          "original_form": "지금하면 잼없을듯 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100002127.1.14",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "2",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100002127.1.15",
          "form": "지금은 인스타갬성이 넘 지배햇어여..",
          "original_form": "지금은 인스타갬성이 넘 지배햇어여..",
          "speaker_id": "1",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100002127.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100002127.1.17",
          "form": "추억은 추억으로 남겨놓고 싶은?",
          "original_form": "추억은 추억으로 남겨놓고 싶은?",
          "speaker_id": "2",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100002127.1.18",
          "form": "막상 하라그러면 하기싫은.. 뭔지 아시죠",
          "original_form": "막상 하라그러면 하기싫은.. 뭔지 아시죠",
