      "utterance": [
        {
          "id": "MDRW2100030333.1.1",
          "form": "환절기니까 애매하긴 하지",
          "original_form": "환절기니까 애매하긴 하지",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.2",
          "form": "나가봐두 어떤사람은 긴팔긴바지입고 어떤사람은 반팔반바지입어서",
          "original_form": "나가봐두 어떤사람은 긴팔긴바지입고 어떤사람은 반팔반바지입어서",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.3",
          "form": "맞어 긴팔입는사람 반팔잇는사람 다있잖어 ㅋㅋ",
          "original_form": "맞어 긴팔입는사람 반팔잇는사람 다있잖어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.4",
          "form": "잘모르겠어..",
          "original_form": "잘모르겠어..",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.5",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.6",
          "form": "근데 각자 느끼는 추위랑 더위가 다르니깐",
          "original_form": "근데 각자 느끼는 추위랑 더위가 다르니깐",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.7",
          "form": "한겨울에 반팔 위에 패딩만 입기도 하더라",
          "original_form": "한겨울에 반팔 위에 패딩만 입기도 하더라",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.8",
          "form": "그렇긴해!! 튿히 난 더위를 많이 타서 가을까진 반팔입을수있어",
          "original_form": "그렇긴해!! 튿히 난 더위를 많이 타서 가을까진 반팔입을수있어",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.9",
          "form": "오 ㅎㅎ 좋겟다",
          "original_form": "오 ㅎㅎ 좋겟다",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.10",
          "form": "그건 좀 춥겠다 ㅋㅋ",
          "original_form": "그건 좀 춥겠다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030333.1.11",
          "form": "ㅋㅋ자기가 안 추우면 됏지",
          "original_form": "ㅋㅋ자기가 안 추우면 됏지",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.12",
          "form": "롱패딩은 정말 엄청난 발명품이야",
          "original_form": "롱패딩은 정말 엄청난 발명품이야",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.13",
          "form": "입고싶은 옷 입는것도 중요하긴 하구",
          "original_form": "입고싶은 옷 입는것도 중요하긴 하구",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.14",
          "form": "아니 내가 그렇다구 어쩌라구",
          "original_form": "아니 내가 그렇다구 어쩌라구",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.15",
          "form": "나도 그렇게 생각해 ㅋㅋ",
          "original_form": "나도 그렇게 생각해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.16",
          "form": "웅 맞아!!",
          "original_form": "웅 맞아!!",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.17",
          "form": "세상은 롱패딩 유행 전과 후로 나뉠거얌",
          "original_form": "세상은 롱패딩 유행 전과 후로 나뉠거얌",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.18",
          "form": "그만큼 짱이야",
          "original_form": "그만큼 짱이야",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.19",
          "form": "그러게.. 롱패딩 유행 시작도 벌써 5년은 지났지",
          "original_form": "그러게.. 롱패딩 유행 시작도 벌써 5년은 지났지",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.20",
          "form": "그전엔 겨울에 항상 추워죽을것같앗는데 ㅎㅎ",
          "original_form": "그전엔 겨울에 항상 추워죽을것같앗는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.21",
          "form": "아니 4년인가",
          "original_form": "아니 4년인가",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.22",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030333.1.23",
          "form": "지금은 다 입잖어",
          "original_form": "지금은 다 입잖어",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030333.1.24",
          "form": "겨울이 별로안추워졌어",
          "original_form": "겨울이 별로안추워졌어",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030333.1.25",
          "form": "그런 유행은 정말 좋은것같어",
          "original_form": "그런 유행은 정말 좋은것같어",
          "speaker_id": "2",
          "time": "20210903 23:53"
        }
      ]
    }
