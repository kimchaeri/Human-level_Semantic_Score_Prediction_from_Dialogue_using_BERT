      "utterance": [
        {
          "id": "MDRW2100009482.1.1",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100009482.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210823 19:00"
        },
        {
          "id": "MDRW2100009482.1.3",
          "form": "지금 하려고요",
          "original_form": "지금 하려고요",
          "speaker_id": "2",
          "time": "20210823 19:00"
        },
        {
          "id": "MDRW2100009482.1.4",
          "form": "하셨어요?",
          "original_form": "하셨어요?",
          "speaker_id": "2",
          "time": "20210823 19:01"
        },
        {
          "id": "MDRW2100009482.1.5",
          "form": "전집이 지금 배달받아서 이제 시켰는데 언제 올지 모르겠어요",
          "original_form": "전집이 지금 배달받아서 이제 시켰는데 언제 올지 모르겠어요",
          "speaker_id": "2",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100009482.1.6",
          "form": "오늘 비와서 한잔만 하게요",
          "original_form": "오늘 비와서 한잔만 하게요",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100009482.1.7",
          "form": "저 진짜 한잔 입니다 ㅎㅎ",
          "original_form": "저 진짜 한잔 입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100009482.1.8",
          "form": "요새 술을 잘못해요",
          "original_form": "요새 술을 잘못해요",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100009482.1.9",
          "form": "와 누구랑 같이요?",
          "original_form": "와 누구랑 같이요?",
          "speaker_id": "1",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100009482.1.10",
          "form": "남편이랑 요",
          "original_form": "남편이랑 요",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100009482.1.11",
          "form": "진짜 달려가고프네요 ㅎ",
          "original_form": "진짜 달려가고프네요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100009482.1.12",
          "form": "네 그러게요",
          "original_form": "네 그러게요",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100009482.1.13",
          "form": "누그든 함께하면좋죠",
          "original_form": "누그든 함께하면좋죠",
          "speaker_id": "2",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.14",
          "form": "이런날 전에 한잔 캬~~부릅네요",
          "original_form": "이런날 전에 한잔 캬~~부릅네요",
          "speaker_id": "1",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.15",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.16",
          "form": "저는 주말부부라",
          "original_form": "저는 주말부부라",
          "speaker_id": "1",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.17",
          "form": "박군 한잔해가 생각나요 ㅋㅋ",
          "original_form": "박군 한잔해가 생각나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.18",
          "form": "아 그러세요",
          "original_form": "아 그러세요",
          "speaker_id": "2",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.19",
          "form": "싱글맘이나 다름없다며 ㅋㅋㅋ",
          "original_form": "싱글맘이나 다름없다며 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.20",
          "form": "저도 예전에 그래서",
          "original_form": "저도 예전에 그래서",
          "speaker_id": "2",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.21",
          "form": "알아요 ㅡㅡ",
          "original_form": "알아요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.22",
          "form": "전 2주나 한달한번 오고 그랬어요",
          "original_form": "전 2주나 한달한번 오고 그랬어요",
          "speaker_id": "2",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100009482.1.23",
          "form": "그러다 보니 저녁을 아이와 간단히 먹곤했어요",
          "original_form": "그러다 보니 저녁을 아이와 간단히 먹곤했어요",
          "speaker_id": "2",
          "time": "20210823 19:08"
        },
        {
          "id": "MDRW2100009482.1.24",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "1",
          "time": "20210823 19:08"
        },
        {
          "id": "MDRW2100009482.1.25",
          "form": "저녁은 뭐 하신거예요?",
          "original_form": "저녁은 뭐 하신거예요?",
          "speaker_id": "2",
          "time": "20210823 19:08"
        },
        {
          "id": "MDRW2100009482.1.26",
          "form": "저희집  냉장고엔 어린이반찬뿐이예요 ㅋ",
          "original_form": "저희집  냉장고엔 어린이반찬뿐이예요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 19:08"
        },
        {
          "id": "MDRW2100009482.1.27",
          "form": "어릴땐 그래요",
          "original_form": "어릴땐 그래요",
          "speaker_id": "2",
          "time": "20210823 19:08"
        },
        {
          "id": "MDRW2100009482.1.28",
          "form": "저도 맨날 하얗게 ㅋㅋ",
          "original_form": "저도 맨날 하얗게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:08"
        },
        {
          "id": "MDRW2100009482.1.29",
          "form": "콩나물도 하얗게",
          "original_form": "콩나물도 하얗게",
          "speaker_id": "2",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100009482.1.30",
          "form": "먹었어요",
          "original_form": "먹었어요",
          "speaker_id": "2",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100009482.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100009482.1.32",
          "form": "맨날 맑은것들뿐이죠",
          "original_form": "맨날 맑은것들뿐이죠",
          "speaker_id": "1",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100009482.1.33",
          "form": "아 싱거워요",
          "original_form": "아 싱거워요",
          "speaker_id": "2",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100009482.1.34",
          "form": "맛나게드시는데 제가 방해가 되겠네요",
          "original_form": "맛나게드시는데 제가 방해가 되겠네요",
          "speaker_id": "1",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100009482.1.35",
          "form": "빨간 고추가루 레시피를 이용못해요",
          "original_form": "빨간 고추가루 레시피를 이용못해요",
          "speaker_id": "2",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100009482.1.36",
          "form": "맛나게드세용",
          "original_form": "맛나게드세용",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100009482.1.37",
          "form": "저 고추장을 거의 안샀었어요",
          "original_form": "저 고추장을 거의 안샀었어요",
          "speaker_id": "2",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100009482.1.38",
          "form": "제가 지금 그래요",
          "original_form": "제가 지금 그래요",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100009482.1.39",
          "form": "그럼 오늘도 하얀 저녁이세요?",
          "original_form": "그럼 오늘도 하얀 저녁이세요?",
          "speaker_id": "2",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100009482.1.40",
          "form": "고추장이 어따쓰는건지 기억이 가물가물 ㅎㅎ",
          "original_form": "고추장이 어따쓰는건지 기억이 가물가물 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100009482.1.41",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100009482.1.42",
          "form": "제가 그랬어요",
          "original_form": "제가 그랬어요",
          "speaker_id": "2",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100009482.1.43",
          "form": "뭐라도 시켜 먹으려고 해도",
          "original_form": "뭐라도 시켜 먹으려고 해도",
          "speaker_id": "2",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100009482.1.44",
          "form": "어른 혼자라 시키기 애매하시죠",
          "original_form": "어른 혼자라 시키기 애매하시죠",
          "speaker_id": "2",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100009482.1.45",
          "form": "맞아유~ 애들입맛에 맞추니",
          "original_form": "맞아유~ 애들입맛에 맞추니",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100009482.1.46",
          "form": "메뉴가 한정적이예요",
          "original_form": "메뉴가 한정적이예요",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100009482.1.47",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100009482.1.48",
          "form": "조리법도 진짜 간단하죠",
          "original_form": "조리법도 진짜 간단하죠",
          "speaker_id": "2",
          "time": "20210823 19:12"
        },
        {
          "id": "MDRW2100009482.1.49",
          "form": "전부 소금 참기름 뭐",
          "original_form": "전부 소금 참기름 뭐",
          "speaker_id": "2",
          "time": "20210823 19:12"
        },
        {
          "id": "MDRW2100009482.1.50",
          "form": "이렇다 할것을 못넣어요",
          "original_form": "이렇다 할것을 못넣어요",
          "speaker_id": "2",
          "time": "20210823 19:12"
        },
        {
          "id": "MDRW2100009482.1.51",
          "form": "간장 ㅋㅋ",
          "original_form": "간장 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:12"
        },
        {
          "id": "MDRW2100009482.1.52",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
