      "utterance": [
        {
          "id": "MDRW2100023707.1.1",
          "form": "김국 비주얼은 짜장같이 생겼는데 극찬받았다니 궁금하네요ㅋㅋ",
          "original_form": "김국 비주얼은 짜장같이 생겼는데 극찬받았다니 궁금하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100023707.1.2",
          "form": "찌개나 국을 좋아하세요?",
          "original_form": "찌개나 국을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100023707.1.3",
          "form": "국은 쏘쏘 찌개는 좋아해요!",
          "original_form": "국은 쏘쏘 찌개는 좋아해요!",
          "speaker_id": "1",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100023707.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100023707.1.5",
          "form": "저는 엄청 좋아하지는 않고 그냥 가끔 생각나는 정도요",
          "original_form": "저는 엄청 좋아하지는 않고 그냥 가끔 생각나는 정도요",
          "speaker_id": "2",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100023707.1.6",
          "form": "부대찌개라든가...",
          "original_form": "부대찌개라든가...",
          "speaker_id": "2",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100023707.1.7",
          "form": "아 부대찌개 쌀쌀하면 당기죠",
          "original_form": "아 부대찌개 쌀쌀하면 당기죠",
          "speaker_id": "1",
          "time": "20210830 15:11"
        },
        {
          "id": "MDRW2100023707.1.8",
          "form": "김치찌개? 된찌? 고르자면?",
          "original_form": "김치찌개? 된찌? 고르자면?",
          "speaker_id": "1",
          "time": "20210830 15:12"
        },
        {
          "id": "MDRW2100023707.1.9",
          "form": "너무 어려운질문...... 지금은 김치찌개!",
          "original_form": "너무 어려운질문...... 지금은 김치찌개!",
          "speaker_id": "2",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100023707.1.10",
          "form": "참치? 고기? 뭘 넣은 걸로요?",
          "original_form": "참치? 고기? 뭘 넣은 걸로요?",
          "speaker_id": "1",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100023707.1.11",
          "form": "예전에 동생이 고기넣고 해줬었는데 그게 진짜 맛있었어요",
          "original_form": "예전에 동생이 고기넣고 해줬었는데 그게 진짜 맛있었어요",
          "speaker_id": "2",
          "time": "20210830 15:14"
        },
        {
          "id": "MDRW2100023707.1.12",
          "form": "비개있는 고기 넣은 김치찌개 예술이죠ㅎ",
          "original_form": "비개있는 고기 넣은 김치찌개 예술이죠ㅎ",
          "speaker_id": "1",
          "time": "20210830 15:14"
        },
        {
          "id": "MDRW2100023707.1.13",
          "form": "저도 그거 좋아해요",
          "original_form": "저도 그거 좋아해요",
          "speaker_id": "1",
          "time": "20210830 15:14"
        },
        {
          "id": "MDRW2100023707.1.14",
          "form": "매운거 잘 드세요? 저는 김찌 매운게 좋더라고요",
          "original_form": "매운거 잘 드세요? 저는 김찌 매운게 좋더라고요",
          "speaker_id": "2",
          "time": "20210830 15:16"
        },
        {
          "id": "MDRW2100023707.1.15",
          "form": "매운거 사랑해요 청양고추 필수",
          "original_form": "매운거 사랑해요 청양고추 필수",
          "speaker_id": "1",
          "time": "20210830 15:17"
        },
        {
          "id": "MDRW2100023707.1.16",
          "form": "저는 좋아하긴 하는데 잘은 못먹어요ㅋㅋ 혹시 엽떡 좋아하세요 그럼?",
          "original_form": "저는 좋아하긴 하는데 잘은 못먹어요ㅋㅋ 혹시 엽떡 좋아하세요 그럼?",
          "speaker_id": "2",
          "time": "20210830 15:24"
        },
        {
          "id": "MDRW2100023707.1.17",
          "form": "떡볶이는...별로 안 먹어서요ㅜ",
          "original_form": "떡볶이는...별로 안 먹어서요ㅜ",
          "speaker_id": "1",
          "time": "20210830 15:30"
        },
        {
          "id": "MDRW2100023707.1.18",
          "form": "로제떡볶이는 또 잘 먹어요ㅎ",
          "original_form": "로제떡볶이는 또 잘 먹어요ㅎ",
          "speaker_id": "1",
          "time": "20210830 15:30"
        },
        {
          "id": "MDRW2100023707.1.19",
          "form": "무슨 떡볶이 좋아하세요?",
          "original_form": "무슨 떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 15:46"
        },
        {
          "id": "MDRW2100023707.1.20",
          "form": "전 떡볶이 다 좋아하는데 엽떡은 너무 맵더라고요",
          "original_form": "전 떡볶이 다 좋아하는데 엽떡은 너무 맵더라고요",
          "speaker_id": "2",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100023707.1.21",
          "form": "로제떡볶이 맛있어요! 주로 어디서 사드세요?",
          "original_form": "로제떡볶이 맛있어요! 주로 어디서 사드세요?",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100023707.1.22",
          "form": "전 그냥 레토르트 사먹어요",
          "original_form": "전 그냥 레토르트 사먹어요",
          "speaker_id": "1",
          "time": "20210831 21:37"
        }
      ]
