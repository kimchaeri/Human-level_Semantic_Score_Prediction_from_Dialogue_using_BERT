      "utterance": [
        {
          "id": "MDRW2100040500.1.1",
          "form": "ㅇ ㅏ 서브퀘",
          "original_form": "ㅇ ㅏ 서브퀘",
          "speaker_id": "1",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100040500.1.2",
          "form": "한번 할 때마다 신상 찍고 체력 충전한 담에 가야하는데",
          "original_form": "한번 할 때마다 신상 찍고 체력 충전한 담에 가야하는데",
          "speaker_id": "1",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100040500.1.3",
          "form": "워프포인트에서 너무 멀어...",
          "original_form": "워프포인트에서 너무 멀어...",
          "speaker_id": "1",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100040500.1.4",
          "form": "ㅇㅇ여긴 워프포인트 위치가 너무 애매해",
          "original_form": "ㅇㅇ여긴 워프포인트 위치가 너무 애매해",
          "speaker_id": "2",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100040500.1.5",
          "form": "특히 나루카미섬",
          "original_form": "특히 나루카미섬",
          "speaker_id": "2",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100040500.1.6",
          "form": "아무리 거기가 스타트지점이었다지만",
          "original_form": "아무리 거기가 스타트지점이었다지만",
          "speaker_id": "2",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100040500.1.7",
          "form": "이도에 있는게 말이 되냐구",
          "original_form": "이도에 있는게 말이 되냐구",
          "speaker_id": "2",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100040500.1.8",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100040500.1.9",
          "form": "이도 진짜 다신 안가는데",
          "original_form": "이도 진짜 다신 안가는데",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100040500.1.10",
          "form": "ㄹㅇ.....",
          "original_form": "ㄹㅇ.....",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100040500.1.11",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040500.1.12",
          "form": "원신 티비광고를",
          "original_form": "원신 티비광고를",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040500.1.13",
          "form": "하고있네",
          "original_form": "하고있네",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040500.1.14",
          "form": "라이덴이랑 시뇨라 나오네",
          "original_form": "라이덴이랑 시뇨라 나오네",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040500.1.15",
          "form": "에엥????",
          "original_form": "에엥????",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040500.1.16",
          "form": "지상파에???",
          "original_form": "지상파에???",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040500.1.17",
          "form": "ㅇㅇㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040500.1.18",
          "form": "엄마가",
          "original_form": "엄마가",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040500.1.19",
          "form": "개는 훌륭하다",
          "original_form": "개는 훌륭하다",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040500.1.20",
          "form": "kbs2에 틀어놨는데",
          "original_form": "kbs2에 틀어놨는데",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040500.1.21",
          "form": "중간타임에 갑자기 원신광고가",
          "original_form": "중간타임에 갑자기 원신광고가",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040500.1.22",
          "form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040500.1.23",
          "form": "아니 근데 야에랑 스카라무슈면 몰라도",
          "original_form": "아니 근데 야에랑 스카라무슈면 몰라도",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040500.1.24",
          "form": "시뇨라...?",
          "original_form": "시뇨라...?",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040500.1.25",
          "form": "허위매물인뎁쇼",
          "original_form": "허위매물인뎁쇼",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100040500.1.26",
          "form": "아냐 시뇨라 보스모습만",
          "original_form": "아냐 시뇨라 보스모습만",
          "speaker_id": "2",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100040500.1.27",
          "form": "나왔어",
          "original_form": "나왔어",
          "speaker_id": "2",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100040500.1.28",
          "form": "글케 길지도 않았고",
          "original_form": "글케 길지도 않았고",
          "speaker_id": "2",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100040500.1.29",
          "form": "와 근데 좀 당황스럽다",
          "original_form": "와 근데 좀 당황스럽다",
          "speaker_id": "1",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040500.1.30",
          "form": "그래픽은 어때보여?",
          "original_form": "그래픽은 어때보여?",
          "speaker_id": "1",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040500.1.31",
          "form": "몰라 익숙한 소리가 들려서 고개를 들어보니까",
          "original_form": "몰라 익숙한 소리가 들려서 고개를 들어보니까",
          "speaker_id": "2",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040500.1.32",
          "form": "행자 둘 라이덴 시뇨라",
          "original_form": "행자 둘 라이덴 시뇨라",
          "speaker_id": "2",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100040500.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040500.1.34",
          "form": "스르륵 지나가고",
          "original_form": "스르륵 지나가고",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040500.1.35",
          "form": "끝나있었어....",
          "original_form": "끝나있었어....",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100040500.1.36",
          "form": "솔직히 이나즈마 광고",
          "original_form": "솔직히 이나즈마 광고",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040500.1.37",
          "form": "코코미도 허위매물이야",
          "original_form": "코코미도 허위매물이야",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040500.1.38",
          "form": "걔가 뭘 하는데요...ㅡㅡ",
          "original_form": "걔가 뭘 하는데요...ㅡㅡ",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100040500.1.39",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ저항 안하는 저항군...",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ저항 안하는 저항군...",
          "speaker_id": "2",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100040500.1.40",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ 그리고 젤 중요한건",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ 그리고 젤 중요한건",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040500.1.41",
          "form": "안수령이 끝나면",
          "original_form": "안수령이 끝나면",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040500.1.42",
          "form": "저항군 머함 ㅋㅋ?",
          "original_form": "저항군 머함 ㅋㅋ?",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040500.1.43",
          "form": "존재 이유가 사라져버림",
          "original_form": "존재 이유가 사라져버림",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040500.1.44",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040500.1.45",
          "form": "엄밀하게 말하면",
          "original_form": "엄밀하게 말하면",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040500.1.46",
          "form": "코코미쪽은",
          "original_form": "코코미쪽은",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040500.1.47",
          "form": "다른 마신을 섬기는 해저종족이라",
          "original_form": "다른 마신을 섬기는 해저종족이라",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040500.1.48",
          "form": "애초에 라이덴 쇼군이 속고(ㅋㅋㅋ) 있었으면",
          "original_form": "애초에 라이덴 쇼군이 속고(ㅋㅋㅋ) 있었으면",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040500.1.49",
          "form": "저항하는 의미도 없었잖아",
          "original_form": "저항하는 의미도 없었잖아",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040500.1.50",
          "form": "차라리 결사대를 짜서",
          "original_form": "차라리 결사대를 짜서",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040500.1.51",
          "form": "라이덴한테 상황전달하기",
          "original_form": "라이덴한테 상황전달하기",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040500.1.52",
          "form": "그런걸 했으면 몰라",
          "original_form": "그런걸 했으면 몰라",
          "speaker_id": "2",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100040500.1.53",
          "form": "사실 저항 아니고",
          "original_form": "사실 저항 아니고",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100040500.1.54",
          "form": "반군에 가깝지 않나 싶음",
          "original_form": "반군에 가깝지 않나 싶음",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100040500.1.55",
          "form": "걍 이나즈마 안에서 안수령이 시행되니까",
          "original_form": "걍 이나즈마 안에서 안수령이 시행되니까",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100040500.1.56",
          "form": "저항군으로 간판만 바꿔단거지",
          "original_form": "저항군으로 간판만 바꿔단거지",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100040500.1.57",
          "form": "와타츠미섬은 아예 번개원소 지배도 안받잖어",
          "original_form": "와타츠미섬은 아예 번개원소 지배도 안받잖어",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100040500.1.58",
          "form": "ㅇㅇ애초에 맘에 안들었는데",
          "original_form": "ㅇㅇ애초에 맘에 안들었는데",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100040500.1.59",
          "form": "악법하나 내놓으니까",
          "original_form": "악법하나 내놓으니까",
          "speaker_id": "2",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100040500.1.60",
          "form": "너 잘걸렸다 한ㅈ느낌?",
          "original_form": "너 잘걸렸다 한ㅈ느낌?",
          "speaker_id": "2",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100040500.1.61",
          "form": "ㅇㅇ 좀 그런느낌",
          "original_form": "ㅇㅇ 좀 그런느낌",
          "speaker_id": "1",
          "time": "20210905 13:48"
        }
      ]
