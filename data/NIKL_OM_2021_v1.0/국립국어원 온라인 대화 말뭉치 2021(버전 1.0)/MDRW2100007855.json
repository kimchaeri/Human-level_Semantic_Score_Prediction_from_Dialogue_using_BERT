      "utterance": [
        {
          "id": "MDRW2100007855.1.1",
          "form": "오랜만에 빨래 돌리니 속이 다 시원하다 ㅎㅎ",
          "original_form": "오랜만에 빨래 돌리니 속이 다 시원하다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100007855.1.2",
          "form": "계속 비내려서 못했지 ㅎㅎ",
          "original_form": "계속 비내려서 못했지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100007855.1.3",
          "form": "3일 전에 널어놓은 이불이 아직도 안말랐어ㅠ",
          "original_form": "3일 전에 널어놓은 이불이 아직도 안말랐어ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007855.1.4",
          "form": "비가 수시로 내리니 안마를 수 밖에 ㅋ",
          "original_form": "비가 수시로 내리니 안마를 수 밖에 ㅋ",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007855.1.5",
          "form": "오늘은 해 바람 콤보로 빨래 엄청 잘 마를듯",
          "original_form": "오늘은 해 바람 콤보로 빨래 엄청 잘 마를듯",
          "speaker_id": "1",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007855.1.6",
          "form": "ㅎㅎ 밀린 빨래 다 해야겠네",
          "original_form": "ㅎㅎ 밀린 빨래 다 해야겠네",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007855.1.7",
          "form": "ㅇㅇ 세탁기 두 번은 돌려야할듯",
          "original_form": "ㅇㅇ 세탁기 두 번은 돌려야할듯",
          "speaker_id": "1",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007855.1.8",
          "form": "훌 다 널 곳은 있어?",
          "original_form": "훌 다 널 곳은 있어?",
          "speaker_id": "2",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100007855.1.9",
          "form": "걱정마 빨래건조대가 두 개나 있는디 뭘 ㅎㅎ",
          "original_form": "걱정마 빨래건조대가 두 개나 있는디 뭘 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100007855.1.10",
          "form": "내일은 또 비온대",
          "original_form": "내일은 또 비온대",
          "speaker_id": "2",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100007855.1.11",
          "form": "오마이갓 오늘 안에 꼭 다 널어야겠네",
          "original_form": "오마이갓 오늘 안에 꼭 다 널어야겠네",
          "speaker_id": "1",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100007855.1.12",
          "form": "다음주 내내 비로 되어있더라고",
          "original_form": "다음주 내내 비로 되어있더라고",
          "speaker_id": "2",
          "time": "20210822 12:45"
        },
        {
          "id": "MDRW2100007855.1.13",
          "form": "빨래 큰일이네 다음 주도 ㅠㅠ",
          "original_form": "빨래 큰일이네 다음 주도 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100007855.1.14",
          "form": "ㅎㅎ 건조기에 조금씩 돌려야지 뭐",
          "original_form": "ㅎㅎ 건조기에 조금씩 돌려야지 뭐",
          "speaker_id": "2",
          "time": "20210822 12:46"
        }
