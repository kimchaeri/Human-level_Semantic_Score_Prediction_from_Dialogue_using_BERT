      "utterance": [
        {
          "id": "MDRW2100019141.1.1",
          "form": "우리 이걸로 더 할 얘기 잇나?",
          "original_form": "우리 이걸로 더 할 얘기 잇나?",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019141.1.2",
          "form": "나 요새 새 보석십자수 뭐사야하나",
          "original_form": "나 요새 새 보석십자수 뭐사야하나",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019141.1.3",
          "form": "고민하고잇긴 해",
          "original_form": "고민하고잇긴 해",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019141.1.4",
          "form": "오 보석십자수",
          "original_form": "오 보석십자수",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019141.1.5",
          "form": "가짓수가 대여섯개 넘어가다보니까 안산거는 다 비슷하더라구",
          "original_form": "가짓수가 대여섯개 넘어가다보니까 안산거는 다 비슷하더라구",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019141.1.6",
          "form": "명화쪽으로 따로 찾아봐야되나?",
          "original_form": "명화쪽으로 따로 찾아봐야되나?",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019141.1.7",
          "form": "명화는 어지간하면 실패하지 않더란",
          "original_form": "명화는 어지간하면 실패하지 않더란",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019141.1.8",
          "form": "저번에 그",
          "original_form": "저번에 그",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019141.1.9",
          "form": "별헤는 밤은 윤동주고 그",
          "original_form": "별헤는 밤은 윤동주고 그",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019141.1.10",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019141.1.11",
          "form": "별......",
          "original_form": "별......",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019141.1.12",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019141.1.13",
          "form": "여튼 그거",
          "original_form": "여튼 그거",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019141.1.14",
          "form": "별이 빛나는 밤 ?",
          "original_form": "별이 빛나는 밤 ?",
          "speaker_id": "1",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019141.1.15",
          "form": "그거는 부모님도 맘에 들엇나봐",
          "original_form": "그거는 부모님도 맘에 들엇나봐",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019141.1.16",
          "form": "아ㅇㅇㅇ",
          "original_form": "아ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019141.1.17",
          "form": "거실에 전시됐어",
          "original_form": "거실에 전시됐어",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019141.1.18",
          "form": "내 생물학적 부친되는 사람이",
          "original_form": "내 생물학적 부친되는 사람이",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019141.1.19",
          "form": "례술 좋아한답시고 그런거에 겁나 까다로운척해서",
          "original_form": "례술 좋아한답시고 그런거에 겁나 까다로운척해서",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019141.1.20",
          "form": "지금까지 했던 꽃이나 풍경은 걍 내방에 먼지 쌓인채로 방치되어있거든",
          "original_form": "지금까지 했던 꽃이나 풍경은 걍 내방에 먼지 쌓인채로 방치되어있거든",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019141.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019141.1.22",
          "form": "명화가 젤 무난한 상품이긴 하니",
          "original_form": "명화가 젤 무난한 상품이긴 하니",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019141.1.23",
          "form": "하지",
          "original_form": "하지",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019141.1.24",
          "form": "ㅇㅇ근데 의외로 잘 없어",
          "original_form": "ㅇㅇ근데 의외로 잘 없어",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019141.1.25",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019141.1.26",
          "form": "명화 십자수가 예쁠거같은데",
          "original_form": "명화 십자수가 예쁠거같은데",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019141.1.27",
          "form": "ㅇㅇ자꾸 좀",
          "original_form": "ㅇㅇ자꾸 좀",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019141.1.28",
          "form": "팝아트나",
          "original_form": "팝아트나",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019141.1.29",
          "form": "디즈니에서 허가없이 맘대로 스샷찍은것같은",
          "original_form": "디즈니에서 허가없이 맘대로 스샷찍은것같은",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019141.1.30",
          "form": "일러스트만",
          "original_form": "일러스트만",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019141.1.31",
          "form": "팔아",
          "original_form": "팔아",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019141.1.32",
          "form": "아 근데 좀 신기한게",
          "original_form": "아 근데 좀 신기한게",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019141.1.33",
          "form": "알라딘에서도 보석십자수 팔더라",
          "original_form": "알라딘에서도 보석십자수 팔더라",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019141.1.34",
          "form": "아 알라딘에서도 팔아?",
          "original_form": "아 알라딘에서도 팔아?",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019141.1.35",
          "form": "ㅇㅇㅇ저어번에",
          "original_form": "ㅇㅇㅇ저어번에",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019141.1.36",
          "form": "그 이북리더기 보러 갔을때",
          "original_form": "그 이북리더기 보러 갔을때",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019141.1.37",
          "form": "다른 굿즈 구경도 하는데",
          "original_form": "다른 굿즈 구경도 하는데",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019141.1.38",
          "form": "보석십자수도 있더라구",
          "original_form": "보석십자수도 있더라구",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019141.1.39",
          "form": "동화테마같은걸로",
          "original_form": "동화테마같은걸로",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019141.1.40",
          "form": "그때도 좀 혹하긴 했는데",
          "original_form": "그때도 좀 혹하긴 했는데",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019141.1.41",
          "form": "좀 작은편이라 참았지",
          "original_form": "좀 작은편이라 참았지",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019141.1.42",
          "form": "동화테마는 예쁘겠다",
          "original_form": "동화테마는 예쁘겠다",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019141.1.43",
          "form": "나 약간 바다배경이나 하늘배경같은",
          "original_form": "나 약간 바다배경이나 하늘배경같은",
          "speaker_id": "1",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100019141.1.44",
          "form": "파란 계열 사고싶어",
          "original_form": "파란 계열 사고싶어",
          "speaker_id": "1",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100019141.1.45",
          "form": "근데 좀 무난한게",
          "original_form": "근데 좀 무난한게",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100019141.1.46",
          "form": "잘없어",
          "original_form": "잘없어",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100019141.1.47",
          "form": "나 잠깐",
          "original_form": "나 잠깐",
          "speaker_id": "1",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100019141.1.48",
          "form": "장 본 얘기 좀 할게 ㅋㅋㅋㅋㅋ",
          "original_form": "장 본 얘기 좀 할게 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:09"
        }
      ]
