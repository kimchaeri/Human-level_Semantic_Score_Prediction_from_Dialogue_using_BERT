      "utterance": [
        {
          "id": "MDRW2100045002.1.1",
          "form": "번개씨앗레벨",
          "original_form": "번개씨앗레벨",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045002.1.2",
          "form": "얼마나 올렷어?",
          "original_form": "얼마나 올렷어?",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045002.1.3",
          "form": "16렙인데",
          "original_form": "16렙인데",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045002.1.4",
          "form": "아까 상자 와르르 까서",
          "original_form": "아까 상자 와르르 까서",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045002.1.5",
          "form": "신사가면 17렙될듯",
          "original_form": "신사가면 17렙될듯",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.6",
          "form": "이제 낟ᆢ",
          "original_form": "이제 낟ᆢ",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.7",
          "form": "2단계 번개장막",
          "original_form": "2단계 번개장막",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.9",
          "form": "벌써??",
          "original_form": "벌써??",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.10",
          "form": "빠르다",
          "original_form": "빠르다",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.11",
          "form": "통과 가능해진다.......",
          "original_form": "통과 가능해진다.......",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.12",
          "form": "ㅇㅏ근데 거기에도",
          "original_form": "ㅇㅏ근데 거기에도",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.13",
          "form": "해파리잇잔아",
          "original_form": "해파리잇잔아",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.14",
          "form": "극혐...",
          "original_form": "극혐...",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045002.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045002.1.16",
          "form": "2단계부터는 씨앗 모양 바껴",
          "original_form": "2단계부터는 씨앗 모양 바껴",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045002.1.17",
          "form": "ㅇㅇ근데",
          "original_form": "ㅇㅇ근데",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045002.1.18",
          "form": "디자인이 썩",
          "original_form": "디자인이 썩",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045002.1.19",
          "form": "구미가 당기는 모양은",
          "original_form": "구미가 당기는 모양은",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045002.1.20",
          "form": "아니더라.....",
          "original_form": "아니더라.....",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045002.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045002.1.22",
          "form": "걍 성능이 더 좋아졋단데에",
          "original_form": "걍 성능이 더 좋아졋단데에",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045002.1.23",
          "form": "의의를 두는 걸로...",
          "original_form": "의의를 두는 걸로...",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045002.1.24",
          "form": "ㅇㅇ.....",
          "original_form": "ㅇㅇ.....",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045002.1.25",
          "form": "번개씨앗 설명만 읽어보면",
          "original_form": "번개씨앗 설명만 읽어보면",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045002.1.26",
          "form": "전기캐로 달고싸우면",
          "original_form": "전기캐로 달고싸우면",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045002.1.27",
          "form": "완전 사기일것같은데",
          "original_form": "완전 사기일것같은데",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045002.1.28",
          "form": "설명은 그런데",
          "original_form": "설명은 그런데",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045002.1.29",
          "form": "실제론 체감 잘 안되더라",
          "original_form": "실제론 체감 잘 안되더라",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045002.1.30",
          "form": "난 잘 체감 못함...",
          "original_form": "난 잘 체감 못함...",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045002.1.31",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045002.1.32",
          "form": "ㅋㅋㅋㅋㅋ 역시 사람 느끼는거 다 똑같군...",
          "original_form": "ㅋㅋㅋㅋㅋ 역시 사람 느끼는거 다 똑같군...",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045002.1.33",
          "form": "라이덴으로 1초에 다섯번씩 원소공격하면",
          "original_form": "라이덴으로 1초에 다섯번씩 원소공격하면",
