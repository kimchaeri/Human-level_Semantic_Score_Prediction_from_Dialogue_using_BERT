      "utterance": [
        {
          "id": "MDRW2100024909.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024909.1.2",
          "form": "집안일은 끝내고",
          "original_form": "집안일은 끝내고",
          "speaker_id": "2",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100024909.1.3",
          "form": "내일 할일들 정리중이요",
          "original_form": "내일 할일들 정리중이요",
          "speaker_id": "2",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100024909.1.4",
          "form": "집안일하세요?",
          "original_form": "집안일하세요?",
          "speaker_id": "2",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100024909.1.5",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024909.1.6",
          "form": "매일하세요?",
          "original_form": "매일하세요?",
          "speaker_id": "2",
          "time": "20210831 23:52"
        },
        {
          "id": "MDRW2100024909.1.7",
          "form": "일주일동안 1번이요",
          "original_form": "일주일동안 1번이요",
          "speaker_id": "1",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100024909.1.8",
          "form": "편하시겠어요",
          "original_form": "편하시겠어요",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100024909.1.9",
          "form": "힘들조",
          "original_form": "힘들조",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100024909.1.10",
          "form": "일주일1번은 할수있을것같아요 ㅎ",
          "original_form": "일주일1번은 할수있을것같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100024909.1.11",
          "form": "그래도 귀찮죠",
          "original_form": "그래도 귀찮죠",
          "speaker_id": "1",
          "time": "20210902 07:19"
        },
        {
          "id": "MDRW2100024909.1.12",
          "form": "저는 머리카락이나 머하나보이면 바로 청소기를 드는사람이라 ㅋ",
          "original_form": "저는 머리카락이나 머하나보이면 바로 청소기를 드는사람이라 ㅋ",
          "speaker_id": "2",
          "time": "20210902 12:39"
        },
        {
          "id": "MDRW2100024909.1.13",
          "form": "부지런하시네여",
          "original_form": "부지런하시네여",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100024909.1.14",
          "form": "보이면 찝찝해서요",
          "original_form": "보이면 찝찝해서요",
