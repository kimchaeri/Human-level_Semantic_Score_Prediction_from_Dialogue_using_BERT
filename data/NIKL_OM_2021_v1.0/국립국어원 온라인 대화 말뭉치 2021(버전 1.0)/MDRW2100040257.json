      "utterance": [
        {
          "id": "MDRW2100040257.1.1",
          "form": "콘서트 가보셨어요?",
          "original_form": "콘서트 가보셨어요?",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040257.1.2",
          "form": "누구 좋아하세요?",
          "original_form": "누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040257.1.3",
          "form": "콘서트는 아.. 생각이 잘 안나는데",
          "original_form": "콘서트는 아.. 생각이 잘 안나는데",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040257.1.4",
          "form": "좋아하는 아티스트는 많은데 이상하게",
          "original_form": "좋아하는 아티스트는 많은데 이상하게",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040257.1.5",
          "form": "콘서트까지 가기는 귀찮더라구요...?",
          "original_form": "콘서트까지 가기는 귀찮더라구요...?",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040257.1.6",
          "form": "네 그리고티켓팅의 압박이ㅠ",
          "original_form": "네 그리고티켓팅의 압박이ㅠ",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040257.1.7",
          "form": "관심있는건 이미 누가 다 좋은자리 채갔더라구요",
          "original_form": "관심있는건 이미 누가 다 좋은자리 채갔더라구요",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040257.1.8",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040257.1.9",
          "form": "저는 수강신청도 스트레스 받아하는데",
          "original_form": "저는 수강신청도 스트레스 받아하는데",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040257.1.10",
          "form": "경쟁하는 거 너무 싫어요 ㅜㅜㅜ",
          "original_form": "경쟁하는 거 너무 싫어요 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040257.1.11",
          "form": "ㅋㅋ앗 저도요ㅋㅋㅋ",
          "original_form": "ㅋㅋ앗 저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040257.1.12",
          "form": "코로나 때문에 자리도 별로 없을텐데...",
          "original_form": "코로나 때문에 자리도 별로 없을텐데...",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040257.1.13",
          "form": "저 좋아하는 아티스트는 되게 많거든요?",
          "original_form": "저 좋아하는 아티스트는 되게 많거든요?",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040257.1.14",
          "form": "근데 항상 느끼는 건데",
          "original_form": "근데 항상 느끼는 건데",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040257.1.15",
          "form": "라이브를 음원만큼 잘 부르진 않아서 제가 좀 실망을 하더라구욬ㅋㅋㅋㅋ",
          "original_form": "라이브를 음원만큼 잘 부르진 않아서 제가 좀 실망을 하더라구욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040257.1.16",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040257.1.17",
          "form": "저는 라이브를 본건 예전에 박진영밖에없어서",
          "original_form": "저는 라이브를 본건 예전에 박진영밖에없어서",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040257.1.18",
          "form": "오?!",
          "original_form": "오?!",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040257.1.19",
          "form": "다들 라이브도 잘하는줄 알았어요",
          "original_form": "다들 라이브도 잘하는줄 알았어요",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040257.1.20",
          "form": "박진영이라니!!!",
          "original_form": "박진영이라니!!!",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040257.1.21",
          "form": "그러고보니 정말 신기한게",
          "original_form": "그러고보니 정말 신기한게",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040257.1.22",
          "form": "공개방송같은거 할때 동네에 와서 가본적 있어요",
          "original_form": "공개방송같은거 할때 동네에 와서 가본적 있어요",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040257.1.23",
          "form": "아이돌 소속사 대형 3사는",
          "original_form": "아이돌 소속사 대형 3사는",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040257.1.24",
          "form": "다 나이 많은 사람들이",
          "original_form": "다 나이 많은 사람들이",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040257.1.25",
          "form": "사장이고",
          "original_form": "사장이고",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040257.1.26",
          "form": "심지어 이수만은 할아버지잖아요?",
          "original_form": "심지어 이수만은 할아버지잖아요?",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040257.1.27",
          "form": "옛날 가수출신이죠ㅋㅋ",
          "original_form": "옛날 가수출신이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040257.1.28",
          "form": "근데 그들 또래는 잘 모르는 시장을 개척한다는게 너무 신기해요",
          "original_form": "근데 그들 또래는 잘 모르는 시장을 개척한다는게 너무 신기해요",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040257.1.29",
          "form": "그니까요!!",
          "original_form": "그니까요!!",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040257.1.30",
          "form": "ㅋㅋㅋ그래서 말아먹은 적도 많더라구요",
          "original_form": "ㅋㅋㅋ그래서 말아먹은 적도 많더라구요",
          "speaker_id": "2",
          "time": "20210904 15:12"
        }
