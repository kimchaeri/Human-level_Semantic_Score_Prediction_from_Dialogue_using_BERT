      "utterance": [
        {
          "id": "MDRW2100000813.1.1",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000813.1.2",
          "form": "네 저 커피 좋아해요",
          "original_form": "네 저 커피 좋아해요",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000813.1.3",
          "form": "안그래도 지금 마시고 있어요!",
          "original_form": "안그래도 지금 마시고 있어요!",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000813.1.4",
          "form": "하루에 한잔 안먹으면 너무 허전해요ㅋㅋ 아메리카노 주로마시는데 어떤거 좋아하세요?",
          "original_form": "하루에 한잔 안먹으면 너무 허전해요ㅋㅋ 아메리카노 주로마시는데 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000813.1.5",
          "form": "저도 아메리카노 좋아해요",
          "original_form": "저도 아메리카노 좋아해요",
          "speaker_id": "2",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000813.1.6",
          "form": "라떼도 좋아하세요? 전 우유 들어가면 잘 못마시겠어요",
          "original_form": "라떼도 좋아하세요? 전 우유 들어가면 잘 못마시겠어요",
          "speaker_id": "2",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100000813.1.7",
          "form": "저도 라떼류는 배가아파서 아메리카노만 마셔요",
          "original_form": "저도 라떼류는 배가아파서 아메리카노만 마셔요",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000813.1.8",
          "form": "사실 단음료는 별로안좋아해서요",
          "original_form": "사실 단음료는 별로안좋아해서요",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000813.1.9",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000813.1.10",
          "form": "간식으로 초콜렛이나 단 건 잘 먹는데",
          "original_form": "간식으로 초콜렛이나 단 건 잘 먹는데",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000813.1.11",
          "form": "음료는 단 거 잘 못 마셔요ㅠㅠ",
          "original_form": "음료는 단 거 잘 못 마셔요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000813.1.12",
          "form": "예전에는 엄청좋아했는데 입이 텁텁한 기분이 들어서 잘못먹겟더라구요",
          "original_form": "예전에는 엄청좋아했는데 입이 텁텁한 기분이 들어서 잘못먹겟더라구요",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000813.1.13",
          "form": "저도 간식으로 달달한건 좋아해요",
          "original_form": "저도 간식으로 달달한건 좋아해요",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000813.1.14",
          "form": "오 신기하다 비슷하네요",
          "original_form": "오 신기하다 비슷하네요",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000813.1.15",
          "form": "간식은 진짜 끝도 없이 먹을 수 있어요",
          "original_form": "간식은 진짜 끝도 없이 먹을 수 있어요",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000813.1.16",
          "form": "그럼 아메에도 시럽안넣으시겠내요",
          "original_form": "그럼 아메에도 시럽안넣으시겠내요",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000813.1.17",
          "form": "네네 그럼요 안넣어요!",
          "original_form": "네네 그럼요 안넣어요!",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000813.1.18",
          "form": "아 근데 가끔 헤이즐넛 아메리카노 먹긴 해요",
          "original_form": "아 근데 가끔 헤이즐넛 아메리카노 먹긴 해요",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000813.1.19",
          "form": "그건 향이 너무 좋아요~ 헤이즐럿",
          "original_form": "그건 향이 너무 좋아요~ 헤이즐럿",
          "speaker_id": "1",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000813.1.20",
          "form": "그쵸 저도 향을 좋아해요",
          "original_form": "그쵸 저도 향을 좋아해요",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000813.1.21",
          "form": "그래서 처음에 향 맡을땐 좋은데",
          "original_form": "그래서 처음에 향 맡을땐 좋은데",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000813.1.22",
          "form": "마실수록 후회해요 너무 달아서ㅠ",
          "original_form": "마실수록 후회해요 너무 달아서ㅠ",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000813.1.23",
          "form": "마실수록 후회한다는 말이 공감되네요ㅋㅋㅋ 저도 다른거 마시먄 후회해요",
          "original_form": "마실수록 후회한다는 말이 공감되네요ㅋㅋㅋ 저도 다른거 마시먄 후회해요",
          "speaker_id": "1",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100000813.1.24",
          "form": "그러니까요 진짜 베이직이 베스트",
          "original_form": "그러니까요 진짜 베이직이 베스트",
          "speaker_id": "2",
          "time": "20210819 21:15"
        },
        {
          "id": "MDRW2100000813.1.25",
          "form": "아 근데 가끔 생과일쥬스 이런거 먹고싶을 때도 있어요",
          "original_form": "아 근데 가끔 생과일쥬스 이런거 먹고싶을 때도 있어요",
          "speaker_id": "2",
          "time": "20210819 21:16"
        },
        {
          "id": "MDRW2100000813.1.26",
          "form": "아 저도 과일은 좋아해요~ㅋㅋ 입맛이 비슷하네요",
          "original_form": "아 저도 과일은 좋아해요~ㅋㅋ 입맛이 비슷하네요",
          "speaker_id": "1",
          "time": "20210819 21:17"
        }
      ]
    }
