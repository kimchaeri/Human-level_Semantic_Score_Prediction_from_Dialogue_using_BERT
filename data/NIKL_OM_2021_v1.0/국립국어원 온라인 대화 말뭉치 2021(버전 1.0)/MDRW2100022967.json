      "utterance": [
        {
          "id": "MDRW2100022967.1.1",
          "form": "겨울 옷 좋아하면",
          "original_form": "겨울 옷 좋아하면",
          "speaker_id": "1",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022967.1.2",
          "form": "코트가 좋아 패딩이 좋아?",
          "original_form": "코트가 좋아 패딩이 좋아?",
          "speaker_id": "1",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022967.1.3",
          "form": "뭐여 채팅 어디갔어",
          "original_form": "뭐여 채팅 어디갔어",
          "speaker_id": "1",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100022967.1.4",
          "form": "겨울옷 좋아하는구나",
          "original_form": "겨울옷 좋아하는구나",
          "speaker_id": "1",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022967.1.5",
          "form": "넌 그럼 겨울옷 중에 코트가 좋아 패딩이 좋아?",
          "original_form": "넌 그럼 겨울옷 중에 코트가 좋아 패딩이 좋아?",
          "speaker_id": "1",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022967.1.6",
          "form": "응 겨울옷은 디자인도 다양해서좋아",
          "original_form": "응 겨울옷은 디자인도 다양해서좋아",
          "speaker_id": "2",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022967.1.7",
          "form": "난 코트가 더 좋더라",
          "original_form": "난 코트가 더 좋더라",
          "speaker_id": "2",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022967.1.8",
          "form": "오올 나도 코트를 더 좋아해",
          "original_form": "오올 나도 코트를 더 좋아해",
          "speaker_id": "1",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100022967.1.9",
          "form": "난 후리스도 좋아해",
          "original_form": "난 후리스도 좋아해",
          "speaker_id": "1",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100022967.1.10",
          "form": "세탁하기 편하거든",
          "original_form": "세탁하기 편하거든",
          "speaker_id": "1",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100022967.1.11",
          "form": "여름은",
          "original_form": "여름은",
          "speaker_id": "2",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022967.1.12",
          "form": "옷이 거기서 거기라",
          "original_form": "옷이 거기서 거기라",
          "speaker_id": "2",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022967.1.13",
          "form": "입을게없어",
          "original_form": "입을게없어",
          "speaker_id": "2",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022967.1.14",
          "form": "맞아 여름옷은 반팔뿐이야",
          "original_form": "맞아 여름옷은 반팔뿐이야",
          "speaker_id": "1",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022967.1.15",
          "form": "넌 티셔츠가 좋니 셔츠가 좋니",
          "original_form": "넌 티셔츠가 좋니 셔츠가 좋니",
          "speaker_id": "1",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022967.1.16",
          "form": "난 무조건 셔츠파야",
          "original_form": "난 무조건 셔츠파야",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100022967.1.17",
          "form": "나도 셔츠가 좋아",
          "original_form": "나도 셔츠가 좋아",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022967.1.18",
          "form": "셔츠는 계절상관없이 입을수있어서",
          "original_form": "셔츠는 계절상관없이 입을수있어서",
          "speaker_id": "2",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022967.1.19",
          "form": "좋더라",
          "original_form": "좋더라",
          "speaker_id": "2",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022967.1.20",
          "form": "맞아 그리고 대충 걸쳐도 차려입은 느낌이야",
          "original_form": "맞아 그리고 대충 걸쳐도 차려입은 느낌이야",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022967.1.21",
          "form": "넌 제일 좋아하는 셔츠 색이 뭐야?",
          "original_form": "넌 제일 좋아하는 셔츠 색이 뭐야?",
          "speaker_id": "1",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100022967.1.22",
          "form": "난 가을같은 브라운색이 좋더라",
          "original_form": "난 가을같은 브라운색이 좋더라",
          "speaker_id": "2",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022967.1.23",
          "form": "나는 여름에 흰 색 입는 걸 좋아해",
          "original_form": "나는 여름에 흰 색 입는 걸 좋아해",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022967.1.24",
          "form": "겨울엔 검정색이 왠지 좋지않냐",
          "original_form": "겨울엔 검정색이 왠지 좋지않냐",
          "speaker_id": "2",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022967.1.25",
          "form": "맞아 검정색 겨울에 진중해보이고 멋있어",
          "original_form": "맞아 검정색 겨울에 진중해보이고 멋있어",
          "speaker_id": "1",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100022967.1.26",
          "form": "여름에도 검정옷 입니?",
          "original_form": "여름에도 검정옷 입니?",
          "speaker_id": "1",
          "time": "20210829 19:58"
        }
      ]
    }
  ]
