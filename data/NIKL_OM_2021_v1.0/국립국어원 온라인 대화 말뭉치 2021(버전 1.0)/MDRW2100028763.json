      "utterance": [
        {
          "id": "MDRW2100028763.1.1",
          "form": "요즘 가을장마 넘 싫어여 ㅎㅎ",
          "original_form": "요즘 가을장마 넘 싫어여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028763.1.2",
          "form": "오늘 계신곳 비왔나요?",
          "original_form": "오늘 계신곳 비왔나요?",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028763.1.3",
          "form": "오늘은 괜찮은데 흐리고",
          "original_form": "오늘은 괜찮은데 흐리고",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028763.1.4",
          "form": "오늘빼곤 거의 비온거 같아요",
          "original_form": "오늘빼곤 거의 비온거 같아요",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028763.1.5",
          "form": "담주 화요일 전국에 비온다네요",
          "original_form": "담주 화요일 전국에 비온다네요",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028763.1.6",
          "form": "요즘엔 우산이 필수인거 같아요",
          "original_form": "요즘엔 우산이 필수인거 같아요",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028763.1.7",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028763.1.8",
          "form": "가디건도 이제 챙겨야 할거 같아요ㅎ",
          "original_form": "가디건도 이제 챙겨야 할거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028763.1.9",
          "form": "전 늘 챙겨요  날씨가 ㅎ",
          "original_form": "전 늘 챙겨요  날씨가 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028763.1.10",
          "form": "전철도 가끔은 춥더라구요",
          "original_form": "전철도 가끔은 춥더라구요",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028763.1.11",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028763.1.12",
          "form": "전 겨울요",
          "original_form": "전 겨울요",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028763.1.13",
          "form": "저도 겨울 좋아해요!",
          "original_form": "저도 겨울 좋아해요!",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028763.1.14",
          "form": "겨울은 해가 짧아서 좋아합니다",
          "original_form": "겨울은 해가 짧아서 좋아합니다",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028763.1.15",
          "form": "전 마스크 쓰긴 겨울이 편해서요",
          "original_form": "전 마스크 쓰긴 겨울이 편해서요",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028763.1.16",
          "form": "앗 그렇죠 추울때 마스크 좋긴 좋아요ㅎ",
          "original_form": "앗 그렇죠 추울때 마스크 좋긴 좋아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028763.1.17",
          "form": "상대방들도  더 잘쓰구요",
          "original_form": "상대방들도  더 잘쓰구요",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028763.1.18",
          "form": "여름엔 더우니 안쓰시는분 많아요 ㅠ",
          "original_form": "여름엔 더우니 안쓰시는분 많아요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028763.1.19",
          "form": "네 오늘도 저녁에 산책하는데",
          "original_form": "네 오늘도 저녁에 산책하는데",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028763.1.20",
          "form": "안쓰는 사람들 넘 많아졌어요",
          "original_form": "안쓰는 사람들 넘 많아졌어요",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028763.1.21",
          "form": "신고하고 싶던...",
          "original_form": "신고하고 싶던...",
          "speaker_id": "1",
          "time": "20210902 22:28"
        }
