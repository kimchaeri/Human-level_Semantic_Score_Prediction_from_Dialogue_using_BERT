      "utterance": [
        {
          "id": "MDRW2100004471.1.1",
          "form": "오늘 아빠가 아침에 나 컴하는데 방에들어와서",
          "original_form": "오늘 아빠가 아침에 나 컴하는데 방에들어와서",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004471.1.2",
          "form": "시원한데 왜 선풍기를 틀어 하면서",
          "original_form": "시원한데 왜 선풍기를 틀어 하면서",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004471.1.3",
          "form": "창문열고 방문열도하는데",
          "original_form": "창문열고 방문열도하는데",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004471.1.4",
          "form": "개스트레스",
          "original_form": "개스트레스",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004471.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌ아웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌ아웃겨",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004471.1.6",
          "form": "아 뭐야 이거 왜 없어져",
          "original_form": "아 뭐야 이거 왜 없어져",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004471.1.7",
          "form": "쨌든 아 에어컨도아니고",
          "original_form": "쨌든 아 에어컨도아니고",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004471.1.8",
          "form": "선풍긴데 왜그러는데요~~~~",
          "original_form": "선풍긴데 왜그러는데요~~~~",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004471.1.9",
          "form": "말은 못하고 속으로 삭이기",
          "original_form": "말은 못하고 속으로 삭이기",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004471.1.10",
          "form": "제발 가족이라도 방에 벌컥벌컥 들어오지 말았으면",
          "original_form": "제발 가족이라도 방에 벌컥벌컥 들어오지 말았으면",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004471.1.11",
          "form": "아빠라서..",
          "original_form": "아빠라서..",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004471.1.12",
          "form": "문잠구면 되자늠",
          "original_form": "문잠구면 되자늠",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004471.1.13",
          "form": "으에에에유ㅠㅠㅠ",
          "original_form": "으에에에유ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004471.1.14",
          "form": "내가 잠그믄걸 별로 안좋아함",
          "original_form": "내가 잠그믄걸 별로 안좋아함",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004471.1.15",
          "form": "헐 왜.. 근데 부모님들 다 그렇더라",
          "original_form": "헐 왜.. 근데 부모님들 다 그렇더라",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004471.1.16",
          "form": "한여름일때보다는 덜들어오시기는 하는데..",
          "original_form": "한여름일때보다는 덜들어오시기는 하는데..",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004471.1.17",
          "form": "이래나저래나 스트레스인건 동일",
          "original_form": "이래나저래나 스트레스인건 동일",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004471.1.18",
          "form": "문을.. 몰래잠구면 어때",
          "original_form": "문을.. 몰래잠구면 어때",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004471.1.19",
          "form": "ㅋㅋㅋㅋㅋ아니 내가 문잠그는걸 별로 안좋아한다니깐요",
          "original_form": "ㅋㅋㅋㅋㅋ아니 내가 문잠그는걸 별로 안좋아한다니깐요",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004471.1.20",
          "form": "그냥 문 살짝열어놓고 시야가리는정도를 좋아하는데",
          "original_form": "그냥 문 살짝열어놓고 시야가리는정도를 좋아하는데",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004471.1.21",
          "form": "엄빠가 벌컥벌컥들어오는 상황이라는거지",
          "original_form": "엄빠가 벌컥벌컥들어오는 상황이라는거지",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004471.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004471.1.23",
          "form": "잘못이해람",
          "original_form": "잘못이해람",
          "speaker_id": "2",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004471.1.24",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ 내가 안좋아하는걸 다르게 이해했구낰ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ 내가 안좋아하는걸 다르게 이해했구낰ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004471.1.25",
          "form": "ㅇㅇㅇㅇㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅇㅇㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004471.1.26",
          "form": "쨌든 가족에게의 요새의 바람은",
          "original_form": "쨌든 가족에게의 요새의 바람은",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004471.1.27",
          "form": "제발 노크좀하세요",
          "original_form": "제발 노크좀하세요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004471.1.28",
          "form": "가 되어버린 상황",
          "original_form": "가 되어버린 상황",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004471.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 노크좀 하고 오라고~~~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 노크좀 하고 오라고~~~",
