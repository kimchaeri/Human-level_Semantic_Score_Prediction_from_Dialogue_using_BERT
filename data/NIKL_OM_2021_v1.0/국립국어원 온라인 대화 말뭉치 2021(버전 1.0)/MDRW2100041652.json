      "utterance": [
        {
          "id": "MDRW2100041652.1.1",
          "form": "닭갈비 좋아하세요?",
          "original_form": "닭갈비 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100041652.1.2",
          "form": "닭갈비 좋아하죠",
          "original_form": "닭갈비 좋아하죠",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041652.1.3",
          "form": "저는 치즈닭갈비 좋아해요",
          "original_form": "저는 치즈닭갈비 좋아해요",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041652.1.4",
          "form": "아 치즈 닭갈비 맛있겠네요",
          "original_form": "아 치즈 닭갈비 맛있겠네요",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041652.1.5",
          "form": "마지막에 꼭 밥볶아 먹기!",
          "original_form": "마지막에 꼭 밥볶아 먹기!",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041652.1.6",
          "form": "우동사리 넣어도 정말 맛있어요",
          "original_form": "우동사리 넣어도 정말 맛있어요",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041652.1.7",
          "form": "하 우동사리.....",
          "original_form": "하 우동사리.....",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041652.1.8",
          "form": "쭈꾸미닭갈비도 맛있어요",
          "original_form": "쭈꾸미닭갈비도 맛있어요",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100041652.1.9",
          "form": "쭈꾸미랑 닭이랑 같이들어가있나요?",
          "original_form": "쭈꾸미랑 닭이랑 같이들어가있나요?",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100041652.1.10",
          "form": "아 너무 먹고싶어요ㅠㅠㅠㅠ내일 점심으로 먹을까..",
          "original_form": "아 너무 먹고싶어요ㅠㅠㅠㅠ내일 점심으로 먹을까..",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100041652.1.11",
          "form": "양배추 샐러드랑 먹으면 너무 맛있어요",
          "original_form": "양배추 샐러드랑 먹으면 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100041652.1.12",
          "form": "맞아요 저도 양배추 매번 엄청 먹어요 ㅋㅋ",
          "original_form": "맞아요 저도 양배추 매번 엄청 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100041652.1.13",
          "form": "유가네 올만에 가고싶네요",
          "original_form": "유가네 올만에 가고싶네요",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100041652.1.14",
          "form": "추억의 유가네 ㅎㅎ",
          "original_form": "추억의 유가네 ㅎㅎ",
