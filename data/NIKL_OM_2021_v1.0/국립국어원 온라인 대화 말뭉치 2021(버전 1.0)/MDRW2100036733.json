      "utterance": [
        {
          "id": "MDRW2100036733.1.1",
          "form": "저번에 티비보고 가운데 뚫어서 부쳐봤는데 안뚢리더라구요?!",
          "original_form": "저번에 티비보고 가운데 뚫어서 부쳐봤는데 안뚢리더라구요?!",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036733.1.2",
          "form": "거기 날씨 어떤가용?",
          "original_form": "거기 날씨 어떤가용?",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036733.1.3",
          "form": "여기는 비가 왔다안왔다그러네요ㅠ",
          "original_form": "여기는 비가 왔다안왔다그러네요ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100036733.1.4",
          "form": "촤아악한번내리면",
          "original_form": "촤아악한번내리면",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100036733.1.5",
          "form": "좋은데",
          "original_form": "좋은데",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100036733.1.6",
          "form": "아ㅠ 맞아요",
          "original_form": "아ㅠ 맞아요",
          "speaker_id": "2",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100036733.1.7",
          "form": "올거면 차라리 시원하게 내림 좋겠어요",
          "original_form": "올거면 차라리 시원하게 내림 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100036733.1.8",
          "form": "오긴와서 운동은 못가는데",
          "original_form": "오긴와서 운동은 못가는데",
          "speaker_id": "1",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100036733.1.9",
          "form": "온다고하긴 너무조금오는,,,",
          "original_form": "온다고하긴 너무조금오는,,,",
          "speaker_id": "1",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100036733.1.10",
          "form": "어후 최악이에요",
          "original_form": "어후 최악이에요",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036733.1.11",
          "form": "꼭 나가면 비오잖아요!",
          "original_form": "꼭 나가면 비오잖아요!",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036733.1.12",
          "form": "그래도오늘 긴팔입으면",
          "original_form": "그래도오늘 긴팔입으면",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036733.1.13",
          "form": "딱좋게",
          "original_form": "딱좋게",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036733.1.14",
          "form": "준비 다해서 나가는건데",
          "original_form": "준비 다해서 나가는건데",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036733.1.15",
          "form": "많이선선해졌더라고요",
          "original_form": "많이선선해졌더라고요",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036733.1.16",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100036733.1.17",
          "form": "확실히 걸치는게 필요해졌구나 느껴용!",
          "original_form": "확실히 걸치는게 필요해졌구나 느껴용!",
          "speaker_id": "2",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036733.1.18",
          "form": "내일까지 비온다던데ㅠ",
          "original_form": "내일까지 비온다던데ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036733.1.19",
          "form": "앗.. 세상에나네요..",
          "original_form": "앗.. 세상에나네요..",
          "speaker_id": "2",
          "time": "20210831 10:52"
        },
        {
          "id": "MDRW2100036733.1.20",
          "form": "저흰 목욜까지 비소식 있네요ㅠ",
          "original_form": "저흰 목욜까지 비소식 있네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100036733.1.21",
          "form": "아이고ㅠ",
          "original_form": "아이고ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100036733.1.22",
          "form": "그래도 남방입는거 좋아요",
          "original_form": "그래도 남방입는거 좋아요",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100036733.1.23",
          "form": "가을이라니!",
          "original_form": "가을이라니!",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100036733.1.24",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100036733.1.25",
          "form": "좀 만끽하고픈데 비가 방해하네요!!",
          "original_form": "좀 만끽하고픈데 비가 방해하네요!!",
          "speaker_id": "2",
          "time": "20210831 10:55"
        },
        {
          "id": "MDRW2100036733.1.26",
          "form": "이러다 금방 겨울이 와버리겠죠ㅠ",
          "original_form": "이러다 금방 겨울이 와버리겠죠ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:56"
        }
      ]
    }
