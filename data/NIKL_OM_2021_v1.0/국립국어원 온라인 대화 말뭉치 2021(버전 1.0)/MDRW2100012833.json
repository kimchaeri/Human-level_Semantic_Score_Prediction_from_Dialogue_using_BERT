      "utterance": [
        {
          "id": "MDRW2100012833.1.1",
          "form": "사고 싶은 물건 있으세요?",
          "original_form": "사고 싶은 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012833.1.2",
          "form": "로봇청소기를 사고 싶어요",
          "original_form": "로봇청소기를 사고 싶어요",
          "speaker_id": "2",
          "time": "20210825 02:16"
        },
        {
          "id": "MDRW2100012833.1.3",
          "form": "강아지를 키워서 포기했지만ㅜ",
          "original_form": "강아지를 키워서 포기했지만ㅜ",
          "speaker_id": "2",
          "time": "20210825 02:16"
        },
        {
          "id": "MDRW2100012833.1.4",
          "form": "어떤 문제점이있나요?",
          "original_form": "어떤 문제점이있나요?",
          "speaker_id": "1",
          "time": "20210825 02:17"
        },
        {
          "id": "MDRW2100012833.1.5",
          "form": "저는 엘지 그램 노트북 사고싶네요",
          "original_form": "저는 엘지 그램 노트북 사고싶네요",
          "speaker_id": "1",
          "time": "20210825 02:17"
        },
        {
          "id": "MDRW2100012833.1.6",
          "form": "강아지가 무서워하는게 하나",
          "original_form": "강아지가 무서워하는게 하나",
          "speaker_id": "2",
          "time": "20210825 02:18"
        },
        {
          "id": "MDRW2100012833.1.7",
          "form": "아 무서워하는 군요",
          "original_form": "아 무서워하는 군요",
          "speaker_id": "1",
          "time": "20210825 02:19"
        },
        {
          "id": "MDRW2100012833.1.8",
          "form": "강아지 대소변 실수했을때 그 잔해를 로봇청소기가 밟고 갈 거 걱정이 또 있고요",
          "original_form": "강아지 대소변 실수했을때 그 잔해를 로봇청소기가 밟고 갈 거 걱정이 또 있고요",
          "speaker_id": "2",
          "time": "20210825 02:19"
        },
        {
          "id": "MDRW2100012833.1.9",
          "form": "사실 후자가 가장 커요",
          "original_form": "사실 후자가 가장 커요",
          "speaker_id": "2",
          "time": "20210825 02:19"
        },
        {
          "id": "MDRW2100012833.1.10",
          "form": "최근에 구매한 물건있으세요?",
          "original_form": "최근에 구매한 물건있으세요?",
          "speaker_id": "1",
          "time": "20210825 02:19"
        },
        {
          "id": "MDRW2100012833.1.11",
          "form": "강아지 사료와 간식을 구입했어요",
          "original_form": "강아지 사료와 간식을 구입했어요",
          "speaker_id": "2",
          "time": "20210825 02:20"
        },
        {
          "id": "MDRW2100012833.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 02:20"
        },
        {
          "id": "MDRW2100012833.1.13",
          "form": "브리타정수기 샀어요",
          "original_form": "브리타정수기 샀어요",
          "speaker_id": "1",
          "time": "20210825 02:20"
        },
        {
          "id": "MDRW2100012833.1.14",
          "form": "강아지가 좋아하던가요?",
          "original_form": "강아지가 좋아하던가요?",
          "speaker_id": "1",
          "time": "20210825 02:21"
        },
        {
          "id": "MDRW2100012833.1.15",
          "form": "이미 강아지 입맛에 검증된 것만 샀지요ㅎ",
          "original_form": "이미 강아지 입맛에 검증된 것만 샀지요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:21"
        },
        {
          "id": "MDRW2100012833.1.16",
          "form": "아 강아지 칫솔 산 것은 싫어하다 못해 증오하네요ㅜ",
          "original_form": "아 강아지 칫솔 산 것은 싫어하다 못해 증오하네요ㅜ",
          "speaker_id": "2",
          "time": "20210825 02:22"
        },
        {
          "id": "MDRW2100012833.1.17",
          "form": "쇼핑몰 어떤 곳이용하세요?",
          "original_form": "쇼핑몰 어떤 곳이용하세요?",
          "speaker_id": "1",
          "time": "20210825 02:22"
        },
        {
          "id": "MDRW2100012833.1.18",
          "form": "강아지용품은 펫프렌즈",
          "original_form": "강아지용품은 펫프렌즈",
          "speaker_id": "2",
          "time": "20210825 02:23"
        },
        {
          "id": "MDRW2100012833.1.19",
          "form": "펫프렌즈 이용해본적있어요",
          "original_form": "펫프렌즈 이용해본적있어요",
          "speaker_id": "1",
