      "utterance": [
        {
          "id": "MDRW2100023015.1.1",
          "form": "혹시 민초파이신가요?",
          "original_form": "혹시 민초파이신가요?",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023015.1.2",
          "form": "앗.. 반민초입니다...",
          "original_form": "앗.. 반민초입니다...",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023015.1.3",
          "form": "치약...",
          "original_form": "치약...",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023015.1.4",
          "form": "민초...",
          "original_form": "민초...",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023015.1.5",
          "form": "ㅋㅋㅋㅋㅋ저도 반민초인데 주위에 민초파가 많아서요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 반민초인데 주위에 민초파가 많아서요",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023015.1.6",
          "form": "자꾸 맛알못이라고ㅠㅠ 그러더라고요ㅠㅠ",
          "original_form": "자꾸 맛알못이라고ㅠㅠ 그러더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023015.1.7",
          "form": "앗ㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023015.1.8",
          "form": "빈민초 합체",
          "original_form": "빈민초 합체{emoji:☆}",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023015.1.9",
          "form": "민초는 융화가 안되여... 어떤맛하고도 조화가안되서ㅠㅠ",
          "original_form": "민초는 융화가 안되여... 어떤맛하고도 조화가안되서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023015.1.10",
          "form": "제주변엔 특별히 민초좋아하는 친구들은 없는것 같아요 ㅎㅎ",
          "original_form": "제주변엔 특별히 민초좋아하는 친구들은 없는것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023015.1.11",
          "form": "그런데 요즘 민초맛 많이 보이지 않나요?",
          "original_form": "그런데 요즘 민초맛 많이 보이지 않나요?",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023015.1.12",
          "form": "신메뉴가 거의 민초...",
          "original_form": "신메뉴가 거의 민초...",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023015.1.13",
          "form": "그쳐... 관심이 없습니다 ㅋㅋㅋㅋㅋ 그들만의 리그라고 생각해요.. 뭔가 특별한 그룹만의 맛 같은 느낌이에요 ㅎㅎ",
          "original_form": "그쳐... 관심이 없습니다 ㅋㅋㅋㅋㅋ 그들만의 리그라고 생각해요.. 뭔가 특별한 그룹만의 맛 같은 느낌이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100023015.1.14",
          "form": "피스타치오 정도는 고소하고 그래서 먹을만한데..",
          "original_form": "피스타치오 정도는 고소하고 그래서 먹을만한데..",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100023015.1.15",
          "form": "민초는 어떻게 만들어진 음식인지 모르겠어요 ㅋㅋㅋ",
          "original_form": "민초는 어떻게 만들어진 음식인지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100023015.1.16",
          "form": "견과류 좋아해서 피스타치오는 괜찮아요",
          "original_form": "견과류 좋아해서 피스타치오는 괜찮아요",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023015.1.17",
          "form": "박하사탕도 괜찮은데",
          "original_form": "박하사탕도 괜찮은데",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023015.1.18",
          "form": "민초... 누가 만든 걸까요ㅋㅋㅋ",
          "original_form": "민초... 누가 만든 걸까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023015.1.19",
          "form": "맞아요 박하사탕까지도 괜찮은데 ㅋㅋ",
          "original_form": "맞아요 박하사탕까지도 괜찮은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023015.1.20",
          "form": "민초의 원재료는 뭐죠....?",
          "original_form": "민초의 원재료는 뭐죠....?",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023015.1.21",
          "form": "민트... 향신료... 그치만 따로 먹는다는 생각... 누가한거죠... ㅋㅋㅋㅋㅋ",
          "original_form": "민트... 향신료... 그치만 따로 먹는다는 생각... 누가한거죠... ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023015.1.22",
          "form": "그쵸 신기하네요.. 발명한 사람이름이 민트인가...",
          "original_form": "그쵸 신기하네요.. 발명한 사람이름이 민트인가...",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023015.1.23",
          "form": "민트 이름은 그리스로마신화에 나와요",
          "original_form": "민트 이름은 그리스로마신화에 나와요",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023015.1.24",
          "form": "하데스랑 바람 핀? 요정 이름이래요",
          "original_form": "하데스랑 바람 핀? 요정 이름이래요",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100023015.1.25",
          "form": "옴",
          "original_form": "옴",
          "speaker_id": "1",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023015.1.26",
          "form": "오모나 ㅋㅋㅋㅋㅋ",
          "original_form": "오모나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023015.1.27",
