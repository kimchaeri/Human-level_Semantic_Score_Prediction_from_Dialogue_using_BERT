      "utterance": [
        {
          "id": "MDRW2100008323.1.1",
          "form": "전 중탕으로 끓여요",
          "original_form": "전 중탕으로 끓여요",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008323.1.2",
          "form": "계란 몇개에 물 어는정도요?",
          "original_form": "계란 몇개에 물 어는정도요?",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008323.1.3",
          "form": "계란. 물 1:1정도. 물에 소금이랑 다시다조금풀고 나중에 섞어요",
          "original_form": "계란. 물 1:1정도. 물에 소금이랑 다시다조금풀고 나중에 섞어요",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008323.1.4",
          "form": "아 중탕으로해야되는군요",
          "original_form": "아 중탕으로해야되는군요",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008323.1.5",
          "form": "식구가있으니 전 3~4개. 마지막으로 우유조금",
          "original_form": "식구가있으니 전 3~4개. 마지막으로 우유조금",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008323.1.6",
          "form": "전자렌지돌리니 푸석하더라구요",
          "original_form": "전자렌지돌리니 푸석하더라구요",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008323.1.7",
          "form": "그래야 기포없이 보들보들하더라구요",
          "original_form": "그래야 기포없이 보들보들하더라구요",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008323.1.8",
          "form": "배달시킴 오는 계란찜은 완전부드럽던데",
          "original_form": "배달시킴 오는 계란찜은 완전부드럽던데",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008323.1.9",
          "form": "스텐조그마한볼에 해요",
          "original_form": "스텐조그마한볼에 해요",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008323.1.10",
          "form": "푸딩처럼",
          "original_form": "푸딩처럼",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008323.1.11",
          "form": "새우젓도 넣으시나요?",
          "original_form": "새우젓도 넣으시나요?",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008323.1.12",
          "form": "그니깐요.  전자렌지에하는건데.  전하면 그렇게 안되더라구요",
          "original_form": "그니깐요.  전자렌지에하는건데.  전하면 그렇게 안되더라구요",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008323.1.13",
          "form": "전 소금. 다시다로 간해요",
          "original_form": "전 소금. 다시다로 간해요",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008323.1.14",
          "form": "새우젓. 설탕 조합도 있더라구요. 이게 별미래요",
          "original_form": "새우젓. 설탕 조합도 있더라구요. 이게 별미래요",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008323.1.15",
          "form": "이따 해먹어봐야겠어요ㅎ",
          "original_form": "이따 해먹어봐야겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008323.1.16",
          "form": "설탕도들어가요?",
          "original_form": "설탕도들어가요?",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008323.1.17",
          "form": "전 안해봤는데 누가 식당에서 알바할때 저렇게했다고~~ 끝내준데요",
          "original_form": "전 안해봤는데 누가 식당에서 알바할때 저렇게했다고~~ 끝내준데요",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008323.1.18",
          "form": "계란찜에요?ㅎ 맛이상할거같은데",
          "original_form": "계란찜에요?ㅎ 맛이상할거같은데",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008323.1.19",
          "form": "저도 그럴꺼같은데 강추한데요",
          "original_form": "저도 그럴꺼같은데 강추한데요",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008323.1.20",
          "form": "조금 조미료치는것처럼 넣나봐요",
          "original_form": "조금 조미료치는것처럼 넣나봐요",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008323.1.21",
          "form": "그런가보네요",
          "original_form": "그런가보네요",
          "speaker_id": "1",
          "time": "20210822 15:13"
        }
      ]
