      "utterance": [
        {
          "id": "MDRW2100007905.1.1",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007905.1.2",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007905.1.3",
          "form": "세례명은 있는데 냉담자예요",
          "original_form": "세례명은 있는데 냉담자예요",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007905.1.4",
          "form": "님은 있으신가요",
          "original_form": "님은 있으신가요",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007905.1.5",
          "form": "저는 없아요",
          "original_form": "저는 없아요",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007905.1.6",
          "form": "부모님은 불교",
          "original_form": "부모님은 불교",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007905.1.7",
          "form": "아 그럼 부모님 따라 절 가보셨겠네요",
          "original_form": "아 그럼 부모님 따라 절 가보셨겠네요",
          "speaker_id": "2",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007905.1.8",
          "form": "친구따라 교회만요",
          "original_form": "친구따라 교회만요",
          "speaker_id": "1",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007905.1.9",
          "form": "아니ㅎㅎ 이 무슨 혼종인가요",
          "original_form": "아니ㅎㅎ 이 무슨 혼종인가요",
          "speaker_id": "2",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007905.1.10",
          "form": "하긴 저도 친구따라 교회는 가봤네요",
          "original_form": "하긴 저도 친구따라 교회는 가봤네요",
          "speaker_id": "2",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007905.1.11",
          "form": "어릴때는 꿀잉죠",
          "original_form": "어릴때는 꿀잉죠",
          "speaker_id": "1",
          "time": "20210822 11:51"
        },
        {
          "id": "MDRW2100007905.1.12",
          "form": "선물도 주고 적극적으로 권하기도 하고요ㅎㅎ",
          "original_form": "선물도 주고 적극적으로 권하기도 하고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:52"
        },
        {
          "id": "MDRW2100007905.1.13",
          "form": "달란트 최고요",
          "original_form": "달란트 최고요",
          "speaker_id": "1",
          "time": "20210822 11:53"
        },
        {
          "id": "MDRW2100007905.1.14",
          "form": "달란트라니 교회 한두번 가보신 게 아닌가봐요",
          "original_form": "달란트라니 교회 한두번 가보신 게 아닌가봐요",
          "speaker_id": "2",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007905.1.15",
          "form": "저도 예전에 따라갔을 때 달란트로 막 소꿉놀이하는 기분이었어요ㅎ",
          "original_form": "저도 예전에 따라갔을 때 달란트로 막 소꿉놀이하는 기분이었어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007905.1.16",
          "form": "그걸로 막 사먹어요",
          "original_form": "그걸로 막 사먹어요",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007905.1.17",
          "form": "오오 생각보다 달란트가 쓰임새가 있네요",
          "original_form": "오오 생각보다 달란트가 쓰임새가 있네요",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007905.1.18",
          "form": "예전엔 달란트가 그냥 가짜돈이구나 했는데",
          "original_form": "예전엔 달란트가 그냥 가짜돈이구나 했는데",
          "speaker_id": "2",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007905.1.19",
          "form": "네 적힌 금액이 다라서 비교하고 했는데",
          "original_form": "네 적힌 금액이 다라서 비교하고 했는데",
          "speaker_id": "1",
          "time": "20210822 12:20"
        }
      ]
