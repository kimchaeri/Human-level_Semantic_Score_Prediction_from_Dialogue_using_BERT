      "utterance": [
        {
          "id": "MDRW2100004194.1.1",
          "form": "이건 내 예전 분야라서 잘 모르겠는데",
          "original_form": "이건 내 예전 분야라서 잘 모르겠는데",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004194.1.2",
          "form": "나도 예전 분야인데",
          "original_form": "나도 예전 분야인데",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004194.1.3",
          "form": "아 나 얼마전에",
          "original_form": "아 나 얼마전에",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004194.1.4",
          "form": "인피니트 앨범 다 팔았다...",
          "original_form": "인피니트 앨범 다 팔았다...",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004194.1.5",
          "form": "거의 90프로 깎음ㅜㅜ",
          "original_form": "거의 90프로 깎음ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004194.1.6",
          "form": "나는 엑소 앨범 아직 있음... 나도 안 팔릴 듯",
          "original_form": "나는 엑소 앨범 아직 있음... 나도 안 팔릴 듯",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004194.1.7",
          "form": "앨범 진짜 헐값이야 진짜 투자용은 아닌듯",
          "original_form": "앨범 진짜 헐값이야 진짜 투자용은 아닌듯",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004194.1.8",
          "form": "맞아 앨범 갖고있는 사람이 너무 많아서 희소성도 떨어지고",
          "original_form": "맞아 앨범 갖고있는 사람이 너무 많아서 희소성도 떨어지고",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004194.1.9",
          "form": "초동 때문에 필요 이상으로 많이 사니까",
          "original_form": "초동 때문에 필요 이상으로 많이 사니까",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004194.1.10",
          "form": "나 그래서 얼마전에 도경수 솔로앨범 나왔는데 그것도 몇장 안 샀어",
          "original_form": "나 그래서 얼마전에 도경수 솔로앨범 나왔는데 그것도 몇장 안 샀어",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004194.1.11",
          "form": "'몇장 안 샀어'",
          "original_form": "'몇장 안 샀어'",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004194.1.12",
          "form": "2장?",
          "original_form": "2장?",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004194.1.13",
          "form": "네장일 걸?",
          "original_form": "네장일 걸?",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004194.1.14",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004194.1.15",
          "form": "엑소 좋아할 때에 비하면 많이 안 산 거 같은데",
          "original_form": "엑소 좋아할 때에 비하면 많이 안 산 거 같은데",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004194.1.16",
          "form": "진짜 쓰리디는",
          "original_form": "진짜 쓰리디는",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004194.1.17",
          "form": "왤케 돈이 많이 드냐",
          "original_form": "왤케 돈이 많이 드냐",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004194.1.18",
          "form": "투디도 돈이 안 드는 건 아니지만",
          "original_form": "투디도 돈이 안 드는 건 아니지만",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004194.1.19",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004194.1.20",
          "form": "나 중딩 때 썼던 돈이랑 비교하면",
          "original_form": "나 중딩 때 썼던 돈이랑 비교하면",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004194.1.21",
          "form": "돈 안 쓰면 눈치 주는 분위기도 한몫 하는 것 같음",
          "original_form": "돈 안 쓰면 눈치 주는 분위기도 한몫 하는 것 같음",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004194.1.22",
          "form": "인피니트가 훨씬 많이 씀",
          "original_form": "인피니트가 훨씬 많이 씀",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004194.1.23",
          "form": "앨범 그게 뭐라고..",
          "original_form": "앨범 그게 뭐라고..",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004194.1.24",
          "form": "맞아 앨범 그게 뭐라고... 초동도 그게 뭐라고...",
          "original_form": "맞아 앨범 그게 뭐라고... 초동도 그게 뭐라고...",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004194.1.25",
          "form": "그리고 막 특전이니 뭐니하면서",
          "original_form": "그리고 막 특전이니 뭐니하면서",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004194.1.26",
          "form": "콘서트 마다 팔아 제끼니까",
          "original_form": "콘서트 마다 팔아 제끼니까",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004194.1.27",
          "form": "소속사에서도 굿즈.. 쓸데없는 거 계속 내고...",
          "original_form": "소속사에서도 굿즈.. 쓸데없는 거 계속 내고...",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004194.1.28",
          "form": "예쁘지도 않고 실용성도 없는 굿즈들...",
          "original_form": "예쁘지도 않고 실용성도 없는 굿즈들...",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004194.1.29",
          "form": "인피니트 팔면서 느낀 거지만",
          "original_form": "인피니트 팔면서 느낀 거지만",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004194.1.30",
          "form": "이제.. 굿즈 욕심 버림",
          "original_form": "이제.. 굿즈 욕심 버림",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004194.1.31",
          "form": "ㅋㅋㅋㅋㅋ 나도 이제 물욕 조금씩 버리는 중...",
          "original_form": "ㅋㅋㅋㅋㅋ 나도 이제 물욕 조금씩 버리는 중...",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004194.1.32",
          "form": "그 돈으로 할 수 있는 게 너무 많다",
          "original_form": "그 돈으로 할 수 있는 게 너무 많다",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004194.1.33",
          "form": "최종목표 : 무소유",
          "original_form": "최종목표 : 무소유",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004194.1.34",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004194.1.35",
          "form": "진짜.. 굿즈 안 산다",
          "original_form": "진짜.. 굿즈 안 산다",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004194.1.36",
          "form": "콘서트는 그래도 비싸지만 경험이 남는데",
          "original_form": "콘서트는 그래도 비싸지만 경험이 남는데",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004194.1.37",
          "form": "앨범과 굿즈는... 다시 팔려면 스트레스만 남음",
          "original_form": "앨범과 굿즈는... 다시 팔려면 스트레스만 남음",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004194.1.38",
          "form": "아무도 사지도 않는 짐덩어리",
          "original_form": "아무도 사지도 않는 짐덩어리",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004194.1.39",
          "form": "나 그래서 엑소 포카도 아직 집에 있음...",
          "original_form": "나 그래서 엑소 포카도 아직 집에 있음...",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004194.1.40",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004194.1.41",
          "form": "결정적으로",
          "original_form": "결정적으로",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004194.1.42",
          "form": "언제 사고를 칠지 모름",
          "original_form": "언제 사고를 칠지 모름",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004194.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그게 맞다...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그게 맞다...",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004194.1.44",
          "form": "사고 치면 굿즈 시세도 엄청 떨어지는데",
          "original_form": "사고 치면 굿즈 시세도 엄청 떨어지는데",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004194.1.45",
          "form": "절대 안 팔리고",
          "original_form": "절대 안 팔리고",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004194.1.46",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004194.1.47",
          "form": "나 인피니트h 앨범만 안 팔림",
          "original_form": "나 인피니트h 앨범만 안 팔림",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004194.1.48",
          "form": "사고친 것도 아니고",
          "original_form": "사고친 것도 아니고",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004194.1.49",
          "form": "걍 탈퇴한건데..",
          "original_form": "걍 탈퇴한건데..",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004194.1.50",
          "form": "근데 우리쪽 사고친 애들은 왜 아직 팬이 있지...",
          "original_form": "근데 우리쪽 사고친 애들은 왜 아직 팬이 있지...",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004194.1.51",
          "form": "...대디?",
          "original_form": "...대디?",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004194.1.52",
          "form": "걔랑... 한명 더...",
          "original_form": "걔랑... 한명 더...",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004194.1.53",
          "form": "누구지...",
          "original_form": "누구지...",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004194.1.54",
          "form": "박...열다리",
          "original_form": "박...열다리",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004194.1.55",
          "form": "아 맞네...",
          "original_form": "아 맞네...",
          "speaker_id": "2",
          "time": "20210821 15:38"
        }
      ]
