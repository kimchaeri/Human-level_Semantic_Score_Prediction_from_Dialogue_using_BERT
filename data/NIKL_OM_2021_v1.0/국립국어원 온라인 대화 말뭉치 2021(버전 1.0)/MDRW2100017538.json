        },
        {
          "id": "MDRW2100017538.1.2",
          "form": "종교 있어요?",
          "original_form": "종교 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017538.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017538.1.4",
          "form": "저는 무굔데 가족이 불교에요",
          "original_form": "저는 무굔데 가족이 불교에요",
          "speaker_id": "2",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017538.1.5",
          "form": "아~ 불교에 대해서 좀 아시면 설명해줄 수 있어요?",
          "original_form": "아~ 불교에 대해서 좀 아시면 설명해줄 수 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017538.1.6",
          "form": "사실 전 절에 가본적도 없어서요 ㅎㅎ",
          "original_form": "사실 전 절에 가본적도 없어서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017538.1.7",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017538.1.8",
          "form": "예전에 기독교 믿었는데 요새는 무교에요 ㅎㅎ",
          "original_form": "예전에 기독교 믿었는데 요새는 무교에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017538.1.9",
          "form": "기독교에 정이 떨어졌어요",
          "original_form": "기독교에 정이 떨어졌어요",
          "speaker_id": "1",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017538.1.10",
          "form": "아 ㅠㅠ 그러시구나",
          "original_form": "아 ㅠㅠ 그러시구나",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017538.1.11",
          "form": "무교가 제일 속은 편한거같아요 ㅋㅋㅋ",
          "original_form": "무교가 제일 속은 편한거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017538.1.12",
          "form": "그쵸~ 그럼 한번도 종교 가져본 적 없어요?",
          "original_form": "그쵸~ 그럼 한번도 종교 가져본 적 없어요?",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017538.1.13",
          "form": "네 없어요 ㅋㅋㅋ 한번도요",
          "original_form": "네 없어요 ㅋㅋㅋ 한번도요",
          "speaker_id": "2",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017538.1.14",
          "form": "어렸을때 친구가 교회 데리고가서 먹을거준 기억은 있는거같아요 ㅋㅋㅋ",
          "original_form": "어렸을때 친구가 교회 데리고가서 먹을거준 기억은 있는거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017538.1.15",
          "form": "ㅋㅋㅋ 저도 그때 기독교 믿었어요...",
          "original_form": "ㅋㅋㅋ 저도 그때 기독교 믿었어요...",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017538.1.16",
          "form": "그때 순수해서 그런지 진짜 하나님 있다고 믿었어요",
          "original_form": "그때 순수해서 그런지 진짜 하나님 있다고 믿었어요",
          "speaker_id": "1",
          "time": "20210826 20:04"
        },
        {
          "id": "MDRW2100017538.1.17",
          "form": "ㅋㅋㅋ 어릴때 특히 기독교친구들이 젤 많은거 같더라구요",
          "original_form": "ㅋㅋㅋ 어릴때 특히 기독교친구들이 젤 많은거 같더라구요",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017538.1.18",
          "form": "근데 중장년들도 기독교 많이 있는 거 보면 조금 신기해요",
          "original_form": "근데 중장년들도 기독교 많이 있는 거 보면 조금 신기해요",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017538.1.19",
          "form": "진짜 있다고 믿는걸까요?",
          "original_form": "진짜 있다고 믿는걸까요?",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017538.1.20",
          "form": "음 그렇지않을까요..??",
          "original_form": "음 그렇지않을까요..??",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017538.1.21",
          "form": "안 믿어봐서 그 마음을 모르겠어요",
          "original_form": "안 믿어봐서 그 마음을 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017538.1.22",
          "form": "며칠 전부터 동료들이 자꾸 저 포교하더라구요 ㅠㅠ",
          "original_form": "며칠 전부터 동료들이 자꾸 저 포교하더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017538.1.23",
          "form": "하나님의 믿음이니 사랑이니.. 휴",
          "original_form": "하나님의 믿음이니 사랑이니.. 휴",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017538.1.24",
          "form": "헉 동료들이 그러다니 ㅠㅠ",
          "original_form": "헉 동료들이 그러다니 ㅠㅠ",
