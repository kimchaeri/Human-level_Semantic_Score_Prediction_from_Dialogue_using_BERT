      "utterance": [
        {
          "id": "MDRW2100034499.1.1",
          "form": "안녕하세ㄹ요",
          "original_form": "안녕하세ㄹ요",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100034499.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100034499.1.3",
          "form": "점심은 맛잇게 드셧어요?",
          "original_form": "점심은 맛잇게 드셧어요?",
          "speaker_id": "1",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100034499.1.4",
          "form": "네 간단하게 먹었어요!",
          "original_form": "네 간단하게 먹었어요!",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034499.1.5",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034499.1.6",
          "form": "저는 오늘은 인스턴트 쌀국수 먹었어요 ㅎㅎ",
          "original_form": "저는 오늘은 인스턴트 쌀국수 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100034499.1.7",
          "form": "인스턴트 쌀국수에도 혹시 고수가 들어가나요?",
          "original_form": "인스턴트 쌀국수에도 혹시 고수가 들어가나요?",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100034499.1.8",
          "form": "들어가는 제품도있고 안들어가는제품도 잇죠 ㅎㅎ",
          "original_form": "들어가는 제품도있고 안들어가는제품도 잇죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100034499.1.9",
          "form": "안 들어가는 제품을 도전해봐야겠네요 ㅎㅎ",
          "original_form": "안 들어가는 제품을 도전해봐야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100034499.1.10",
          "form": "아직 무서워서 고수를 안 먹어봤는데",
          "original_form": "아직 무서워서 고수를 안 먹어봤는데",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034499.1.11",
          "form": "안전하게 먹어야겠어요 ㅎㅎ",
          "original_form": "안전하게 먹어야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034499.1.12",
          "form": "아 무서우시군여 ㅜㅜ 그러면 이국적인 요리는 피하셔야겟네요. 은근 비누향같은게 나더라고요",
          "original_form": "아 무서우시군여 ㅜㅜ 그러면 이국적인 요리는 피하셔야겟네요. 은근 비누향같은게 나더라고요",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100034499.1.13",
          "form": "name1님 혹시 오이 잘 드시나요?",
          "original_form": "&name1&님 혹시 오이 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100034499.1.14",
          "form": "저는 이젠 잘 먹긴해요 ㅎㅎㅎ",
          "original_form": "저는 이젠 잘 먹긴해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100034499.1.15",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034499.1.16",
          "form": "저는 잘 못 먹어요ㅠ",
          "original_form": "저는 잘 못 먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100034499.1.17",
          "form": "오이 잘 먹으면 고수도 잘 먹는다고 들어서 여쭤봤어요 ㅎㅎ",
          "original_form": "오이 잘 먹으면 고수도 잘 먹는다고 들어서 여쭤봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100034499.1.18",
          "form": "오 그렇균요.. 그것도 아니더라고요 ㅎㅎㅎㅎ\r\n제동생이 그래요 ㅋㅋ",
          "original_form": "오 그렇균요.. 그것도 아니더라고요 ㅎㅎㅎㅎ\r\n제동생이 그래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034499.1.19",
          "form": "아 어렵다... 그냥 다음에 작정하고 고수를 한 번 도전해봐야겠어요",
          "original_form": "아 어렵다... 그냥 다음에 작정하고 고수를 한 번 도전해봐야겠어요",
