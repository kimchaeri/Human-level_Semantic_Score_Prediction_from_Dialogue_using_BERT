      "utterance": [
        {
          "id": "MDRW2100007925.1.1",
          "form": "요즘 날씨가 많이 춥죠?",
          "original_form": "요즘 날씨가 많이 춥죠?",
          "speaker_id": "1",
          "time": "20210822 11:53"
        },
        {
          "id": "MDRW2100007925.1.2",
          "form": "제 마음이 얼었던게 녹을거같은데요?",
          "original_form": "제 마음이 얼었던게 녹을거같은데요?",
          "speaker_id": "2",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007925.1.3",
          "form": "아직까지는 반팔입고 다녀야 해서 춥다고 못느끼네요",
          "original_form": "아직까지는 반팔입고 다녀야 해서 춥다고 못느끼네요",
          "speaker_id": "2",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007925.1.4",
          "form": "언제쯤 긴팔 입을 생각이세요?",
          "original_form": "언제쯤 긴팔 입을 생각이세요?",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007925.1.5",
          "form": "긴팔은 안입고",
          "original_form": "긴팔은 안입고",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007925.1.6",
          "form": "겉옷 들고 다니고 있어요",
          "original_form": "겉옷 들고 다니고 있어요",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007925.1.7",
          "form": "지금은 겉옷 들고 다닐정도는 아닌거같는데 밤낮으로 차이도 없고",
          "original_form": "지금은 겉옷 들고 다닐정도는 아닌거같는데 밤낮으로 차이도 없고",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007925.1.8",
          "form": "밤에도 더워요.. 9월은 되야 서늘해질거 같는데",
          "original_form": "밤에도 더워요.. 9월은 되야 서늘해질거 같는데",
          "speaker_id": "2",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007925.1.9",
          "form": "저희 동네는 오래 날씨가 흐려서",
          "original_form": "저희 동네는 오래 날씨가 흐려서",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007925.1.10",
          "form": "많이 추워요",
          "original_form": "많이 추워요",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007925.1.11",
          "form": "지금부터 춥다고 느끼면 올해 겨울 날씨 장난 아니겠네요",
          "original_form": "지금부터 춥다고 느끼면 올해 겨울 날씨 장난 아니겠네요",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007925.1.12",
          "form": "거긴 춥고 덥고의 날씨가 어떤가요?",
          "original_form": "거긴 춥고 덥고의 날씨가 어떤가요?",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007925.1.13",
          "form": "기온차가 많이 심해서",
          "original_form": "기온차가 많이 심해서",
          "speaker_id": "1",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007925.1.14",
          "form": "극과 극을 찍고 있어요",
          "original_form": "극과 극을 찍고 있어요",
          "speaker_id": "1",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007925.1.15",
          "form": "그럼 밤낮 기온차가 심하겠네요 저녁에 추워서 감기 조심해야 겠어요",
          "original_form": "그럼 밤낮 기온차가 심하겠네요 저녁에 추워서 감기 조심해야 겠어요",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007925.1.16",
          "form": "그렇다면 그쪽은 어떤 계절을 좋아해요?",
          "original_form": "그렇다면 그쪽은 어떤 계절을 좋아해요?",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007925.1.17",
          "form": "저는 겨울 좋아해요",
          "original_form": "저는 겨울 좋아해요",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007925.1.18",
          "form": "이제 겨울 다가오고 여름은 끝나가고 좋은거 아닌가요?",
          "original_form": "이제 겨울 다가오고 여름은 끝나가고 좋은거 아닌가요?",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007925.1.19",
          "form": "저도 여름보다는 겨울이 설레고 추워도 기분이 좋더라고요",
          "original_form": "저도 여름보다는 겨울이 설레고 추워도 기분이 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007925.1.20",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007925.1.21",
          "form": "여름이 끝나가서 좋긴 하지만 환절기는 힘들어요",
          "original_form": "여름이 끝나가서 좋긴 하지만 환절기는 힘들어요",
          "speaker_id": "1",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007925.1.22",
          "form": "환절기때 건강관리 잘하셔야 해요.. 갑자기 몸이 아파지기 시작해요 옷도 따뜻하게 입어야하고",
          "original_form": "환절기때 건강관리 잘하셔야 해요.. 갑자기 몸이 아파지기 시작해요 옷도 따뜻하게 입어야하고",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007925.1.23",
          "form": "오늘 날씨는 어때요?",
          "original_form": "오늘 날씨는 어때요?",
          "speaker_id": "2",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007925.1.24",
          "form": "아직 흐려요 비는 안내리고",
          "original_form": "아직 흐려요 비는 안내리고",
          "speaker_id": "1",
          "time": "20210822 12:35"
        }
      ]
    }
