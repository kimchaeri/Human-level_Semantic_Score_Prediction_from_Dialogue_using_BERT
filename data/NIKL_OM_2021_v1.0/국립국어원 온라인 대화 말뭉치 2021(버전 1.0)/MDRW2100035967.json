      "utterance": [
        {
          "id": "MDRW2100035967.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100035967.1.2",
          "form": "다녀오신 여행지는 어디어디 있으세용??",
          "original_form": "다녀오신 여행지는 어디어디 있으세용??",
          "speaker_id": "1",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100035967.1.3",
          "form": "3월에 제주도 당일치기 5월에 변산반도 1박 짧게 짧게 다녀왔어요!!",
          "original_form": "3월에 제주도 당일치기 5월에 변산반도 1박 짧게 짧게 다녀왔어요!!",
          "speaker_id": "2",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035967.1.4",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035967.1.5",
          "form": "여름휴가 다녀오셨나요?!",
          "original_form": "여름휴가 다녀오셨나요?!",
          "speaker_id": "2",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035967.1.6",
          "form": "그래도 많이 다니신 편이네용!",
          "original_form": "그래도 많이 다니신 편이네용!",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035967.1.7",
          "form": "아니용ㅠㅠ 집콕했어요ㅠㅠ",
          "original_form": "아니용ㅠㅠ 집콕했어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035967.1.8",
          "form": "코로나라 가기 힘들죠 ㅠㅠ 저도 친구들이 가자구해서 갔는데 ㅋㅋㅋㅋ 제안없었으면 집콕이었을거에요 집순이거든요 ㅋㅋㅋ",
          "original_form": "코로나라 가기 힘들죠 ㅠㅠ 저도 친구들이 가자구해서 갔는데 ㅋㅋㅋㅋ 제안없었으면 집콕이었을거에요 집순이거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100035967.1.9",
          "form": "앗 저도 집순이라ㅋㅋㅋ 친구분들 덕분에",
          "original_form": "앗 저도 집순이라ㅋㅋㅋ 친구분들 덕분에",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100035967.1.10",
          "form": "좋은 여행도 하시고",
          "original_form": "좋은 여행도 하시고",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100035967.1.11",
          "form": "추억도 쌓으셨겠네용",
          "original_form": "추억도 쌓으셨겠네용",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100035967.1.12",
          "form": "넹 ㅋㅋㅋ 회사 스트레스속에서 잠깐의 기분전환 ㅠㅠ",
          "original_form": "넹 ㅋㅋㅋ 회사 스트레스속에서 잠깐의 기분전환 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035967.1.13",
          "form": "저 변산반도는 첨가봤는데 좋더라구요",
          "original_form": "저 변산반도는 첨가봤는데 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035967.1.14",
          "form": "변산반도가 어디에용???",
          "original_form": "변산반도가 어디에용???",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035967.1.15",
          "form": "서해라서 노을이 진짜 이뻤어요 아직도 잊지 못해...",
          "original_form": "서해라서 노을이 진짜 이뻤어요 아직도 잊지 못해...",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035967.1.16",
          "form": "남해쪽인가요?",
          "original_form": "남해쪽인가요?",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035967.1.17",
          "form": "아하 서해!",
          "original_form": "아하 서해!",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100035967.1.18",
          "form": "넹넹!! 서해 ㅋㅋㅋ저도 첨가봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "넹넹!! 서해 ㅋㅋㅋ저도 첨가봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035967.1.19",
          "form": "서해로는 생각해보니까 여행 안가본것같은데",
          "original_form": "서해로는 생각해보니까 여행 안가본것같은데",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035967.1.20",
          "form": "노을이라니",
          "original_form": "노을이라니",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035967.1.21",
          "form": "넘 멋져요....",
          "original_form": "넘 멋져요....",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035967.1.22",
          "form": "저는 동해을 안가봤어요!! 부산을 가봤는데 강릉 속초 이쪽 안가봐서 가보고싶어요 ㅠㅠ",
          "original_form": "저는 동해을 안가봤어요!! 부산을 가봤는데 강릉 속초 이쪽 안가봐서 가보고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100035967.1.23",
          "form": "그렇게나 이쁘다던데",
          "original_form": "그렇게나 이쁘다던데",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100035967.1.24",
          "form": "속초 맛있는거 진짜 많구",
          "original_form": "속초 맛있는거 진짜 많구",
          "speaker_id": "1",
          "time": "20210830 13:03"
        }
      ]
    }
  ]
