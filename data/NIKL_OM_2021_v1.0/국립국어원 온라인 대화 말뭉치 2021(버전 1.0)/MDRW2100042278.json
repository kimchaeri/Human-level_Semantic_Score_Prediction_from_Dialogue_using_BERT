      "utterance": [
        {
          "id": "MDRW2100042278.1.1",
          "form": "도착한 날",
          "original_form": "도착한 날",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042278.1.2",
          "form": "안압지인가 거기 구경할까",
          "original_form": "안압지인가 거기 구경할까",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042278.1.3",
          "form": "동궁과 월지 거기",
          "original_form": "동궁과 월지 거기",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042278.1.4",
          "form": "야경보자",
          "original_form": "야경보자",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042278.1.5",
          "form": "안압지는 많이들어봤는데",
          "original_form": "안압지는 많이들어봤는데",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042278.1.6",
          "form": "동궁 월지는 뭐야?",
          "original_form": "동궁 월지는 뭐야?",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042278.1.7",
          "form": "아아아아ㅏ",
          "original_form": "아아아아ㅏ",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042278.1.8",
          "form": "저기 야경",
          "original_form": "저기 야경",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042278.1.9",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042278.1.10",
          "form": "사진으로 ㄱ ㅐ많이봄ㅋㅋㅋㅋㅋ",
          "original_form": "사진으로 ㄱ ㅐ많이봄ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042278.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042278.1.12",
          "form": "하고 황리단길 가고",
          "original_form": "하고 황리단길 가고",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042278.1.13",
          "form": "하 벌써 설렌다",
          "original_form": "하 벌써 설렌다",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042278.1.14",
          "form": "황리단길은 뭐야",
          "original_form": "황리단길은 뭐야",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042278.1.15",
          "form": "에어비앤비에도 막 다 황리단길에서 몇분 이러던데",
          "original_form": "에어비앤비에도 막 다 황리단길에서 몇분 이러던데",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042278.1.16",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042278.1.17",
          "form": "무슨느낌이라 해야하지",
          "original_form": "무슨느낌이라 해야하지",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042278.1.18",
          "form": "서면 느낌인데",
          "original_form": "서면 느낌인데",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042278.1.19",
          "form": "가게들이 다 한옥?",
          "original_form": "가게들이 다 한옥?",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042278.1.20",
          "form": "이라고 보면 돼",
          "original_form": "이라고 보면 돼",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042278.1.21",
          "form": "ㅇㅎㅇㅎ",
          "original_form": "ㅇㅎㅇㅎ",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042278.1.22",
          "form": "가서 너 한복 입으래?",
          "original_form": "가서 너 한복 입으래?",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042278.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042278.1.24",
          "form": "ㄴㄴㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄴㄴㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042278.1.25",
          "form": "난 안입을껀데",
          "original_form": "난 안입을껀데",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042278.1.26",
          "form": "근데 동궁과월지 리뷰보는데",
          "original_form": "근데 동궁과월지 리뷰보는데",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042278.1.27",
          "form": "9월6일 리뷰로 누가",
          "original_form": "9월6일 리뷰로 누가",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042278.1.28",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042278.1.29",
