      "utterance": [
        {
          "id": "MDRW2100026485.1.1",
          "form": "음원이라 저는 ost 좋아해요!",
          "original_form": "음원이라 저는 ost 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026485.1.2",
          "form": "영화요? 아님 드라마쪽인가요??",
          "original_form": "영화요? 아님 드라마쪽인가요??",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026485.1.3",
          "form": "둘다 좋아요~ 저 뉴에이지도 좋아하거든요",
          "original_form": "둘다 좋아요~ 저 뉴에이지도 좋아하거든요",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026485.1.4",
          "form": "사연있는 느낌 좋아요 ㅋㅋㅋ",
          "original_form": "사연있는 느낌 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026485.1.5",
          "form": "사연있는 느낌 ㄹㅇ이네요 ㅋㅋㅋ",
          "original_form": "사연있는 느낌 ㄹㅇ이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026485.1.6",
          "form": "저 아련+벅차오름 콜라보 좋아해요 ㅋㅋㅋ 딱 한국식 기승전결 음악",
          "original_form": "저 아련+벅차오름 콜라보 좋아해요 ㅋㅋㅋ 딱 한국식 기승전결 음악",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026485.1.7",
          "form": "뉴에이지 음악은 뭘 가장 좋아하세요??",
          "original_form": "뉴에이지 음악은 뭘 가장 좋아하세요??",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026485.1.8",
          "form": "불꽃심장님 노래요!!",
          "original_form": "불꽃심장님 노래요!!",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026485.1.9",
          "form": "오 그러네요 한국식 기승전결 ㅇ0ㅇ",
          "original_form": "오 그러네요 한국식 기승전결 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026485.1.10",
          "form": "환각의 춤 좋아해요!",
          "original_form": "환각의 춤 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026485.1.11",
          "form": "완전 딥하고 빠른 음악이에요",
          "original_form": "완전 딥하고 빠른 음악이에요",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026485.1.12",
          "form": "진짜 추천이용",
          "original_form": "진짜 추천이용",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026485.1.13",
          "form": "아 안그래도 문외한이라 방금 검색해봤는데 환각의 춤이 나오더라구요",
          "original_form": "아 안그래도 문외한이라 방금 검색해봤는데 환각의 춤이 나오더라구요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026485.1.14",
          "form": "자주 들으시나요??",
          "original_form": "자주 들으시나요??",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026485.1.15",
          "form": "네! 듣고 싶을 때가 딱 와요!",
          "original_form": "네! 듣고 싶을 때가 딱 와요!",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026485.1.16",
          "form": "음원 다운도 받아놨어요 크크",
          "original_form": "음원 다운도 받아놨어요 크크",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026485.1.17",
          "form": "오 보통 아침에 많으 들으시나요 기상송으로??",
          "original_form": "오 보통 아침에 많으 들으시나요 기상송으로??",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026485.1.18",
          "form": "아뇨!! 낮에 ㅋㅋㅋ 잠깰때요!",
          "original_form": "아뇨!! 낮에 ㅋㅋㅋ 잠깰때요!",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026485.1.19",
          "form": "밥먹고 좀 졸리려 할때~? 들음 좋아요",
          "original_form": "밥먹고 좀 졸리려 할때~? 들음 좋아요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026485.1.20",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026485.1.21",
          "form": "정말 뒤로갈수록 파워풀해지니까 주의 환기에 좋을 것 같아요",
          "original_form": "정말 뒤로갈수록 파워풀해지니까 주의 환기에 좋을 것 같아요",
