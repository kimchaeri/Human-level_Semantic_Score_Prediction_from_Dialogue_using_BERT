      "utterance": [
        {
          "id": "MDRW2100026434.1.1",
          "form": "프라하는 몇월에갔어요?",
          "original_form": "프라하는 몇월에갔어요?",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026434.1.2",
          "form": "저는4월초에갔는데 좀 쌀쌀했거든요.",
          "original_form": "저는4월초에갔는데 좀 쌀쌀했거든요.",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026434.1.3",
          "form": "저는 그때가 10월이었나?",
          "original_form": "저는 그때가 10월이었나?",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026434.1.4",
          "form": "날씨좋을때 다시 가보고싶어요",
          "original_form": "날씨좋을때 다시 가보고싶어요",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026434.1.5",
          "form": "연휴때 갔던거 같아요",
          "original_form": "연휴때 갔던거 같아요",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026434.1.6",
          "form": "저도 진짜 다시 가보고 싶어요 프라하",
          "original_form": "저도 진짜 다시 가보고 싶어요 프라하",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026434.1.7",
          "form": "사진니간 코트입고찍은사진밖에 없더라구요.",
          "original_form": "사진니간 코트입고찍은사진밖에 없더라구요.",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.8",
          "form": "그냥 그날의 모든게 느낌이 좋았어요 건물들도 멋지구 ㅋㅋ",
          "original_form": "그냥 그날의 모든게 느낌이 좋았어요 건물들도 멋지구 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.9",
          "form": "외투는한개 가져가서ㅠ",
          "original_form": "외투는한개 가져가서ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.10",
          "form": "맞아요.  그 느낌!",
          "original_form": "맞아요.  그 느낌!",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.11",
          "form": "저도 사진 맨날 봐요 ㅋㅋ 여행가서 찍은거",
          "original_form": "저도 사진 맨날 봐요 ㅋㅋ 여행가서 찍은거",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.12",
          "form": "어디를걸어도 너무 이쁘고.",
          "original_form": "어디를걸어도 너무 이쁘고.",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.13",
          "form": "까를교!!",
          "original_form": "까를교!!",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.14",
          "form": "걸었죠?",
          "original_form": "걸었죠?",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.15",
          "form": "어제도 여행사진 찍은거 보고 엄마한테도 또 보내고 ㅋㅋ",
          "original_form": "어제도 여행사진 찍은거 보고 엄마한테도 또 보내고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.16",
          "form": "네! 걸었어용",
          "original_form": "네! 걸었어용",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026434.1.17",
          "form": "아 너무 다들 여유로워 보여서",
          "original_form": "아 너무 다들 여유로워 보여서",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026434.1.18",
          "form": "부러웠어요ㅠㅠ",
          "original_form": "부러웠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026434.1.19",
          "form": "ㅋㅋ 저도오늘 프라하 사진 정리했거든요.",
          "original_form": "ㅋㅋ 저도오늘 프라하 사진 정리했거든요.",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026434.1.20",
          "form": "오 ㅋㅋㅋ 이런 우연이!ㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋ 이런 우연이!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026434.1.21",
          "form": "스냅스에 앨범 만들고있어서ㅋㅋ",
          "original_form": "스냅스에 앨범 만들고있어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026434.1.22",
          "form": "우왕ㅋㅋ 정리를 다 하시는 군요",
          "original_form": "우왕ㅋㅋ 정리를 다 하시는 군요",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026434.1.23",
          "form": "혹시 거기서 비눗방울 하시는분 봤어요?",
          "original_form": "혹시 거기서 비눗방울 하시는분 봤어요?",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026434.1.24",
          "form": "거기서찍은사진이 이쁘더라구요",
          "original_form": "거기서찍은사진이 이쁘더라구요",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026434.1.25",
          "form": "헉 그래요? 그건 못 찍은거 같아요ㅠㅠ",
          "original_form": "헉 그래요? 그건 못 찍은거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026434.1.26",
          "form": "역시 다시 가야겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "역시 다시 가야겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026434.1.27",
          "form": "ㅋㅋ 꼭 다시 가요",
          "original_form": "ㅋㅋ 꼭 다시 가요",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026434.1.28",
          "form": "그리고 그거봤어요??  막 공중에 떠있는사람들.",
          "original_form": "그리고 그거봤어요??  막 공중에 떠있는사람들.",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026434.1.29",
          "form": "지팡이같은거한개 밖에없는데",
          "original_form": "지팡이같은거한개 밖에없는데",
          "speaker_id": "2",
          "time": "20210901 22:17"
        }
      ]
