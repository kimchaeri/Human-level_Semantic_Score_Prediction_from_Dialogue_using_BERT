        {
          "id": "MDRW2100017820.1.1",
          "form": "요즘 어떤 운동하세요",
          "original_form": "요즘 어떤 운동하세요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017820.1.2",
          "form": "숨쉬기운동이욥",
          "original_form": "숨쉬기운동이욥",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017820.1.3",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017820.1.4",
          "form": "진짜 해야하는데 큰일임다ㅜ",
          "original_form": "진짜 해야하는데 큰일임다ㅜ",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017820.1.5",
          "form": "저는 방송댄스랑 강아지랑 산책이요",
          "original_form": "저는 방송댄스랑 강아지랑 산책이요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017820.1.6",
          "form": "저녁에 선선한데 한바퀴 다녀오셔유ㅠㅠ",
          "original_form": "저녁에 선선한데 한바퀴 다녀오셔유ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017820.1.7",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.8",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.9",
          "form": "방송댄스",
          "original_form": "방송댄스",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.10",
          "form": "저는 지독한집수니라서",
          "original_form": "저는 지독한집수니라서",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.11",
          "form": "아스트로 아세요?",
          "original_form": "아스트로 아세요?",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.12",
          "form": "침대벗어나기힘들어요",
          "original_form": "침대벗어나기힘들어요",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.13",
          "form": "아스트로 원이랑 애프터 미드나잇 배웠어요",
          "original_form": "아스트로 원이랑 애프터 미드나잇 배웠어요",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.14",
          "form": "차은우?",
          "original_form": "차은우?",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.15",
          "form": "오 맞아요 차은우 그룹",
          "original_form": "오 맞아요 차은우 그룹",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.16",
          "form": "멋잇어요ㅜㅜ 우와ㅜㅜ",
          "original_form": "멋잇어요ㅜㅜ 우와ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017820.1.17",
          "form": "요즘 빠져서 그 노래로 배우고있어요 레슨 받아요",
          "original_form": "요즘 빠져서 그 노래로 배우고있어요 레슨 받아요",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017820.1.18",
          "form": "크ㅠㅠ",
