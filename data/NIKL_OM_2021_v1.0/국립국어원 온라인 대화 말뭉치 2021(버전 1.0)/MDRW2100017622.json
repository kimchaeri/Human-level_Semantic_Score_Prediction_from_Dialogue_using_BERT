      "utterance": [
        {
          "id": "MDRW2100017622.1.1",
          "form": "구입하려 벼르는 물건이 있으신가요",
          "original_form": "구입하려 벼르는 물건이 있으신가요",
          "speaker_id": "2",
          "time": "20210827 08:10"
        },
        {
          "id": "MDRW2100017622.1.2",
          "form": "저 제트플립이요 ㅠㅠ",
          "original_form": "저 제트플립이요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100017622.1.3",
          "form": "아 진짜 너무 귀엽죠ㅜ",
          "original_form": "아 진짜 너무 귀엽죠ㅜ",
          "speaker_id": "2",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100017622.1.4",
          "form": "골수 노트파인데 엄청 혹해요",
          "original_form": "골수 노트파인데 엄청 혹해요",
          "speaker_id": "2",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100017622.1.5",
          "form": "아 강경노트파이시구나!!",
          "original_form": "아 강경노트파이시구나!!",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100017622.1.6",
          "form": "플립은 뭔가 폰이아니라 악세서리같아서 더 갖고싶어요 ㅠ",
          "original_form": "플립은 뭔가 폰이아니라 악세서리같아서 더 갖고싶어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100017622.1.7",
          "form": "무슨 색이 원픽인가요",
          "original_form": "무슨 색이 원픽인가요",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100017622.1.8",
          "form": "저는 라벤더요!",
          "original_form": "저는 라벤더요!",
          "speaker_id": "1",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100017622.1.9",
          "form": "진리의 라벤더군요ㅎ",
          "original_form": "진리의 라벤더군요ㅎ",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100017622.1.10",
          "form": "저는 원래 어두운색만 써서 블랙 그린 고민했는데",
          "original_form": "저는 원래 어두운색만 써서 블랙 그린 고민했는데",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100017622.1.11",
          "form": "폰꾸 한거 보고 크림에 혹해 있어요",
          "original_form": "폰꾸 한거 보고 크림에 혹해 있어요",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100017622.1.12",
          "form": "맞아요 확실히 폰꾸하기에는 크림이 최고죠",
          "original_form": "맞아요 확실히 폰꾸하기에는 크림이 최고죠",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100017622.1.13",
          "form": "저는 실물보니 라벤더랑 생각지도않던 다크그린이 예쁘더라구요",
          "original_form": "저는 실물보니 라벤더랑 생각지도않던 다크그린이 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100017622.1.14",
          "form": "근데 폰꾸 안하면 빛바랜 흰색일까봐ㅜ",
          "original_form": "근데 폰꾸 안하면 빛바랜 흰색일까봐ㅜ",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100017622.1.15",
          "form": "다크그린과 라벤더가 실물깡패라 그러더라고요",
          "original_form": "다크그린과 라벤더가 실물깡패라 그러더라고요",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100017622.1.16",
          "form": "삼성이 이번에 칼간듯",
          "original_form": "삼성이 이번에 칼간듯",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100017622.1.17",
          "form": "앜ㅋㅋㅋ 그 크림색은 샤넬 아이보리컬러랑 거의 유사하다고하더라구요!",
          "original_form": "앜ㅋㅋㅋ 그 크림색은 샤넬 아이보리컬러랑 거의 유사하다고하더라구요!",
          "speaker_id": "1",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100017622.1.18",
          "form": "그래서 여성분들이 더 좋아한다며 ㅋㅋ",
          "original_form": "그래서 여성분들이 더 좋아한다며 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100017622.1.19",
          "form": "마쟈요 칼갈앗어요증말",
          "original_form": "마쟈요 칼갈앗어요증말",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100017622.1.20",
          "form": "이러다 정말 노트는 손절할까봐 그게 걱정이긴 한데ㅜ",
          "original_form": "이러다 정말 노트는 손절할까봐 그게 걱정이긴 한데ㅜ",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100017622.1.21",
          "form": "지금은 무슨 폰 쓰세요",
          "original_form": "지금은 무슨 폰 쓰세요",
          "speaker_id": "2",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100017622.1.22",
          "form": "지금 엘지폰써요 ㅠ 폰사업접어서 너무 슬픈 일인입니다",
          "original_form": "지금 엘지폰써요 ㅠ 폰사업접어서 너무 슬픈 일인입니다",
          "speaker_id": "1",
          "time": "20210827 12:25"
        }
      ]
    }
  ]
