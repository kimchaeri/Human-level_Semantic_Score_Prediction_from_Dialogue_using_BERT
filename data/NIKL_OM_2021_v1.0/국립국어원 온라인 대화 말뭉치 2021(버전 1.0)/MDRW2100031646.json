        {
          "id": "MDRW2100031646.1.1",
          "form": "공연 좋아해요?",
          "original_form": "공연 좋아해요?",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031646.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031646.1.3",
          "form": "어떤공연 ?",
          "original_form": "어떤공연 ?",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031646.1.4",
          "form": "딱히 가리진않는데,,,",
          "original_form": "딱히 가리진않는데,,,",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031646.1.5",
          "form": "무서운것도 봐요 ?",
          "original_form": "무서운것도 봐요 ?",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031646.1.6",
          "form": "아 무서운건안봅니다!",
          "original_form": "아 무서운건안봅니다!",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031646.1.7",
          "form": "ㅎㅎ 그렇군요",
          "original_form": "ㅎㅎ 그렇군요",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031646.1.8",
          "form": "전시회 봐요 ?",
          "original_form": "전시회 봐요 ?",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031646.1.9",
          "form": "아전시회는 볼줄을 몰라서 그런지,,ㅋㅋ즐기지않아요",
          "original_form": "아전시회는 볼줄을 몰라서 그런지,,ㅋㅋ즐기지않아요",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031646.1.10",
          "form": "아 , 저도 그래요 ㅎ",
          "original_form": "아 , 저도 그래요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031646.1.11",
          "form": "콘서트가보셨나요?!",
          "original_form": "콘서트가보셨나요?!",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031646.1.12",
          "form": "아뇨ㅡ 님은요 ?",
          "original_form": "아뇨ㅡ 님은요 ?",
          "speaker_id": "2",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031646.1.13",
          "form": "저도안가봤어요ㅠ",
          "original_form": "저도안가봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031646.1.14",
          "form": "가보고싶긴해요!",
          "original_form": "가보고싶긴해요!",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031646.1.15",
          "form": "코로나 끝나고 가보면 좋겠네요 .",
          "original_form": "코로나 끝나고 가보면 좋겠네요 .",
          "speaker_id": "2",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031646.1.16",
          "form": "맞아요 코로나ㅠ",
          "original_form": "맞아요 코로나ㅠ",
