      "utterance": [
        {
          "id": "MDRW2100022150.1.1",
          "form": "저는 요즘 사고싶은 물건들이 많아요",
          "original_form": "저는 요즘 사고싶은 물건들이 많아요",
          "speaker_id": "2",
          "time": "20210829 09:33"
        },
        {
          "id": "MDRW2100022150.1.2",
          "form": "하지만 돈이 없어요!!!",
          "original_form": "하지만 돈이 없어요!!!",
          "speaker_id": "2",
          "time": "20210829 09:33"
        },
        {
          "id": "MDRW2100022150.1.3",
          "form": "무엇이 사고싶으신가요?",
          "original_form": "무엇이 사고싶으신가요?",
          "speaker_id": "1",
          "time": "20210829 09:33"
        },
        {
          "id": "MDRW2100022150.1.4",
          "form": "일단 새 운동화도 갖고싶고",
          "original_form": "일단 새 운동화도 갖고싶고",
          "speaker_id": "2",
          "time": "20210829 09:33"
        },
        {
          "id": "MDRW2100022150.1.5",
          "form": "얼마전에 산 게 가품이었어서...",
          "original_form": "얼마전에 산 게 가품이었어서...",
          "speaker_id": "2",
          "time": "20210829 09:33"
        },
        {
          "id": "MDRW2100022150.1.6",
          "form": "바닥이 반으로 갈라졌거든요 ㅠㅠ",
          "original_form": "바닥이 반으로 갈라졌거든요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 09:33"
        },
        {
          "id": "MDRW2100022150.1.7",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "1",
          "time": "20210829 09:33"
        },
        {
          "id": "MDRW2100022150.1.8",
          "form": "혹시 나이키였나요???",
          "original_form": "혹시 나이키였나요???",
          "speaker_id": "1",
          "time": "20210829 09:33"
        },
        {
          "id": "MDRW2100022150.1.9",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210829 09:34"
        },
        {
          "id": "MDRW2100022150.1.10",
          "form": "나이키가 짝퉁이 많나요??",
          "original_form": "나이키가 짝퉁이 많나요??",
          "speaker_id": "2",
          "time": "20210829 09:34"
        },
        {
          "id": "MDRW2100022150.1.11",
          "form": "그렇다고 들었어요",
          "original_form": "그렇다고 들었어요",
          "speaker_id": "1",
          "time": "20210829 09:34"
        },
        {
          "id": "MDRW2100022150.1.12",
          "form": "저는 어제 새벽에도",
          "original_form": "저는 어제 새벽에도",
          "speaker_id": "1",
          "time": "20210829 09:34"
        },
        {
          "id": "MDRW2100022150.1.13",
          "form": "공영쇼핑 어플에 들어갔어요",
          "original_form": "공영쇼핑 어플에 들어갔어요",
          "speaker_id": "1",
          "time": "20210829 09:34"
        },
        {
          "id": "MDRW2100022150.1.14",
          "form": "새벽 쇼핑을 좋아하시나보다 ㅎㅎ",
          "original_form": "새벽 쇼핑을 좋아하시나보다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 09:34"
        },
        {
          "id": "MDRW2100022150.1.15",
          "form": "그리고 얼마 전에는",
          "original_form": "그리고 얼마 전에는",
          "speaker_id": "2",
          "time": "20210829 09:34"
        },
        {
          "id": "MDRW2100022150.1.16",
          "form": "스마트 워치도 사고싶었어요",
          "original_form": "스마트 워치도 사고싶었어요",
          "speaker_id": "2",
          "time": "20210829 09:35"
        },
        {
          "id": "MDRW2100022150.1.17",
          "form": "애플이요?",
          "original_form": "애플이요?",
          "speaker_id": "1",
          "time": "20210829 09:35"
        },
        {
          "id": "MDRW2100022150.1.18",
          "form": "갤럭시?",
          "original_form": "갤럭시?",
          "speaker_id": "1",
          "time": "20210829 09:35"
        },
        {
          "id": "MDRW2100022150.1.19",
          "form": "물론 애플이면 더 좋았겠지만",
          "original_form": "물론 애플이면 더 좋았겠지만",
          "speaker_id": "2",
          "time": "20210829 09:35"
        },
        {
          "id": "MDRW2100022150.1.20",
          "form": "제가 갤럭시를 써서 갤럭시 워치요 ㅎㅎ",
          "original_form": "제가 갤럭시를 써서 갤럭시 워치요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 09:35"
        },
        {
          "id": "MDRW2100022150.1.21",
          "form": "요즘은 갤럭시도 괜찮게 나오더라구요",
          "original_form": "요즘은 갤럭시도 괜찮게 나오더라구요",
          "speaker_id": "1",
          "time": "20210829 09:35"
        },
        {
          "id": "MDRW2100022150.1.22",
          "form": "저도 스마트 워치가 탐나더라구요",
          "original_form": "저도 스마트 워치가 탐나더라구요",
          "speaker_id": "1",
          "time": "20210829 09:35"
        },
        {
          "id": "MDRW2100022150.1.23",
          "form": "그쵸 ㅎㅎ 여남노소 있으면 다 좋을 것 같은 물건이에요",
          "original_form": "그쵸 ㅎㅎ 여남노소 있으면 다 좋을 것 같은 물건이에요",
          "speaker_id": "2",
          "time": "20210829 09:36"
        },
        {
          "id": "MDRW2100022150.1.24",
          "form": "그리고 또 뭐더라...",
          "original_form": "그리고 또 뭐더라...",
          "speaker_id": "2",
          "time": "20210829 09:36"
        },
        {
          "id": "MDRW2100022150.1.25",
          "form": "갖고싶은거 투성이에요",
          "original_form": "갖고싶은거 투성이에요",
          "speaker_id": "2",
          "time": "20210829 09:36"
        },
        {
          "id": "MDRW2100022150.1.26",
          "form": "연청바지도 사고싶었는데",
          "original_form": "연청바지도 사고싶었는데",
          "speaker_id": "2",
          "time": "20210829 09:36"
        },
        {
          "id": "MDRW2100022150.1.27",
          "form": "핏이 마음에 드는게 잘 없어요",
          "original_form": "핏이 마음에 드는게 잘 없어요",
          "speaker_id": "2",
          "time": "20210829 09:36"
        },
        {
          "id": "MDRW2100022150.1.28",
          "form": "바지는 입어보고 사야하지 않나요?",
          "original_form": "바지는 입어보고 사야하지 않나요?",
          "speaker_id": "1",
          "time": "20210829 09:37"
        },
        {
          "id": "MDRW2100022150.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 09:37"
        },
        {
          "id": "MDRW2100022150.1.30",
          "form": "친구들 보니까",
          "original_form": "친구들 보니까",
          "speaker_id": "2",
          "time": "20210829 09:37"
        },
        {
          "id": "MDRW2100022150.1.31",
          "form": "그래서 바지 길이를 다 알아놓고 있더라구요",
          "original_form": "그래서 바지 길이를 다 알아놓고 있더라구요",
          "speaker_id": "2",
          "time": "20210829 09:37"
        },
        {
          "id": "MDRW2100022150.1.32",
          "form": "인터넷 쇼핑할 때 정말 편하겠어요~",
          "original_form": "인터넷 쇼핑할 때 정말 편하겠어요~",
          "speaker_id": "1",
          "time": "20210829 09:37"
        },
        {
          "id": "MDRW2100022150.1.33",
          "form": "저는 믹서기도 사고싶어요",
          "original_form": "저는 믹서기도 사고싶어요",
          "speaker_id": "1",
          "time": "20210829 09:37"
        },
        {
          "id": "MDRW2100022150.1.34",
          "form": "아침에 과일 갈아마실 때 정말 편할거같아요",
          "original_form": "아침에 과일 갈아마실 때 정말 편할거같아요",
          "speaker_id": "1",
          "time": "20210829 09:37"
        },
        {
          "id": "MDRW2100022150.1.35",
          "form": "공감해요",
          "original_form": "공감해요",
          "speaker_id": "2",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100022150.1.36",
          "form": "무엇인가 갖고싶다는 생각이 드는 거 자체가",
          "original_form": "무엇인가 갖고싶다는 생각이 드는 거 자체가",
          "speaker_id": "2",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100022150.1.37",
          "form": "삶에 의욕이 있다는 말 같아요",
          "original_form": "삶에 의욕이 있다는 말 같아요",
          "speaker_id": "2",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100022150.1.38",
          "form": "저도 그렇게 생각해요",
          "original_form": "저도 그렇게 생각해요",
          "speaker_id": "1",
          "time": "20210829 09:38"
        }
      ]
    }
  ]
