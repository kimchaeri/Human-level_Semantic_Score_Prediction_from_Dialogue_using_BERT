      "utterance": [
        {
          "id": "MDRW2100019900.1.1",
          "form": "찾아보면",
          "original_form": "찾아보면",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.2",
          "form": "있음 찾아보셈",
          "original_form": "있음 찾아보셈",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.3",
          "form": "있겠지",
          "original_form": "있겠지",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.4",
          "form": "알았다 니도 찾아봐라",
          "original_form": "알았다 니도 찾아봐라",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.5",
          "form": "닭발먹든가",
          "original_form": "닭발먹든가",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.6",
          "form": "반띵한다잉",
          "original_form": "반띵한다잉",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.7",
          "form": "닭발 지겹다",
          "original_form": "닭발 지겹다",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.8",
          "form": "우라빠닭강정 해서 반반먹던가",
          "original_form": "우라빠닭강정 해서 반반먹던가",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.9",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.10",
          "form": "물게엄노",
          "original_form": "물게엄노",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.11",
          "form": "배송비 적은데 없으면 거기 ㄱㄱ",
          "original_form": "배송비 적은데 없으면 거기 ㄱㄱ",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.12",
          "form": "순대국밥 시켜라",
          "original_form": "순대국밥 시켜라",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.13",
          "form": "됐다 배부르다",
          "original_form": "됐다 배부르다",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.14",
          "form": "헤비하다",
          "original_form": "헤비하다",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019900.1.15",
          "form": "라이트한걸로",
          "original_form": "라이트한걸로",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019900.1.16",
          "form": "찾아서시켜라",
          "original_form": "찾아서시켜라",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019900.1.17",
          "form": "간단한 떡보낑 어떰?",
          "original_form": "간단한 떡보낑 어떰?",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019900.1.18",
          "form": "라이트한거 뭐없나",
          "original_form": "라이트한거 뭐없나",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019900.1.19",
          "form": "라이트한거 떡볶이지",
          "original_form": "라이트한거 떡볶이지",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019900.1.20",
          "form": "떡보끼 겁나 헤비하네",
          "original_form": "떡보끼 겁나 헤비하네",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019900.1.21",
          "form": "배달음식중에 살 안찌는게",
          "original_form": "배달음식중에 살 안찌는게",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019900.1.22",
          "form": "뭐가 있지",
          "original_form": "뭐가 있지",
          "speaker_id": "1",
          "time": "20210827 22:45"
        }
