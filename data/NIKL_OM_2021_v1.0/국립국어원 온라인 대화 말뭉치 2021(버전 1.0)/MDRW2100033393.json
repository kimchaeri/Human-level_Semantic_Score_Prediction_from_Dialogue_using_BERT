      "utterance": [
        {
          "id": "MDRW2100033393.1.1",
          "form": "여보 동물 뭐 키울꺼야??",
          "original_form": "여보 동물 뭐 키울꺼야??",
          "speaker_id": "1",
          "time": "20210907 03:17"
        },
        {
          "id": "MDRW2100033393.1.2",
          "form": "동물??? 집에서 안키운다매???",
          "original_form": "동물??? 집에서 안키운다매???",
          "speaker_id": "2",
          "time": "20210907 03:17"
        },
        {
          "id": "MDRW2100033393.1.3",
          "form": "아니 요즘 애들이 부쩍 나가면 동물에 관심 보이더라고..",
          "original_form": "아니 요즘 애들이 부쩍 나가면 동물에 관심 보이더라고..",
          "speaker_id": "1",
          "time": "20210907 03:17"
        },
        {
          "id": "MDRW2100033393.1.4",
          "form": "아무래도 그럴 나이니까 그렇겠지..",
          "original_form": "아무래도 그럴 나이니까 그렇겠지..",
          "speaker_id": "2",
          "time": "20210907 03:17"
        },
        {
          "id": "MDRW2100033393.1.5",
          "form": "우리도 반려동물 한 번 생각해 볼까??",
          "original_form": "우리도 반려동물 한 번 생각해 볼까??",
          "speaker_id": "1",
          "time": "20210907 03:18"
        },
        {
          "id": "MDRW2100033393.1.6",
          "form": "뭐 키우고 싶은 반려동물이라도 있나??",
          "original_form": "뭐 키우고 싶은 반려동물이라도 있나??",
          "speaker_id": "2",
          "time": "20210907 03:18"
        },
        {
          "id": "MDRW2100033393.1.7",
          "form": "아파트니까 개는 좀 그렇고...",
          "original_form": "아파트니까 개는 좀 그렇고...",
          "speaker_id": "1",
          "time": "20210907 03:19"
        },
        {
          "id": "MDRW2100033393.1.8",
          "form": "거북이나... 사슴벌레처럼 수조에서 기를 수 있는거??",
          "original_form": "거북이나... 사슴벌레처럼 수조에서 기를 수 있는거??",
          "speaker_id": "1",
          "time": "20210907 03:19"
        },
        {
          "id": "MDRW2100033393.1.9",
          "form": "개말고 뭐???",
          "original_form": "개말고 뭐???",
          "speaker_id": "2",
          "time": "20210907 03:19"
        },
        {
          "id": "MDRW2100033393.1.10",
          "form": "거북이나 사슴벌레라고!!",
          "original_form": "거북이나 사슴벌레라고!!",
          "speaker_id": "1",
          "time": "20210907 03:19"
        },
        {
          "id": "MDRW2100033393.1.11",
          "form": "거북이.... 얼마나 커지는지 알지???",
          "original_form": "거북이.... 얼마나 커지는지 알지???",
          "speaker_id": "2",
          "time": "20210907 03:20"
        },
        {
          "id": "MDRW2100033393.1.12",
          "form": "그 파는 작고 작은 거북이???",
          "original_form": "그 파는 작고 작은 거북이???",
          "speaker_id": "1",
          "time": "20210907 03:20"
        },
        {
          "id": "MDRW2100033393.1.13",
          "form": "그거 엄청 커지는 거야...",
          "original_form": "그거 엄청 커지는 거야...",
          "speaker_id": "2",
          "time": "20210907 03:20"
        },
        {
          "id": "MDRW2100033393.1.14",
          "form": "그... 그래??",
          "original_form": "그... 그래??",
          "speaker_id": "1",
          "time": "20210907 03:20"
        },
        {
          "id": "MDRW2100033393.1.15",
          "form": "함부로 동물키운다고 하지 말아줄래???",
          "original_form": "함부로 동물키운다고 하지 말아줄래???",
