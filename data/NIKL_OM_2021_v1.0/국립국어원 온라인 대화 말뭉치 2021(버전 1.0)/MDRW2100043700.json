      "utterance": [
        {
          "id": "MDRW2100043700.1.1",
          "form": "k쇼핑은 주로모사요?",
          "original_form": "k쇼핑은 주로모사요?",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043700.1.2",
          "form": "겜을 잘하셔서 ㅎㅎㅎ",
          "original_form": "겜을 잘하셔서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043700.1.3",
          "form": "쌀도 좋구요",
          "original_form": "쌀도 좋구요",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043700.1.4",
          "form": "매달 2번 구매하면 7천주는건가요?",
          "original_form": "매달 2번 구매하면 7천주는건가요?",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043700.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043700.1.6",
          "form": "이번달은 나눠서 사야겠네요",
          "original_form": "이번달은 나눠서 사야겠네요",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043700.1.7",
          "form": "이번달은 잘 계획하셔서 사세요",
          "original_form": "이번달은 잘 계획하셔서 사세요",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043700.1.8",
          "form": "참치나 꽁치 통조림",
          "original_form": "참치나 꽁치 통조림",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043700.1.9",
          "form": "저는 포인트 소멸있어서",
          "original_form": "저는 포인트 소멸있어서",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043700.1.10",
          "form": "아 소멸도 있군요",
          "original_form": "아 소멸도 있군요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043700.1.11",
          "form": "며칠전에 소액으로",
          "original_form": "며칠전에 소액으로",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043700.1.12",
          "form": "키친타올 4개짜리 샀어요",
          "original_form": "키친타올 4개짜리 샀어요",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043700.1.13",
          "form": "소액으로 털기엔 키친타올도 괜찮아요",
          "original_form": "소액으로 털기엔 키친타올도 괜찮아요",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043700.1.14",
          "form": "그것도 괜찮겠어요",
          "original_form": "그것도 괜찮겠어요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043700.1.15",
          "form": "금액도 괜찮고 무배라",
          "original_form": "금액도 괜찮고 무배라",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043700.1.16",
          "form": "적립금 15일 소멸이라",
          "original_form": "적립금 15일 소멸이라",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043700.1.17",
          "form": "언능사긴해야겠어요",
          "original_form": "언능사긴해야겠어요",
          "speaker_id": "2",
          "time": "20210907 20:04"
        }
