      "utterance": [
        {
          "id": "MDRW2100016820.1.1",
          "form": "최근 구매하신 물건이 있나요?",
          "original_form": "최근 구매하신 물건이 있나요?",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016820.1.2",
          "form": "어제 가방 셀프선물 햇어요",
          "original_form": "어제 가방 셀프선물 햇어요",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016820.1.3",
          "form": "최근 구매한거중 젤 행복감이 큽니다",
          "original_form": "최근 구매한거중 젤 행복감이 큽니다",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016820.1.4",
          "form": "우와 굿이네요!",
          "original_form": "우와 굿이네요!",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016820.1.5",
          "form": "전 h패션몰 출석체크 이벤트해서 버킷햇 샀어요 ㅋㅋ",
          "original_form": "전 h패션몰 출석체크 이벤트해서 버킷햇 샀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016820.1.6",
          "form": "앗 ㅎㅎ 야무지고 현명한 소비!!!!!",
          "original_form": "앗 ㅎㅎ 야무지고 현명한 소비!!!!!",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016820.1.7",
          "form": "ㅎㅎ실결제금액이 있긴했지만 맘에 드는 소비를 했습니다!! ㅎㅎㅎ",
          "original_form": "ㅎㅎ실결제금액이 있긴했지만 맘에 드는 소비를 했습니다!! ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016820.1.8",
          "form": "돈 쓰는게 제일 재밌네요!",
          "original_form": "돈 쓰는게 제일 재밌네요!",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016820.1.9",
          "form": "저는 버킷햇 출퇴근길에 즐겨써요",
          "original_form": "저는 버킷햇 출퇴근길에 즐겨써요",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016820.1.10",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016820.1.11",
          "form": "오 뭔가 힙해힙해! 맞아요 햇빛 가리는 용도로 최고인거같아요 ㅋㅋㅋ",
          "original_form": "오 뭔가 힙해힙해! 맞아요 햇빛 가리는 용도로 최고인거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016820.1.12",
          "form": "기미 무서워서 앞으로도 애용할꺼에요 ㅎㅎㅎ",
          "original_form": "기미 무서워서 앞으로도 애용할꺼에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016820.1.13",
          "form": "저도 출퇴근 용으로 써봐야겠어요 ㅎㅎ",
          "original_form": "저도 출퇴근 용으로 써봐야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016820.1.14",
          "form": "위시리스트템 잇으신가용?",
          "original_form": "위시리스트템 잇으신가용?",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016820.1.15",
          "form": "전 패드 사고 싶어요! name1님은요?",
          "original_form": "전 패드 사고 싶어요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016820.1.16",
          "form": "오 패드!! 저는 갑자기",
          "original_form": "오 패드!! 저는 갑자기",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016820.1.17",
          "form": "플립 ㅠㅠ",
          "original_form": "플립 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016820.1.18",
          "form": "겨울쯤 바꾸지않을까 싶어요",
          "original_form": "겨울쯤 바꾸지않을까 싶어요",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016820.1.19",
          "form": "진짜 넘 이쁘더라구요 ㅠㅠㅠ",
          "original_form": "진짜 넘 이쁘더라구요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016820.1.20",
          "form": "플립 사시면 저 100번만 열고 닫아볼게요 ㅋㅋㅋㅋ",
          "original_form": "플립 사시면 저 100번만 열고 닫아볼게요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016820.1.21",
          "form": "ㅋㅋ저는 꾸미는데 관심 없으면서 괜히 뽐뿌가 와가지고",
          "original_form": "ㅋㅋ저는 꾸미는데 관심 없으면서 괜히 뽐뿌가 와가지고",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016820.1.22",
          "form": "ㅋㅋ늘 새 것이 탐나는 법이죠!!ㅠㅠ",
          "original_form": "ㅋㅋ늘 새 것이 탐나는 법이죠!!ㅠㅠ",
