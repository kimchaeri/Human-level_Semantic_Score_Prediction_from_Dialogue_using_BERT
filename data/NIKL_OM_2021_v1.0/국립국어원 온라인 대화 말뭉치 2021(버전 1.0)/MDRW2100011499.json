      "utterance": [
        {
          "id": "MDRW2100011499.1.1",
          "form": "유튜브 자주보시나요",
          "original_form": "유튜브 자주보시나요",
          "speaker_id": "1",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011499.1.2",
          "form": "네! 유튜브 맨날 봐요 ㅎㅎㅎ",
          "original_form": "네! 유튜브 맨날 봐요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011499.1.3",
          "form": "오 누구꺼 보시나요",
          "original_form": "오 누구꺼 보시나요",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011499.1.4",
          "form": "브이로그 같은건 잘 안봐요 ㅎㅎ",
          "original_form": "브이로그 같은건 잘 안봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011499.1.5",
          "form": "방송클립 많이보고~",
          "original_form": "방송클립 많이보고~",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011499.1.6",
          "form": "구독하는 채널은 거의 없어요~",
          "original_form": "구독하는 채널은 거의 없어요~",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011499.1.7",
          "form": "유툽 많이 보시나봐여@!",
          "original_form": "유툽 많이 보시나봐여@!",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011499.1.8",
          "form": "저는 여행유튜브 좋아해요ㅎㅎ",
          "original_form": "저는 여행유튜브 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011499.1.9",
          "form": "어떤거 보시는데요?",
          "original_form": "어떤거 보시는데요?",
          "speaker_id": "2",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011499.1.10",
          "form": "빠니보틀 재밌어요!",
          "original_form": "빠니보틀 재밌어요!",
          "speaker_id": "1",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011499.1.11",
          "form": "설명 좀 해주세요 ㅎㅎ 어떤 채널인지~~",
          "original_form": "설명 좀 해주세요 ㅎㅎ 어떤 채널인지~~",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011499.1.12",
          "form": "저도 유툽 들어가면 가끔씩 메인에 뜨긴 하더라구요! 그 채널ㅎㅎ 인기가 많은가봐요",
          "original_form": "저도 유툽 들어가면 가끔씩 메인에 뜨긴 하더라구요! 그 채널ㅎㅎ 인기가 많은가봐요",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011499.1.13",
          "form": "지금은 미국 가있어요ㅋㅋㅋ",
          "original_form": "지금은 미국 가있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011499.1.14",
          "form": "와우 혼자 다니는 유투버예요??",
          "original_form": "와우 혼자 다니는 유투버예요??",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011499.1.15",
          "form": "요즘같은때에 여행괜찮을까여...",
          "original_form": "요즘같은때에 여행괜찮을까여...",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011499.1.16",
          "form": "하긴 여행유투버는 그게 직업이니까ㅜㅜㅜ 컨텐츠 개발하지 않는이상 여행안하고 한국에만 있을순 없겠네요",
          "original_form": "하긴 여행유투버는 그게 직업이니까ㅜㅜㅜ 컨텐츠 개발하지 않는이상 여행안하고 한국에만 있을순 없겠네요",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100011499.1.17",
          "form": "요즘엔 많이 나가더라구요ㅠ",
          "original_form": "요즘엔 많이 나가더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011499.1.18",
          "form": "아 그런가요 ㅎㅎ 저는 동물컨텐츠 유튜브만 좀 챙겨보는 편이에여",
          "original_form": "아 그런가요 ㅎㅎ 저는 동물컨텐츠 유튜브만 좀 챙겨보는 편이에여",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011499.1.19",
          "form": "오 동물컨텐츠도 인기 많더라구요",
          "original_form": "오 동물컨텐츠도 인기 많더라구요",
          "speaker_id": "1",
          "time": "20210824 17:44"
        }
      ]
