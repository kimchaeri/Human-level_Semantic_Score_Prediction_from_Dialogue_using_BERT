      "utterance": [
        {
          "id": "MDRW2100024575.1.1",
          "form": "전 강아지 길렀어요",
          "original_form": "전 강아지 길렀어요",
          "speaker_id": "2",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100024575.1.2",
          "form": "언니의 강아지 궁금해요!!!",
          "original_form": "언니의 강아지 궁금해요!!!",
          "speaker_id": "1",
          "time": "20210831 15:34"
        },
        {
          "id": "MDRW2100024575.1.3",
          "form": "사진 찍어둔거 있나요???",
          "original_form": "사진 찍어둔거 있나요???",
          "speaker_id": "1",
          "time": "20210831 15:34"
        },
        {
          "id": "MDRW2100024575.1.4",
          "form": "20년전이라",
          "original_form": "20년전이라",
          "speaker_id": "2",
          "time": "20210831 15:46"
        },
        {
          "id": "MDRW2100024575.1.5",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 15:46"
        },
        {
          "id": "MDRW2100024575.1.6",
          "form": "제 기억속에나 희미하게 있어요",
          "original_form": "제 기억속에나 희미하게 있어요",
          "speaker_id": "2",
          "time": "20210831 15:46"
        },
        {
          "id": "MDRW2100024575.1.7",
          "form": "앗 그렇군요 ㅠㅠ 아쉬워라",
          "original_form": "앗 그렇군요 ㅠㅠ 아쉬워라",
          "speaker_id": "1",
          "time": "20210831 15:46"
        },
        {
          "id": "MDRW2100024575.1.8",
          "form": "강아지 이름은 뭐였어요?!",
          "original_form": "강아지 이름은 뭐였어요?!",
          "speaker_id": "1",
          "time": "20210831 15:46"
        },
        {
          "id": "MDRW2100024575.1.9",
          "form": "저도 강아지 있었으면 하는데,,",
          "original_form": "저도 강아지 있었으면 하는데,,",
          "speaker_id": "1",
          "time": "20210831 15:46"
        },
        {
          "id": "MDRW2100024575.1.10",
          "form": "인형도 이렇게 좋아하는데 반려동물한텐 진짜 자식처럼 기를거같아요 저는 ㅋㅋㅋㅋ",
          "original_form": "인형도 이렇게 좋아하는데 반려동물한텐 진짜 자식처럼 기를거같아요 저는 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100024575.1.11",
          "form": "제 이름이랑 동생이름이랑 합쳐서",
          "original_form": "제 이름이랑 동생이름이랑 합쳐서",
          "speaker_id": "2",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100024575.1.12",
          "form": "name3이라고...ㅋㅋ",
          "original_form": "&name3&이라고...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100024575.1.13",
          "form": "앗 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100024575.1.14",
          "form": "왠지 의젓할 것 같은 이름인데요!!!",
          "original_form": "왠지 의젓할 것 같은 이름인데요!!!",
          "speaker_id": "1",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100024575.1.15",
          "form": "의젓했죠ㅋㅋㅋ",
          "original_form": "의젓했죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.16",
          "form": "점잖은 강아지...",
          "original_form": "점잖은 강아지...",
          "speaker_id": "1",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.17",
          "form": "말티즈계열인것 치곤 얌전함",
          "original_form": "말티즈계열인것 치곤 얌전함",
          "speaker_id": "2",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.18",
          "form": "오 말티즈랑 푸들이",
          "original_form": "오 말티즈랑 푸들이",
          "speaker_id": "1",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.19",
          "form": "나중에 알고보니 그쪽애들 성깔이 장난아니라죠",
          "original_form": "나중에 알고보니 그쪽애들 성깔이 장난아니라죠",
          "speaker_id": "2",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.20",
          "form": "성격이 강하기로 유명하죠?!",
          "original_form": "성격이 강하기로 유명하죠?!",
          "speaker_id": "1",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.21",
          "form": "그러니까요 어렸을 때는",
          "original_form": "그러니까요 어렸을 때는",
          "speaker_id": "1",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.22",
          "form": "네ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.23",
          "form": "자주 보여서",
          "original_form": "자주 보여서",
          "speaker_id": "1",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.24",
          "form": "그냥 그러려니 했는데",
          "original_form": "그냥 그러려니 했는데",
          "speaker_id": "1",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024575.1.25",
          "form": "요즘은 또 말티즈 / 요크셔테리어 / 푸들 이런 애들 잘 안보이는 것 같지 않아요?!;",
          "original_form": "요즘은 또 말티즈 / 요크셔테리어 / 푸들 이런 애들 잘 안보이는 것 같지 않아요?!;",
          "speaker_id": "1",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100024575.1.26",
          "form": "이런 것도 다 유행이 있나봐요",
          "original_form": "이런 것도 다 유행이 있나봐요",
          "speaker_id": "1",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100024575.1.27",
          "form": "그렇다더라구요",
          "original_form": "그렇다더라구요",
          "speaker_id": "2",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100024575.1.28",
          "form": "그래서 유기견도....",
          "original_form": "그래서 유기견도....",
          "speaker_id": "2",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100024575.1.29",
          "form": "전 첫개가 그래서 그런지 비슷한 느낌의 개들한테 마음이 많이 가요",
          "original_form": "전 첫개가 그래서 그런지 비슷한 느낌의 개들한테 마음이 많이 가요",
          "speaker_id": "2",
          "time": "20210831 15:51"
        },
        {
          "id": "MDRW2100024575.1.30",
          "form": "저 같아도 그럴 거 같아요!",
          "original_form": "저 같아도 그럴 거 같아요!",
          "speaker_id": "1",
          "time": "20210831 15:51"
        },
        {
          "id": "MDRW2100024575.1.31",
          "form": "저는 반려동물 버리는 사람들이 정말 이해할 수가 없어요...",
          "original_form": "저는 반려동물 버리는 사람들이 정말 이해할 수가 없어요...",
          "speaker_id": "1",
          "time": "20210831 15:51"
        },
        {
          "id": "MDRW2100024575.1.32",
          "form": "나쁘다고 생각하기 이전에 리터럴리 이해가 안감,,",
          "original_form": "나쁘다고 생각하기 이전에 리터럴리 이해가 안감,,",
          "speaker_id": "1",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100024575.1.33",
          "form": "기르는게 쉬운일은 아니죠",
          "original_form": "기르는게 쉬운일은 아니죠",
          "speaker_id": "2",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100024575.1.34",
          "form": "그치만 걔들을 정말 가족이라 생각하면 감히 그럴수 없을텐데",
          "original_form": "그치만 걔들을 정말 가족이라 생각하면 감히 그럴수 없을텐데",
          "speaker_id": "2",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024575.1.35",
          "form": "그러게 말이에요,,,",
          "original_form": "그러게 말이에요,,,",
          "speaker_id": "1",
          "time": "20210831 15:53"
        }
      ]
    }
