      "utterance": [
        {
          "id": "MDRW2100047479.1.1",
          "form": "다시 비 올 것 같은 하늘이네요",
          "original_form": "다시 비 올 것 같은 하늘이네요",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100047479.1.2",
          "form": "네 저희 동네도 날이 꾸물거려요",
          "original_form": "네 저희 동네도 날이 꾸물거려요",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100047479.1.3",
          "form": "일기예보 상으로는 밤부터 비가 온다고 하더라구요",
          "original_form": "일기예보 상으로는 밤부터 비가 온다고 하더라구요",
          "speaker_id": "2",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100047479.1.4",
          "form": "또 비가 오는군요..지겹네요",
          "original_form": "또 비가 오는군요..지겹네요",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100047479.1.5",
          "form": "맞아요 ㅋㅋ 주말에는 그래도 맑을 예정이더라구요",
          "original_form": "맞아요 ㅋㅋ 주말에는 그래도 맑을 예정이더라구요",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100047479.1.6",
          "form": "오~ 좋은 소식이네요~!",
          "original_form": "오~ 좋은 소식이네요~!",
          "speaker_id": "1",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100047479.1.7",
          "form": "밀린 빨래 해야겠어요",
          "original_form": "밀린 빨래 해야겠어요",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100047479.1.8",
          "form": "내일도 비오려나요?",
          "original_form": "내일도 비오려나요?",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100047479.1.9",
          "form": "내일은 비 온다 그랬어요",
          "original_form": "내일은 비 온다 그랬어요",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100047479.1.10",
          "form": "앗 내일도 오는군요ㅠㅠ",
          "original_form": "앗 내일도 오는군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100047479.1.11",
          "form": "지긋지긋한 비. 가을장마란 말이 딱이에요",
          "original_form": "지긋지긋한 비. 가을장마란 말이 딱이에요",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100047479.1.12",
          "form": "네 진짜 장마같이 오네요",
          "original_form": "네 진짜 장마같이 오네요",
          "speaker_id": "1",
