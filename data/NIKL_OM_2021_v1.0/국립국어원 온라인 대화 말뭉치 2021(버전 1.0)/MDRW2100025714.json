      "utterance": [
        {
          "id": "MDRW2100025714.1.1",
          "form": "요새도 쌍둥이거 둘 다 해?",
          "original_form": "요새도 쌍둥이거 둘 다 해?",
          "speaker_id": "2",
          "time": "20210901 16:21"
        },
        {
          "id": "MDRW2100025714.1.2",
          "form": "ㅋㅋㅋㅋㅋ아니 한명만..",
          "original_form": "ㅋㅋㅋㅋㅋ아니 한명만..",
          "speaker_id": "1",
          "time": "20210901 16:21"
        },
        {
          "id": "MDRW2100025714.1.3",
          "form": "요즘 솜인형들 옷도 많이팔더라.. 홍대에 매장도 있대",
          "original_form": "요즘 솜인형들 옷도 많이팔더라.. 홍대에 매장도 있대",
          "speaker_id": "1",
          "time": "20210901 16:21"
        },
        {
          "id": "MDRW2100025714.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ 결국 하나 손절하셨군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 결국 하나 손절하셨군요",
          "speaker_id": "2",
          "time": "20210901 16:21"
        },
        {
          "id": "MDRW2100025714.1.5",
          "form": "오오 대박이당",
          "original_form": "오오 대박이당",
          "speaker_id": "2",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025714.1.6",
          "form": "네...둘다 키우기엔 허리가 휘어서요",
          "original_form": "네...둘다 키우기엔 허리가 휘어서요",
          "speaker_id": "1",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025714.1.7",
          "form": "그쵸? 둘은 좀 글쵸?",
          "original_form": "그쵸? 둘은 좀 글쵸?",
          "speaker_id": "2",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025714.1.8",
          "form": "ㅇㅇ버거워...ㅋㅋㅋ옷들이 다 비싸",
          "original_form": "ㅇㅇ버거워...ㅋㅋㅋ옷들이 다 비싸",
          "speaker_id": "1",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025714.1.9",
          "form": "인형 옷이 더 비싸 내 옷 보다ㅋㅋㅋ",
          "original_form": "인형 옷이 더 비싸 내 옷 보다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025714.1.10",
          "form": "진짜 비싸.. 대충 만든 망토같은거도 1~2만원 하더라",
          "original_form": "진짜 비싸.. 대충 만든 망토같은거도 1~2만원 하더라",
          "speaker_id": "1",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100025714.1.11",
          "form": "그런거 만들어서 팔까?",
          "original_form": "그런거 만들어서 팔까?",
          "speaker_id": "2",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100025714.1.12",
          "form": "ㅋㅋㅋㅋ은근 인형들 또 다 사서 만들어야해서..ㅠ",
          "original_form": "ㅋㅋㅋㅋ은근 인형들 또 다 사서 만들어야해서..ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100025714.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ인형사는 돈이 더 들겠다!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ인형사는 돈이 더 들겠다!",
          "speaker_id": "2",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100025714.1.14",
          "form": "그니까 사이즈 별로 또 다 모델이 있더라",
          "original_form": "그니까 사이즈 별로 또 다 모델이 있더라",
          "speaker_id": "1",
          "time": "20210901 16:26"
        },
        {
          "id": "MDRW2100025714.1.15",
          "form": "아휴 그거 하는 사람들도 돈이랑 시간 많이 들겠다",
          "original_form": "아휴 그거 하는 사람들도 돈이랑 시간 많이 들겠다",
