      "utterance": [
        {
          "id": "MDRW2100032658.1.1",
          "form": "막걸리 좋아하시나봐요?",
          "original_form": "막걸리 좋아하시나봐요?",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032658.1.2",
          "form": "네!!! 파전이랑 먹으면 너무 맛있어요 ㅠㅠㅠ",
          "original_form": "네!!! 파전이랑 먹으면 너무 맛있어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032658.1.3",
          "form": "구쵸ㅠㅠ",
          "original_form": "구쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032658.1.4",
          "form": "곰표 맥주 드셔보셨나여",
          "original_form": "곰표 맥주 드셔보셨나여",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032658.1.5",
          "form": "헉 아뇨!!!",
          "original_form": "헉 아뇨!!!",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032658.1.6",
          "form": "맛있어요??",
          "original_form": "맛있어요??",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032658.1.7",
          "form": "청포도 에일 새로나온거 맛나더라고요",
          "original_form": "청포도 에일 새로나온거 맛나더라고요",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032658.1.8",
          "form": "오오 청포도 너무 좋아요 ㅠㅠ",
          "original_form": "오오 청포도 너무 좋아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 00:22"
        },
        {
          "id": "MDRW2100032658.1.9",
          "form": "제가 청포도 에이드 진짜 좋아하거든요 ㅎㅎ",
          "original_form": "제가 청포도 에이드 진짜 좋아하거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:22"
        },
        {
          "id": "MDRW2100032658.1.10",
          "form": "과일맛 맥주 좋아하세요?",
          "original_form": "과일맛 맥주 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 00:22"
        },
        {
          "id": "MDRW2100032658.1.11",
          "form": "저도요 ㅋㅋㅌ",
          "original_form": "저도요 ㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210906 07:02"
        },
        {
          "id": "MDRW2100032658.1.12",
          "form": "맥주는 그냥 보리맛이 나은거갗아여 ㅌㅋ",
          "original_form": "맥주는 그냥 보리맛이 나은거갗아여 ㅌㅋ",
          "speaker_id": "1",
          "time": "20210906 07:02"
        },
        {
          "id": "MDRW2100032658.1.13",
          "form": "저도요!!! 숙취가 심하드라구요 ㅋㅋㅋ",
          "original_form": "저도요!!! 숙취가 심하드라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032658.1.14",
          "form": "곰표가 편의점에 파는거죠.?",
          "original_form": "곰표가 편의점에 파는거죠.?",
          "speaker_id": "2",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032658.1.15",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100032658.1.16",
          "form": "곰표 팝콘도 있고 나초도 있어요",
          "original_form": "곰표 팝콘도 있고 나초도 있어요",
          "speaker_id": "1",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100032658.1.17",
          "form": "우아 팝콘은 무슨 맛일까요 ㅋㅋㅋ",
          "original_form": "우아 팝콘은 무슨 맛일까요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:26"
        },
        {
          "id": "MDRW2100032658.1.18",
          "form": "팝콘은 안먹어보고 나쵸만 먹어봤어요",
          "original_form": "팝콘은 안먹어보고 나쵸만 먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100032658.1.19",
          "form": "오오 저도 곰표팝콘 먹어봐야겟어요 ㅋㅋ",
          "original_form": "오오 저도 곰표팝콘 먹어봐야겟어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:04"
        },
        {
          "id": "MDRW2100032658.1.20",
          "form": "진로두꺼비는 소주라서 못마실거같아요",
          "original_form": "진로두꺼비는 소주라서 못마실거같아요",
