      "utterance": [
        {
          "id": "MDRW2100014494.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014494.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014494.1.3",
          "form": "종교있으신가요?",
          "original_form": "종교있으신가요?",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014494.1.4",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014494.1.5",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014494.1.6",
          "form": "전 게을러서 주일에 교회도 못가요",
          "original_form": "전 게을러서 주일에 교회도 못가요",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014494.1.7",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014494.1.8",
          "form": "전 무교예요",
          "original_form": "전 무교예요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014494.1.9",
          "form": "그래도 불교는 좋아해요",
          "original_form": "그래도 불교는 좋아해요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014494.1.10",
          "form": "절에서 따뜻한 느낌이 좋아요",
          "original_form": "절에서 따뜻한 느낌이 좋아요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014494.1.11",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014494.1.12",
          "form": "템플해보셨어요?",
          "original_form": "템플해보셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014494.1.13",
          "form": "네 한번 해봤는데",
          "original_form": "네 한번 해봤는데",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014494.1.14",
          "form": "인적드문 절이 좋은거 같아요",
          "original_form": "인적드문 절이 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014494.1.15",
          "form": "세상이 고요하고 풍경이 보는 곳마다 이뻐요",
          "original_form": "세상이 고요하고 풍경이 보는 곳마다 이뻐요",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014494.1.16",
          "form": "자연과 함께 절이 주는 느낌이 있는거 같아요",
          "original_form": "자연과 함께 절이 주는 느낌이 있는거 같아요",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014494.1.17",
          "form": "와~ 해보고싶은것중 하나인데",
          "original_form": "와~ 해보고싶은것중 하나인데",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014494.1.18",
          "form": "혹시나 도전 하시면 도시가까운 곳 아니고\r\n자연 산 속에 절 추천할께요",
          "original_form": "혹시나 도전 하시면 도시가까운 곳 아니고\r\n자연 산 속에 절 추천할께요",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014494.1.19",
          "form": "겨울엔 하지마세요\r\n너무 너무 추워요",
          "original_form": "겨울엔 하지마세요\r\n너무 너무 추워요",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014494.1.20",
          "form": "그쵸 템플은 산이죠",
          "original_form": "그쵸 템플은 산이죠",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014494.1.21",
          "form": "그래서 두렵구요 벌레",
          "original_form": "그래서 두렵구요 벌레",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014494.1.22",
          "form": "ㅎㅎㅎ날씨 애매할때 가세요",
          "original_form": "ㅎㅎㅎ날씨 애매할때 가세요",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014494.1.23",
          "form": "겨울엔 너무 춥구",
          "original_form": "겨울엔 너무 춥구",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014494.1.24",
          "form": "여름엔 벌레 많으니까요",
          "original_form": "여름엔 벌레 많으니까요",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014494.1.25",
          "form": "그리고 절음식이 안맞을까봐",
          "original_form": "그리고 절음식이 안맞을까봐",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014494.1.26",
          "form": "고기없는 건 식사가 아니여서...",
          "original_form": "고기없는 건 식사가 아니여서...",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014494.1.27",
          "form": "엇 식사가 힘드시겠어요",
          "original_form": "엇 식사가 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014494.1.28",
          "form": "아무래도 풀뿐이라ㅋㅋㅋ",
          "original_form": "아무래도 풀뿐이라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014494.1.29",
          "form": "넹. 비빔밥도 싫어해서",
          "original_form": "넹. 비빔밥도 싫어해서",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014494.1.30",
          "form": "간김에 다이어트겸 단식할까 생각중",
          "original_form": "간김에 다이어트겸 단식할까 생각중",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014494.1.31",
          "form": "절이랑 생활이 안 맞네여ㅜㅜㅋ\r\n진짜 큰맘 먹고 가야겠어요ㅎㅎ",
          "original_form": "절이랑 생활이 안 맞네여ㅜㅜㅋ\r\n진짜 큰맘 먹고 가야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014494.1.32",
          "form": "그래도 꼭 체험은 해보고 싶은것 중 하나예요",
          "original_form": "그래도 꼭 체험은 해보고 싶은것 중 하나예요",
          "speaker_id": "1",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014494.1.33",
          "form": "그럼 템플을 수입 수단으로 하는 곳 말고\r\n작은 암자 같은 곳은 주지스님이 양해 해주시면\r\n식단 따로 가능할거같긴해요\r\n\r\n꼭 해보시길 추천드려요",
          "original_form": "그럼 템플을 수입 수단으로 하는 곳 말고\r\n작은 암자 같은 곳은 주지스님이 양해 해주시면\r\n식단 따로 가능할거같긴해요\r\n\r\n꼭 해보시길 추천드려요",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014494.1.34",
          "form": "오 꿀팁 감사합니다 꼭해봐야겠어요",
          "original_form": "오 꿀팁 감사합니다 꼭해봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 21:00"
        }
      ]
    }
