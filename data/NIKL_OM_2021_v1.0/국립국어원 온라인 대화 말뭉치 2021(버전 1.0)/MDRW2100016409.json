      "utterance": [
        {
          "id": "MDRW2100016409.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016409.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016409.1.3",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016409.1.4",
          "form": "배구요",
          "original_form": "배구요",
          "speaker_id": "2",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016409.1.5",
          "form": "저희 아버지께서 배구 굉장히 좋아하세요 ㅋㅋㅋㅋㅋ",
          "original_form": "저희 아버지께서 배구 굉장히 좋아하세요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016409.1.6",
          "form": "오 그렇군요 직관 가보셨나요?",
          "original_form": "오 그렇군요 직관 가보셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016409.1.7",
          "form": "어릴적에 몇번 가본적 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "어릴적에 몇번 가본적 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016409.1.8",
          "form": "아버지 따라서 ㅋㅋㅋㅋㅋ",
          "original_form": "아버지 따라서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016409.1.9",
          "form": "직관 자주 가세요?",
          "original_form": "직관 자주 가세요?",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016409.1.10",
          "form": "아니요 가보고 싶어요!",
          "original_form": "아니요 가보고 싶어요!",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016409.1.11",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016409.1.12",
          "form": "네 룰도 잘 모르지만 즐겁게 봤던 기억이 있어요",
          "original_form": "네 룰도 잘 모르지만 즐겁게 봤던 기억이 있어요",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016409.1.13",
          "form": "배구좋아하시면",
          "original_form": "배구좋아하시면",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016409.1.14",
          "form": "이번 올림픽도 즐겁게 보셨겠네요",
          "original_form": "이번 올림픽도 즐겁게 보셨겠네요",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016409.1.15",
          "form": "올림픽을 계기로 좋아하게되었어요",
          "original_form": "올림픽을 계기로 좋아하게되었어요",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016409.1.16",
          "form": "이번올림픽이 계기인가요?",
          "original_form": "이번올림픽이 계기인가요?",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016409.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016409.1.18",
          "form": "저도 이번올림픽 너무 재밌게 봤어요 ㅋㅋㅋ",
          "original_form": "저도 이번올림픽 너무 재밌게 봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
