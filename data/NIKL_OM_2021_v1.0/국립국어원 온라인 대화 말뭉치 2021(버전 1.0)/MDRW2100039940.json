      "utterance": [
        {
          "id": "MDRW2100039940.1.1",
          "form": "미숫가루라떼 은근 파는 곳 찾기 어렵던데요",
          "original_form": "미숫가루라떼 은근 파는 곳 찾기 어렵던데요",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100039940.1.2",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100039940.1.3",
          "form": "힐링여행이네요 진짜",
          "original_form": "힐링여행이네요 진짜",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100039940.1.4",
          "form": "여름에 더워서 힘들었는데",
          "original_form": "여름에 더워서 힘들었는데",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100039940.1.5",
          "form": "사찰 근처 카페에서 쉬어가면서",
          "original_form": "사찰 근처 카페에서 쉬어가면서",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100039940.1.6",
          "form": "와 진짜 그 달달함이 정수리까지 오더라구요ㅋㅋㅋ",
          "original_form": "와 진짜 그 달달함이 정수리까지 오더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039940.1.7",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039940.1.8",
          "form": "정말 글만 봐도 너무 행복해요ㅠㅠㅠㅠㅠ",
          "original_form": "정말 글만 봐도 너무 행복해요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039940.1.9",
          "form": "천국이 따로 없을 것 같아요 ㅋㅋㅋ",
          "original_form": "천국이 따로 없을 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039940.1.10",
          "form": "추천드릴게여! 저도 기억떠올리니너무 좋았네요ㅜㅜ",
          "original_form": "추천드릴게여! 저도 기억떠올리니너무 좋았네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039940.1.11",
          "form": "예천은 어떠셨어요?",
          "original_form": "예천은 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039940.1.12",
          "form": "사실 예천은 회룡포 말고는 별로였어요",
          "original_form": "사실 예천은 회룡포 말고는 별로였어요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039940.1.13",
          "form": "앗 음식도요?",
          "original_form": "앗 음식도요?",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039940.1.14",
          "form": "근처 동네 이름이 용궁면인데요 용궁역에 가면 토끼간빵을 팔아요",
          "original_form": "근처 동네 이름이 용궁면인데요 용궁역에 가면 토끼간빵을 팔아요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039940.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039940.1.16",
          "form": "음식이 제일 중요한데",
          "original_form": "음식이 제일 중요한데",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039940.1.17",
          "form": "앗ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039940.1.18",
          "form": "음식은 순대국이 유명한데 그냥 그랬어요",
          "original_form": "음식은 순대국이 유명한데 그냥 그랬어요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039940.1.19",
          "form": "궁금하다 토끼간빵ㅋㅋ",
          "original_form": "궁금하다 토끼간빵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039940.1.20",
          "form": "그쵸 음식이 가장 중요해서 아쉬워요 ㅋㅋㅋ",
          "original_form": "그쵸 음식이 가장 중요해서 아쉬워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:16"
        }
