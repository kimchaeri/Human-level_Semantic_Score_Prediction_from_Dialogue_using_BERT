      "utterance": [
        {
          "id": "MDRW2100012956.1.1",
          "form": "다들 여행이 얼마나 가고싶겠노",
          "original_form": "다들 여행이 얼마나 가고싶겠노",
          "speaker_id": "2",
          "time": "20210825 09:28"
        },
        {
          "id": "MDRW2100012956.1.2",
          "form": "그니깐~우리도 이렇게 가고싶은데",
          "original_form": "그니깐~우리도 이렇게 가고싶은데",
          "speaker_id": "1",
          "time": "20210825 09:28"
        },
        {
          "id": "MDRW2100012956.1.3",
          "form": "코로나 끝나고 첫 해외여행은 같이 가자~",
          "original_form": "코로나 끝나고 첫 해외여행은 같이 가자~",
          "speaker_id": "1",
          "time": "20210825 09:28"
        },
        {
          "id": "MDRW2100012956.1.4",
          "form": "비행기티켓 구하기도 힘들어지겠다",
          "original_form": "비행기티켓 구하기도 힘들어지겠다",
          "speaker_id": "2",
          "time": "20210825 09:28"
        },
        {
          "id": "MDRW2100012956.1.5",
          "form": "ㅇㅇ같이가자 애들도 좋아하겠다",
          "original_form": "ㅇㅇ같이가자 애들도 좋아하겠다",
          "speaker_id": "2",
          "time": "20210825 09:28"
        },
        {
          "id": "MDRW2100012956.1.6",
          "form": "그렇겠지",
          "original_form": "그렇겠지",
          "speaker_id": "1",
          "time": "20210825 09:29"
        },
        {
          "id": "MDRW2100012956.1.7",
          "form": "동남아로 가야겠네",
          "original_form": "동남아로 가야겠네",
          "speaker_id": "1",
          "time": "20210825 09:29"
        },
        {
          "id": "MDRW2100012956.1.8",
          "form": "더좋은곳으로 가보자",
          "original_form": "더좋은곳으로 가보자",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012956.1.9",
          "form": "오키나와 가볼래??",
          "original_form": "오키나와 가볼래??",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012956.1.10",
          "form": "일본은 오염때문에ㅜ",
          "original_form": "일본은 오염때문에ㅜ",
          "speaker_id": "1",
          "time": "20210825 09:43"
        },
        {
          "id": "MDRW2100012956.1.11",
          "form": "오키나와는 거리가있어서 괜찮다",
          "original_form": "오키나와는 거리가있어서 괜찮다",
          "speaker_id": "2",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012956.1.12",
          "form": "그런가 어디든 다 좋아",
          "original_form": "그런가 어디든 다 좋아",
          "speaker_id": "1",
          "time": "20210825 09:45"
        },
        {
          "id": "MDRW2100012956.1.13",
          "form": "진짜 여행은 어디든 떠나고 싶다",
          "original_form": "진짜 여행은 어디든 떠나고 싶다",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100012956.1.14",
          "form": "언제든 어디로든 가고싶다~~",
          "original_form": "언제든 어디로든 가고싶다~~",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100012956.1.15",
          "form": "피행기타고 가는 첫날이 얼마나 설레이노\r\n다시 느껴보고싶네",
          "original_form": "피행기타고 가는 첫날이 얼마나 설레이노\r\n다시 느껴보고싶네",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100012956.1.16",
          "form": "원래 비행기가 탈때가 좋지~",
          "original_form": "원래 비행기가 탈때가 좋지~",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100012956.1.17",
          "form": "어 설레이고...어디갈지 생각하면서...",
          "original_form": "어 설레이고...어디갈지 생각하면서...",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100012956.1.18",
          "form": "대만 가고 싶어~",
          "original_form": "대만 가고 싶어~",
          "speaker_id": "1",
          "time": "20210826 19:33"
        }
      ]
