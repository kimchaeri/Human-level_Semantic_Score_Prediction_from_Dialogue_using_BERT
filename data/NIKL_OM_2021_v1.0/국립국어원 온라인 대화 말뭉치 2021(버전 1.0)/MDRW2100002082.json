      "utterance": [
        {
          "id": "MDRW2100002082.1.1",
          "form": "우리 싸이월드 세대잖아",
          "original_form": "우리 싸이월드 세대잖아",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002082.1.2",
          "form": "부활하나봐...",
          "original_form": "부활하나봐...",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002082.1.3",
          "form": "헐 대박... 예전 사진 부활 하는 거니",
          "original_form": "헐 대박... 예전 사진 부활 하는 거니",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002082.1.4",
          "form": "당황스러웠잖아 ㅜㅜ",
          "original_form": "당황스러웠잖아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002082.1.5",
          "form": "사진 복구했다그래서...",
          "original_form": "사진 복구했다그래서...",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002082.1.6",
          "form": "설마...ㅜㅜ 아직 남았으려나",
          "original_form": "설마...ㅜㅜ 아직 남았으려나",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002082.1.7",
          "form": "원하지 않는데..",
          "original_form": "원하지 않는데..",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002082.1.8",
          "form": "궁금하긴 하지만 찾아볼꺼야?",
          "original_form": "궁금하긴 하지만 찾아볼꺼야?",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002082.1.9",
          "form": "지워버리고싶은데 ㅜㅜ",
          "original_form": "지워버리고싶은데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002082.1.10",
          "form": "또 보기 두렵다ㅠㅋㅋㅋㅋㅋ",
          "original_form": "또 보기 두렵다ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002082.1.11",
          "form": "흑역사 ㅜㅜ",
          "original_form": "흑역사 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002082.1.12",
          "form": "정말ㅋㅋㅋㅋㅋ 얼마나 오글거릴까",
          "original_form": "정말ㅋㅋㅋㅋㅋ 얼마나 오글거릴까",
          "speaker_id": "2",
          "time": "20210820 08:24"
        },
        {
          "id": "MDRW2100002082.1.13",
          "form": "싸이월드때 했던 브금 간만에 들어야짘ㅋㅋㅋㅋ",
          "original_form": "싸이월드때 했던 브금 간만에 들어야짘ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 08:25"
        },
        {
          "id": "MDRW2100002082.1.14",
          "form": "나도 ㅋㅋㅋ 그중에서 가끔 아직도 듣는거 있어",
          "original_form": "나도 ㅋㅋㅋ 그중에서 가끔 아직도 듣는거 있어",
          "speaker_id": "1",
          "time": "20210820 08:59"
        },
        {
          "id": "MDRW2100002082.1.15",
          "form": "추억의 노래 ㅋㅋㅋ",
          "original_form": "추억의 노래 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:59"
        },
        {
          "id": "MDRW2100002082.1.16",
          "form": "어떤거??",
          "original_form": "어떤거??",
          "speaker_id": "2",
          "time": "20210820 09:00"
        },
        {
          "id": "MDRW2100002082.1.17",
          "form": "난 몽환의 숲 듣는다 ㅎㅎ",
          "original_form": "난 몽환의 숲 듣는다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 09:00"
        },
        {
          "id": "MDRW2100002082.1.18",
          "form": "나도 몽환의 숲이랑 프리스타일 노래... ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 몽환의 숲이랑 프리스타일 노래... ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:01"
        },
        {
          "id": "MDRW2100002082.1.19",
          "form": "웃기다 ㅋㅋㅋㅋ 싸이로 추억 소환",
          "original_form": "웃기다 ㅋㅋㅋㅋ 싸이로 추억 소환",
          "speaker_id": "1",
          "time": "20210820 09:01"
        },
        {
          "id": "MDRW2100002082.1.20",
          "form": "쉿 몽환의숲 ㅎㅎ 진짜 싸이월드 예전에도 자료 받으라고",
          "original_form": "쉿 몽환의숲 ㅎㅎ 진짜 싸이월드 예전에도 자료 받으라고",
          "speaker_id": "2",
          "time": "20210820 09:03"
        },
        {
          "id": "MDRW2100002082.1.21",
          "form": "몇년전에 그랬던것 같은데",
          "original_form": "몇년전에 그랬던것 같은데",
          "speaker_id": "2",
          "time": "20210820 09:03"
        },
        {
          "id": "MDRW2100002082.1.22",
          "form": "맞아 ㅜㅜ 근데 그 자료들을 복구할줄은 몰랐어 ㅋㅋㅋ",
          "original_form": "맞아 ㅜㅜ 근데 그 자료들을 복구할줄은 몰랐어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:08"
        },
        {
          "id": "MDRW2100002082.1.23",
          "form": "빅데이터잖아 ㅋㅋㅋ",
          "original_form": "빅데이터잖아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:09"
        },
        {
          "id": "MDRW2100002082.1.24",
          "form": "내말이 ㅎㅎ",
          "original_form": "내말이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 09:11"
        },
        {
          "id": "MDRW2100002082.1.25",
          "form": "도토리도 다시 준다고 하고 대박이야",
          "original_form": "도토리도 다시 준다고 하고 대박이야",
          "speaker_id": "2",
          "time": "20210820 09:11"
        },
        {
          "id": "MDRW2100002082.1.26",
          "form": "도토리도 다시 준다그랬어??",
          "original_form": "도토리도 다시 준다그랬어??",
          "speaker_id": "1",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002082.1.27",
          "form": "대박 진짜 진심이구나",
          "original_form": "대박 진짜 진심이구나",
          "speaker_id": "1",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002082.1.28",
          "form": "싸이월드 살리기에 ㅋㅋㅋㅋ",
          "original_form": "싸이월드 살리기에 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:15"
        }
