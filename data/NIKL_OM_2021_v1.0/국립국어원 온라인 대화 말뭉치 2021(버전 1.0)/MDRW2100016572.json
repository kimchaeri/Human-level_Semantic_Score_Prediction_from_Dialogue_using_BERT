      "utterance": [
        {
          "id": "MDRW2100016572.1.1",
          "form": "취업하기가 어렵네요ㅠ",
          "original_form": "취업하기가 어렵네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016572.1.2",
          "form": "그쵸 ㅠㅜ",
          "original_form": "그쵸 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016572.1.3",
          "form": "준비하고 계세요?",
          "original_form": "준비하고 계세요?",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016572.1.4",
          "form": "공고 찾아보고는 있는데....",
          "original_form": "공고 찾아보고는 있는데....",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016572.1.5",
          "form": "지원할만한 회사는 없더라고요..",
          "original_form": "지원할만한 회사는 없더라고요..",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016572.1.6",
          "form": "과가 어디세요??",
          "original_form": "과가 어디세요??",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016572.1.7",
          "form": "자연계 전공인데 그냥 사무직 알아보고 있어요~",
          "original_form": "자연계 전공인데 그냥 사무직 알아보고 있어요~",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016572.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016572.1.9",
          "form": "그럼 지금 졸업하신거죠?",
          "original_form": "그럼 지금 졸업하신거죠?",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016572.1.10",
          "form": "네네 졸업한지 2년정도?",
          "original_form": "네네 졸업한지 2년정도?",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016572.1.11",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016572.1.12",
          "form": "아뇨아뇨 학생입니당",
          "original_form": "아뇨아뇨 학생입니당",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100016572.1.13",
          "form": "복학생이에요",
          "original_form": "복학생이에요",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100016572.1.14",
          "form": "대학생이면 스펙 쌓을때네요~",
          "original_form": "대학생이면 스펙 쌓을때네요~",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100016572.1.15",
          "form": "아르바이트 하세요?",
          "original_form": "아르바이트 하세요?",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100016572.1.16",
          "form": "넹 지금 카페 아르바이트해요!",
          "original_form": "넹 지금 카페 아르바이트해요!",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100016572.1.17",
          "form": "지금 아르바이트 하시나요??",
          "original_form": "지금 아르바이트 하시나요??",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100016572.1.18",
          "form": "재택으로 문서알바해요",
          "original_form": "재택으로 문서알바해요",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100016572.1.19",
          "form": "고정적인건 아니고 아주가끔씩 건별로..",
          "original_form": "고정적인건 아니고 아주가끔씩 건별로..",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100016572.1.20",
          "form": "카페알바 하고싶었는데ㅜㅜ",
          "original_form": "카페알바 하고싶었는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100016572.1.21",
          "form": "여태까지 해봤던 알바중에 카페가 제일 나은 것 같아요",
          "original_form": "여태까지 해봤던 알바중에 카페가 제일 나은 것 같아요",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100016572.1.22",
          "form": "근데 카패는 경력자만 뽑고 ....",
          "original_form": "근데 카패는 경력자만 뽑고 ....",
          "speaker_id": "1",
          "time": "20210828 03:53"
        },
        {
          "id": "MDRW2100016572.1.23",
          "form": "신입은 카페알바 대체 어디서할수있는지ㅜ",
          "original_form": "신입은 카페알바 대체 어디서할수있는지ㅜ",
          "speaker_id": "1",
          "time": "20210828 03:54"
        }
      ]
