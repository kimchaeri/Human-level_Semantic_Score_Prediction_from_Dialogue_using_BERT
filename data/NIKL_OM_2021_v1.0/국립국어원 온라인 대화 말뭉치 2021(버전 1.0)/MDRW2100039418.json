      "utterance": [
        {
          "id": "MDRW2100039418.1.1",
          "form": "애완동물 키우시나요?",
          "original_form": "애완동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039418.1.2",
          "form": "아니요. 키우세요?",
          "original_form": "아니요. 키우세요?",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039418.1.3",
          "form": "아뇨 저도 안키워요ㅎㅎ",
          "original_form": "아뇨 저도 안키워요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039418.1.4",
          "form": "어떤 동물 좋아하세요?",
          "original_form": "어떤 동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039418.1.5",
          "form": "저 고양이 좋아해요.",
          "original_form": "저 고양이 좋아해요.",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039418.1.6",
          "form": "강아지도 좋구요ㅋㅋ",
          "original_form": "강아지도 좋구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039418.1.7",
          "form": "저도요!! 어떤거키우고싶으세요?",
          "original_form": "저도요!! 어떤거키우고싶으세요?",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100039418.1.8",
          "form": "강아지는 코기가 귀엽더라구요",
          "original_form": "강아지는 코기가 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039418.1.9",
          "form": "토종견들도 멋있던데요ㅋㅋㅋ",
          "original_form": "토종견들도 멋있던데요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039418.1.10",
          "form": "코기 크고 귀엽죠ㅋㅋㅋ",
          "original_form": "코기 크고 귀엽죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039418.1.11",
          "form": "토종견들도 늑대같고 예뻐요ㅠㅠㅠ",
          "original_form": "토종견들도 늑대같고 예뻐요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039418.1.12",
          "form": "저는 사냥견들도 좋더라구요",
          "original_form": "저는 사냥견들도 좋더라구요",
          "speaker_id": "1",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039418.1.13",
          "form": "키울준비는 안되서 그냥 영상만 찾아봐여ㅋㅋㅋ",
          "original_form": "키울준비는 안되서 그냥 영상만 찾아봐여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039418.1.14",
          "form": "맞아요.. 여러가지 생각하면 아직 못키우겠어요",
          "original_form": "맞아요.. 여러가지 생각하면 아직 못키우겠어요",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039418.1.15",
          "form": "그러니까요. 일일이 다 챙겨줘야한다더라구요",
          "original_form": "그러니까요. 일일이 다 챙겨줘야한다더라구요",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039418.1.16",
          "form": "저는 고양이알러지가 있더라구요",
          "original_form": "저는 고양이알러지가 있더라구요",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039418.1.17",
          "form": "강아지는 어떨지모르겠어요ㅠㅠ",
          "original_form": "강아지는 어떨지모르겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039418.1.18",
          "form": "ㅠㅠ털 알러지인가요? 키우려면 다 검사해봐야겠네요",
          "original_form": "ㅠㅠ털 알러지인가요? 키우려면 다 검사해봐야겠네요",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039418.1.19",
          "form": "그러니까요ㅠㅠ",
          "original_form": "그러니까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100039418.1.20",
          "form": "알러지는 따로 없어요",
          "original_form": "알러지는 따로 없어요",
          "speaker_id": "2",
          "time": "20210903 21:09"
        }
      ]
