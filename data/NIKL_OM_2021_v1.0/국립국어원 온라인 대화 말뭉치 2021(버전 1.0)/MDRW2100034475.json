      "utterance": [
        {
          "id": "MDRW2100034475.1.1",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100034475.1.2",
          "form": "6월에 맞았는데",
          "original_form": "6월에 맞았는데",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100034475.1.3",
          "form": "제가 아는분들은 개인적으로 아는 간호사한테",
          "original_form": "제가 아는분들은 개인적으로 아는 간호사한테",
          "speaker_id": "1",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100034475.1.4",
          "form": "그전에 신청했어요",
          "original_form": "그전에 신청했어요",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100034475.1.5",
          "form": "오래 기다렸어요",
          "original_form": "오래 기다렸어요",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100034475.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100034475.1.7",
          "form": "전 모르는데라",
          "original_form": "전 모르는데라",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100034475.1.8",
          "form": "좀 구해달라해서 2명꺼",
          "original_form": "좀 구해달라해서 2명꺼",
          "speaker_id": "1",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100034475.1.9",
          "form": "신청 기간 이였어요",
          "original_form": "신청 기간 이였어요",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100034475.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100034475.1.11",
          "form": "구해줘서 신랑이랑 맞았다고",
          "original_form": "구해줘서 신랑이랑 맞았다고",
          "speaker_id": "1",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100034475.1.12",
          "form": "뭐 그럼좋죠",
          "original_form": "뭐 그럼좋죠",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100034475.1.13",
          "form": "병원도 잔여로 안올리고",
          "original_form": "병원도 잔여로 안올리고",
          "speaker_id": "1",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100034475.1.14",
          "form": "엥 그렇군요",
          "original_form": "엥 그렇군요",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100034475.1.15",
          "form": "지인들께 알음알음 나눠주더라구요",
          "original_form": "지인들께 알음알음 나눠주더라구요",
          "speaker_id": "1",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100034475.1.16",
          "form": "전 합법이라",
          "original_form": "전 합법이라",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100034475.1.17",
          "form": "다 그런거죠 뭐",
          "original_form": "다 그런거죠 뭐",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034475.1.18",
          "form": "안그럼 버리고 그런다 더라구요",
          "original_form": "안그럼 버리고 그런다 더라구요",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034475.1.19",
          "form": "난 아는지인이 치과의사뿐이라 ㅋ",
          "original_form": "난 아는지인이 치과의사뿐이라 ㅋ",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034475.1.20",
          "form": "소용이 없고 ㅋㅋ",
          "original_form": "소용이 없고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034475.1.21",
          "form": "ㅋㅋ아 아쉽다",
          "original_form": "ㅋㅋ아 아쉽다",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034475.1.22",
          "form": "전 그때 허용기간이라 신청했는데",
          "original_form": "전 그때 허용기간이라 신청했는데",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034475.1.23",
          "form": "그냥 예약한날 맞아야쥬",
          "original_form": "그냥 예약한날 맞아야쥬",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034475.1.24",
          "form": "운이 좋았죠",
          "original_form": "운이 좋았죠",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100034475.1.25",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "1",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.26",
          "form": "네 맘편히 맞으세요",
          "original_form": "네 맘편히 맞으세요",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.27",
          "form": "저희집다 잔여 잡아서",
          "original_form": "저희집다 잔여 잡아서",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.28",
          "form": "일찍 끝났어요",
          "original_form": "일찍 끝났어요",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.29",
          "form": "그럼 가족분들모두 2차까지다 완료하신거예요?",
          "original_form": "그럼 가족분들모두 2차까지다 완료하신거예요?",
          "speaker_id": "1",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.31",
          "form": "별 이상반응은 없구요?",
          "original_form": "별 이상반응은 없구요?",
          "speaker_id": "1",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.32",
          "form": "아들 고3 이라",
          "original_form": "아들 고3 이라",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.33",
          "form": "나라에서 빠르게 해줘요",
          "original_form": "나라에서 빠르게 해줘요",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.34",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.35",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.36",
          "form": "아들만 두통이요",
          "original_form": "아들만 두통이요",
          "speaker_id": "2",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100034475.1.37",
          "form": "걱정마세요",
          "original_form": "걱정마세요",
          "speaker_id": "2",
          "time": "20210828 13:20"
        },
        {
          "id": "MDRW2100034475.1.38",
          "form": "진짜 소수만 안좋아요",
          "original_form": "진짜 소수만 안좋아요",
          "speaker_id": "2",
          "time": "20210828 13:20"
        },
        {
          "id": "MDRW2100034475.1.39",
          "form": "별일없곟쥬 ㅎ",
          "original_form": "별일없곟쥬 ㅎ",
          "speaker_id": "1",
          "time": "20210828 13:20"
        },
        {
          "id": "MDRW2100034475.1.40",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210828 13:20"
        }
      ]
    }
  ]
