          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.3",
          "form": "엄청자유롭당",
          "original_form": "엄청자유롭당",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.4",
          "form": "굉장히 마음에들어",
          "original_form": "굉장히 마음에들어",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.5",
          "form": "하지만 생활이 잦같을뿐",
          "original_form": "하지만 생활이 잦같을뿐",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.6",
          "form": "학교는일단",
          "original_form": "학교는일단",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.7",
          "form": "자유로워!",
          "original_form": "자유로워!",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.8",
          "form": "오티엠티이런거없고",
          "original_form": "오티엠티이런거없고",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.9",
          "form": "동아리에든다면 더재밌을거야",
          "original_form": "동아리에든다면 더재밌을거야",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.10",
          "form": "오 부럽네...",
          "original_form": "오 부럽네...",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.11",
          "form": "워낙 학생들 나이가 다양하니까",
          "original_form": "워낙 학생들 나이가 다양하니까",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006030.1.12",
          "form": "첨부터 미국 학교 입학했다 했지??",
          "original_form": "첨부터 미국 학교 입학했다 했지??",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006030.1.13",
          "form": "학교내에 어린이들 맡아주는",
          "original_form": "학교내에 어린이들 맡아주는",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006030.1.14",
          "form": "그런데도있고",
          "original_form": "그런데도있고",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006030.1.15",
          "form": "예아",
          "original_form": "예아",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006030.1.16",
          "form": "수업할때 와기데리고오는데도있어",
          "original_form": "수업할때 와기데리고오는데도있어",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006030.1.17",
          "form": "그럼 sat나 act 같은거 친거???",
          "original_form": "그럼 sat나 act 같은거 친거???",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006030.1.18",
