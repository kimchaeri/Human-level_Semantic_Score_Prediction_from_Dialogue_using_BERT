      "utterance": [
        {
          "id": "MDRW2100045975.1.1",
          "form": "출퇴근시간 지하철도 힘들긴 하겠다........",
          "original_form": "출퇴근시간 지하철도 힘들긴 하겠다........",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045975.1.2",
          "form": "역시 걸어서나..그냥 기뷴삼아 자전거 출근이나...(?",
          "original_form": "역시 걸어서나..그냥 기뷴삼아 자전거 출근이나...(?",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045975.1.3",
          "form": "(10분 안짝이라는 말임)",
          "original_form": "(10분 안짝이라는 말임)",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045975.1.4",
          "form": "ㅋㅋㅋㅋㅋ 가능하면 그게 젤 좋긴 해",
          "original_form": "ㅋㅋㅋㅋㅋ 가능하면 그게 젤 좋긴 해",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045975.1.5",
          "form": "아냐 나 30분정도 걸어가",
          "original_form": "아냐 나 30분정도 걸어가",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045975.1.6",
          "form": "앟 그렇군",
          "original_form": "앟 그렇군",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045975.1.7",
          "form": "30분 정도면 뭐..",
          "original_form": "30분 정도면 뭐..",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045975.1.8",
          "form": "여름에는 죽겠지만 괜찮을거 같아",
          "original_form": "여름에는 죽겠지만 괜찮을거 같아",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045975.1.9",
          "form": "여름에는 그래서 지하철 탐 ㅋㅋㅋㅋㅋ",
          "original_form": "여름에는 그래서 지하철 탐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045975.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아 인간이 살아야지....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아 인간이 살아야지....",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045975.1.11",
          "form": "여름엔 걸어다니면",
          "original_form": "여름엔 걸어다니면",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045975.1.12",
          "form": "일 할 기운이 없어",
          "original_form": "일 할 기운이 없어",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045975.1.13",
          "form": "가뜩이나 민원인 대면해야하는데...",
          "original_form": "가뜩이나 민원인 대면해야하는데...",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045975.1.14",
          "form": "ㅋ큐ㅠㅠ 마자마자...",
          "original_form": "ㅋ큐ㅠㅠ 마자마자...",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045975.1.15",
          "form": "그리고 땀 뻘뻘 흘리는 창구도 별로일거야 민원인도(?",
          "original_form": "그리고 땀 뻘뻘 흘리는 창구도 별로일거야 민원인도(?",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045975.1.16",
          "form": "ㅋㅋㅋ 맞아",
          "original_form": "ㅋㅋㅋ 맞아",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045975.1.17",
          "form": "공공기관은 에어컨도 잘 안튼다규",
          "original_form": "공공기관은 에어컨도 잘 안튼다규",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045975.1.18",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045975.1.19",
          "form": "개인 선풍기 틀고 잇더라구",
          "original_form": "개인 선풍기 틀고 잇더라구",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045975.1.20",
          "form": "처음엔 난 선풍기도 안줬어 헝엉",
          "original_form": "처음엔 난 선풍기도 안줬어 헝엉",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045975.1.21",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ 슬프다 진짜...",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ 슬프다 진짜...",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045975.1.22",
          "form": "나도 겨울에 히터 애매하게 틀어서",
          "original_form": "나도 겨울에 히터 애매하게 틀어서",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045975.1.23",
          "form": "발난로 틀음 ㅋㅋㅋㅋ",
          "original_form": "발난로 틀음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:33"
