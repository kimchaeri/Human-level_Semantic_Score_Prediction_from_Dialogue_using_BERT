      "utterance": [
        {
          "id": "MDRW2100040178.1.1",
          "form": "아 구럴 슈 있다",
          "original_form": "아 구럴 슈 있다",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.2",
          "form": "방송국이있음?",
          "original_form": "방송국이있음?",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.3",
          "form": "건강이 필요하니까",
          "original_form": "건강이 필요하니까",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.4",
          "form": "그럣나보ㅓ",
          "original_form": "그럣나보ㅓ",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.6",
          "form": "꽤 걷긴 했는데 감",
          "original_form": "꽤 걷긴 했는데 감",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.7",
          "form": "신기한게",
          "original_form": "신기한게",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.8",
          "form": "ㅇㅇㅇ엉",
          "original_form": "ㅇㅇㅇ엉",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.9",
          "form": "방송국 안에 만화 캐릭터들 있더라",
          "original_form": "방송국 안에 만화 캐릭터들 있더라",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.10",
          "form": "드래곤볼이랑",
          "original_form": "드래곤볼이랑",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.11",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100040178.1.12",
          "form": "어떤 가정집 만화",
