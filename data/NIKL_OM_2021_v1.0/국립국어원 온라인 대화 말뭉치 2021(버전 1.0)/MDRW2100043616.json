      "utterance": [
        {
          "id": "MDRW2100043616.1.1",
          "form": "저는 치즈 돈까스 먹었어요",
          "original_form": "저는 치즈 돈까스 먹었어요",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043616.1.2",
          "form": "오 외식인가요",
          "original_form": "오 외식인가요",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043616.1.3",
          "form": "아니요 냉동 ㅎㅎ",
          "original_form": "아니요 냉동 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043616.1.4",
          "form": "치즈돈까스는 풀무원꺼 사먹었어요",
          "original_form": "치즈돈까스는 풀무원꺼 사먹었어요",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043616.1.5",
          "form": "ㅎㅎ에프에돌렸나요",
          "original_form": "ㅎㅎ에프에돌렸나요",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043616.1.6",
          "form": "네 에프에 돌리면 다 맛있어요 ㅋㅋㅋ",
          "original_form": "네 에프에 돌리면 다 맛있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043616.1.7",
          "form": "최고의발명품이에요",
          "original_form": "최고의발명품이에요",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043616.1.8",
          "form": "저도 에어컨에 이은 혁명적인 발명품이라 생각해요",
          "original_form": "저도 에어컨에 이은 혁명적인 발명품이라 생각해요",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043616.1.9",
          "form": "ㅋㅋㅋ 군만두 구우면 굿..",
          "original_form": "ㅋㅋㅋ 군만두 구우면 굿..",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043616.1.10",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043616.1.11",
          "form": "남은치킨돌려먹어도",
          "original_form": "남은치킨돌려먹어도",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043616.1.12",
          "form": "남은치킨!!!",
          "original_form": "남은치킨!!!",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043616.1.13",
          "form": "정말 맛있져",
          "original_form": "정말 맛있져",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043616.1.14",
          "form": "프라이드ㅎㅎ맛있어요",
          "original_form": "프라이드ㅎㅎ맛있어요",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043616.1.15",
          "form": "ㅋㅋㅋ 텐더 구워먹을때 너무 맛있어서",
          "original_form": "ㅋㅋㅋ 텐더 구워먹을때 너무 맛있어서",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043616.1.16",
          "form": "아 이제 안사먹어도 되겠다 했어요 ㅋㅋㅋ",
          "original_form": "아 이제 안사먹어도 되겠다 했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043616.1.17",
