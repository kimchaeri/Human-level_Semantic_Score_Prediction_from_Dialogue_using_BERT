      "utterance": [
        {
          "id": "MDRW2100010481.1.1",
          "form": "즐겨드시는 음료가 있나요?",
          "original_form": "즐겨드시는 음료가 있나요?",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010481.1.2",
          "form": "커피 좀 자주마셔요",
          "original_form": "커피 좀 자주마셔요",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010481.1.3",
          "form": "물을 마셔야하는데",
          "original_form": "물을 마셔야하는데",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010481.1.4",
          "form": "자주 드시는거 있으세요?",
          "original_form": "자주 드시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010481.1.5",
          "form": "저도 커피는 하루 두잔 먹어요",
          "original_form": "저도 커피는 하루 두잔 먹어요",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010481.1.6",
          "form": "물을 잘 안드시나요 ?",
          "original_form": "물을 잘 안드시나요 ?",
          "speaker_id": "2",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100010481.1.7",
          "form": "물은 자주먹어야하는데",
          "original_form": "물은 자주먹어야하는데",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100010481.1.8",
          "form": "그게 잘안되네요",
          "original_form": "그게 잘안되네요",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100010481.1.9",
          "form": "저도 물을 잘 마시지 않아요",
          "original_form": "저도 물을 잘 마시지 않아요",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100010481.1.10",
          "form": "몰마시는것이 어렵네요",
          "original_form": "몰마시는것이 어렵네요",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100010481.1.11",
          "form": "하루 1리터 이상마시라고하는데",
          "original_form": "하루 1리터 이상마시라고하는데",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100010481.1.12",
          "form": "500정도먹는거같아요 ㅎ",
          "original_form": "500정도먹는거같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100010481.1.13",
          "form": "저는 500도 안될것 같아요",
          "original_form": "저는 500도 안될것 같아요",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100010481.1.14",
          "form": "설마요 ㅎ",
          "original_form": "설마요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100010481.1.15",
          "form": "저녁은 드셨어요?",
          "original_form": "저녁은 드셨어요?",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100010481.1.16",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100010481.1.17",
          "form": "저두 두유하나 먹고 운동왔는데",
          "original_form": "저두 두유하나 먹고 운동왔는데",
          "speaker_id": "1",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100010481.1.18",
          "form": "운동끝나고나믄 배고플듯요 ㅎ",
          "original_form": "운동끝나고나믄 배고플듯요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100010481.1.19",
          "form": "전 물거의 안먹어요\r\n국국물도 않먹어요",
          "original_form": "전 물거의 안먹어요\r\n국국물도 않먹어요",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100010481.1.20",
          "form": "국국물도요?",
          "original_form": "국국물도요?",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100010481.1.21",
          "form": "와 진짜 500안되시겠어요",
          "original_form": "와 진짜 500안되시겠어요",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100010481.1.22",
          "form": "전 운동 끝나면 과일조금 먹어요",
          "original_form": "전 운동 끝나면 과일조금 먹어요",
          "speaker_id": "2",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100010481.1.23",
          "form": "아 과일",
          "original_form": "아 과일",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100010481.1.24",
          "form": "집에 복숭아 먹어야겠어요",
          "original_form": "집에 복숭아 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100010481.1.25",
          "form": "요즘 포도도 않이 나오죠",
          "original_form": "요즘 포도도 않이 나오죠",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100010481.1.26",
          "form": "네 포도도 맛있더라구요",
          "original_form": "네 포도도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100010481.1.27",
          "form": "얼마전에 샤인머스캣",
          "original_form": "얼마전에 샤인머스캣",
          "speaker_id": "1",
          "time": "20210902 18:40"
        }
      ]
    }
