      "utterance": [
        {
          "id": "MDRW2100018827.1.1",
          "form": "그래 친구는 밖에서만 만나는 걸로",
          "original_form": "그래 친구는 밖에서만 만나는 걸로",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018827.1.2",
          "form": "언니 애들을 위해서 햄스터 키울 일은 없나요",
          "original_form": "언니 애들을 위해서 햄스터 키울 일은 없나요",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018827.1.3",
          "form": "응 없단다",
          "original_form": "응 없단다",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018827.1.4",
          "form": "키운다 하면 집 줄게요",
          "original_form": "키운다 하면 집 줄게요",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.5",
          "form": "햄 찌는 잘 크고 있나??",
          "original_form": "햄 찌는 잘 크고 있나??",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.7",
          "form": "ㄱ 겁나 단호함",
          "original_form": "ㄱ 겁나 단호함",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.8",
          "form": "왜 햄찌는??",
          "original_form": "왜 햄찌는??",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.9",
          "form": "너무 크고 무서워서 별로예요",
          "original_form": "너무 크고 무서워서 별로예요",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.10",
          "form": "응 난 햄스터는 정말 별로임",
          "original_form": "응 난 햄스터는 정말 별로임",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.11",
          "form": "나도 정말 별로 랍니다",
          "original_form": "나도 정말 별로 랍니다",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.12",
          "form": "쥐다 그냥",
          "original_form": "쥐다 그냥",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.13",
          "form": "엄청 오래 살 것 같아 걱정이에요",
          "original_form": "엄청 오래 살 것 같아 걱정이에요",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.15",
          "form": "너희집 첫째는 뭐라고 하니",
          "original_form": "너희집 첫째는 뭐라고 하니",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.16",
          "form": "좋아는 하는데 무서워해요",
          "original_form": "좋아는 하는데 무서워해요",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018827.1.17",
          "form": "무슨 사인지 모르겠어요",
          "original_form": "무슨 사인지 모르겠어요",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018827.1.18",
          "form": "ㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018827.1.19",
          "form": "가깝고도 먼 사이 네",
          "original_form": "가깝고도 먼 사이 네",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018827.1.20",
          "form": "크니까 이제 물려고 해서",
          "original_form": "크니까 이제 물려고 해서",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018827.1.21",
          "form": "그럼 손에 올려놓고 만지고 이런 건 안하나??",
          "original_form": "그럼 손에 올려놓고 만지고 이런 건 안하나??",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018827.1.22",
          "form": "어제 신랑이 때렸어요",
          "original_form": "어제 신랑이 때렸어요",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018827.1.23",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018827.1.24",
          "form": "웁스.....",
          "original_form": "웁스.....",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018827.1.25",
          "form": "햄스터학대네...",
          "original_form": "햄스터학대네...",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018827.1.26",
          "form": "얼마나 크길래 무노",
          "original_form": "얼마나 크길래 무노",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.27",
          "form": "만지는 거 못해요 언니 너무 무서워요",
          "original_form": "만지는 거 못해요 언니 너무 무서워요",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.28",
          "form": "아 진짜??",
          "original_form": "아 진짜??",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.29",
          "form": "그만큼??",
          "original_form": "그만큼??",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.30",
          "form": "앞니도 겁나 길어요",
          "original_form": "앞니도 겁나 길어요",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.31",
          "form": "아...싫다 정말",
          "original_form": "아...싫다 정말",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.32",
          "form": "그리고 너무 빨라서 잘못만 치면 도 주할 것 같아요",
          "original_form": "그리고 너무 빨라서 잘못만 치면 도 주할 것 같아요",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.33",
          "form": "쥐.",
          "original_form": "쥐.",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.34",
          "form": "헐......",
          "original_form": "헐......",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.35",
          "form": "지 맞아요",
          "original_form": "지 맞아요",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.36",
          "form": "도망치면 못 잡는 거가",
          "original_form": "도망치면 못 잡는 거가",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018827.1.37",
          "form": "파는 아줌마 말로는",
          "original_form": "파는 아줌마 말로는",
          "speaker_id": "1",
          "time": "20210827 12:36"
        }
      ]
    }
