      "utterance": [
        {
          "id": "MDRW2100041941.1.1",
          "form": "혹시 반려동물 키우시나용?",
          "original_form": "혹시 반려동물 키우시나용?",
          "speaker_id": "1",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041941.1.2",
          "form": "아뇨 ㅠㅠ 키우고싶은데 환경이 안돼요",
          "original_form": "아뇨 ㅠㅠ 키우고싶은데 환경이 안돼요",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041941.1.3",
          "form": "키으세요?",
          "original_form": "키으세요?",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041941.1.4",
          "form": "저도 환경이ㅜㅜ 예전에 어릴때는 강아지랑 구피 달팽이 이것저것 키웠었어요!",
          "original_form": "저도 환경이ㅜㅜ 예전에 어릴때는 강아지랑 구피 달팽이 이것저것 키웠었어요!",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041941.1.5",
          "form": "구피는 물고기 맞나요?",
          "original_form": "구피는 물고기 맞나요?",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041941.1.6",
          "form": "네네!! 물고기요!",
          "original_form": "네네!! 물고기요!",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041941.1.7",
          "form": "저도 초딩때 키워봤던거같아요 ㅋㅋㅋㅋ",
          "original_form": "저도 초딩때 키워봤던거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041941.1.8",
          "form": "저는 어릴때 뭣모르고 햄스터 키웟는데 지금생각하면 너무 미안해요..",
          "original_form": "저는 어릴때 뭣모르고 햄스터 키웟는데 지금생각하면 너무 미안해요..",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041941.1.9",
          "form": "생명의 귀함을 느끼다보니 함부로 못키우겟어요",
          "original_form": "생명의 귀함을 느끼다보니 함부로 못키우겟어요",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041941.1.10",
          "form": "맞아요..ㅜㅜ",
          "original_form": "맞아요..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041941.1.11",
          "form": "귀엽고 키우고 싶지만 책임질 능력이 안되는거 같아서 ㅠ",
          "original_form": "귀엽고 키우고 싶지만 책임질 능력이 안되는거 같아서 ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041941.1.12",
          "form": "맞아요 정말 통장으로 책임감으로 길러야 하잖아요",
          "original_form": "맞아요 정말 통장으로 책임감으로 길러야 하잖아요",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041941.1.13",
          "form": "저는 키우게된다면 입양해서 키우려구요",
          "original_form": "저는 키우게된다면 입양해서 키우려구요",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041941.1.14",
          "form": "맞아요 통장으로 ㅠㅠ",
          "original_form": "맞아요 통장으로 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041941.1.15",
          "form": "비용부담이 진짜 힘든거 같아요",
          "original_form": "비용부담이 진짜 힘든거 같아요",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041941.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100041941.1.17",
          "form": "달팽이는 어떻게 키워요?",
          "original_form": "달팽이는 어떻게 키워요?",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041941.1.18",
          "form": "어렸을 때라 기억은 잘 안나는데",
          "original_form": "어렸을 때라 기억은 잘 안나는데",
          "speaker_id": "1",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041941.1.19",
          "form": "먹이로 당근이나 상추 애호박 이런거 주고",
          "original_form": "먹이로 당근이나 상추 애호박 이런거 주고",
          "speaker_id": "1",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041941.1.20",
          "form": "등껍질 단단하게 하려고 계란 껍질같은것도 먹여요!",
          "original_form": "등껍질 단단하게 하려고 계란 껍질같은것도 먹여요!",
          "speaker_id": "1",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041941.1.21",
          "form": "진짜 신기하다 ㅋㅋㅋㅋ",
          "original_form": "진짜 신기하다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041941.1.22",
          "form": "잘컷나요?",
          "original_form": "잘컷나요?",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041941.1.23",
          "form": "네네! 거의 손바닥 만하게 컸었어요!",
          "original_form": "네네! 거의 손바닥 만하게 컸었어요!",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041941.1.24",
          "form": "손가락 제외한 손바닥!",
          "original_form": "손가락 제외한 손바닥!",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041941.1.25",
          "form": "우와! 달팽이도 종류가 많던데",
          "original_form": "우와! 달팽이도 종류가 많던데",
          "speaker_id": "2",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100041941.1.26",
          "form": "따로 데려오신거에요?",
          "original_form": "따로 데려오신거에요?",
          "speaker_id": "2",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100041941.1.27",
          "form": "네네 부모님이 데려온거라 정확하겐 모르겠는데 백와 ? 달팽이였던거 같아요!",
          "original_form": "네네 부모님이 데려온거라 정확하겐 모르겠는데 백와 ? 달팽이였던거 같아요!",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100041941.1.28",
          "form": "우와 너무 신기해요!!!",
          "original_form": "우와 너무 신기해요!!!",
          "speaker_id": "2",
          "time": "20210906 20:05"
        }
      ]
    }
  ]
