        },
        {
          "id": "MDRW2100010859.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "1",
          "time": "20210902 08:38"
        },
        {
          "id": "MDRW2100010859.1.3",
          "form": "이스포츠 즐겨보시나요",
          "original_form": "이스포츠 즐겨보시나요",
          "speaker_id": "2",
          "time": "20210902 08:39"
        },
        {
          "id": "MDRW2100010859.1.4",
          "form": "카트라이더 리그 봐요",
          "original_form": "카트라이더 리그 봐요",
          "speaker_id": "1",
          "time": "20210902 08:42"
        },
        {
          "id": "MDRW2100010859.1.5",
          "form": "어느 팀 응원하세요?",
          "original_form": "어느 팀 응원하세요?",
          "speaker_id": "2",
          "time": "20210902 08:44"
        },
        {
          "id": "MDRW2100010859.1.6",
          "form": "전 문호준 선수 응원했었는데",
          "original_form": "전 문호준 선수 응원했었는데",
          "speaker_id": "2",
          "time": "20210902 08:44"
        },
        {
          "id": "MDRW2100010859.1.7",
          "form": "저도요ㅋㅋ 지금은 감독으로 있는 팀 응원해요",
          "original_form": "저도요ㅋㅋ 지금은 감독으로 있는 팀 응원해요",
          "speaker_id": "1",
          "time": "20210902 08:54"
        },
        {
          "id": "MDRW2100010859.1.8",
          "form": "감독직으로 옮기고 나서는 안봤어요",
          "original_form": "감독직으로 옮기고 나서는 안봤어요",
          "speaker_id": "2",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100010859.1.9",
          "form": "선수로 뛰는걸 좋아해서요",
          "original_form": "선수로 뛰는걸 좋아해서요",
          "speaker_id": "2",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100010859.1.10",
          "form": "전 다른 선수들도 응원했어서요",
          "original_form": "전 다른 선수들도 응원했어서요",
          "speaker_id": "1",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100010859.1.11",
          "form": "요새 최영훈 선수도 잘하구요",
          "original_form": "요새 최영훈 선수도 잘하구요",
          "speaker_id": "1",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100010859.1.12",
          "form": "그 팀 요즘에도 상위권인가요",
          "original_form": "그 팀 요즘에도 상위권인가요",
          "speaker_id": "2",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100010859.1.13",
          "form": "리그 계속 뛰던 선수들이라 아무래도 그렇죠",
          "original_form": "리그 계속 뛰던 선수들이라 아무래도 그렇죠",
          "speaker_id": "1",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100010859.1.14",
          "form": "오랜만에 한번 봐야겠네요",
          "original_form": "오랜만에 한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210902 09:10"
        },
        {
          "id": "MDRW2100010859.1.15",
          "form": "카트엔진도 바뀌어서 그 차이도 재밌더라구요",
          "original_form": "카트엔진도 바뀌어서 그 차이도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100010859.1.16",
          "form": "엔진이 또 바뀌었나요ㄷㄷ",
          "original_form": "엔진이 또 바뀌었나요ㄷㄷ",
