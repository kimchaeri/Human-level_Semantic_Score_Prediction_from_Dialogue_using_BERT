      "utterance": [
        {
          "id": "MDRW2100027958.1.1",
          "form": "저는 음...노래마다 달라서 어렵네요",
          "original_form": "저는 음...노래마다 달라서 어렵네요",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027958.1.2",
          "form": "저는 세븐틴 노래 신나더라구요",
          "original_form": "저는 세븐틴 노래 신나더라구요",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027958.1.3",
          "form": "맞아요 세븐틴노래 좋나요",
          "original_form": "맞아요 세븐틴노래 좋나요",
          "speaker_id": "2",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027958.1.4",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027958.1.5",
          "form": "비와서 신나는곡이 좋아요",
          "original_form": "비와서 신나는곡이 좋아요",
          "speaker_id": "1",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027958.1.6",
          "form": "맞아요 축축처져요 ㅠㅠ",
          "original_form": "맞아요 축축처져요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027958.1.7",
          "form": "저도 신나는곡듣고싶은데",
          "original_form": "저도 신나는곡듣고싶은데",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027958.1.8",
          "form": "또 비오는거만 보면 갬성이 있기때믄에 ㅋㅋㅋ",
          "original_form": "또 비오는거만 보면 갬성이 있기때믄에 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027958.1.9",
          "form": "비가 또와요 ㅠㅠ",
          "original_form": "비가 또와요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100027958.1.10",
          "form": "그러세요..비는집에있을땨오는게젤좋은거같어요",
          "original_form": "그러세요..비는집에있을땨오는게젤좋은거같어요",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100027958.1.11",
          "form": "이럴때 공포 영화가 짱인데",
          "original_form": "이럴때 공포 영화가 짱인데",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100027958.1.12",
          "form": "어유 저는 무서워서 울어요",
          "original_form": "어유 저는 무서워서 울어요",
          "speaker_id": "2",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100027958.1.13",
          "form": "고럼 어떤 장르 좋아하세요",
          "original_form": "고럼 어떤 장르 좋아하세요",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100027958.1.14",
