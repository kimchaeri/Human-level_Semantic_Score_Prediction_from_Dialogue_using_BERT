      "utterance": [
        {
          "id": "MDRW2100023448.1.1",
          "form": "가정용 3~4구 삿어",
          "original_form": "가정용 3~4구 삿어",
          "speaker_id": "1",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100023448.1.2",
          "form": "나 그거 할아버지댁에 가져가려고",
          "original_form": "나 그거 할아버지댁에 가져가려고",
          "speaker_id": "2",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100023448.1.3",
          "form": "한구만",
          "original_form": "한구만",
          "speaker_id": "2",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100023448.1.4",
          "form": "그걸로 가성비예쁜손녀하려구",
          "original_form": "그걸로 가성비예쁜손녀하려구",
          "speaker_id": "2",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100023448.1.5",
          "form": "엄마 나 아빠 할아버지 외할아버지 지금 다 못먹어봄",
          "original_form": "엄마 나 아빠 할아버지 외할아버지 지금 다 못먹어봄",
          "speaker_id": "2",
          "time": "20210830 03:16"
        },
        {
          "id": "MDRW2100023448.1.6",
          "form": "헉 진짜 이쁜 손녀네",
          "original_form": "헉 진짜 이쁜 손녀네",
          "speaker_id": "1",
          "time": "20210830 03:16"
        },
        {
          "id": "MDRW2100023448.1.7",
          "form": "1/3이나 드리는거가",
          "original_form": "1/3이나 드리는거가",
          "speaker_id": "1",
          "time": "20210830 03:16"
        },
        {
          "id": "MDRW2100023448.1.8",
          "form": "우리 할머니도 아마 못드셔봤을거같은데",
          "original_form": "우리 할머니도 아마 못드셔봤을거같은데",
          "speaker_id": "1",
          "time": "20210830 03:16"
        },
        {
          "id": "MDRW2100023448.1.9",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.10",
          "form": "난 할머니 드린다는 발상 사실 해본적도 없어",
          "original_form": "난 할머니 드린다는 발상 사실 해본적도 없어",
          "speaker_id": "1",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.11",
          "form": "나는근데 갈때마다 용돈을받는다",
          "original_form": "나는근데 갈때마다 용돈을받는다",
          "speaker_id": "2",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.12",
          "form": "그래서 갈때마다 롤케익을 사간다",
          "original_form": "그래서 갈때마다 롤케익을 사간다",
          "speaker_id": "2",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.13",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.14",
          "form": "그럼 샤인머스켓 정도는 드려도 될듯...",
          "original_form": "그럼 샤인머스켓 정도는 드려도 될듯...",
          "speaker_id": "1",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.15",
          "form": "이번엔 샤인머스켓으로 바뀐것뿐",
          "original_form": "이번엔 샤인머스켓으로 바뀐것뿐",
          "speaker_id": "2",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.16",
          "form": "용돈을 주시며는....",
          "original_form": "용돈을 주시며는....",
          "speaker_id": "1",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.18",
          "form": "많이주시는건아니고 5~10정도",
          "original_form": "많이주시는건아니고 5~10정도",
          "speaker_id": "2",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.19",
          "form": "완전 효녀구만",
          "original_form": "완전 효녀구만",
          "speaker_id": "1",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.20",
          "form": "갈때마다 그정도면 많이주시는거맞는디??",
          "original_form": "갈때마다 그정도면 많이주시는거맞는디??",
          "speaker_id": "1",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.21",
          "form": "요양보호사님 일본갈때도 30씩 드렸대",
          "original_form": "요양보호사님 일본갈때도 30씩 드렸대",
          "speaker_id": "2",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.22",
          "form": "내가자주안가",
          "original_form": "내가자주안가",
          "speaker_id": "2",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.23",
          "form": "울 할머니는 일년에 3번정도 5만원씩 주셔",
          "original_form": "울 할머니는 일년에 3번정도 5만원씩 주셔",
          "speaker_id": "1",
          "time": "20210830 03:17"
        },
        {
          "id": "MDRW2100023448.1.24",
          "form": "연금이 한달에 300가까이나오다보니...",
          "original_form": "연금이 한달에 300가까이나오다보니...",
          "speaker_id": "2",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.25",
          "form": "설날 추석 할아버지제사",
          "original_form": "설날 추석 할아버지제사",
          "speaker_id": "1",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.26",
          "form": "나 일년에 3번안가",
          "original_form": "나 일년에 3번안가",
          "speaker_id": "2",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.27",
          "form": "ㄷㄷㄷㄷㄷ일본 놀러갈때 30이나 챙겨주셔...?",
          "original_form": "ㄷㄷㄷㄷㄷ일본 놀러갈때 30이나 챙겨주셔...?",
          "speaker_id": "1",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.28",
          "form": "비슷하게받네...너도갈때마다 롤케이크사가라",
          "original_form": "비슷하게받네...너도갈때마다 롤케이크사가라",
          "speaker_id": "2",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.29",
          "form": "와 확실히 연금을 받으시니까 여유로우시다",
          "original_form": "와 확실히 연금을 받으시니까 여유로우시다",
          "speaker_id": "1",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.30",
          "form": "연금이 큰거같아",
          "original_form": "연금이 큰거같아",
          "speaker_id": "2",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.31",
          "form": "난 할머니가 나 싫어해서 안가",
          "original_form": "난 할머니가 나 싫어해서 안가",
          "speaker_id": "1",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.32",
          "form": "할머니 만날때마다 내가 고모들 욕해서",
          "original_form": "할머니 만날때마다 내가 고모들 욕해서",
          "speaker_id": "1",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.33",
          "form": "너도 나중에 연금받으면 나랑 같이 살아",
          "original_form": "너도 나중에 연금받으면 나랑 같이 살아",
          "speaker_id": "2",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.34",
          "form": "할머니가 이제 날 시러해....",
          "original_form": "할머니가 이제 날 시러해....",
          "speaker_id": "1",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.35",
          "form": "기생할래",
          "original_form": "기생할래",
          "speaker_id": "2",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.36",
          "form": "그치만 고모들 욕하는거 재밋어서 계속하고잇어",
          "original_form": "그치만 고모들 욕하는거 재밋어서 계속하고잇어",
          "speaker_id": "1",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.37",
          "form": "ㅠㅠ아냐속으론예뻐하실거야",
          "original_form": "ㅠㅠ아냐속으론예뻐하실거야",
          "speaker_id": "2",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100023448.1.38",
          "form": "ㅋㅋ그렇구나",
          "original_form": "ㅋㅋ그렇구나",
          "speaker_id": "2",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.39",
          "form": "할인소식...",
          "original_form": "할인소식...",
          "speaker_id": "2",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ기생 노!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ기생 노!",
          "speaker_id": "1",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.41",
          "form": "난 실버타운에 기생할거야",
          "original_form": "난 실버타운에 기생할거야",
          "speaker_id": "1",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.42",
          "form": "아맞다 왓챠팝콘이라고앎?",
          "original_form": "아맞다 왓챠팝콘이라고앎?",
          "speaker_id": "2",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.43",
          "form": "아 기생노 빈정상해",
          "original_form": "아 기생노 빈정상해",
          "speaker_id": "2",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.44",
          "form": "왓챠팝콘이 뭐야??",
          "original_form": "왓챠팝콘이 뭐야??",
          "speaker_id": "1",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.45",
          "form": "미안해...",
          "original_form": "미안해...",
          "speaker_id": "1",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.46",
          "form": "내가백수라고또무시하네",
          "original_form": "내가백수라고또무시하네",
          "speaker_id": "2",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.47",
          "form": "왓챠팝콘 2000원팝콘인데",
          "original_form": "왓챠팝콘 2000원팝콘인데",
          "speaker_id": "2",
          "time": "20210830 03:19"
        },
        {
          "id": "MDRW2100023448.1.48",
          "form": "왓챠이용권이 랜덤으로들어있어",
          "original_form": "왓챠이용권이 랜덤으로들어있어",
          "speaker_id": "2",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.49",
          "form": "2주/1개월/3개월 골든티켓 1년",
          "original_form": "2주/1개월/3개월 골든티켓 1년",
          "speaker_id": "2",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.50",
          "form": "아냐.... 난 무시한적없어...",
          "original_form": "아냐.... 난 무시한적없어...",
          "speaker_id": "1",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.51",
          "form": "전에 일 어쩌구 그것도 오해엿자나 너의....!",
          "original_form": "전에 일 어쩌구 그것도 오해엿자나 너의....!",
          "speaker_id": "1",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.52",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.53",
          "form": "어디서 파는건데",
          "original_form": "어디서 파는건데",
          "speaker_id": "1",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.54",
          "form": "ㄱㅊ지?",
          "original_form": "ㄱㅊ지?",
          "speaker_id": "2",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.55",
          "form": "이마트24",
          "original_form": "이마트24",
          "speaker_id": "2",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.56",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.57",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.58",
          "form": "ㅎㅎ믿어주겠다",
          "original_form": "ㅎㅎ믿어주겠다",
          "speaker_id": "2",
          "time": "20210830 03:20"
        },
        {
          "id": "MDRW2100023448.1.59",
          "form": "대신에 왓챠팝콘이벤트로한거는",
          "original_form": "대신에 왓챠팝콘이벤트로한거는",
          "speaker_id": "2",
          "time": "20210830 03:21"
        },
        {
          "id": "MDRW2100023448.1.60",
          "form": "헐헐",
          "original_form": "헐헐",
          "speaker_id": "1",
          "time": "20210830 03:21"
        },
        {
          "id": "MDRW2100023448.1.61",
          "form": "왓챠팝콘 사면백프로 들어잇대",
          "original_form": "왓챠팝콘 사면백프로 들어잇대",
          "speaker_id": "1",
          "time": "20210830 03:21"
        },
        {
          "id": "MDRW2100023448.1.62",
          "form": "이용권이",
          "original_form": "이용권이",
          "speaker_id": "1",
          "time": "20210830 03:21"
        },
        {
          "id": "MDRW2100023448.1.63",
          "form": "이벤트당 한번밖에등록안됨",
          "original_form": "이벤트당 한번밖에등록안됨",
          "speaker_id": "2",
          "time": "20210830 03:21"
        },
        {
          "id": "MDRW2100023448.1.64",
