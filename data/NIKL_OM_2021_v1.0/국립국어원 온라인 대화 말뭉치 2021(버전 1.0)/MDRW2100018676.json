        {
          "id": "MDRW2100018676.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100018676.1.2",
          "form": "별로안해요",
          "original_form": "별로안해요",
          "speaker_id": "2",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018676.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018676.1.4",
          "form": "전 청소보단 설거지 좋아해요 ㅋㅋㅋ",
          "original_form": "전 청소보단 설거지 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018676.1.5",
          "form": "화장실청소가가장 힘들죠",
          "original_form": "화장실청소가가장 힘들죠",
          "speaker_id": "1",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100018676.1.6",
          "form": "사용해보니 추천할만한 청소용품 있으신가요?",
          "original_form": "사용해보니 추천할만한 청소용품 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100018676.1.7",
          "form": "락스가 최공요",
          "original_form": "락스가 최공요",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100018676.1.8",
          "form": "락스와 세제라는 제품 괜찮았어요 ㅋㅋ  이름이 락스와 세제에요 ㅋㅋ",
          "original_form": "락스와 세제라는 제품 괜찮았어요 ㅋㅋ  이름이 락스와 세제에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100018676.1.9",
          "form": "그른가요?",
          "original_form": "그른가요?",
          "speaker_id": "1",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100018676.1.10",
          "form": "청소를 잘하는 비결이 있으신가요",
          "original_form": "청소를 잘하는 비결이 있으신가요",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100018676.1.11",
          "form": "환기잘해도 곰팡이생길까보ㅓ",
          "original_form": "환기잘해도 곰팡이생길까보ㅓ",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100018676.1.12",
          "form": "벽지에 곰팡이 생긴적있는데 튜브에 든 젤형태 락스로 다 없앴었어요",
          "original_form": "벽지에 곰팡이 생긴적있는데 튜브에 든 젤형태 락스로 다 없앴었어요",
