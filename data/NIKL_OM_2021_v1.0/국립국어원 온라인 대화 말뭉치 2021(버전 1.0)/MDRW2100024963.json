      "utterance": [
        {
          "id": "MDRW2100024963.1.1",
          "form": "요즘 읽은 것 중에 인상 깊은 거 있으세요",
          "original_form": "요즘 읽은 것 중에 인상 깊은 거 있으세요",
          "speaker_id": "1",
          "time": "20210901 13:12"
        },
        {
          "id": "MDRW2100024963.1.2",
          "form": "요즘 책 읽은지 너무 오래됐네요",
          "original_form": "요즘 책 읽은지 너무 오래됐네요",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100024963.1.3",
          "form": "책제목은 기억안나는데 토끼캐릭한 여행기를 재미지게 봤네요",
          "original_form": "책제목은 기억안나는데 토끼캐릭한 여행기를 재미지게 봤네요",
          "speaker_id": "2",
          "time": "20210901 13:30"
        },
        {
          "id": "MDRW2100024963.1.4",
          "form": "오 뭔가 귀엽네요 동화책인가요",
          "original_form": "오 뭔가 귀엽네요 동화책인가요",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100024963.1.5",
          "form": "삽화있는 수필이더라요",
          "original_form": "삽화있는 수필이더라요",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100024963.1.6",
          "form": "오 뭔가 깜찍하겠네요",
          "original_form": "오 뭔가 깜찍하겠네요",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100024963.1.7",
          "form": "저는 요즘 뭔가 철학책 읽어보고 싶다는 생각이 들어 읽는 중인데",
          "original_form": "저는 요즘 뭔가 철학책 읽어보고 싶다는 생각이 들어 읽는 중인데",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100024963.1.8",
          "form": "위트있게 적어둔 책이라 나름 술술 읽히네요",
          "original_form": "위트있게 적어둔 책이라 나름 술술 읽히네요",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100024963.1.9",
          "form": "소크라테스 익스프레스라고",
          "original_form": "소크라테스 익스프레스라고",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100024963.1.10",
          "form": "기차여행하면서 철학자들을 생각하는 내용이에요",
          "original_form": "기차여행하면서 철학자들을 생각하는 내용이에요",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100024963.1.11",
          "form": "장바구니에 넣었어요",
          "original_form": "장바구니에 넣었어요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100024963.1.12",
          "form": "오ㅎㅎㅎ 바로 넣으시다니",
          "original_form": "오ㅎㅎㅎ 바로 넣으시다니",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100024963.1.13",
          "form": "호기심 자극하는 내용이여서",
          "original_form": "호기심 자극하는 내용이여서",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100024963.1.14",
          "form": "네 뭔가 생각하게 만드는 책이네요ㅋㅋㅋ",
          "original_form": "네 뭔가 생각하게 만드는 책이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100024963.1.15",
          "form": "가을인데. 꼭 읽어봐야겠어요",
          "original_form": "가을인데. 꼭 읽어봐야겠어요",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100024963.1.16",
          "form": "ㅋㅋㅋ그리고 또 최근에 만화책을 하나 읽었는데",
          "original_form": "ㅋㅋㅋ그리고 또 최근에 만화책을 하나 읽었는데",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100024963.1.17",
          "form": "위국일기라고 되게 감정표현 섬세해서",
          "original_form": "위국일기라고 되게 감정표현 섬세해서",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100024963.1.18",
          "form": "수필이나 소설책 읽는 느낌들었어요",
          "original_form": "수필이나 소설책 읽는 느낌들었어요",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100024963.1.19",
          "form": "오~ 만화방에 있으면 봐야겠어요",
          "original_form": "오~ 만화방에 있으면 봐야겠어요",
          "speaker_id": "2",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100024963.1.20",
          "form": "만화방에 금액도 남아있는데",
          "original_form": "만화방에 금액도 남아있는데",
          "speaker_id": "2",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100024963.1.21",
          "form": "아 만화방 자주 가시나요 ㅋㅋ",
          "original_form": "아 만화방 자주 가시나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:39"
        },
        {
          "id": "MDRW2100024963.1.22",
          "form": "요즘엔 못가는데",
          "original_form": "요즘엔 못가는데",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100024963.1.23",
          "form": "예전에는 하루 날잡아서 하루죙일 있는 타입이요",
          "original_form": "예전에는 하루 날잡아서 하루죙일 있는 타입이요",
          "speaker_id": "2",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100024963.1.24",
          "form": "하루종일 맛난거 먹으몀서 읽는거 좋은데",
          "original_form": "하루종일 맛난거 먹으몀서 읽는거 좋은데",
          "speaker_id": "1",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100024963.1.25",
          "form": "요즘은 못가서 너무 아쉬워요",
          "original_form": "요즘은 못가서 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100024963.1.26",
          "form": "넹 특히 이런꾸물꾸물한 날",
          "original_form": "넹 특히 이런꾸물꾸물한 날",
          "speaker_id": "2",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100024963.1.27",
          "form": "라면에 오징어에",
          "original_form": "라면에 오징어에",
          "speaker_id": "2",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100024963.1.28",
          "form": "크으으 조만간 가야겠네요 못참겠다",
          "original_form": "크으으 조만간 가야겠네요 못참겠다",
          "speaker_id": "1",
          "time": "20210901 17:02"
        }
      ]
    }
