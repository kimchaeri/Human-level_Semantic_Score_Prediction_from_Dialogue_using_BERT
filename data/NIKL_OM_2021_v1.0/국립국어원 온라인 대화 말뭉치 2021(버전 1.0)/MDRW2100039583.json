      "utterance": [
        {
          "id": "MDRW2100039583.1.1",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039583.1.2",
          "form": "랜선집사 생활 중이시군요",
          "original_form": "랜선집사 생활 중이시군요",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039583.1.3",
          "form": "저도 다른 동물들 키워보고 싶은데 거북이 하나로도 벅차네요..ㅎㅎ",
          "original_form": "저도 다른 동물들 키워보고 싶은데 거북이 하나로도 벅차네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039583.1.4",
          "form": "강아지 만약에 키우게 되면 이름 뭘로 짓고 싶으세요?",
          "original_form": "강아지 만약에 키우게 되면 이름 뭘로 짓고 싶으세요?",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039583.1.5",
          "form": "저는.... 음 뿌꾸요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는.... 음 뿌꾸요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039583.1.6",
          "form": "그냥 뭔가 귀여울 것 같아요ㅋㅋㅋ",
          "original_form": "그냥 뭔가 귀여울 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039583.1.7",
          "form": "ㅋㅋㅋ귀엽네요!",
          "original_form": "ㅋㅋㅋ귀엽네요!",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039583.1.8",
          "form": "하하 거북이들 이름은 뭔가요?",
          "original_form": "하하 거북이들 이름은 뭔가요?",
          "speaker_id": "1",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039583.1.9",
          "form": "망고랑 수박이에요!",
          "original_form": "망고랑 수박이에요!",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039583.1.10",
          "form": "헉 너무 귀엽네요!!",
          "original_form": "헉 너무 귀엽네요!!",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039583.1.11",
          "form": "성별은 뭐예요???",
          "original_form": "성별은 뭐예요???",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039583.1.12",
          "form": "성별은 구별이 힘들어서 아직도 모르겠습니다.....ㅋㅋ... ..",
          "original_form": "성별은 구별이 힘들어서 아직도 모르겠습니다.....ㅋㅋ... ..",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039583.1.13",
          "form": "약간 눈에 딱 띄는 차이가 없어서용",
          "original_form": "약간 눈에 딱 띄는 차이가 없어서용",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039583.1.14",
          "form": "아아아 잘 알 수 있는게 아니군요..!!",
          "original_form": "아아아 잘 알 수 있는게 아니군요..!!",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039583.1.15",
          "form": "뭔가 생김새라도 다를 줄 알았어요ㅋㅋㅋ",
          "original_form": "뭔가 생김새라도 다를 줄 알았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039583.1.16",
          "form": "그런건 아니더라구요! ㅎㅎ",
          "original_form": "그런건 아니더라구요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100039583.1.17",
          "form": "자주 보시는 동물 유튜브 뭐뭐있으세용?",
          "original_form": "자주 보시는 동물 유튜브 뭐뭐있으세용?",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100039583.1.18",
          "form": "저는 가끔 애니뭘봐?그거 보구 스펀지 유튜브에서 동물 관련 내용 클립 나오더라구요ㅋㅋㅋ 그거 가끔 봐요!",
          "original_form": "저는 가끔 애니뭘봐?그거 보구 스펀지 유튜브에서 동물 관련 내용 클립 나오더라구요ㅋㅋㅋ 그거 가끔 봐요!",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100039583.1.19",
          "form": "아 애니뭘봐 재밌죠!! ㅋㅋ",
          "original_form": "아 애니뭘봐 재밌죠!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100039583.1.20",
          "form": "저 조류공포증 있는데",
          "original_form": "저 조류공포증 있는데",
          "speaker_id": "2",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100039583.1.21",
          "form": "애니뭘봐에 나오는 새 이야기는 너무 웃겨서",
          "original_form": "애니뭘봐에 나오는 새 이야기는 너무 웃겨서",
          "speaker_id": "2",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100039583.1.22",
          "form": "창 제일 작게 해놓고 봐요 ㅋㅋ",
          "original_form": "창 제일 작게 해놓고 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100039583.1.23",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100039583.1.24",
          "form": "저도 새는 잘 못봐요ㅜㅜㅠ 저는 새 눈이 너무 싫어요,,,",
          "original_form": "저도 새는 잘 못봐요ㅜㅜㅠ 저는 새 눈이 너무 싫어요,,,",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100039583.1.25",
          "form": "새 눈도 무섭구 깃털들이 왠지 모르게 소름끼쳐요",
          "original_form": "새 눈도 무섭구 깃털들이 왠지 모르게 소름끼쳐요",
          "speaker_id": "2",
          "time": "20210904 14:34"
        },
        {
          "id": "MDRW2100039583.1.26",
          "form": "여러모로 많이 무셔용..ㅋㅋ ㅠㅠ",
          "original_form": "여러모로 많이 무셔용..ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039583.1.27",
          "form": "근데 막 안날고 뛰어다니는 독수리 같은 애들은",
          "original_form": "근데 막 안날고 뛰어다니는 독수리 같은 애들은",
          "speaker_id": "2",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039583.1.28",
          "form": "너무 궁금하고 그래서 봤어요....ㅋㅋㅋ",
          "original_form": "너무 궁금하고 그래서 봤어요....ㅋㅋㅋ",
