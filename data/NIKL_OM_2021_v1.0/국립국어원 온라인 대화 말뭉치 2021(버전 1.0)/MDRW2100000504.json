      "utterance": [
        {
          "id": "MDRW2100000504.1.1",
          "form": "이번주 주말에 비가 온대요",
          "original_form": "이번주 주말에 비가 온대요",
          "speaker_id": "1",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000504.1.2",
          "form": "안그래도 다음주까지 비가 엄청 오더라구요",
          "original_form": "안그래도 다음주까지 비가 엄청 오더라구요",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000504.1.3",
          "form": "근데 일기예보는 자주 틀려서...",
          "original_form": "근데 일기예보는 자주 틀려서...",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000504.1.4",
          "form": "비 온다고했다가 안오는거보단",
          "original_form": "비 온다고했다가 안오는거보단",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000504.1.5",
          "form": "비 안온다고 했는데 비오는게 더 짜증나요",
          "original_form": "비 안온다고 했는데 비오는게 더 짜증나요",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000504.1.6",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000504.1.7",
          "form": "근데 반대로일때도 우산 손에 들고다니면",
          "original_form": "근데 반대로일때도 우산 손에 들고다니면",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000504.1.8",
          "form": "엄청 귀찮아요ㅠㅠ",
          "original_form": "엄청 귀찮아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000504.1.9",
          "form": "저는 맨날 가방들고다녀서 안에 넣긴하는데 챙기기 귀찮긴해요.. 가방 용량도 차지하고..",
          "original_form": "저는 맨날 가방들고다녀서 안에 넣긴하는데 챙기기 귀찮긴해요.. 가방 용량도 차지하고..",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000504.1.10",
          "form": "와 부지런하시네요",
          "original_form": "와 부지런하시네요",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000504.1.11",
          "form": "그리고 부쩍 덜더워진 것 같아요",
          "original_form": "그리고 부쩍 덜더워진 것 같아요",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000504.1.12",
          "form": "맞아요 오늘 아침에 나오는데 선선하더라고요",
          "original_form": "맞아요 오늘 아침에 나오는데 선선하더라고요",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000504.1.13",
          "form": "에어컨 안켜도 될것같아요",
          "original_form": "에어컨 안켜도 될것같아요",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000504.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000504.1.15",
          "form": "근데 또 습해서 버스에서는 에어컨 필수더라구요",
          "original_form": "근데 또 습해서 버스에서는 에어컨 필수더라구요",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000504.1.16",
          "form": "습한가요??? 아침에는 그렇게 습한건 아닌데 비오는날 저녁엔 많이 습해지는거같긴해요..",
          "original_form": "습한가요??? 아침에는 그렇게 습한건 아닌데 비오는날 저녁엔 많이 습해지는거같긴해요..",
          "speaker_id": "1",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000504.1.17",
          "form": "맞아요 저녁에 좀 습한 것 같아요",
          "original_form": "맞아요 저녁에 좀 습한 것 같아요",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000504.1.18",
          "form": "날씨가 덜더워도 습하면",
          "original_form": "날씨가 덜더워도 습하면",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000504.1.19",
          "form": "조금만 걸어도 땀이 나더라구요",
          "original_form": "조금만 걸어도 땀이 나더라구요",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000504.1.20",
          "form": "취미가 자전거라 퇴근하고 맨날 타는데",
          "original_form": "취미가 자전거라 퇴근하고 맨날 타는데",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000504.1.21",
          "form": "햇빛 쨍쨍할때보다 습할때가 땀이 엄청 많이나더라고요",
          "original_form": "햇빛 쨍쨍할때보다 습할때가 땀이 엄청 많이나더라고요",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000504.1.22",
          "form": "아 취미가 자전거시면 날씨에 예민하시겠네요",
          "original_form": "아 취미가 자전거시면 날씨에 예민하시겠네요",
          "speaker_id": "2",
          "time": "20210819 16:50"
        },
        {
          "id": "MDRW2100000504.1.23",
          "form": "요즘은 맨날 들어가면 땀범벅이에요 ㅠㅠ 햇빛을 너무받아서",
          "original_form": "요즘은 맨날 들어가면 땀범벅이에요 ㅠㅠ 햇빛을 너무받아서",
          "speaker_id": "1",
          "time": "20210819 16:52"
        }
      ]
    }
