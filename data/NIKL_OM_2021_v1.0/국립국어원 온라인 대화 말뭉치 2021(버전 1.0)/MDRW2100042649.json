      "utterance": [
        {
          "id": "MDRW2100042649.1.1",
          "form": "아, 그 강다녤이 엠씨인거?",
          "original_form": "아, 그 강다녤이 엠씨인거?",
          "speaker_id": "2",
          "time": "20210907 00:21"
        },
        {
          "id": "MDRW2100042649.1.2",
          "form": "중고태용이 심사위원이라며",
          "original_form": "중고태용이 심사위원이라며",
          "speaker_id": "2",
          "time": "20210907 00:21"
        },
        {
          "id": "MDRW2100042649.1.3",
          "form": "ㅇㅇㅇ강다녤이 엠씨인거!",
          "original_form": "ㅇㅇㅇ강다녤이 엠씨인거!",
          "speaker_id": "1",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042649.1.4",
          "form": "그니까... 중고태용이 누가 누굴 심사한다고",
          "original_form": "그니까... 중고태용이 누가 누굴 심사한다고",
          "speaker_id": "1",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042649.1.5",
          "form": "거기 나오는 애들이 실력 훨씬 나을텐데...",
          "original_form": "거기 나오는 애들이 실력 훨씬 나을텐데...",
          "speaker_id": "1",
          "time": "20210907 00:22"
        },
        {
          "id": "MDRW2100042649.1.6",
          "form": "같은 아이돌인 채연은 참가자 아님?",
          "original_form": "같은 아이돌인 채연은 참가자 아님?",
          "speaker_id": "2",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042649.1.7",
          "form": "소속사빨인가",
          "original_form": "소속사빨인가",
          "speaker_id": "2",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042649.1.8",
          "form": "ㅇㅇㅇ채연은 참가자 ㅋㅋㅋ",
          "original_form": "ㅇㅇㅇ채연은 참가자 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100042649.1.9",
          "form": "근대 태용 춤 보니까 채연이 낫던데",
          "original_form": "근대 태용 춤 보니까 채연이 낫던데",
          "speaker_id": "1",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100042649.1.10",
          "form": "소속사빨 ㅇㅈ...거기 보아도 나오고 안무가도 sm소속이래",
          "original_form": "소속사빨 ㅇㅈ...거기 보아도 나오고 안무가도 sm소속이래",
          "speaker_id": "1",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100042649.1.11",
          "form": "완전 스엠파뤼",
          "original_form": "완전 스엠파뤼",
          "speaker_id": "1",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100042649.1.12",
          "form": "ㅋㅋㅋㅋㅋ\r\n요즘 노래는 뭐 들음??",
          "original_form": "ㅋㅋㅋㅋㅋ\r\n요즘 노래는 뭐 들음??",
          "speaker_id": "2",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100042649.1.13",
          "form": "요즘 진짜 듣던거 계속 듣고 그러는듯ㅋㅋㅋㅋ",
          "original_form": "요즘 진짜 듣던거 계속 듣고 그러는듯ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100042649.1.14",
          "form": "볼빨간사춘기랑 아이유 노래 마니 들어 ㅋㅋㅋㅋ",
          "original_form": "볼빨간사춘기랑 아이유 노래 마니 들어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100042649.1.15",
          "form": "오, 옛날부터 좋아하던 가수들이네",
          "original_form": "오, 옛날부터 좋아하던 가수들이네",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100042649.1.16",
          "form": "최근에 영화는 본거있음??",
          "original_form": "최근에 영화는 본거있음??",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100042649.1.17",
          "form": "ㅇㅇㅋㅋㅋㅋ 나는...스테디하다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋ 나는...스테디하다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100042649.1.18",
          "form": "최근에 영화본거 없는듯... 영화관 자체를 안간거같어! 너는?",
          "original_form": "최근에 영화본거 없는듯... 영화관 자체를 안간거같어! 너는?",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100042649.1.19",
          "form": "나 최근에 모가디슈 싱크홀 올드 인질 봄ㅋㅋㅋ",
          "original_form": "나 최근에 모가디슈 싱크홀 올드 인질 봄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100042649.1.20",
          "form": "오 모가디슈 유명하더만ㅋㅋㅋㅋ 코로나만 아니었음 흥했을거라고",
          "original_form": "오 모가디슈 유명하더만ㅋㅋㅋㅋ 코로나만 아니었음 흥했을거라고",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100042649.1.21",
          "form": "닌 꽤 많이 봤네??",
          "original_form": "닌 꽤 많이 봤네??",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100042649.1.22",
          "form": "ㅇㅇ 엄마가 영화관 가는 거 좋아혀",
          "original_form": "ㅇㅇ 엄마가 영화관 가는 거 좋아혀",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100042649.1.23",
          "form": "ㅋㅋㅋㅋ이욜~~",
          "original_form": "ㅋㅋㅋㅋ이욜~~",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100042649.1.24",
          "form": "어무니랑 같이 문화생활 잘 누리누ㅎ 조타",
          "original_form": "어무니랑 같이 문화생활 잘 누리누ㅎ 조타",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100042649.1.25",
          "form": "너는 넷플릭스나 왓챠 봄??",
          "original_form": "너는 넷플릭스나 왓챠 봄??",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
