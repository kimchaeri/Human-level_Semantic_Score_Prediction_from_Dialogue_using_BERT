      "utterance": [
        {
          "id": "MDRW2100006934.1.1",
          "form": "동물좋아하시나요",
          "original_form": "동물좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006934.1.2",
          "form": "네!!! 특히 고양이를 무척 좋아해요",
          "original_form": "네!!! 특히 고양이를 무척 좋아해요",
          "speaker_id": "2",
          "time": "20210822 07:20"
        },
        {
          "id": "MDRW2100006934.1.3",
          "form": "그래서 임보도 여러마리 맡아서 하구 새 가족도 찾아주고 한답니다 ㅋㅋㅋㅋ",
          "original_form": "그래서 임보도 여러마리 맡아서 하구 새 가족도 찾아주고 한답니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 07:20"
        },
        {
          "id": "MDRW2100006934.1.4",
          "form": "대단해요",
          "original_form": "대단해요",
          "speaker_id": "1",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100006934.1.5",
          "form": "임보진짜 어려운데",
          "original_form": "임보진짜 어려운데",
          "speaker_id": "1",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100006934.1.6",
          "form": "우리는 래브라도리트리버 골든리트리버키워요",
          "original_form": "우리는 래브라도리트리버 골든리트리버키워요",
          "speaker_id": "1",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100006934.1.7",
          "form": "주택이라서요ㅎ",
          "original_form": "주택이라서요ㅎ",
          "speaker_id": "1",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100006934.1.8",
          "form": "헉 진짜 부러워요!!!!!!!!",
          "original_form": "헉 진짜 부러워요!!!!!!!!",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100006934.1.9",
          "form": "저도 주택에서 사는게 꿈입니다ㅜㅜ",
          "original_form": "저도 주택에서 사는게 꿈입니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100006934.1.10",
          "form": "너무 귀엽겠어요...",
          "original_form": "너무 귀엽겠어요...{emoji:♡}",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100006934.1.11",
          "form": "마당이넓어요 100평",
          "original_form": "마당이넓어요 100평",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100006934.1.12",
          "form": "애기들 활동량 많아서 아침저녁 산책시키는데",
          "original_form": "애기들 활동량 많아서 아침저녁 산책시키는데",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100006934.1.13",
          "form": "그거로부족해서 마당에서 뛰어놀아요",
          "original_form": "그거로부족해서 마당에서 뛰어놀아요",
          "speaker_id": "1",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100006934.1.14",
          "form": "와 진짜 넓네요!!!",
          "original_form": "와 진짜 넓네요!!!",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100006934.1.15",
          "form": "역시 동물들은 마당있는 집에서 함께 살아야해요ㅜㅜ",
          "original_form": "역시 동물들은 마당있는 집에서 함께 살아야해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100006934.1.16",
          "form": "시골로 이사왔어요ㅋㅋ\r\n애기도키울겸",
          "original_form": "시골로 이사왔어요ㅋㅋ\r\n애기도키울겸",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100006934.1.17",
          "form": "제 꿈이에요 진심 ㅋㅋㅋㅋㅋ",
          "original_form": "제 꿈이에요 진심 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100006934.1.18",
          "form": "저는 마당있는데서 고양이까지 함께...",
          "original_form": "저는 마당있는데서 고양이까지 함께...",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100006934.1.19",
          "form": "집에 큰창을 만들어서 개들이 마당이랑 집 오가요",
          "original_form": "집에 큰창을 만들어서 개들이 마당이랑 집 오가요",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100006934.1.20",
          "form": "임보는 정말 대단해요\r\n경계도 풀어야되고 정쌓으면 보내야되고",
          "original_form": "임보는 정말 대단해요\r\n경계도 풀어야되고 정쌓으면 보내야되고",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100006934.1.21",
          "form": "마음이아플거같아요",
          "original_form": "마음이아플거같아요",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100006934.1.22",
          "form": "좀 그런 면이 있죠 ㅜㅜ",
          "original_form": "좀 그런 면이 있죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100006934.1.23",
          "form": "하지만 그만큼 좋은 가족에게 가는거라서",
          "original_form": "하지만 그만큼 좋은 가족에게 가는거라서",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100006934.1.24",
          "form": "좋게 생각하고있어요+",
          "original_form": "좋게 생각하고있어요+",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100006934.1.25",
          "form": "마인드가 멋있어요",
          "original_form": "마인드가 멋있어요",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100006934.1.26",
          "form": "저는 대형견2마리에 애기까지 있어서 임보못할것같은데",
          "original_form": "저는 대형견2마리에 애기까지 있어서 임보못할것같은데",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100006934.1.27",
          "form": "그 전에도 해본적은없어요",
          "original_form": "그 전에도 해본적은없어요",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100006934.1.28",
          "form": "학대견입양에서 키우고 유기견입양해서 키워본적은있어요",
          "original_form": "학대견입양에서 키우고 유기견입양해서 키워본적은있어요",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100006934.1.29",
          "form": "그쵸그쵸 원래 가족 있는 분들은 임보 힘들어하시더라구요!",
          "original_form": "그쵸그쵸 원래 가족 있는 분들은 임보 힘들어하시더라구요!",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100006934.1.30",
          "form": "진짜 대단하세요",
          "original_form": "진짜 대단하세요",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100006934.1.31",
          "form": "역시 가족을 맞이할 땐 입양이죠",
          "original_form": "역시 가족을 맞이할 땐 입양이죠",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100006934.1.32",
          "form": "학대 유기견 아직도 친정에 있어요 두마리",
          "original_form": "학대 유기견 아직도 친정에 있어요 두마리",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100006934.1.33",
          "form": "엄마가 입양했어요",
          "original_form": "엄마가 입양했어요",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100006934.1.34",
          "form": "멋진 가족이네요..! 진짜 말못한다고 학대하는 것들은 똑같이 해줘야해요ㅡㅡ",
          "original_form": "멋진 가족이네요..! 진짜 말못한다고 학대하는 것들은 똑같이 해줘야해요ㅡㅡ",
