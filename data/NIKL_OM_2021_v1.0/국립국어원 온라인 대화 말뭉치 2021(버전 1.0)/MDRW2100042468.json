      "utterance": [
        {
          "id": "MDRW2100042468.1.1",
          "form": "헉 인교진이 예상외로 출연을 많이 햇다",
          "original_form": "헉 인교진이 예상외로 출연을 많이 햇다",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042468.1.2",
          "form": "어디 나왔어요?",
          "original_form": "어디 나왔어요?",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042468.1.3",
          "form": "소이현 처럼 우리가 안본드라마거나 조연이라 기억이 안나는듯",
          "original_form": "소이현 처럼 우리가 안본드라마거나 조연이라 기억이 안나는듯",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042468.1.4",
          "form": "아 그렇구나. 인교진도 열심히 살았네",
          "original_form": "아 그렇구나. 인교진도 열심히 살았네",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042468.1.5",
          "form": "아역으로 전원일기에 나왔나봐",
          "original_form": "아역으로 전원일기에 나왔나봐",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042468.1.6",
          "form": "ㅋㅋ 첨 들어보네요",
          "original_form": "ㅋㅋ 첨 들어보네요",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042468.1.7",
          "form": "그러게 나도 첨들어봄",
          "original_form": "그러게 나도 첨들어봄",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042468.1.8",
          "form": "아역 탈렌트 출신이네요ㅋ",
          "original_form": "아역 탈렌트 출신이네요ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042468.1.9",
          "form": "80년대생인데 고딩학교때 찍은듯",
          "original_form": "80년대생인데 고딩학교때 찍은듯",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042468.1.10",
          "form": "아 80이네요ㅋ",
          "original_form": "아 80이네요ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042468.1.11",
          "form": "나이 더 들어보이던데",
          "original_form": "나이 더 들어보이던데",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042468.1.12",
          "form": "엉 전원일기 다음이 어쩌면 좋아 드라마인데 2001년",
          "original_form": "엉 전원일기 다음이 어쩌면 좋아 드라마인데 2001년",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042468.1.13",
          "form": "진심 모르겠네요",
          "original_form": "진심 모르겠네요",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042468.1.14",
          "form": "일요드라마라서 다들 안본듯",
          "original_form": "일요드라마라서 다들 안본듯",
          "speaker_id": "2",
          "time": "20210906 22:24"
