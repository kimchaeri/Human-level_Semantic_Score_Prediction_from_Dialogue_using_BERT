        },
        {
          "id": "MDRW2100034182.1.4",
          "form": "웹디자인 중 선택못함",
          "original_form": "웹디자인 중 선택못함",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100034182.1.5",
          "form": "지금배우는거는?",
          "original_form": "지금배우는거는?",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100034182.1.6",
          "form": "빨리 빨리 못따라가서 속터짐",
          "original_form": "빨리 빨리 못따라가서 속터짐",
          "speaker_id": "1",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034182.1.7",
          "form": "늙었나 이제 ㅜㅜㅜ",
          "original_form": "늙었나 이제 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034182.1.8",
          "form": "첨엔 다그래",
          "original_form": "첨엔 다그래",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034182.1.9",
          "form": "강사 말투는 진짜 이제 신경 안쓰려하는중",
          "original_form": "강사 말투는 진짜 이제 신경 안쓰려하는중",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034182.1.10",
          "form": "ㅋㅋ익숙해졌군",
          "original_form": "ㅋㅋ익숙해졌군",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034182.1.11",
          "form": "근데 요새 자꾸 졸아서 짜증남ㅜ",
          "original_form": "근데 요새 자꾸 졸아서 짜증남ㅜ",
          "speaker_id": "1",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100034182.1.12",
          "form": "왜 식곤증인가",
          "original_form": "왜 식곤증인가",
          "speaker_id": "2",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100034182.1.13",
          "form": "이번에 큰맘먹고",
          "original_form": "이번에 큰맘먹고",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100034182.1.14",
          "form": "새로운거 배우기로했으니견뎌야한다",
          "original_form": "새로운거 배우기로했으니견뎌야한다",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100034182.1.15",
          "form": "아휴 눈마주치면서 조는데",
          "original_form": "아휴 눈마주치면서 조는데",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100034182.1.16",
          "form": "또 딱히 안깨우심",
          "original_form": "또 딱히 안깨우심",
          "speaker_id": "1",
          "time": "20210827 21:17"
