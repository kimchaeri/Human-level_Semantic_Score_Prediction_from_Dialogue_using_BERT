      "utterance": [
        {
          "id": "MDRW2100001704.1.1",
          "form": "나두~ \n곧 그날이 오기를 소망 ㅎ",
          "original_form": "나두~ \n곧 그날이 오기를 소망 ㅎ",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001704.1.2",
          "form": "웅 코로나 때문에 여행도 잘 못가고 ㅠ불편",
          "original_form": "웅 코로나 때문에 여행도 잘 못가고 ㅠ불편",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001704.1.3",
          "form": "코로나 감기 수준이라는데 넘 예민하게 반응하는지 ㅠ",
          "original_form": "코로나 감기 수준이라는데 넘 예민하게 반응하는지 ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001704.1.4",
          "form": "머라는겨~ 조심해야지!!",
          "original_form": "머라는겨~ 조심해야지!!",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001704.1.5",
          "form": "어디가고싶어",
          "original_form": "어디가고싶어",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001704.1.6",
          "form": "해외여행 ㅠㅠ",
          "original_form": "해외여행 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001704.1.7",
          "form": "미국 가고싶네~",
          "original_form": "미국 가고싶네~",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001704.1.8",
          "form": "미국 가면 되지  \n백신도 2차까지 맞았으니",
          "original_form": "미국 가면 되지  \n백신도 2차까지 맞았으니",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001704.1.9",
          "form": "근데 미국에 코로나 확진자가 넘 많아서 ㅠ",
          "original_form": "근데 미국에 코로나 확진자가 넘 많아서 ㅠ",
          "speaker_id": "1",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001704.1.10",
          "form": "엄만 친구 미쿡딸네집에 가서 두달째 있음",
          "original_form": "엄만 친구 미쿡딸네집에 가서 두달째 있음",
          "speaker_id": "2",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001704.1.11",
          "form": "집에 방콕해야할듯",
          "original_form": "집에 방콕해야할듯",
          "speaker_id": "1",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001704.1.12",
          "form": "아직은 이를지도 ㅠㅜㅠ",
          "original_form": "아직은 이를지도 ㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001704.1.13",
          "form": "구램 ㅠ조심해야해유~ 여행은 나중에~",
          "original_form": "구램 ㅠ조심해야해유~ 여행은 나중에~",
