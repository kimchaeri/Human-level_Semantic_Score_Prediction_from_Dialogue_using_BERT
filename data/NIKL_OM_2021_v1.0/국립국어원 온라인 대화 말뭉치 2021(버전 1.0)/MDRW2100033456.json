      "utterance": [
        {
          "id": "MDRW2100033456.1.1",
          "form": "나혼자산다 보시나요?",
          "original_form": "나혼자산다 보시나요?",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100033456.1.2",
          "form": "엄마가 은근 챙겨보시더라고쇼",
          "original_form": "엄마가 은근 챙겨보시더라고쇼",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100033456.1.3",
          "form": "고요",
          "original_form": "고요",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100033456.1.4",
          "form": "그 웨이브 같은 데서요 ㅋㅋ",
          "original_form": "그 웨이브 같은 데서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100033456.1.5",
          "form": "저는 즐겨보거든요",
          "original_form": "저는 즐겨보거든요",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100033456.1.6",
          "form": "지금도 오상욱나오는거 보고있어요 ㅎ",
          "original_form": "지금도 오상욱나오는거 보고있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100033456.1.7",
          "form": "전에 김희철 생파해주는건 봤었어요",
          "original_form": "전에 김희철 생파해주는건 봤었어요",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033456.1.8",
          "form": "엄마 요리 맞추는거 ㅋㅋ",
          "original_form": "엄마 요리 맞추는거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033456.1.9",
          "form": "김희철이요?",
          "original_form": "김희철이요?",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100033456.1.10",
          "form": "혹시 미우새 아니구요?",
          "original_form": "혹시 미우새 아니구요?",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100033456.1.11",
          "form": "아 맞네요 ㅋㅋㅋ",
          "original_form": "아 맞네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033456.1.12",
          "form": "세상에 ㅋㅋ 엉뚱한 이야기했네여 ㄷㄷㄷ",
          "original_form": "세상에 ㅋㅋ 엉뚱한 이야기했네여 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033456.1.13",
          "form": "김희철 이야기하시기에 바로 미우새가 생각났어요",
          "original_form": "김희철 이야기하시기에 바로 미우새가 생각났어요",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100033456.1.14",
          "form": "두프로그램이 비슷한면이 있긴해요",
          "original_form": "두프로그램이 비슷한면이 있긴해요",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100033456.1.15",
          "form": "전 사실 외국드리마에 빠져서요 ㅋㅋ",
          "original_form": "전 사실 외국드리마에 빠져서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100033456.1.16",
          "form": "외국드라마요? 어떤거예요?",
          "original_form": "외국드라마요? 어떤거예요?",
          "speaker_id": "1",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100033456.1.17",
          "form": "저는 좀가벼운 수사물 좋아해요",
          "original_form": "저는 좀가벼운 수사물 좋아해요",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100033456.1.18",
          "form": "장르로 따지면 코믹수사물이고요",
          "original_form": "장르로 따지면 코믹수사물이고요",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100033456.1.19",
          "form": "오~ 수사물 인데 코믹이예요?",
          "original_form": "오~ 수사물 인데 코믹이예요?",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100033456.1.20",
          "form": "CSI같은거 아니구요?",
          "original_form": "CSI같은거 아니구요?",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100033456.1.21",
          "form": "미드 영드 일드 다 좋아해요",
          "original_form": "미드 영드 일드 다 좋아해요",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100033456.1.22",
          "form": "음 간단히말하자면",
          "original_form": "음 간단히말하자면",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100033456.1.23",
          "form": "코난같은거죠",
          "original_form": "코난같은거죠",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100033456.1.24",
          "form": "우리 코난보고 징그러워하고 막 못자고 그러지않잖아요",
          "original_form": "우리 코난보고 징그러워하고 막 못자고 그러지않잖아요",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100033456.1.25",
          "form": "와우 대단하세요 ㅎ",
          "original_form": "와우 대단하세요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100033456.1.26",
          "form": "저는 CSI만 좋아해요 ㅎ",
          "original_form": "저는 CSI만 좋아해요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100033456.1.27",
          "form": "csi진짜 열심히 봣었어요 이제 다 완결 나지 않았나요",
          "original_form": "csi진짜 열심히 봣었어요 이제 다 완결 나지 않았나요",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100033456.1.28",
          "form": "네 다 완결인데 종류별로 다르게 나오는거같기도해요",
          "original_form": "네 다 완결인데 종류별로 다르게 나오는거같기도해요",
          "speaker_id": "1",
          "time": "20210823 23:53"
        }
      ]
    }
