          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100031568.1.3",
          "form": "종교 있어?",
          "original_form": "종교 있어?",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100031568.1.4",
          "form": "아니 없어",
          "original_form": "아니 없어",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100031568.1.5",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100031568.1.6",
          "form": "나도없어",
          "original_form": "나도없어",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031568.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031568.1.8",
          "form": "무교야",
          "original_form": "무교야",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031568.1.9",
          "form": "ㅋㅋㅋ그렇구나",
          "original_form": "ㅋㅋㅋ그렇구나",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031568.1.10",
          "form": "절 가본적 있어?",
          "original_form": "절 가본적 있어?",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031568.1.11",
          "form": "ㅇㅇ친가가 불교라",
          "original_form": "ㅇㅇ친가가 불교라",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031568.1.12",
          "form": "절은 자주갔었어",
          "original_form": "절은 자주갔었어",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031568.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100031568.1.14",
          "form": "절 디게 분위기가 평화롭지 않아?",
          "original_form": "절 디게 분위기가 평화롭지 않아?",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100031568.1.15",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031568.1.16",
          "form": "조용하고 좋은거같아",
          "original_form": "조용하고 좋은거같아",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031568.1.17",
          "form": "다들 산에 있어서 그런지 조용조용한게 절의 장점인거같아",
          "original_form": "다들 산에 있어서 그런지 조용조용한게 절의 장점인거같아",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031568.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031568.1.19",
          "form": "템플스테이",
          "original_form": "템플스테이",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031568.1.20",
          "form": "왜하는지 알거같아",
          "original_form": "왜하는지 알거같아",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031568.1.21",
          "form": "진짜 완전 조용하니까 좋더라고",
          "original_form": "진짜 완전 조용하니까 좋더라고",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100031568.1.22",
          "form": "그럼 교회는 가본적 있어?",
          "original_form": "그럼 교회는 가본적 있어?",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100031568.1.23",
          "form": "응 친구따라?",
          "original_form": "응 친구따라?",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100031568.1.24",
          "form": "맛있는거준다길래ㅋㅋㅋ",
          "original_form": "맛있는거준다길래ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031568.1.25",
          "form": "진짜 교회는 다들 어릴때 먹을꺼 준대서 가보는 경위가 많은거 같아ㅋㅋㅋ",
          "original_form": "진짜 교회는 다들 어릴때 먹을꺼 준대서 가보는 경위가 많은거 같아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031568.1.26",
          "form": "맞아ㅋㅋㅋ 거의 그런 듯?",
          "original_form": "맞아ㅋㅋㅋ 거의 그런 듯?",
          "speaker_id": "1",
          "time": "20210905 00:16"
        }
      ]
