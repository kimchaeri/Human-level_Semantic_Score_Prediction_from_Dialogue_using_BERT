      "utterance": [
        {
          "id": "MDRW2100001944.1.1",
          "form": "지금 티비에 뭐해요?",
          "original_form": "지금 티비에 뭐해요?",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001944.1.2",
          "form": "저 슬기로운의사생활 보고있어요",
          "original_form": "저 슬기로운의사생활 보고있어요",
          "speaker_id": "1",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001944.1.3",
          "form": "name2님은 뭐보시나요?",
          "original_form": "&name2&님은 뭐보시나요?",
          "speaker_id": "1",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001944.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001944.1.5",
          "form": "16부작인줄알았는데 12부작인가봐요",
          "original_form": "16부작인줄알았는데 12부작인가봐요",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001944.1.6",
          "form": "몇부작 안되네요",
          "original_form": "몇부작 안되네요",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001944.1.7",
          "form": "주2회가 아니라서 그런가봐요",
          "original_form": "주2회가 아니라서 그런가봐요",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001944.1.8",
          "form": "3개월은 하는거니",
          "original_form": "3개월은 하는거니",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001944.1.9",
          "form": "거기다 의료쪽이라 전문용어도 많아 힘들거 같아요",
          "original_form": "거기다 의료쪽이라 전문용어도 많아 힘들거 같아요",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001944.1.10",
          "form": "맞아요 근데 의학드라마치곤 수술하고 그런건 많지않아서 보기편한거같아요",
          "original_form": "맞아요 근데 의학드라마치곤 수술하고 그런건 많지않아서 보기편한거같아요",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001944.1.11",
          "form": "그래요 동기들 의리를 많이 보여주니",
          "original_form": "그래요 동기들 의리를 많이 보여주니",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001944.1.12",
          "form": "저도 저런친구들 있음 좋을것같아요",
          "original_form": "저도 저런친구들 있음 좋을것같아요",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001944.1.13",
          "form": "그러게요 부럽네요",
          "original_form": "그러게요 부럽네요",
          "speaker_id": "2",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001944.1.14",
          "form": "알고있지만 도 잼나던데 보시나요?",
          "original_form": "알고있지만 도 잼나던데 보시나요?",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001944.1.15",
          "form": "아니요 그건 누가 나와요? 언뜻 지나가며 본거 같은데",
          "original_form": "아니요 그건 누가 나와요? 언뜻 지나가며 본거 같은데",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001944.1.16",
          "form": "송강이랑 여주는 이름이 ㅋ 한소희?",
          "original_form": "송강이랑 여주는 이름이 ㅋ 한소희?",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001944.1.17",
          "form": "아 그러니까 잠깐 본듯해요",
          "original_form": "아 그러니까 잠깐 본듯해요",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001944.1.18",
          "form": "살짝 야한데 ㅎㅎ 송강이 매력이 넘쳐요",
          "original_form": "살짝 야한데 ㅎㅎ 송강이 매력이 넘쳐요",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001944.1.19",
          "form": "그럼 봐야겠네요 ㅎㅎ",
          "original_form": "그럼 봐야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001944.1.20",
          "form": "1편부터보세요ㅎ 보면 연애하고싶어져요",
          "original_form": "1편부터보세요ㅎ 보면 연애하고싶어져요",
          "speaker_id": "1",
          "time": "20210819 22:40"
        }
