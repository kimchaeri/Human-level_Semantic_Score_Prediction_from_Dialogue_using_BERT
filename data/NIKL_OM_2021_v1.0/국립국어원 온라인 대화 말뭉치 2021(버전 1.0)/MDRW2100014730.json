      "utterance": [
        {
          "id": "MDRW2100014730.1.1",
          "form": "아니요 근데 키우고싶네요",
          "original_form": "아니요 근데 키우고싶네요",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014730.1.2",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014730.1.3",
          "form": "어떤종류를 키우고 싶으세요?",
          "original_form": "어떤종류를 키우고 싶으세요?",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014730.1.4",
          "form": "저는 강아지 키우고 있어요",
          "original_form": "저는 강아지 키우고 있어요",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014730.1.5",
          "form": "와 무슨 종이에요??",
          "original_form": "와 무슨 종이에요??",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014730.1.6",
          "form": "푸들이에요",
          "original_form": "푸들이에요",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014730.1.7",
          "form": "갈색푸들",
          "original_form": "갈색푸들",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014730.1.8",
          "form": "와 엄청 귀엽겠다",
          "original_form": "와 엄청 귀엽겠다",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014730.1.9",
          "form": "크기는 작아요?",
          "original_form": "크기는 작아요?",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014730.1.10",
          "form": "아쉽게도",
          "original_form": "아쉽게도",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014730.1.11",
          "form": "8키로 나가는 뚱뚱한 강아지랍니다",
          "original_form": "8키로 나가는 뚱뚱한 강아지랍니다",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014730.1.12",
          "form": "아하 그게 더 귀엽죠 근데 푸들이면 털도 별로 안빠지겠네요",
          "original_form": "아하 그게 더 귀엽죠 근데 푸들이면 털도 별로 안빠지겠네요",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014730.1.13",
          "form": "맞아요 똑똑하고 털도 안빠져서",
          "original_form": "맞아요 똑똑하고 털도 안빠져서",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014730.1.14",
          "form": "키우기 편한 종 같아요",
          "original_form": "키우기 편한 종 같아요",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014730.1.15",
          "form": "오 애교도 많나요? 장기자랑도 잘할거같네요",
          "original_form": "오 애교도 많나요? 장기자랑도 잘할거같네요",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014730.1.16",
          "form": "애교는 잘 모르겠어요 ㅋㅋㅋ 승질머리가 있어서 그런가!",
          "original_form": "애교는 잘 모르겠어요 ㅋㅋㅋ 승질머리가 있어서 그런가!",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014730.1.17",
          "form": "그래도 간식먹을때 폴짝폴짝 뛰는 모습은 너무 귀여워요",
          "original_form": "그래도 간식먹을때 폴짝폴짝 뛰는 모습은 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014730.1.18",
          "form": "와 진짜 저도 강아지 키우고싶어지네요 ㅋㅋ",
          "original_form": "와 진짜 저도 강아지 키우고싶어지네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014730.1.19",
          "form": "장난감으로 놀아주나요?",
          "original_form": "장난감으로 놀아주나요?",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014730.1.20",
          "form": "넵!인형같은건 금방 물어뜯어버려서",
          "original_form": "넵!인형같은건 금방 물어뜯어버려서",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014730.1.21",
          "form": "양말돌돌말아서 던져주고 놀아요 ㅋㅋ",
          "original_form": "양말돌돌말아서 던져주고 놀아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014730.1.22",
          "form": "귀엽겠네요 ㅋㅋ 머리끈같은것도 좋아하는거같던데 ㅋㅋ",
          "original_form": "귀엽겠네요 ㅋㅋ 머리끈같은것도 좋아하는거같던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014730.1.23",
          "form": "제지인도 푸들 키우는데 엄청 귀엽더라구요",
          "original_form": "제지인도 푸들 키우는데 엄청 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014730.1.24",
          "form": "주접같아 보이겠지만",
          "original_form": "주접같아 보이겠지만",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014730.1.25",
          "form": "지금 가만히 보고있어도 귀엽고 또보고싶어요",
          "original_form": "지금 가만히 보고있어도 귀엽고 또보고싶어요",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014730.1.26",
          "form": "강아지는 존재만으로도 행복을 주네요ㅠㅠ",
          "original_form": "강아지는 존재만으로도 행복을 주네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014730.1.27",
          "form": "와 확실히 반려동물이 주는 행복이 큰거같네요 부럽습니다",
          "original_form": "와 확실히 반려동물이 주는 행복이 큰거같네요 부럽습니다",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014730.1.28",
          "form": "심심하지않겠네요 ㅋㅋ",
          "original_form": "심심하지않겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:37"
        }
      ]
    }
  ]
