      "utterance": [
        {
          "id": "MDRW2100024514.1.1",
          "form": "네 맞아요ㅎ 근데 벌써 일케 산지 1년 넘어서",
          "original_form": "네 맞아요ㅎ 근데 벌써 일케 산지 1년 넘어서",
          "speaker_id": "2",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024514.1.2",
          "form": "이미 체념 단계네요ㅋㅋㅋ 취업 전에 낫고 싶어요 회식갈라면",
          "original_form": "이미 체념 단계네요ㅋㅋㅋ 취업 전에 낫고 싶어요 회식갈라면",
          "speaker_id": "2",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024514.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024514.1.4",
          "form": "아! 회식엔 거의 고기죠ㅜㅜㅠ",
          "original_form": "아! 회식엔 거의 고기죠ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100024514.1.5",
          "form": "술도 못드시는거죠?",
          "original_form": "술도 못드시는거죠?",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100024514.1.6",
          "form": "술은..! 별개로 체질적으로 잘 못먹어요",
          "original_form": "술은..! 별개로 체질적으로 잘 못먹어요",
          "speaker_id": "2",
          "time": "20210831 12:56"
        },
        {
          "id": "MDRW2100024514.1.7",
          "form": "얼굴이 빨개져요 ㅋㅋ",
          "original_form": "얼굴이 빨개져요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100024514.1.8",
          "form": "앗!ㅋㅋ",
          "original_form": "앗!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100024514.1.9",
          "form": "저도요ㅋㅋ 학생땐 잘마시고싶었는데 지나니 뭐~ 걍 가끔씩 마심 되지 싶어요",
          "original_form": "저도요ㅋㅋ 학생땐 잘마시고싶었는데 지나니 뭐~ 걍 가끔씩 마심 되지 싶어요",
          "speaker_id": "1",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100024514.1.10",
          "form": "전 지금도 잘 마시고 싶은데...ㅠ",
          "original_form": "전 지금도 잘 마시고 싶은데...ㅠ",
          "speaker_id": "2",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100024514.1.11",
          "form": "긍까 먼저 취하고 싶지 않아요",
          "original_form": "긍까 먼저 취하고 싶지 않아요",
          "speaker_id": "2",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100024514.1.12",
          "form": "근데 어쩔수 없는 부분이라 걍 살고 있네요ㅋㅋ",
          "original_form": "근데 어쩔수 없는 부분이라 걍 살고 있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100024514.1.13",
          "form": "아ㅋㅋㅋ 그건 있죠!",
          "original_form": "아ㅋㅋㅋ 그건 있죠!",
          "speaker_id": "1",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100024514.1.14",
          "form": "특히 일다니면 더 그렇잖아요!",
          "original_form": "특히 일다니면 더 그렇잖아요!",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100024514.1.15",
          "form": "흐트러지기 싫고",
          "original_form": "흐트러지기 싫고",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100024514.1.16",
          "form": "네 정말. 그렇습니다",
          "original_form": "네 정말. 그렇습니다",
          "speaker_id": "2",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100024514.1.17",
          "form": "안마셔도 선택해서 안마시고 싶은데..",
          "original_form": "안마셔도 선택해서 안마시고 싶은데..",
          "speaker_id": "2",
          "time": "20210831 13:02"
        },
        {
          "id": "MDRW2100024514.1.18",
          "form": "슬프네요 ㅎ",
          "original_form": "슬프네요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 13:02"
        },
        {
          "id": "MDRW2100024514.1.19",
          "form": "그니까요ㅋㅋ",
          "original_form": "그니까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100024514.1.20",
          "form": "건강때문에 안마시겠습니다! 해보고싶네오",
          "original_form": "건강때문에 안마시겠습니다! 해보고싶네오",
          "speaker_id": "1",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100024514.1.21",
          "form": "그러게 말입니다..ㅋㅋㅋㅋㅋ",
          "original_form": "그러게 말입니다..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:07"
        },
        {
          "id": "MDRW2100024514.1.22",
          "form": "저는... 썸남 앞에서 술먹고 토한적이 있답니다",
          "original_form": "저는... 썸남 앞에서 술먹고 토한적이 있답니다",
          "speaker_id": "2",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100024514.1.23",
          "form": "트라우마죠",
          "original_form": "트라우마죠",
          "speaker_id": "2",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100024514.1.24",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "1",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100024514.1.25",
          "form": "숙연해지는군요",
          "original_form": "숙연해지는군요",
          "speaker_id": "2",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100024514.1.26",
          "form": "저도모르게..",
          "original_form": "저도모르게..",
          "speaker_id": "1",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100024514.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ흑역사예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ흑역사예요",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.28",
          "form": "진짜 슐이 사람 환장하게해요",
          "original_form": "진짜 슐이 사람 환장하게해요",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.29",
          "form": "ㅋㅋㅋㅋㅋ저도 짝사랑하다ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ저도 짝사랑하다ㅎ",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.30",
          "form": "맞아요.. 그리고 전 갑자기 확 취하더라구요ㅋㅋ",
          "original_form": "맞아요.. 그리고 전 갑자기 확 취하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.31",
          "form": "컨디션 잘타고",
          "original_form": "컨디션 잘타고",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.32",
          "form": "진짜 다행인건 전화안한거에요",
          "original_form": "진짜 다행인건 전화안한거에요",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ아 정말 다행이네요..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 정말 다행이네요..",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.34",
          "form": "저도요ㅣㄱㅋㅋㅋㅋ",
          "original_form": "저도요ㅣㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.35",
          "form": "전 전화 했답니다",
          "original_form": "전 전화 했답니다",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.36",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.37",
          "form": "신나서 막 난리 치며 마시다 가조",
          "original_form": "신나서 막 난리 치며 마시다 가조",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.38",
          "form": "ㅋㅋㅋㅋ취해서 전화기에",
          "original_form": "ㅋㅋㅋㅋ취해서 전화기에",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.39",
          "form": "앗..ㅋㅋ.ㅈ",
          "original_form": "앗..ㅋㅋ.ㅈ",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.40",
          "form": "내가 아는 사람 얘기해줄게 시전했습니다",
          "original_form": "내가 아는 사람 얘기해줄게 시전했습니다",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024514.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024514.1.42",
          "form": "역시 술은 좋지 않아요",
          "original_form": "역시 술은 좋지 않아요",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024514.1.43",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "1",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024514.1.44",
          "form": "세상이 좀 너그러워지고있어 다행이에요",
          "original_form": "세상이 좀 너그러워지고있어 다행이에요",
          "speaker_id": "1",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024514.1.45",
          "form": "어디가 너그러워져요ㅋㅋㅋㅋ",
          "original_form": "어디가 너그러워져요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100024514.1.46",
          "form": "아 술에 대해서요? 회식자리",
          "original_form": "아 술에 대해서요? 회식자리",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100024514.1.47",
          "form": "넹ㅋㅋㅋ",
          "original_form": "넹ㅋㅋㅋ",
