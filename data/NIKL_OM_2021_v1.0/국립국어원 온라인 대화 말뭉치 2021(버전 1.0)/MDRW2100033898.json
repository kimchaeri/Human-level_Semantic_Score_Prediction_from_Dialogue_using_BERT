      "utterance": [
        {
          "id": "MDRW2100033898.1.1",
          "form": "학교 다니세요?",
          "original_form": "학교 다니세요?",
          "speaker_id": "1",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100033898.1.2",
          "form": "아뇨 졸업한지 오래됐어요 name1님은요?",
          "original_form": "아뇨 졸업한지 오래됐어요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100033898.1.3",
          "form": "저도 까마득한 옛날입니다 ㅎㅎ 무슨 과목 좋아하셨나요?",
          "original_form": "저도 까마득한 옛날입니다 ㅎㅎ 무슨 과목 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100033898.1.4",
          "form": "저 문과감성을가진 이과생이었어서",
          "original_form": "저 문과감성을가진 이과생이었어서",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100033898.1.5",
          "form": "ㅋㅋㅋㅋㅋ전공과목도 이과계열이었는데 넘안맞았어요",
          "original_form": "ㅋㅋㅋㅋㅋ전공과목도 이과계열이었는데 넘안맞았어요",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033898.1.6",
          "form": "그런데 어쩌다가 이과를??",
          "original_form": "그런데 어쩌다가 이과를??",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033898.1.7",
          "form": "친구따라 강남간다고...어차피 공부도 못했었고 관심도없었어서 학창시절에 걍흘러가는대로 막살았어요 ㅋㅋㅋㅋㅋ",
          "original_form": "친구따라 강남간다고...어차피 공부도 못했었고 관심도없었어서 학창시절에 걍흘러가는대로 막살았어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033898.1.8",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100033898.1.9",
          "form": "전 뼛속까지 이과생이예요",
          "original_form": "전 뼛속까지 이과생이예요",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100033898.1.10",
          "form": "오 뼛속까지이과생이라니 수학과학잘하셨겠어요!",
          "original_form": "오 뼛속까지이과생이라니 수학과학잘하셨겠어요!",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100033898.1.11",
          "form": "네~ 대신 국어가 저 바닥에 ㅋㅋㅋ",
          "original_form": "네~ 대신 국어가 저 바닥에 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100033898.1.12",
          "form": "ㅎㅎ부러워요",
          "original_form": "ㅎㅎ부러워요",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100033898.1.13",
          "form": "학교다닐때 동아리활동하셨어요?",
          "original_form": "학교다닐때 동아리활동하셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100033898.1.14",
          "form": "네~ 합기도 동아리요 name2님은요?",
          "original_form": "네~ 합기도 동아리요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100033898.1.15",
          "form": "우와 합기도 운동동아리멋있어요",
          "original_form": "우와 합기도 운동동아리멋있어요",
          "speaker_id": "2",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100033898.1.16",
          "form": "전 연극동아리여 ㅎㅎ",
          "original_form": "전 연극동아리여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100033898.1.17",
          "form": "우와!! 연극 동아리도 멋진데요",
          "original_form": "우와!! 연극 동아리도 멋진데요",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100033898.1.18",
          "form": "근데 전 연기를 못해서 남들이 하는 연기구경동아리가 됐었죠...",
