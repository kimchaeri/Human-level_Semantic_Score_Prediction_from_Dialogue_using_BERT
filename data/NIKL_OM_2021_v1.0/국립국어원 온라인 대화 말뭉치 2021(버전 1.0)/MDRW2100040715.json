      "utterance": [
        {
          "id": "MDRW2100040715.1.1",
          "form": "헐ㅋㅋㅋㄱ",
          "original_form": "헐ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100040715.1.2",
          "form": "고래밥먹고싶당",
          "original_form": "고래밥먹고싶당",
          "speaker_id": "1",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100040715.1.3",
          "form": "너그거봄?",
          "original_form": "너그거봄?",
          "speaker_id": "1",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100040715.1.4",
          "form": "아진짜ㅋㅋㅋ어렵다 ㅋㄱ",
          "original_form": "아진짜ㅋㅋㅋ어렵다 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100040715.1.5",
          "form": "뭐??",
          "original_form": "뭐??",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100040715.1.6",
          "form": "쌀로새! ㅋㅋㅋ",
          "original_form": "쌀로새! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100040715.1.7",
          "form": "쌀로별친구임",
          "original_form": "쌀로별친구임",
          "speaker_id": "1",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100040715.1.8",
          "form": "그게뭐야?ㅋㅋㅋㅋ",
          "original_form": "그게뭐야?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100040715.1.9",
          "form": "쌀로별몰라? 과자",
          "original_form": "쌀로별몰라? 과자",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100040715.1.10",
          "form": "조청유과이전에",
          "original_form": "조청유과이전에",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100040715.1.11",
          "form": "쌀 ㅇ 과 ㅇ 자",
          "original_form": "쌀 ㅇ 과 ㅇ 자",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100040715.1.12",
          "form": "쌀로별알아! 쌀로새가 뭐임ㅋㄱ",
          "original_form": "쌀로별알아! 쌀로새가 뭐임ㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100040715.1.13",
          "form": "원탑이었는데 ㅋㅋㅋ",
          "original_form": "원탑이었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100040715.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100040715.1.15",
          "form": "어이가없네",
          "original_form": "어이가없네",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100040715.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ진짜이거어이업ㅅ엉",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ진짜이거어이업ㅅ엉",
          "speaker_id": "2",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100040715.1.17",
          "form": "암튼 그 쌀로별 친구 쌀로새가 새로나옴ㅋㅋ",
          "original_form": "암튼 그 쌀로별 친구 쌀로새가 새로나옴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100040715.1.18",
          "form": "우리오빠원픽",
          "original_form": "우리오빠원픽",
          "speaker_id": "2",
          "time": "20210905 00:34"
        },
        {
          "id": "MDRW2100040715.1.19",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:34"
        },
        {
          "id": "MDRW2100040715.1.20",
          "form": "맛있게보인다 찾아봄!",
          "original_form": "맛있게보인다 찾아봄!",
          "speaker_id": "2",
          "time": "20210905 00:34"
        },
        {
          "id": "MDRW2100040715.1.21",
          "form": "어 맛있을거같애 ㅋㅋ새우깡맛이래",
          "original_form": "어 맛있을거같애 ㅋㅋ새우깡맛이래",
          "speaker_id": "1",
          "time": "20210905 00:35"
        },
        {
          "id": "MDRW2100040715.1.22",
          "form": "이거 동네에 팔까",
          "original_form": "이거 동네에 팔까",
          "speaker_id": "2",
          "time": "20210905 00:35"
        },
        {
          "id": "MDRW2100040715.1.23",
          "form": "먹고싶은데",
          "original_form": "먹고싶은데",
          "speaker_id": "2",
          "time": "20210905 00:35"
        },
        {
          "id": "MDRW2100040715.1.24",
          "form": "팔면연락좀...",
          "original_form": "팔면연락좀...",
