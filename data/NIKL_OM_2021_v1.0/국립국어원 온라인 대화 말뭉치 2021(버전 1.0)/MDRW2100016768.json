      "utterance": [
        {
          "id": "MDRW2100016768.1.1",
          "form": "편의점거 드셔보셨어요?",
          "original_form": "편의점거 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016768.1.2",
          "form": "아뇨 편의점에도 잇는걸 처음 알앗어요!",
          "original_form": "아뇨 편의점에도 잇는걸 처음 알앗어요!",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016768.1.3",
          "form": "세상에 전 사실 진짜 죠스보다 편의점게 더 맛있더라구요 희한하게 ㅋㅋㅋ",
          "original_form": "세상에 전 사실 진짜 죠스보다 편의점게 더 맛있더라구요 희한하게 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016768.1.4",
          "form": "꼭 드셔보세요 ㅋㅋㅋ",
          "original_form": "꼭 드셔보세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016768.1.5",
          "form": "대박ㅋㅋㅋㅋㅋ 신기하네요!!!! 그래야겟어요",
          "original_form": "대박ㅋㅋㅋㅋㅋ 신기하네요!!!! 그래야겟어요",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016768.1.6",
          "form": "요즘엔",
          "original_form": "요즘엔",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016768.1.7",
          "form": "가성비도 좋고 ㅎㅎ",
          "original_form": "가성비도 좋고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016768.1.8",
          "form": "편의좀 분식도 맛있는것같아요",
          "original_form": "편의좀 분식도 맛있는것같아요",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016768.1.9",
          "form": "맞아요 가성비좋고!",
          "original_form": "맞아요 가성비좋고!",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016768.1.10",
          "form": "맞아요 접근성도 좋고 ㅋㅋㅋ",
          "original_form": "맞아요 접근성도 좋고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016768.1.11",
          "form": "그쵸ㅎㅎ 좋아하는 배달음식 또 뭐가 있나요?!?",
          "original_form": "그쵸ㅎㅎ 좋아하는 배달음식 또 뭐가 있나요?!?",
          "speaker_id": "2",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016768.1.12",
          "form": "유가네닭갈비 너무 좋아해요ㅋㅋㅋ",
          "original_form": "유가네닭갈비 너무 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016768.1.13",
          "form": "헐 저두요",
          "original_form": "헐 저두요",
          "speaker_id": "2",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016768.1.14",
          "form": "넘 맛있죠",
          "original_form": "넘 맛있죠",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016768.1.15",
          "form": "그리고 수제버거랑 인도카레도 자주 먹어요!!!",
          "original_form": "그리고 수제버거랑 인도카레도 자주 먹어요!!!",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016768.1.16",
          "form": "좀 특이한 음식도 좋아하는편이라",
          "original_form": "좀 특이한 음식도 좋아하는편이라",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016768.1.17",
          "form": "오 저 원래 햄버거 안 좋아하는데 다운타우너 먹고 생각이 바뀌었어요",
          "original_form": "오 저 원래 햄버거 안 좋아하는데 다운타우너 먹고 생각이 바뀌었어요",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016768.1.18",
          "form": "거기 아보카도버거 진짜  .....넘 맛잇어요",
          "original_form": "거기 아보카도버거 진짜  .....넘 맛잇어요",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016768.1.19",
          "form": "치즈버거도 맛잇구",
          "original_form": "치즈버거도 맛잇구",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016768.1.20",
          "form": "와 전 아보카도 자체를 못 먹어봐서 너무 궁금해요",
          "original_form": "와 전 아보카도 자체를 못 먹어봐서 너무 궁금해요",
