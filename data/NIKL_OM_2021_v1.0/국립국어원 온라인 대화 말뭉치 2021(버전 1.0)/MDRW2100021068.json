      "utterance": [
        {
          "id": "MDRW2100021068.1.1",
          "form": "안녕하세요 날씨가 흐리네요 거긴 날씨가 어떤가요?",
          "original_form": "안녕하세요 날씨가 흐리네요 거긴 날씨가 어떤가요?",
          "speaker_id": "1",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100021068.1.2",
          "form": "안녕하세요 여긴 맑아요",
          "original_form": "안녕하세요 여긴 맑아요",
          "speaker_id": "2",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100021068.1.3",
          "form": "거긴 비가 오고 있나요?",
          "original_form": "거긴 비가 오고 있나요?",
          "speaker_id": "2",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100021068.1.4",
          "form": "아뇨 비는 아직인데 그냥 흐리기만 하고 약간 덥네요!",
          "original_form": "아뇨 비는 아직인데 그냥 흐리기만 하고 약간 덥네요!",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100021068.1.5",
          "form": "맑은 날씨라 좋으시겠어요 기분도 맑아질 것 같아요!",
          "original_form": "맑은 날씨라 좋으시겠어요 기분도 맑아질 것 같아요!",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100021068.1.6",
          "form": "오랜만에 상쾌한 기분이네요",
          "original_form": "오랜만에 상쾌한 기분이네요",
          "speaker_id": "2",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021068.1.7",
          "form": "약간 덥긴 해요",
          "original_form": "약간 덥긴 해요",
          "speaker_id": "2",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021068.1.8",
          "form": "아직 가을이 안왔나봐요",
          "original_form": "아직 가을이 안왔나봐요",
          "speaker_id": "2",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021068.1.9",
          "form": "좋은 날씨엔 놀러가고 싶지 않나요? 마침 주말이기도 하고요",
          "original_form": "좋은 날씨엔 놀러가고 싶지 않나요? 마침 주말이기도 하고요",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021068.1.10",
          "form": "그런가봐요 곧 가을인줄 알았더니....!",
          "original_form": "그런가봐요 곧 가을인줄 알았더니....!",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021068.1.11",
          "form": "맞아요 그런데 코로나때문에 놀러가는것도 쉽지는 않네요 ㅠㅠ",
          "original_form": "맞아요 그런데 코로나때문에 놀러가는것도 쉽지는 않네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 15:37"
        },
        {
          "id": "MDRW2100021068.1.12",
          "form": "가을 옷은 사셧어요?",
          "original_form": "가을 옷은 사셧어요?",
          "speaker_id": "2",
          "time": "20210828 15:37"
        },
        {
          "id": "MDRW2100021068.1.13",
          "form": "그러게요 코로나가 ㅠㅠ 편하게 뭘 즐길 수가 없네요",
          "original_form": "그러게요 코로나가 ㅠㅠ 편하게 뭘 즐길 수가 없네요",
          "speaker_id": "1",
          "time": "20210828 15:37"
        },
        {
          "id": "MDRW2100021068.1.14",
          "form": "가을옷은 이제 슬슬 찾아볼까 해요 사셨나요?",
          "original_form": "가을옷은 이제 슬슬 찾아볼까 해요 사셨나요?",
          "speaker_id": "1",
          "time": "20210828 15:38"
        },
        {
          "id": "MDRW2100021068.1.15",
          "form": "아니요 ㅋㅋㅋ저도 아직이에요",
          "original_form": "아니요 ㅋㅋㅋ저도 아직이에요",
          "speaker_id": "2",
          "time": "20210828 15:39"
        },
        {
          "id": "MDRW2100021068.1.16",
          "form": "입을 옷이 없는데 갑자기 가을 오면 큰일이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "입을 옷이 없는데 갑자기 가을 오면 큰일이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100021068.1.17",
          "form": "왜 입을 옷은 매번 없을까요...? 계절 바뀔 때마다 생각해요ㅠㅠ",
          "original_form": "왜 입을 옷은 매번 없을까요...? 계절 바뀔 때마다 생각해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100021068.1.18",
          "form": "저도요 ㅋㅋㅋ늘 입을 옷이 없어욬ㅋㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋ늘 입을 옷이 없어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:41"
        },
        {
          "id": "MDRW2100021068.1.19",
          "form": "저는 여름엔 옷을 제일 안 사는 것 같아요....",
          "original_form": "저는 여름엔 옷을 제일 안 사는 것 같아요....",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021068.1.20",
          "form": "어느 계절에 옷을 사고 싶으세요?",
          "original_form": "어느 계절에 옷을 사고 싶으세요?",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021068.1.21",
          "form": "전 겨울옷이 그렇게 예뻐요",
          "original_form": "전 겨울옷이 그렇게 예뻐요",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021068.1.22",
          "form": "니트만 보면 눈돌아가요 ㅋㅋㅋㅋ",
          "original_form": "니트만 보면 눈돌아가요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021068.1.23",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021068.1.24",
          "form": "크으으 니트 정말 예쁘죠! 패턴마다 컬러마다 다 매력이 다른 것도 좋구요",
          "original_form": "크으으 니트 정말 예쁘죠! 패턴마다 컬러마다 다 매력이 다른 것도 좋구요",
