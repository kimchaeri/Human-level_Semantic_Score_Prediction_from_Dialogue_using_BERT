          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027266.1.3",
          "form": "밍밍한맛 별로입니다",
          "original_form": "밍밍한맛 별로입니다",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027266.1.4",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027266.1.5",
          "form": "물 안드세여?",
          "original_form": "물 안드세여?",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027266.1.6",
          "form": "맹물 좋아함 ㅋㅋㅋㅋㅋ",
          "original_form": "맹물 좋아함 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027266.1.7",
          "form": "아님 보리차?",
          "original_form": "아님 보리차?",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027266.1.8",
          "form": "뭐야..",
          "original_form": "뭐야..",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027266.1.9",
          "form": "이사람 이상해...",
          "original_form": "이사람 이상해...",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027266.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027266.1.11",
          "form": "쨋든 먹는것만 먹는다는 소립니다,,",
          "original_form": "쨋든 먹는것만 먹는다는 소립니다,,",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027266.1.12",
          "form": "맹물이 심심하니까 차 넣는건데",
          "original_form": "맹물이 심심하니까 차 넣는건데",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027266.1.13",
          "form": "아 꽃향 이런게 별로인듯",
          "original_form": "아 꽃향 이런게 별로인듯",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027266.1.14",
          "form": "풀내음",
          "original_form": "풀내음",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027266.1.15",
          "form": "풀내가 별론데",
          "original_form": "풀내가 별론데",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027266.1.16",
          "form": "점심용 샐러드는 시키셨다?...",
          "original_form": "점심용 샐러드는 시키셨다?...",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027266.1.17",
          "form": "페페짤 보내고싶다",
          "original_form": "페페짤 보내고싶다",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027266.1.18",
          "form": "아뇨 아 그느,낌 이해를 못하나봐",
          "original_form": "아뇨 아 그느,낌 이해를 못하나봐",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027266.1.19",
          "form": "제가 향기나는꽃을 안좋아함 ㅋㅋㅋㅋ",
          "original_form": "제가 향기나는꽃을 안좋아함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027266.1.20",
          "form": "꽃다발 이런거 싫어요?",
          "original_form": "꽃다발 이런거 싫어요?",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027266.1.21",
          "form": "향나는거시러여 꽃향",
          "original_form": "향나는거시러여 꽃향",
          "speaker_id": "1",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027266.1.22",
          "form": "왜냐면 벌꼬임",
          "original_form": "왜냐면 벌꼬임",
          "speaker_id": "1",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027266.1.23",
          "form": "벌은 똑똑한 곤충임미다",
          "original_form": "벌은 똑똑한 곤충임미다",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027266.1.24",
          "form": "꽃다발 싫어하시는구나",
          "original_form": "꽃다발 싫어하시는구나",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027266.1.25",
          "form": "의외네",
          "original_form": "의외네",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027266.1.26",
          "form": "넘싫음 ㅠㅠ 벌 ㅠ",
          "original_form": "넘싫음 ㅠㅠ 벌 ㅠ",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027266.1.27",
          "form": "근데 또 예쁘니 좋긴하던데",
          "original_form": "근데 또 예쁘니 좋긴하던데",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027266.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027266.1.29",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027266.1.30",
          "form": "이소리하면 또 ? 이럴듯",
          "original_form": "이소리하면 또 ? 이럴듯",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027266.1.31",
          "form": "그래도 꽃은 받고싶다 이소리",
          "original_form": "그래도 꽃은 받고싶다 이소리",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027266.1.32",
          "form": "꽃 말고 상담을 한번 받아보시는 것도",
          "original_form": "꽃 말고 상담을 한번 받아보시는 것도",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027266.1.33",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027266.1.34",
          "form": "대충 무슨 느낌인진 알겠어요",
          "original_form": "대충 무슨 느낌인진 알겠어요",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027266.1.35",
          "form": "블랑 싫죠?",
          "original_form": "블랑 싫죠?",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027266.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027266.1.37",
          "form": "맥주 블랑만먹음.,,.",
          "original_form": "맥주 블랑만먹음.,,.",
          "speaker_id": "1",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027266.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027266.1.39",
          "form": ".....",
          "original_form": ".....",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027266.1.40",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027266.1.41",
          "form": "이쯤되니 내가 이상한건가",
          "original_form": "이쯤되니 내가 이상한건가",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027266.1.42",
          "form": "블랑 좋아하면 곰표도 좋아하시겠네",
          "original_form": "블랑 좋아하면 곰표도 좋아하시겠네",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027266.1.43",
          "form": "곰표안먹어봣어요",
          "original_form": "곰표안먹어봣어요",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027266.1.44",
          "form": "맛있음?",
          "original_form": "맛있음?",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027266.1.45",
          "form": "ㅇㅇ 블랑이랑 비슷해요",
          "original_form": "ㅇㅇ 블랑이랑 비슷해요",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100027266.1.46",
          "form": "맛 괜찮음",
          "original_form": "맛 괜찮음",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100027266.1.47",
          "form": "블랑보다 살짝 더 보드라운 느낌",
          "original_form": "블랑보다 살짝 더 보드라운 느낌",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100027266.1.48",
          "form": "오 마셔봐야겟네요 근데 맥주는 배불러서 잘안먹고 저는 소주파",
          "original_form": "오 마셔봐야겟네요 근데 맥주는 배불러서 잘안먹고 저는 소주파",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027266.1.49",
          "form": "소맥먹을때 각이네",
          "original_form": "소맥먹을때 각이네",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027266.1.50",
          "form": "소주시름",
          "original_form": "소주시름",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027266.1.51",
          "form": "소맥은 갠춘",
          "original_form": "소맥은 갠춘",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027266.1.52",
          "form": "뭐 마셔요? 저번에 진로라했나?",
          "original_form": "뭐 마셔요? 저번에 진로라했나?",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027266.1.53",
          "form": "진로마심",
          "original_form": "진로마심",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027266.1.54",
          "form": "요즘술,집들 참이슬4천인데 진로만 5천임",
          "original_form": "요즘술,집들 참이슬4천인데 진로만 5천임",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027266.1.55",
          "form": "완전사,기꾼들임",
          "original_form": "완전사,기꾼들임",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027266.1.56",
          "form": "진로 주식 얼마지",
          "original_form": "진로 주식 얼마지",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027266.1.57",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027266.1.58",
          "form": "참이슬이랑 같은곳아님?",
          "original_form": "참이슬이랑 같은곳아님?",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027266.1.59",
          "form": "하이트진로",
          "original_form": "하이트진로",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027266.1.60",
          "form": "어제 오징어회 1.8인데 배달 2부터 시작해서",
          "original_form": "어제 오징어회 1.8인데 배달 2부터 시작해서",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027266.1.61",
          "form": "한맥 마셔봤거는요?",
          "original_form": "한맥 마셔봤거는요?",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027266.1.62",
          "form": "그냥 그렇더라구요",
          "original_form": "그냥 그렇더라구요",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027266.1.63",
          "form": "걍그래요 한맥",
          "original_form": "걍그래요 한맥",
          "speaker_id": "1",
          "time": "20210902 11:24"
        },
        {
          "id": "MDRW2100027266.1.64",
          "form": "한번마셔본듯",
          "original_form": "한번마셔본듯",
          "speaker_id": "1",
          "time": "20210902 11:24"
        },
        {
          "id": "MDRW2100027266.1.65",
