      "utterance": [
        {
          "id": "MDRW2100020632.1.1",
          "form": "꼭 사드셔보세요",
          "original_form": "꼭 사드셔보세요",
          "speaker_id": "2",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020632.1.2",
          "form": "달달하고",
          "original_form": "달달하고",
          "speaker_id": "2",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020632.1.3",
          "form": "뭐 드셨어요 궁금해 미치겠어 거기서 끊으면 어떡해",
          "original_form": "뭐 드셨어요 궁금해 미치겠어 거기서 끊으면 어떡해",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020632.1.4",
          "form": "풍미가 있어요",
          "original_form": "풍미가 있어요",
          "speaker_id": "2",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020632.1.5",
          "form": "이름이 뭐죠",
          "original_form": "이름이 뭐죠",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020632.1.6",
          "form": "얼음컵에 부어서 먹는",
          "original_form": "얼음컵에 부어서 먹는",
          "speaker_id": "2",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020632.1.7",
          "form": "어느편의점에서 사셨죠",
          "original_form": "어느편의점에서 사셨죠",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020632.1.8",
          "form": "편의점마다 다파는데",
          "original_form": "편의점마다 다파는데",
          "speaker_id": "2",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020632.1.9",
          "form": "가장 맛있었던 곳은",
          "original_form": "가장 맛있었던 곳은",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.10",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.11",
          "form": "지에스꺼에요",
          "original_form": "지에스꺼에요",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.12",
          "form": "오케이",
          "original_form": "오케이",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.13",
          "form": "메모합니다",
          "original_form": "메모합니다",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.14",
          "form": "지에스 헤이즐넛",
          "original_form": "지에스 헤이즐넛",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.15",
          "form": "여름에 엄청 더울 때",
          "original_form": "여름에 엄청 더울 때",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.16",
          "form": "오늘따라 우유들어간 커피가 부담스러울 때!",
          "original_form": "오늘따라 우유들어간 커피가 부담스러울 때!",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.17",
          "form": "그때 드셔보셔요",
          "original_form": "그때 드셔보셔요",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.18",
          "form": "아 그기분 알죠.. 진짜 세상이 아름다워 보이는 기적",
          "original_form": "아 그기분 알죠.. 진짜 세상이 아름다워 보이는 기적",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.19",
          "form": "한모금 마시면",
          "original_form": "한모금 마시면",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.20",
          "form": "아..!  이거지",
          "original_form": "아..!  이거지",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020632.1.21",
          "form": "그쵸 식도에서부터",
          "original_form": "그쵸 식도에서부터",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020632.1.22",
          "form": "찬기운이 퍼지는",
          "original_form": "찬기운이 퍼지는",
          "speaker_id": "2",
          "time": "20210828 10:39"
        }
      ]
    }
