      "utterance": [
        {
          "id": "MDRW2100024702.1.1",
          "form": "그거는 파노라마보다 조아",
          "original_form": "그거는 파노라마보다 조아",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024702.1.2",
          "form": "진심??!",
          "original_form": "진심??!",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024702.1.3",
          "form": "어진짜루",
          "original_form": "어진짜루",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024702.1.4",
          "form": "난 둘다조아",
          "original_form": "난 둘다조아",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024702.1.5",
          "form": "그냥 그건 계속 기다리고잇엇어",
          "original_form": "그냥 그건 계속 기다리고잇엇어",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024702.1.6",
          "form": "언니가?",
          "original_form": "언니가?",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024702.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024702.1.8",
          "form": "언제?",
          "original_form": "언제?",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024702.1.9",
          "form": "하라떳을때부터",
          "original_form": "하라떳을때부터",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.10",
          "form": "쭉??",
          "original_form": "쭉??",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.11",
          "form": "어 진짜...",
          "original_form": "어 진짜...",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.12",
          "form": "대박이다",
          "original_form": "대박이다",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.13",
          "form": "그치 나도 그때 이정도로조을줄몰랏어",
          "original_form": "그치 나도 그때 이정도로조을줄몰랏어",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.14",
          "form": "팬클럽 가입하지",
          "original_form": "팬클럽 가입하지",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.15",
          "form": "근데그럼 내 현생은...",
          "original_form": "근데그럼 내 현생은...",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.16",
          "form": "그거는그렇다",
          "original_form": "그거는그렇다",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.17",
          "form": "글고 공부해야햇으니까",
          "original_form": "글고 공부해야햇으니까",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.18",
          "form": "마쟈ㅠ",
          "original_form": "마쟈ㅠ",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024702.1.19",
          "form": "아쉽네...",
          "original_form": "아쉽네...",
          "speaker_id": "2",
