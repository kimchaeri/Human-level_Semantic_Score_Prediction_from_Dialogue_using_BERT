      "utterance": [
        {
          "id": "MDRW2100002055.1.1",
          "form": "음료 어떤 거 좋아하세요?",
          "original_form": "음료 어떤 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100002055.1.2",
          "form": "사이다 좋아해요",
          "original_form": "사이다 좋아해요",
          "speaker_id": "2",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100002055.1.3",
          "form": "스프라이트 칠성사이다 이런거요. 자몽주스도 좋아하구요.",
          "original_form": "스프라이트 칠성사이다 이런거요. 자몽주스도 좋아하구요.",
          "speaker_id": "2",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100002055.1.4",
          "form": "name1님은 어떤 음료 좋아하세요?",
          "original_form": "&name1&님은 어떤 음료 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100002055.1.5",
          "form": "저는 아메리카노 좋아해요",
          "original_form": "저는 아메리카노 좋아해요",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100002055.1.6",
          "form": "탄산음료도 좋죠! 저도 자몽주스 좋아해요ㅎㅎ",
          "original_form": "탄산음료도 좋죠! 저도 자몽주스 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100002055.1.7",
          "form": "아메리카노 제가 커피를 안마셔서 아메리카노의 맛을 표현한다면?",
          "original_form": "아메리카노 제가 커피를 안마셔서 아메리카노의 맛을 표현한다면?",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100002055.1.8",
          "form": "저도 예전엔 못마셨었는데 뭔가 깔끔해서 좋아요",
          "original_form": "저도 예전엔 못마셨었는데 뭔가 깔끔해서 좋아요",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100002055.1.9",
          "form": "우유 들어간건 제가 잘 못마시거든요ㅠㅠ",
          "original_form": "우유 들어간건 제가 잘 못마시거든요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100002055.1.10",
          "form": "아 유당불내증 있으신가요?",
          "original_form": "아 유당불내증 있으신가요?",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002055.1.11",
          "form": "아니면 그냥 맛과 향 때문에?",
          "original_form": "아니면 그냥 맛과 향 때문에?",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002055.1.12",
          "form": "유당불내증까진 아니고 흰우유를 별로 안 좋아해요ㅠㅠ",
          "original_form": "유당불내증까진 아니고 흰우유를 별로 안 좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002055.1.13",
          "form": "입에 뭐가 남는 느낌이어가지구",
          "original_form": "입에 뭐가 남는 느낌이어가지구",
          "speaker_id": "1",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002055.1.14",
          "form": "근데 또 빵이나 이런거랑은 잘 먹어요ㅎ",
          "original_form": "근데 또 빵이나 이런거랑은 잘 먹어요ㅎ",
          "speaker_id": "1",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002055.1.15",
          "form": "아 그럴 수 있어요 그 느낌 알 것 같아요",
          "original_form": "아 그럴 수 있어요 그 느낌 알 것 같아요",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002055.1.16",
          "form": "빵 무슨 빵 좋아하세요",
          "original_form": "빵 무슨 빵 좋아하세요",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002055.1.17",
          "form": "저 빵은 다 잘먹어요",
          "original_form": "저 빵은 다 잘먹어요",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002055.1.18",
          "form": "그냥 담백한 빵도 좋아해요ㅎㅎ",
          "original_form": "그냥 담백한 빵도 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002055.1.19",
          "form": "식빵이나 크루아상같은거 좋아하실거같아요",
          "original_form": "식빵이나 크루아상같은거 좋아하실거같아요",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002055.1.20",
          "form": "저는 애입맛이라 소시지들어간게 좋더라구요",
          "original_form": "저는 애입맛이라 소시지들어간게 좋더라구요",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002055.1.21",
          "form": "오 맞아요 절 너무 잘 아시는데요",
          "original_form": "오 맞아요 절 너무 잘 아시는데요",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002055.1.22",
          "form": "요새 크루아상에 아몬드나 크림 이런거 있는것도 너무 좋아요",
          "original_form": "요새 크루아상에 아몬드나 크림 이런거 있는것도 너무 좋아요",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002055.1.23",
          "form": "쫄깃하고 달콤하고 너무 좋죠",
          "original_form": "쫄깃하고 달콤하고 너무 좋죠",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002055.1.24",
          "form": "빵은 진짜 다 맛있는 거 같아요",
          "original_form": "빵은 진짜 다 맛있는 거 같아요",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002055.1.25",
          "form": "그쵸 저 삼송빵집이랑 성심당 이런 곳도 좋아해요",
          "original_form": "그쵸 저 삼송빵집이랑 성심당 이런 곳도 좋아해요",
          "speaker_id": "1",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002055.1.26",
          "form": "세상에 맛있는 빵이 너무 많아용",
          "original_form": "세상에 맛있는 빵이 너무 많아용",
