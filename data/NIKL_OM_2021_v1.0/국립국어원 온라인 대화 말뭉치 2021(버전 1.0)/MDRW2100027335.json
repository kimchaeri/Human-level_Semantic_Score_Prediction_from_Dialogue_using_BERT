      "utterance": [
        {
          "id": "MDRW2100027335.1.1",
          "form": "가족관계가 어떻게 되시나요",
          "original_form": "가족관계가 어떻게 되시나요",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027335.1.2",
          "form": "부모님이랑 여동생있어요~",
          "original_form": "부모님이랑 여동생있어요~",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027335.1.3",
          "form": "여동생이 있군요. 저는 오빠가 있어요",
          "original_form": "여동생이 있군요. 저는 오빠가 있어요",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027335.1.4",
          "form": "몇살 차이가 나나요? 저는 세살차이가 나요.",
          "original_form": "몇살 차이가 나나요? 저는 세살차이가 나요.",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027335.1.5",
          "form": "전 어렸을때 오빠있는친구들 부러웟는데",
          "original_form": "전 어렸을때 오빠있는친구들 부러웟는데",
          "speaker_id": "2",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027335.1.6",
          "form": "2살터울이에요!",
          "original_form": "2살터울이에요!",
          "speaker_id": "2",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027335.1.7",
          "form": "저는 언니를 갖고싶었어요",
          "original_form": "저는 언니를 갖고싶었어요",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027335.1.8",
          "form": "죄송한데 님 여성이세요?",
          "original_form": "죄송한데 님 여성이세요?",
          "speaker_id": "2",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027335.1.9",
          "form": "오빠는 음. 현실남매에요",
          "original_form": "오빠는 음. 현실남매에요",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027335.1.10",
          "form": "네ㅎ",
          "original_form": "네ㅎ",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027335.1.11",
          "form": "그러니 오빠가 있쥬",
          "original_form": "그러니 오빠가 있쥬",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027335.1.12",
          "form": "자매 있는 분이 젤 부러워요",
          "original_form": "자매 있는 분이 젤 부러워요",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027335.1.13",
          "form": "저흰 그렇게 친하진않아서 ㅎ",
          "original_form": "저흰 그렇게 친하진않아서 ㅎ",
