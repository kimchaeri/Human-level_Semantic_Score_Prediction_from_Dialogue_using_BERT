      "utterance": [
        {
          "id": "MDRW2100042919.1.1",
          "form": "오늘은 쪼끔 한가해..",
          "original_form": "오늘은 쪼끔 한가해..",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042919.1.2",
          "form": "그래서 도장도 찍고",
          "original_form": "그래서 도장도 찍고",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042919.1.3",
          "form": "형광펜으로 서류 그어주고",
          "original_form": "형광펜으로 서류 그어주고",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042919.1.4",
          "form": "그랬어..",
          "original_form": "그랬어..",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042919.1.5",
          "form": "일을 던져주고 간다",
          "original_form": "일을 던져주고 간다",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042919.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042919.1.7",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "1",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042919.1.8",
          "form": "안바쁜 꼴을 못보나봐",
          "original_form": "안바쁜 꼴을 못보나봐",
          "speaker_id": "2",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042919.1.9",
          "form": "너무하는군",
          "original_form": "너무하는군",
          "speaker_id": "2",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042919.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042919.1.11",
          "form": "눈이 돌아가서 좀있으면 머리 뒤도 볼거 같은데",
          "original_form": "눈이 돌아가서 좀있으면 머리 뒤도 볼거 같은데",
          "speaker_id": "2",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042919.1.12",
          "form": "그래도 고객상대보다는",
          "original_form": "그래도 고객상대보다는",
          "speaker_id": "1",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042919.1.13",
          "form": "서류가 낫지",
          "original_form": "서류가 낫지",
          "speaker_id": "1",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042919.1.14",
          "form": "그건 그렇지...",
          "original_form": "그건 그렇지...",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042919.1.15",
          "form": "그렇지만 아예 안하는건 ㅏㅇ니니까ㅏ",
          "original_form": "그렇지만 아예 안하는건 ㅏㅇ니니까ㅏ",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042919.1.16",
          "form": "고객상대도 틈틈이 해야 하는",
          "original_form": "고객상대도 틈틈이 해야 하는",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042919.1.17",
          "form": "맘마 먹으러 가라고 보내서 일단 나왔다 구내식당 싫엉...",
          "original_form": "맘마 먹으러 가라고 보내서 일단 나왔다 구내식당 싫엉...",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042919.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042919.1.19",
          "form": "점심시간 몇시까지여?",
          "original_form": "점심시간 몇시까지여?",
          "speaker_id": "1",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042919.1.20",
          "form": "1시간이니까 12시 반..",
          "original_form": "1시간이니까 12시 반..",
          "speaker_id": "2",
          "time": "20210907 11:39"
        },
        {
          "id": "MDRW2100042919.1.21",
          "form": "눈깜빡하면 끝나는 시간..",
          "original_form": "눈깜빡하면 끝나는 시간..",
          "speaker_id": "2",
          "time": "20210907 11:40"
        },
        {
          "id": "MDRW2100042919.1.22",
          "form": "ㅇㅎ 12시 반에 끝나는규나",
          "original_form": "ㅇㅎ 12시 반에 끝나는규나",
          "speaker_id": "1",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100042919.1.23",
          "form": "ㅇㅇ 빨리 먹고 빨리 끝나는",
          "original_form": "ㅇㅇ 빨리 먹고 빨리 끝나는",
          "speaker_id": "2",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100042919.1.24",
          "form": "조삼모사",
          "original_form": "조삼모사",
          "speaker_id": "2",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100042919.1.25",
          "form": "ㅋㅋㅋㅋ 1시까지는 해주지",
          "original_form": "ㅋㅋㅋㅋ 1시까지는 해주지",
          "speaker_id": "1",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100042919.1.26",
          "form": "너무하네",
          "original_form": "너무하네",
          "speaker_id": "1",
          "time": "20210907 11:42"
        },
        {
          "id": "MDRW2100042919.1.27",
          "form": "그러니까... 한시간 반 한다고 누구 안죽는데",
          "original_form": "그러니까... 한시간 반 한다고 누구 안죽는데",
          "speaker_id": "2",
          "time": "20210907 11:48"
        },
        {
          "id": "MDRW2100042919.1.28",
          "form": "그러면서 돈은 다 달라고 하면 양심리스긴 하겠다",
          "original_form": "그러면서 돈은 다 달라고 하면 양심리스긴 하겠다",
          "speaker_id": "2",
          "time": "20210907 11:48"
        }
      ]
    }
  ]
