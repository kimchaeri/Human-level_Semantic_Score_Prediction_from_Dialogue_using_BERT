      "utterance": [
        {
          "id": "MDRW2100022153.1.1",
          "form": "지금 날씨가 쌀쌀해요...",
          "original_form": "지금 날씨가 쌀쌀해요...",
          "speaker_id": "2",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022153.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022153.1.3",
          "form": "저희 엄마가 광복절만 지나면",
          "original_form": "저희 엄마가 광복절만 지나면",
          "speaker_id": "1",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022153.1.4",
          "form": "날씨가 금방 쌀쌀해진다고 했는데",
          "original_form": "날씨가 금방 쌀쌀해진다고 했는데",
          "speaker_id": "1",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022153.1.5",
          "form": "ㅎㅎㅎ 네네",
          "original_form": "ㅎㅎㅎ 네네",
          "speaker_id": "2",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022153.1.6",
          "form": "진짜 딱 그렇게 되어서 너무 신기해요",
          "original_form": "진짜 딱 그렇게 되어서 너무 신기해요",
          "speaker_id": "1",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022153.1.7",
          "form": "맞아요 그리고 요즘",
          "original_form": "맞아요 그리고 요즘",
          "speaker_id": "2",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022153.1.8",
          "form": "일교차가 커져서",
          "original_form": "일교차가 커져서",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.9",
          "form": "비염 환자들이 고생이더라구요",
          "original_form": "비염 환자들이 고생이더라구요",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.10",
          "form": "아이고 비염",
          "original_form": "아이고 비염",
          "speaker_id": "1",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.11",
          "form": "제 얘기 하시네요 ㅠㅠ ㅎㅎㅎㅎ",
          "original_form": "제 얘기 하시네요 ㅠㅠ ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.12",
          "form": "요즘은 그나~마 괜찮은데",
          "original_form": "요즘은 그나~마 괜찮은데",
          "speaker_id": "1",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.13",
          "form": "한 달 전에는 비염 때문에 진짜 고생 많이 했어요",
          "original_form": "한 달 전에는 비염 때문에 진짜 고생 많이 했어요",
          "speaker_id": "1",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.14",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.15",
          "form": "우리 막내딸도 비염이 있어요",
          "original_form": "우리 막내딸도 비염이 있어요",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.16",
          "form": "여름에서 가을로 넘어갈 때",
          "original_form": "여름에서 가을로 넘어갈 때",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.17",
          "form": "힘들어하더라구요",
          "original_form": "힘들어하더라구요",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022153.1.18",
          "form": "네네 ㅠㅠ",
          "original_form": "네네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022153.1.19",
          "form": "근데 태풍은 이제 이렇게 끝인건가요?",
          "original_form": "근데 태풍은 이제 이렇게 끝인건가요?",
          "speaker_id": "1",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022153.1.20",
          "form": "그런가봐요 저번주에는 비 자주 오더니",
          "original_form": "그런가봐요 저번주에는 비 자주 오더니",
          "speaker_id": "2",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022153.1.21",
          "form": "이제는 비 안오나봐요",
          "original_form": "이제는 비 안오나봐요",
          "speaker_id": "2",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022153.1.22",
          "form": "제가 올해부터 장화를 마련했는데",
          "original_form": "제가 올해부터 장화를 마련했는데",
          "speaker_id": "1",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022153.1.23",
          "form": "비 오는 날 필수 아이템이에요",
          "original_form": "비 오는 날 필수 아이템이에요",
          "speaker_id": "1",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022153.1.24",
          "form": "저도 장화 사고싶어지네요",
          "original_form": "저도 장화 사고싶어지네요",
          "speaker_id": "2",
          "time": "20210829 09:42"
        },
        {
          "id": "MDRW2100022153.1.25",
          "form": "근데 이번 여름은 정말 더웠어요",
          "original_form": "근데 이번 여름은 정말 더웠어요",
          "speaker_id": "2",
          "time": "20210829 09:42"
        },
        {
          "id": "MDRW2100022153.1.26",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210829 09:42"
        },
        {
          "id": "MDRW2100022153.1.27",
          "form": "가만히 있어도 땀이 흐르더라구요",
          "original_form": "가만히 있어도 땀이 흐르더라구요",
          "speaker_id": "1",
          "time": "20210829 09:42"
        },
        {
          "id": "MDRW2100022153.1.28",
          "form": "그래서 이번 여름에는",
          "original_form": "그래서 이번 여름에는",
          "speaker_id": "2",
          "time": "20210829 09:42"
        },
        {
          "id": "MDRW2100022153.1.29",
          "form": "수박을 그렇게 많이 먹었어요",
          "original_form": "수박을 그렇게 많이 먹었어요",
          "speaker_id": "2",
          "time": "20210829 09:43"
        },
        {
          "id": "MDRW2100022153.1.30",
          "form": "더울 때는 역시 수박이죠!!!",
          "original_form": "더울 때는 역시 수박이죠!!!",
          "speaker_id": "1",
          "time": "20210829 09:43"
        },
        {
          "id": "MDRW2100022153.1.31",
          "form": "이제 곧 추워지면",
          "original_form": "이제 곧 추워지면",
          "speaker_id": "2",
          "time": "20210829 09:43"
        },
        {
          "id": "MDRW2100022153.1.32",
          "form": "옷장에서 옷 꺼내야겠어요",
          "original_form": "옷장에서 옷 꺼내야겠어요",
          "speaker_id": "2",
          "time": "20210829 09:43"
        },
        {
          "id": "MDRW2100022153.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 09:43"
        },
        {
          "id": "MDRW2100022153.1.34",
          "form": "이런 것도 다 일이에요...",
          "original_form": "이런 것도 다 일이에요...",
          "speaker_id": "1",
          "time": "20210829 09:44"
        },
        {
          "id": "MDRW2100022153.1.35",
          "form": "계절이란 게 참 신기해요",
          "original_form": "계절이란 게 참 신기해요",
          "speaker_id": "2",
          "time": "20210829 09:44"
        }
      ]
    }
