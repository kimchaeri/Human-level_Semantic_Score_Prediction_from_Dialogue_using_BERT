      "utterance": [
        {
          "id": "MDRW2100040626.1.1",
          "form": "네네 진짜 그래요",
          "original_form": "네네 진짜 그래요",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040626.1.2",
          "form": "소릉 안먹는 대신",
          "original_form": "소릉 안먹는 대신",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040626.1.3",
          "form": "양고가를 많이 먹는댔나..?",
          "original_form": "양고가를 많이 먹는댔나..?",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040626.1.4",
          "form": "근데 인도 사람들은 (특히 남자) 그냥 안씻어요",
          "original_form": "근데 인도 사람들은 (특히 남자) 그냥 안씻어요",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040626.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040626.1.6",
          "form": "여긴 한국인데 왜 그럴까요",
          "original_form": "여긴 한국인데 왜 그럴까요",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040626.1.7",
          "form": "왜 안씻을까요",
          "original_form": "왜 안씻을까요",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040626.1.8",
          "form": "거기가 신분제가 강해서",
          "original_form": "거기가 신분제가 강해서",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040626.1.9",
          "form": "높은 신분이었던 사람들은 한국에 오면",
          "original_form": "높은 신분이었던 사람들은 한국에 오면",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040626.1.10",
          "form": "대학원생들이 다 자기보다 아래니까",
          "original_form": "대학원생들이 다 자기보다 아래니까",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100040626.1.11",
          "form": "잘 보일 필요도 없고",
          "original_form": "잘 보일 필요도 없고",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100040626.1.12",
          "form": "그냥... 습관이 안되어있어요ㅠ",
          "original_form": "그냥... 습관이 안되어있어요ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100040626.1.13",
          "form": "아니 뭔 다른나라에서 신분을 찾지요..? 이상한 사람들이네요",
          "original_form": "아니 뭔 다른나라에서 신분을 찾지요..? 이상한 사람들이네요",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040626.1.14",
          "form": "암튼 그런 냄새라면",
          "original_form": "암튼 그런 냄새라면",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040626.1.15",
          "form": "저는 도전하지 않을래요",
          "original_form": "저는 도전하지 않을래요",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040626.1.16",
          "form": "들디님 덕분에 피해갑니다",
          "original_form": "들디님 덕분에 피해갑니다",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040626.1.17",
          "form": "감사합니당 ㅋㅋㅋㅋ",
          "original_form": "감사합니당 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040626.1.18",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 네네네네 히히",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 네네네네 히히",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040626.1.19",
          "form": "맞아요 근데 이게",
          "original_form": "맞아요 근데 이게",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040626.1.20",
          "form": "세뇌가 되어있다 보니까.. 그렇더라구요",
          "original_form": "세뇌가 되어있다 보니까.. 그렇더라구요",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040626.1.21",
          "form": "아 근데 또 우유는 마시더라구요?",
          "original_form": "아 근데 또 우유는 마시더라구요?",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040626.1.22",
          "form": "소한테 나온건데요?",
          "original_form": "소한테 나온건데요?",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040626.1.23",
          "form": "뭐져?",
          "original_form": "뭐져?",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040626.1.24",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040626.1.25",
          "form": "그래서 참... 종교라는 것이!!!",
          "original_form": "그래서 참... 종교라는 것이!!!",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040626.1.26",
          "form": "그리고 예전에",
          "original_form": "그리고 예전에",
          "speaker_id": "1",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040626.1.27",
          "form": "롯데리아였나? 어떤 치킨버거에",
          "original_form": "롯데리아였나? 어떤 치킨버거에",
          "speaker_id": "1",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100040626.1.28",
          "form": "소고기가 좀 들어있었는데 그거 모르고 먹었다고 괴로워하더라구요ㅠ",
          "original_form": "소고기가 좀 들어있었는데 그거 모르고 먹었다고 괴로워하더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100040626.1.29",
          "form": "그런건 좀 안쓰럽네요..ㅠ 실수니까 그냥 넘어가도 될텐데",
          "original_form": "그런건 좀 안쓰럽네요..ㅠ 실수니까 그냥 넘어가도 될텐데",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040626.1.30",
          "form": "아! 그럼 name1님은 소고기 vs 돼지고기",
          "original_form": "아! 그럼 &name1&님은 소고기 vs 돼지고기",
          "speaker_id": "2",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040626.1.31",
          "form": "그쵸그쵸ㅠ",
          "original_form": "그쵸그쵸ㅠ",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040626.1.32",
          "form": "저는 그때 그때 다르긴한데",
          "original_form": "저는 그때 그때 다르긴한데",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040626.1.33",
          "form": "건강 생각하면 돼지인데",
          "original_form": "건강 생각하면 돼지인데",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040626.1.34",
          "form": "맛은 소고기요!!!",
          "original_form": "맛은 소고기요!!!",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040626.1.35",
          "form": "왜요?돼지 몸에 안좋아요?",
          "original_form": "왜요?돼지 몸에 안좋아요?",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040626.1.36",
          "form": "아 아뇨아뇨",
          "original_form": "아 아뇨아뇨",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040626.1.37",
          "form": "돼지가 차라리 몸에 괜찮아요!",
          "original_form": "돼지가 차라리 몸에 괜찮아요!",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040626.1.38",
          "form": "원래 육식 자체가 좀 건강 생각하면 지양해야하긴 하는데",
          "original_form": "원래 육식 자체가 좀 건강 생각하면 지양해야하긴 하는데",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040626.1.39",
          "form": "건강 생각하면 오리고기가 제일 좋을거예욬ㅋㅋㅋ",
          "original_form": "건강 생각하면 오리고기가 제일 좋을거예욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040626.1.40",
          "form": "아!! 말을 잘못했네요 소가 몸에 안좋아요?",
          "original_form": "아!! 말을 잘못했네요 소가 몸에 안좋아요?",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040626.1.41",
          "form": "그다음은 방목해서 기른 닭...?",
          "original_form": "그다음은 방목해서 기른 닭...?",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040626.1.42",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040626.1.43",
          "form": "네네 그렇다고 들었어요 아무래도",
          "original_form": "네네 그렇다고 들었어요 아무래도",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040626.1.44",
          "form": "뭐가 달라요?",
          "original_form": "뭐가 달라요?",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040626.1.45",
          "form": "오래 살고",
          "original_form": "오래 살고",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040626.1.46",
          "form": "그럴수록",
          "original_form": "그럴수록",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040626.1.47",
          "form": "항생제같은거 많이 맞고...",
          "original_form": "항생제같은거 많이 맞고...",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040626.1.48",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210905 00:13"
        }
      ]
