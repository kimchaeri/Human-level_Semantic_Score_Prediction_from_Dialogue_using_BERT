      "utterance": [
        {
          "id": "MDRW2100015156.1.1",
          "form": "막창좋아하시나요",
          "original_form": "막창좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015156.1.2",
          "form": "제 인생은 두가지로 나뉩니다",
          "original_form": "제 인생은 두가지로 나뉩니다",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015156.1.3",
          "form": "팔공막창을 알기전과후로요",
          "original_form": "팔공막창을 알기전과후로요",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015156.1.4",
          "form": "생막창과막장필수요",
          "original_form": "생막창과막장필수요",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015156.1.5",
          "form": "그막창으로는 한시간에 쇠주3병은 순식간이에요",
          "original_form": "그막창으로는 한시간에 쇠주3병은 순식간이에요",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015156.1.6",
          "form": "막창은그냥먹을수없죠",
          "original_form": "막창은그냥먹을수없죠",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015156.1.7",
          "form": "비가쏟아지는날한번가고싶네요",
          "original_form": "비가쏟아지는날한번가고싶네요",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015156.1.8",
          "form": "비오는날막창과함께",
          "original_form": "비오는날막창과함께",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015156.1.9",
          "form": "지금 비오네요",
          "original_form": "지금 비오네요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015156.1.10",
          "form": "가고싶네요",
          "original_form": "가고싶네요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015156.1.11",
          "form": "지금비오나요?",
          "original_form": "지금비오나요?",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015156.1.12",
          "form": "막창에 막장이진짜",
          "original_form": "막창에 막장이진짜",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015156.1.13",
          "form": "다른거찍어먹을수없어요",
          "original_form": "다른거찍어먹을수없어요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015156.1.14",
          "form": "그리고 포슬한 계란찜까지",
          "original_form": "그리고 포슬한 계란찜까지",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015156.1.15",
          "form": "그건인정이죠",
          "original_form": "그건인정이죠",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015156.1.16",
          "form": "보들보들해야합니다",
          "original_form": "보들보들해야합니다",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015156.1.17",
          "form": "푸딩처럼",
          "original_form": "푸딩처럼",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015156.1.18",
          "form": "대구막창이 진짜 넘사이긴하더라고요",
          "original_form": "대구막창이 진짜 넘사이긴하더라고요",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015156.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015156.1.20",
          "form": "서울에서파는건 맛없고비싸요",
          "original_form": "서울에서파는건 맛없고비싸요",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015156.1.21",
          "form": "듣기로는 대구원조이름걸고 판다던데요?",
          "original_form": "듣기로는 대구원조이름걸고 판다던데요?",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015156.1.22",
          "form": "맞아요ㅋㅋ근데다르죠",
          "original_form": "맞아요ㅋㅋ근데다르죠",
