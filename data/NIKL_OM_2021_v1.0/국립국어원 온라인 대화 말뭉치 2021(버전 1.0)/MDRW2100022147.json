      "utterance": [
        {
          "id": "MDRW2100022147.1.1",
          "form": "반려동물이 있으신가요?",
          "original_form": "반려동물이 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 09:25"
        },
        {
          "id": "MDRW2100022147.1.2",
          "form": "아니요 없습니다",
          "original_form": "아니요 없습니다",
          "speaker_id": "1",
          "time": "20210829 09:26"
        },
        {
          "id": "MDRW2100022147.1.3",
          "form": "하지만 기르고 싶어요!",
          "original_form": "하지만 기르고 싶어요!",
          "speaker_id": "1",
          "time": "20210829 09:26"
        },
        {
          "id": "MDRW2100022147.1.4",
          "form": "어떤 동물을 기르고 싶으세요?",
          "original_form": "어떤 동물을 기르고 싶으세요?",
          "speaker_id": "2",
          "time": "20210829 09:27"
        },
        {
          "id": "MDRW2100022147.1.5",
          "form": "저는 강아지가 좋지만",
          "original_form": "저는 강아지가 좋지만",
          "speaker_id": "1",
          "time": "20210829 09:27"
        },
        {
          "id": "MDRW2100022147.1.6",
          "form": "고양이도 괜찮아요",
          "original_form": "고양이도 괜찮아요",
          "speaker_id": "1",
          "time": "20210829 09:27"
        },
        {
          "id": "MDRW2100022147.1.7",
          "form": "강아지는 같이 놀아줘야해서",
          "original_form": "강아지는 같이 놀아줘야해서",
          "speaker_id": "2",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100022147.1.8",
          "form": "집에 혼자 두면 엄청 미안할거같아요",
          "original_form": "집에 혼자 두면 엄청 미안할거같아요",
          "speaker_id": "2",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100022147.1.9",
          "form": "근데 그건 고양이도 마찬가지래요~",
          "original_form": "근데 그건 고양이도 마찬가지래요~",
          "speaker_id": "1",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100022147.1.10",
          "form": "혼자 두면",
          "original_form": "혼자 두면",
          "speaker_id": "1",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100022147.1.11",
          "form": "외로워서 스트레스 받는다고 하더라구요",
          "original_form": "외로워서 스트레스 받는다고 하더라구요",
          "speaker_id": "1",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100022147.1.12",
          "form": "하긴...",
          "original_form": "하긴...",
          "speaker_id": "2",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100022147.1.13",
          "form": "고양이 기르는 친구들은",
          "original_form": "고양이 기르는 친구들은",
          "speaker_id": "2",
          "time": "20210829 09:28"
        },
        {
          "id": "MDRW2100022147.1.14",
          "form": "그런 말 안하긴 했지만",
          "original_form": "그런 말 안하긴 했지만",
          "speaker_id": "2",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100022147.1.15",
          "form": "뭐든 쉽지 않네요~~~",
          "original_form": "뭐든 쉽지 않네요~~~",
          "speaker_id": "2",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100022147.1.16",
          "form": "강아지들은",
          "original_form": "강아지들은",
          "speaker_id": "1",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100022147.1.17",
          "form": "주인이 자기 전부라서",
          "original_form": "주인이 자기 전부라서",
          "speaker_id": "1",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100022147.1.18",
          "form": "잘 못해주면 너무 미안할거같아요",
          "original_form": "잘 못해주면 너무 미안할거같아요",
          "speaker_id": "1",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100022147.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100022147.1.20",
          "form": "제 친구도 얼마전에",
          "original_form": "제 친구도 얼마전에",
          "speaker_id": "2",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100022147.1.21",
          "form": "강아지가 하늘나라로 떠났어요 ㅠㅠ",
          "original_form": "강아지가 하늘나라로 떠났어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100022147.1.22",
          "form": "고양이도...",
          "original_form": "고양이도...",
          "speaker_id": "2",
          "time": "20210829 09:29"
        },
        {
          "id": "MDRW2100022147.1.23",
          "form": "고양이가 죽으면",
          "original_form": "고양이가 죽으면",
          "speaker_id": "1",
          "time": "20210829 09:30"
        },
        {
          "id": "MDRW2100022147.1.24",
          "form": "고양이별로 떠났다고 하더라구요",
          "original_form": "고양이별로 떠났다고 하더라구요",
          "speaker_id": "1",
          "time": "20210829 09:30"
        },
        {
          "id": "MDRW2100022147.1.25",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 09:30"
        },
        {
          "id": "MDRW2100022147.1.26",
          "form": "강아지는",
          "original_form": "강아지는",
          "speaker_id": "2",
          "time": "20210829 09:30"
        },
        {
          "id": "MDRW2100022147.1.27",
          "form": "무지개 다리 건넜다고 하더라구요...",
          "original_form": "무지개 다리 건넜다고 하더라구요...",
          "speaker_id": "2",
          "time": "20210829 09:30"
        },
        {
          "id": "MDRW2100022147.1.28",
          "form": "아름다운 표현이지만",
          "original_form": "아름다운 표현이지만",
          "speaker_id": "1",
          "time": "20210829 09:30"
        },
        {
          "id": "MDRW2100022147.1.29",
          "form": "마음이 아픈 것도 사실이에요",
          "original_form": "마음이 아픈 것도 사실이에요",
          "speaker_id": "1",
          "time": "20210829 09:30"
        },
        {
          "id": "MDRW2100022147.1.30",
          "form": "아 그리고 제 블로그 이웃 중 한 분은",
          "original_form": "아 그리고 제 블로그 이웃 중 한 분은",
          "speaker_id": "2",
          "time": "20210829 09:31"
        },
        {
          "id": "MDRW2100022147.1.31",
          "form": "햄스터를 기르셨는데",
          "original_form": "햄스터를 기르셨는데",
          "speaker_id": "2",
          "time": "20210829 09:31"
        },
        {
          "id": "MDRW2100022147.1.32",
          "form": "햄스터도 한 달 동안 아프더니",
          "original_form": "햄스터도 한 달 동안 아프더니",
          "speaker_id": "2",
          "time": "20210829 09:31"
        },
        {
          "id": "MDRW2100022147.1.33",
          "form": "해씨별로 떠났다고 하더라구요",
          "original_form": "해씨별로 떠났다고 하더라구요",
          "speaker_id": "2",
          "time": "20210829 09:31"
        },
        {
          "id": "MDRW2100022147.1.34",
          "form": "이별은 누구와 하든 항상 아픈거같아요ㅠ",
          "original_form": "이별은 누구와 하든 항상 아픈거같아요ㅠ",
          "speaker_id": "1",
          "time": "20210829 09:31"
        },
        {
          "id": "MDRW2100022147.1.35",
          "form": "친구분들과 들디님의 이웃님 모두",
          "original_form": "친구분들과 들디님의 이웃님 모두",
          "speaker_id": "1",
          "time": "20210829 09:31"
        },
        {
          "id": "MDRW2100022147.1.36",
          "form": "상처를 극복하시길 바랄게요",
          "original_form": "상처를 극복하시길 바랄게요",
          "speaker_id": "1",
          "time": "20210829 09:31"
        },
        {
          "id": "MDRW2100022147.1.37",
          "form": "감사합니다 ㅎㅎ",
          "original_form": "감사합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 09:31"
        }
      ]
    }
