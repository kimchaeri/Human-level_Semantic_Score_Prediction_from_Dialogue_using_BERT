      "utterance": [
        {
          "id": "MDRW2100038285.1.1",
          "form": "어제 횟집 가서 회먹었어요ㅋㅋㅋ",
          "original_form": "어제 횟집 가서 회먹었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100038285.1.2",
          "form": "매운탕까지 코스로!!",
          "original_form": "매운탕까지 코스로!!",
          "speaker_id": "2",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100038285.1.3",
          "form": "근데 남친이 아무래도 저보다 적게 먹는 거 같아요",
          "original_form": "근데 남친이 아무래도 저보다 적게 먹는 거 같아요",
          "speaker_id": "2",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100038285.1.4",
          "form": "크으......",
          "original_form": "크으......",
          "speaker_id": "1",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100038285.1.5",
          "form": "회는 언제 먹어도 맛있죠ㅜㅜ",
          "original_form": "회는 언제 먹어도 맛있죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100038285.1.6",
          "form": "어제 저녁 뭐 드셨나요ㅎㅎ",
          "original_form": "어제 저녁 뭐 드셨나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:09"
        },
        {
          "id": "MDRW2100038285.1.7",
          "form": "괜찮아요 먹다보면 늘어날거예요ㅋㅋㅋㅋ",
          "original_form": "괜찮아요 먹다보면 늘어날거예요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:09"
        },
        {
          "id": "MDRW2100038285.1.8",
          "form": "늘 맛있는거 챙겨드시던데",
          "original_form": "늘 맛있는거 챙겨드시던데",
          "speaker_id": "2",
          "time": "20210902 08:09"
        },
        {
          "id": "MDRW2100038285.1.9",
          "form": "ㅜㅜ오늘 약속있어서 맛난거 많이 먹으려고",
          "original_form": "ㅜㅜ오늘 약속있어서 맛난거 많이 먹으려고",
          "speaker_id": "1",
          "time": "20210902 08:10"
        },
        {
          "id": "MDRW2100038285.1.10",
          "form": "굶다가 밤에 결국",
          "original_form": "굶다가 밤에 결국",
          "speaker_id": "1",
          "time": "20210902 08:11"
        },
        {
          "id": "MDRW2100038285.1.11",
          "form": "컵라면과 편의점 도시락 먹었어요ㅋㅋ",
          "original_form": "컵라면과 편의점 도시락 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:11"
        },
        {
          "id": "MDRW2100038285.1.12",
          "form": "앜ㅋㅋ뭐 얼마나 맛잇는 걸 드실려고 미리 굶으려고 했어요",
          "original_form": "앜ㅋㅋ뭐 얼마나 맛잇는 걸 드실려고 미리 굶으려고 했어요",
          "speaker_id": "2",
          "time": "20210902 08:12"
        },
        {
          "id": "MDRW2100038285.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ새로생긴 핫한카페랑",
          "original_form": "ㅋㅋㅋㅋㅋㅋ새로생긴 핫한카페랑",
          "speaker_id": "1",
          "time": "20210902 08:14"
        },
        {
          "id": "MDRW2100038285.1.14",
          "form": "제가 좋아하는",
          "original_form": "제가 좋아하는",
          "speaker_id": "1",
          "time": "20210902 08:14"
        },
        {
          "id": "MDRW2100038285.1.15",
          "form": "제가 좋아하는 안주 먹으러 가려구요",
          "original_form": "제가 좋아하는 안주 먹으러 가려구요",
          "speaker_id": "1",
          "time": "20210902 08:14"
        },
        {
          "id": "MDRW2100038285.1.16",
          "form": "오랜만에 술!",
          "original_form": "오랜만에 술!",
          "speaker_id": "1",
          "time": "20210902 08:14"
        },
        {
          "id": "MDRW2100038285.1.17",
          "form": "ㅋㅋㅋㅋ우왕 한잔하기 좋은 날씨에요",
          "original_form": "ㅋㅋㅋㅋ우왕 한잔하기 좋은 날씨에요",
          "speaker_id": "2",
          "time": "20210902 08:14"
        },
        {
          "id": "MDRW2100038285.1.18",
          "form": "좋아하는 안주가 뭐에요",
          "original_form": "좋아하는 안주가 뭐에요",
          "speaker_id": "2",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100038285.1.19",
          "form": "다 좋아하는데 오늘 가는곳은",
          "original_form": "다 좋아하는데 오늘 가는곳은",
          "speaker_id": "1",
          "time": "20210902 08:18"
        },
        {
          "id": "MDRW2100038285.1.20",
          "form": "오꼬노미야끼요ㅋㅋㅋㅋ",
          "original_form": "오꼬노미야끼요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:18"
        },
        {
          "id": "MDRW2100038285.1.21",
          "form": "20살되자마자 가던 곳인데 최근 몇년은",
          "original_form": "20살되자마자 가던 곳인데 최근 몇년은",
          "speaker_id": "1",
          "time": "20210902 08:18"
        },
        {
          "id": "MDRW2100038285.1.22",
          "form": "술친구가 없어서",
          "original_form": "술친구가 없어서",
          "speaker_id": "1",
          "time": "20210902 08:18"
        },
        {
          "id": "MDRW2100038285.1.23",
          "form": "못갔더든요ㅜㅜ",
          "original_form": "못갔더든요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 08:18"
        },
        {
          "id": "MDRW2100038285.1.24",
          "form": "앜ㅋㅋ맛있는 거 맞네요",
          "original_form": "앜ㅋㅋ맛있는 거 맞네요",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038285.1.25",
          "form": "술친구 이제 생겼어오?",
          "original_form": "술친구 이제 생겼어오?",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038285.1.26",
          "form": "친구가 술 잘 안 마셨는데",
          "original_form": "친구가 술 잘 안 마셨는데",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038285.1.27",
          "form": "오늘은 차 두고 마시자고 각잡아서",
          "original_form": "오늘은 차 두고 마시자고 각잡아서",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038285.1.28",
          "form": "달리러갑니다ㅋㅋㅋㅋ",
          "original_form": "달리러갑니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038285.1.29",
          "form": "와ㅋㅋㅋㅋ재밌겠어요 저두 각잡고 마신 게 꽤 오래됐네요",
          "original_form": "와ㅋㅋㅋㅋ재밌겠어요 저두 각잡고 마신 게 꽤 오래됐네요",
          "speaker_id": "2",
