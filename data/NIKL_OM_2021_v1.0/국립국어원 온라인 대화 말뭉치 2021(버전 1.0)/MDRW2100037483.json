      "utterance": [
        {
          "id": "MDRW2100037483.1.1",
          "form": "오늘 저녁에 같이 치킨 시켜먹을까",
          "original_form": "오늘 저녁에 같이 치킨 시켜먹을까",
          "speaker_id": "1",
          "time": "20210901 00:56"
        },
        {
          "id": "MDRW2100037483.1.2",
          "form": "아이스크림 먹는다며",
          "original_form": "아이스크림 먹는다며",
          "speaker_id": "2",
          "time": "20210901 00:56"
        },
        {
          "id": "MDRW2100037483.1.3",
          "form": "그건 두고 먹어도 되고 후식이지",
          "original_form": "그건 두고 먹어도 되고 후식이지",
          "speaker_id": "1",
          "time": "20210901 00:56"
        },
        {
          "id": "MDRW2100037483.1.4",
          "form": "치킨먹고싶은거야",
          "original_form": "치킨먹고싶은거야",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100037483.1.5",
          "form": "혼자서 치킨먹으면 항상 남아서",
          "original_form": "혼자서 치킨먹으면 항상 남아서",
          "speaker_id": "1",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100037483.1.6",
          "form": "아 그렇겠다",
          "original_form": "아 그렇겠다",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100037483.1.7",
          "form": "언니 온 김에 치킨 시켜 먹을려고",
          "original_form": "언니 온 김에 치킨 시켜 먹을려고",
          "speaker_id": "1",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100037483.1.8",
          "form": "어디꺼",
          "original_form": "어디꺼",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100037483.1.9",
          "form": "비비큐나 굽네",
          "original_form": "비비큐나 굽네",
          "speaker_id": "1",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100037483.1.10",
          "form": "교촌은",
          "original_form": "교촌은",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100037483.1.11",
          "form": "교촌은 양념이 짜다 그리고 기름지고",
          "original_form": "교촌은 양념이 짜다 그리고 기름지고",
          "speaker_id": "1",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100037483.1.12",
          "form": "응 그럼 굽네로",
          "original_form": "응 그럼 굽네로",
          "speaker_id": "2",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100037483.1.13",
          "form": "볼케이노나 사이드로 닭발 시킬까",
          "original_form": "볼케이노나 사이드로 닭발 시킬까",
          "speaker_id": "1",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100037483.1.14",
          "form": "두마리나",
          "original_form": "두마리나",
          "speaker_id": "2",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100037483.1.15",
          "form": "이건 반마리씩 안해주더라",
          "original_form": "이건 반마리씩 안해주더라",
          "speaker_id": "1",
