      "utterance": [
        {
          "id": "MDRW2100027888.1.1",
          "form": "요즘 선선해졌죠 아무래도",
          "original_form": "요즘 선선해졌죠 아무래도",
          "speaker_id": "2",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027888.1.2",
          "form": "그쵸 그리고 어제도 오늘도 비가 온다",
          "original_form": "그쵸 그리고 어제도 오늘도 비가 온다",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027888.1.3",
          "form": "부산도 비 오나요... 광주도...",
          "original_form": "부산도 비 오나요... 광주도...",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027888.1.4",
          "form": "그래두 저는 더운 거보다 나은 거 같아요",
          "original_form": "그래두 저는 더운 거보다 나은 거 같아요",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027888.1.5",
          "form": "여름 완전 싫어",
          "original_form": "여름 완전 싫어",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027888.1.6",
          "form": "맞다 다만 퇴근할 때 쫄딱 젖을 뿐...",
          "original_form": "맞다 다만 퇴근할 때 쫄딱 젖을 뿐...",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027888.1.7",
          "form": "습기 차있으니 확 내리는 게 낫지",
          "original_form": "습기 차있으니 확 내리는 게 낫지",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027888.1.8",
          "form": "집 근처 하천이 늘 넘치지만...",
          "original_form": "집 근처 하천이 늘 넘치지만...",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027888.1.9",
          "form": "마자 젖는 건 싫죠....",
          "original_form": "마자 젖는 건 싫죠....",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027888.1.10",
          "form": "하천 넘친다니 무섭잖아....",
          "original_form": "하천 넘친다니 무섭잖아....",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027888.1.11",
          "form": "비 그 정도로 많이 와요?",
          "original_form": "비 그 정도로 많이 와요?",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027888.1.12",
          "form": "그냥... 심심하면 넘쳐",
          "original_form": "그냥... 심심하면 넘쳐",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027888.1.13",
          "form": "저번에 많이 왔을 땐 넘칠 만하긴 했지만",
          "original_form": "저번에 많이 왔을 땐 넘칠 만하긴 했지만",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027888.1.14",
          "form": "잉... 무섭다",
          "original_form": "잉... 무섭다",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027888.1.15",
          "form": "address 사는 것도 좋지만은 않군요",
          "original_form": "&address& 사는 것도 좋지만은 않군요",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027888.1.16",
          "form": "뭔가 address 살고싶다고 생각했었는데...",
          "original_form": "뭔가 &address& 살고싶다고 생각했었는데...",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027888.1.17",
          "form": "산책로랑 하천 높이가 별로 차이 안 나서 그런 것 같기도 하고...",
          "original_form": "산책로랑 하천 높이가 별로 차이 안 나서 그런 것 같기도 하고...",
          "speaker_id": "1",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027888.1.18",
          "form": "하지만 있으면 좋다 날씨 좋으면 견주들 엄청 산책 나옴",
          "original_form": "하지만 있으면 좋다 날씨 좋으면 견주들 엄청 산책 나옴",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027888.1.19",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027888.1.20",
          "form": "맞아 견주들 엄청 많겠다",
          "original_form": "맞아 견주들 엄청 많겠다",
          "speaker_id": "2",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100027888.1.21",
          "form": "요즘 산책하기 딱 좋은 날씨잖아요",
          "original_form": "요즘 산책하기 딱 좋은 날씨잖아요",
          "speaker_id": "2",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100027888.1.22",
          "form": "아주 귀여워요.....저한텐 달리기 좋은 날씨지만...",
          "original_form": "아주 귀여워요.....저한텐 달리기 좋은 날씨지만...",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027888.1.23",
          "form": "마자 name1님 런닝하는 거 멋져...",
          "original_form": "마자 &name1&님 런닝하는 거 멋져...",
          "speaker_id": "2",
          "time": "20210902 17:30"
        },
        {
