          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100011495.1.12",
          "form": "저도해보고싶어요",
          "original_form": "저도해보고싶어요",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100011495.1.13",
          "form": "등산 좋아하시나봐요",
          "original_form": "등산 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100011495.1.14",
          "form": "한번도요ㅋㅋㅋㅋ",
          "original_form": "한번도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100011495.1.15",
          "form": "그래서 해보고싶네요",
