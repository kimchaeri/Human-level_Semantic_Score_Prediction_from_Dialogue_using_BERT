          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100026399.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100026399.1.49",
