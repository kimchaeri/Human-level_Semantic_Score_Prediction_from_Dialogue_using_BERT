      "utterance": [
        {
          "id": "MDRW2100026399.1.1",
          "form": "결혼할 생각 있어?",
          "original_form": "결혼할 생각 있어?",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026399.1.2",
          "form": "글쎄 아직은 없는거 같아",
          "original_form": "글쎄 아직은 없는거 같아",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026399.1.3",
          "form": "우선 옆에 앉은 사람이 있는지부커",
          "original_form": "우선 옆에 앉은 사람이 있는지부커",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026399.1.4",
          "form": "물어주셔야...",
          "original_form": "물어주셔야...",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026399.1.5",
          "form": "ㅋㅋㅋㅋㅋ 애인 없어도 결혼은 하고싶을 수 있지",
          "original_form": "ㅋㅋㅋㅋㅋ 애인 없어도 결혼은 하고싶을 수 있지",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026399.1.6",
          "form": "연애할 생각은 있고?",
          "original_form": "연애할 생각은 있고?",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026399.1.7",
          "form": "그것도 글쎄...",
          "original_form": "그것도 글쎄...",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026399.1.8",
          "form": "아직 혼자가 편한걸",
          "original_form": "아직 혼자가 편한걸",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026399.1.9",
          "form": "마자 나도 연애할 생각 없는듯",
          "original_form": "마자 나도 연애할 생각 없는듯",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026399.1.10",
          "form": "연애나 결혼이나 너무 다른사람에게 많이 맞춰줘야하는것 같음",
          "original_form": "연애나 결혼이나 너무 다른사람에게 많이 맞춰줘야하는것 같음",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026399.1.11",
          "form": "맞아 서로 맞춰가야하는데",
          "original_form": "맞아 서로 맞춰가야하는데",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026399.1.12",
          "form": "나는 내 자신 맞추기도 버거운거 같음",
          "original_form": "나는 내 자신 맞추기도 버거운거 같음",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026399.1.13",
          "form": "나 혼자 살기에도 힘든데 남에게 맞춰가면서 사는 건 더 힘들겠지",
          "original_form": "나 혼자 살기에도 힘든데 남에게 맞춰가면서 사는 건 더 힘들겠지",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026399.1.14",
          "form": "그래서 나는 아직 혼자가 편한거 같아",
          "original_form": "그래서 나는 아직 혼자가 편한거 같아",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026399.1.15",
          "form": "이 마음 변하지 않으면 평생 혼자이지 않을까 싶음",
          "original_form": "이 마음 변하지 않으면 평생 혼자이지 않을까 싶음",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026399.1.16",
          "form": "ㅎㅎ 비혼이면 할 얘기 없으니까 결혼이랑 연애 한다고 치고",
          "original_form": "ㅎㅎ 비혼이면 할 얘기 없으니까 결혼이랑 연애 한다고 치고",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026399.1.17",
          "form": "어떤 사람이면 연애할 거 같음?",
          "original_form": "어떤 사람이면 연애할 거 같음?",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026399.1.18",
          "form": "나는 내가 맞춰주긴 힘들다고 생각하니깐",
          "original_form": "나는 내가 맞춰주긴 힘들다고 생각하니깐",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026399.1.19",
          "form": "나한테 맞춰주는 사람",
          "original_form": "나한테 맞춰주는 사람",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026399.1.20",
          "form": "너는 어때",
          "original_form": "너는 어때",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100026399.1.21",
          "form": "만나게되면 서로 맞춰가려고 하겠지만",
          "original_form": "만나게되면 서로 맞춰가려고 하겠지만",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100026399.1.22",
          "form": "나만 바라봐주는 사람이면 좋겠지만 그건 사치겠다 싶고",
          "original_form": "나만 바라봐주는 사람이면 좋겠지만 그건 사치겠다 싶고",
          "speaker_id": "2",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100026399.1.23",
          "form": "그렇긴 해",
          "original_form": "그렇긴 해",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100026399.1.24",
          "form": "급한 상황에서 1순위가 내가 되는 사람",
          "original_form": "급한 상황에서 1순위가 내가 되는 사람",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100026399.1.25",
          "form": "이면 좋겠다고 생각해",
          "original_form": "이면 좋겠다고 생각해",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100026399.1.26",
          "form": "오 나름대로 합리적인 판단이네",
          "original_form": "오 나름대로 합리적인 판단이네",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100026399.1.27",
          "form": "하긴 평소에 다 맞춰줘도 급할때 어긋나면 파국이니까",
          "original_form": "하긴 평소에 다 맞춰줘도 급할때 어긋나면 파국이니까",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100026399.1.28",
          "form": "그리고 취미가 맞으면 좋지만",
          "original_form": "그리고 취미가 맞으면 좋지만",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100026399.1.29",
          "form": "같이 하는건 원하지 않아",
          "original_form": "같이 하는건 원하지 않아",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100026399.1.30",
          "form": "뭔지 아니",
          "original_form": "뭔지 아니",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100026399.1.31",
          "form": "같이 하다가 싸울까봐인데ㅋㅋㅋ",
          "original_form": "같이 하다가 싸울까봐인데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100026399.1.32",
          "form": "그것도 그렇고",
          "original_form": "그것도 그렇고",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100026399.1.33",
          "form": "취미는 내 힐링인데",
          "original_form": "취미는 내 힐링인데",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100026399.1.34",
          "form": "한두번이면 몰라 꾸준히? 흠",
          "original_form": "한두번이면 몰라 꾸준히? 흠",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100026399.1.35",
          "form": "ㅋㅋㅋㅋ남의 취미는 존중해줄 수 있지만",
          "original_form": "ㅋㅋㅋㅋ남의 취미는 존중해줄 수 있지만",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100026399.1.36",
          "form": "내 취미에 태클건다?",
          "original_form": "내 취미에 태클건다?",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100026399.1.37",
          "form": "바로 죽빵나가는거임",
          "original_form": "바로 죽빵나가는거임",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100026399.1.38",
          "form": "ㅋㅋㅋㅋㅋㄹ 취미에 태클거는건 애인이라도 좀 그렇지",
          "original_form": "ㅋㅋㅋㅋㅋㄹ 취미에 태클거는건 애인이라도 좀 그렇지",
          "speaker_id": "1",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100026399.1.39",
          "form": "그리고 아마 만나더라도",
          "original_form": "그리고 아마 만나더라도",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100026399.1.40",
          "form": "운동하는 곳에서 만나자고",
          "original_form": "운동하는 곳에서 만나자고",
