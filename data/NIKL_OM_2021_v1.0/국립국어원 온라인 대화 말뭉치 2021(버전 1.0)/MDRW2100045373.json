      "utterance": [
        {
          "id": "MDRW2100045373.1.1",
          "form": "원래 우리나라에서",
          "original_form": "원래 우리나라에서",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045373.1.2",
          "form": "괜히 막드가 흥하는게 아니니까",
          "original_form": "괜히 막드가 흥하는게 아니니까",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045373.1.3",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045373.1.4",
          "form": "한번 보러가여",
          "original_form": "한번 보러가여",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045373.1.5",
          "form": "물론 막드라고 다 흥하는건 아니지만",
          "original_form": "물론 막드라고 다 흥하는건 아니지만",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045373.1.6",
          "form": "지킬은 증명된 작품이니..",
          "original_form": "지킬은 증명된 작품이니..",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045373.1.7",
          "form": "류님공연은 다시안올라올수도있어서",
          "original_form": "류님공연은 다시안올라올수도있어서",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045373.1.8",
          "form": "아 양준모 컨프롱 영상을 보내두었습니다",
          "original_form": "아 양준모 컨프롱 영상을 보내두었습니다",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045373.1.9",
          "form": "나중에보소소",
          "original_form": "나중에보소소",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045373.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045373.1.11",
          "form": "류님 드큘도 보내놨습니다",
          "original_form": "류님 드큘도 보내놨습니다",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045373.1.12",
          "form": "그거두개는 꼭 보소서...",
          "original_form": "그거두개는 꼭 보소서...",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045373.1.13",
          "form": "추천드립니다...",
          "original_form": "추천드립니다...",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045373.1.14",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.15",
          "form": "앗 이따",
          "original_form": "앗 이따",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.16",
          "form": "자기전에 보고 자야겟다",
          "original_form": "자기전에 보고 자야겟다",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.17",
          "form": "꿈자리가 조금",
          "original_form": "꿈자리가 조금",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.18",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.19",
          "form": "걱정은 되지만...",
          "original_form": "걱정은 되지만...",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.20",
          "form": "ㅋㅋㅋㅋ좋응ㄴ걸로만",
          "original_form": "ㅋㅋㅋㅋ좋응ㄴ걸로만",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.21",
          "form": "보내드렸습ㄴ디ㅏ",
          "original_form": "보내드렸습ㄴ디ㅏ",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.22",
          "form": "꿈에는",
          "original_form": "꿈에는",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.23",
          "form": "안좋겠지만...",
          "original_form": "안좋겠지만...",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.25",
          "form": "ㅋㅋㅋㅋ꿈ㅇ는..",
          "original_form": "ㅋㅋㅋㅋ꿈ㅇ는..",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.26",
          "form": "안좋지..",
          "original_form": "안좋지..",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.27",
          "form": "악몽꾸고싶을떄",
          "original_form": "악몽꾸고싶을떄",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.28",
          "form": "안그래도 그거 뮤지컬 보고 오니까",
          "original_form": "안그래도 그거 뮤지컬 보고 오니까",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.29",
          "form": "도움이되겠네...",
          "original_form": "도움이되겠네...",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045373.1.30",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045373.1.31",
          "form": "그런 분위기의 꿈을 자꾸 꾸긴",
          "original_form": "그런 분위기의 꿈을 자꾸 꾸긴",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045373.1.32",
          "form": "하더라고여",
          "original_form": "하더라고여",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045373.1.33",
          "form": "영향을 받나...",
          "original_form": "영향을 받나...",
