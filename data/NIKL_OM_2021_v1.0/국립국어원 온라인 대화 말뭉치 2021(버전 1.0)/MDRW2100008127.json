      "utterance": [
        {
          "id": "MDRW2100008127.1.1",
          "form": "배달 음식 드세요?",
          "original_form": "배달 음식 드세요?",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008127.1.2",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008127.1.3",
          "form": "라멘 되게 좋아해요",
          "original_form": "라멘 되게 좋아해요",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008127.1.4",
          "form": "라멘 좋아하세요?",
          "original_form": "라멘 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008127.1.5",
          "form": "가끔 먹어요",
          "original_form": "가끔 먹어요",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008127.1.6",
          "form": "어떤 라멘 좋아하세요?",
          "original_form": "어떤 라멘 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008127.1.7",
          "form": "저는 쇼유라멘 좋아해요",
          "original_form": "저는 쇼유라멘 좋아해요",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008127.1.8",
          "form": "그럼 어떤음식 좋아하세요?",
          "original_form": "그럼 어떤음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008127.1.9",
          "form": "저는 요즘 마라탕 먹어요",
          "original_form": "저는 요즘 마라탕 먹어요",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008127.1.10",
          "form": "마라탕 드셔보셨어요?",
          "original_form": "마라탕 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008127.1.11",
          "form": "마라탕면이었나 그거는 먹어봤어요",
          "original_form": "마라탕면이었나 그거는 먹어봤어요",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008127.1.12",
          "form": "제가 매운걸 잘 못먹어서",
          "original_form": "제가 매운걸 잘 못먹어서",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008127.1.13",
          "form": "아 그러면 힘들죠",
          "original_form": "아 그러면 힘들죠",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008127.1.14",
          "form": "요리는 자주 해드시나요?",
          "original_form": "요리는 자주 해드시나요?",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008127.1.15",
          "form": "엄청 자주하는편은 아닌데 그래도 맛있는건 좋아해요",
          "original_form": "엄청 자주하는편은 아닌데 그래도 맛있는건 좋아해요",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008127.1.16",
          "form": "어떤 요리 할줄아세요?",
          "original_form": "어떤 요리 할줄아세요?",
          "speaker_id": "2",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008127.1.17",
          "form": "해먹는 건 국수 자주 해먹는 거 같아요",
          "original_form": "해먹는 건 국수 자주 해먹는 거 같아요",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008127.1.18",
          "form": "요즘에는 식단 조절중이라 그냥 고기 좀 굽는 거 말고는 요리랄 게 없어서요ㅎㅎ",
          "original_form": "요즘에는 식단 조절중이라 그냥 고기 좀 굽는 거 말고는 요리랄 게 없어서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008127.1.19",
          "form": "아 다이어트 하시는구나",
          "original_form": "아 다이어트 하시는구나",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008127.1.20",
          "form": "다이어트할때는 계란이 배 많이부르게 해서 좋은거같아요",
          "original_form": "다이어트할때는 계란이 배 많이부르게 해서 좋은거같아요",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008127.1.21",
          "form": "네 건강 때문에",
          "original_form": "네 건강 때문에",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008127.1.22",
          "form": "계란 요리 좋아하세요?",
          "original_form": "계란 요리 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008127.1.23",
          "form": "네 요즘은 교정하느라 계란요리 자주먹는거같아요",
          "original_form": "네 요즘은 교정하느라 계란요리 자주먹는거같아요",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008127.1.24",
          "form": "제 친구도 교정할 때 계란찜 많이 먹더라고요",
          "original_form": "제 친구도 교정할 때 계란찜 많이 먹더라고요",
          "speaker_id": "1",
          "time": "20210822 14:55"
        }
