      "utterance": [
        {
          "id": "MDRW2100043640.1.1",
          "form": "으으음 쌀이 들어가나요?",
          "original_form": "으으음 쌀이 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043640.1.2",
          "form": "제가 생각하는 음식을",
          "original_form": "제가 생각하는 음식을",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043640.1.3",
          "form": "맞춰주세욯",
          "original_form": "맞춰주세욯",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043640.1.4",
          "form": "쌀 안 들어가는 음식이예요?!",
          "original_form": "쌀 안 들어가는 음식이예요?!",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043640.1.5",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043640.1.6",
          "form": "밥 안들어갑니다",
          "original_form": "밥 안들어갑니다",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043640.1.7",
          "form": "그렇담 면이 들어가나요?",
          "original_form": "그렇담 면이 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043640.1.8",
          "form": "아니요! 면 안들어가요",
          "original_form": "아니요! 면 안들어가요",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043640.1.9",
          "form": "하지만 밀가루는 들어간답니다",
          "original_form": "하지만 밀가루는 들어간답니다",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043640.1.10",
          "form": "헉 그럼 빵 들어가나요?",
          "original_form": "헉 그럼 빵 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043640.1.11",
          "form": "빵....의 일종일까요? 이건모르겠는데",
          "original_form": "빵....의 일종일까요? 이건모르겠는데",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043640.1.12",
          "form": "디저트입니다ㅋㅋ",
          "original_form": "디저트입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043640.1.13",
          "form": "엇 그럼",
          "original_form": "엇 그럼",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043640.1.14",
          "form": "소시지가 들어가나요?",
          "original_form": "소시지가 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043640.1.15",
          "form": "아니요!!",
          "original_form": "아니요!!",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043640.1.16",
          "form": "전혀 들어가지않슴당",
          "original_form": "전혀 들어가지않슴당",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043640.1.17",
          "form": "달콤한 맛이 나는 음식인가요?",
          "original_form": "달콤한 맛이 나는 음식인가요?",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043640.1.18",
          "form": "네네! 완전 달달입니다",
          "original_form": "네네! 완전 달달입니다",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043640.1.19",
          "form": "헉 그럼 바삭바삭란 음식인가요",
          "original_form": "헉 그럼 바삭바삭란 음식인가요",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043640.1.20",
          "form": "바삭바삭보단",
          "original_form": "바삭바삭보단",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043640.1.21",
          "form": "쫀득쫀득에 가깝습니다!!",
          "original_form": "쫀득쫀득에 가깝습니다!!",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043640.1.22",
          "form": "바삭보단 쫀득 쫀득에 가까워요!",
          "original_form": "바삭보단 쫀득 쫀득에 가까워요!",
