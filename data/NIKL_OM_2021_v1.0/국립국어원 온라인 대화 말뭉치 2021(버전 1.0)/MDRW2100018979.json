      "utterance": [
        {
          "id": "MDRW2100018979.1.1",
          "form": "넷플릭스 자주보시나요??",
          "original_form": "넷플릭스 자주보시나요??",
          "speaker_id": "1",
          "time": "20210827 14:15"
        },
        {
          "id": "MDRW2100018979.1.2",
          "form": "저는 넷플릭스는 안봐요ㅠㅠ",
          "original_form": "저는 넷플릭스는 안봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018979.1.3",
          "form": "보통 유투브봐요!",
          "original_form": "보통 유투브봐요!",
          "speaker_id": "2",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018979.1.4",
          "form": "유투브 어떤거 많이보세요??",
          "original_form": "유투브 어떤거 많이보세요??",
          "speaker_id": "1",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100018979.1.5",
          "form": "저는 예능이나 드라마 클립 많이 봐요",
          "original_form": "저는 예능이나 드라마 클립 많이 봐요",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100018979.1.6",
          "form": "먹방은 별로 안좋아하는 편이구요",
          "original_form": "먹방은 별로 안좋아하는 편이구요",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100018979.1.7",
          "form": "name1님은 어떤거 많이 보세요?",
          "original_form": "&name1&님은 어떤거 많이 보세요?",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100018979.1.8",
          "form": "저는 요즘 기타를 취미로 하고 있어서",
          "original_form": "저는 요즘 기타를 취미로 하고 있어서",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100018979.1.9",
          "form": "기타치는거 많이 보고있고",
          "original_form": "기타치는거 많이 보고있고",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100018979.1.10",
          "form": "강아지유투브들도 많이봐요",
          "original_form": "강아지유투브들도 많이봐요",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100018979.1.11",
          "form": "오 기타",
          "original_form": "오 기타",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100018979.1.12",
          "form": "기타배우시는구나",
          "original_form": "기타배우시는구나",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100018979.1.13",
          "form": "아주 아주초보에요 ㅋㅋㅋ",
          "original_form": "아주 아주초보에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100018979.1.14",
          "form": "요즘은 유투브에서 악기 가르쳐주는 컨텐츠도 많은 것 같아요",
          "original_form": "요즘은 유투브에서 악기 가르쳐주는 컨텐츠도 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100018979.1.15",
          "form": "기타 어떤거치세요?",
          "original_form": "기타 어떤거치세요?",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
