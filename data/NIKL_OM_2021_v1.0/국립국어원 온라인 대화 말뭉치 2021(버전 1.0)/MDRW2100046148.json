          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046148.1.10",
          "form": "맛잘알ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맛잘알ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046148.1.11",
          "form": "2 + 1이라서",
          "original_form": "2 + 1이라서",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046148.1.12",
          "form": "딸기맛도 샀어요",
          "original_form": "딸기맛도 샀어요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
