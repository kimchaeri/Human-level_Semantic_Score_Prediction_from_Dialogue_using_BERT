        },
        {
          "id": "MDRW2100046148.1.2",
          "form": "우유 사 먹었어요",
          "original_form": "우유 사 먹었어요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046148.1.3",
          "form": "밥을 먹어야죠!",
          "original_form": "밥을 먹어야죠!",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046148.1.4",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046148.1.5",
          "form": "덴마크 요구르트 아세요?",
          "original_form": "덴마크 요구르트 아세요?",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046148.1.6",
          "form": "알죠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "알죠ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046148.1.7",
          "form": "사과맛 진리",
          "original_form": "사과맛 진리",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046148.1.8",
          "form": "사과 맛 샀어요",
          "original_form": "사과 맛 샀어요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
