          "id": "MDRW2100026078.1.1",
          "form": "중고거래해요ㅌ?",
          "original_form": "중고거래해요ㅌ?",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100026078.1.2",
          "form": "오늘도 당근 하나 팔고왓어요",
          "original_form": "오늘도 당근 하나 팔고왓어요",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026078.1.3",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026078.1.4",
          "form": "홍삼음료요 ㅋㅋㅋ",
          "original_form": "홍삼음료요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026078.1.5",
          "form": "아 ㅎㅎ",
          "original_form": "아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026078.1.6",
          "form": "전 팔기만요",
