      "utterance": [
        {
          "id": "MDRW2100023218.1.1",
          "form": "스텔라비스같은게",
          "original_form": "스텔라비스같은게",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023218.1.2",
          "form": "초반에라도 흥하는 꼴을",
          "original_form": "초반에라도 흥하는 꼴을",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023218.1.3",
          "form": "보이면 안되는건데",
          "original_form": "보이면 안되는건데",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023218.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023218.1.5",
          "form": "아니 그리고",
          "original_form": "아니 그리고",
          "speaker_id": "1",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023218.1.6",
          "form": "데이세븐이나 루시드림이나",
          "original_form": "데이세븐이나 루시드림이나",
          "speaker_id": "1",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023218.1.7",
          "form": "소소하게 명맥 이어오는 겜들을 조금이라도 해봤으면",
          "original_form": "소소하게 명맥 이어오는 겜들을 조금이라도 해봤으면",
          "speaker_id": "1",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023218.1.8",
          "form": "ㅇㅇㅎ",
          "original_form": "ㅇㅇㅎ",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023218.1.9",
          "form": "이렇게 낼 생각을 안했을텐데...",
          "original_form": "이렇게 낼 생각을 안했을텐데...",
          "speaker_id": "1",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023218.1.10",
          "form": "ㄹㅇ걍 비주얼 노벨인데",
          "original_form": "ㄹㅇ걍 비주얼 노벨인데",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023218.1.11",
          "form": "자기들 게임 정체성이 뭔지 잘 모르는듯",
          "original_form": "자기들 게임 정체성이 뭔지 잘 모르는듯",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023218.1.12",
          "form": "오픈도 전에 텀블벅 투자로 1억이나 받았으면",
          "original_form": "오픈도 전에 텀블벅 투자로 1억이나 받았으면",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023218.1.13",
          "form": "앙스타나 애프터라이프 흉내내기엔",
          "original_form": "앙스타나 애프터라이프 흉내내기엔",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023218.1.14",
          "form": "여성향이 인기가 없다 이런소리도",
          "original_form": "여성향이 인기가 없다 이런소리도",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023218.1.15",
          "form": "못꺼내",
          "original_form": "못꺼내",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023218.1.16",
          "form": "구조가 가챠도 아니고 이것저것 할 거 많은 모바일겜 타입 자체가 아니잖아",
          "original_form": "구조가 가챠도 아니고 이것저것 할 거 많은 모바일겜 타입 자체가 아니잖아",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023218.1.17",
          "form": "그정도 해줬으면 어느정도 퀄의 결과를 가져왔어야지",
          "original_form": "그정도 해줬으면 어느정도 퀄의 결과를 가져왔어야지",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023218.1.18",
          "form": "그럼 데이세븐이나 수메나 구운몽처럼 오픈때 완결을 내서 들고와야하는데",
          "original_form": "그럼 데이세븐이나 수메나 구운몽처럼 오픈때 완결을 내서 들고와야하는데",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023218.1.19",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.20",
          "form": "이도저도 아니게 단점만 뽑아옴",
          "original_form": "이도저도 아니게 단점만 뽑아옴",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.21",
          "form": "ㄹㅇ걍",
          "original_form": "ㄹㅇ걍",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.22",
          "form": "여성향정도가 아니라",
          "original_form": "여성향정도가 아니라",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.23",
          "form": "게임업계에 대한",
          "original_form": "게임업계에 대한",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.24",
          "form": "이해도가 있는건지",
          "original_form": "이해도가 있는건지",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.25",
          "form": "의문이야",
          "original_form": "의문이야",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.26",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.27",
          "form": "저정도면 내가 보기엔",
          "original_form": "저정도면 내가 보기엔",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.28",
          "form": "오토게 아니더라도",
          "original_form": "오토게 아니더라도",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.29",
          "form": "게임 한두개만 해봤으면",
          "original_form": "게임 한두개만 해봤으면",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.30",
          "form": "중간에 팀 와해됐다가",
          "original_form": "중간에 팀 와해됐다가",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.31",
          "form": "저렇게는 안냈을텐데",
          "original_form": "저렇게는 안냈을텐데",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.32",
          "form": "텀블벅 먹튀논란 날까봐",
          "original_form": "텀블벅 먹튀논란 날까봐",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023218.1.33",
          "form": "ㅇㅇㅎ",
          "original_form": "ㅇㅇㅎ",
          "speaker_id": "2",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100023218.1.34",
          "form": "급하게 수습해서 만든것까지만 낸 느낌임",
          "original_form": "급하게 수습해서 만든것까지만 낸 느낌임",
          "speaker_id": "1",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100023218.1.35",
          "form": "그림쟁이랑",
          "original_form": "그림쟁이랑",
          "speaker_id": "2",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100023218.1.36",
