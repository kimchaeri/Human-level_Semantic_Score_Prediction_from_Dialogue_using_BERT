      "utterance": [
        {
          "id": "MDRW2100024823.1.1",
          "form": "저녁먹게?",
          "original_form": "저녁먹게?",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024823.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024823.1.3",
          "form": "저녁 안먹을거야",
          "original_form": "저녁 안먹을거야",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024823.1.4",
          "form": "점심을 먹었기 때문이지",
          "original_form": "점심을 먹었기 때문이지",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024823.1.5",
          "form": "점심을 언제먹은거야 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "점심을 언제먹은거야 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024823.1.6",
          "form": "2시반?",
          "original_form": "2시반?",
          "speaker_id": "2",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024823.1.7",
          "form": "근데 요새 밥 잘 안먹어서",
          "original_form": "근데 요새 밥 잘 안먹어서",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024823.1.8",
          "form": "한끼면 충분해.....",
          "original_form": "한끼면 충분해.....",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024823.1.9",
          "form": "두끼는 먹어...",
          "original_form": "두끼는 먹어...",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024823.1.10",
          "form": "배 안고파??",
          "original_form": "배 안고파??",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024823.1.11",
          "form": "ㅇㅇ그닥",
          "original_form": "ㅇㅇ그닥",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024823.1.12",
          "form": "그래서 안먹게 되더라구",
          "original_form": "그래서 안먹게 되더라구",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024823.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024823.1.14",
          "form": "난 회사에서 맨날 점심 왕창 먹얻ㅎ",
          "original_form": "난 회사에서 맨날 점심 왕창 먹얻ㅎ",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024823.1.15",
          "form": "어도",
          "original_form": "어도",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024823.1.16",
          "form": "퇴근할 때 되면 배고파...",
          "original_form": "퇴근할 때 되면 배고파...",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024823.1.17",
          "form": "아 나도",
          "original_form": "아 나도",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024823.1.18",
          "form": "일하면 그러더라ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "일하면 그러더라ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024823.1.19",
          "form": "일하면서 저녁까지 먹고",
          "original_form": "일하면서 저녁까지 먹고",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024823.1.20",
          "form": "확실히 회사다니면",
          "original_form": "확실히 회사다니면",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024823.1.21",
          "form": "뭔가 기 빨리는게 있나봄",
          "original_form": "뭔가 기 빨리는게 있나봄",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024823.1.22",
          "form": "편의점에서 간식 또사먹고",
          "original_form": "편의점에서 간식 또사먹고",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024823.1.23",
          "form": "그랬어",
          "original_form": "그랬어",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024823.1.24",
          "form": "난 과자는 안땡기는데",
          "original_form": "난 과자는 안땡기는데",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024823.1.25",
          "form": "식사류가 엄청 땡김",
          "original_form": "식사류가 엄청 땡김",
          "speaker_id": "1",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100024823.1.26",
          "form": "탕비실에서도 과자 안먹고 물만 마시는데",
          "original_form": "탕비실에서도 과자 안먹고 물만 마시는데",
          "speaker_id": "1",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100024823.1.27",
          "form": "컵라면이 탐나.....",
          "original_form": "컵라면이 탐나.....",
          "speaker_id": "1",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100024823.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅂㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅂㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100024823.1.29",
          "form": "난 걍",
          "original_form": "난 걍",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100024823.1.30",
          "form": "모든것의 섭취량이",
          "original_form": "모든것의 섭취량이",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100024823.1.31",
          "form": "세배정도 늘어났었어",
          "original_form": "세배정도 늘어났었어",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100024823.1.32",
          "form": "커피 두잔씩 마시고 초콜렛 가방에 쟁이고",
          "original_form": "커피 두잔씩 마시고 초콜렛 가방에 쟁이고",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100024823.1.33",
          "form": "밥 두끼 먹고 편의점에서 컵라면 삼김 그 외 간단한 간식 또먹고",
          "original_form": "밥 두끼 먹고 편의점에서 컵라면 삼김 그 외 간단한 간식 또먹고",
          "speaker_id": "2",
          "time": "20210831 19:39"
        },
        {
          "id": "MDRW2100024823.1.34",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100024823.1.35",
          "form": "그렇게 먹으면 몸에 무리 안갔냐궄",
          "original_form": "그렇게 먹으면 몸에 무리 안갔냐궄",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100024823.1.36",
          "form": "8개월짜리라 다행이었지.......",
          "original_form": "8개월짜리라 다행이었지.......",
          "speaker_id": "2",
          "time": "20210831 19:50"
        }
      ]
