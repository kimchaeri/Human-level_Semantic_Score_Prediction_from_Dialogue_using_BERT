        },
        {
          "id": "MDRW2100038289.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 08:40"
        },
        {
          "id": "MDRW2100038289.1.3",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
          "speaker_id": "1",
          "time": "20210902 08:41"
        },
        {
          "id": "MDRW2100038289.1.4",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038289.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038289.1.6",
          "form": "저도 있죠ㅎㅎ 서로 얘기할까요",
          "original_form": "저도 있죠ㅎㅎ 서로 얘기할까요",
          "speaker_id": "1",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038289.1.7",
          "form": "네! 말씀해주세요 ㅋㅋ",
          "original_form": "네! 말씀해주세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100038289.1.8",
          "form": "저는 방탄이랑 투바투요",
          "original_form": "저는 방탄이랑 투바투요",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100038289.1.9",
          "form": "아하 방탄 인기 많더라구요 ㅋㅋ 전 옛날사람... 하이라이트 좋아해요",
          "original_form": "아하 방탄 인기 많더라구요 ㅋㅋ 전 옛날사람... 하이라이트 좋아해요",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038289.1.10",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038289.1.11",
          "form": "알죠ㅋㅋ",
          "original_form": "알죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038289.1.12",
          "form": "비스트 때 노래 엄청 좋아했어요",
          "original_form": "비스트 때 노래 엄청 좋아했어요",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038289.1.13",
          "form": "하이라이트 되고 나서는 챙겨듣질 못했네요ㅠ",
          "original_form": "하이라이트 되고 나서는 챙겨듣질 못했네요ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038289.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 요즘 인기 없다구 맨날 광광대는데 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 요즘 인기 없다구 맨날 광광대는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038289.1.15",
          "form": "전 오히려 하이라이트 되고 나서 더 열심히 하는 것 같아요 ㅋㅋ",
          "original_form": "전 오히려 하이라이트 되고 나서 더 열심히 하는 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038289.1.16",
          "form": "그런가요ㅎㅎ 오랜만에 찾아봐야겠어요",
          "original_form": "그런가요ㅎㅎ 오랜만에 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100038289.1.17",
          "form": "ㅋㅋㅋㅋㅋ 네 군백기 동안 열심히 기다렸는데 코로나 때문에 공연 이나 오프더 다 취소괴고 그래서 아쉬워요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ 네 군백기 동안 열심히 기다렸는데 코로나 때문에 공연 이나 오프더 다 취소괴고 그래서 아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100038289.1.18",
          "form": "아이고 코로나가 잘못했네요",
          "original_form": "아이고 코로나가 잘못했네요",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100038289.1.19",
          "form": "ㅠ 요즘 컴백하는 연예인들은 다 그러니까요...",
          "original_form": "ㅠ 요즘 컴백하는 연예인들은 다 그러니까요...",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100038289.1.20",
          "form": "맞아요... 방청객도 없구요",
          "original_form": "맞아요... 방청객도 없구요",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100038289.1.21",
          "form": "방탄도 그렇죠?? 게다가 미국가있느라고 국내활동 많이 못 했을거같아요",
          "original_form": "방탄도 그렇죠?? 게다가 미국가있느라고 국내활동 많이 못 했을거같아요",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100038289.1.22",
          "form": "이젠 미국도 잘 안가요 해외이다보니...",
          "original_form": "이젠 미국도 잘 안가요 해외이다보니...",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100038289.1.23",
          "form": "아 유엔 연설 때문에 조만간 나가긴 해요",
          "original_form": "아 유엔 연설 때문에 조만간 나가긴 해요",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100038289.1.24",
          "form": "헉 유엔 연설도 해요? 대박이다 ㅋㅋㅋㅋ",
          "original_form": "헉 유엔 연설도 해요? 대박이다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100038289.1.25",
          "form": "몇년전에도 한번 했었어요",
          "original_form": "몇년전에도 한번 했었어요",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100038289.1.26",
          "form": "몰랐어요 ㅋㅋㅋㅋ",
          "original_form": "몰랐어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100038289.1.27",
          "form": "그때 너무 늦게 해서 밤샜어요",
          "original_form": "그때 너무 늦게 해서 밤샜어요",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100038289.1.28",
          "form": "덕분에 영어 리스닝이 안돼서 들은둥만둥...",
          "original_form": "덕분에 영어 리스닝이 안돼서 들은둥만둥...",
          "speaker_id": "1",
          "time": "20210902 18:47"
        }
      ]
