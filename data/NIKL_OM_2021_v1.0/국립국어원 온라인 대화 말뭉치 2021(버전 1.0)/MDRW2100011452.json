      "utterance": [
        {
          "id": "MDRW2100011452.1.1",
          "form": "최근에 사신것있나요?",
          "original_form": "최근에 사신것있나요?",
          "speaker_id": "1",
          "time": "20210824 12:47"
        },
        {
          "id": "MDRW2100011452.1.2",
          "form": "변기 청소 용품이요ㅎㅎ",
          "original_form": "변기 청소 용품이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100011452.1.3",
          "form": "아ㅎㅎㅎ",
          "original_form": "아ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100011452.1.4",
          "form": "청소하시려구요?",
          "original_form": "청소하시려구요?",
          "speaker_id": "1",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100011452.1.5",
          "form": "네 ㅎㅎ 다 써서!",
          "original_form": "네 ㅎㅎ 다 써서!",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011452.1.6",
          "form": "뭐 사셨어요~~",
          "original_form": "뭐 사셨어요~~",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011452.1.7",
          "form": "전 운동화요!",
          "original_form": "전 운동화요!",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100011452.1.8",
          "form": "이뻐요?",
          "original_form": "이뻐요?",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100011452.1.9",
          "form": "네네 편해요!",
          "original_form": "네네 편해요!",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100011452.1.10",
          "form": "그게 최고죠 ㅎㅎ",
          "original_form": "그게 최고죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100011452.1.11",
          "form": "사고싶으신거",
          "original_form": "사고싶으신거",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100011452.1.12",
          "form": "있나요?",
          "original_form": "있나요?",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100011452.1.13",
          "form": "집이요ㅋㅋㅋㅋㅋ",
          "original_form": "집이요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100011452.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:59"
        },
        {
          "id": "MDRW2100011452.1.15",
          "form": "그건못사잖아요ㅠ",
          "original_form": "그건못사잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 00:59"
        },
        {
          "id": "MDRW2100011452.1.16",
          "form": "ㅋㅋ노력해야죠!",
          "original_form": "ㅋㅋ노력해야죠!",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100011452.1.17",
          "form": "일단은 특가 뜬거 사야쥬",
          "original_form": "일단은 특가 뜬거 사야쥬",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100011452.1.18",
          "form": "특가가",
          "original_form": "특가가",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100011452.1.19",
          "form": "뜨나요?",
          "original_form": "뜨나요?",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100011452.1.20",
          "form": "아아 아뇨 11번가 ㅋㅋ",
          "original_form": "아아 아뇨 11번가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100011452.1.21",
          "form": "집을요????",
          "original_form": "집을요????",
          "speaker_id": "1",
          "time": "20210825 01:40"
        },
        {
          "id": "MDRW2100011452.1.22",
          "form": "아뇨 집사고 싶은데ㅋㅋ",
          "original_form": "아뇨 집사고 싶은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100011452.1.23",
          "form": "일단 11번가에서 생필품이나",
          "original_form": "일단 11번가에서 생필품이나",
          "speaker_id": "2",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100011452.1.24",
          "form": "사야지였어요",
          "original_form": "사야지였어요",
          "speaker_id": "2",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100011452.1.25",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100011452.1.26",
          "form": "놀랐잖아요!",
          "original_form": "놀랐잖아요!",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100011452.1.27",
          "form": "제가 너무 함축적으로 말했네요ㅋㅋㅋ",
          "original_form": "제가 너무 함축적으로 말했네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:51"
        },
        {
          "id": "MDRW2100011452.1.28",
