      "utterance": [
        {
          "id": "MDRW2100034505.1.1",
          "form": "다들 소식하시나봐요ㅜㅋㅋ",
          "original_form": "다들 소식하시나봐요ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100034505.1.2",
          "form": "아니요 ㅋㅋㅋ 저희가족이 특히 심한거같아요,,",
          "original_form": "아니요 ㅋㅋㅋ 저희가족이 특히 심한거같아요,,",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100034505.1.3",
          "form": "제가 그나마 저희가족중에 젤 많이먹어여 ㅋㅋㅋ",
          "original_form": "제가 그나마 저희가족중에 젤 많이먹어여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100034505.1.4",
          "form": "ㅋㅋ 치킨시켜서 그럼 두조각 드시는건가요? 너무 아쉬워요!!!",
          "original_form": "ㅋㅋ 치킨시켜서 그럼 두조각 드시는건가요? 너무 아쉬워요!!!",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034505.1.5",
          "form": "조각이 많아서 두개는 절대아니에요 ㅋㅋㅋㅋ",
          "original_form": "조각이 많아서 두개는 절대아니에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034505.1.6",
          "form": "근데 신기하게 메뉴마다 먹는양이 달라지더라구요",
          "original_form": "근데 신기하게 메뉴마다 먹는양이 달라지더라구요",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034505.1.7",
          "form": "ㅋㅋ그리고 컨디션도 중요해요 닭도 나도",
          "original_form": "ㅋㅋ그리고 컨디션도 중요해요 닭도 나도",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034505.1.8",
          "form": "잘튀겨진 날이 있어요",
          "original_form": "잘튀겨진 날이 있어요",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034505.1.9",
          "form": "마쟈요 그리고 치킨 의외로 점바점 심해요 ㅠ",
          "original_form": "마쟈요 그리고 치킨 의외로 점바점 심해요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034505.1.10",
          "form": "맞아요! 체인점들인데도 사람손을 타더라고요",
          "original_form": "맞아요! 체인점들인데도 사람손을 타더라고요",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034505.1.11",
          "form": "역시 손맛이 중요한가봐옄ㅋㅋㄴㄴ",
          "original_form": "역시 손맛이 중요한가봐옄ㅋㅋㄴㄴ",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034505.1.12",
          "form": "보통 배달 많이하시죠?",
          "original_form": "보통 배달 많이하시죠?",
          "speaker_id": "1",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034505.1.13",
          "form": "그쵸~ 포장해와서 많이먹곤해요",
          "original_form": "그쵸~ 포장해와서 많이먹곤해요",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100034505.1.14",
          "form": "저도 포장이 좋아요 배달비 너무 부담돼요!!!!",
          "original_form": "저도 포장이 좋아요 배달비 너무 부담돼요!!!!",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100034505.1.15",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034505.1.16",
          "form": "방문포장하면 배달비도아끼고 운동도되고 좋아요",
          "original_form": "방문포장하면 배달비도아끼고 운동도되고 좋아요",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034505.1.17",
          "form": "근데 배민 맨날 배달만 쿠폰줘서 속상해요",
          "original_form": "근데 배민 맨날 배달만 쿠폰줘서 속상해요",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034505.1.18",
          "form": "앜ㅋㅋㅋㅋ마쟈요 ㅠㅠ 배달시키고 쿠폰써도 포장이더싼게함정이에요",
          "original_form": "앜ㅋㅋㅋㅋ마쟈요 ㅠㅠ 배달시키고 쿠폰써도 포장이더싼게함정이에요",
          "speaker_id": "2",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100034505.1.19",
          "form": "그냥 배민안쓰고 동네 치킨집가는게 가장싸긴해요ㄱㅋ",
          "original_form": "그냥 배민안쓰고 동네 치킨집가는게 가장싸긴해요ㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034505.1.20",
          "form": "앜ㅋㅋㅋㅋㅋ 그게정답이네요",
          "original_form": "앜ㅋㅋㅋㅋㅋ 그게정답이네요",
