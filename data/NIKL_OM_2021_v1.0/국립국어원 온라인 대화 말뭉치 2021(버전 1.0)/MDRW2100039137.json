      "utterance": [
        {
          "id": "MDRW2100039137.1.1",
          "form": "건조기가 진짜 삶의질이달라지더라고요",
          "original_form": "건조기가 진짜 삶의질이달라지더라고요",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100039137.1.2",
          "form": "오 건조기 쓰세요?",
          "original_form": "오 건조기 쓰세요?",
          "speaker_id": "2",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100039137.1.3",
          "form": "네네 너무좋아요",
          "original_form": "네네 너무좋아요",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039137.1.4",
          "form": "수건도 보송",
          "original_form": "수건도 보송",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039137.1.5",
          "form": "빨리마리구",
          "original_form": "빨리마리구",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039137.1.6",
          "form": "이렇게 비가 자주 오면 너무 좋을거같아여",
          "original_form": "이렇게 비가 자주 오면 너무 좋을거같아여",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039137.1.7",
          "form": "요즘 빨래가 잘 안마르더라고요",
          "original_form": "요즘 빨래가 잘 안마르더라고요",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039137.1.8",
          "form": "그쵸 습해서",
          "original_form": "그쵸 습해서",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039137.1.9",
          "form": "건조기 강추",
          "original_form": "건조기 강추",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039137.1.10",
          "form": "제습기 혹시 있으세요?",
          "original_form": "제습기 혹시 있으세요?",
          "speaker_id": "2",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039137.1.11",
          "form": "제습기는 없어용",
          "original_form": "제습기는 없어용",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039137.1.12",
          "form": "있나요?",
          "original_form": "있나요?",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039137.1.13",
          "form": "아뇨 ㅎ 혹시 있으시면 어떤지 물어볼까했어여ㅋㅋㅋ",
          "original_form": "아뇨 ㅎ 혹시 있으시면 어떤지 물어볼까했어여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039137.1.14",
          "form": "고민했었거든요",
          "original_form": "고민했었거든요",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039137.1.15",
          "form": "살까말까",
          "original_form": "살까말까",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039137.1.16",
          "form": "아 친구들 다 쓰더라고요",
          "original_form": "아 친구들 다 쓰더라고요",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039137.1.17",
          "form": "물이 은근 많이 찬대요",
          "original_form": "물이 은근 많이 찬대요",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039137.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039137.1.19",
          "form": "저번에 살까해서",
          "original_form": "저번에 살까해서",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039137.1.20",
          "form": "알아봤는데 생각보다 가격이 있더라고요ㅎ",
          "original_form": "알아봤는데 생각보다 가격이 있더라고요ㅎ",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039137.1.21",
          "form": "그렇구나ㅠㅠ",
          "original_form": "그렇구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100039137.1.22",
          "form": "제습기는 내년에 사셔요",
          "original_form": "제습기는 내년에 사셔요",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100039137.1.23",
          "form": "그래야겠어요 ㅎ 여름전에 사야 세일도 하고 그런다더라고용",
          "original_form": "그래야겠어요 ㅎ 여름전에 사야 세일도 하고 그런다더라고용",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100039137.1.24",
          "form": "여름에 사면 비싼듯요",
          "original_form": "여름에 사면 비싼듯요",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100039137.1.25",
          "form": "맞아용 미리 겨울에주문하셔야겠다",
          "original_form": "맞아용 미리 겨울에주문하셔야겠다",
          "speaker_id": "1",
          "time": "20210903 23:46"
        }
      ]
    }
