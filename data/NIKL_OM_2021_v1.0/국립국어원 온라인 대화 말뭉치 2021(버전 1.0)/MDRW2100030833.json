      "utterance": [
        {
          "id": "MDRW2100030833.1.1",
          "form": "디저트류 좋아하시나요?",
          "original_form": "디저트류 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030833.1.2",
          "form": "네!! 저 마카롱도 되게 좋아해요!",
          "original_form": "네!! 저 마카롱도 되게 좋아해요!",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030833.1.3",
          "form": "치즈맛!",
          "original_form": "치즈맛!",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030833.1.4",
          "form": "와 저도 치즈 짱 좋아해요!",
          "original_form": "와 저도 치즈 짱 좋아해요!",
          "speaker_id": "1",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030833.1.5",
          "form": "뚱카롱을 좋아하시나요?",
          "original_form": "뚱카롱을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100030833.1.6",
          "form": "네네!! 차갑게 먹으면 맛있어서ㅜ",
          "original_form": "네네!! 차갑게 먹으면 맛있어서ㅜ",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030833.1.7",
          "form": "마카롱 파는 집이 많아져서 좋아요",
          "original_form": "마카롱 파는 집이 많아져서 좋아요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030833.1.8",
          "form": "저두요! 그리고 저렴하게 파는 마리웨일도 많아져서 좋구요ㅠ",
          "original_form": "저두요! 그리고 저렴하게 파는 마리웨일도 많아져서 좋구요ㅠ",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030833.1.9",
          "form": "상업적인 맛이지만..",
          "original_form": "상업적인 맛이지만..",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030833.1.10",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030833.1.11",
          "form": "무인 편의점에서도 판매하는거 한번씩 급하게 당 충전하고 싶으면 먹기도 하는데",
          "original_form": "무인 편의점에서도 판매하는거 한번씩 급하게 당 충전하고 싶으면 먹기도 하는데",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030833.1.12",
          "form": "이것도 상업의맛...",
          "original_form": "이것도 상업의맛...",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030833.1.13",
          "form": "아 그거 맛있죠 ㅋㅋㅋㅋ 생각보다 퀄리티가 나쁘지 않아요 그거",
          "original_form": "아 그거 맛있죠 ㅋㅋㅋㅋ 생각보다 퀄리티가 나쁘지 않아요 그거",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030833.1.14",
          "form": "은근 나쁘지 않아서 가끔 한번씩 먹거든요 ㅋㅋㅋ",
          "original_form": "은근 나쁘지 않아서 가끔 한번씩 먹거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030833.1.15",
          "form": "겸사겸사 옆에있는 아이스크림도 사구요 ㅋㅋ",
          "original_form": "겸사겸사 옆에있는 아이스크림도 사구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030833.1.16",
          "form": "편의점 아이스크림 너무 비싸지 않나요?",
          "original_form": "편의점 아이스크림 너무 비싸지 않나요?",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030833.1.17",
          "form": "저는 빵빠레 커스타드맛 궁금해서 샀다가",
          "original_form": "저는 빵빠레 커스타드맛 궁금해서 샀다가",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030833.1.18",
          "form": "1800원이라는 악마의 가격에 놀랐어요",
          "original_form": "1800원이라는 악마의 가격에 놀랐어요",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030833.1.19",
          "form": "역시 사악하군요",
          "original_form": "역시 사악하군요",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030833.1.20",
          "form": "맛은 어땠어요??",
          "original_form": "맛은 어땠어요??",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030833.1.21",
          "form": "그냥 평범한 맛이었어요 우왓 엄청 맛있어!! 까진 아닌거같은...",
          "original_form": "그냥 평범한 맛이었어요 우왓 엄청 맛있어!! 까진 아닌거같은...",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030833.1.22",
          "form": "엑셀런트?",
          "original_form": "엑셀런트?",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030833.1.23",
          "form": "바닐라맛 나는 네모난 아이스크림이 있는데",
          "original_form": "바닐라맛 나는 네모난 아이스크림이 있는데",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030833.1.24",
          "form": "그거 빵빠레처럼 콘으로 나와서 먹어봤는데",
          "original_form": "그거 빵빠레처럼 콘으로 나와서 먹어봤는데",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030833.1.25",
          "form": "가격이 사악하지만 맛은 괜찮았어요!",
          "original_form": "가격이 사악하지만 맛은 괜찮았어요!",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030833.1.26",
          "form": "아!! 그거 알아요!!",
          "original_form": "아!! 그거 알아요!!",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030833.1.27",
          "form": "저는 엑셀런트 콘으로 나온건 못봤는데",
          "original_form": "저는 엑셀런트 콘으로 나온건 못봤는데",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030833.1.28",
          "form": "2500원 정도 아니던가요?",
          "original_form": "2500원 정도 아니던가요?",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030833.1.29",
          "form": "각각 바닐라맛 다른게 붙어있어서 맛있다고 들었어요",
          "original_form": "각각 바닐라맛 다른게 붙어있어서 맛있다고 들었어요",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030833.1.30",
          "form": "맞아요! 바닐라 두게 맛이 다른게 붙어있어요!",
          "original_form": "맞아요! 바닐라 두게 맛이 다른게 붙어있어요!",
          "speaker_id": "1",
          "time": "20210904 14:45"
        },
        {
          "id": "MDRW2100030833.1.31",
          "form": "가격 진짜 나쁘죠 ㅋㅋ",
          "original_form": "가격 진짜 나쁘죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:46"
        },
        {
          "id": "MDRW2100030833.1.32",
          "form": "이건 아이스크림 할인점에서도 할인이 안되고 그가격이더라구요",
          "original_form": "이건 아이스크림 할인점에서도 할인이 안되고 그가격이더라구요",
          "speaker_id": "2",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100030833.1.33",
          "form": "맛있긴 한데 악마예요.. 그러면 배라를 갈텐데",
          "original_form": "맛있긴 한데 악마예요.. 그러면 배라를 갈텐데",
          "speaker_id": "2",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100030833.1.34",
          "form": "드셔보셨군요!",
          "original_form": "드셔보셨군요!",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030833.1.35",
          "form": "맞아요... 그럴거면 그냥 베라 가는게 나을 수도 있어요...",
          "original_form": "맞아요... 그럴거면 그냥 베라 가는게 나을 수도 있어요...",
          "speaker_id": "1",
          "time": "20210904 15:07"
        }
      ]
    }
