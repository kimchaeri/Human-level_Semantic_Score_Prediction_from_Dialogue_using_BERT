      "utterance": [
        {
          "id": "MDRW2100023256.1.1",
          "form": "보통 언제자요?",
          "original_form": "보통 언제자요?",
          "speaker_id": "2",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023256.1.2",
          "form": "저 열두시쯤자요 ㅋㅋㅋㅋ",
          "original_form": "저 열두시쯤자요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023256.1.3",
          "form": "늦게자네요?ㅋㅋ",
          "original_form": "늦게자네요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100023256.1.4",
          "form": "아 늦나요? ㅋㅋㅋㅋ 언제 주무시는데요?",
          "original_form": "아 늦나요? ㅋㅋㅋㅋ 언제 주무시는데요?",
          "speaker_id": "1",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100023256.1.5",
          "form": "저도 12시쯤이요 ㅋㅋㅋ",
          "original_form": "저도 12시쯤이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100023256.1.6",
          "form": "자기전에 늘 통화하세요?",
          "original_form": "자기전에 늘 통화하세요?",
          "speaker_id": "1",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100023256.1.7",
          "form": "넵 여친잘때까지 해줘요",
          "original_form": "넵 여친잘때까지 해줘요",
          "speaker_id": "2",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100023256.1.8",
          "form": "여친이 좀 일찍자서",
          "original_form": "여친이 좀 일찍자서",
          "speaker_id": "2",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100023256.1.9",
          "form": "오호 그러시구나",
          "original_form": "오호 그러시구나",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100023256.1.10",
          "form": "저도 남친이 한 10시 반쯤 자러가거든요",
          "original_form": "저도 남친이 한 10시 반쯤 자러가거든요",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100023256.1.11",
          "form": "티비보다 잔다 그래서",
          "original_form": "티비보다 잔다 그래서",
          "speaker_id": "1",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100023256.1.12",
          "form": "일찍자네요",
          "original_form": "일찍자네요",
          "speaker_id": "2",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100023256.1.13",
          "form": "그랴서 잘 통화안하게 도ㅐ요 ㅋㅋㅋ",
          "original_form": "그랴서 잘 통화안하게 도ㅐ요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100023256.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 애매하긴하네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 애매하긴하네요",
          "speaker_id": "2",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100023256.1.15",
          "form": "그럼 퇴근하고 저녁먹고 잠깐 하면 되지 않아요?",
          "original_form": "그럼 퇴근하고 저녁먹고 잠깐 하면 되지 않아요?",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023256.1.16",
          "form": "저녁을 계속 친구랑 먹거나 직장동료랑 먹고 약속이 언제 끝나나 몰라서 ㅋㅋㅋㅋ",
          "original_form": "저녁을 계속 친구랑 먹거나 직장동료랑 먹고 약속이 언제 끝나나 몰라서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023256.1.17",
          "form": "물어봐요 ㅋㅋㅋ",
          "original_form": "물어봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023256.1.18",
          "form": "끝났냐고",
          "original_form": "끝났냐고",
          "speaker_id": "2",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100023256.1.19",
          "form": "ㅋㅋㅋㅋㅋ아이 그렇게 하는데 뭔가 타이밍이 안맞아요...그랴서 통화 언제햇더라 가물가물",
          "original_form": "ㅋㅋㅋㅋㅋ아이 그렇게 하는데 뭔가 타이밍이 안맞아요...그랴서 통화 언제햇더라 가물가물",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023256.1.20",
          "form": "10시반에 자니까 9시반엔 집에오지 않을까요?",
          "original_form": "10시반에 자니까 9시반엔 집에오지 않을까요?",
          "speaker_id": "2",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023256.1.21",
          "form": "아니다 요즘 식당 9시까지만 하지 않아요?",
          "original_form": "아니다 요즘 식당 9시까지만 하지 않아요?",
          "speaker_id": "2",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023256.1.22",
          "form": "부산은 다른가?",
          "original_form": "부산은 다른가?",
          "speaker_id": "2",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023256.1.23",
          "form": "9시까지 마자여",
          "original_form": "9시까지 마자여",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023256.1.24",
          "form": "근데 드라이브를 가고 그런",
          "original_form": "근데 드라이브를 가고 그런",
          "speaker_id": "1",
          "time": "20210829 23:49"
        }
      ]
    }
