      "utterance": [
        {
          "id": "MDRW2100004301.1.1",
          "form": "저녁 먹고 돌리면 될듯",
          "original_form": "저녁 먹고 돌리면 될듯",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004301.1.2",
          "form": "레드북 못봐서 아쉽네",
          "original_form": "레드북 못봐서 아쉽네",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004301.1.3",
          "form": "코로나 때문에 관극하기가 너무 어렵다~",
          "original_form": "코로나 때문에 관극하기가 너무 어렵다~",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004301.1.4",
          "form": "응 아쉽아쉽",
          "original_form": "응 아쉽아쉽",
          "speaker_id": "1",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004301.1.5",
          "form": "그치만 코로나가 심한데 극장 가기도 좀 꺼려지니까",
          "original_form": "그치만 코로나가 심한데 극장 가기도 좀 꺼려지니까",
          "speaker_id": "1",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004301.1.6",
          "form": "뮤지컬중에 뭐가 젤 좋았어?",
          "original_form": "뮤지컬중에 뭐가 젤 좋았어?",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004301.1.7",
          "form": "음 위키드",
          "original_form": "음 위키드",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004301.1.8",
          "form": "뉴욕 브로드웨이에서 봤지",
          "original_form": "뉴욕 브로드웨이에서 봤지",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004301.1.9",
          "form": "역시 위키드지",
          "original_form": "역시 위키드지",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004301.1.10",
          "form": "같이본 사람이 훌륭해서 그런거 아닐까",
          "original_form": "같이본 사람이 훌륭해서 그런거 아닐까",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004301.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004301.1.12",
          "form": "그것도 맞아",
          "original_form": "그것도 맞아",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004301.1.13",
          "form": "위키드가 젤 좋지만",
          "original_form": "위키드가 젤 좋지만",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004301.1.14",
          "form": "지난번에 본 마리퀴리도 좋았어",
          "original_form": "지난번에 본 마리퀴리도 좋았어",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004301.1.15",
          "form": "배우들이 너무 좋았다",
          "original_form": "배우들이 너무 좋았다",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004301.1.16",
          "form": "응 그 온라인으로 본 거",
          "original_form": "응 그 온라인으로 본 거",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004301.1.17",
          "form": "코로나 때문에 직관 못하는 건 아쉬워도",
          "original_form": "코로나 때문에 직관 못하는 건 아쉬워도",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004301.1.18",
          "form": "온라인으로 공개하는 건 좋음",
          "original_form": "온라인으로 공개하는 건 좋음",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004301.1.19",
          "form": "아 맞아맞아",
          "original_form": "아 맞아맞아",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004301.1.20",
          "form": "마리퀴리에서 그 히어라배우가 너무 좋았구",
          "original_form": "마리퀴리에서 그 히어라배우가 너무 좋았구",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004301.1.21",
          "form": "정인지배우도 참 좋더라",
          "original_form": "정인지배우도 참 좋더라",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004301.1.22",
          "form": "정인지배우가 그거 뭐야 그 조선시대 여성작가..",
          "original_form": "정인지배우가 그거 뭐야 그 조선시대 여성작가..",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004301.1.23",
          "form": "허난설헌?",
          "original_form": "허난설헌?",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004301.1.24",
          "form": "아 맞아맞아",
          "original_form": "아 맞아맞아",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004301.1.25",
          "form": "그 뮤지컬도 좋아보이더라 언제 할진 모르지만",
          "original_form": "그 뮤지컬도 좋아보이더라 언제 할진 모르지만",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004301.1.26",
          "form": "응 10월인가 다윈영도 한댔는데",
          "original_form": "응 10월인가 다윈영도 한댔는데",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004301.1.27",
          "form": "서울예술단",
          "original_form": "서울예술단",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004301.1.28",
          "form": "어 맞아 올해 한댔지",
          "original_form": "어 맞아 올해 한댔지",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004301.1.29",
          "form": "배우들 많이 바뀌었더라",
          "original_form": "배우들 많이 바뀌었더라",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004301.1.30",
          "form": "캐스팅",
          "original_form": "캐스팅",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004301.1.31",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004301.1.32",
          "form": "메인롤은 거의 비슷한 것 같았는데.. 아닌가?",
          "original_form": "메인롤은 거의 비슷한 것 같았는데.. 아닌가?",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004301.1.33",
          "form": "엉 나 봤을때랑 캐스팅이 달라진듯",
          "original_form": "엉 나 봤을때랑 캐스팅이 달라진듯",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004301.1.34",
          "form": "오 그렇구만",
          "original_form": "오 그렇구만",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004301.1.35",
          "form": "다윈가족도 글코 그 여자애 말고는 친구도 다른 배우 같았는데",
          "original_form": "다윈가족도 글코 그 여자애 말고는 친구도 다른 배우 같았는데",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004301.1.36",
          "form": "비교해본건 아니라 정확하지 않을 수 있음",
          "original_form": "비교해본건 아니라 정확하지 않을 수 있음",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004301.1.37",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004301.1.38",
          "form": "ㅋㅋㅋ응응",
          "original_form": "ㅋㅋㅋ응응",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004301.1.39",
          "form": "그건 보러갈 수 있으려나",
          "original_form": "그건 보러갈 수 있으려나",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004301.1.40",
          "form": "코로나가 워낙 잠잠해지나 싶음 다시 기승이라",
          "original_form": "코로나가 워낙 잠잠해지나 싶음 다시 기승이라",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004301.1.41",
          "form": "그러네 특히 이동하는데 시간이 걸리고 길면 좀 어려워",
          "original_form": "그러네 특히 이동하는데 시간이 걸리고 길면 좀 어려워",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004301.1.42",
          "form": "응응 대중교통 이용해야하니 더더욱",
          "original_form": "응응 대중교통 이용해야하니 더더욱",
