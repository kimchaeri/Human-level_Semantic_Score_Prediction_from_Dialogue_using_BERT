      "utterance": [
        {
          "id": "MDRW2100025077.1.1",
          "form": "뭔가 정형돈이 생각나는데",
          "original_form": "뭔가 정형돈이 생각나는데",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025077.1.2",
          "form": "검색해보고 오겠어요",
          "original_form": "검색해보고 오겠어요",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025077.1.3",
          "form": "정돈에서는 꼭 안심을드세용",
          "original_form": "정돈에서는 꼭 안심을드세용",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025077.1.4",
          "form": "와이씨",
          "original_form": "와이씨",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025077.1.5",
          "form": "비쥬얼 미쳤네여",
          "original_form": "비쥬얼 미쳤네여",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025077.1.6",
          "form": "등심도맛있지만",
          "original_form": "등심도맛있지만",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025077.1.7",
          "form": "도랏어요 도랏어요",
          "original_form": "도랏어요 도랏어요",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025077.1.8",
          "form": "만약 모듬가스를 먹으면",
          "original_form": "만약 모듬가스를 먹으면",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025077.1.9",
          "form": "반드시등심부터먼저드시고안심을드세요",
          "original_form": "반드시등심부터먼저드시고안심을드세요",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025077.1.10",
          "form": "부드러움이완전달라요",
          "original_form": "부드러움이완전달라요",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025077.1.11",
          "form": "저",
          "original_form": "저",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025077.1.12",
          "form": "똥그란 건 무엇인가요?",
          "original_form": "똥그란 건 무엇인가요?",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025077.1.13",
          "form": "멘치인가?",
          "original_form": "멘치인가?",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025077.1.14",
          "form": "동그란게안심",
          "original_form": "동그란게안심",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025077.1.15",
          "form": "헉!!!!!!!!",
          "original_form": "헉!!!!!!!!",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025077.1.16",
          "form": "비계붙어있5ㅡㄴ게등심이용",
          "original_form": "비계붙어있5ㅡㄴ게등심이용",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025077.1.17",
          "form": "아니 저 저렇게",
          "original_form": "아니 저 저렇게",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025077.1.18",
          "form": "덜익은 돼지고기돈가스",
          "original_form": "덜익은 돼지고기돈가스",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025077.1.19",
          "form": "먹어보지 못햇어요",
          "original_form": "먹어보지 못햇어요",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025077.1.20",
          "form": "신선해야 저렇게 튀길 수 있다던데",
          "original_form": "신선해야 저렇게 튀길 수 있다던데",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025077.1.21",
          "form": "덜익은건아니고 무슨반응이있어서",
          "original_form": "덜익은건아니고 무슨반응이있어서",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025077.1.22",
          "form": "빨갛게보이는거라던데",
          "original_form": "빨갛게보이는거라던데",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025077.1.23",
          "form": "아무튼부드러움이 진짜",
          "original_form": "아무튼부드러움이 진짜",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025077.1.24",
          "form": "웨이팅도 잊게하는맛...",
          "original_form": "웨이팅도 잊게하는맛...",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025077.1.25",
          "form": "으어억백종원은 덜익혀서 부드러운거라고 신선해야된다던데여..!",
          "original_form": "으어억백종원은 덜익혀서 부드러운거라고 신선해야된다던데여..!",
          "speaker_id": "1",
          "time": "20210831 22:57"
        }
      ]
    }
