      "utterance": [
        {
          "id": "MDRW2100031290.1.1",
          "form": "그나저나 솧씨는 대화 얼마나 쌓임??",
          "original_form": "그나저나 솧씨는 대화 얼마나 쌓임??",
          "speaker_id": "1",
          "time": "20210904 20:02"
        },
        {
          "id": "MDRW2100031290.1.2",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031290.1.3",
          "form": "나 15개... 우허는?",
          "original_form": "나 15개... 우허는?",
          "speaker_id": "2",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031290.1.4",
          "form": "헐 빠르네",
          "original_form": "헐 빠르네",
          "speaker_id": "1",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031290.1.5",
          "form": "나 6개",
          "original_form": "나 6개",
          "speaker_id": "1",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031290.1.6",
          "form": "헉 빨리빨리대화해보자",
          "original_form": "헉 빨리빨리대화해보자",
          "speaker_id": "2",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031290.1.7",
          "form": "학교 요즘어때",
          "original_form": "학교 요즘어때",
          "speaker_id": "2",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031290.1.8",
          "form": "요즘도 식사 띄어앉아서하고있엉?",
          "original_form": "요즘도 식사 띄어앉아서하고있엉?",
          "speaker_id": "2",
          "time": "20210904 20:04"
        },
        {
          "id": "MDRW2100031290.1.9",
          "form": "막 가림막하고 수업해?!",
          "original_form": "막 가림막하고 수업해?!",
          "speaker_id": "2",
          "time": "20210904 20:04"
        },
        {
          "id": "MDRW2100031290.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:05"
        },
        {
          "id": "MDRW2100031290.1.11",
          "form": "식사때만 가림막 있고",
          "original_form": "식사때만 가림막 있고",
          "speaker_id": "1",
          "time": "20210904 20:05"
        },
        {
          "id": "MDRW2100031290.1.12",
          "form": "아 책상은 다 하나씩 띄워져 있네",
          "original_form": "아 책상은 다 하나씩 띄워져 있네",
          "speaker_id": "1",
          "time": "20210904 20:05"
        },
        {
          "id": "MDRW2100031290.1.13",
          "form": "가림막은 따로 없고",
          "original_form": "가림막은 따로 없고",
          "speaker_id": "1",
          "time": "20210904 20:05"
        },
        {
          "id": "MDRW2100031290.1.14",
          "form": "헉헉",
          "original_form": "헉헉",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031290.1.15",
          "form": "고2때부터 그렇게 된거야?",
          "original_form": "고2때부터 그렇게 된거야?",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031290.1.16",
          "form": "어때 많이불편하지ㅠㅠㅠ",
          "original_form": "어때 많이불편하지ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031290.1.17",
          "form": "이미 익숙해졌어...",
          "original_form": "이미 익숙해졌어...",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031290.1.18",
          "form": "name2씨는 학교생활 어때??",
          "original_form": "&name2&씨는 학교생활 어때??",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031290.1.19",
          "form": "학교... 그냥... 학교 안간지1년반넘어서",
          "original_form": "학교... 그냥... 학교 안간지1년반넘어서",
          "speaker_id": "2",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031290.1.20",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031290.1.21",
          "form": "원격+휴학 하니까 학교생활이 없어진거같아",
          "original_form": "원격+휴학 하니까 학교생활이 없어진거같아",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031290.1.22",
          "form": "학비아까워!!!",
          "original_form": "학비아까워!!!",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031290.1.23",
          "form": "옛날부터 궁금했던 거!!",
          "original_form": "옛날부터 궁금했던 거!!",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031290.1.24",
          "form": "입학을 거기로 한거야 아님 편입한거야??",
          "original_form": "입학을 거기로 한거야 아님 편입한거야??",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031290.1.25",
          "form": "편입했엉",
          "original_form": "편입했엉",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031290.1.26",
          "form": "편입후로한번도안갔네학교를...",
          "original_form": "편입후로한번도안갔네학교를...",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100031290.1.27",
          "form": "근데 편입전 학교에서 범죄학 교양들으면서",
          "original_form": "근데 편입전 학교에서 범죄학 교양들으면서",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100031290.1.28",
          "form": "존제이대학으로 수업들으러갔었어",
          "original_form": "존제이대학으로 수업들으러갔었어",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100031290.1.29",
          "form": "근데 별관에서 수업을 들어서 본관의 맛을 못본게 아쉬웠짚",
          "original_form": "근데 별관에서 수업을 들어서 본관의 맛을 못본게 아쉬웠짚",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100031290.1.30",
          "form": "오 편입은 어떻게 한거야??",
          "original_form": "오 편입은 어떻게 한거야??",
          "speaker_id": "1",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031290.1.31",
          "form": "나도 유학에 약간 관심이 있어서...",
          "original_form": "나도 유학에 약간 관심이 있어서...",
          "speaker_id": "1",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031290.1.32",
          "form": "오오 나는 아예 시티 콜리지에 들어갔어",
          "original_form": "오오 나는 아예 시티 콜리지에 들어갔어",
          "speaker_id": "2",
          "time": "20210904 20:29"
        },
        {
          "id": "MDRW2100031290.1.33",
          "form": "뉴욕에는 CUNY라고 city university of new york이라고 있는데",
          "original_form": "뉴욕에는 CUNY라고 city university of new york이라고 있는데",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100031290.1.34",
          "form": "그안에서는 편입이 자유로와",
          "original_form": "그안에서는 편입이 자유로와",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100031290.1.35",
          "form": "유학은.... 독일로가는거어때",
          "original_form": "유학은.... 독일로가는거어때",
          "speaker_id": "2",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031290.1.36",
          "form": "독일은 학비도싸고...",
          "original_form": "독일은 학비도싸고...",
          "speaker_id": "2",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031290.1.37",
          "form": "좋단말이지",
          "original_form": "좋단말이지",
          "speaker_id": "2",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031290.1.38",
          "form": "쾌적한환경도있고...",
          "original_form": "쾌적한환경도있고...",
          "speaker_id": "2",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031290.1.39",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031290.1.40",
          "form": "독일은 언어가 안 돼서...",
          "original_form": "독일은 언어가 안 돼서...",
          "speaker_id": "1",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100031290.1.41",
          "form": "헉 독일인들... 영어잘쓰던데",
          "original_form": "헉 독일인들... 영어잘쓰던데",
          "speaker_id": "2",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031290.1.42",
          "form": "일본은어땨",
          "original_form": "일본은어땨",
          "speaker_id": "2",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031290.1.43",
          "form": "수업은 독어로 하지 않아??",
          "original_form": "수업은 독어로 하지 않아??",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031290.1.44",
          "form": "입학은 아얘 시티 칼리지로 한거야?",
          "original_form": "입학은 아얘 시티 칼리지로 한거야?",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031290.1.45",
          "form": "한국 대학 없이?",
          "original_form": "한국 대학 없이?",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031290.1.46",
          "form": "예아",
          "original_form": "예아",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031290.1.47",
          "form": "아 수업독어로하려나",
          "original_form": "아 수업독어로하려나",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031290.1.48",
          "form": "독일 프랑스도 생각해봤는데",
          "original_form": "독일 프랑스도 생각해봤는데",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031290.1.49",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031290.1.50",
          "form": "언어가 안 돼서...",
          "original_form": "언어가 안 돼서...",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031290.1.51",
          "form": "허걱 ㅠ",
          "original_form": "허걱 ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031290.1.52",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031290.1.53",
          "form": "나중에 방문학생이라거나",
          "original_form": "나중에 방문학생이라거나",
          "speaker_id": "2",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031290.1.54",
          "form": "그런제도도 대학가면많으니까",
          "original_form": "그런제도도 대학가면많으니까",
