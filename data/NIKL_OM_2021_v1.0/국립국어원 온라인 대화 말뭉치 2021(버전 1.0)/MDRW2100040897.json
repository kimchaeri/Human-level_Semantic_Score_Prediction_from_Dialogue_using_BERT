      "utterance": [
        {
          "id": "MDRW2100040897.1.1",
          "form": "당근",
          "original_form": "당근",
          "speaker_id": "2",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100040897.1.2",
          "form": "오늘은 돼지바를 먹어야쥐",
          "original_form": "오늘은 돼지바를 먹어야쥐",
          "speaker_id": "2",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100040897.1.3",
          "form": "돼지 바",
          "original_form": "돼지 바",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100040897.1.4",
          "form": "주꾸미",
          "original_form": "주꾸미",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100040897.1.5",
          "form": "환상적인 코스네",
          "original_form": "환상적인 코스네",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100040897.1.6",
          "form": "좋구만 아름다운 일요일 점심",
          "original_form": "좋구만 아름다운 일요일 점심",
          "speaker_id": "2",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100040897.1.7",
          "form": "맛있는 점심식사",
          "original_form": "맛있는 점심식사",
          "speaker_id": "1",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100040897.1.8",
          "form": "배부르게 먹읍쉬다 기대된당 맛있었으면",
          "original_form": "배부르게 먹읍쉬다 기대된당 맛있었으면",
          "speaker_id": "2",
          "time": "20210905 11:57"
        },
        {
          "id": "MDRW2100040897.1.9",
          "form": "그르게 맛있었으면",
          "original_form": "그르게 맛있었으면",
          "speaker_id": "1",
          "time": "20210905 12:03"
        },
        {
          "id": "MDRW2100040897.1.10",
          "form": "아빠는 덜맵게를 주문하셨어",
          "original_form": "아빠는 덜맵게를 주문하셨어",
          "speaker_id": "1",
          "time": "20210905 12:03"
        },
        {
          "id": "MDRW2100040897.1.11",
          "form": "중간맵인데",
          "original_form": "중간맵인데",
          "speaker_id": "2",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100040897.1.12",
          "form": "쭈꾸미는.. 덜맵이 없을걸...?",
          "original_form": "쭈꾸미는.. 덜맵이 없을걸...?",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100040897.1.13",
          "form": "응 중간맵으로 해야지",
          "original_form": "응 중간맵으로 해야지",
          "speaker_id": "1",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100040897.1.14",
          "form": "아님 그 배달메시지에",
          "original_form": "아님 그 배달메시지에",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100040897.1.15",
          "form": "조금덜맵게 해주세요 해야하나",
          "original_form": "조금덜맵게 해주세요 해야하나",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100040897.1.16",
          "form": "한번 써보기는 하겠습니다",
          "original_form": "한번 써보기는 하겠습니다",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100040897.1.17",
          "form": "덜맵",
          "original_form": "덜맵",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100040897.1.18",
          "form": "양념 넉넉",
          "original_form": "양념 넉넉",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100040897.1.19",
          "form": "네 좋아유 넘 길면 양념넉넉은 빼1",
          "original_form": "네 좋아유 넘 길면 양념넉넉은 빼1",
          "speaker_id": "2",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100040897.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100040897.1.21",
          "form": "엄마 소면 드시고 싶어하잖아",
          "original_form": "엄마 소면 드시고 싶어하잖아",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100040897.1.22",
          "form": "ㅋㅋㅋㅋ그려 일단 적어봅쉬다",
          "original_form": "ㅋㅋㅋㅋ그려 일단 적어봅쉬다",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100040897.1.23",
          "form": "이제 배달음식 주문하겠습니다",
          "original_form": "이제 배달음식 주문하겠습니다",
          "speaker_id": "1",
          "time": "20210905 12:19"
        },
        {
          "id": "MDRW2100040897.1.24",
          "form": "좋습니다",
          "original_form": "좋습니다",
          "speaker_id": "2",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100040897.1.25",
          "form": "배달합쉬다",
          "original_form": "배달합쉬다",
          "speaker_id": "2",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100040897.1.26",
          "form": "배달 주문 완료",
          "original_form": "배달 주문 완료",
          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100040897.1.27",
          "form": "주꾸미가 1시간 쯤 후에 도착합니다~",
          "original_form": "주꾸미가 1시간 쯤 후에 도착합니다~",
          "speaker_id": "1",
          "time": "20210905 12:26"
        },
        {
          "id": "MDRW2100040897.1.28",
          "form": "좋습니다 엄청 기대되네요",
          "original_form": "좋습니다 엄청 기대되네요",
          "speaker_id": "2",
          "time": "20210905 12:27"
        },
        {
          "id": "MDRW2100040897.1.29",
          "form": "배고푸당",
          "original_form": "배고푸당",
          "speaker_id": "2",
          "time": "20210905 12:27"
        },
        {
          "id": "MDRW2100040897.1.30",
          "form": "엉 1시간 배달 기다리라는데",
          "original_form": "엉 1시간 배달 기다리라는데",
          "speaker_id": "1",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100040897.1.31",
          "form": "그것보단 좀 일찍 오겠지",
          "original_form": "그것보단 좀 일찍 오겠지",
          "speaker_id": "1",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100040897.1.32",
          "form": "오면 맛있게 먹자!",
          "original_form": "오면 맛있게 먹자!",
          "speaker_id": "1",
          "time": "20210905 12:29"
        },
        {
          "id": "MDRW2100040897.1.33",
          "form": "엉 그때까지 수업준비하면서 배를 비워야..",
          "original_form": "엉 그때까지 수업준비하면서 배를 비워야..",
          "speaker_id": "2",
          "time": "20210905 12:29"
        },
        {
          "id": "MDRW2100040897.1.34",
          "form": "배는 이미 비었다...",
          "original_form": "배는 이미 비었다...",
          "speaker_id": "1",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100040897.1.35",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100040897.1.36",
          "form": "주문하니 배고프네",
          "original_form": "주문하니 배고프네",
          "speaker_id": "1",
          "time": "20210905 12:36"
        }
      ]
    }
