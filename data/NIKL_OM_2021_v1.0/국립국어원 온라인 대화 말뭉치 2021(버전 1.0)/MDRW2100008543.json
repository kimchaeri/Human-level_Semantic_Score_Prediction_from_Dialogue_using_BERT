      "utterance": [
        {
          "id": "MDRW2100008543.1.1",
          "form": "오늘 주말인데 집안일 한거있어요?",
          "original_form": "오늘 주말인데 집안일 한거있어요?",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.2",
          "form": "저는 해야하는데 오백번 미루고 누워있어요",
          "original_form": "저는 해야하는데 오백번 미루고 누워있어요",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.3",
          "form": "저... 집안일을... 미루는 편이라",
          "original_form": "저... 집안일을... 미루는 편이라",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.4",
          "form": "앗 저두",
          "original_form": "앗 저두",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.5",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저인줄",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저인줄",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.7",
          "form": "맛잘알은",
          "original_form": "맛잘알은",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.8",
          "form": "미루는것도",
          "original_form": "미루는것도",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.9",
          "form": "역시 집안일은 미뤄야제맛",
          "original_form": "역시 집안일은 미뤄야제맛",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.10",
          "form": "맞아요... 미래의 내가 어차피 해주니까",
          "original_form": "맞아요... 미래의 내가 어차피 해주니까",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008543.1.11",
          "form": "어떻게든 된다구요",
          "original_form": "어떻게든 된다구요",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008543.1.12",
          "form": "그쵸 지금의 나는 지금을 즐기면된다구요",
          "original_form": "그쵸 지금의 나는 지금을 즐기면된다구요",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008543.1.13",
          "form": "미래의 내가 욕하면서 치울테니까",
          "original_form": "미래의 내가 욕하면서 치울테니까",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008543.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008543.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ해야되는 상황이 오면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ해야되는 상황이 오면",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008543.1.16",
          "form": "알아서 할테니까..",
          "original_form": "알아서 할테니까..",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008543.1.17",
          "form": "그쵸 사람은 닥치면 다 하게 되더라구요",
          "original_form": "그쵸 사람은 닥치면 다 하게 되더라구요",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008543.1.18",
          "form": "그럼 난 이게 제일 귀찮다",
          "original_form": "그럼 난 이게 제일 귀찮다",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008543.1.19",
          "form": "뭐가있어요",
          "original_form": "뭐가있어요",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008543.1.20",
          "form": "설...거...지...",
          "original_form": "설...거...지...",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008543.1.21",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008543.1.22",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008543.1.23",
          "form": "설거지 제일귀찮아",
          "original_form": "설거지 제일귀찮아",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008543.1.24",
          "form": "제일 귀찮아요....",
          "original_form": "제일 귀찮아요....",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008543.1.25",
          "form": "먹는건 재밌는데 설거지 제일싫어",
          "original_form": "먹는건 재밌는데 설거지 제일싫어",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008543.1.26",
          "form": "그때그때 하는 사람들이 대단한 거야...",
          "original_form": "그때그때 하는 사람들이 대단한 거야...",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008543.1.27",
