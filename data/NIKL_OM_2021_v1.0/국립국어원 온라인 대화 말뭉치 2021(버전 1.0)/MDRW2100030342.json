      "utterance": [
        {
          "id": "MDRW2100030342.1.1",
          "form": "헐 왜??",
          "original_form": "헐 왜??",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030342.1.2",
          "form": "누구 사기 당햇어?",
          "original_form": "누구 사기 당햇어?",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030342.1.3",
          "form": "난 잠만보 인형 2개 샀다가 1개보내주고 잠수탄",
          "original_form": "난 잠만보 인형 2개 샀다가 1개보내주고 잠수탄",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030342.1.4",
          "form": "판매자를 만났어",
          "original_form": "판매자를 만났어",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030342.1.5",
          "form": "헐... 심각허내",
          "original_form": "헐... 심각허내",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030342.1.6",
          "form": "잠수탔을때 얼마나 애탔는지 몰라",
          "original_form": "잠수탔을때 얼마나 애탔는지 몰라",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030342.1.7",
          "form": "그런 비양심적인 판매자도 다 잇네 ㅡㅡ",
          "original_form": "그런 비양심적인 판매자도 다 잇네 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030342.1.8",
          "form": "한 3번은 보냈어 톡을",
          "original_form": "한 3번은 보냈어 톡을",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.9",
          "form": "정말 짜증낫겟다",
          "original_form": "정말 짜증낫겟다",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.10",
          "form": "그러니까 사람이 상도덕이있는데",
          "original_form": "그러니까 사람이 상도덕이있는데",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.11",
          "form": "진짜 짜증났어 그래서 인터넷 쇼핑을 할때는",
          "original_form": "진짜 짜증났어 그래서 인터넷 쇼핑을 할때는",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.12",
          "form": "후기를 잘 보고사야해",
          "original_form": "후기를 잘 보고사야해",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.13",
          "form": "맞어..",
          "original_form": "맞어..",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.14",
          "form": "올려놓은 이미지랑",
          "original_form": "올려놓은 이미지랑",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.15",
          "form": "다음부터는 리뷰를 잘 보려고",
          "original_form": "다음부터는 리뷰를 잘 보려고",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.16",
          "form": "다른 상품 보내는 경우도 많잖어",
          "original_form": "다른 상품 보내는 경우도 많잖어",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.17",
          "form": "그래 ㅎㅎ",
          "original_form": "그래 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.18",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.19",
          "form": "사람들 후기가 중요허지",
          "original_form": "사람들 후기가 중요허지",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.20",
          "form": "진짜 짜증날것같아",
          "original_form": "진짜 짜증날것같아",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.21",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.22",
          "form": "그런경우 없게 조심해야겟다",
          "original_form": "그런경우 없게 조심해야겟다",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030342.1.23",
          "form": "만약 생기면 신고도 잘 하구",
          "original_form": "만약 생기면 신고도 잘 하구",
          "speaker_id": "2",
          "time": "20210903 23:57"
        }
      ]
