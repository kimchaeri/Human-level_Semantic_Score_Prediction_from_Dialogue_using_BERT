      "utterance": [
        {
          "id": "MDRW2100010309.1.1",
          "form": "영양제 뭐 필요해?",
          "original_form": "영양제 뭐 필요해?",
          "speaker_id": "1",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100010309.1.2",
          "form": "비타민d 필요해",
          "original_form": "비타민d 필요해",
          "speaker_id": "2",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010309.1.3",
          "form": "먹어보니 효과있어?",
          "original_form": "먹어보니 효과있어?",
          "speaker_id": "1",
          "time": "20210823 16:42"
        },
        {
          "id": "MDRW2100010309.1.4",
          "form": "효과 있을거야",
          "original_form": "효과 있을거야",
          "speaker_id": "2",
          "time": "20210823 16:43"
        },
        {
          "id": "MDRW2100010309.1.5",
          "form": "효과없으면 먹을필요 없지",
          "original_form": "효과없으면 먹을필요 없지",
          "speaker_id": "1",
          "time": "20210823 16:43"
        },
        {
          "id": "MDRW2100010309.1.6",
          "form": "효과 없는거 아니야",
          "original_form": "효과 없는거 아니야",
          "speaker_id": "2",
          "time": "20210823 16:44"
        },
        {
          "id": "MDRW2100010309.1.7",
          "form": "비타민c는?",
          "original_form": "비타민c는?",
          "speaker_id": "1",
          "time": "20210823 16:44"
        },
        {
          "id": "MDRW2100010309.1.8",
          "form": "비타민c 효과 있어",
          "original_form": "비타민c 효과 있어",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010309.1.9",
          "form": "알약이랑 가루 중에 뭐가 더 좋아?",
          "original_form": "알약이랑 가루 중에 뭐가 더 좋아?",
          "speaker_id": "1",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010309.1.10",
          "form": "둘 다 좋아",
          "original_form": "둘 다 좋아",
          "speaker_id": "2",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010309.1.11",
          "form": "종합비타민 먹다가 안먹으니까 어때?",
          "original_form": "종합비타민 먹다가 안먹으니까 어때?",
          "speaker_id": "1",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010309.1.12",
          "form": "별차이 없지?",
          "original_form": "별차이 없지?",
          "speaker_id": "1",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010309.1.13",
          "form": "응 별차이 없어",
          "original_form": "응 별차이 없어",
          "speaker_id": "2",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010309.1.14",
          "form": "그럼 비타민c, d만 계속 먹자",
          "original_form": "그럼 비타민c, d만 계속 먹자",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010309.1.15",
          "form": "알았어 그렇게 하자",
          "original_form": "알았어 그렇게 하자",
          "speaker_id": "2",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010309.1.16",
          "form": "계속 내가 샀으니까 이번은 너가 주문해",
          "original_form": "계속 내가 샀으니까 이번은 너가 주문해",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010309.1.17",
          "form": "어떻게 주문할지 모르겠다",
          "original_form": "어떻게 주문할지 모르겠다",
          "speaker_id": "2",
          "time": "20210823 16:50"
        },
        {
          "id": "MDRW2100010309.1.18",
          "form": "돈주면 내가 주문할게 ㅋㅋ",
          "original_form": "돈주면 내가 주문할게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:50"
        },
        {
          "id": "MDRW2100010309.1.19",
          "form": "영양제 얼마야?",
          "original_form": "영양제 얼마야?",
          "speaker_id": "2",
          "time": "20210823 16:50"
        },
        {
          "id": "MDRW2100010309.1.20",
          "form": "나중에 가격 보내줄게",
          "original_form": "나중에 가격 보내줄게",
          "speaker_id": "1",
          "time": "20210823 16:51"
        }
      ]
