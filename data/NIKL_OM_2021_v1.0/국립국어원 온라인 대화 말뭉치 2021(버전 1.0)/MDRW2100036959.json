        },
        {
          "id": "MDRW2100036959.1.2",
          "form": "이번 주말에 쇼핑몰 가려고",
          "original_form": "이번 주말에 쇼핑몰 가려고",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036959.1.3",
          "form": "오 나랑 같이 가자",
          "original_form": "오 나랑 같이 가자",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036959.1.4",
          "form": "너는 뭐 사러 갈 건데",
          "original_form": "너는 뭐 사러 갈 건데",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036959.1.5",
          "form": "나는 가을맞이 옷을 사려고",
          "original_form": "나는 가을맞이 옷을 사려고",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036959.1.6",
          "form": "나도 옷 사러 가는 거야",
          "original_form": "나도 옷 사러 가는 거야",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036959.1.7",
          "form": "서로 골라주면 되겠다",
          "original_form": "서로 골라주면 되겠다",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036959.1.8",
          "form": "코엑스로 쇼핑하러 갈까",
          "original_form": "코엑스로 쇼핑하러 갈까",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036959.1.9",
          "form": "코엑스가 좋지 좋아",
          "original_form": "코엑스가 좋지 좋아",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036959.1.10",
          "form": "옷 말고는 뭐 살 거 없어",
          "original_form": "옷 말고는 뭐 살 거 없어",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036959.1.11",
          "form": "집에 먹을 게 없어서 뭐 좀 사려고",
          "original_form": "집에 먹을 게 없어서 뭐 좀 사려고",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036959.1.12",
          "form": "그럼 잠깐 마트도 들려",
          "original_form": "그럼 잠깐 마트도 들려",
          "speaker_id": "1",
          "time": "20210831 15:43"
