      "utterance": [
        {
          "id": "MDRW2100027500.1.1",
          "form": "오오 친탁외탁 골고루 했군요!",
          "original_form": "오오 친탁외탁 골고루 했군요!",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027500.1.2",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027500.1.3",
          "form": "매탈남님 유투브 보신적 있으신가요",
          "original_form": "매탈남님 유투브 보신적 있으신가요",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027500.1.4",
          "form": "아아 그 누리네 말씀이지요?",
          "original_form": "아아 그 누리네 말씀이지요?",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027500.1.5",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027500.1.6",
          "form": "거도 재밌더라구요",
          "original_form": "거도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027500.1.7",
          "form": "하하하는 연님이밖에 몰라요",
          "original_form": "하하하는 연님이밖에 몰라요",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027500.1.8",
          "form": "하하하님네 고등어가 참 많은데",
          "original_form": "하하하님네 고등어가 참 많은데",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027500.1.9",
          "form": "저는 마릴린이 매력있더라고요",
          "original_form": "저는 마릴린이 매력있더라고요",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027500.1.10",
          "form": "외모만 봅니다ㅎㅎ",
          "original_form": "외모만 봅니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027500.1.11",
          "form": "오 이름은 들어봤어요 ㅎㅎ",
          "original_form": "오 이름은 들어봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027500.1.12",
          "form": "저도 고등어 제일 좋아해요",
          "original_form": "저도 고등어 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027500.1.13",
          "form": "누리네는 치즈냥 천국",
          "original_form": "누리네는 치즈냥 천국",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100027500.1.14",
          "form": "저는 언젠가 턱시도냥이랑 같이 살고싶어요",
          "original_form": "저는 언젠가 턱시도냥이랑 같이 살고싶어요",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027500.1.15",
          "form": "흰양말 신은 턱시도가 이상냥이입니다",
          "original_form": "흰양말 신은 턱시도가 이상냥이입니다",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027500.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027500.1.17",
          "form": "어떤분도 그러시던데",
          "original_form": "어떤분도 그러시던데",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027500.1.18",
          "form": "턱시도냥이 인기네요?!",
          "original_form": "턱시도냥이 인기네요?!",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027500.1.19",
          "form": "작은이모",
          "original_form": "작은이모",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027500.1.20",
          "form": "3살된냥 애기 주웠는데",
          "original_form": "3살된냥 애기 주웠는데",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027500.1.21",
          "form": "턱시도 흰양말냥이에요",
          "original_form": "턱시도 흰양말냥이에요",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027500.1.22",
          "form": "너무 귀엽겠다ㅜㅜ",
          "original_form": "너무 귀엽겠다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027500.1.23",
          "form": "그냥 이유없이 너무 예뻐보여요",
          "original_form": "그냥 이유없이 너무 예뻐보여요",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027500.1.24",
          "form": "사실 냥이면 다좋아하는 저 ㅋㅋㅋ",
          "original_form": "사실 냥이면 다좋아하는 저 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027500.1.25",
          "form": "포도젤리 냄새맡고싶어요ㅜㅜ",
          "original_form": "포도젤리 냄새맡고싶어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027500.1.26",
          "form": "ㅎㅎ 핑크젤리 까만젤리 우리집엔 다있다는",
          "original_form": "ㅎㅎ 핑크젤리 까만젤리 우리집엔 다있다는",
          "speaker_id": "1",
          "time": "20210902 13:20"
        }
      ]
