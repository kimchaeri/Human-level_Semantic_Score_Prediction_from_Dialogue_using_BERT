      "utterance": [
        {
          "id": "MDRW2100020644.1.1",
          "form": "오늘 점심엔 뭐 먹을까?",
          "original_form": "오늘 점심엔 뭐 먹을까?",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020644.1.2",
          "form": "닭갈비 해동하고 있어 ㅎㅎ",
          "original_form": "닭갈비 해동하고 있어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020644.1.3",
          "form": "어제 도착했다더니 바로 먹네",
          "original_form": "어제 도착했다더니 바로 먹네",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020644.1.4",
          "form": "고럼 사리면까지 주셨는데 열심히 먹어야지",
          "original_form": "고럼 사리면까지 주셨는데 열심히 먹어야지",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020644.1.5",
          "form": "할인 문자만 오면 1등으로 사는 우리 ㅋㅋ",
          "original_form": "할인 문자만 오면 1등으로 사는 우리 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020644.1.6",
          "form": "주인 아저씨가 좋아하실거야 ㅎㅎ",
          "original_form": "주인 아저씨가 좋아하실거야 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020644.1.7",
          "form": "완전 충성고객 ㅋ",
          "original_form": "완전 충성고객 ㅋ",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020644.1.8",
          "form": "고구마랑 양배추 떡 다 있어 ㅎㅎ",
          "original_form": "고구마랑 양배추 떡 다 있어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020644.1.9",
          "form": "재료 넣어서 볶기만 하면 되네",
          "original_form": "재료 넣어서 볶기만 하면 되네",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020644.1.10",
          "form": "점심 메뉴 해결되서 너무 좋다",
          "original_form": "점심 메뉴 해결되서 너무 좋다",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020644.1.11",
          "form": "남은 걸로는 저녁 볶음밥? ㅋ",
          "original_form": "남은 걸로는 저녁 볶음밥? ㅋ",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020644.1.12",
          "form": "오 그럼 하루 해결이네ㅜㅋㅋ",
          "original_form": "오 그럼 하루 해결이네ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020644.1.13",
          "form": "닭갈비 맛있겠다 ~ 몇 시에 먹지?",
          "original_form": "닭갈비 맛있겠다 ~ 몇 시에 먹지?",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020644.1.14",
          "form": "11시반이나 12시쯤",
          "original_form": "11시반이나 12시쯤",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020644.1.15",
          "form": "하긴 쉬는 날이니 아무때나 편할 때 먹으면 되겠다 ㅎㅎ",
          "original_form": "하긴 쉬는 날이니 아무때나 편할 때 먹으면 되겠다 ㅎㅎ",
