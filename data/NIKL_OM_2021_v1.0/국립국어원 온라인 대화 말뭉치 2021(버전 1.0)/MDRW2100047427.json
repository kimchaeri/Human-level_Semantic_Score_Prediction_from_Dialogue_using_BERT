      "utterance": [
        {
          "id": "MDRW2100047427.1.1",
          "form": "여름을 좋아하시나요?",
          "original_form": "여름을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100047427.1.2",
          "form": "네 생기가 느껴져서요",
          "original_form": "네 생기가 느껴져서요",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100047427.1.3",
          "form": "오 그렇군요 ㅋㅋㅋ 생각지도 못한 답변이었어요 ㅋㅋ",
          "original_form": "오 그렇군요 ㅋㅋㅋ 생각지도 못한 답변이었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100047427.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100047427.1.5",
          "form": "저는 쾌적한 환경이좋아서 봄과 가을을 선호하는편이에요.ㅋㅋ",
          "original_form": "저는 쾌적한 환경이좋아서 봄과 가을을 선호하는편이에요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100047427.1.6",
          "form": "곧 가을이네요!!!",
          "original_form": "곧 가을이네요!!!",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100047427.1.7",
          "form": "맞아요.ㅋㅋ 차 타고 단풍구경가야하는데 말이죠 ㅜㅜ",
          "original_form": "맞아요.ㅋㅋ 차 타고 단풍구경가야하는데 말이죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100047427.1.8",
          "form": "봄가을이 짧아져서 아쉬워요",
          "original_form": "봄가을이 짧아져서 아쉬워요",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047427.1.9",
          "form": "ㅠㅠ 맞아요.\r\n봄 조금 지나면 장미축제도 가보고 그래야하는데 말이죠",
          "original_form": "ㅠㅠ 맞아요.\r\n봄 조금 지나면 장미축제도 가보고 그래야하는데 말이죠",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047427.1.10",
          "form": "장미축제도 있나요,ㅈ",
          "original_form": "장미축제도 있나요,ㅈ",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100047427.1.11",
          "form": "어디서하나요?",
          "original_form": "어디서하나요?",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100047427.1.12",
          "form": "네.ㅋㅋ 찾아보니 여러지역에서 하는것같네요.",
          "original_form": "네.ㅋㅋ 찾아보니 여러지역에서 하는것같네요.",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100047427.1.13",
          "form": "저는 강원도 삼척갔다왔엇어요.",
          "original_form": "저는 강원도 삼척갔다왔엇어요.",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100047427.1.14",
          "form": "맛있는거 드셨나요?",
          "original_form": "맛있는거 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100047427.1.15",
          "form": "한 3년전일이라 기억이 잘 안나네요 ㅋㅋ",
          "original_form": "한 3년전일이라 기억이 잘 안나네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:47"
        }
