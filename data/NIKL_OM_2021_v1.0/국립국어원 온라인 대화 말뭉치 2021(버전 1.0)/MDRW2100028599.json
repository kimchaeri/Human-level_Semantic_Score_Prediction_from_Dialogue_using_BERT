      "utterance": [
        {
          "id": "MDRW2100028599.1.1",
          "form": "저녁은 뭘로 드셨어요?",
          "original_form": "저녁은 뭘로 드셨어요?",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028599.1.2",
          "form": "축구와 함께 치킨이요",
          "original_form": "축구와 함께 치킨이요",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028599.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028599.1.4",
          "form": "걍 밥이랑 반찬이요ㅋㅋ",
          "original_form": "걍 밥이랑 반찬이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028599.1.5",
          "form": "후식은 아이스크림",
          "original_form": "후식은 아이스크림",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028599.1.6",
          "form": "오 무슨아스크림인가요",
          "original_form": "오 무슨아스크림인가요",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028599.1.7",
          "form": "배라 아몬드봉봉이요",
          "original_form": "배라 아몬드봉봉이요",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028599.1.8",
          "form": "좋아하는 아이스크림 있어요?",
          "original_form": "좋아하는 아이스크림 있어요?",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028599.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028599.1.10",
          "form": "그거 저도 요즘 최애에 등극했는데",
          "original_form": "그거 저도 요즘 최애에 등극했는데",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028599.1.11",
          "form": "파스타치오나 솜사탕 좋아하다가 ㅎㅎ",
          "original_form": "파스타치오나 솜사탕 좋아하다가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028599.1.12",
          "form": "피스타치오 ㅋㅋ",
          "original_form": "피스타치오 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028599.1.13",
          "form": "맞아요 클래식한 맛이 질리지 않아서 넘좋아요ㅋㅋ",
          "original_form": "맞아요 클래식한 맛이 질리지 않아서 넘좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028599.1.14",
          "form": "전 땅땅치킨 먹었는데",
          "original_form": "전 땅땅치킨 먹었는데",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028599.1.15",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028599.1.16",
          "form": "아하 알아요! 우리동네에도 있어요ㅋㅋ",
          "original_form": "아하 알아요! 우리동네에도 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028599.1.17",
          "form": "정답 경상도 사람!",
          "original_form": "정답 경상도 사람!",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028599.1.18",
          "form": "ㅋㅋㅋ정말 다른 지역엔 없다니 믿기지 않아요 여긴 엄청 많은데",
          "original_form": "ㅋㅋㅋ정말 다른 지역엔 없다니 믿기지 않아요 여긴 엄청 많은데",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028599.1.19",
          "form": "그나저나 치킨 요즘 비싸서 잘 안 먹지만.. 사먹는 디저트 가격 합치면 치킨값 비슷해요ㅋㅋ",
          "original_form": "그나저나 치킨 요즘 비싸서 잘 안 먹지만.. 사먹는 디저트 가격 합치면 치킨값 비슷해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028599.1.20",
          "form": "맞아요 안비싼게ㅜ없다는",
          "original_form": "맞아요 안비싼게ㅜ없다는",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028599.1.21",
          "form": "떡볶이두 원가는 싸다는데",
          "original_form": "떡볶이두 원가는 싸다는데",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028599.1.22",
          "form": "아 맞아 떡볶이 요새 엄청 비싸죠",
          "original_form": "아 맞아 떡볶이 요새 엄청 비싸죠",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028599.1.23",
          "form": "1인분을 못 사요ㅠ",
          "original_form": "1인분을 못 사요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028599.1.24",
          "form": "마카롱 마들렌 쿠키 이런것도 비싸고... 그치만 맛있고...",
          "original_form": "마카롱 마들렌 쿠키 이런것도 비싸고... 그치만 맛있고...",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028599.1.25",
          "form": "마카롱 넘 마싯더라구요",
          "original_form": "마카롱 넘 마싯더라구요",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028599.1.26",
          "form": "근데 맛집을 찾기가 어려운...",
          "original_form": "근데 맛집을 찾기가 어려운...",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028599.1.27",
          "form": "이제 날이 서늘해지면 택배 시켜야죠ㅋㅋ",
          "original_form": "이제 날이 서늘해지면 택배 시켜야죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028599.1.28",
          "form": "저는 만원에 5개 마카롱집 먹는데 괜찮더라구요 ㅎㅎ",
          "original_form": "저는 만원에 5개 마카롱집 먹는데 괜찮더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028599.1.29",
          "form": "오 맛집이 가까이 있다니 행운이네요",
          "original_form": "오 맛집이 가까이 있다니 행운이네요",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028599.1.30",
          "form": "근데 배민으로 보니 이제 마카롱 안하나바요 ㄷㄷ 안간사이",
          "original_form": "근데 배민으로 보니 이제 마카롱 안하나바요 ㄷㄷ 안간사이",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028599.1.31",
          "form": "헐... 충격사건.. 가게도 빠르게 변하네요ㅠ",
          "original_form": "헐... 충격사건.. 가게도 빠르게 변하네요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:37"
        }
      ]
