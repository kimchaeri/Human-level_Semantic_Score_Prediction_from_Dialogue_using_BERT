      "utterance": [
        {
          "id": "MDRW2100031122.1.1",
          "form": "구렇군요 ㅠㅠ",
          "original_form": "구렇군요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031122.1.2",
          "form": "이제 일이 많이 익으셨나봐요",
          "original_form": "이제 일이 많이 익으셨나봐요",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031122.1.3",
          "form": "아 좀 생길법하다가도",
          "original_form": "아 좀 생길법하다가도",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031122.1.4",
          "form": "계좌보면 싹 사라집니다",
          "original_form": "계좌보면 싹 사라집니다",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031122.1.5",
          "form": "오 다행입니다",
          "original_form": "오 다행입니다",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031122.1.6",
          "form": "전 월급도 작고 딱히 동기부여가 안되네요ㅠㅠ",
          "original_form": "전 월급도 작고 딱히 동기부여가 안되네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031122.1.7",
          "form": "ㅋㅋ 공기업이면",
          "original_form": "ㅋㅋ 공기업이면",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031122.1.8",
          "form": "비슷비슷 할껄요??",
          "original_form": "비슷비슷 할껄요??",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031122.1.9",
          "form": "저희는 야간수당 때문에",
          "original_form": "저희는 야간수당 때문에",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031122.1.10",
          "form": "좀 뻥튀기 경향이 있어서",
          "original_form": "좀 뻥튀기 경향이 있어서",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031122.1.11",
          "form": "아 야간 수당 있으시구나",
          "original_form": "아 야간 수당 있으시구나",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031122.1.12",
          "form": "그래도 이 코로나 사태에 감사하고 다녀야하는데",
          "original_form": "그래도 이 코로나 사태에 감사하고 다녀야하는데",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031122.1.13",
          "form": "빌런들 땜에 속편한 날이 며칠 없네요",
          "original_form": "빌런들 땜에 속편한 날이 며칠 없네요",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031122.1.14",
          "form": "야간에 일하므로 야간수당이ㅋㅋ",
          "original_form": "야간에 일하므로 야간수당이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031122.1.15",
          "form": "어떤 빌런이 속 썪이나요",
          "original_form": "어떤 빌런이 속 썪이나요",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031122.1.16",
          "form": "동기빌런이요 ㅋㅋㅋㅋ",
          "original_form": "동기빌런이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100031122.1.17",
          "form": "동기가 나빴네요",
          "original_form": "동기가 나빴네요",
          "speaker_id": "2",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100031122.1.18",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100031122.1.19",
          "form": "그쵸 흑흑",
          "original_form": "그쵸 흑흑",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031122.1.20",
          "form": "회사가기시러여 푹 쉬셨나요",
          "original_form": "회사가기시러여 푹 쉬셨나요",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031122.1.21",
          "form": "비번 푹 쉬었습니당",
          "original_form": "비번 푹 쉬었습니당",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100031122.1.22",
          "form": "내일 쉬고 월요일 출근해야ㅠ",
          "original_form": "내일 쉬고 월요일 출근해야ㅠ",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100031122.1.23",
          "form": "그러네요 ㅠ 오늘 푹쉬고 낼 또 출근해야져",
          "original_form": "그러네요 ㅠ 오늘 푹쉬고 낼 또 출근해야져",
          "speaker_id": "1",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100031122.1.24",
          "form": "ㅋㅋ당직서면",
          "original_form": "ㅋㅋ당직서면",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031122.1.25",
          "form": "당직비 얼마주나요?",
          "original_form": "당직비 얼마주나요?",
          "speaker_id": "2",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100031122.1.26",
