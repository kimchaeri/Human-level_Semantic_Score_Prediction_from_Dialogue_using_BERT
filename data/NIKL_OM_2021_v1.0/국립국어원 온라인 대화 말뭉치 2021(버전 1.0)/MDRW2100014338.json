      "utterance": [
        {
          "id": "MDRW2100014338.1.1",
          "form": "갤워치 삿그등요~~",
          "original_form": "갤워치 삿그등요~~",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014338.1.2",
          "form": "요새 갤워치 싸다고 난리더라구요",
          "original_form": "요새 갤워치 싸다고 난리더라구요",
          "speaker_id": "2",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014338.1.3",
          "form": "손에 뭐 차는거 좋아하세요?",
          "original_form": "손에 뭐 차는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014338.1.4",
          "form": "아뇨 손목에뭐잇ㅇ면 걸리적거엿는데",
          "original_form": "아뇨 손목에뭐잇ㅇ면 걸리적거엿는데",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014338.1.5",
          "form": "운동좀할라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "운동좀할라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014338.1.6",
          "form": "운동핑계로 하나장만햇어요",
          "original_form": "운동핑계로 하나장만햇어요",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014338.1.7",
          "form": "ㅋㅋㅋㅋ운동할 때는 진짜 도움되더라구요 땀이 차서 문제지",
          "original_form": "ㅋㅋㅋㅋ운동할 때는 진짜 도움되더라구요 땀이 차서 문제지",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100014338.1.8",
          "form": "방금 스트랩하나삿어요",
          "original_form": "방금 스트랩하나삿어요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100014338.1.9",
          "form": "신난다 역시 신문물은 좋아요",
          "original_form": "신난다 역시 신문물은 좋아요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100014338.1.10",
          "form": "워치는 스트랩 바꾸는 재미던데요",
          "original_form": "워치는 스트랩 바꾸는 재미던데요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100014338.1.11",
          "form": "그 배경이랑ㅋㅋ",
          "original_form": "그 배경이랑ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100014338.1.12",
          "form": "배경지금 너무귀요워여",
          "original_form": "배경지금 너무귀요워여",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100014338.1.13",
          "form": "보여주고싶댜",
          "original_form": "보여주고싶댜",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100014338.1.14",
          "form": "가왜나쁜말이냐 어이없네",
          "original_form": "가왜나쁜말이냐 어이없네",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100014338.1.15",
          "form": "ㅋㅋㅋㅋ뭐로 하셨어요?",
          "original_form": "ㅋㅋㅋㅋ뭐로 하셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100014338.1.16",
          "form": "종류도 많던데",
          "original_form": "종류도 많던데",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100014338.1.17",
          "form": "전 나이키 하나랑 에르메스ㅋㅋㅋㅋㅋ 했었어요",
          "original_form": "전 나이키 하나랑 에르메스ㅋㅋㅋㅋㅋ 했었어요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100014338.1.18",
          "form": "에르메스 우아",
          "original_form": "에르메스 우아",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100014338.1.19",
          "form": "에르메스 궁금하다 검색해봐야겟어요",
          "original_form": "에르메스 궁금하다 검색해봐야겟어요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100014338.1.20",
          "form": "에르메스 주황색이었을건데 그거 이쁘더라구요",
          "original_form": "에르메스 주황색이었을건데 그거 이쁘더라구요",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100014338.1.21",
          "form": "배경!? 커스텀???",
          "original_form": "배경!? 커스텀???",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100014338.1.22",
          "form": "대전에 오능 신세계오픈햇어요 쿄쿄",
          "original_form": "대전에 오능 신세계오픈햇어요 쿄쿄",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100014338.1.23",
          "form": "구경갈럇더니 사람많을까봐 못가겟어여",
          "original_form": "구경갈럇더니 사람많을까봐 못가겟어여",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100014338.1.24",
