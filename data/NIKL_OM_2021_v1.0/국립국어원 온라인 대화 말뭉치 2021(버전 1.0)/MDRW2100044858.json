      "utterance": [
        {
          "id": "MDRW2100044858.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044858.1.2",
          "form": "네ㅎㅎ아까 고추잡채먹었어요!",
          "original_form": "네ㅎㅎ아까 고추잡채먹었어요!",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044858.1.3",
          "form": "맛있는거 드셨네요!",
          "original_form": "맛있는거 드셨네요!",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044858.1.4",
          "form": "직접 해드셨나요?",
          "original_form": "직접 해드셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044858.1.5",
          "form": "아뇨! 삼촌이 해주셨어요ㅎ",
          "original_form": "아뇨! 삼촌이 해주셨어요ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044858.1.6",
          "form": "세상ㅇ0",
          "original_form": "세상ㅇ0",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044858.1.7",
          "form": "요리왕이시네요!",
          "original_form": "요리왕이시네요!",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044858.1.8",
          "form": "삼촌님!!",
          "original_form": "삼촌님!!",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044858.1.9",
          "form": "네ㅋㅋ요리사는 아니지만 요리 잘하세요ㅎ",
          "original_form": "네ㅋㅋ요리사는 아니지만 요리 잘하세요ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044858.1.10",
          "form": "요리 잘하는분 있으면 좋을거같아요",
          "original_form": "요리 잘하는분 있으면 좋을거같아요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044858.1.11",
          "form": "좋아하는 음식 있으신가요",
          "original_form": "좋아하는 음식 있으신가요",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044858.1.12",
          "form": "저는 국물있는 음식이요!  칼국수나 냉면 순두부찌개같은...!",
          "original_form": "저는 국물있는 음식이요!  칼국수나 냉면 순두부찌개같은...!",
          "speaker_id": "2",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044858.1.13",
          "form": "아 냉면, 순두부찌개 너무 좋죠",
          "original_form": "아 냉면, 순두부찌개 너무 좋죠",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044858.1.14",
          "form": "비냉, 물냉 뭐 좋아하시나요?",
          "original_form": "비냉, 물냉 뭐 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044858.1.15",
          "form": "물냉이요!",
          "original_form": "물냉이요!",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044858.1.16",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044858.1.17",
          "form": "저도 물냉!!!!",
          "original_form": "저도 물냉!!!!",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044858.1.18",
          "form": "면 먹고 국물 호로록하면 그렇게 맛있더라구요...",
          "original_form": "면 먹고 국물 호로록하면 그렇게 맛있더라구요...",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044858.1.19",
          "form": "평냉 드셔보셯엉ㆍㆍ?",
          "original_form": "평냉 드셔보셯엉ㆍㆍ?",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044858.1.20",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044858.1.21",
          "form": "아니요ㅠㅠㅠ",
          "original_form": "아니요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044858.1.22",
          "form": "좀 다른가요?",
          "original_form": "좀 다른가요?",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044858.1.23",
          "form": "맛이 좀 더 슴슴해요ㅋㅋ",
          "original_form": "맛이 좀 더 슴슴해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044858.1.24",
          "form": "면발은 잘 끊기는 편이라 소화도 잘되구요",
          "original_form": "면발은 잘 끊기는 편이라 소화도 잘되구요",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044858.1.25",
          "form": "전 평냉을 특히 좋아해요",
          "original_form": "전 평냉을 특히 좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044858.1.26",
          "form": "저 이제 자러가볼게요! ㅜ",
          "original_form": "저 이제 자러가볼게요! ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:31"
        }
      ]
    }
