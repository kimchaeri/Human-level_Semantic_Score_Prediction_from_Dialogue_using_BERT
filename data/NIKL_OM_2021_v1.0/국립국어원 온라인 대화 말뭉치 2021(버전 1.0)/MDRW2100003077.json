      "utterance": [
        {
          "id": "MDRW2100003077.1.1",
          "form": "반려동물키우세요?",
          "original_form": "반려동물키우세요?",
          "speaker_id": "1",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003077.1.2",
          "form": "구피요",
          "original_form": "구피요",
          "speaker_id": "2",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003077.1.3",
          "form": "애들 어릴 때 이것저것 많이 키워봤어요",
          "original_form": "애들 어릴 때 이것저것 많이 키워봤어요",
          "speaker_id": "2",
          "time": "20210820 18:23"
        },
        {
          "id": "MDRW2100003077.1.4",
          "form": "우왕 구피! 물갈아주고 신경쓸께 좀 있지않나요?",
          "original_form": "우왕 구피! 물갈아주고 신경쓸께 좀 있지않나요?",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003077.1.5",
          "form": "물 갈아주는게 신경 쓰이는데 남편이 보통해요",
          "original_form": "물 갈아주는게 신경 쓰이는데 남편이 보통해요",
          "speaker_id": "2",
          "time": "20210820 18:32"
        },
        {
          "id": "MDRW2100003077.1.6",
          "form": "요즘 5살조카보니 사슴벌레도 키우더라구요\n저는 강아지는 친정에서 키우는데 사슴벌레 같은건 자신이없어요!",
          "original_form": "요즘 5살조카보니 사슴벌레도 키우더라구요\n저는 강아지는 친정에서 키우는데 사슴벌레 같은건 자신이없어요!",
          "speaker_id": "1",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003077.1.7",
          "form": "저희 사슴벌레 키웠었어요",
          "original_form": "저희 사슴벌레 키웠었어요",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003077.1.8",
          "form": "장수풍뎅이도 키우고",
          "original_form": "장수풍뎅이도 키우고",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003077.1.9",
          "form": "관리하기힘든가요?",
          "original_form": "관리하기힘든가요?",
          "speaker_id": "1",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003077.1.10",
          "form": "쉬워요.",
          "original_form": "쉬워요.",
          "speaker_id": "2",
          "time": "20210820 23:47"
        },
        {
          "id": "MDRW2100003077.1.11",
          "form": "키우는 반려동물 키우세요?",
          "original_form": "키우는 반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100003077.1.12",
          "form": "저희 친정에 강아지키워요ㅎㅎ",
          "original_form": "저희 친정에 강아지키워요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100003077.1.13",
          "form": "10살 노견이네요",
          "original_form": "10살 노견이네요",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100003077.1.14",
          "form": "보통 강아지 몇년 살아요?",
          "original_form": "보통 강아지 몇년 살아요?",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100003077.1.15",
          "form": "요즘은 15년까지도 사는것같아요\n사료값이 생각보다 꽤들더라구요",
          "original_form": "요즘은 15년까지도 사는것같아요\n사료값이 생각보다 꽤들더라구요",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100003077.1.16",
          "form": "강아지 vs 고양이 뭐가 더 좋으세요?",
          "original_form": "강아지 vs 고양이 뭐가 더 좋으세요?",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100003077.1.17",
          "form": "전 강아지는 무서워하고 고양이도 그닥..",
          "original_form": "전 강아지는 무서워하고 고양이도 그닥..",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100003077.1.18",
          "form": "새끼 고양이는 엄청 귀엽더라구요.^^",
          "original_form": "새끼 고양이는 엄청 귀엽더라구요.^^",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100003077.1.19",
          "form": "아 알러지는 없으시죠?",
          "original_form": "아 알러지는 없으시죠?",
          "speaker_id": "1",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100003077.1.20",
          "form": "알러지는 없는데 좀 무서워해요",
          "original_form": "알러지는 없는데 좀 무서워해요",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100003077.1.21",
          "form": "무서워하면 동물들도 앝본다고하긴하더라구요",
          "original_form": "무서워하면 동물들도 앝본다고하긴하더라구요",
