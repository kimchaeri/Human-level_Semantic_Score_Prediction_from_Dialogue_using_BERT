        },
        {
          "id": "MDRW2100032664.1.2",
          "form": "국민지원금 쓰기 스벅 참좋은데",
          "original_form": "국민지원금 쓰기 스벅 참좋은데",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100032664.1.3",
          "form": "안타깝죠",
          "original_form": "안타깝죠",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100032664.1.4",
          "form": "스벅안되죵ㅜ",
          "original_form": "스벅안되죵ㅜ",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100032664.1.5",
          "form": "맞아여ㅠ 스벜이 다 직영점이라던데",
          "original_form": "맞아여ㅠ 스벜이 다 직영점이라던데",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100032664.1.6",
          "form": "그거 몰랐어요",
          "original_form": "그거 몰랐어요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100032664.1.7",
          "form": "ㅜㅜ맞아요",
          "original_form": "ㅜㅜ맞아요",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100032664.1.8",
          "form": "이케아가보셨나요",
          "original_form": "이케아가보셨나요",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100032664.1.9",
          "form": "아 저는 이케아 매장 안가봤어요ㅋㅋㅋ",
          "original_form": "아 저는 이케아 매장 안가봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100032664.1.10",
          "form": "지방 살아가지규",
          "original_form": "지방 살아가지규",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100032664.1.11",
          "form": "아항ㅎㅎ",
          "original_form": "아항ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100032664.1.12",
          "form": "스벅은많죵",
          "original_form": "스벅은많죵",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100032664.1.13",
          "form": "맞아여 그래서 스벅이 좀 아쉬워요",
          "original_form": "맞아여 그래서 스벅이 좀 아쉬워요",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100032664.1.14",
          "form": "저거신청하셨나요?",
          "original_form": "저거신청하셨나요?",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100032664.1.15",
          "form": "저는 아직 신청은 안했어요!",
          "original_form": "저는 아직 신청은 안했어요!",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100032664.1.16",
          "form": "저도용ㅎㅎㅎ",
          "original_form": "저도용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100032664.1.17",
          "form": "곧 해야져 꼭 완전",
          "original_form": "곧 해야져 꼭 완전",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100032664.1.18",
          "form": "좋은 지원금 이니까여",
          "original_form": "좋은 지원금 이니까여",
