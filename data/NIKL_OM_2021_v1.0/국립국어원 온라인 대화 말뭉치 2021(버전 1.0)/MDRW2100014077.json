      "utterance": [
        {
          "id": "MDRW2100014077.1.1",
          "form": "전 말랑한아기토끼",
          "original_form": "전 말랑한아기토끼",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014077.1.2",
          "form": "강아지가 일하는 내 무릎 위에 올라와서 잤으면",
          "original_form": "강아지가 일하는 내 무릎 위에 올라와서 잤으면",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014077.1.3",
          "form": "아기토끼",
          "original_form": "아기토끼",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014077.1.4",
          "form": "사랑스러워",
          "original_form": "사랑스러워",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014077.1.5",
          "form": "아기토끼",
          "original_form": "아기토끼",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014077.1.6",
          "form": "아기토끼발바닥",
          "original_form": "아기토끼발바닥",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014077.1.7",
          "form": "주먹만한 솜뭉치",
          "original_form": "주먹만한 솜뭉치",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014077.1.8",
          "form": "함냐라고싶어요",
          "original_form": "함냐라고싶어요",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014077.1.9",
          "form": "토끼는 귀가너무 귀여워",
          "original_form": "토끼는 귀가너무 귀여워",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014077.1.10",
          "form": "동글한 털뭉치에",
          "original_form": "동글한 털뭉치에",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014077.1.11",
          "form": "요즘은 장모햄스터가 좀 끌리더라고요",
          "original_form": "요즘은 장모햄스터가 좀 끌리더라고요",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014077.1.12",
          "form": "튀어나왓어",
          "original_form": "튀어나왓어",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014077.1.13",
          "form": "고불고불털 햄",
          "original_form": "고불고불털 햄",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014077.1.14",
          "form": "그 부숭한 생명체가 너무 사랑스러워요",
          "original_form": "그 부숭한 생명체가 너무 사랑스러워요",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014077.1.15",
          "form": "털 빗어주고싶어요",
          "original_form": "털 빗어주고싶어요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014077.1.16",
          "form": "장모햄",
          "original_form": "장모햄",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014077.1.17",
          "form": "배출성공하면",
          "original_form": "배출성공하면",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014077.1.18",
          "form": "털축축",
          "original_form": "털축축",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014077.1.19",
          "form": "고양이발바닥냄새맡고싶어요",
          "original_form": "고양이발바닥냄새맡고싶어요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014077.1.20",
          "form": "해지나요?",
          "original_form": "해지나요?",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014077.1.21",
          "form": "조금 힘드네요",
          "original_form": "조금 힘드네요",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014077.1.22",
          "form": "글세요...",
          "original_form": "글세요...",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014077.1.23",
          "form": "고영이들 젤리는 굳은살도없이",
          "original_form": "고영이들 젤리는 굳은살도없이",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.24",
          "form": "역시 동물키우는 친구의 옆집에 사는게",
          "original_form": "역시 동물키우는 친구의 옆집에 사는게",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.25",
          "form": "말랑해요",
          "original_form": "말랑해요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.26",
          "form": "최대야",
          "original_form": "최대야",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.27",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.28",
          "form": "굳은살이 없어요???",
          "original_form": "굳은살이 없어요???",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.29",
          "form": "발 까지지않나",
          "original_form": "발 까지지않나",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.30",
          "form": "말랑~",
          "original_form": "말랑~",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.31",
          "form": "갱얼쥐들은 좀 딴딴한데",
          "original_form": "갱얼쥐들은 좀 딴딴한데",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.32",
          "form": "고영들은",
          "original_form": "고영들은",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.33",
          "form": "말랑해요",
          "original_form": "말랑해요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.34",
          "form": "특히 아기고영은",
          "original_form": "특히 아기고영은",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.35",
          "form": "아스팔트에 맨발로 다녀도 돼요?",
          "original_form": "아스팔트에 맨발로 다녀도 돼요?",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.36",
          "form": "반짝거리는젤리같아요",
          "original_form": "반짝거리는젤리같아요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.37",
          "form": "걱정된다",
          "original_form": "걱정된다",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014077.1.38",
          "form": "쫜득한",
          "original_form": "쫜득한",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014077.1.39",
          "form": "젤리",
          "original_form": "젤리",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014077.1.40",
          "form": "하...고영들은대체 어떻개 그런 모습일까요",
          "original_form": "하...고영들은대체 어떻개 그런 모습일까요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014077.1.41",
          "form": "천사가분명해요",
          "original_form": "천사가분명해요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014077.1.42",
          "form": "천사",
          "original_form": "천사",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014077.1.43",
          "form": "얼마전에 천사한테",
          "original_form": "얼마전에 천사한테",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014077.1.44",
          "form": "물줬어요",
          "original_form": "물줬어요",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014077.1.45",
          "form": "천사는 힘든삶을 사는군요",
          "original_form": "천사는 힘든삶을 사는군요",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014077.1.46",
          "form": "당신 천국 가시겠내요",
          "original_form": "당신 천국 가시겠내요",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014077.1.47",
          "form": "살인예고인가요?",
          "original_form": "살인예고인가요?",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014077.1.48",
          "form": "천국 보내드립니다.",
          "original_form": "천국 보내드립니다.",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014077.1.49",
          "form": "감사합니다.",
          "original_form": "감사합니다.",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014077.1.50",
          "form": "고영천국",
          "original_form": "고영천국",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014077.1.51",
          "form": "동물 뭐 키웠었나요?",
          "original_form": "동물 뭐 키웠었나요?",
