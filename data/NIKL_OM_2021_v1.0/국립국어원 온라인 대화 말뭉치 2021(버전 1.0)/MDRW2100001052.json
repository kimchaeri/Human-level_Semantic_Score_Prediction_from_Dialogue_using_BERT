      "utterance": [
        {
          "id": "MDRW2100001052.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210722 21:01"
        },
        {
          "id": "MDRW2100001052.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210722 21:01"
        },
        {
          "id": "MDRW2100001052.1.3",
          "form": "최근에 어떤영화보셨어요?",
          "original_form": "최근에 어떤영화보셨어요?",
          "speaker_id": "1",
          "time": "20210722 21:02"
        },
        {
          "id": "MDRW2100001052.1.4",
          "form": "영화를 잘안봐서...",
          "original_form": "영화를 잘안봐서...",
          "speaker_id": "2",
          "time": "20210722 21:02"
        },
        {
          "id": "MDRW2100001052.1.5",
          "form": "그나마 최근건 승리호 였나? 그거봤어요",
          "original_form": "그나마 최근건 승리호 였나? 그거봤어요",
          "speaker_id": "2",
          "time": "20210722 21:02"
        },
        {
          "id": "MDRW2100001052.1.6",
          "form": "오호 저는 어제 블랙위도우봤어요!",
          "original_form": "오호 저는 어제 블랙위도우봤어요!",
          "speaker_id": "1",
          "time": "20210722 21:03"
        },
        {
          "id": "MDRW2100001052.1.7",
          "form": "승리호 안봐서 내용을 모르겠네요 ㅜ",
          "original_form": "승리호 안봐서 내용을 모르겠네요 ㅜ",
          "speaker_id": "1",
          "time": "20210722 21:03"
        },
        {
          "id": "MDRW2100001052.1.8",
          "form": "그렇군요...",
          "original_form": "그렇군요...",
          "speaker_id": "2",
          "time": "20210722 21:03"
        },
        {
          "id": "MDRW2100001052.1.9",
          "form": "저는 음악방송 보는걸 좋아해서",
          "original_form": "저는 음악방송 보는걸 좋아해서",
          "speaker_id": "2",
          "time": "20210722 21:03"
        },
        {
          "id": "MDRW2100001052.1.10",
          "form": "오늘 케이콘서트봤어요",
          "original_form": "오늘 케이콘서트봤어요",
          "speaker_id": "2",
          "time": "20210722 21:04"
        },
        {
          "id": "MDRW2100001052.1.11",
          "form": "직접가서 보신거예요?",
          "original_form": "직접가서 보신거예요?",
          "speaker_id": "1",
          "time": "20210722 21:04"
        },
        {
          "id": "MDRW2100001052.1.12",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210722 21:04"
        },
        {
          "id": "MDRW2100001052.1.13",
          "form": "방송으로 했어요!",
          "original_form": "방송으로 했어요!",
          "speaker_id": "2",
          "time": "20210722 21:04"
        },
        {
          "id": "MDRW2100001052.1.14",
          "form": "코로나라 무관중으로 하더라구요ㅠㅠ",
          "original_form": "코로나라 무관중으로 하더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 21:05"
        },
        {
          "id": "MDRW2100001052.1.15",
          "form": "아하 ㅜ",
          "original_form": "아하 ㅜ",
          "speaker_id": "1",
          "time": "20210722 21:05"
        },
        {
          "id": "MDRW2100001052.1.16",
          "form": "저는 예전에 온라인방청으로 음악방송관중(?) 알바했었어요!",
          "original_form": "저는 예전에 온라인방청으로 음악방송관중(?) 알바했었어요!",
          "speaker_id": "1",
          "time": "20210722 21:05"
        },
        {
          "id": "MDRW2100001052.1.17",
          "form": "오 부러와요!",
          "original_form": "오 부러와요!",
          "speaker_id": "2",
          "time": "20210722 21:05"
        },
        {
          "id": "MDRW2100001052.1.18",
          "form": "그때 막 방탄도 있었고",
          "original_form": "그때 막 방탄도 있었고",
          "speaker_id": "1",
          "time": "20210722 21:06"
        },
        {
          "id": "MDRW2100001052.1.19",
          "form": "오 실물보셨네요",
          "original_form": "오 실물보셨네요",
          "speaker_id": "2",
          "time": "20210722 21:06"
        },
        {
          "id": "MDRW2100001052.1.20",
          "form": "그럼 막 티켓팅이런것도 해보셨어요?",
          "original_form": "그럼 막 티켓팅이런것도 해보셨어요?",
          "speaker_id": "1",
          "time": "20210722 21:06"
        },
        {
          "id": "MDRW2100001052.1.21",
          "form": "방탄 잘생겼죠...",
          "original_form": "방탄 잘생겼죠...",
          "speaker_id": "2",
          "time": "20210722 21:06"
        },
        {
          "id": "MDRW2100001052.1.22",
          "form": "티켓팅 해봤죠",
          "original_form": "티켓팅 해봤죠",
          "speaker_id": "2",
          "time": "20210722 21:06"
        },
        {
          "id": "MDRW2100001052.1.23",
          "form": "목소리가 짱이예요..!",
          "original_form": "목소리가 짱이예요..!",
          "speaker_id": "1",
          "time": "20210722 21:06"
        }
      ]
