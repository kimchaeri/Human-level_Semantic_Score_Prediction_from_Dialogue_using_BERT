      "utterance": [
        {
          "id": "MDRW2100030378.1.1",
          "form": "그래그래ㅠㅠ",
          "original_form": "그래그래ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030378.1.2",
          "form": "미용과 건강이라....",
          "original_form": "미용과 건강이라....",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030378.1.3",
          "form": "나 요즘 락시크라는 패션에 빠졌어",
          "original_form": "나 요즘 락시크라는 패션에 빠졌어",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030378.1.4",
          "form": "락시크? 말 그대로",
          "original_form": "락시크? 말 그대로",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030378.1.5",
          "form": "락+ 시크  인가",
          "original_form": "락+ 시크  인가",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030378.1.6",
          "form": "약간 퐁키하고 롹~~~ 하는 느낌인가",
          "original_form": "약간 퐁키하고 롹~~~ 하는 느낌인가",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100030378.1.7",
          "form": "약간 가죽잠바 입고",
          "original_form": "약간 가죽잠바 입고",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.8",
          "form": "오토바이 드릉드릉 할 거 같은",
          "original_form": "오토바이 드릉드릉 할 거 같은",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.9",
          "form": "잠바는 너무 옛날 사람같다ㅠㅠ",
          "original_form": "잠바는 너무 옛날 사람같다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.10",
          "form": "아 ㅋㅋㅋㅋ 이해 했다 완전 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ 이해 했다 완전 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.11",
          "form": "가죽자켓..?",
          "original_form": "가죽자켓..?",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.13",
          "form": "가죽 자켓",
          "original_form": "가죽 자켓",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.14",
          "form": "뙇 걸치고 ㅋㅋㅋ",
          "original_form": "뙇 걸치고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.15",
          "form": "ㅋㅋㅋ맞아맞아",
          "original_form": "ㅋㅋㅋ맞아맞아",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.16",
          "form": "썬글라스 끼고 가죽 바지에 검은 티",
          "original_form": "썬글라스 끼고 가죽 바지에 검은 티",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.17",
          "form": "정확하네",
          "original_form": "정확하네",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030378.1.18",
          "form": "뭔가 멋있더라고",
          "original_form": "뭔가 멋있더라고",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030378.1.19",
          "form": "그리고 악세서리는 은색이어야함",
          "original_form": "그리고 악세서리는 은색이어야함",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030378.1.20",
          "form": "내가 입던 스타일이 아니니까",
          "original_form": "내가 입던 스타일이 아니니까",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030378.1.21",
          "form": "아 맞아ㅋㅋㅋㅋ그게 핵심이지",
          "original_form": "아 맞아ㅋㅋㅋㅋ그게 핵심이지",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030378.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 어째서 이런 스타일을...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 어째서 이런 스타일을...",
