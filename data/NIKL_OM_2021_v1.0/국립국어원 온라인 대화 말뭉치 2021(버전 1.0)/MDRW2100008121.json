        {
          "id": "MDRW2100008121.1.1",
          "form": "식사는요",
          "original_form": "식사는요",
          "speaker_id": "1",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008121.1.2",
          "form": "지금 먹고있어요 분식이요",
          "original_form": "지금 먹고있어요 분식이요",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008121.1.3",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008121.1.4",
          "form": "국밥이요",
          "original_form": "국밥이요",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008121.1.5",
          "form": "전어제 감자탕이요",
          "original_form": "전어제 감자탕이요",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008121.1.6",
          "form": "밥이 좋아요",
          "original_form": "밥이 좋아요",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008121.1.7",
          "form": "밥심이죠",
          "original_form": "밥심이죠",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008121.1.8",
          "form": "갈비도 먹고싶네요 숯불로다가",
          "original_form": "갈비도 먹고싶네요 숯불로다가",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008121.1.9",
          "form": "캬 생각맘 해도",
          "original_form": "캬 생각맘 해도",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008121.1.10",
          "form": "직접가고싶은데 아기가 어려서 엄두가 안나네요",
          "original_form": "직접가고싶은데 아기가 어려서 엄두가 안나네요",
          "speaker_id": "2",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008121.1.11",
          "form": "집에서 해드세여",
          "original_form": "집에서 해드세여",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008121.1.12",
          "form": "숯불로다가 그맛이 보고싶어서요",
          "original_form": "숯불로다가 그맛이 보고싶어서요",
          "speaker_id": "2",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008121.1.13",
          "form": "아 그른가요",
          "original_form": "아 그른가요",
          "speaker_id": "1",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008121.1.14",
          "form": "밑반찬과 설거지안하고 ㅋ",
          "original_form": "밑반찬과 설거지안하고 ㅋ",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008121.1.15",
          "form": "분담 해야죠",
          "original_form": "분담 해야죠",
