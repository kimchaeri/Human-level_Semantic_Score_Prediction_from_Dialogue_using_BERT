        {
          "id": "MDRW2100008000.1.1",
          "form": "요즘 조금씩 하고 있어요",
          "original_form": "요즘 조금씩 하고 있어요",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100008000.1.2",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100008000.1.3",
          "form": "홈트해요",
          "original_form": "홈트해요",
          "speaker_id": "1",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100008000.1.4",
          "form": "얼마나 자주하세요?",
          "original_form": "얼마나 자주하세요?",
          "speaker_id": "2",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100008000.1.5",
          "form": "매일요",
          "original_form": "매일요",
          "speaker_id": "1",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100008000.1.6",
          "form": "몇 시간정도 하세요?",
          "original_form": "몇 시간정도 하세요?",
          "speaker_id": "2",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100008000.1.7",
          "form": "3세트요",
          "original_form": "3세트요",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008000.1.8",
          "form": "어느 부위 운동하세요?",
          "original_form": "어느 부위 운동하세요?",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008000.1.9",
          "form": "플랭크요",
          "original_form": "플랭크요",
          "speaker_id": "1",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100008000.1.10",
          "form": "전신",
          "original_form": "전신",
          "speaker_id": "1",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100008000.1.11",
          "form": "아침이랑 저녁 중에 언제 운동하세요?",
          "original_form": "아침이랑 저녁 중에 언제 운동하세요?",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008000.1.12",
          "form": "저녁이요",
          "original_form": "저녁이요",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008000.1.13",
          "form": "아침에 하면 하루종일 피곤요",
          "original_form": "아침에 하면 하루종일 피곤요",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
