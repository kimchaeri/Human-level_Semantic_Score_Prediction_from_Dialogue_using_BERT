      "utterance": [
        {
          "id": "MDRW2100031986.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100031986.1.2",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100031986.1.3",
          "form": "돈은 역시 쓰는게 최고죠? ㅋㅋ",
          "original_form": "돈은 역시 쓰는게 최고죠? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100031986.1.4",
          "form": "name1님께서는 어떤걸 쇼핑하는 걸 즐기시나요?",
          "original_form": "&name1&님께서는 어떤걸 쇼핑하는 걸 즐기시나요?",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031986.1.5",
          "form": "저는 요즘에 무선 이어폰 사고싶어서",
          "original_form": "저는 요즘에 무선 이어폰 사고싶어서",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031986.1.6",
          "form": "돈 모으고 있어요",
          "original_form": "돈 모으고 있어요",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031986.1.7",
          "form": "에어팟...?",
          "original_form": "에어팟...?",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.8",
          "form": "아마 버즈로 살거같아요ㅎㅎ",
          "original_form": "아마 버즈로 살거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.9",
          "form": "갤럭시 유저거든요",
          "original_form": "갤럭시 유저거든요",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.10",
          "form": "에어팟은 호환이..",
          "original_form": "에어팟은 호환이..",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.11",
          "form": "버즈! 이번에 나온 버즈 괜찮아보였어요!",
          "original_form": "버즈! 이번에 나온 버즈 괜찮아보였어요!",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.12",
          "form": "맞아요... 에어팟 호환...",
          "original_form": "맞아요... 에어팟 호환...",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.13",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.14",
          "form": "저도 갤럭시 유저에요!",
          "original_form": "저도 갤럭시 유저에요!",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.15",
          "form": "오오~~!!!",
          "original_form": "오오~~!!!",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.16",
          "form": "제가 이번에 태블릿을 샀는데",
          "original_form": "제가 이번에 태블릿을 샀는데",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031986.1.17",
          "form": "이어폰 꼽는데가 없는거에요!!ㅠㅠ",
          "original_form": "이어폰 꼽는데가 없는거에요!!ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031986.1.18",
          "form": "그래서 어쩔수 없이 사야겠더라구요..ㅠㅠㅠ",
          "original_form": "그래서 어쩔수 없이 사야겠더라구요..ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031986.1.19",
          "form": "name2님은 무선이어폰 사용하세요??",
          "original_form": "&name2&님은 무선이어폰 사용하세요??",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031986.1.20",
          "form": "요새 나오는거에 없죠 ㅠ",
          "original_form": "요새 나오는거에 없죠 ㅠ",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031986.1.21",
          "form": "저는 버즈 플러스 사용하고 있어요",
          "original_form": "저는 버즈 플러스 사용하고 있어요",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031986.1.22",
          "form": "오 그거 괜찮은가요??",
          "original_form": "오 그거 괜찮은가요??",
          "speaker_id": "1",
          "time": "20210905 10:43"
        },
        {
          "id": "MDRW2100031986.1.23",
          "form": "그거 살까 하다가 새로 나온다길래 기다렸는데",
          "original_form": "그거 살까 하다가 새로 나온다길래 기다렸는데",
          "speaker_id": "1",
          "time": "20210905 10:43"
        },
        {
          "id": "MDRW2100031986.1.24",
          "form": "어떤 목적으로 사용하실 예정이세요?",
          "original_form": "어떤 목적으로 사용하실 예정이세요?",
          "speaker_id": "2",
          "time": "20210905 10:43"
        },
        {
          "id": "MDRW2100031986.1.25",
          "form": "플러스는 노이즈캔슬링은 안돼요...",
          "original_form": "플러스는 노이즈캔슬링은 안돼요...",
          "speaker_id": "2",
          "time": "20210905 10:43"
        },
        {
          "id": "MDRW2100031986.1.26",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "1",
          "time": "20210905 10:53"
        },
        {
          "id": "MDRW2100031986.1.27",
          "form": "저는 주로 강의 들을때 사용할 것 같아요",
          "original_form": "저는 주로 강의 들을때 사용할 것 같아요",
          "speaker_id": "1",
          "time": "20210905 10:54"
        },
        {
          "id": "MDRW2100031986.1.28",
          "form": "그런거라면 노이즈 캔슬링 되는게 좋지 않을까요??",
          "original_form": "그런거라면 노이즈 캔슬링 되는게 좋지 않을까요??",
          "speaker_id": "2",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100031986.1.29",
          "form": "사실 이거도 괜찮긴한데",
          "original_form": "사실 이거도 괜찮긴한데",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031986.1.30",
          "form": "장시간 들어야하니까 아무래도?",
          "original_form": "장시간 들어야하니까 아무래도?",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031986.1.31",
          "form": "음질은 헤드폰이 역시 최고인것 같지만요 ㅠㅠ",
          "original_form": "음질은 헤드폰이 역시 최고인것 같지만요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031986.1.32",
          "form": "돈을 조금 더 모아서 신형을 사보시는 것도 괜찮을 것 같아요",
          "original_form": "돈을 조금 더 모아서 신형을 사보시는 것도 괜찮을 것 같아요",
          "speaker_id": "2",
          "time": "20210905 10:58"
        },
        {
          "id": "MDRW2100031986.1.33",
          "form": "아아 그러네요",
          "original_form": "아아 그러네요",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100031986.1.34",
          "form": "장시간 사용하긴 아무래도 노이즈캔슬링이 있는게 나을것같아요",
          "original_form": "장시간 사용하긴 아무래도 노이즈캔슬링이 있는게 나을것같아요",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100031986.1.35",
          "form": "추천 감사합니다~",
          "original_form": "추천 감사합니다~",
          "speaker_id": "1",
          "time": "20210905 11:10"
        }
      ]
