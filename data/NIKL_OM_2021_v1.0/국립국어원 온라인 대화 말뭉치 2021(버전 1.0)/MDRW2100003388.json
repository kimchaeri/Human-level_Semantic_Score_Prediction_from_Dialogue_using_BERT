      "utterance": [
        {
          "id": "MDRW2100003388.1.1",
          "form": "잠깐 나갔다 왔는데 너무 덥다",
          "original_form": "잠깐 나갔다 왔는데 너무 덥다",
          "speaker_id": "2",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003388.1.2",
          "form": "밖엔 아직 덥지.",
          "original_form": "밖엔 아직 덥지.",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003388.1.3",
          "form": "방미라 좀 시원할 줄 알았어",
          "original_form": "방미라 좀 시원할 줄 알았어",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003388.1.4",
          "form": "해가 없으니까",
          "original_form": "해가 없으니까",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003388.1.5",
          "form": "밤이라",
          "original_form": "밤이라",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003388.1.6",
          "form": "밤이어도 27도야",
          "original_form": "밤이어도 27도야",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003388.1.7",
          "form": "그니까... 샹각보다 더 더웠어",
          "original_form": "그니까... 샹각보다 더 더웠어",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003388.1.8",
          "form": "생각보다",
          "original_form": "생각보다",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003388.1.9",
          "form": "내일 제주도는 호우 특보래",
          "original_form": "내일 제주도는 호우 특보래",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003388.1.10",
          "form": "오 비 많이 올 건가 보네",
          "original_form": "오 비 많이 올 건가 보네",
          "speaker_id": "2",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003388.1.11",
          "form": "응. 비 오면 쌀쌀해지니까 옷 잘 챙겨 입어.",
          "original_form": "응. 비 오면 쌀쌀해지니까 옷 잘 챙겨 입어.",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003388.1.12",
          "form": "알겠슴당~",
          "original_form": "알겠슴당~",
          "speaker_id": "2",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003388.1.13",
          "form": "내일 겉에 입을 거 하나 챙겨가야겠다",
          "original_form": "내일 겉에 입을 거 하나 챙겨가야겠다",
          "speaker_id": "2",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003388.1.14",
          "form": "그래. 그나마 미세먼지는 좋음이니까 다행이네.",
          "original_form": "그래. 그나마 미세먼지는 좋음이니까 다행이네.",
          "speaker_id": "1",
          "time": "20210820 22:35"
        },
        {
          "id": "MDRW2100003388.1.15",
          "form": "오 비 오니까 미세먼지가 씻겨 내려가나 봐~~",
          "original_form": "오 비 오니까 미세먼지가 씻겨 내려가나 봐~~",
          "speaker_id": "2",
          "time": "20210820 22:35"
        },
        {
          "id": "MDRW2100003388.1.16",
          "form": "그런 것도 있지.",
          "original_form": "그런 것도 있지.",
          "speaker_id": "1",
          "time": "20210820 22:35"
        },
        {
          "id": "MDRW2100003388.1.17",
          "form": "습하지는 않아?",
          "original_form": "습하지는 않아?",
          "speaker_id": "1",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003388.1.18",
          "form": "습할 땐 곰팡이 잘 생기니까 조심하고.",
          "original_form": "습할 땐 곰팡이 잘 생기니까 조심하고.",
          "speaker_id": "1",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003388.1.19",
          "form": "환기 잘 시켜.",
          "original_form": "환기 잘 시켜.",
          "speaker_id": "1",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003388.1.20",
          "form": "여름이니까 아무래도 습하긴 하지",
          "original_form": "여름이니까 아무래도 습하긴 하지",
          "speaker_id": "2",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003388.1.21",
          "form": "비 온 뒤에 습도가 더 올라가겠네",
          "original_form": "비 온 뒤에 습도가 더 올라가겠네",
          "speaker_id": "2",
          "time": "20210820 22:38"
        },
        {
          "id": "MDRW2100003388.1.22",
          "form": "그렇지.",
          "original_form": "그렇지.",
          "speaker_id": "1",
          "time": "20210820 22:38"
        },
        {
          "id": "MDRW2100003388.1.23",
          "form": "덥고 습해서 열대야가 나타날 수 있다네",
          "original_form": "덥고 습해서 열대야가 나타날 수 있다네",
          "speaker_id": "1",
          "time": "20210820 22:39"
        },
        {
          "id": "MDRW2100003388.1.24",
          "form": "날씨예보 찾아봤어?",
          "original_form": "날씨예보 찾아봤어?",
          "speaker_id": "2",
          "time": "20210820 22:39"
        },
        {
          "id": "MDRW2100003388.1.25",
          "form": "어. 예보 검색해봤어.",
          "original_form": "어. 예보 검색해봤어.",
          "speaker_id": "1",
          "time": "20210820 22:40"
        },
        {
          "id": "MDRW2100003388.1.26",
          "form": "언제 기온이 확 내려가나 해서.",
          "original_form": "언제 기온이 확 내려가나 해서.",
          "speaker_id": "1",
          "time": "20210820 22:40"
        },
        {
          "id": "MDRW2100003388.1.27",
          "form": "그러게~ 말복도 한참 지났는데",
          "original_form": "그러게~ 말복도 한참 지났는데",
          "speaker_id": "2",
          "time": "20210820 22:40"
        },
        {
          "id": "MDRW2100003388.1.28",
          "form": "언제 시원해진대?",
          "original_form": "언제 시원해진대?",
          "speaker_id": "2",
          "time": "20210820 22:41"
        },
        {
          "id": "MDRW2100003388.1.29",
          "form": "다음주부터 좀 선선해지고 9월 중순 지나면 완전 가을 날씨일 것 같아.",
          "original_form": "다음주부터 좀 선선해지고 9월 중순 지나면 완전 가을 날씨일 것 같아.",
          "speaker_id": "1",
          "time": "20210820 22:41"
        },
        {
          "id": "MDRW2100003388.1.30",
          "form": "헐 제발 그랬으면 좋겠다",
          "original_form": "헐 제발 그랬으면 좋겠다",
          "speaker_id": "2",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003388.1.31",
          "form": "요즘 여름은 너무 길고 가을은 너무 짧아ㅠㅠ",
          "original_form": "요즘 여름은 너무 길고 가을은 너무 짧아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003388.1.32",
          "form": "다음주에 비가 좀 많이 올 거라는데.",
          "original_form": "다음주에 비가 좀 많이 올 거라는데.",
          "speaker_id": "1",
          "time": "20210820 22:43"
        },
        {
          "id": "MDRW2100003388.1.33",
          "form": "아 난 비오는 거 싫어~",
          "original_form": "아 난 비오는 거 싫어~",
          "speaker_id": "2",
          "time": "20210820 22:43"
        },
        {
          "id": "MDRW2100003388.1.34",
          "form": "비 오는 게 왜 싫어",
          "original_form": "비 오는 게 왜 싫어",
          "speaker_id": "1",
          "time": "20210820 22:43"
        },
        {
          "id": "MDRW2100003388.1.35",
          "form": "비 오면 바지가 젖잖아 신발도 그렇게",
          "original_form": "비 오면 바지가 젖잖아 신발도 그렇게",
          "speaker_id": "2",
          "time": "20210820 22:44"
        },
        {
          "id": "MDRW2100003388.1.36",
          "form": "신발도 그렇고",
          "original_form": "신발도 그렇고",
          "speaker_id": "2",
          "time": "20210820 22:44"
        },
        {
          "id": "MDRW2100003388.1.37",
          "form": "그래도 비가 와야 곡식도 자라고 과일도 잘 여물지.",
          "original_form": "그래도 비가 와야 곡식도 자라고 과일도 잘 여물지.",
          "speaker_id": "1",
          "time": "20210820 22:45"
        },
        {
          "id": "MDRW2100003388.1.38",
          "form": "요즘 지구가 이상해져서 비도 안 올 땐 너무 안 오고 올 때는 또 많이 오니까 문제지",
          "original_form": "요즘 지구가 이상해져서 비도 안 올 땐 너무 안 오고 올 때는 또 많이 오니까 문제지",
          "speaker_id": "2",
          "time": "20210820 22:46"
        },
        {
          "id": "MDRW2100003388.1.39",
          "form": "그러게.",
          "original_form": "그러게.",
          "speaker_id": "1",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003388.1.40",
          "form": "태풍 오마이스가 북상 중이래",
          "original_form": "태풍 오마이스가 북상 중이래",
          "speaker_id": "1",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003388.1.41",
          "form": "그래서 소나기 내리나보다",
          "original_form": "그래서 소나기 내리나보다",
          "speaker_id": "1",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003388.1.42",
          "form": "알겠어 안 까먹게 우산 미리 꺼내놔야겠다",
          "original_form": "알겠어 안 까먹게 우산 미리 꺼내놔야겠다",
          "speaker_id": "2",
          "time": "20210820 22:48"
        },
        {
          "id": "MDRW2100003388.1.43",
          "form": "응. 덥다고 에어컨 너무 세게 틀지 말고.",
          "original_form": "응. 덥다고 에어컨 너무 세게 틀지 말고.",
          "speaker_id": "1",
          "time": "20210820 22:49"
        },
        {
          "id": "MDRW2100003388.1.44",
          "form": "오늘은 비 오네ㅜㅜ",
          "original_form": "오늘은 비 오네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:07"
        }
      ]
    }
  ]
