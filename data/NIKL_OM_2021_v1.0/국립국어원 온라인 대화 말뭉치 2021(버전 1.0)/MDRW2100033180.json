        },
        {
          "id": "MDRW2100033180.1.3",
          "form": "name1님은 어떤거 쓰고 계세요?",
          "original_form": "&name1&님은 어떤거 쓰고 계세요?",
          "speaker_id": "2",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100033180.1.4",
          "form": "오 최신폰이네요!!",
          "original_form": "오 최신폰이네요!!",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100033180.1.5",
          "form": "저는 a80인데 보급형이에요 ㅋㅋ",
          "original_form": "저는 a80인데 보급형이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100033180.1.6",
          "form": "혹시 아시려나?",
          "original_form": "혹시 아시려나?",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100033180.1.7",
          "form": "헉 네네 알아요!",
          "original_form": "헉 네네 알아요!",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100033180.1.8",
          "form": "저는 이번에 바꾼거라서 !ㅠㅠ",
          "original_form": "저는 이번에 바꾼거라서 !ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100033180.1.9",
          "form": "늦게 바꿀걸 그랬어요 플립3 예쁘던데 ㅠㅠ",
          "original_form": "늦게 바꿀걸 그랬어요 플립3 예쁘던데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100033180.1.10",
          "form": "아~ 그러니깐요 저도 플립3 너무 아른거리는데",
          "original_form": "아~ 그러니깐요 저도 플립3 너무 아른거리는데",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100033180.1.11",
          "form": "아직 약정이 남아서",
          "original_form": "아직 약정이 남아서",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100033180.1.12",
          "form": "결국 못바꿨어요 ㅜ",
          "original_form": "결국 못바꿨어요 ㅜ",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100033180.1.13",
          "form": "실물 보셨어요?",
          "original_form": "실물 보셨어요?",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033180.1.14",
          "form": "ㅠㅠㅠㅠ 아쉬워요",
          "original_form": "ㅠㅠㅠㅠ 아쉬워요",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033180.1.15",
          "form": "실물은 아직요!",
          "original_form": "실물은 아직요!",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033180.1.16",
          "form": "지인분이 사진찍어준거 봤는데 색깔도 잘뽑았더라구요 ㅠㅠ",
          "original_form": "지인분이 사진찍어준거 봤는데 색깔도 잘뽑았더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033180.1.17",
          "form": "맞아요 저는 사전예약기간때 보러갔는데",
          "original_form": "맞아요 저는 사전예약기간때 보러갔는데",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033180.1.18",
          "form": "크림이랑 라벤더도 예쁘고 그린도 예쁘더라구요",
          "original_form": "크림이랑 라벤더도 예쁘고 그린도 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033180.1.19",
          "form": "근데 블랙도 무광이라 예뻐보이고 ㅋㄱㄱ",
          "original_form": "근데 블랙도 무광이라 예뻐보이고 ㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033180.1.20",
          "form": "라벤더랑 크림 진짜 취향이었어요 ㅠ",
          "original_form": "라벤더랑 크림 진짜 취향이었어요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033180.1.21",
          "form": "맞아요 사람들이 그 두개를 많이 사더라구요",
          "original_form": "맞아요 사람들이 그 두개를 많이 사더라구요",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033180.1.22",
          "form": "저는 못샀지만 제 친구는 라벤더로 바꿨거든요",
          "original_form": "저는 못샀지만 제 친구는 라벤더로 바꿨거든요",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100033180.1.23",
          "form": "예쁘더라구요...",
          "original_form": "예쁘더라구요...",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100033180.1.24",
          "form": "크 맞아요.. 플립 3 ㅜㅜ",
          "original_form": "크 맞아요.. 플립 3 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033180.1.25",
          "form": "이번에 같이 나온 워치도 탐나더라구요",
          "original_form": "이번에 같이 나온 워치도 탐나더라구요",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033180.1.26",
          "form": "인바디 기능이 들어있어서",
          "original_form": "인바디 기능이 들어있어서",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
