      "utterance": [
        {
          "id": "MDRW2100026838.1.1",
          "form": "암튼 노래가 재밌네여",
          "original_form": "암튼 노래가 재밌네여",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026838.1.2",
          "form": "덕질하기죠은",
          "original_form": "덕질하기죠은",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026838.1.3",
          "form": "ㅋㅋㅋㄱ사실 이전에 전혀본적이없어서",
          "original_form": "ㅋㅋㅋㄱ사실 이전에 전혀본적이없어서",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026838.1.4",
          "form": "나만 신선한가봐 코쓱",
          "original_form": "나만 신선한가봐 코쓱",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026838.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아니 근데 이렇게까지 본격적으로",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아니 근데 이렇게까지 본격적으로",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026838.1.6",
          "form": "한국쪽 설화 건드리면서",
          "original_form": "한국쪽 설화 건드리면서",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026838.1.7",
          "form": "창법까지 딱 떨어지게 어울리는 가수가",
          "original_form": "창법까지 딱 떨어지게 어울리는 가수가",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026838.1.8",
          "form": "흔치는 않지...",
          "original_form": "흔치는 않지...",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026838.1.9",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026838.1.10",
          "form": "아 저는 사실 일본이",
          "original_form": "아 저는 사실 일본이",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026838.1.11",
          "form": "토지신얘기하고",
          "original_form": "토지신얘기하고",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026838.1.12",
          "form": "토속신앙 토속문화 이런거 잘살린거보면",
          "original_form": "토속신앙 토속문화 이런거 잘살린거보면",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026838.1.13",
          "form": "우리나라민속학도 재밌을거같단생각ㅇ르했거든요",
          "original_form": "우리나라민속학도 재밌을거같단생각ㅇ르했거든요",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026838.1.14",
          "form": "그게컨텐츠로 나올줄",
          "original_form": "그게컨텐츠로 나올줄",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026838.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026838.1.16",
          "form": "그런거 좋아하는데..",
          "original_form": "그런거 좋아하는데..",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026838.1.17",
          "form": "그래서 신과함께 같은 세계관 좋아하는거고",
          "original_form": "그래서 신과함께 같은 세계관 좋아하는거고",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100026838.1.18",
          "form": "근데 메이저하게 사랑받을만한 컨텐츠는.. 신함 이후로 잘 안 나오는 듯ㅠㅠ",
          "original_form": "근데 메이저하게 사랑받을만한 컨텐츠는.. 신함 이후로 잘 안 나오는 듯ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026838.1.19",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026838.1.20",
          "form": "근데 신함도 사실 잘만들면 좋았을건데",
          "original_form": "근데 신함도 사실 잘만들면 좋았을건데",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026838.1.21",
          "form": "흥행잘되니까 이상한 장면 넣고 그래서",
          "original_form": "흥행잘되니까 이상한 장면 넣고 그래서",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026838.1.22",
