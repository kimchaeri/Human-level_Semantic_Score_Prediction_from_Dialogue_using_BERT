      "utterance": [
        {
          "id": "MDRW2100046158.1.1",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046158.1.2",
          "form": "매운탕같은건데",
          "original_form": "매운탕같은건데",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046158.1.3",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046158.1.4",
          "form": "민물고기",
          "original_form": "민물고기",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046158.1.5",
          "form": "처음 들어봐요 ㅋㅋㅋ",
          "original_form": "처음 들어봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046158.1.6",
          "form": "추어탕 매기매운탕 이런거요",
          "original_form": "추어탕 매기매운탕 이런거요",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046158.1.7",
          "form": "전 술은 안 마시는데",
          "original_form": "전 술은 안 마시는데",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046158.1.8",
          "form": "얼큰한 걸 좋아해서",
          "original_form": "얼큰한 걸 좋아해서",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046158.1.9",
          "form": "매운탕이나 어탕을 좋아하거든요",
          "original_form": "매운탕이나 어탕을 좋아하거든요",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046158.1.10",
          "form": "오 저도 안마셔요 ㅋㅋ",
          "original_form": "오 저도 안마셔요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046158.1.11",
          "form": "완전 술안주네용",
          "original_form": "완전 술안주네용",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046158.1.12",
          "form": "ㅎㅎ 그렇죠 닭발도 그렇고",
          "original_form": "ㅎㅎ 그렇죠 닭발도 그렇고",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046158.1.13",
          "form": "술안주는 좋아하고",
          "original_form": "술안주는 좋아하고",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046158.1.14",
          "form": "술은 안마시죠 ㅎㅎ",
          "original_form": "술은 안마시죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046158.1.15",
          "form": "호프집",
          "original_form": "호프집",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046158.1.16",
          "form": "가면 안주만 엄청 먹어요 ㅋㅋ",
          "original_form": "가면 안주만 엄청 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.17",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.18",
          "form": "안주만 주구장창 먹고 오죠",
          "original_form": "안주만 주구장창 먹고 오죠",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.19",
          "form": "회식하면",
          "original_form": "회식하면",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.20",
          "form": "안주",
          "original_form": "안주",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.21",
          "form": "저는 술을 안 마셔서",
          "original_form": "저는 술을 안 마셔서",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.22",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.23",
          "form": "다 집에 태워다 주고",
          "original_form": "다 집에 태워다 주고",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.24",
          "form": "회식하면 술 강요 안하나요?",
          "original_form": "회식하면 술 강요 안하나요?",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.25",
          "form": "집에 와요",
          "original_form": "집에 와요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046158.1.26",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046158.1.27",
          "form": "에 저희는",
          "original_form": "에 저희는",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046158.1.28",
          "form": "술은 강요하지 않아서",
          "original_form": "술은 강요하지 않아서",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046158.1.29",
          "form": "마시고 싶은 사람만 마셔요",
          "original_form": "마시고 싶은 사람만 마셔요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046158.1.30",
          "form": "우아 너무 좋은 회사네용",
          "original_form": "우아 너무 좋은 회사네용",
          "speaker_id": "1",
          "time": "20210908 10:53"
        }
      ]
