      "utterance": [
        {
          "id": "MDRW2100009627.1.1",
          "form": "오 여행 좋아하시나요",
          "original_form": "오 여행 좋아하시나요",
          "speaker_id": "2",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009627.1.2",
          "form": "저 놀라다니는 거 좋아합니다",
          "original_form": "저 놀라다니는 거 좋아합니다",
          "speaker_id": "1",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009627.1.3",
          "form": "아무래도 콘서트가 만들어준 취미이죠",
          "original_form": "아무래도 콘서트가 만들어준 취미이죠",
          "speaker_id": "1",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009627.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009627.1.5",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:23"
        },
        {
          "id": "MDRW2100009627.1.6",
          "form": "콘섴트때문에 여기까지 가봤다..!",
          "original_form": "콘섴트때문에 여기까지 가봤다..!",
          "speaker_id": "2",
          "time": "20210823 02:23"
        },
        {
          "id": "MDRW2100009627.1.7",
          "form": "하는거 있으신가요",
          "original_form": "하는거 있으신가요",
          "speaker_id": "2",
          "time": "20210823 02:23"
        },
        {
          "id": "MDRW2100009627.1.8",
          "form": "저는 부산 콘서트때문에 첨내려갔었어요",
          "original_form": "저는 부산 콘서트때문에 첨내려갔었어요",
          "speaker_id": "2",
          "time": "20210823 02:24"
        },
        {
          "id": "MDRW2100009627.1.9",
          "form": "저...",
          "original_form": "저...",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.10",
          "form": "서울까지",
          "original_form": "서울까지",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.11",
          "form": "무궁화호타고",
          "original_form": "무궁화호타고",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.12",
          "form": "가봤습니다",
          "original_form": "가봤습니다",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.13",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.14",
          "form": "그.잠실이요?!",
          "original_form": "그.잠실이요?!",
          "speaker_id": "2",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.15",
          "form": "무려 장장 6시간",
          "original_form": "무려 장장 6시간",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.16",
          "form": "헐헐",
          "original_form": "헐헐",
          "speaker_id": "2",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.17",
          "form": "네 잠실 맞아여",
          "original_form": "네 잠실 맞아여",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.19",
          "form": "아니 6시간이면 호텔 잡고 오셔야겠는데요 진짜",
          "original_form": "아니 6시간이면 호텔 잡고 오셔야겠는데요 진짜",
          "speaker_id": "2",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009627.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 끈데 그거",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 끈데 그거",
          "speaker_id": "1",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009627.1.22",
          "form": "당일치기라",
          "original_form": "당일치기라",
          "speaker_id": "1",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009627.1.23",
          "form": "내려오는 기차역에서",
          "original_form": "내려오는 기차역에서",
          "speaker_id": "1",
          "time": "20210823 02:28"
        },
        {
          "id": "MDRW2100009627.1.24",
          "form": "노숙하고 그랬어요...",
          "original_form": "노숙하고 그랬어요...",
          "speaker_id": "1",
          "time": "20210823 02:28"
        },
        {
          "id": "MDRW2100009627.1.25",
          "form": "헐 찐사랑이셨군요",
          "original_form": "헐 찐사랑이셨군요",
          "speaker_id": "2",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009627.1.26",
          "form": "아니 지금도 진행중인",
          "original_form": "아니 지금도 진행중인",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009627.1.27",
          "form": "그럼 엥간한데는 그냥 걱정없이 다니시겠어요",
          "original_form": "그럼 엥간한데는 그냥 걱정없이 다니시겠어요",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009627.1.28",
          "form": "이동시간 3시간? = 갈만하다",
          "original_form": "이동시간 3시간? = 갈만하다",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009627.1.29",
          "form": "아 콘서트는",
          "original_form": "아 콘서트는",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.30",
          "form": "버스대절만 있으면",
          "original_form": "버스대절만 있으면",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.31",
          "form": "넙죽넙죽 가는 편이고",
          "original_form": "넙죽넙죽 가는 편이고",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.32",
          "form": "아헙",
          "original_form": "아헙",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.34",
          "form": "여행은... 자차가 없어서",
          "original_form": "여행은... 자차가 없어서",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.35",
          "form": "대중교통 닿는 곳만",
          "original_form": "대중교통 닿는 곳만",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.36",
          "form": "그 저 궁금한게",
          "original_form": "그 저 궁금한게",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.37",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.38",
          "form": "버스대절해서 오실 때",
          "original_form": "버스대절해서 오실 때",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009627.1.39",
          "form": "그 안에서도 막 왁자지껄 하나요?",
          "original_form": "그 안에서도 막 왁자지껄 하나요?",
          "speaker_id": "2",
          "time": "20210823 02:32"
        },
        {
          "id": "MDRW2100009627.1.40",
          "form": "역시 여행은..자차가 있어야..",
          "original_form": "역시 여행은..자차가 있어야..",
          "speaker_id": "2",
          "time": "20210823 02:32"
        },
        {
          "id": "MDRW2100009627.1.41",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009627.1.42",
          "form": "잡니다....",
          "original_form": "잡니다....",
          "speaker_id": "1",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009627.1.43",
          "form": "옆사람이랑 말 트고 얘기좀 하다가",
          "original_form": "옆사람이랑 말 트고 얘기좀 하다가",
          "speaker_id": "1",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009627.1.44",
          "form": "자다가",
          "original_form": "자다가",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009627.1.45",
          "form": "제2롯데월드 보이면",
          "original_form": "제2롯데월드 보이면",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009627.1.46",
          "form": "소리질러요",
          "original_form": "소리질러요",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009627.1.47",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009627.1.48",
          "form": "아 롯데월드부터 시작이군요",
          "original_form": "아 롯데월드부터 시작이군요",
