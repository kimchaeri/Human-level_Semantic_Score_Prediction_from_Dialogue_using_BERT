      "utterance": [
        {
          "id": "MDRW2100037607.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 13:19"
        },
        {
          "id": "MDRW2100037607.1.2",
          "form": "최근 잘샀다 생각하시는 꿀템있으신가요",
          "original_form": "최근 잘샀다 생각하시는 꿀템있으신가요",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100037607.1.3",
          "form": "저는 전기모기채요!! ㅋㅋ",
          "original_form": "저는 전기모기채요!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037607.1.4",
          "form": "첨엔 무서웠는데 잘산템입니다ㅠㅠ",
          "original_form": "첨엔 무서웠는데 잘산템입니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037607.1.5",
          "form": "오 다이소에서 사셨나요?",
          "original_form": "오 다이소에서 사셨나요?",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037607.1.6",
          "form": "배송료없이 생활잡화사면참좋은데 ㅋㅋ",
          "original_form": "배송료없이 생활잡화사면참좋은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037607.1.7",
          "form": "아 저는 인터넷이요 한 3만원짜리샀어요ㅠ",
          "original_form": "아 저는 인터넷이요 한 3만원짜리샀어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037607.1.8",
          "form": "다이소에 5천원짜리 잘되는거있던데",
          "original_form": "다이소에 5천원짜리 잘되는거있던데",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037607.1.9",
          "form": "3만원인 만큼 튼튼할거에요!",
          "original_form": "3만원인 만큼 튼튼할거에요!",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100037607.1.10",
          "form": "그래도 많이 사냥성공하셨군요 ㅋ기",
          "original_form": "그래도 많이 사냥성공하셨군요 ㅋ기",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100037607.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100037607.1.12",
          "form": "맞아요 ㅋㅋㅋ조카도 신생아고 강아지도 모기물리면 안돼서",
          "original_form": "맞아요 ㅋㅋㅋ조카도 신생아고 강아지도 모기물리면 안돼서",
          "speaker_id": "2",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100037607.1.13",
          "form": "잘산템 뭐 있으세용",
          "original_form": "잘산템 뭐 있으세용",
          "speaker_id": "2",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100037607.1.14",
          "form": "저는 최근 홈쇼핑에서 장어샀어요",
          "original_form": "저는 최근 홈쇼핑에서 장어샀어요",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100037607.1.15",
          "form": "가격이 44900원이었던거같아요 49800원인가? 하여간 고정도 ㅋㅋ",
          "original_form": "가격이 44900원이었던거같아요 49800원인가? 하여간 고정도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100037607.1.16",
          "form": "우와 장어 구이 하시나요",
          "original_form": "우와 장어 구이 하시나요",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100037607.1.17",
          "form": "안그래도 가게가서먹음 비싼대",
          "original_form": "안그래도 가게가서먹음 비싼대",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037607.1.18",
          "form": "맘껏 먹어보자면서 10마리 세트 2개질렀어요 ㅋㅋ",
          "original_form": "맘껏 먹어보자면서 10마리 세트 2개질렀어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037607.1.19",
          "form": "오 그렇구나 전 장어 잘 못먹어요ㅠ가시땨문에ㅜㅠ",
          "original_form": "오 그렇구나 전 장어 잘 못먹어요ㅠ가시땨문에ㅜㅠ",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037607.1.20",
          "form": "제가 산거 다 손질되서 왔어요ㅋㅋ",
          "original_form": "제가 산거 다 손질되서 왔어요ㅋㅋ",
