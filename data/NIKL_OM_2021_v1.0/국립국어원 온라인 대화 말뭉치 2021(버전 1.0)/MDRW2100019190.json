      "utterance": [
        {
          "id": "MDRW2100019190.1.1",
          "form": "도깨비 아시나요?",
          "original_form": "도깨비 아시나요?",
          "speaker_id": "1",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019190.1.2",
          "form": "네! 도깨비 재밌게봤어요 ㅋㅋ",
          "original_form": "네! 도깨비 재밌게봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100019190.1.3",
          "form": "아뇨 이번에 나오는 메타버스 게임입니다ㅋ",
          "original_form": "아뇨 이번에 나오는 메타버스 게임입니다ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100019190.1.4",
          "form": "아 그렇구나.. 죄송해요 이방면으론 제가 좀 무지해서요 ㅋㅋ",
          "original_form": "아 그렇구나.. 죄송해요 이방면으론 제가 좀 무지해서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100019190.1.5",
          "form": "저도 안해봤어요ㅋ",
          "original_form": "저도 안해봤어요ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019190.1.6",
          "form": "네이버 제패토랑 로블록스 정도 있다는 것만 알아요ㅋ",
          "original_form": "네이버 제패토랑 로블록스 정도 있다는 것만 알아요ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019190.1.7",
          "form": "아 오 제패토! 이거 대학교에서 과제제출할때 써본적있어요ㅋㅋㅋ",
          "original_form": "아 오 제패토! 이거 대학교에서 과제제출할때 써본적있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100019190.1.8",
          "form": "로블록스는 대략어떤건가요?",
          "original_form": "로블록스는 대략어떤건가요?",
          "speaker_id": "2",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019190.1.9",
          "form": "헛 미국 초딩들이한다던데",
          "original_form": "헛 미국 초딩들이한다던데",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019190.1.10",
          "form": "내용은 몰라요ㅋ",
          "original_form": "내용은 몰라요ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019190.1.11",
          "form": "아 그렇군요 ㅋㅋ",
          "original_form": "아 그렇군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019190.1.12",
          "form": "도깨비도 엄청 리얼하대요ㅋ",
          "original_form": "도깨비도 엄청 리얼하대요ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019190.1.13",
          "form": "위에서 도깨비 드라마말씀하시는건줄알았어요ㅋㅋㅋ 그건뭔가요?",
          "original_form": "위에서 도깨비 드라마말씀하시는건줄알았어요ㅋㅋㅋ 그건뭔가요?",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019190.1.14",
          "form": "이거는 메타버스 게임이에요ㅋ",
          "original_form": "이거는 메타버스 게임이에요ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019190.1.15",
          "form": "곧 나온대요ㅋ",
          "original_form": "곧 나온대요ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019190.1.16",
          "form": "오!! 기회되면 도전해봐야겠어요 ㅋㅋ",
          "original_form": "오!! 기회되면 도전해봐야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019190.1.17",
