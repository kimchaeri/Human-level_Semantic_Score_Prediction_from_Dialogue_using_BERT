        {
          "id": "MDRW2100020799.1.3",
          "form": "뭐샀어",
          "original_form": "뭐샀어",
          "speaker_id": "1",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020799.1.4",
          "form": "난 스콘",
          "original_form": "난 스콘",
          "speaker_id": "1",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020799.1.5",
          "form": "스콘 은비스 브래드??",
          "original_form": "스콘 은비스 브래드??",
          "speaker_id": "2",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020799.1.6",
          "form": "응 은비",
          "original_form": "응 은비",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020799.1.7",
          "form": "7개면 박스 포장이드만",
          "original_form": "7개면 박스 포장이드만",
          "speaker_id": "2",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020799.1.8",
          "form": "바로그거산거",
          "original_form": "바로그거산거",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020799.1.9",
          "form": "뭐뭐 샀어?",
          "original_form": "뭐뭐 샀어?",
          "speaker_id": "2",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020799.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020799.1.11",
          "form": "시오사토 카라멜무화과 대파치즈 얼그레이..",
          "original_form": "시오사토 카라멜무화과 대파치즈 얼그레이..",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020799.1.12",
          "form": "등등",
          "original_form": "등등",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020799.1.13",
          "form": "얼그레이 나두 먹어보고 싶음",
          "original_form": "얼그레이 나두 먹어보고 싶음",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020799.1.14",
          "form": "조만간 뿌신다",
          "original_form": "조만간 뿌신다",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020799.1.15",
          "form": "대파치즈는 어떨까",
          "original_form": "대파치즈는 어떨까",
          "speaker_id": "2",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020799.1.16",
          "form": "짱맛일듯",
          "original_form": "짱맛일듯",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020799.1.17",
          "form": "애플크럼블도산듯",
          "original_form": "애플크럼블도산듯",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020799.1.18",
          "form": "그것도 안먹어봄 ㅋㅋㅋㅋ",
          "original_form": "그것도 안먹어봄 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020799.1.19",
          "form": "후기쪄드림",
          "original_form": "후기쪄드림",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020799.1.20",
          "form": "맛있음 짱겨울 말고",
          "original_form": "맛있음 짱겨울 말고",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020799.1.21",
          "form": "초초겨울쯤 놀러오세",
          "original_form": "초초겨울쯤 놀러오세",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020799.1.22",
          "form": "조앗서",
          "original_form": "조앗서",
          "speaker_id": "1",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020799.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 후후 내입이 정확해야 할텐데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 후후 내입이 정확해야 할텐데",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020799.1.24",
          "form": "추천인으로써 걱정쓰 ㅋㅋㅋㅋ",
          "original_form": "추천인으로써 걱정쓰 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020799.1.25",
