      "utterance": [
        {
          "id": "MDRW2100044874.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ이런 아직 감이 안 잡혀요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이런 아직 감이 안 잡혀요",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044874.1.2",
          "form": "제가 맞추고 말겠어요",
          "original_form": "제가 맞추고 말겠어요",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044874.1.3",
          "form": "굽는 음식인가요?",
          "original_form": "굽는 음식인가요?",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044874.1.4",
          "form": "흠 구울수도있고 삶을수도 있죠!",
          "original_form": "흠 구울수도있고 삶을수도 있죠!",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044874.1.5",
          "form": "재밌네요 ㅋㅋ",
          "original_form": "재밌네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044874.1.6",
          "form": "으으으윽 이럴수가",
          "original_form": "으으으윽 이럴수가",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044874.1.7",
          "form": "고기가 들어가나요?",
          "original_form": "고기가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044874.1.8",
          "form": "헉 ㅋㅋ고기입니다   .. 종류는 ?!",
          "original_form": "헉 ㅋㅋ고기입니다   .. 종류는 ?!",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044874.1.9",
          "form": "닭고기인가요?!",
          "original_form": "닭고기인가요?!",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044874.1.10",
          "form": "아니죠 !!",
          "original_form": "아니죠 !!",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044874.1.11",
          "form": "그렇담 오리고기인가요! 기름지다면!",
          "original_form": "그렇담 오리고기인가요! 기름지다면!",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044874.1.12",
          "form": "아니 ㅠㅠ 조금더 쉽게 생각하세용",
          "original_form": "아니 ㅠㅠ 조금더 쉽게 생각하세용",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044874.1.13",
          "form": "기름 하면 소고기!",
          "original_form": "기름 하면 소고기!",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044874.1.14",
          "form": "소고기 들어가나요?",
          "original_form": "소고기 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044874.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044874.1.16",
          "form": "아니 ㅠㅠ 입맛이 다른거같아요 ...",
          "original_form": "아니 ㅠㅠ 입맛이 다른거같아요 ...",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044874.1.17",
          "form": "전 소고기가 기름지지않아요 ㅠ",
          "original_form": "전 소고기가 기름지지않아요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044874.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이럴수가 다 틀리다니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이럴수가 다 틀리다니",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044874.1.19",
          "form": "혹시 삼겹살?",
          "original_form": "혹시 삼겹살?",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044874.1.20",
          "form": "정답입니다ㅋㅋㅋㅋㅋ",
          "original_form": "정답입니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044874.1.21",
          "form": "한번에 맞추실줄 ..",
          "original_form": "한번에 맞추실줄 ..",
