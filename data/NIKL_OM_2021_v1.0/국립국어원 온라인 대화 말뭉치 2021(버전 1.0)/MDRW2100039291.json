        },
        {
          "id": "MDRW2100039291.1.2",
          "form": "난 디즈니씨",
          "original_form": "난 디즈니씨",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039291.1.3",
          "form": "왜우리나라 안생겨..? 넘행",
          "original_form": "왜우리나라 안생겨..? 넘행",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039291.1.4",
          "form": "거긴 좀",
          "original_form": "거긴 좀",
          "speaker_id": "1",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039291.1.5",
          "form": "재미없었오",
          "original_form": "재미없었오",
          "speaker_id": "1",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039291.1.6",
          "form": "모야 그래?",
          "original_form": "모야 그래?",
          "speaker_id": "2",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039291.1.7",
          "form": "누가 더 재밌다던데",
          "original_form": "누가 더 재밌다던데",
          "speaker_id": "2",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039291.1.8",
          "form": "나랑같이안가서그랴",
          "original_form": "나랑같이안가서그랴",
          "speaker_id": "2",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039291.1.9",
          "form": "아냐 흥미가부족",
          "original_form": "아냐 흥미가부족",
          "speaker_id": "1",
          "time": "20210903 15:48"
        },
        {
          "id": "MDRW2100039291.1.10",
          "form": "넓기만하고",
          "original_form": "넓기만하고",
          "speaker_id": "1",
          "time": "20210903 15:48"
        },
        {
          "id": "MDRW2100039291.1.11",
          "form": "ㅎ디즈니 왜 볼거많지않나..",
          "original_form": "ㅎ디즈니 왜 볼거많지않나..",
          "speaker_id": "2",
          "time": "20210903 15:48"
        },
        {
          "id": "MDRW2100039291.1.12",
          "form": "해리포터 같이 덕질하는게없어서인가",
          "original_form": "해리포터 같이 덕질하는게없어서인가",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039291.1.13",
          "form": "어맞아 그냥 아기자기한데",
          "original_form": "어맞아 그냥 아기자기한데",
          "speaker_id": "1",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039291.1.14",
          "form": "우와아 는 없어",
          "original_form": "우와아 는 없어",
          "speaker_id": "1",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039291.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ웃기다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ웃기다",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039291.1.16",
          "form": "la나 유럽이런데도 가고파",
          "original_form": "la나 유럽이런데도 가고파",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039291.1.17",
          "form": "잼날듯",
          "original_form": "잼날듯",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039291.1.18",
          "form": "영국가게되면가자",
          "original_form": "영국가게되면가자",
          "speaker_id": "2",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039291.1.19",
          "form": "어나도",
          "original_form": "어나도",
          "speaker_id": "1",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039291.1.20",
          "form": "미국가야하나",
          "original_form": "미국가야하나",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039291.1.21",
          "form": "하루 24시간이 모자르게 놀아야겠지",
          "original_form": "하루 24시간이 모자르게 놀아야겠지",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039291.1.22",
          "form": "잼날거같아",
          "original_form": "잼날거같아",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039291.1.23",
          "form": "10일은가야할듯",
          "original_form": "10일은가야할듯",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039291.1.24",
          "form": "땅덩어리도 크고 미국은",
          "original_form": "땅덩어리도 크고 미국은",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039291.1.25",
          "form": "햄버거냠...",
          "original_form": "햄버거냠...",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039291.1.26",
          "form": "그러다 한인식당 뿌시고 올듯",
          "original_form": "그러다 한인식당 뿌시고 올듯",
          "speaker_id": "2",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100039291.1.27",
          "form": "미국이",
          "original_form": "미국이",
          "speaker_id": "1",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100039291.1.28",
