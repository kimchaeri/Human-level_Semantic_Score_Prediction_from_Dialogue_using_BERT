      "utterance": [
        {
          "id": "MDRW2100038182.1.1",
          "form": "쭈꾸미 먹었어요 오늘",
          "original_form": "쭈꾸미 먹었어요 오늘",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100038182.1.2",
          "form": "친구가 먹는대서 저도 땡기지 뭐에요",
          "original_form": "친구가 먹는대서 저도 땡기지 뭐에요",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100038182.1.3",
          "form": "오호~",
          "original_form": "오호~",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100038182.1.4",
          "form": "쭈꾸미최고에용",
          "original_form": "쭈꾸미최고에용",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100038182.1.5",
          "form": "하지만 인생 최고의쭈꾸미는 다시 맛볼수 없네요",
          "original_form": "하지만 인생 최고의쭈꾸미는 다시 맛볼수 없네요",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100038182.1.6",
          "form": "어머멋... 왜죠ㅠㅜ 인생최고의 쭈꾸미라니..!!",
          "original_form": "어머멋... 왜죠ㅠㅜ 인생최고의 쭈꾸미라니..!!",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100038182.1.7",
          "form": "아.... 그런맛집 사라지면.. 슬프죠.. ㅜㅜ",
          "original_form": "아.... 그런맛집 사라지면.. 슬프죠.. ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038182.1.8",
          "form": "저 오늘 하남쭈꾸미 먹는데",
          "original_form": "저 오늘 하남쭈꾸미 먹는데",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038182.1.9",
          "form": "엄마도 같이먹었는데 맛있대요",
          "original_form": "엄마도 같이먹었는데 맛있대요",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038182.1.10",
          "form": "오오 첨드셧나요 엄니?",
          "original_form": "오오 첨드셧나요 엄니?",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038182.1.11",
          "form": "얼마냐고 물어보고 막 ㅋㅋㅋ엄청 매워보였는데 적당히 맵고 맛있대요",
          "original_form": "얼마냐고 물어보고 막 ㅋㅋㅋ엄청 매워보였는데 적당히 맵고 맛있대요",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038182.1.12",
          "form": "넹 그동안 저 혼자만 먹었거든요",
          "original_form": "넹 그동안 저 혼자만 먹었거든요",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038182.1.13",
          "form": "왜혼자만드셧어요",
          "original_form": "왜혼자만드셧어요",
          "speaker_id": "2",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100038182.1.14",
          "form": "엄마는 제가 뭐 사오면 맨날 안먹는다고그래요 케잌도 그렇고",
          "original_form": "엄마는 제가 뭐 사오면 맨날 안먹는다고그래요 케잌도 그렇고",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038182.1.15",
          "form": "그래서 저혼자 먹었죠",
          "original_form": "그래서 저혼자 먹었죠",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038182.1.16",
          "form": "엄ㅁ머 먹는걸 안조아하시나요?",
          "original_form": "엄ㅁ머 먹는걸 안조아하시나요?",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038182.1.17",
          "form": "아뇨 걍 자기가 먹는거만 먹는 스탈..??",
          "original_form": "아뇨 걍 자기가 먹는거만 먹는 스탈..??",
          "speaker_id": "1",
          "time": "20210902 01:42"
        },
        {
          "id": "MDRW2100038182.1.18",
          "form": "무화과도 엄청 잘먹으면서 무화과케잌은 안먹어요",
          "original_form": "무화과도 엄청 잘먹으면서 무화과케잌은 안먹어요",
          "speaker_id": "1",
          "time": "20210902 01:43"
        },
        {
          "id": "MDRW2100038182.1.19",
          "form": "근데 티비에서 무화과케잌의 장점 이런거 만약 나온다먄 바로 드실듯여~~",
          "original_form": "근데 티비에서 무화과케잌의 장점 이런거 만약 나온다먄 바로 드실듯여~~",
          "speaker_id": "1",
          "time": "20210902 01:43"
        },
        {
          "id": "MDRW2100038182.1.20",
          "form": "티비아니고 이젠 유툽도 추가여 ㅋㅋㅋㅋㅋ",
          "original_form": "티비아니고 이젠 유툽도 추가여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:43"
        },
        {
          "id": "MDRW2100038182.1.21",
          "form": "어머니 왜 안드실까~",
          "original_form": "어머니 왜 안드실까~",
          "speaker_id": "2",
          "time": "20210902 01:54"
        },
        {
          "id": "MDRW2100038182.1.22",
          "form": "엌ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:54"
        },
        {
          "id": "MDRW2100038182.1.23",
          "form": "건강중시!",
          "original_form": "건강중시!",
          "speaker_id": "2",
          "time": "20210902 01:54"
        },
        {
          "id": "MDRW2100038182.1.24",
          "form": "저번엔 커피 위에 거품을 다 걷어내고 드시는거에여",
          "original_form": "저번엔 커피 위에 거품을 다 걷어내고 드시는거에여",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100038182.1.25",
          "form": "거기에 지방이많다나",
          "original_form": "거기에 지방이많다나",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100038182.1.26",
          "form": "그러다가 갑자기 티비에서 그 거품이 진짜 몸에좋은거라고 나왔더니",
          "original_form": "그러다가 갑자기 티비에서 그 거품이 진짜 몸에좋은거라고 나왔더니",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100038182.1.27",
          "form": "그동안 거품 걷어내고 먹었던 거 아깝다고 ㅡㅡ",
          "original_form": "그동안 거품 걷어내고 먹었던 거 아깝다고 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100038182.1.28",
          "form": "제가 그 거품이 맛있는건데 그걸 왜버리냐고 했을땐 귓등으로도 안듣더니",
          "original_form": "제가 그 거품이 맛있는건데 그걸 왜버리냐고 했을땐 귓등으로도 안듣더니",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100038182.1.29",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100038182.1.30",
          "form": "딸 말을 들어쥬세요 엄니",
          "original_form": "딸 말을 들어쥬세요 엄니",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100038182.1.31",
          "form": "근데거품이 몸에 좋아요 ? ㅋㅋ",
          "original_form": "근데거품이 몸에 좋아요 ? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100038182.1.32",
          "form": "그렇다네요 저도 봤는데 그게 콜레스테롤에 좋댔나 그랬어요",
          "original_form": "그렇다네요 저도 봤는데 그게 콜레스테롤에 좋댔나 그랬어요",
          "speaker_id": "1",
          "time": "20210902 14:53"
        },
