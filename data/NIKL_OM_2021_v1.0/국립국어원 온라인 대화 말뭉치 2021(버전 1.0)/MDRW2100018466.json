      "utterance": [
        {
          "id": "MDRW2100018466.1.1",
          "form": "갤워치 어떤 점때문에 가장 사고싶으셨어요?",
          "original_form": "갤워치 어떤 점때문에 가장 사고싶으셨어요?",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018466.1.2",
          "form": "그 건강쪽으로 체크가 되더라고요?",
          "original_form": "그 건강쪽으로 체크가 되더라고요?",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.4",
          "form": "의외로 정확도가 높다그래서 놀랬어요",
          "original_form": "의외로 정확도가 높다그래서 놀랬어요",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.5",
          "form": "이번 인바디요?!",
          "original_form": "이번 인바디요?!",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.7",
          "form": "네네 마자요",
          "original_form": "네네 마자요",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.8",
          "form": "글고 그 수면체크도 할 수 있다더라고요",
          "original_form": "글고 그 수면체크도 할 수 있다더라고요",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.9",
          "form": "신기술에 매번 놀라요",
          "original_form": "신기술에 매번 놀라요",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.10",
          "form": "원래 워치종류 가지고 계신거 있으세여?!",
          "original_form": "원래 워치종류 가지고 계신거 있으세여?!",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.11",
          "form": "수면체크 그거 있으면 유용하던데",
          "original_form": "수면체크 그거 있으면 유용하던데",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018466.1.12",
          "form": "지금은 쓰는 거 없어요",
          "original_form": "지금은 쓰는 거 없어요",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018466.1.13",
          "form": "주변에서 추천만 ㅁ받구ㅎㅎ",
          "original_form": "주변에서 추천만 ㅁ받구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018466.1.14",
          "form": "앗 ㅎㅎ ㅎ저도요",
          "original_form": "앗 ㅎㅎ ㅎ저도요",
          "speaker_id": "1",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018466.1.15",
          "form": "만보기용으로 하나 있다가",
          "original_form": "만보기용으로 하나 있다가",
          "speaker_id": "1",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018466.1.16",
          "form": "근데 만보가 다른 어플이랑 연동이 안된다해서",
          "original_form": "근데 만보가 다른 어플이랑 연동이 안된다해서",
          "speaker_id": "1",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018466.1.17",
          "form": "그것때문에 망설이고있어욬ㅋㅋㅋ",
          "original_form": "그것때문에 망설이고있어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018466.1.18",
          "form": "아 그것도 고르는 데 중요하죠",
          "original_form": "아 그것도 고르는 데 중요하죠",
          "speaker_id": "2",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018466.1.19",
          "form": "어플만 자꾸 늘어나면 곤란하니깐",
          "original_form": "어플만 자꾸 늘어나면 곤란하니깐",
          "speaker_id": "2",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018466.1.20",
          "form": "기록 체크도 안되구",
          "original_form": "기록 체크도 안되구",
          "speaker_id": "2",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018466.1.21",
          "form": "맞아유ㅠㅠㅠ",
          "original_form": "맞아유ㅠㅠㅠ",
