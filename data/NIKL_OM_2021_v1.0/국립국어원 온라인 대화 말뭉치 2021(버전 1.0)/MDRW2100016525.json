      "utterance": [
        {
          "id": "MDRW2100016525.1.1",
          "form": "결혼하셨어요??",
          "original_form": "결혼하셨어요??",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100016525.1.2",
          "form": "아뇽 name1님은요",
          "original_form": "아뇽 &name1&님은요",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100016525.1.3",
          "form": "저도 미혼이에요ㅎㅎ",
          "original_form": "저도 미혼이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100016525.1.4",
          "form": "생각있으신가요",
          "original_form": "생각있으신가요",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100016525.1.5",
          "form": "연애는 진행중이세요?",
          "original_form": "연애는 진행중이세요?",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100016525.1.6",
          "form": "전 아직ㅋㅋ",
          "original_form": "전 아직ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100016525.1.7",
          "form": "연애...뭐하는둥마는둥 ㅜㅋㅋㅋㅋ",
          "original_form": "연애...뭐하는둥마는둥 ㅜㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100016525.1.8",
          "form": "아직 어리세요?",
          "original_form": "아직 어리세요?",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100016525.1.9",
          "form": "어후.. 많아요ㅋㅋ",
          "original_form": "어후.. 많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100016525.1.10",
          "form": "ㅋㅋㅋ 걍 생각이 없으신거구낭",
          "original_form": "ㅋㅋㅋ 걍 생각이 없으신거구낭",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100016525.1.11",
          "form": "요즘 다그렇죠 ㅠㅠ 힘들죠",
          "original_form": "요즘 다그렇죠 ㅠㅠ 힘들죠",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100016525.1.12",
          "form": "ㅋㅋ연애를 하는둥마는둥 하시는거 어떤거예요? ㅋㅋ",
          "original_form": "ㅋㅋ연애를 하는둥마는둥 하시는거 어떤거예요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100016525.1.13",
          "form": "진짜 좋은 인연 찾기가 너무 어렵고ㅎㅎ",
          "original_form": "진짜 좋은 인연 찾기가 너무 어렵고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100016525.1.14",
          "form": "위태롭습니다 ㅋㅋㅋㅋ",
          "original_form": "위태롭습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100016525.1.15",
          "form": "name1님은 연애 하세요?",
          "original_form": "&name1&님은 연애 하세요?",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100016525.1.16",
          "form": "위태롭다니ㅜ 뭐든 name2님이 행복한 쪽으로 해결되길 바랄게요!!!",
          "original_form": "위태롭다니ㅜ 뭐든 &name2&님이 행복한 쪽으로 해결되길 바랄게요!!!",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100016525.1.17",
          "form": "전 아뇨ㅋㅋ",
          "original_form": "전 아뇨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100016525.1.18",
          "form": "넹 ㅋㅋㅋ 연애할 사람이 없으신가요",
          "original_form": "넹 ㅋㅋㅋ 연애할 사람이 없으신가요",
