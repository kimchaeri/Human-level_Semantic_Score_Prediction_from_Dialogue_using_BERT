      "utterance": [
        {
          "id": "MDRW2100030278.1.1",
          "form": "초보시군요",
          "original_form": "초보시군요",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030278.1.2",
          "form": "실은 저 소주 싫어합니다",
          "original_form": "실은 저 소주 싫어합니다",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030278.1.3",
          "form": "거짓말하지마세요",
          "original_form": "거짓말하지마세요",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030278.1.4",
          "form": "지금 채팅창에",
          "original_form": "지금 채팅창에",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030278.1.5",
          "form": "알콜 냄새 나요",
          "original_form": "알콜 냄새 나요",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030278.1.6",
          "form": "진심이에요",
          "original_form": "진심이에요",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030278.1.7",
          "form": "그저 해산물 호를 너무너무 사랑하는데",
          "original_form": "그저 해산물 호를 너무너무 사랑하는데",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030278.1.8",
          "form": "해산물에는 소주라ㅜ 안주때문에 소주 고르는 거예요!",
          "original_form": "해산물에는 소주라ㅜ 안주때문에 소주 고르는 거예요!",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030278.1.9",
          "form": "거짓말하지마세요",
          "original_form": "거짓말하지마세요",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030278.1.10",
          "form": "소주 향도 맛도 시름!",
          "original_form": "소주 향도 맛도 시름!",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030278.1.11",
          "form": "지금 오타 막나죠",
          "original_form": "지금 오타 막나죠",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030278.1.12",
          "form": "근데 멍게랑 소주 먹음",
          "original_form": "근데 멍게랑 소주 먹음",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030278.1.13",
          "form": "소주가 달아집니다",
          "original_form": "소주가 달아집니다",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030278.1.14",
          "form": "그러다가 취객 되는 겁니다",
          "original_form": "그러다가 취객 되는 겁니다",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030278.1.15",
          "form": "왜 이러심",
          "original_form": "왜 이러심",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030278.1.16",
          "form": "전 자기관리 철저한 차도녀입니다",
          "original_form": "전 자기관리 철저한 차도녀입니다",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030278.1.17",
          "form": "아 일끝났다",
          "original_form": "아 일끝났다",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030278.1.18",
          "form": "배고픈데요",
          "original_form": "배고픈데요",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030278.1.19",
          "form": "멍게 시켜 드세요!!!",
          "original_form": "멍게 시켜 드세요!!!",
