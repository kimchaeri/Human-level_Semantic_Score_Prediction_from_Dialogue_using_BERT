      "utterance": [
        {
          "id": "MDRW2100014161.1.1",
          "form": "상호간의 존중",
          "original_form": "상호간의 존중",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014161.1.2",
          "form": "언니분도 좋은짝 만나시거나",
          "original_form": "언니분도 좋은짝 만나시거나",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014161.1.3",
          "form": "나 빼고 다 존중하는 듯",
          "original_form": "나 빼고 다 존중하는 듯",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014161.1.4",
          "form": "좋은 부동산 만나서 혼자 사실 수 있으시길",
          "original_form": "좋은 부동산 만나서 혼자 사실 수 있으시길",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014161.1.5",
          "form": "둘 중 하나다",
          "original_form": "둘 중 하나다",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014161.1.6",
          "form": "반려인이냐 반려 부동산이냐",
          "original_form": "반려인이냐 반려 부동산이냐",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014161.1.7",
          "form": "그럴 수 있을까..",
          "original_form": "그럴 수 있을까..",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014161.1.8",
          "form": "게을러가지고",
          "original_form": "게을러가지고",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.9",
          "form": "맨날 누워있던데",
          "original_form": "맨날 누워있던데",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.10",
          "form": "늙다보면 동생 챙기지 않을까요",
          "original_form": "늙다보면 동생 챙기지 않을까요",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.11",
          "form": "ㅇㄴ 왜 그려지지 눈앞에",
          "original_form": "ㅇㄴ 왜 그려지지 눈앞에",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.12",
          "form": "시간이 지나면 또 모르죠",
          "original_form": "시간이 지나면 또 모르죠",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.13",
          "form": "오빠분도 자주 그러시나봐요",
          "original_form": "오빠분도 자주 그러시나봐요",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.14",
          "form": "누워서 손 하나 까딱을 안해",
          "original_form": "누워서 손 하나 까딱을 안해",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.15",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.16",
          "form": "손까딱할 때?",
          "original_form": "손까딱할 때?",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.17",
          "form": "겜할때뿐",
          "original_form": "겜할때뿐",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.18",
          "form": "침대와 혼연일체수준",
          "original_form": "침대와 혼연일체수준",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.19",
          "form": "아 ㅇㅈ",
          "original_form": "아 ㅇㅈ",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014161.1.20",
          "form": "열받아서 블루투스 스위치 사다줬어요",
          "original_form": "열받아서 블루투스 스위치 사다줬어요",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014161.1.21",
          "form": "불끄라고 부르지말라고",
          "original_form": "불끄라고 부르지말라고",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014161.1.22",
          "form": "ㅋㅋㅋㅋㅋ 그랬더니 좋아하던가요",
          "original_form": "ㅋㅋㅋㅋㅋ 그랬더니 좋아하던가요",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014161.1.23",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014161.1.24",
          "form": "절 계속 부르더라고요",
          "original_form": "절 계속 부르더라고요",
          "speaker_id": "2",
          "time": "20210825 17:36"
        }
      ]
