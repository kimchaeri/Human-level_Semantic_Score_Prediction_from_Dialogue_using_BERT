      "utterance": [
        {
          "id": "MDRW2100038075.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038075.1.2",
          "form": "홈베이킹을 해보셨나요?",
          "original_form": "홈베이킹을 해보셨나요?",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038075.1.3",
          "form": "앗 넹넹",
          "original_form": "앗 넹넹",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038075.1.4",
          "form": "쉬운거! ㅋㅋ",
          "original_form": "쉬운거! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038075.1.5",
          "form": "어떤건지 궁금해요",
          "original_form": "어떤건지 궁금해요",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038075.1.6",
          "form": "전 수플레 케이크를 해봤는데 딱딱하게 나왔어욬",
          "original_form": "전 수플레 케이크를 해봤는데 딱딱하게 나왔어욬",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038075.1.7",
          "form": "스콘이요!!",
          "original_form": "스콘이요!!",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038075.1.8",
          "form": "아 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038075.1.9",
          "form": "온도를 잘못맞췃나 ㅠ",
          "original_form": "온도를 잘못맞췃나 ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038075.1.10",
          "form": "이유를 모르겠어요 왜 안부풀죠",
          "original_form": "이유를 모르겠어요 왜 안부풀죠",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038075.1.11",
          "form": "죽지마 머랭거품아,,,",
          "original_form": "죽지마 머랭거품아,,,",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038075.1.12",
          "form": "머랭에게 노래 좀 불러주고 올께요",
          "original_form": "머랭에게 노래 좀 불러주고 올께요",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038075.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038075.1.14",
          "form": "흠..손으로 하셧나요",
          "original_form": "흠..손으로 하셧나요",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038075.1.15",
          "form": "아 레몬즙을 안넣어서 3분 굽다가 레몬즙을 뿌리고",
          "original_form": "아 레몬즙을 안넣어서 3분 굽다가 레몬즙을 뿌리고",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038075.1.16",
          "form": "팬안에서 저었어요",
          "original_form": "팬안에서 저었어요",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038075.1.17",
          "form": "아항... 수플레 ... 만들기어려워보이네요",
          "original_form": "아항... 수플레 ... 만들기어려워보이네요",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038075.1.18",
          "form": "사먹는걸로..zㅋㅋㅋㅋ",
          "original_form": "사먹는걸로..zㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038075.1.19",
          "form": "ㅋㅋㅋ전 재료를 다 쓸때까진 츄라치츄라이",
          "original_form": "ㅋㅋㅋ전 재료를 다 쓸때까진 츄라치츄라이",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038075.1.20",
          "form": "아 ㅋㅋㅋ 재료 머머잇으 ㅅㅔ요",
          "original_form": "아 ㅋㅋㅋ 재료 머머잇으 ㅅㅔ요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038075.1.21",
          "form": "밀가루 설탕",
          "original_form": "밀가루 설탕",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038075.1.22",
          "form": "바닐라 익스트랙",
          "original_form": "바닐라 익스트랙",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038075.1.23",
          "form": "크림치즈",
          "original_form": "크림치즈",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038075.1.24",
          "form": "요 !ㅋㅋ",
          "original_form": "요 !ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038075.1.25",
          "form": "바스크 치즈케익? 이건어때오",
          "original_form": "바스크 치즈케익? 이건어때오",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038075.1.26",
          "form": "이미 3판을 구웠죠 하하하",
          "original_form": "이미 3판을 구웠죠 하하하",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038075.1.27",
          "form": "전 이제 바스크 장인입니다",
          "original_form": "전 이제 바스크 장인입니다",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038075.1.28",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038075.1.29",
          "form": "오븐 쓰시나요",
          "original_form": "오븐 쓰시나요",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038075.1.30",
          "form": "아뇨 에어프라이어요",
          "original_form": "아뇨 에어프라이어요",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038075.1.31",
          "form": "그래서 수플레가 망한걸까요?",
          "original_form": "그래서 수플레가 망한걸까요?",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038075.1.32",
          "form": "ㅋㅋㅋ 그건 모르겟네요. ... 수플레는 뭔가 섬세해야할거같아요",
          "original_form": "ㅋㅋㅋ 그건 모르겟네요. ... 수플레는 뭔가 섬세해야할거같아요",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038075.1.33",
          "form": "망해도 뭐,, 제가 먹을테니",
          "original_form": "망해도 뭐,, 제가 먹을테니",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038075.1.34",
          "form": "먹고나서 어떤지 말슴드릴께요",
          "original_form": "먹고나서 어떤지 말슴드릴께요",
          "speaker_id": "1",
          "time": "20210901 23:18"
        }
