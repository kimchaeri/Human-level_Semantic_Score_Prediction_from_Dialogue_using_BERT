      "utterance": [
        {
          "id": "MDRW2100026406.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026406.1.2",
          "form": "안뇽하세요",
          "original_form": "안뇽하세요",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026406.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026406.1.4",
          "form": "아뇨 전 졸업했어요ㅋㅋ",
          "original_form": "아뇨 전 졸업했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026406.1.5",
          "form": "학교 다니시나요?",
          "original_form": "학교 다니시나요?",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026406.1.6",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026406.1.7",
          "form": "오늘 대학개강일이네요",
          "original_form": "오늘 대학개강일이네요",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026406.1.8",
          "form": "헐 그래요? 헐 맞네요 9월이니까",
          "original_form": "헐 그래요? 헐 맞네요 9월이니까",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026406.1.9",
          "form": "ㅋㅋㅋ올해가 얼마안남았어요",
          "original_form": "ㅋㅋㅋ올해가 얼마안남았어요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026406.1.10",
          "form": "그러게요.. 학교다닐땐 진짜 시간 엄청 안간다 생각했는데",
          "original_form": "그러게요.. 학교다닐땐 진짜 시간 엄청 안간다 생각했는데",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026406.1.11",
          "form": "과제 하나만 해도 죽을 것 같았는데 요새는...",
          "original_form": "과제 하나만 해도 죽을 것 같았는데 요새는...",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026406.1.12",
          "form": "ㅋㅋ학교가 그리워요...",
          "original_form": "ㅋㅋ학교가 그리워요...",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026406.1.13",
          "form": "걍.. 내주는 과제 하면서 공부하는게 좋은거였을 줄이야..",
          "original_form": "걍.. 내주는 과제 하면서 공부하는게 좋은거였을 줄이야..",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026406.1.14",
          "form": "그때도 나름 힘들었지만 그때가좋앗어요ㅠㅋㅋ",
          "original_form": "그때도 나름 힘들었지만 그때가좋앗어요ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026406.1.15",
          "form": "맞아요ㅠㅠ 아 혹시 대학친구랑 요새도 만나시나요?",
          "original_form": "맞아요ㅠㅠ 아 혹시 대학친구랑 요새도 만나시나요?",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026406.1.16",
          "form": "저는 아직 만나는데 슬슬 하나씩 멀어짐..ㅠㅠ",
          "original_form": "저는 아직 만나는데 슬슬 하나씩 멀어짐..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026406.1.17",
          "form": "저도 있긴한데 점점 수가 줄어요ㅋㅋ",
          "original_form": "저도 있긴한데 점점 수가 줄어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026406.1.18",
          "form": "코로나때문에도그렇고ㅜ",
          "original_form": "코로나때문에도그렇고ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026406.1.19",
          "form": "서로바쁘다고 연락만겨우하네요",
          "original_form": "서로바쁘다고 연락만겨우하네요",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026406.1.20",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026406.1.21",
          "form": "뭐랄까 대학친구는 금방 헤어진다잖아요?",
          "original_form": "뭐랄까 대학친구는 금방 헤어진다잖아요?",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026406.1.22",
          "form": "저는 그거 부정하는 입장이었는데",
          "original_form": "저는 그거 부정하는 입장이었는데",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026406.1.23",
          "form": "이쯤되니 대학친구부터 사라지긴하더라구요",
          "original_form": "이쯤되니 대학친구부터 사라지긴하더라구요",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026406.1.24",
          "form": "저는 언제만났든 상관없이 마음맞는 친구일수록 주로 보는거같아요",
          "original_form": "저는 언제만났든 상관없이 마음맞는 친구일수록 주로 보는거같아요",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026406.1.25",
          "form": "오래전에 알았다고 오래가지도않고",
          "original_form": "오래전에 알았다고 오래가지도않고",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026406.1.26",
          "form": "오히려 중학교친구들은 너무 바뀌어서 거의 연락도 안해요",
          "original_form": "오히려 중학교친구들은 너무 바뀌어서 거의 연락도 안해요",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026406.1.27",
          "form": "와 저는 최근에 중학교 친구한테 급 연락와서 요새 제일 연락 많이 해요ㅋㅋㅋ",
          "original_form": "와 저는 최근에 중학교 친구한테 급 연락와서 요새 제일 연락 많이 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026406.1.28",
          "form": "근데 맨날 샌드위치 이야기 함ㅋㅋ",
          "original_form": "근데 맨날 샌드위치 이야기 함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026406.1.29",
          "form": "급연락하면 반갑긴하겠어요ㅋㅋ",
          "original_form": "급연락하면 반갑긴하겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026406.1.30",
          "form": "샌드위치..먹는걸로요?ㅋㅋㅋㅋ",
          "original_form": "샌드위치..먹는걸로요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026406.1.31",
          "form": "네네 전 샌드위치 좋아하고 친구는 도시락으로 샌드위치를 많이 만들어서",
          "original_form": "네네 전 샌드위치 좋아하고 친구는 도시락으로 샌드위치를 많이 만들어서",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026406.1.32",
          "form": "약간 노하우 공유??",
          "original_form": "약간 노하우 공유??",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026406.1.33",
          "form": "오 저도 노하우 알고싶어요ㅋㅋ",
          "original_form": "오 저도 노하우 알고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:28"
        }
      ]
