      "utterance": [
        {
          "id": "MDRW2100001475.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211016 00:27"
        },
        {
          "id": "MDRW2100001475.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211016 00:27"
        },
        {
          "id": "MDRW2100001475.1.3",
          "form": "최근에 사신 물건이 있으세요?",
          "original_form": "최근에 사신 물건이 있으세요?",
          "speaker_id": "2",
          "time": "20211016 00:27"
        },
        {
          "id": "MDRW2100001475.1.4",
          "form": "네 저는 마우스를 구매했어요",
          "original_form": "네 저는 마우스를 구매했어요",
          "speaker_id": "1",
          "time": "20211016 00:28"
        },
        {
          "id": "MDRW2100001475.1.5",
          "form": "오 어떤 마우스인가요?",
          "original_form": "오 어떤 마우스인가요?",
          "speaker_id": "2",
          "time": "20211016 00:28"
        },
        {
          "id": "MDRW2100001475.1.6",
          "form": "무선마우스인데 노트북에 연결해서 사용하려구요",
          "original_form": "무선마우스인데 노트북에 연결해서 사용하려구요",
          "speaker_id": "1",
          "time": "20211016 00:28"
        },
        {
          "id": "MDRW2100001475.1.7",
          "form": "아하 저도 로지텍에서 나온거 쓰고있어요 ㅋㅋㅋㅋ 무선마우스",
          "original_form": "아하 저도 로지텍에서 나온거 쓰고있어요 ㅋㅋㅋㅋ 무선마우스",
          "speaker_id": "2",
          "time": "20211016 00:29"
        },
        {
          "id": "MDRW2100001475.1.8",
          "form": "아ㅋㅋㅋ그러시군요",
          "original_form": "아ㅋㅋㅋ그러시군요",
          "speaker_id": "1",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100001475.1.9",
          "form": "로지텍꺼는 좋은가요?",
          "original_form": "로지텍꺼는 좋은가요?",
          "speaker_id": "1",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100001475.1.10",
          "form": "한번도 안써봐서 궁금하네요",
          "original_form": "한번도 안써봐서 궁금하네요",
          "speaker_id": "1",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100001475.1.11",
          "form": "네 저는 마우스는 로지텍꺼만 써요!",
          "original_form": "네 저는 마우스는 로지텍꺼만 써요!",
          "speaker_id": "2",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100001475.1.12",
          "form": "가격대비 제일 괜찮은 느낌 ㅎㅎㅎ",
          "original_form": "가격대비 제일 괜찮은 느낌 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100001475.1.13",
          "form": "그렇군요 다음에 구매해 봐야겠어요!",
          "original_form": "그렇군요 다음에 구매해 봐야겠어요!",
          "speaker_id": "1",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100001475.1.14",
          "form": "게이밍은 잘 모르겠고..",
          "original_form": "게이밍은 잘 모르겠고..",
          "speaker_id": "2",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100001475.1.15",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100001475.1.16",
          "form": "평소에 가성비 좋은 물건을 잘찾으시나요",
          "original_form": "평소에 가성비 좋은 물건을 잘찾으시나요",
          "speaker_id": "1",
          "time": "20211016 00:30"
        },
        {
          "id": "MDRW2100001475.1.17",
          "form": "저는 전자제품은 가성비를 좀 많이 따지는 거 같아요 ㅠㅠ",
          "original_form": "저는 전자제품은 가성비를 좀 많이 따지는 거 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100001475.1.18",
          "form": "쓸데없이 비싼 경우도 많아서",
          "original_form": "쓸데없이 비싼 경우도 많아서",
          "speaker_id": "2",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100001475.1.19",
          "form": "성능이나 후기 엄청 찾아보구 가성비 제일 좋다는 제품으로 사요 허허",
          "original_form": "성능이나 후기 엄청 찾아보구 가성비 제일 좋다는 제품으로 사요 허허",
          "speaker_id": "2",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100001475.1.20",
          "form": "공감입니다 저도 너무 비싸서 한번살때 많이 고민되되더라구요",
          "original_form": "공감입니다 저도 너무 비싸서 한번살때 많이 고민되되더라구요",
          "speaker_id": "1",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100001475.1.21",
          "form": "그죠... 돈이 많았음 좋겠지만 흑흑",
          "original_form": "그죠... 돈이 많았음 좋겠지만 흑흑",
          "speaker_id": "2",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100001475.1.22",
          "form": "유트브나 인터넷 검색도 많이합니다",
          "original_form": "유트브나 인터넷 검색도 많이합니다",
          "speaker_id": "1",
          "time": "20211016 00:31"
        },
        {
          "id": "MDRW2100001475.1.23",
          "form": "네.. 정말 점점 가격이 너무 오르네요",
          "original_form": "네.. 정말 점점 가격이 너무 오르네요",
          "speaker_id": "1",
          "time": "20211016 00:32"
        },
        {
          "id": "MDRW2100001475.1.24",
          "form": "저두요 유튜브 덕에 좋은 제품 많이 찾았어요 ㅎㅎ",
          "original_form": "저두요 유튜브 덕에 좋은 제품 많이 찾았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211016 00:32"
        }
      ]
    }
