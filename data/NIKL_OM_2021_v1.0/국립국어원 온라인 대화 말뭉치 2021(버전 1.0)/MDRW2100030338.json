      "utterance": [
        {
          "id": "MDRW2100030338.1.1",
          "form": "스포츠 경기 본 적 있어?",
          "original_form": "스포츠 경기 본 적 있어?",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030338.1.2",
          "form": "웅!!난 야구경기나 배구경기 보러간 적 잇어",
          "original_form": "웅!!난 야구경기나 배구경기 보러간 적 잇어",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030338.1.3",
          "form": "야구장엔 꼭 치킨이나 피자를 들구가야돼 ㅎㅎ",
          "original_form": "야구장엔 꼭 치킨이나 피자를 들구가야돼 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030338.1.4",
          "form": "우와 짱이다",
          "original_form": "우와 짱이다",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.5",
          "form": "경기 보면서 먹는거야?",
          "original_form": "경기 보면서 먹는거야?",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.6",
          "form": "응 ㅎㅎ 야구보면서 먹으면",
          "original_form": "응 ㅎㅎ 야구보면서 먹으면",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.7",
          "form": "더 맛잇기두 해..",
          "original_form": "더 맛잇기두 해..",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.8",
          "form": "더 맛업기도 하겟지만..",
          "original_form": "더 맛업기도 하겟지만..",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.9",
          "form": "우와 맛있겠다!!",
          "original_form": "우와 맛있겠다!!",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.10",
          "form": "응원하는것도 재밋구",
          "original_form": "응원하는것도 재밋구",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.11",
          "form": "치킨은 항상 맛잇긴 하지 ㅋ",
          "original_form": "치킨은 항상 맛잇긴 하지 ㅋ",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.12",
          "form": "맞아 소음이 큰 곳에는 맛이 더 없게 느껴진다고 들었어.",
          "original_form": "맞아 소음이 큰 곳에는 맛이 더 없게 느껴진다고 들었어.",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.13",
          "form": "실제론 맛이 더 없었겠지",
          "original_form": "실제론 맛이 더 없었겠지",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.14",
          "form": "진좌? 신기하당",
          "original_form": "진좌? 신기하당",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ웃긴다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ웃긴다",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.16",
          "form": "그러게 ㅎㅎ",
          "original_form": "그러게 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.17",
          "form": "그래도 소음이라고는 잘 안느껴져",
          "original_form": "그래도 소음이라고는 잘 안느껴져",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030338.1.18",
          "form": "난 올림픽!! 봤어 맨날",
          "original_form": "난 올림픽!! 봤어 맨날",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030338.1.19",
          "form": "그렇구나 의외네",
          "original_form": "그렇구나 의외네",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030338.1.20",
          "form": "되게 시끄러울줄알았어 응원때문에",
          "original_form": "되게 시끄러울줄알았어 응원때문에",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030338.1.21",
          "form": "너두 나중에 한번 가봐 ㅎㅎ",
          "original_form": "너두 나중에 한번 가봐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030338.1.22",
          "form": "아니면 같이 가든강",
          "original_form": "아니면 같이 가든강",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030338.1.23",
          "form": "올림픽도 재밋지",
          "original_form": "올림픽도 재밋지",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030338.1.24",
          "form": "좋아 !!",
          "original_form": "좋아 !!",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030338.1.25",
          "form": "다음에 같이 가자 ㅎㅎ",
          "original_form": "다음에 같이 가자 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
