        },
        {
          "id": "MDRW2100027635.1.2",
          "form": "하노이쪽이 좋더라",
          "original_form": "하노이쪽이 좋더라",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027635.1.3",
          "form": "나도. 사계절이 좋았어",
          "original_form": "나도. 사계절이 좋았어",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027635.1.4",
          "form": "사파랑 닌빈 참 좋았는데 너무 추웠지",
          "original_form": "사파랑 닌빈 참 좋았는데 너무 추웠지",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027635.1.5",
          "form": "그게 다 추억이야",
          "original_form": "그게 다 추억이야",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027635.1.6",
          "form": "하노이 쌀국수 먹고싶다",
          "original_form": "하노이 쌀국수 먹고싶다",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027635.1.7",
          "form": "하노이는 분짜야",
          "original_form": "하노이는 분짜야",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027635.1.8",
          "form": "요거트커피도좋구",
          "original_form": "요거트커피도좋구",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027635.1.9",
          "form": "아냐 에그커피 맛집이 최고",
          "original_form": "아냐 에그커피 맛집이 최고",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027635.1.10",
          "form": "분탕도 인상적이었어",
          "original_form": "분탕도 인상적이었어",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027635.1.11",
          "form": "뜨끈했지",
          "original_form": "뜨끈했지",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027635.1.12",
          "form": "하노이 여름은 습해",
          "original_form": "하노이 여름은 습해",
