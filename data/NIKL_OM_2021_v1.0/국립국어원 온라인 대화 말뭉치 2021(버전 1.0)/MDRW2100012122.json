      "utterance": [
        {
          "id": "MDRW2100012122.1.1",
          "form": "ㅎㅎ 그 때 말도 예쁘게 하고 좋았지",
          "original_form": "ㅎㅎ 그 때 말도 예쁘게 하고 좋았지",
          "speaker_id": "2",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100012122.1.2",
          "form": "그 시간이 너무 그리워",
          "original_form": "그 시간이 너무 그리워",
          "speaker_id": "1",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100012122.1.3",
          "form": "벌써 그리우면 안되지~~ 그런 건 나이들어서나 ㅎㅎ",
          "original_form": "벌써 그리우면 안되지~~ 그런 건 나이들어서나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012122.1.4",
          "form": "아기 때 모습 너무 귀엽고 기억이 새록새록 ㅎㅎ",
          "original_form": "아기 때 모습 너무 귀엽고 기억이 새록새록 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012122.1.5",
          "form": "벌써 추억에 빠져들지마",
          "original_form": "벌써 추억에 빠져들지마",
          "speaker_id": "2",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012122.1.6",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012122.1.7",
          "form": "작은 아들 기억나지? 뭐만 잘못하면 미안해요 잘못했어요 싹싹 빌면서",
          "original_form": "작은 아들 기억나지? 뭐만 잘못하면 미안해요 잘못했어요 싹싹 빌면서",
          "speaker_id": "1",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012122.1.8",
          "form": "ㅎㅎ 지금은 말대꾸가 쓰리콤보인데",
          "original_form": "ㅎㅎ 지금은 말대꾸가 쓰리콤보인데",
          "speaker_id": "2",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012122.1.9",
          "form": "예쁜 아가 어디갔나 ㅋㅋ",
          "original_form": "예쁜 아가 어디갔나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012122.1.10",
          "form": "요즘 우리집엔 반항아만 둘 남았네 ㅋ",
          "original_form": "요즘 우리집엔 반항아만 둘 남았네 ㅋ",
          "speaker_id": "2",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012122.1.11",
          "form": "그래도 우리집 애들 정도면 진짜 착한거야",
          "original_form": "그래도 우리집 애들 정도면 진짜 착한거야",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012122.1.12",
          "form": "다른 애들은 안봐서 모르지 ㅋㅋ",
          "original_form": "다른 애들은 안봐서 모르지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012122.1.13",
          "form": "그리고 지금도 예쁘기만 한데 뭘",
          "original_form": "그리고 지금도 예쁘기만 한데 뭘",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012122.1.14",
          "form": "자라는 모습 사진으로 많이 남겨놔",
          "original_form": "자라는 모습 사진으로 많이 남겨놔",
