      "utterance": [
        {
          "id": "MDRW2100028708.1.1",
          "form": "오늘 완전 가을 날씨 된거보고 깜쩍 놀렀어요",
          "original_form": "오늘 완전 가을 날씨 된거보고 깜쩍 놀렀어요",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028708.1.2",
          "form": "지금은 좀 추워지는듯해요",
          "original_form": "지금은 좀 추워지는듯해요",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028708.1.3",
          "form": "저녁은 쌀쌀",
          "original_form": "저녁은 쌀쌀",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028708.1.4",
          "form": "가을 옷 꺼내야겠어요",
          "original_form": "가을 옷 꺼내야겠어요",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028708.1.5",
          "form": "낮은 좀 덥고",
          "original_form": "낮은 좀 덥고",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028708.1.6",
          "form": "그래봤자 운동복 이지만요^^^^",
          "original_form": "그래봤자 운동복 이지만요^^^^",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028708.1.7",
          "form": "name1도 점퍼 걸치고 다니세요",
          "original_form": "&name1&도 점퍼 걸치고 다니세요",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028708.1.8",
          "form": "정말요? 저는 요즘 집에서 잘 안나가요",
          "original_form": "정말요? 저는 요즘 집에서 잘 안나가요",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028708.1.9",
          "form": "이런날 감기 걸리기 딱이예요",
          "original_form": "이런날 감기 걸리기 딱이예요",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028708.1.10",
          "form": "학교 일 이외에는",
          "original_form": "학교 일 이외에는",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028708.1.11",
          "form": "감사해요 name2도 옷 걸치세요",
          "original_form": "감사해요 &name2&도 옷 걸치세요",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028708.1.12",
          "form": "기침하면 다들 쳐다봐요",
          "original_form": "기침하면 다들 쳐다봐요",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028708.1.13",
          "form": "감사해요..",
          "original_form": "감사해요..",
