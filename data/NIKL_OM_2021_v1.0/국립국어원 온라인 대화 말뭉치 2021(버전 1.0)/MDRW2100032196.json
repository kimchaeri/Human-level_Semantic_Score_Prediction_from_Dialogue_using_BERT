      "utterance": [
        {
          "id": "MDRW2100032196.1.1",
          "form": "오늘 쌍커풀 코 수술 하는 카페에",
          "original_form": "오늘 쌍커풀 코 수술 하는 카페에",
          "speaker_id": "1",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032196.1.2",
          "form": "접속해 보셨나요",
          "original_form": "접속해 보셨나요",
          "speaker_id": "1",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032196.1.3",
          "form": "커뮤니티는 왜 때문에 자주 나올까요",
          "original_form": "커뮤니티는 왜 때문에 자주 나올까요",
          "speaker_id": "2",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032196.1.4",
          "form": "저는 오늘 접속했는데",
          "original_form": "저는 오늘 접속했는데",
          "speaker_id": "2",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032196.1.5",
          "form": "사람들이 커뮤니티에 관심이 많아서 그런 걸까요",
          "original_form": "사람들이 커뮤니티에 관심이 많아서 그런 걸까요",
          "speaker_id": "1",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032196.1.6",
          "form": "보아의 오빠가 사망에서 숙연 합니다",
          "original_form": "보아의 오빠가 사망에서 숙연 합니다",
          "speaker_id": "2",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032196.1.7",
          "form": "요즘은 사람들이 만나는 것보다 온라인상에서 정보를 주고받는 일이 중요하니까 많으니까 그래서 자주 나오는 것 아닐까요",
          "original_form": "요즘은 사람들이 만나는 것보다 온라인상에서 정보를 주고받는 일이 중요하니까 많으니까 그래서 자주 나오는 것 아닐까요",
          "speaker_id": "1",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032196.1.8",
          "form": "몇 달 전에 근황을 올린 글을 보았거든요",
          "original_form": "몇 달 전에 근황을 올린 글을 보았거든요",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.9",
          "form": "아 그렇습니까 몰랐습니다",
          "original_form": "아 그렇습니까 몰랐습니다",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.10",
          "form": "나왔으면 했는데 나이가 동갑이 더라고요",
          "original_form": "나왔으면 했는데 나이가 동갑이 더라고요",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.11",
          "form": "저는 카페에 가면 공포 게시판을 자주 들락날락 합니다",
          "original_form": "저는 카페에 가면 공포 게시판을 자주 들락날락 합니다",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.12",
          "form": "영화 감독 일해요 꿈도 많았을 텐데",
          "original_form": "영화 감독 일해요 꿈도 많았을 텐데",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.13",
          "form": "아이고 젊은 나이에 안타깝네요",
          "original_form": "아이고 젊은 나이에 안타깝네요",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.14",
          "form": "부가 살이 방 말씀이지요",
          "original_form": "부가 살이 방 말씀이지요",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.15",
          "form": "어떻게 하다가 죽게 되었나요 차 사고 였나요",
          "original_form": "어떻게 하다가 죽게 되었나요 차 사고 였나요",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.16",
          "form": "네 맞습니다",
          "original_form": "네 맞습니다",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.17",
          "form": "말기암 일해요",
          "original_form": "말기암 일해요",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.18",
          "form": "지병이 있었나요",
          "original_form": "지병이 있었나요",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032196.1.19",
          "form": "여명이 3개월 남았다고 글 올려서거든요",
          "original_form": "여명이 3개월 남았다고 글 올려서거든요",
          "speaker_id": "2",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032196.1.20",
          "form": "와 요즘에는 나이가 적어도 암이 찾아올 수 있으니",
          "original_form": "와 요즘에는 나이가 적어도 암이 찾아올 수 있으니",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032196.1.21",
          "form": "본인이 올린 걸 보았지요",
          "original_form": "본인이 올린 걸 보았지요",
          "speaker_id": "2",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032196.1.22",
          "form": "너무 안타깝네요",
          "original_form": "너무 안타깝네요",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032196.1.23",
          "form": "가족들 마음이 어떠했을지 참",
          "original_form": "가족들 마음이 어떠했을지 참",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032196.1.24",
          "form": "네 건강 관리 잘하라는 메시지를 남겼어요",
          "original_form": "네 건강 관리 잘하라는 메시지를 남겼어요",
          "speaker_id": "2",
          "time": "20210905 13:58"
        }
      ]
    }
