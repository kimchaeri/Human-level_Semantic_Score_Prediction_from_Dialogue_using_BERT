      "utterance": [
        {
          "id": "MDRW2100011952.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011952.1.2",
          "form": "스포츠는 잘 안챙겨보는데 이번에 올림픽은 재밌게 봤어요!",
          "original_form": "스포츠는 잘 안챙겨보는데 이번에 올림픽은 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011952.1.3",
          "form": "여자 배구!!",
          "original_form": "여자 배구!!",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011952.1.4",
          "form": "한일전 진짜 대박이었죠!!",
          "original_form": "한일전 진짜 대박이었죠!!",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011952.1.5",
          "form": "아 여자배구 진짜대박..",
          "original_form": "아 여자배구 진짜대박..",
          "speaker_id": "1",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011952.1.6",
          "form": "한일전 보셨어여?.",
          "original_form": "한일전 보셨어여?.",
          "speaker_id": "2",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100011952.1.7",
          "form": "네 진짜 지는줄알았는데",
          "original_form": "네 진짜 지는줄알았는데",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100011952.1.8",
          "form": "이겼죠 ㅋㅋㅋㅋ",
          "original_form": "이겼죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100011952.1.9",
          "form": "양궁도 재밌게 봤어요 ㅎㅎㅎ",
          "original_form": "양궁도 재밌게 봤어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100011952.1.10",
          "form": "혼성 대박",
          "original_form": "혼성 대박",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100011952.1.11",
          "form": "안산선수도 대박ㅠㅠ",
          "original_form": "안산선수도 대박ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100011952.1.12",
          "form": "ㅎㅎㅎ 금메달 따는거 실시간으로 봤는데 진짜 멋짐폭발 ㅠㅠㅠ",
          "original_form": "ㅎㅎㅎ 금메달 따는거 실시간으로 봤는데 진짜 멋짐폭발 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100011952.1.13",
          "form": "완전 지금 대스타잖아요ㅠㅠ 완전 능력자...",
          "original_form": "완전 지금 대스타잖아요ㅠㅠ 완전 능력자...",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100011952.1.14",
          "form": "진짜 실력이 넘사벽..",
          "original_form": "진짜 실력이 넘사벽..",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100011952.1.15",
          "form": "진짜 아무도 못따라온다...",
          "original_form": "진짜 아무도 못따라온다...",
          "speaker_id": "2",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100011952.1.16",
          "form": "마자요 금 3개라니...",
          "original_form": "마자요 금 3개라니...",
          "speaker_id": "1",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100011952.1.17",
          "form": "경기 직접 보러가신적은 있나여??",
          "original_form": "경기 직접 보러가신적은 있나여??",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100011952.1.18",
          "form": "축구 가봤어요!",
          "original_form": "축구 가봤어요!",
          "speaker_id": "1",
          "time": "20210824 18:59"
        }
      ]
