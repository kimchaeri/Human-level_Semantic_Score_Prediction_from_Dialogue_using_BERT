      "utterance": [
        {
          "id": "MDRW2100045647.1.1",
          "form": "이번에는 제가 지금 먹고싶은 음식 ㅋㅋㅋㅋ",
          "original_form": "이번에는 제가 지금 먹고싶은 음식 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045647.1.2",
          "form": "좋슴다 맞추고 말테다",
          "original_form": "좋슴다 맞추고 말테다",
          "speaker_id": "2",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045647.1.3",
          "form": "화이팅",
          "original_form": "화이팅",
          "speaker_id": "1",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045647.1.4",
          "form": "음료수인가요?",
          "original_form": "음료수인가요?",
          "speaker_id": "2",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045647.1.5",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045647.1.6",
          "form": "밀가루 들어가나요?",
          "original_form": "밀가루 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045647.1.7",
          "form": "밀가루 안들어가요",
          "original_form": "밀가루 안들어가요",
          "speaker_id": "1",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045647.1.8",
          "form": "쌀이 들어가나요?",
          "original_form": "쌀이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045647.1.9",
          "form": "네 쌀이들어가요",
          "original_form": "네 쌀이들어가요",
          "speaker_id": "1",
          "time": "20210908 04:46"
        },
        {
          "id": "MDRW2100045647.1.10",
          "form": "밥인가요? 아니면 쌀을 가공한 건가요",
          "original_form": "밥인가요? 아니면 쌀을 가공한 건가요",
          "speaker_id": "2",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045647.1.11",
          "form": "밥입니다",
          "original_form": "밥입니다",
          "speaker_id": "1",
          "time": "20210908 04:47"
        },
        {
          "id": "MDRW2100045647.1.12",
          "form": "밥을 볶았나요?",
          "original_form": "밥을 볶았나요?",
          "speaker_id": "2",
          "time": "20210908 04:48"
        },
        {
          "id": "MDRW2100045647.1.13",
          "form": "아니요 볶은 밥 아니에요",
          "original_form": "아니요 볶은 밥 아니에요",
          "speaker_id": "1",
          "time": "20210908 04:48"
        },
        {
          "id": "MDRW2100045647.1.14",
          "form": "으으음 국물요리예요?",
          "original_form": "으으음 국물요리예요?",
          "speaker_id": "2",
          "time": "20210908 04:49"
        },
        {
          "id": "MDRW2100045647.1.15",
          "form": "국물요리 아니에요",
          "original_form": "국물요리 아니에요",
