        {
          "id": "MDRW2100016520.1.2",
          "form": "네 넷플보세요?",
          "original_form": "네 넷플보세요?",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016520.1.3",
          "form": "넵 봅니다 킹덤ㅎㅎ",
          "original_form": "넵 봅니다 킹덤ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016520.1.4",
          "form": "아 네 저도 봤어요",
          "original_form": "아 네 저도 봤어요",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016520.1.5",
          "form": "아신전도보셨나요",
          "original_form": "아신전도보셨나요",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016520.1.6",
          "form": "네ㅎㅎㅎ근데 좀 잔인해서",
          "original_form": "네ㅎㅎㅎ근데 좀 잔인해서",
