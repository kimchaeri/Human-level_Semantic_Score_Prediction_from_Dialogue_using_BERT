      "utterance": [
        {
          "id": "MDRW2100025645.1.1",
          "form": "요즘 콜라보 한거 드셔보신게 있으신가요?",
          "original_form": "요즘 콜라보 한거 드셔보신게 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025645.1.2",
          "form": "어....뭐가있죠?",
          "original_form": "어....뭐가있죠?",
          "speaker_id": "2",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100025645.1.3",
          "form": "음 맥주.. 과자.. 아이스크림",
          "original_form": "음 맥주.. 과자.. 아이스크림",
          "speaker_id": "1",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100025645.1.4",
          "form": "저는 맥주를 주로 먹어봤어욕",
          "original_form": "저는 맥주를 주로 먹어봤어욕",
          "speaker_id": "1",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100025645.1.5",
          "form": "곰표",
          "original_form": "곰표",
          "speaker_id": "1",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100025645.1.6",
          "form": "랑...",
          "original_form": "랑...",
          "speaker_id": "1",
          "time": "20210901 14:56"
        },
        {
          "id": "MDRW2100025645.1.7",
          "form": "안그래도 맥주 맛있다고하더라구요!",
          "original_form": "안그래도 맥주 맛있다고하더라구요!",
          "speaker_id": "2",
          "time": "20210901 14:56"
        },
        {
          "id": "MDRW2100025645.1.8",
          "form": "밀맥주좋아해서 관심은가요",
          "original_form": "밀맥주좋아해서 관심은가요",
          "speaker_id": "2",
          "time": "20210901 14:56"
        },
        {
          "id": "MDRW2100025645.1.9",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210901 14:56"
        },
        {
          "id": "MDRW2100025645.1.10",
          "form": "제 취향에는 맞는 맥주였어요",
          "original_form": "제 취향에는 맞는 맥주였어요",
          "speaker_id": "1",
          "time": "20210901 14:56"
        },
        {
          "id": "MDRW2100025645.1.11",
          "form": "향도 과일향나고",
          "original_form": "향도 과일향나고",
          "speaker_id": "1",
          "time": "20210901 14:56"
        },
        {
          "id": "MDRW2100025645.1.12",
          "form": "밀맥주 좋아하시면 별로 안좋아하실거같기두해요",
          "original_form": "밀맥주 좋아하시면 별로 안좋아하실거같기두해요",
          "speaker_id": "1",
          "time": "20210901 14:56"
        },
        {
          "id": "MDRW2100025645.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025645.1.14",
          "form": "헉 그런가요ㅠㅠ 그래도 다음에 한번 먹어볼게요!",
          "original_form": "헉 그런가요ㅠㅠ 그래도 다음에 한번 먹어볼게요!",
          "speaker_id": "2",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025645.1.15",
          "form": "요즘 지플립3 나오면서 콜라보 한것두 많더라구요",
          "original_form": "요즘 지플립3 나오면서 콜라보 한것두 많더라구요",
          "speaker_id": "1",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025645.1.16",
          "form": "갠시리 콜라보한 무선충전기에 혹해서",
          "original_form": "갠시리 콜라보한 무선충전기에 혹해서",
          "speaker_id": "1",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025645.1.17",
          "form": "휴대폰 갖고싶었어요",
          "original_form": "휴대폰 갖고싶었어요",
          "speaker_id": "1",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025645.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025645.1.19",
          "form": "의자같은거요?",
          "original_form": "의자같은거요?",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025645.1.20",
          "form": "네네맞아요",
          "original_form": "네네맞아요",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025645.1.21",
          "form": "의자같은거 잘 만들어놨더라구요",
          "original_form": "의자같은거 잘 만들어놨더라구요",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025645.1.22",
          "form": "종류도 엄청 많아요",
          "original_form": "종류도 엄청 많아요",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025645.1.23",
          "form": "콜라보 하는게 마케팅에는 확실히 도움이 되는거같아요",
          "original_form": "콜라보 하는게 마케팅에는 확실히 도움이 되는거같아요",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025645.1.24",
          "form": "name1은 콜라보좋아하시나요",
          "original_form": "&name1&은 콜라보좋아하시나요",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025645.1.25",
          "form": "사고싶은 충동..?",
          "original_form": "사고싶은 충동..?",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025645.1.26",
          "form": "음 전 좀 그런거같아요",
          "original_form": "음 전 좀 그런거같아요",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025645.1.27",
          "form": "뭔가 혹하면서 한번 사볼까~?생각이들어서",
          "original_form": "뭔가 혹하면서 한번 사볼까~?생각이들어서",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025645.1.28",
          "form": "name2은 어떤가요?",
          "original_form": "&name2&은 어떤가요?",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025645.1.29",
          "form": "확실히 마케팅에는 도움될거같아요",
          "original_form": "확실히 마케팅에는 도움될거같아요",
          "speaker_id": "2",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025645.1.30",
          "form": "저처럼 혹해서 사는 사람이 많겠죠?",
          "original_form": "저처럼 혹해서 사는 사람이 많겠죠?",
          "speaker_id": "1",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025645.1.31",
          "form": "ㅋㅋㅋ맞아요 많을거에요",
          "original_form": "ㅋㅋㅋ맞아요 많을거에요",
