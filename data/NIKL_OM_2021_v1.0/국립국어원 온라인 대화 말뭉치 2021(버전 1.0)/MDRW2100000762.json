      "utterance": [
        {
          "id": "MDRW2100000762.1.1",
          "form": "여행 어디 가는 거 좋아하세요?",
          "original_form": "여행 어디 가는 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000762.1.2",
          "form": "해외 좋아하는데 못가고 있어요",
          "original_form": "해외 좋아하는데 못가고 있어요",
          "speaker_id": "1",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100000762.1.3",
          "form": "해외 어디 좋아하세요????",
          "original_form": "해외 어디 좋아하세요????",
          "speaker_id": "2",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000762.1.4",
          "form": "스페인 가고 싶어요",
          "original_form": "스페인 가고 싶어요",
          "speaker_id": "1",
          "time": "20210819 19:19"
        },
        {
          "id": "MDRW2100000762.1.5",
          "form": "우와 스페인 너무 좋을 거 같아여",
          "original_form": "우와 스페인 너무 좋을 거 같아여",
          "speaker_id": "2",
          "time": "20210819 19:28"
        },
        {
          "id": "MDRW2100000762.1.6",
          "form": "전 안가봤거든요",
          "original_form": "전 안가봤거든요",
          "speaker_id": "2",
          "time": "20210819 19:29"
        },
        {
          "id": "MDRW2100000762.1.7",
          "form": "네 저도 듣기만 해서 너무 좋다고 하더라구요",
          "original_form": "네 저도 듣기만 해서 너무 좋다고 하더라구요",
          "speaker_id": "1",
          "time": "20210819 19:29"
        },
        {
          "id": "MDRW2100000762.1.8",
          "form": "아하 너무 좋을 거 같아요",
          "original_form": "아하 너무 좋을 거 같아요",
          "speaker_id": "2",
          "time": "20210819 19:31"
        },
        {
          "id": "MDRW2100000762.1.9",
          "form": "해외 어디가보셨어요?",
          "original_form": "해외 어디가보셨어요?",
          "speaker_id": "2",
          "time": "20210819 19:31"
        },
        {
          "id": "MDRW2100000762.1.10",
          "form": "파리 호주 괌 정도요",
          "original_form": "파리 호주 괌 정도요",
          "speaker_id": "1",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000762.1.11",
          "form": "우와 많이 가보셨네요",
          "original_form": "우와 많이 가보셨네요",
          "speaker_id": "2",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000762.1.12",
          "form": "전 예전에 간 일본이 다에요 ㅎ",
          "original_form": "전 예전에 간 일본이 다에요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000762.1.13",
          "form": "전 방사능때문에 일본은 아직",
          "original_form": "전 방사능때문에 일본은 아직",
          "speaker_id": "1",
          "time": "20210819 19:33"
        },
        {
          "id": "MDRW2100000762.1.14",
          "form": "이제는 안가여!!",
          "original_form": "이제는 안가여!!",
          "speaker_id": "2",
          "time": "20210819 19:36"
        },
        {
          "id": "MDRW2100000762.1.15",
          "form": "홍콩은 야경이 이쁜데 도로가 더럽더라구요",
          "original_form": "홍콩은 야경이 이쁜데 도로가 더럽더라구요",
          "speaker_id": "1",
          "time": "20210819 19:37"
        },
        {
          "id": "MDRW2100000762.1.16",
          "form": "도로에 바퀴벌레가 마트에도 돌아다네여",
          "original_form": "도로에 바퀴벌레가 마트에도 돌아다네여",
          "speaker_id": "1",
          "time": "20210819 19:37"
        },
        {
          "id": "MDRW2100000762.1.17",
          "form": "헉 도로에 바퀴벌레요? 제가 가기에는 어려울 거 같네요",
          "original_form": "헉 도로에 바퀴벌레요? 제가 가기에는 어려울 거 같네요",
          "speaker_id": "2",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000762.1.18",
          "form": "그래도 홍콩 정말 예쁘던데...!",
