      "utterance": [
        {
          "id": "MDRW2100006998.1.1",
          "form": "된장지깨?",
          "original_form": "된장지깨?",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006998.1.2",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006998.1.3",
          "form": "된장찌개맛있나 난별로던데",
          "original_form": "된장찌개맛있나 난별로던데",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006998.1.4",
          "form": "맛있게 먹었잖아",
          "original_form": "맛있게 먹었잖아",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.5",
          "form": "그리고 먹은거 청국장임",
          "original_form": "그리고 먹은거 청국장임",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.6",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.7",
          "form": "청국장은 괜찮지",
          "original_form": "청국장은 괜찮지",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.8",
          "form": "좀 출출한데 데워먹을까",
          "original_form": "좀 출출한데 데워먹을까",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.9",
          "form": "야식시켜라",
          "original_form": "야식시켜라",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.10",
          "form": "월요일까지 휴가인데, 야식정도는 괜찮잖아?",
          "original_form": "월요일까지 휴가인데, 야식정도는 괜찮잖아?",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.11",
          "form": "됐다 돈아껴라",
          "original_form": "됐다 돈아껴라",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.12",
          "form": "갈땐가더라도?",
          "original_form": "갈땐가더라도?",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.13",
          "form": "살찐다",
          "original_form": "살찐다",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006998.1.14",
          "form": "딱 야식먹기 좋은날씨구만",
          "original_form": "딱 야식먹기 좋은날씨구만",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006998.1.15",
          "form": "술좀 많이 먹지마라",
          "original_form": "술좀 많이 먹지마라",
