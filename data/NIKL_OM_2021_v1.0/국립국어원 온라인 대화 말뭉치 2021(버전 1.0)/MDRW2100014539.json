      "utterance": [
        {
          "id": "MDRW2100014539.1.1",
          "form": "싸이월드 했었어?",
          "original_form": "싸이월드 했었어?",
          "speaker_id": "1",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014539.1.2",
          "form": "옛날에",
          "original_form": "옛날에",
          "speaker_id": "1",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014539.1.3",
          "form": "ㅇㅇ 했었지",
          "original_form": "ㅇㅇ 했었지",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014539.1.4",
          "form": "흑역사 덩어리....  삭제시켜야되는데",
          "original_form": "흑역사 덩어리....  삭제시켜야되는데",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014539.1.5",
          "form": "살아있나?",
          "original_form": "살아있나?",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014539.1.6",
          "form": "드가봄?",
          "original_form": "드가봄?",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014539.1.7",
          "form": "아니 정신이 없어서 ㅋㅋ 걍 냅둘까싶기도함",
          "original_form": "아니 정신이 없어서 ㅋㅋ 걍 냅둘까싶기도함",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014539.1.8",
          "form": "난 예전에 폐지?없어진다해서 없어졌을거같아",
          "original_form": "난 예전에 폐지?없어진다해서 없어졌을거같아",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014539.1.9",
          "form": "그때공지함서 유지할사람 백업받아랬는데 안했어서ㅎ",
          "original_form": "그때공지함서 유지할사람 백업받아랬는데 안했어서ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014539.1.10",
          "form": "그때 없앤다했는데 다시 돌아온거 아녀?",
          "original_form": "그때 없앤다했는데 다시 돌아온거 아녀?",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014539.1.11",
          "form": "사람들 왜 내 계정 살리냐고 난리던데 ㅋㅋ",
          "original_form": "사람들 왜 내 계정 살리냐고 난리던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014539.1.12",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014539.1.13",
          "form": "난 그때 보관했던 사람들꺼 살려주는거라 생각했어",
          "original_form": "난 그때 보관했던 사람들꺼 살려주는거라 생각했어",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014539.1.14",
          "form": "모지",
          "original_form": "모지",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014539.1.15",
          "form": "그럼 수련이 흐른 지금까지",
          "original_form": "그럼 수련이 흐른 지금까지",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014539.1.16",
          "form": "개인정보를 보관중이란건가",
          "original_form": "개인정보를 보관중이란건가",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014539.1.17",
          "form": "ㅇㅇ그런거 같더라",
          "original_form": "ㅇㅇ그런거 같더라",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014539.1.18",
          "form": "어..좀 너무한데ㅋ",
          "original_form": "어..좀 너무한데ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014539.1.19",
          "form": "맞제 그래서 그때 난리난 기억이 있당",
          "original_form": "맞제 그래서 그때 난리난 기억이 있당",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014539.1.20",
          "form": "난 자세히 안봐서 그냥 막연히",
          "original_form": "난 자세히 안봐서 그냥 막연히",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014539.1.21",
          "form": "당연히 그때 보관했던 사람들이라 생각했다",
          "original_form": "당연히 그때 보관했던 사람들이라 생각했다",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014539.1.22",
          "form": "싸이월드 그당시 그감성이 재미나기도 했어서",
          "original_form": "싸이월드 그당시 그감성이 재미나기도 했어서",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014539.1.23",
          "form": "부활하는거야 뭐",
          "original_form": "부활하는거야 뭐",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014539.1.24",
          "form": "난 귀찮기도 하고 뭔가 옛날꺼 꺼내보기 무서워서 냅둘까싶엉 ㅋㅋ",
          "original_form": "난 귀찮기도 하고 뭔가 옛날꺼 꺼내보기 무서워서 냅둘까싶엉 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014539.1.25",
          "form": "다시 함 가입해볼까 생각했었는데",
          "original_form": "다시 함 가입해볼까 생각했었는데",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014539.1.26",
          "form": "뒷통수 느낌",
          "original_form": "뒷통수 느낌",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014539.1.27",
          "form": "그랬구만 ㅠㅠ",
          "original_form": "그랬구만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014539.1.28",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014539.1.29",
          "form": "그래서 싸이월드 부활이란 키워드가 있구만",
          "original_form": "그래서 싸이월드 부활이란 키워드가 있구만",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014539.1.30",
          "form": "덕분에 몰랐던 정보를 알았네",
          "original_form": "덕분에 몰랐던 정보를 알았네",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014539.1.31",
          "form": "ㅇㅇ 싸이월드 진짜 추억의 사이트",
          "original_form": "ㅇㅇ 싸이월드 진짜 추억의 사이트",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014539.1.32",
          "form": "마져",
          "original_form": "마져",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014539.1.33",
          "form": "난 그때도 애들 다 있는데",
          "original_form": "난 그때도 애들 다 있는데",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014539.1.34",
          "form": "안해서 애들이 만들라고만들라고ㅋ",
          "original_form": "안해서 애들이 만들라고만들라고ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014539.1.35",
          "form": "만들고 관리안함ㅋ",
          "original_form": "만들고 관리안함ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014539.1.36",
          "form": "그때부터 알았지 내 스탈 아니구나ㅋ",
          "original_form": "그때부터 알았지 내 스탈 아니구나ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014539.1.37",
          "form": "여전히 인스타 페북 난리일때",
          "original_form": "여전히 인스타 페북 난리일때",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014539.1.38",
          "form": "도 안하더라ㅋㅋㅋ",
          "original_form": "도 안하더라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
