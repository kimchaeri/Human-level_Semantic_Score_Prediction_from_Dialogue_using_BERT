      "utterance": [
        {
          "id": "MDRW2100035290.1.1",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100035290.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100035290.1.3",
          "form": "맞으셨나요?",
          "original_form": "맞으셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100035290.1.4",
          "form": "1차도 아직이신가요?",
          "original_form": "1차도 아직이신가요?",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100035290.1.5",
          "form": "전 돌아오는 수요일에 2차 맞아요",
          "original_form": "전 돌아오는 수요일에 2차 맞아요",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035290.1.6",
          "form": "네ㅠ 9월에 맞아요",
          "original_form": "네ㅠ 9월에 맞아요",
          "speaker_id": "2",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100035290.1.7",
          "form": "뭐 맞으시나요?",
          "original_form": "뭐 맞으시나요?",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100035290.1.8",
          "form": "모더나요!",
          "original_form": "모더나요!",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100035290.1.9",
          "form": "어떤거 맞으셨어요?",
          "original_form": "어떤거 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100035290.1.10",
          "form": "전 화이자요~ 모더나는 주변에 맞으신 분이 없어서 잘 모르겠네요",
          "original_form": "전 화이자요~ 모더나는 주변에 맞으신 분이 없어서 잘 모르겠네요",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100035290.1.11",
          "form": "오! 백신맞으시고 괜찮으셨어요?",
          "original_form": "오! 백신맞으시고 괜찮으셨어요?",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100035290.1.12",
          "form": "친구는 손이 퉁퉁부었었다더라구요",
          "original_form": "친구는 손이 퉁퉁부었었다더라구요",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100035290.1.13",
          "form": "1차맞고 엄청 겁나거 타이레놀을 4시간 간격으로 먹었더니 아무렇지 않게 지나갔어요",
          "original_form": "1차맞고 엄청 겁나거 타이레놀을 4시간 간격으로 먹었더니 아무렇지 않게 지나갔어요",
          "speaker_id": "1",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100035290.1.14",
          "form": "오오.. 타이레놀은 필수군요",
          "original_form": "오오.. 타이레놀은 필수군요",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100035290.1.15",
          "form": "맞기전에 먹으라는 친구도 있었거든요",
          "original_form": "맞기전에 먹으라는 친구도 있었거든요",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100035290.1.16",
          "form": "맞기 전에 먹으면 항체 형성을 방해한다는 말이 있더라고요",
          "original_form": "맞기 전에 먹으면 항체 형성을 방해한다는 말이 있더라고요",
          "speaker_id": "1",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100035290.1.17",
          "form": "헉!",
          "original_form": "헉!",
          "speaker_id": "2",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100035290.1.18",
          "form": "갔다와서 먹어야겠군요..",
          "original_form": "갔다와서 먹어야겠군요..",
          "speaker_id": "2",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100035290.1.19",
          "form": "그건 몰랐네요",
          "original_form": "그건 몰랐네요",
          "speaker_id": "2",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100035290.1.20",
          "form": "그리고 맞고 거기서 20분 대기 하거든요.  전 집에 오자마자 밥 먹고 바로 약 먹고 잤어요. 비록 대낮이지만",
          "original_form": "그리고 맞고 거기서 20분 대기 하거든요.  전 집에 오자마자 밥 먹고 바로 약 먹고 잤어요. 비록 대낮이지만",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035290.1.21",
          "form": "거기서도 대기하고 오는군요",
          "original_form": "거기서도 대기하고 오는군요",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035290.1.22",
          "form": "저도 맞으면 약먹고 바로 자야겠네요",
          "original_form": "저도 맞으면 약먹고 바로 자야겠네요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        }
      ]
    }
