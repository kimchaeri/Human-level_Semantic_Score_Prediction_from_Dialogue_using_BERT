      "utterance": [
        {
          "id": "MDRW2100031819.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031819.1.2",
          "form": "오늘 머드셧어요?",
          "original_form": "오늘 머드셧어요?",
          "speaker_id": "1",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031819.1.3",
          "form": "안녕하세요 저 오늘 저녁에 결혼식을 다녀와서 스테이크랑 뷔페 먹었네요",
          "original_form": "안녕하세요 저 오늘 저녁에 결혼식을 다녀와서 스테이크랑 뷔페 먹었네요",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031819.1.4",
          "form": "간만에 뷔페였어요",
          "original_form": "간만에 뷔페였어요",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031819.1.5",
          "form": "name1님은여??",
          "original_form": "&name1&님은여??",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031819.1.6",
          "form": "오 저는 연어초밥 먹엇어요!!",
          "original_form": "오 저는 연어초밥 먹엇어요!!",
          "speaker_id": "1",
          "time": "20210905 01:24"
        },
        {
          "id": "MDRW2100031819.1.7",
          "form": "스테이크 맛잇겟네요ㅠ",
          "original_form": "스테이크 맛잇겟네요ㅠ",
          "speaker_id": "1",
          "time": "20210905 01:24"
        },
        {
          "id": "MDRW2100031819.1.8",
          "form": "name1님도 맛있는거 드셨네요",
          "original_form": "&name1&님도 맛있는거 드셨네요",
          "speaker_id": "2",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100031819.1.9",
          "form": "스테이크 좀 식어서 아쉽긴 한데 맛있었어요",
          "original_form": "스테이크 좀 식어서 아쉽긴 한데 맛있었어요",
          "speaker_id": "2",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100031819.1.10",
          "form": "연어초밥 저도 참 좋아하는데요",
          "original_form": "연어초밥 저도 참 좋아하는데요",
          "speaker_id": "2",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100031819.1.11",
          "form": "스테이크는 식어도 맛잇죠",
          "original_form": "스테이크는 식어도 맛잇죠",
          "speaker_id": "1",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100031819.1.12",
          "form": "고기는 다 좋은거같아요 ㅋㅋ",
          "original_form": "고기는 다 좋은거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100031819.1.13",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210905 01:26"
        },
        {
          "id": "MDRW2100031819.1.14",
          "form": "연어도 있었으면 좋았을텐데",
          "original_form": "연어도 있었으면 좋았을텐데",
          "speaker_id": "2",
          "time": "20210905 01:26"
        },
        {
          "id": "MDRW2100031819.1.15",
          "form": "아쉽게 연어는 없더라고요",
          "original_form": "아쉽게 연어는 없더라고요",
          "speaker_id": "2",
          "time": "20210905 01:26"
        },
        {
          "id": "MDRW2100031819.1.16",
          "form": "name1님이 연어 말씀하셔서 좀 땡기네여",
          "original_form": "&name1&님이 연어 말씀하셔서 좀 땡기네여",
          "speaker_id": "2",
          "time": "20210905 01:27"
        },
        {
          "id": "MDRW2100031819.1.17",
          "form": "ㅋㅋ 연어 오랜만에 먹으니 맛잇더라구요",
          "original_form": "ㅋㅋ 연어 오랜만에 먹으니 맛잇더라구요",
          "speaker_id": "1",
          "time": "20210905 01:27"
        },
        {
          "id": "MDRW2100031819.1.18",
          "form": "여름이라 못먹었었어요",
          "original_form": "여름이라 못먹었었어요",
          "speaker_id": "2",
          "time": "20210905 01:27"
        },
        {
          "id": "MDRW2100031819.1.19",
          "form": "혹시나 싶어서용ㅋㅋㅋ",
          "original_form": "혹시나 싶어서용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:27"
        },
        {
          "id": "MDRW2100031819.1.20",
          "form": "아 해산물이라",
          "original_form": "아 해산물이라",
          "speaker_id": "1",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031819.1.21",
          "form": "살짝 그렇긴 하겟네여",
          "original_form": "살짝 그렇긴 하겟네여",
          "speaker_id": "1",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031819.1.22",
          "form": "혹시나 하는 마음엨ㅋㅋㅋ 근데 이제는 딱 먹기 좋은 때인거 같아요",
          "original_form": "혹시나 하는 마음엨ㅋㅋㅋ 근데 이제는 딱 먹기 좋은 때인거 같아요",
          "speaker_id": "2",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100031819.1.23",
          "form": "쌀쌀하니까 연어초밥도 먹고 오뎅탕도 먹고 싶어요",
          "original_form": "쌀쌀하니까 연어초밥도 먹고 오뎅탕도 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100031819.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:30"
        },
        {
          "id": "MDRW2100031819.1.25",
          "form": "헉 오뎅탕 땡겨요",
          "original_form": "헉 오뎅탕 땡겨요",
          "speaker_id": "1",
          "time": "20210905 01:33"
        },
        {
          "id": "MDRW2100031819.1.26",
          "form": "오뎅 말하니 붕어빵도 땡기네요 ㅋㅋㅋ",
          "original_form": "오뎅 말하니 붕어빵도 땡기네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:34"
        },
        {
          "id": "MDRW2100031819.1.27",
          "form": "저 잔치국수도 껴주세요",
          "original_form": "저 잔치국수도 껴주세요",
          "speaker_id": "2",
          "time": "20210905 01:34"
        },
        {
          "id": "MDRW2100031819.1.28",
          "form": "ㅋㅋㅋ 붕어빵 맛있죠 저 팥붕",
          "original_form": "ㅋㅋㅋ 붕어빵 맛있죠 저 팥붕",
          "speaker_id": "2",
          "time": "20210905 01:34"
        },
        {
          "id": "MDRW2100031819.1.29",
          "form": "오 저도 팥붕이요 ㅋㅋㅋ",
          "original_form": "오 저도 팥붕이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:36"
        },
        {
          "id": "MDRW2100031819.1.30",
          "form": "슈크림 먼가 좀 느끼해서 별로 안좋아해요",
          "original_form": "슈크림 먼가 좀 느끼해서 별로 안좋아해요",
          "speaker_id": "1",
          "time": "20210905 01:37"
        },
        {
          "id": "MDRW2100031819.1.31",
          "form": "슈크림은 팥붕이랑 좀 섞어먹어야 맛있고 역시 팥붕이져?",
          "original_form": "슈크림은 팥붕이랑 좀 섞어먹어야 맛있고 역시 팥붕이져?",
          "speaker_id": "2",
          "time": "20210905 01:37"
        },
        {
          "id": "MDRW2100031819.1.32",
          "form": "맛을 잘 아시네욬ㅋㅋㅋ",
          "original_form": "맛을 잘 아시네욬ㅋㅋㅋ",
