      "utterance": [
        {
          "id": "MDRW2100020133.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100020133.1.2",
          "form": "점심 맛잇게 드셧어요?",
          "original_form": "점심 맛잇게 드셧어요?",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100020133.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100020133.1.4",
          "form": "네 순두부찌개 먹었어요",
          "original_form": "네 순두부찌개 먹었어요",
          "speaker_id": "2",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100020133.1.5",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100020133.1.6",
          "form": "오 맛잇엇겟네요!!",
          "original_form": "오 맛잇엇겟네요!!",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100020133.1.7",
          "form": "아뇨.. 곧 라면먹으려고요 ㅎㅎㅎ",
          "original_form": "아뇨.. 곧 라면먹으려고요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100020133.1.8",
          "form": "어떤 라면 드세요?",
          "original_form": "어떤 라면 드세요?",
          "speaker_id": "2",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100020133.1.9",
          "form": "저는 스낵면 ㅁ먹을까 고민중이에오 ㅎㅎㅎ",
          "original_form": "저는 스낵면 ㅁ먹을까 고민중이에오 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100020133.1.10",
          "form": "좋아하시는 라면 잇으세여?",
          "original_form": "좋아하시는 라면 잇으세여?",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100020133.1.11",
          "form": "스낵면 맛있죠",
          "original_form": "스낵면 맛있죠",
          "speaker_id": "2",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100020133.1.12",
          "form": "전 컵누들 좋아해요",
          "original_form": "전 컵누들 좋아해요",
          "speaker_id": "2",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100020133.1.13",
          "form": "오.. 사이즈 어떤걸로 한그릇만드세여?",
          "original_form": "오.. 사이즈 어떤걸로 한그릇만드세여?",
          "speaker_id": "1",
          "time": "20210831 12:56"
        },
        {
          "id": "MDRW2100020133.1.14",
          "form": "컵누들은 작은 것만 나와서 두개 먹어요 ㅋㅋㅋ",
          "original_form": "컵누들은 작은 것만 나와서 두개 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:56"
        },
        {
          "id": "MDRW2100020133.1.15",
          "form": "특별히 좋아하는 맛이 잇으세여?\n다 맛잇돈데 ㅎㅎ",
          "original_form": "특별히 좋아하는 맛이 잇으세여?\n다 맛잇돈데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100020133.1.16",
          "form": "매운맛이랑 우동맛이요",
          "original_form": "매운맛이랑 우동맛이요",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100020133.1.17",
          "form": "스낵면말고 다른 라면 좋아하는거 잇으세요?",
          "original_form": "스낵면말고 다른 라면 좋아하는거 잇으세요?",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100020133.1.18",
          "form": "ㅋㅋㅋㅋ 저눈 불닭이랑 너구리도 좋아하고 짜장라면도 좋아해요",
          "original_form": "ㅋㅋㅋㅋ 저눈 불닭이랑 너구리도 좋아하고 짜장라면도 좋아해요",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100020133.1.19",
          "form": "불닭 일반맛이요?",
          "original_form": "불닭 일반맛이요?",
          "speaker_id": "2",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100020133.1.20",
          "form": "그거 치즈 녹여먹어요 ㅎㅎㅎ",
          "original_form": "그거 치즈 녹여먹어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100020133.1.21",
          "form": "그쵸 그냥 먹기엔 엄청 매워요 ㅋㅋㅋ",
          "original_form": "그쵸 그냥 먹기엔 엄청 매워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100020133.1.22",
          "form": "맞아요 ㅠㅠ 저는 위장이 그렇게 안튼튼해요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅠㅠ 저는 위장이 그렇게 안튼튼해요 ㅋㅋㅋ",
