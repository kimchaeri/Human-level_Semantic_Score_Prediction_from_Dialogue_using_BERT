      "utterance": [
        {
          "id": "MDRW2100019464.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019464.1.2",
          "form": "혹시 커뮤니티 활동 중이신가요?",
          "original_form": "혹시 커뮤니티 활동 중이신가요?",
          "speaker_id": "1",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019464.1.3",
          "form": "네네 여시해요 ㅋㅋ",
          "original_form": "네네 여시해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019464.1.4",
          "form": "아 여시하시는구나~",
          "original_form": "아 여시하시는구나~",
          "speaker_id": "1",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019464.1.5",
          "form": "거기서는 주로 어떤 정보 얻어요?",
          "original_form": "거기서는 주로 어떤 정보 얻어요?",
          "speaker_id": "1",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019464.1.6",
          "form": "진짜 모든 빅데이터",
          "original_form": "진짜 모든 빅데이터",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019464.1.7",
          "form": "맛집 화장품 물건들...",
          "original_form": "맛집 화장품 물건들...",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019464.1.8",
          "form": "와~ 진짜 다양하네요",
          "original_form": "와~ 진짜 다양하네요",
          "speaker_id": "1",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019464.1.9",
          "form": "커뮤니티 하세요?",
          "original_form": "커뮤니티 하세요?",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019464.1.10",
          "form": "저 게임 커뮤니티 해서 별 도움이 안돼요 ㅜㅜ",
          "original_form": "저 게임 커뮤니티 해서 별 도움이 안돼요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019464.1.11",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019464.1.12",
          "form": "여시 재밌어요 ㅋㅋㅋ",
          "original_form": "여시 재밌어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019464.1.13",
          "form": "맛집 같은건 꼭 멀리 있더라구요 ㅠㅠ",
          "original_form": "맛집 같은건 꼭 멀리 있더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019464.1.14",
          "form": "세상에 다양한 사람이 많구나.. 느껴요",
          "original_form": "세상에 다양한 사람이 많구나.. 느껴요",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019464.1.15",
          "form": "동네 맛집만 보이게 할 수도 있어요?",
          "original_form": "동네 맛집만 보이게 할 수도 있어요?",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019464.1.16",
          "form": "동네 사람들만 모인 달글이 있어요",
          "original_form": "동네 사람들만 모인 달글이 있어요",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019464.1.17",
          "form": "거기서 동네 정보 교류해요 댓글로",
          "original_form": "거기서 동네 정보 교류해요 댓글로",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019464.1.18",
          "form": "와 진짜 이용하기 편리하겠다",
          "original_form": "와 진짜 이용하기 편리하겠다",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019464.1.19",
          "form": "약간 동네 커뮤니티 탭이 따로 있나봐요?",
          "original_form": "약간 동네 커뮤니티 탭이 따로 있나봐요?",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019464.1.20",
          "form": "맞아요 ㅋㅋㅋ ㅋ그거 진짜 유용해요",
          "original_form": "맞아요 ㅋㅋㅋ ㅋ그거 진짜 유용해요",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019464.1.21",
          "form": "그리고 막 원플원으로 파는 거",
          "original_form": "그리고 막 원플원으로 파는 거",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019464.1.22",
          "form": "나눠서 한개씩 살 수도 있고요",
          "original_form": "나눠서 한개씩 살 수도 있고요",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019464.1.23",
          "form": "ㅋㅋㅋㅋ넘 편해여",
          "original_form": "ㅋㅋㅋㅋ넘 편해여",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019464.1.24",
          "form": "아 그거 진짜 부러웠어요 ㅜㅜ",
          "original_form": "아 그거 진짜 부러웠어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:45"
