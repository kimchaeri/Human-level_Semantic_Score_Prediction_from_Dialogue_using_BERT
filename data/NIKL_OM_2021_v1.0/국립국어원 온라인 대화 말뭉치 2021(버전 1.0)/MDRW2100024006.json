      "utterance": [
        {
          "id": "MDRW2100024006.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100024006.1.2",
          "form": "반가워요 여행 좋아하시나요?",
          "original_form": "반가워요 여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024006.1.3",
          "form": "네 ~ 여행좋아요",
          "original_form": "네 ~ 여행좋아요",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024006.1.4",
          "form": "이번주에 여행가려구요",
          "original_form": "이번주에 여행가려구요",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024006.1.5",
          "form": "최근에 강릉 가보셨어요~?",
          "original_form": "최근에 강릉 가보셨어요~?",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024006.1.6",
          "form": "최근에 강릉 못갔구 한 3년전쯤 갔습니다",
          "original_form": "최근에 강릉 못갔구 한 3년전쯤 갔습니다",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024006.1.7",
          "form": "테라로사 라는 곳에 가서 커피 마신 기억이 있네요\r\n한 4시간 걸려서 간것 같아요",
          "original_form": "테라로사 라는 곳에 가서 커피 마신 기억이 있네요\r\n한 4시간 걸려서 간것 같아요",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024006.1.8",
          "form": "네시간",
          "original_form": "네시간",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024006.1.9",
          "form": "저도 여기에서 가면 4,5시간 걸릴듯해요",
          "original_form": "저도 여기에서 가면 4,5시간 걸릴듯해요",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024006.1.10",
          "form": "어디서 출발하시나요?",
          "original_form": "어디서 출발하시나요?",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024006.1.11",
          "form": "테라로사 본점 가려했는데 뚜벅이는 못 갈 위치라고 하더라구요 ㅋㅋㅋ",
          "original_form": "테라로사 본점 가려했는데 뚜벅이는 못 갈 위치라고 하더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024006.1.12",
          "form": "대전이에요 ㅋㅋ",
          "original_form": "대전이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024006.1.13",
          "form": "대전에서도 그만큼 걸리는군요",
          "original_form": "대전에서도 그만큼 걸리는군요",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024006.1.14",
          "form": "상당히 먼거리입니다",
          "original_form": "상당히 먼거리입니다",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024006.1.15",
          "form": "ㅋㅋ강원도의 위엄..",
          "original_form": "ㅋㅋ강원도의 위엄..",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024006.1.16",
          "form": "어느지역에서 가셨어요?",
          "original_form": "어느지역에서 가셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024006.1.17",
          "form": "전 대구에서요 ㅎㅎ",
          "original_form": "전 대구에서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024006.1.18",
          "form": "대전 여행 안가봣는덱",
          "original_form": "대전 여행 안가봣는덱",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024006.1.19",
          "form": "앗 대고~",
          "original_form": "앗 대고~",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024006.1.20",
          "form": "카이스트에 축구하러 갓네요",
          "original_form": "카이스트에 축구하러 갓네요",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024006.1.21",
          "form": "여행올곳은 ... 아니지요 대전 ㅎㅎㅎㅎ",
          "original_form": "여행올곳은 ... 아니지요 대전 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024006.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024006.1.23",
          "form": "대구에서 거기까지 ㅋㅋ",
          "original_form": "대구에서 거기까지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024006.1.24",
          "form": "대전 명소 잘 보고가셨네요 ,, 스포츠까지",
          "original_form": "대전 명소 잘 보고가셨네요 ,, 스포츠까지",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024006.1.25",
          "form": "명소인가요?",
          "original_form": "명소인가요?",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024006.1.26",
          "form": "학생인가요?",
          "original_form": "학생인가요?",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024006.1.27",
          "form": "학생일때 가볼만한곳은 어디일까요?",
          "original_form": "학생일때 가볼만한곳은 어디일까요?",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024006.1.28",
          "form": "코로나만 아니면 유럽추천인데",
          "original_form": "코로나만 아니면 유럽추천인데",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100024006.1.29",
          "form": "직장인이에요 ㅎㅎ~",
          "original_form": "직장인이에요 ㅎㅎ~",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024006.1.30",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024006.1.31",
          "form": "대전엔 카이스트.. 연구소만 잔뜩해서요.. ㅋㅋㅋㅋㅋ",
          "original_form": "대전엔 카이스트.. 연구소만 잔뜩해서요.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024006.1.32",
          "form": "연구소 다니시나요,",
          "original_form": "연구소 다니시나요,",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024006.1.33",
          "form": "정출연?",
          "original_form": "정출연?",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024006.1.34",
          "form": "학생이세요?? 아니요 저는 연구소안다녀요 ㅋㅋ",
          "original_form": "학생이세요?? 아니요 저는 연구소안다녀요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024006.1.35",
          "form": "아녀 저도 일합니다 ㅋㅋ",
          "original_form": "아녀 저도 일합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024006.1.36",
          "form": "그렇군요 직장인이 가는 여행은 진정한 힐링이죠",
          "original_form": "그렇군요 직장인이 가는 여행은 진정한 힐링이죠",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024006.1.37",
          "form": "강릉 너무 부럽습니다",
          "original_form": "강릉 너무 부럽습니다",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024006.1.38",
          "form": "아 학생땐 아까말씀하신 유럽 여행 다녀왓어요 ㅎㅎ",
          "original_form": "아 학생땐 아까말씀하신 유럽 여행 다녀왓어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024006.1.39",
          "form": "강릉.. 바다가 보고싶어서요 ㅎㅎ 바다하면 동해바다 아니겠습니까 ㅎ",
          "original_form": "강릉.. 바다가 보고싶어서요 ㅎㅎ 바다하면 동해바다 아니겠습니까 ㅎ",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024006.1.40",
          "form": "여행 계획 잡으셔야죠~",
          "original_form": "여행 계획 잡으셔야죠~",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024006.1.41",
          "form": "저는 9월에 거제 벌베디어 예약해놧습니다",
          "original_form": "저는 9월에 거제 벌베디어 예약해놧습니다",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024006.1.42",
          "form": "뽀로로룸 없어서 그냥 오션뷰했습니다",
          "original_form": "뽀로로룸 없어서 그냥 오션뷰했습니다",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024006.1.43",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024006.1.44",
          "form": "곧 여행가시네요~~",
          "original_form": "곧 여행가시네요~~",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024006.1.45",
          "form": "뽀로로룸도잇나요? ㅋㅋㅋㅋ",
          "original_form": "뽀로로룸도잇나요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024006.1.46",
          "form": "가족여행이라 참 기대됩니다",
          "original_form": "가족여행이라 참 기대됩니다",
          "speaker_id": "2",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024006.1.47",
          "form": "있습니다 ㅋ뽀로로로 도배되어있어요",
          "original_form": "있습니다 ㅋ뽀로로로 도배되어있어요",
          "speaker_id": "2",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024006.1.48",
          "form": "제가모르는 세계 ㅋㅋㅋㅋㅋ",
          "original_form": "제가모르는 세계 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024006.1.49",
          "form": "애기들 놀기 좋다고 합니다",
          "original_form": "애기들 놀기 좋다고 합니다",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024006.1.50",
          "form": "좋은곳이네요 ,,,",
          "original_form": "좋은곳이네요 ,,,",
          "speaker_id": "1",
          "time": "20210830 21:34"
        }
      ]
