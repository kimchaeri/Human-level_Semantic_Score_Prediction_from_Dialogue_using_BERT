      "utterance": [
        {
          "id": "MDRW2100009238.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009238.1.2",
          "form": "거기도 비오나요?",
          "original_form": "거기도 비오나요?",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009238.1.3",
          "form": "아니요.. 아래지방 이신가봐요.. 비구름",
          "original_form": "아니요.. 아래지방 이신가봐요.. 비구름",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009238.1.4",
          "form": "올라오던ㄷ",
          "original_form": "올라오던ㄷ",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009238.1.5",
          "form": "날이 많이 시원해져서 좋네요",
          "original_form": "날이 많이 시원해져서 좋네요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009238.1.6",
          "form": "그러게요.. 올여름 진짜 최악..",
          "original_form": "그러게요.. 올여름 진짜 최악..",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009238.1.7",
          "form": "근데 갑자기 비 계속 오네요",
          "original_form": "근데 갑자기 비 계속 오네요",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009238.1.8",
          "form": "태풍이랑 장마가 갑자기 오네요",
          "original_form": "태풍이랑 장마가 갑자기 오네요",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009238.1.9",
          "form": "비피해는 없으세요? 얼마전 아랫지방 비 엄청 왔자나요",
          "original_form": "비피해는 없으세요? 얼마전 아랫지방 비 엄청 왔자나요",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009238.1.10",
          "form": "괜찮아요!",
          "original_form": "괜찮아요!",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009238.1.11",
          "form": "소나기가 종종 오는 거 말고는 피해없는 거같아요",
          "original_form": "소나기가 종종 오는 거 말고는 피해없는 거같아요",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009238.1.12",
          "form": "낼부터 또 비오네요.. 아..",
          "original_form": "낼부터 또 비오네요.. 아..",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009238.1.13",
          "form": "태풍이죠..",
          "original_form": "태풍이죠..",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009238.1.14",
          "form": "비오는 날은 집에 있어야하는데...",
          "original_form": "비오는 날은 집에 있어야하는데...",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009238.1.15",
          "form": "어디가시나요?",
          "original_form": "어디가시나요?",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009238.1.16",
          "form": "회사갑니당ㅠㅠ",
          "original_form": "회사갑니당ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009238.1.17",
          "form": "요즘 일교차 심해서 옷입기도 애매하지않나요?",
          "original_form": "요즘 일교차 심해서 옷입기도 애매하지않나요?",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009238.1.18",
          "form": "아..ㅋㅋ 먹고살랴면... 해야쥬..",
          "original_form": "아..ㅋㅋ 먹고살랴면... 해야쥬..",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009238.1.19",
          "form": "저는 땀쟁이라 무조건 반팔이요..",
          "original_form": "저는 땀쟁이라 무조건 반팔이요..",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009238.1.20",
          "form": "추위 많이 타시나요?",
          "original_form": "추위 많이 타시나요?",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009238.1.21",
          "form": "저도 한더위타는데 갑자기 기온 떨어지니 너무 춥더라구요",
          "original_form": "저도 한더위타는데 갑자기 기온 떨어지니 너무 춥더라구요",
          "speaker_id": "1",
          "time": "20210822 22:38"
        }
