        },
        {
          "id": "MDRW2100042410.1.2",
          "form": "난.. 자.세.가.이.상.한.가?",
          "original_form": "난.. 자.세.가.이.상.한.가?",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042410.1.3",
          "form": "별로 안아프네",
          "original_form": "별로 안아프네",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042410.1.4",
          "form": "왜오ㅑ",
          "original_form": "왜오ㅑ",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042410.1.5",
          "form": "어디하고 있는데??",
          "original_form": "어디하고 있는데??",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042410.1.6",
          "form": "목",
          "original_form": "목",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042410.1.7",
          "form": "목?",
          "original_form": "목?",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042410.1.8",
          "form": "ㅈ응응",
          "original_form": "ㅈ응응",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042410.1.9",
          "form": "그 폼롤러 울퉁불퉁 한거야??",
          "original_form": "그 폼롤러 울퉁불퉁 한거야??",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042410.1.10",
          "form": "도리도리 끄덕끄덕중",
          "original_form": "도리도리 끄덕끄덕중",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042410.1.11",
          "form": "ㄴㄴㄴㄴ",
          "original_form": "ㄴㄴㄴㄴ",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042410.1.12",
          "form": "그냥 ㅁ매끈한 스티로폼",
          "original_form": "그냥 ㅁ매끈한 스티로폼",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042410.1.13",
          "form": "아 난 울퉁불퉁 한걸로 해서 그런가보다",
          "original_form": "아 난 울퉁불퉁 한걸로 해서 그런가보다",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042410.1.14",
          "form": "다음에 우리집 와서 해봐",
          "original_form": "다음에 우리집 와서 해봐",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042410.1.15",
          "form": "진짜...",
          "original_form": "진짜...",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042410.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅋㅇㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042410.1.17",
          "form": "해보고 만ㄹ이아프면",
          "original_form": "해보고 만ㄹ이아프면",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.18",
          "form": "사야겠다",
          "original_form": "사야겠다",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.19",
          "form": "아 그",
          "original_form": "아 그",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.21",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.22",
          "form": "너 요가링 해봤어?",
          "original_form": "너 요가링 해봤어?",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.23",
          "form": "종아리에 끼우는거",
          "original_form": "종아리에 끼우는거",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.24",
          "form": "아 그 종아리에 하는거?",
          "original_form": "아 그 종아리에 하는거?",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.25",
          "form": "아니 해보진 않았어",
          "original_form": "아니 해보진 않았어",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.26",
          "form": "ㅇㅇㄹㄹ",
          "original_form": "ㅇㅇㄹㄹ",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.27",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.28",
          "form": "아그래?",
          "original_form": "아그래?",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.29",
          "form": "고등학교때 애들 많이 하고 다녔는데",
          "original_form": "고등학교때 애들 많이 하고 다녔는데",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.30",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042410.1.31",
          "form": "아니 난 많이 앉아있으니까",
          "original_form": "아니 난 많이 앉아있으니까",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042410.1.32",
          "form": "앉아있을때 끼고있으면 좋을거같아서",
          "original_form": "앉아있을때 끼고있으면 좋을거같아서",
          "speaker_id": "1",
          "time": "20210906 22:04"
        }
