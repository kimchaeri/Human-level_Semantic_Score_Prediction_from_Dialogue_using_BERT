      "utterance": [
        {
          "id": "MDRW2100044509.1.1",
          "form": "여행 가고 싶은 곳 있으세요?",
          "original_form": "여행 가고 싶은 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044509.1.2",
          "form": "전호캉스요~",
          "original_form": "전호캉스요~",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044509.1.3",
          "form": "추천 호텔은요?",
          "original_form": "추천 호텔은요?",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044509.1.4",
          "form": "ㅠㅠ 좋은곳 못가봐서",
          "original_form": "ㅠㅠ 좋은곳 못가봐서",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044509.1.5",
          "form": "5성급한번ㅋㅋ",
          "original_form": "5성급한번ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044509.1.6",
          "form": "가보고싶어용",
          "original_form": "가보고싶어용",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044509.1.7",
          "form": "스파되고",
          "original_form": "스파되고",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044509.1.8",
          "form": "그런곳",
          "original_form": "그런곳",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044509.1.9",
          "form": "전 롯데랑 힐튼 가봤어요",
          "original_form": "전 롯데랑 힐튼 가봤어요",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044509.1.10",
          "form": "지방러라ㅠ 롯데월드갈때 어디그근처에서 자본것같은데",
          "original_form": "지방러라ㅠ 롯데월드갈때 어디그근처에서 자본것같은데",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044509.1.11",
          "form": "그리 좋은곳은 아니었던것 같고ㅠㅠ",
          "original_form": "그리 좋은곳은 아니었던것 같고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044509.1.12",
          "form": "아직 스파되는비싼호텔 못가봤어여ㅠ",
          "original_form": "아직 스파되는비싼호텔 못가봤어여ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044509.1.13",
          "form": "스파되는 비싼 호텔이.어딘가요?",
          "original_form": "스파되는 비싼 호텔이.어딘가요?",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044509.1.14",
          "form": "찜해둔 곳 있으세요?",
          "original_form": "찜해둔 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044509.1.15",
          "form": "그렇다고 딱히찾아본곳도 없어여ㅠ못갈거알고",
          "original_form": "그렇다고 딱히찾아본곳도 없어여ㅠ못갈거알고",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044509.1.16",
          "form": "안찾아봤나봐요ㅋㅋ",
          "original_form": "안찾아봤나봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044509.1.17",
          "form": "롯데월드 갈때 있던 그 호텔은 좀 낡았죠? 하지만 서비스는.좋았어요",
          "original_form": "롯데월드 갈때 있던 그 호텔은 좀 낡았죠? 하지만 서비스는.좋았어요",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044509.1.18",
          "form": "그바로앞에있던 그거겧죠?ㅋ",
          "original_form": "그바로앞에있던 그거겧죠?ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044509.1.19",
          "form": "다들가시는그곳ㅋㅋㅋ",
          "original_form": "다들가시는그곳ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044509.1.20",
          "form": "네~ 낡은곳.",
          "original_form": "네~ 낡은곳.",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044509.1.21",
          "form": "ㅋㅋㅋㅋㅋ  낡았었나 석촌호수그어딘가에",
          "original_form": "ㅋㅋㅋㅋㅋ  낡았었나 석촌호수그어딘가에",
