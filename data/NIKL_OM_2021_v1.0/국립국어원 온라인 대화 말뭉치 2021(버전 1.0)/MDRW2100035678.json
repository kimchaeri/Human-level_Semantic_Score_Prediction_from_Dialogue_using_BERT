        },
        {
          "id": "MDRW2100035678.1.2",
          "form": "안녕하세요 저녁 드셨나요?",
          "original_form": "안녕하세요 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035678.1.3",
          "form": "네 name2님도 드셨나요?",
          "original_form": "네 &name2&님도 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035678.1.4",
          "form": "그럼요ㅎ 메뉴가 뭐였나요",
          "original_form": "그럼요ㅎ 메뉴가 뭐였나요",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035678.1.5",
          "form": "저는 어묵볶음이요 name2님은 뭐 드셨나요?",
          "original_form": "저는 어묵볶음이요 &name2&님은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035678.1.6",
          "form": "저는 어죽먹었어요",
          "original_form": "저는 어죽먹었어요",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035678.1.7",
          "form": "직접 하신 건가요?",
          "original_form": "직접 하신 건가요?",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035678.1.8",
          "form": "아니요 집에 있는거 먹었어요",
          "original_form": "아니요 집에 있는거 먹었어요",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035678.1.9",
          "form": "어죽은 직접 만드신건가요?",
          "original_form": "어죽은 직접 만드신건가요?",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035678.1.10",
          "form": "아ㅎ 당연히... 배달시켰죠",
          "original_form": "아ㅎ 당연히... 배달시켰죠",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035678.1.11",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035678.1.12",
          "form": "제가 먹을 수 있을 정도만 해요",
          "original_form": "제가 먹을 수 있을 정도만 해요",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035678.1.13",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035678.1.14",
          "form": "저에게 요리는 허락되지 않았어요ㅎ",
          "original_form": "저에게 요리는 허락되지 않았어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035678.1.15",
          "form": "파괴신임",
          "original_form": "파괴신임",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035678.1.16",
          "form": "주장기 요리가 뭔가요?",
          "original_form": "주장기 요리가 뭔가요?",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035678.1.17",
          "form": "저도 냉동음식 조리하거나 밀키트 정도 만드신거예요 그나마 잘하는 요리라면 김치찌개정도?",
          "original_form": "저도 냉동음식 조리하거나 밀키트 정도 만드신거예요 그나마 잘하는 요리라면 김치찌개정도?",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035678.1.18",
          "form": "헉 김치찌개라니",
          "original_form": "헉 김치찌개라니",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035678.1.19",
          "form": "고수인데요 충분히ㄷㄷㄷ",
          "original_form": "고수인데요 충분히ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035678.1.20",
          "form": "김치찌개엔 고기? 참치? 뭐 넣으셔요?",
          "original_form": "김치찌개엔 고기? 참치? 뭐 넣으셔요?",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035678.1.21",
          "form": "저는 그냥 고기 넣는거 좋아해요ㅋㅋ",
          "original_form": "저는 그냥 고기 넣는거 좋아해요ㅋㅋ",
