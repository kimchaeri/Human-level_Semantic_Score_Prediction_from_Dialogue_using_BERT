      "utterance": [
        {
          "id": "MDRW2100024160.1.1",
          "form": "유튜브자주 보시나요?",
          "original_form": "유튜브자주 보시나요?",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024160.1.2",
          "form": "네 오늘도 봤어요 ㅋㅋㅋㅋ",
          "original_form": "네 오늘도 봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024160.1.3",
          "form": "어떤카테고리를 주로 보시나요?",
          "original_form": "어떤카테고리를 주로 보시나요?",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024160.1.4",
          "form": "주로 음",
          "original_form": "주로 음",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024160.1.5",
          "form": "아이돌도 보고 동물들도 보고 다큐 이런것도 보고",
          "original_form": "아이돌도 보고 동물들도 보고 다큐 이런것도 보고",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024160.1.6",
          "form": "되게 여러가지 봐요",
          "original_form": "되게 여러가지 봐요",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024160.1.7",
          "form": "동물좋아하시나요?",
          "original_form": "동물좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 08:11"
        },
        {
          "id": "MDRW2100024160.1.8",
          "form": "네 동물 좋아해요",
          "original_form": "네 동물 좋아해요",
          "speaker_id": "2",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100024160.1.9",
          "form": "보면 귀여워서 힐링되더라구요",
          "original_form": "보면 귀여워서 힐링되더라구요",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100024160.1.10",
          "form": "저도 유튜브로 고양이채널보고 있어요ㅋㅋㅋ",
          "original_form": "저도 유튜브로 고양이채널보고 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:21"
        },
        {
          "id": "MDRW2100024160.1.11",
          "form": "보고있으면 기분이 좋아지더라구요ㅎㅎ",
          "original_form": "보고있으면 기분이 좋아지더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:21"
        },
        {
          "id": "MDRW2100024160.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024160.1.13",
          "form": "진짜 보는것만으로도 안정되고 힐링",
          "original_form": "진짜 보는것만으로도 안정되고 힐링",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100024160.1.14",
          "form": "ㅎㅎ 즐겨보시는 채널이 있으신가요?",
          "original_form": "ㅎㅎ 즐겨보시는 채널이 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024160.1.15",
          "form": "하하하랑 냥꼬리봐요",
          "original_form": "하하하랑 냥꼬리봐요",
          "speaker_id": "2",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100024160.1.16",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100024160.1.17",
          "form": "하하하랑 매탈남이요ㅎㅎㅎ",
          "original_form": "하하하랑 매탈남이요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:04"
        },
        {
          "id": "MDRW2100024160.1.18",
          "form": "매탈남",
          "original_form": "매탈남",
          "speaker_id": "2",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100024160.1.19",
          "form": "웃겨욬ㅋㅋㅋ",
          "original_form": "웃겨욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100024160.1.20",
          "form": "예전에 몇번 봤는데 고양이들 받아들이는 과정이 웃기더라구요",
          "original_form": "예전에 몇번 봤는데 고양이들 받아들이는 과정이 웃기더라구요",
          "speaker_id": "2",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100024160.1.21",
          "form": "그쵸?ㅋㅋㅋ\r\n꾸준히 챙겨보고 있어요ㅋㅋㅋㅋ",
          "original_form": "그쵸?ㅋㅋㅋ\r\n꾸준히 챙겨보고 있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:06"
        },
        {
          "id": "MDRW2100024160.1.22",
          "form": "정감가시는 분이셔서ㅋㅋ",
          "original_form": "정감가시는 분이셔서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:06"
        },
        {
          "id": "MDRW2100024160.1.23",
          "form": "네 또 금손이고",
          "original_form": "네 또 금손이고",
          "speaker_id": "2",
          "time": "20210831 14:06"
        },
        {
          "id": "MDRW2100024160.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:06"
        },
        {
          "id": "MDRW2100024160.1.25",
          "form": "매탈남이너 하하하 다 시골에 사셔서",
          "original_form": "매탈남이너 하하하 다 시골에 사셔서",
          "speaker_id": "2",
          "time": "20210831 14:07"
        },
        {
          "id": "MDRW2100024160.1.26",
          "form": "자연환경 보는맛도 있어요",
          "original_form": "자연환경 보는맛도 있어요",
          "speaker_id": "2",
          "time": "20210831 14:07"
        },
        {
          "id": "MDRW2100024160.1.27",
          "form": "눈썹이도 매탈남님이랑 조금 적응하고 있더라구요ㅋ",
          "original_form": "눈썹이도 매탈남님이랑 조금 적응하고 있더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210831 14:08"
        },
        {
          "id": "MDRW2100024160.1.28",
          "form": "오 눈썹이",
          "original_form": "오 눈썹이",
          "speaker_id": "2",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100024160.1.29",
          "form": "새로운 친구군요",
          "original_form": "새로운 친구군요",
          "speaker_id": "2",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100024160.1.30",
          "form": "나중에 한번 봐야겠어요",
          "original_form": "나중에 한번 봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100024160.1.31",
          "form": "네 재미있으실꺼에요~^^",
          "original_form": "네 재미있으실꺼에요~^^",
