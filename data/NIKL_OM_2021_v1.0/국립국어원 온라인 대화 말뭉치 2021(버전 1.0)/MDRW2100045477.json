      "utterance": [
        {
          "id": "MDRW2100045477.1.1",
          "form": "헉 저도요",
          "original_form": "헉 저도요",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045477.1.2",
          "form": "소라 없어서 못먹어요",
          "original_form": "소라 없어서 못먹어요",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045477.1.3",
          "form": "소라 너무 맛잇죠",
          "original_form": "소라 너무 맛잇죠",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045477.1.4",
          "form": "꼬들꼬들",
          "original_form": "꼬들꼬들",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045477.1.5",
          "form": "소라 회 멱고싶은데",
          "original_form": "소라 회 멱고싶은데",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045477.1.6",
          "form": "제주도나 해안가 아니면 없더라구요 ㅠ",
          "original_form": "제주도나 해안가 아니면 없더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045477.1.7",
          "form": "맞아요 잘 없어요 ㅠㅠ",
          "original_form": "맞아요 잘 없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045477.1.8",
          "form": "소라찜으로 아쉬움을 달래고 ㅠㅠ",
          "original_form": "소라찜으로 아쉬움을 달래고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045477.1.9",
          "form": "추워지면 다시 먹을 수 있겠네요",
          "original_form": "추워지면 다시 먹을 수 있겠네요",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045477.1.10",
          "form": "네 여름엔 아무래도 ㅠㅠ",
          "original_form": "네 여름엔 아무래도 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045477.1.11",
          "form": "어패류들 진짜 아쉬워요....",
          "original_form": "어패류들 진짜 아쉬워요....",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045477.1.12",
          "form": "맞아요 잘못먹으면 큰일나니까ㅠㅠ",
          "original_form": "맞아요 잘못먹으면 큰일나니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045477.1.13",
          "form": "그쵸... 생선은 가끔 무시하고 먹는데",
          "original_form": "그쵸... 생선은 가끔 무시하고 먹는데",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045477.1.14",
          "form": "조개류는 진짜 껴려지더라구요",
          "original_form": "조개류는 진짜 껴려지더라구요",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045477.1.15",
          "form": "맞아요 상하기도 워낙 잘 상하고 ㅠㅠ",
          "original_form": "맞아요 상하기도 워낙 잘 상하고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045477.1.16",
          "form": "여름철엔 독?이 있다고도 하고요 ㅠ",
          "original_form": "여름철엔 독?이 있다고도 하고요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045477.1.17",
          "form": "맞아요 ㅠㅠ",
