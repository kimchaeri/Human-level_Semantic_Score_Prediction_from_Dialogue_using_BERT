      "utterance": [
        {
          "id": "MDRW2100043096.1.1",
          "form": "최근에 영화 보신거 있으세요?",
          "original_form": "최근에 영화 보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210907 14:12"
        },
        {
          "id": "MDRW2100043096.1.2",
          "form": "저 얼마전에 3주 연속으로 봤어요ㅋㅋㅋ",
          "original_form": "저 얼마전에 3주 연속으로 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100043096.1.3",
          "form": "어떤 영화 봤어요??",
          "original_form": "어떤 영화 봤어요??",
          "speaker_id": "1",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043096.1.4",
          "form": "인질 올드 싱크홀이요",
          "original_form": "인질 올드 싱크홀이요",
          "speaker_id": "2",
          "time": "20210907 14:32"
        },
        {
          "id": "MDRW2100043096.1.5",
          "form": "저는 최근에 모가디슈봤어요",
          "original_form": "저는 최근에 모가디슈봤어요",
          "speaker_id": "1",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043096.1.6",
          "form": "아, 저도 봤었어요",
          "original_form": "아, 저도 봤었어요",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043096.1.7",
          "form": "재밌게 보셨나요?",
          "original_form": "재밌게 보셨나요?",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043096.1.8",
          "form": "네 기대했던거 보다 재밌었어요",
          "original_form": "네 기대했던거 보다 재밌었어요",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043096.1.9",
          "form": "기대중인 영화는 있으신가요?",
          "original_form": "기대중인 영화는 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043096.1.10",
          "form": "올드라는 영화를 보고 싶더라고요",
          "original_form": "올드라는 영화를 보고 싶더라고요",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043096.1.11",
          "form": "그 영화 재밌었나요?",
          "original_form": "그 영화 재밌었나요?",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043096.1.12",
          "form": "오, 올드\r\n저는 보통이었어요",
          "original_form": "오, 올드\r\n저는 보통이었어요",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043096.1.13",
          "form": "예고편은 흥미로웠는데",
          "original_form": "예고편은 흥미로웠는데",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043096.1.14",
          "form": "본 사람들 반응 대부분이 그냥그랬다고 하더라고요",
          "original_form": "본 사람들 반응 대부분이 그냥그랬다고 하더라고요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043096.1.15",
          "form": "그쵸? 예고편이 제일 재밌었어요",
          "original_form": "그쵸? 예고편이 제일 재밌었어요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043096.1.16",
          "form": "영화보면 은근히 예고편이 제일 재미있는 경우가 많더라고요 ㅋㅋ",
          "original_form": "영화보면 은근히 예고편이 제일 재미있는 경우가 많더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043096.1.17",
          "form": "맞아요ㅋㅋㅋ\r\n기대하는 영화 또 있으세요?",
          "original_form": "맞아요ㅋㅋㅋ\r\n기대하는 영화 또 있으세요?",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043096.1.18",
          "form": "그 마블영호 샹치도 개봉했더라고요",
          "original_form": "그 마블영호 샹치도 개봉했더라고요",
          "speaker_id": "1",
          "time": "20210907 21:30"
        }
