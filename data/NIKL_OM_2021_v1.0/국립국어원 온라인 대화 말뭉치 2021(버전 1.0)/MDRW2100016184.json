        },
        {
          "id": "MDRW2100016184.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016184.1.3",
          "form": "요즘 하시는 운동 있으신가요?",
          "original_form": "요즘 하시는 운동 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016184.1.4",
          "form": "홈트해보려구요",
          "original_form": "홈트해보려구요",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016184.1.5",
          "form": "영상 보면서 하시는건가요?",
          "original_form": "영상 보면서 하시는건가요?",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100016184.1.6",
          "form": "음 네네 보면서햅려구요",
          "original_form": "음 네네 보면서햅려구요",
          "speaker_id": "1",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100016184.1.7",
          "form": "오 봐둔 영상 있으세요?",
          "original_form": "오 봐둔 영상 있으세요?",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100016184.1.8",
          "form": "음 추천하실거있나요",
          "original_form": "음 추천하실거있나요",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100016184.1.9",
          "form": "저는 안해봤는데",
          "original_form": "저는 안해봤는데",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100016184.1.10",
          "form": "친구가 땅끄부부 칼소폭 좋았다고 했던게 기억나네요!",
          "original_form": "친구가 땅끄부부 칼소폭 좋았다고 했던게 기억나네요!",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100016184.1.11",
          "form": "그분들 유명하죠ㅎㅎ",
          "original_form": "그분들 유명하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100016184.1.12",
          "form": "해본적 있으신가요?",
          "original_form": "해본적 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100016184.1.13",
          "form": "이제 시작하려구요",
          "original_form": "이제 시작하려구요",
          "speaker_id": "1",
          "time": "20210828 15:37"
        },
        {
          "id": "MDRW2100016184.1.14",
          "form": "이전에 다이어트 해본적 있으세요?",
          "original_form": "이전에 다이어트 해본적 있으세요?",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100016184.1.15",
          "form": "다이어트는 해본적없어요ㅎㅎ",
          "original_form": "다이어트는 해본적없어요ㅎㅎ",
          "speaker_id": "1",
