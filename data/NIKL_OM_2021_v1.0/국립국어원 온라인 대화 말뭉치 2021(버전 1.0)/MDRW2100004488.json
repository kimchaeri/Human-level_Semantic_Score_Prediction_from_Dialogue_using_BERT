      "utterance": [
        {
          "id": "MDRW2100004488.1.1",
          "form": "좋아하는 영화 장르가 있나요?",
          "original_form": "좋아하는 영화 장르가 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004488.1.2",
          "form": "음악 영화를 좋아해요",
          "original_form": "음악 영화를 좋아해요",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004488.1.3",
          "form": "뮤지컬 영화도 좋고",
          "original_form": "뮤지컬 영화도 좋고",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004488.1.4",
          "form": "가장 기억에 남았던 영화가 뭔가요??",
          "original_form": "가장 기억에 남았던 영화가 뭔가요??",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004488.1.5",
          "form": "음 보헤미안 랩소디",
          "original_form": "음 보헤미안 랩소디",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004488.1.6",
          "form": "내용은 별생각 없이 봤는데",
          "original_form": "내용은 별생각 없이 봤는데",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004488.1.7",
          "form": "영화관에서 노래가 나올때",
          "original_form": "영화관에서 노래가 나올때",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004488.1.8",
          "form": "전율이 느꺄졌어요",
          "original_form": "전율이 느꺄졌어요",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004488.1.9",
          "form": "영화관 사운드가 최고긴 하죠",
          "original_form": "영화관 사운드가 최고긴 하죠",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004488.1.10",
          "form": "저는 해리포터 시리즈를 제일 재밌게 봤어요",
          "original_form": "저는 해리포터 시리즈를 제일 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004488.1.11",
          "form": "보신 적 있나요?",
          "original_form": "보신 적 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004488.1.12",
          "form": "아 그럼요!",
          "original_form": "아 그럼요!",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004488.1.13",
          "form": "완전 유명하니까요",
          "original_form": "완전 유명하니까요",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004488.1.14",
          "form": "저도 한때 푹 빠졌었는데",
          "original_form": "저도 한때 푹 빠졌었는데",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004488.1.15",
          "form": "특별히 좋아하시는 편 있나요?",
          "original_form": "특별히 좋아하시는 편 있나요?",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004488.1.16",
          "form": "저는 다 좋아해요!",
          "original_form": "저는 다 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004488.1.17",
          "form": "완결 났는데도 해리포터만한 영화가 없네요",
          "original_form": "완결 났는데도 해리포터만한 영화가 없네요",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004488.1.18",
          "form": "정말 애정하는 작품인가보네요",
          "original_form": "정말 애정하는 작품인가보네요",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004488.1.19",
          "form": "전시회같은건",
          "original_form": "전시회같은건",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004488.1.20",
          "form": "보러 다니시나요?",
          "original_form": "보러 다니시나요?",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004488.1.21",
          "form": "제가 사는 곳은 전시회가 별로 없어서",
          "original_form": "제가 사는 곳은 전시회가 별로 없어서",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004488.1.22",
          "form": "기회가 된다면 보러가고 싶네요",
          "original_form": "기회가 된다면 보러가고 싶네요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004488.1.23",
          "form": "어떤 전시회 좋아하시나요?",
          "original_form": "어떤 전시회 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004488.1.24",
          "form": "미술작품이나 사진전을 좋아해요",
          "original_form": "미술작품이나 사진전을 좋아해요",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004488.1.25",
          "form": "최근에 앨리스 달튼 브라운 전시를 가고싶어서",
          "original_form": "최근에 앨리스 달튼 브라운 전시를 가고싶어서",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004488.1.26",
          "form": "고민 중이에요 ㅎㅎ",
          "original_form": "고민 중이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004488.1.27",
          "form": "와 작품 직접 보면 좋을 것 같아요",
          "original_form": "와 작품 직접 보면 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004488.1.28",
          "form": "그쵸..",
          "original_form": "그쵸..",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004488.1.29",
          "form": "여름 느낌 나보여서",
          "original_form": "여름 느낌 나보여서",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004488.1.30",
          "form": "가서 파도그림보고 대리만족이라도 하고싶어요",
          "original_form": "가서 파도그림보고 대리만족이라도 하고싶어요",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004488.1.31",
          "form": "또 사진 촬영 가능한 작품이 몇개없는데",
          "original_form": "또 사진 촬영 가능한 작품이 몇개없는데",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004488.1.32",
          "form": "그점 덕분에 한산하게 즐길 수 있을거같고",
          "original_form": "그점 덕분에 한산하게 즐길 수 있을거같고",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004488.1.33",
          "form": "보고싶은 전시회 주제가 여름인가요?? 아님 파도??",
          "original_form": "보고싶은 전시회 주제가 여름인가요?? 아님 파도??",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004488.1.34",
          "form": "여름 휴양지가 주제인거같아요",
          "original_form": "여름 휴양지가 주제인거같아요",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004488.1.35",
          "form": "빛이 머무는 자리 라고 쓰여있던데",
          "original_form": "빛이 머무는 자리 라고 쓰여있던데",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004488.1.36",
          "form": "눈부실거같고 ㅠㅠ",
          "original_form": "눈부실거같고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:19"
        }
      ]
