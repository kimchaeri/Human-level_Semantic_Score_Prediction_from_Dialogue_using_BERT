      "utterance": [
        {
          "id": "MDRW2100034569.1.1",
          "form": "뭐하는지 봐라",
          "original_form": "뭐하는지 봐라",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034569.1.2",
          "form": "엠비스 쇼 음악중심 하네",
          "original_form": "엠비스 쇼 음악중심 하네",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100034569.1.3",
          "form": "ㅋㅋㅋ  누구 나오노",
          "original_form": "ㅋㅋㅋ  누구 나오노",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100034569.1.4",
          "form": "청공소년 모르겠다",
          "original_form": "청공소년 모르겠다",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100034569.1.5",
          "form": "청공소녀가 누고 ㅋㅋ",
          "original_form": "청공소녀가 누고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100034569.1.6",
          "form": "청공소년이네",
          "original_form": "청공소년이네",
          "speaker_id": "1",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100034569.1.7",
          "form": "5인조 남성그룹이네",
          "original_form": "5인조 남성그룹이네",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100034569.1.8",
          "form": "마지막 귀여운척ㅋ",
          "original_form": "마지막 귀여운척ㅋ",
          "speaker_id": "1",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100034569.1.9",
          "form": "ㅋㅋ봤구나",
          "original_form": "ㅋㅋ봤구나",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100034569.1.10",
          "form": "엠씨도 누군지 모르겠다",
          "original_form": "엠씨도 누군지 모르겠다",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034569.1.11",
          "form": "나 중간에 저 여자만 알겠다",
          "original_form": "나 중간에 저 여자만 알겠다",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034569.1.12",
          "form": "민주 누군데 아이돌이지?",
          "original_form": "민주 누군데 아이돌이지?",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034569.1.13",
          "form": "해체한 아이즈원",
          "original_form": "해체한 아이즈원",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034569.1.14",
          "form": "친절하게 마이크에 이름표 부착",
          "original_form": "친절하게 마이크에 이름표 부착",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034569.1.15",
          "form": "음방은 다 저렇더라",
          "original_form": "음방은 다 저렇더라",
          "speaker_id": "1",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100034569.1.16",
          "form": "아이즈원 해체했나 인기 괜찮았지 않았나",
          "original_form": "아이즈원 해체했나 인기 괜찮았지 않았나",
          "speaker_id": "2",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100034569.1.17",
          "form": "말 많아 가지고 끝내 해체",
          "original_form": "말 많아 가지고 끝내 해체",
          "speaker_id": "1",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100034569.1.18",
          "form": "근데 음악방송 엄청 일찍하네",
          "original_form": "근데 음악방송 엄청 일찍하네",
