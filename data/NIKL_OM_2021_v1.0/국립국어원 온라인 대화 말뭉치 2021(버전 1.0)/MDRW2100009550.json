      "utterance": [
        {
          "id": "MDRW2100009550.1.1",
          "form": "휴 다시마도 그렇개 꿀맛일 수가 없어요",
          "original_form": "휴 다시마도 그렇개 꿀맛일 수가 없어요",
          "speaker_id": "2",
          "time": "20210823 07:03"
        },
        {
          "id": "MDRW2100009550.1.2",
          "form": "근데 순두부열라면 드셔보셨나요?",
          "original_form": "근데 순두부열라면 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210823 07:03"
        },
        {
          "id": "MDRW2100009550.1.3",
          "form": "저는 아직이요 그렇게 든든하고 맛있다면서요!",
          "original_form": "저는 아직이요 그렇게 든든하고 맛있다면서요!",
          "speaker_id": "1",
          "time": "20210823 08:57"
        },
        {
          "id": "MDRW2100009550.1.4",
          "form": "네 ㅠㅠ 진짜 너무 맛있더리고요",
          "original_form": "네 ㅠㅠ 진짜 너무 맛있더리고요",
          "speaker_id": "2",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009550.1.5",
          "form": "완전 해장국이에요bb",
          "original_form": "완전 해장국이에요bb",
          "speaker_id": "2",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009550.1.6",
          "form": "한끼식사로 꼭 먹어봐야겠어요ㅠㅠ",
          "original_form": "한끼식사로 꼭 먹어봐야겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 09:14"
        },
        {
          "id": "MDRW2100009550.1.7",
          "form": "평이 다들 좋던데 후추가 포인트라면서요?",
          "original_form": "평이 다들 좋던데 후추가 포인트라면서요?",
          "speaker_id": "1",
          "time": "20210823 09:15"
        },
        {
          "id": "MDRW2100009550.1.8",
          "form": "아 맞아요 ㅠㅠ너무 시원해요",
          "original_form": "아 맞아요 ㅠㅠ너무 시원해요",
          "speaker_id": "2",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100009550.1.9",
          "form": "슬슬 추워지는 지금 날씨에 딱이네요",
          "original_form": "슬슬 추워지는 지금 날씨에 딱이네요",
          "speaker_id": "2",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100009550.1.10",
          "form": "캬..술이 땅겨요..",
          "original_form": "캬..술이 땅겨요..",
          "speaker_id": "2",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100009550.1.11",
          "form": "술좋아하시나요?",
          "original_form": "술좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100009550.1.12",
          "form": "술 좋아하죠 ㅠㅠ",
          "original_form": "술 좋아하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 18:43"
        },
        {
          "id": "MDRW2100009550.1.13",
          "form": "건강생각해서 못마시지만",
          "original_form": "건강생각해서 못마시지만",
          "speaker_id": "2",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100009550.1.14",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100009550.1.15",
          "form": "저는 알쓰예요",
          "original_form": "저는 알쓰예요",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100009550.1.16",
          "form": "안마신지도 오래되었구 마실때도 소주 세잔정도 마셨어요",
          "original_form": "안마신지도 오래되었구 마실때도 소주 세잔정도 마셨어요",
          "speaker_id": "1",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100009550.1.17",
          "form": "술하고 어울리는 메뉴중 뭐가 제일이세요?",
          "original_form": "술하고 어울리는 메뉴중 뭐가 제일이세요?",
          "speaker_id": "1",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100009550.1.18",
          "form": "흐음.. 저는",
          "original_form": "흐음.. 저는",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100009550.1.19",
          "form": "감자 마니아라 감튀가 재일 좋더라고요 ㅋㅋㅋ",
          "original_form": "감자 마니아라 감튀가 재일 좋더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009550.1.20",
          "form": "라면도 좋아하고요",
          "original_form": "라면도 좋아하고요",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009550.1.21",
          "form": "감튀는 역시 맥주죠?",
          "original_form": "감튀는 역시 맥주죠?",
          "speaker_id": "1",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100009550.1.22",
          "form": "맥주도 좋지만 하이볼 아세요?!",
          "original_form": "맥주도 좋지만 하이볼 아세요?!",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100009550.1.23",
          "form": "상큼~! 해서 너무 잘어울려요",
          "original_form": "상큼~! 해서 너무 잘어울려요",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100009550.1.24",
          "form": "하이볼! 일본에서 자주 봤어요 깔끔한맛이였던거 같은데 맞나요?ㅋㅋㅋ",
          "original_form": "하이볼! 일본에서 자주 봤어요 깔끔한맛이였던거 같은데 맞나요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100009550.1.25",
          "form": "레몬향이 많이나서 맛맀더라고요 ㅎㅎ 깔끔한것도 맞고여!!",
          "original_form": "레몬향이 많이나서 맛맀더라고요 ㅎㅎ 깔끔한것도 맞고여!!",
          "speaker_id": "2",
          "time": "20210823 21:02"
        },
        {
          "id": "MDRW2100009550.1.26",
          "form": "그거 말고도 아이셔에 이슬이라고 작년에 나온건가? 되게 맛있는 소주 있는데",
          "original_form": "그거 말고도 아이셔에 이슬이라고 작년에 나온건가? 되게 맛있는 소주 있는데",
          "speaker_id": "2",
          "time": "20210823 21:02"
        },
        {
          "id": "MDRW2100009550.1.27",
          "form": "그것도 엄청 좋아해요 ㅜㅜ",
          "original_form": "그것도 엄청 좋아해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 21:02"
        },
        {
          "id": "MDRW2100009550.1.28",
          "form": "맛잘알이시더니 술도 안주도 잘알이시네요",
          "original_form": "맛잘알이시더니 술도 안주도 잘알이시네요",
          "speaker_id": "1",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100009550.1.29",
          "form": "겨울은 길거리음식의 계절인데 ㅠㅠ",
