      "utterance": [
        {
          "id": "MDRW2100009300.1.1",
          "form": "최근에 영화 보신거 있으세요?",
          "original_form": "최근에 영화 보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009300.1.2",
          "form": "거의 매일 봐요!!",
          "original_form": "거의 매일 봐요!!",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009300.1.3",
          "form": "오 뭐보셨어요?",
          "original_form": "오 뭐보셨어요?",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009300.1.4",
          "form": "음 요즘 보는건",
          "original_form": "음 요즘 보는건",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009300.1.5",
          "form": "죽음보다 무서운 비밀",
          "original_form": "죽음보다 무서운 비밀",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009300.1.6",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009300.1.7",
          "form": "아 아니요",
          "original_form": "아 아니요",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009300.1.8",
          "form": "무슨내용인가요?",
          "original_form": "무슨내용인가요?",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009300.1.9",
          "form": "음 나치 잔당에 대한??",
          "original_form": "음 나치 잔당에 대한??",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.10",
          "form": "그냥 자기전에",
          "original_form": "그냥 자기전에",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.11",
          "form": "보면 잠 잘오더라고요 ㅋㅋㅋㅋ",
          "original_form": "보면 잠 잘오더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.12",
          "form": "아 ㅋㅋ 그런 영화들 있어요",
          "original_form": "아 ㅋㅋ 그런 영화들 있어요",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.13",
          "form": "네 그 피어스트리트도 재밌다던데",
          "original_form": "네 그 피어스트리트도 재밌다던데",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.14",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.15",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.16",
          "form": "넷플릭스로 보시는건가요?",
          "original_form": "넷플릭스로 보시는건가요?",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.17",
          "form": "네 ㅋㅋ 넷플 뽕뽑아야죠",
          "original_form": "네 ㅋㅋ 넷플 뽕뽑아야죠",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.18",
          "form": "쓰세요?",
          "original_form": "쓰세요?",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.19",
          "form": "네 저도 결제만 하고 있어요",
          "original_form": "네 저도 결제만 하고 있어요",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009300.1.20",
          "form": "전 잘 안봐서 ㅎㅎ",
          "original_form": "전 잘 안봐서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009300.1.21",
          "form": "네 잘 안보게되죠 ㅋㅋㅋ",
          "original_form": "네 잘 안보게되죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009300.1.22",
          "form": "저도 한참 안 보다가",
          "original_form": "저도 한참 안 보다가",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009300.1.23",
          "form": "요즘 자기 전에 봐요",
          "original_form": "요즘 자기 전에 봐요",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009300.1.24",
          "form": "볼게 별로 없는 거 같아요",
          "original_form": "볼게 별로 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009300.1.25",
          "form": "네 저도 그냥 한국 드라마 좀 틀어놔요 ㅋㅋ",
          "original_form": "네 저도 그냥 한국 드라마 좀 틀어놔요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009300.1.26",
          "form": "왓챠도 보세요?",
          "original_form": "왓챠도 보세요?",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009300.1.27",
          "form": "네 그것도 보긴하는데",
          "original_form": "네 그것도 보긴하는데",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009300.1.28",
          "form": "더 잘 안봐요 ㅋㅋ",
          "original_form": "더 잘 안봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009300.1.29",
          "form": "전 아예 끊었어요 ㅎㅎ",
          "original_form": "전 아예 끊었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009300.1.30",
          "form": "그게 나을거 같기도 해요 ㅋㅋㅋ",
          "original_form": "그게 나을거 같기도 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:18"
        }
      ]
