      "utterance": [
        {
          "id": "MDRW2100011925.1.1",
          "form": "네네랑 교촌이요",
          "original_form": "네네랑 교촌이요",
          "speaker_id": "2",
          "time": "20210824 21:48"
        },
        {
          "id": "MDRW2100011925.1.2",
          "form": "오 교촌에서 뭐 좋아하시나요?",
          "original_form": "오 교촌에서 뭐 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100011925.1.3",
          "form": "저는 허니콤보 좋아합니다!",
          "original_form": "저는 허니콤보 좋아합니다!",
          "speaker_id": "1",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100011925.1.4",
          "form": "저는 레드콤보요 ㅋㅋ",
          "original_form": "저는 레드콤보요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100011925.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100011925.1.6",
          "form": "네네는 뭐 좋아하세요?",
          "original_form": "네네는 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100011925.1.7",
          "form": "저는 스노윙 야채맛...",
          "original_form": "저는 스노윙 야채맛...",
          "speaker_id": "1",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100011925.1.8",
          "form": "네네는 아이들 땜에 먹는거라 후반양반이여ㅠ",
          "original_form": "네네는 아이들 땜에 먹는거라 후반양반이여ㅠ",
          "speaker_id": "2",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100011925.1.9",
          "form": "오 네네 후라이드도 맛있나요?",
          "original_form": "오 네네 후라이드도 맛있나요?",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100011925.1.10",
          "form": "양념은 맛나던데!!",
          "original_form": "양념은 맛나던데!!",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100011925.1.11",
          "form": "네 바삭하고 맛있어요",
          "original_form": "네 바삭하고 맛있어요",
          "speaker_id": "2",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100011925.1.12",
          "form": "나중에 먹어봐야겠네요!",
          "original_form": "나중에 먹어봐야겠네요!",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100011925.1.13",
          "form": "마랑탕 드셔보셨나요?",
          "original_form": "마랑탕 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100011925.1.14",
          "form": "아니요 아직요ㅎ",
          "original_form": "아니요 아직요ㅎ",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100011925.1.15",
          "form": "아 저번에 먹어봤는데 맛있더라구요ㅎ",
          "original_form": "아 저번에 먹어봤는데 맛있더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100011925.1.16",
          "form": "카페 자주 가시나요?",
          "original_form": "카페 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100011925.1.17",
          "form": "주말엔 한번씩. 가요. 애땜에 평일엔 잘 안 나가거든요ㅠ코로나 때문에ㅠ",
          "original_form": "주말엔 한번씩. 가요. 애땜에 평일엔 잘 안 나가거든요ㅠ코로나 때문에ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100011925.1.18",
          "form": "오 주로 어떤 음료 드시나요?",
          "original_form": "오 주로 어떤 음료 드시나요?",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100011925.1.19",
          "form": "주로 프라푸치노 종류로~ 밖에서 먹을땐 달달구리로",
          "original_form": "주로 프라푸치노 종류로~ 밖에서 먹을땐 달달구리로",
          "speaker_id": "2",
          "time": "20210825 23:51"
        }
      ]
