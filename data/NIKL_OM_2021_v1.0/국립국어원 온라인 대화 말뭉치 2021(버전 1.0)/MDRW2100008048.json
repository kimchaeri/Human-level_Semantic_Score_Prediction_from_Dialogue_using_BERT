        {
          "id": "MDRW2100008048.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100008048.1.2",
          "form": "아니요 혹시 반려동물 있으세요",
          "original_form": "아니요 혹시 반려동물 있으세요",
          "speaker_id": "2",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008048.1.3",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008048.1.4",
          "form": "키우고싶은 동물은 있으세요?",
          "original_form": "키우고싶은 동물은 있으세요?",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008048.1.5",
          "form": "강아지여",
          "original_form": "강아지여",
          "speaker_id": "1",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008048.1.6",
          "form": "특별한 종도 있으세요 요새 푸들이 귀엽더라고요",
          "original_form": "특별한 종도 있으세요 요새 푸들이 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008048.1.7",
          "form": "특히 실버푸들이 귀엽더라고요",
          "original_form": "특히 실버푸들이 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210822 13:43"
        },
        {
          "id": "MDRW2100008048.1.8",
          "form": "그런것도 있나요?",
          "original_form": "그런것도 있나요?",
          "speaker_id": "1",
          "time": "20210822 13:45"
        },
        {
          "id": "MDRW2100008048.1.9",
          "form": "푸들인데 색이 회색이더라고요",
          "original_form": "푸들인데 색이 회색이더라고요",
          "speaker_id": "2",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008048.1.10",
          "form": "일반적인 흰색이나 갈색이 아니라",
          "original_form": "일반적인 흰색이나 갈색이 아니라",
          "speaker_id": "2",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008048.1.11",
          "form": "독특하다는생각이듭니다.",
          "original_form": "독특하다는생각이듭니다.",
          "speaker_id": "1",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008048.1.12",
          "form": "그만큼 비싸고 따로 분양받거나 해야하지마뇨",
          "original_form": "그만큼 비싸고 따로 분양받거나 해야하지마뇨",
          "speaker_id": "2",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008048.1.13",
          "form": "만요",
          "original_form": "만요",
          "speaker_id": "2",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008048.1.14",
          "form": "그래도 푸들이 똑똑하다고 하더라고요",
          "original_form": "그래도 푸들이 똑똑하다고 하더라고요",
          "speaker_id": "2",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008048.1.15",
          "form": "푸들 정신없죠?",
          "original_form": "푸들 정신없죠?",
          "speaker_id": "1",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008048.1.16",
          "form": "아이큐가 높고 공감 능력이 좋다고하더라고요",
          "original_form": "아이큐가 높고 공감 능력이 좋다고하더라고요",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008048.1.17",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
