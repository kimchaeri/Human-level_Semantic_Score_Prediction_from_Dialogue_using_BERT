      "utterance": [
        {
          "id": "MDRW2100038036.1.1",
          "form": "언니 양치 구석구석 잘 하구요",
          "original_form": "언니 양치 구석구석 잘 하구요",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100038036.1.2",
          "form": "얼른 낸네 고 코 하시라요",
          "original_form": "얼른 낸네 고 코 하시라요",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100038036.1.3",
          "form": "나는 이제 사러 갑니다",
          "original_form": "나는 이제 사러 갑니다",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100038036.1.4",
          "form": "일어나요 언니~ 새 아침이 밝았어요",
          "original_form": "일어나요 언니~ 새 아침이 밝았어요",
          "speaker_id": "1",
          "time": "20210902 06:30"
        },
        {
          "id": "MDRW2100038036.1.5",
          "form": "그래 아침을 늘 너와 맞이하는구나",
          "original_form": "그래 아침을 늘 너와 맞이하는구나",
          "speaker_id": "2",
          "time": "20210902 08:59"
        },
        {
          "id": "MDRW2100038036.1.6",
          "form": "푹 잤나",
          "original_form": "푹 잤나",
          "speaker_id": "2",
          "time": "20210902 08:59"
        },
        {
          "id": "MDRW2100038036.1.7",
          "form": "난 어제 한 스무 번은 깬 듯",
          "original_form": "난 어제 한 스무 번은 깬 듯",
          "speaker_id": "2",
          "time": "20210902 08:59"
        },
        {
          "id": "MDRW2100038036.1.8",
          "form": "와요??",
          "original_form": "와요??",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100038036.1.9",
          "form": "턱아파가??ㅋㅋㅋ",
          "original_form": "턱아파가??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100038036.1.10",
          "form": "지는 꿀잠 잤구먼요~",
          "original_form": "지는 꿀잠 잤구먼요~",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100038036.1.11",
          "form": "내한테 인사 안하고 가서 그래여 ㅋㅋㅋ",
          "original_form": "내한테 인사 안하고 가서 그래여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100038036.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038036.1.13",
          "form": "잘잤네 ㅋㅋㄱ부럽다",
          "original_form": "잘잤네 ㅋㅋㄱ부럽다",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038036.1.14",
          "form": "난 안그래도 화장실간다고 한두번은 깨는데",
          "original_form": "난 안그래도 화장실간다고 한두번은 깨는데",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038036.1.15",
          "form": "어제는 계속 선잠잔듯",
          "original_form": "어제는 계속 선잠잔듯",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038036.1.16",
          "form": "꿈꾸고 깨고 꿈꾸고 깨고",
          "original_form": "꿈꾸고 깨고 꿈꾸고 깨고",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100038036.1.17",
          "form": "저녁에 뭐먹고자면 나도 화장실간다고 5시쯤 꼭깨여",
          "original_form": "저녁에 뭐먹고자면 나도 화장실간다고 5시쯤 꼭깨여",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038036.1.18",
          "form": "언니도 야식먹음 안되긋구만요~",
          "original_form": "언니도 야식먹음 안되긋구만요~",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038036.1.19",
          "form": "엉 어제 야식 먹어서 그런가",
          "original_form": "엉 어제 야식 먹어서 그런가",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100038036.1.20",
          "form": "여튼 너무 자주깨서",
          "original_form": "여튼 너무 자주깨서",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100038036.1.21",
          "form": "잔거같지도 않다",
          "original_form": "잔거같지도 않다",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100038036.1.22",
          "form": "그렇겠네요ㅠ",
          "original_form": "그렇겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100038036.1.23",
          "form": "오늘도 늦겠네요;",
          "original_form": "오늘도 늦겠네요;",
          "speaker_id": "1",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100038036.1.24",
          "form": "아니 선잠을 자서 일찍 일어났음",
          "original_form": "아니 선잠을 자서 일찍 일어났음",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100038036.1.25",
          "form": "오늘은 5분 일찍 출근했다",
          "original_form": "오늘은 5분 일찍 출근했다",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100038036.1.26",
          "form": "아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100038036.1.27",
          "form": "일찍일어나서 5분일찍이라 ㅋㅋㅋㅋ",
          "original_form": "일찍일어나서 5분일찍이라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100038036.1.28",
          "form": "오늘은 1분남겨놓고 세이프 아녀요??ㅋㅋ",
          "original_form": "오늘은 1분남겨놓고 세이프 아녀요??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100038036.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038036.1.30",
          "form": "늘 간당간당하지",
          "original_form": "늘 간당간당하지",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038036.1.31",
          "form": "나 예전에 일할때 같이일하는덩생이 이랬음 ㅋㅋㅋㅋ",
          "original_form": "나 예전에 일할때 같이일하는덩생이 이랬음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038036.1.32",
          "form": "출근하고 1초지나면 딱 정각에서 넘어감 ㅋㅋㅋㅋ",
          "original_form": "출근하고 1초지나면 딱 정각에서 넘어감 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038036.1.33",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ 글치",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ 글치",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038036.1.34",
          "form": "도착만 간당간당",
          "original_form": "도착만 간당간당",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038036.1.35",
          "form": "옷갈아입고 나오면 시간 지나있다 ㅋㅋㅋㅋ",
          "original_form": "옷갈아입고 나오면 시간 지나있다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038036.1.36",
          "form": "일한다고 정신이 없나??",
          "original_form": "일한다고 정신이 없나??",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100038036.1.37",
          "form": "와따 언니",
          "original_form": "와따 언니",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100038036.1.38",
          "form": "오늘 손님 계속 있어서;;",
          "original_form": "오늘 손님 계속 있어서;;",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100038036.1.39",
          "form": "늠 피곤허구만요ㅠ",
          "original_form": "늠 피곤허구만요ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100038036.1.40",
          "form": "아 오늘 손님 있어서 얘기 못한거가??",
          "original_form": "아 오늘 손님 있어서 얘기 못한거가??",
          "speaker_id": "2",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100038036.1.41",
          "form": "괜히 대타 뛰었네",
          "original_form": "괜히 대타 뛰었네",
          "speaker_id": "2",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100038036.1.42",
          "form": "토요일날 영어 학원간다면 있는데 같이 갈래??",
          "original_form": "토요일날 영어 학원간다면 있는데 같이 갈래??",
          "speaker_id": "2",
