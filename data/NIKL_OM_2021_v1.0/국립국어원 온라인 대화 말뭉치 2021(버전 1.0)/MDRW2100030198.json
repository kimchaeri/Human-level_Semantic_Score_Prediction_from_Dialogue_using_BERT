      "utterance": [
        {
          "id": "MDRW2100030198.1.1",
          "form": "너는 그 벨트 계속 고민중이니",
          "original_form": "너는 그 벨트 계속 고민중이니",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030198.1.2",
          "form": "의상수선실 가면",
          "original_form": "의상수선실 가면",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030198.1.3",
          "form": "ㅋㅋㅋㅋ응 요즘 벨트랑 신발 고민중이야",
          "original_form": "ㅋㅋㅋㅋ응 요즘 벨트랑 신발 고민중이야",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030198.1.4",
          "form": "벨트 구멍 더뚫어주거든",
          "original_form": "벨트 구멍 더뚫어주거든",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030198.1.5",
          "form": "마감까지 해죽ᆢ",
          "original_form": "마감까지 해죽ᆢ",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030198.1.6",
          "form": "신발은 어떤거야?",
          "original_form": "신발은 어떤거야?",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030198.1.7",
          "form": "수선실도 한번 생각해봐야겠다 고마워",
          "original_form": "수선실도 한번 생각해봐야겠다 고마워",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030198.1.8",
          "form": "신발은 아직 품목도 안 정했지 뭐야ㅋㅋㅋ",
          "original_form": "신발은 아직 품목도 안 정했지 뭐야ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030198.1.9",
          "form": "근데 사이즈 좀 넉넉한 걸로 사려구",
          "original_form": "근데 사이즈 좀 넉넉한 걸로 사려구",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030198.1.10",
          "form": "운동화 살꺼니??",
          "original_form": "운동화 살꺼니??",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030198.1.11",
          "form": "운동화는 아직 수명이 있어서 보류고",
          "original_form": "운동화는 아직 수명이 있어서 보류고",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030198.1.12",
          "form": "난 운동화 밑창이 닳아서",
          "original_form": "난 운동화 밑창이 닳아서",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030198.1.13",
          "form": "비오면 빙판길 걷는 느낌이야ㅋㅋㄱㅋ",
          "original_form": "비오면 빙판길 걷는 느낌이야ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030198.1.14",
          "form": "워커나 로퍼나 스니커즈처럼 좀 깔끔한 거 찾아보ㅕ구",
          "original_form": "워커나 로퍼나 스니커즈처럼 좀 깔끔한 거 찾아보ㅕ구",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030198.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ그러다가 머리깨지면 어떡해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러다가 머리깨지면 어떡해",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030198.1.16",
          "form": "얼른 바꿔야겠는걸",
          "original_form": "얼른 바꿔야겠는걸",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030198.1.17",
          "form": "스니커즈 하니까",
          "original_form": "스니커즈 하니까",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030198.1.18",
          "form": "저번에 그 브랜드는 어때",
          "original_form": "저번에 그 브랜드는 어때",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030198.1.19",
          "form": "이름이 뭐더라",
          "original_form": "이름이 뭐더라",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030198.1.20",
          "form": "캐치볼",
          "original_form": "캐치볼",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030198.1.21",
          "form": "ㅋㅋㅋㅋㄱ고새 까먹었어",
          "original_form": "ㅋㅋㅋㅋㄱ고새 까먹었어",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030198.1.22",
          "form": "거기꺼 평이 좋더라고",
          "original_form": "거기꺼 평이 좋더라고",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030198.1.23",
          "form": "근데 가격이 비싸서",
          "original_form": "근데 가격이 비싸서",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030198.1.24",
          "form": "고민이 돼",
          "original_form": "고민이 돼",
          "speaker_id": "2",
          "time": "20210903 23:01"
        }
      ]
