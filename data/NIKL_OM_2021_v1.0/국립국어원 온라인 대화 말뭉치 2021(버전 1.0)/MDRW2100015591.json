      "utterance": [
        {
          "id": "MDRW2100015591.1.1",
          "form": "그리고 그 근처에 저렴이 칼국수집",
          "original_form": "그리고 그 근처에 저렴이 칼국수집",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015591.1.2",
          "form": "맞아요ㅜㅜ진짜 이삭과 다른맛이에요",
          "original_form": "맞아요ㅜㅜ진짜 이삭과 다른맛이에요",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015591.1.3",
          "form": "할아버지 손칼국수라고 있는데 여기도 저렴한데 맛있어요!",
          "original_form": "할아버지 손칼국수라고 있는데 여기도 저렴한데 맛있어요!",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015591.1.4",
          "form": "김치도맛있어요ㅋㅋ",
          "original_form": "김치도맛있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015591.1.5",
          "form": "올랐는지모르겠는데 칼국수 3500원이에요",
          "original_form": "올랐는지모르겠는데 칼국수 3500원이에요",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015591.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015591.1.7",
          "form": "우와 진짜 저렴하네요",
          "original_form": "우와 진짜 저렴하네요",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015591.1.8",
          "form": "저 오늘 저녁에 칼국수 먹었는데",
          "original_form": "저 오늘 저녁에 칼국수 먹었는데",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015591.1.9",
          "form": "와 맛있으셨겠어요",
          "original_form": "와 맛있으셨겠어요",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015591.1.10",
          "form": "보통 한그릇 8-9천원하더라구요",
          "original_form": "보통 한그릇 8-9천원하더라구요",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015591.1.11",
          "form": "맞아요 저 오늘 8천원에 먹었답니다",
          "original_form": "맞아요 저 오늘 8천원에 먹었답니다",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015591.1.12",
          "form": "하 근데 이런 날씨는 진짜 국물 먹어줘야하죠",
          "original_form": "하 근데 이런 날씨는 진짜 국물 먹어줘야하죠",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015591.1.13",
          "form": "그집은 할아부지가 69년도부터 3대째하고계시대요...ㅋㅋ건물주시려나",
          "original_form": "그집은 할아부지가 69년도부터 3대째하고계시대요...ㅋㅋ건물주시려나",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015591.1.14",
          "form": "맞아요ㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015591.1.15",
          "form": "마라탕이랑 고민하다가 칼국수 먹었어요ㅎㅎ",
          "original_form": "마라탕이랑 고민하다가 칼국수 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015591.1.16",
          "form": "마라탕 맛있죠!!!",
          "original_form": "마라탕 맛있죠!!!",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015591.1.17",
          "form": "한때 엄청 먹다요즘의",
          "original_form": "한때 엄청 먹다요즘의",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015591.1.18",
          "form": "은 시들해졌어요ㅜㅜ",
          "original_form": "은 시들해졌어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015591.1.19",
          "form": "그쵸 저도 그래요",
          "original_form": "그쵸 저도 그래요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015591.1.20",
          "form": "너무 더워서 그런지ㅠㅠ",
          "original_form": "너무 더워서 그런지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015591.1.21",
          "form": "맞아요ㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015591.1.22",
          "form": "그래도 이제 날씨 풀리니까",
          "original_form": "그래도 이제 날씨 풀리니까",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015591.1.23",
          "form": "다시 마라탕으로 돌아갈지도 모르겠어요ㅋㅋㅋ",
          "original_form": "다시 마라탕으로 돌아갈지도 모르겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015591.1.24",
          "form": "앜ㅋㅋㅋ 저는 뜨끈한 들깨수제비먹고싶어요",
          "original_form": "앜ㅋㅋㅋ 저는 뜨끈한 들깨수제비먹고싶어요",
