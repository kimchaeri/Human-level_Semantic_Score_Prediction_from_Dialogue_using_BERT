      "utterance": [
        {
          "id": "MDRW2100020573.1.1",
          "form": "안녕하세요 혹시 영화 최근에 뭐 보셨어요?",
          "original_form": "안녕하세요 혹시 영화 최근에 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020573.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020573.1.3",
          "form": "최근에는 코로나때문에 영화관을 안갔어요",
          "original_form": "최근에는 코로나때문에 영화관을 안갔어요",
          "speaker_id": "2",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020573.1.4",
          "form": "영화 보셨나요?",
          "original_form": "영화 보셨나요?",
          "speaker_id": "2",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020573.1.5",
          "form": "네 저는 킬러의 보디가드라는 영화 봤어요",
          "original_form": "네 저는 킬러의 보디가드라는 영화 봤어요",
          "speaker_id": "1",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020573.1.6",
          "form": "최근 개봉한 영화인가요?",
          "original_form": "최근 개봉한 영화인가요?",
          "speaker_id": "2",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020573.1.7",
          "form": "네네 시리즈물인데 지금 2탄까지 나왔어요",
          "original_form": "네네 시리즈물인데 지금 2탄까지 나왔어요",
          "speaker_id": "1",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020573.1.8",
          "form": "누가 출연하나요?",
          "original_form": "누가 출연하나요?",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020573.1.9",
          "form": "라이언레이놀즈랑 사무엘잭슨이 나와요",
          "original_form": "라이언레이놀즈랑 사무엘잭슨이 나와요",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020573.1.10",
          "form": "혹시 아시나요?",
          "original_form": "혹시 아시나요?",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020573.1.11",
          "form": "라이언레이놀즈는 들어봤어요",
          "original_form": "라이언레이놀즈는 들어봤어요",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020573.1.12",
          "form": "라이언레이놀즈는 데드풀로 유명한 배우고",
          "original_form": "라이언레이놀즈는 데드풀로 유명한 배우고",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020573.1.13",
          "form": "사무엘잭슨은 마블 영화에서",
          "original_form": "사무엘잭슨은 마블 영화에서",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020573.1.14",
          "form": "닉퓨리 국장으로 유명한 배우에요",
          "original_form": "닉퓨리 국장으로 유명한 배우에요",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020573.1.15",
          "form": "아 닉퓨리 역할이었던 배우군요",
          "original_form": "아 닉퓨리 역할이었던 배우군요",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020573.1.16",
          "form": "이름이 낯설었는데 얼굴이 익숙하네요",
          "original_form": "이름이 낯설었는데 얼굴이 익숙하네요",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020573.1.17",
          "form": "ㅎㅎ 그쵸 제가 좋아하는 배우들이 나와서 본 영화인데",
          "original_form": "ㅎㅎ 그쵸 제가 좋아하는 배우들이 나와서 본 영화인데",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020573.1.18",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020573.1.19",
          "form": "전 최근 영화관은 못갔지만 크루엘라 풀리면 보려고 기다리고 있어요",
          "original_form": "전 최근 영화관은 못갔지만 크루엘라 풀리면 보려고 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020573.1.20",
          "form": "크루엘라 vod로 풀려서 2주전쯤 봤어요!",
          "original_form": "크루엘라 vod로 풀려서 2주전쯤 봤어요!",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020573.1.21",
          "form": "아 벌써iptv에 들어왔나요?",
          "original_form": "아 벌써iptv에 들어왔나요?",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020573.1.22",
          "form": "네네 들어온지 좀 된걸로알고있어요",
          "original_form": "네네 들어온지 좀 된걸로알고있어요",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020573.1.23",
          "form": "그럼 조만간 봐야겠네요",
          "original_form": "그럼 조만간 봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020573.1.24",
          "form": "다들 평이 좋더라구요",
          "original_form": "다들 평이 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020573.1.25",
          "form": "디즈니가 이런 영화도 만들 수 있구나 감탄하며 봤어요",
          "original_form": "디즈니가 이런 영화도 만들 수 있구나 감탄하며 봤어요",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020573.1.26",
          "form": "네 다들 음악이랑 의상 둘다 좋다고 하더라구요",
          "original_form": "네 다들 음악이랑 의상 둘다 좋다고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 10:10"
        }
      ]
