      "utterance": [
        {
          "id": "MDRW2100037089.1.1",
          "form": "최근에 뭐 사셨나요?",
          "original_form": "최근에 뭐 사셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100037089.1.2",
          "form": "일러스트레이터 책 샀어요",
          "original_form": "일러스트레이터 책 샀어요",
          "speaker_id": "1",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100037089.1.3",
          "form": "아 그럼 따라 그리는거예요?",
          "original_form": "아 그럼 따라 그리는거예요?",
          "speaker_id": "2",
          "time": "20210831 19:37"
        },
        {
          "id": "MDRW2100037089.1.4",
          "form": "컴퓨터로 일러스트레이터란 프로그램 있거든요 그 사용법 알려 주는 책이에요",
          "original_form": "컴퓨터로 일러스트레이터란 프로그램 있거든요 그 사용법 알려 주는 책이에요",
          "speaker_id": "1",
          "time": "20210831 19:37"
        },
        {
          "id": "MDRW2100037089.1.5",
          "form": "아 멋진데요? 전 영양제샀어요ㅋㅋ",
          "original_form": "아 멋진데요? 전 영양제샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:41"
        },
        {
          "id": "MDRW2100037089.1.6",
          "form": "종합 비타민 사신 거예요?",
          "original_form": "종합 비타민 사신 거예요?",
          "speaker_id": "1",
          "time": "20210831 19:45"
        },
        {
          "id": "MDRW2100037089.1.7",
          "form": "아뇨 비타민디에 마그네슘 함께 들어있는거요",
          "original_form": "아뇨 비타민디에 마그네슘 함께 들어있는거요",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100037089.1.8",
          "form": "마그네슘이 눈떨림에 좋다고 하더라고요",
          "original_form": "마그네슘이 눈떨림에 좋다고 하더라고요",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100037089.1.9",
          "form": "네 먹다 안먹으면 확 다른 느낌이라 먹어요",
          "original_form": "네 먹다 안먹으면 확 다른 느낌이라 먹어요",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100037089.1.10",
          "form": "중고거래도 하세요?",
          "original_form": "중고거래도 하세요?",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100037089.1.11",
          "form": "예전에 눈이 떨려서 검색해보니 마그네슘 부족이라 해서 알았어요",
          "original_form": "예전에 눈이 떨려서 검색해보니 마그네슘 부족이라 해서 알았어요",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037089.1.12",
          "form": "가끔해요",
          "original_form": "가끔해요",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037089.1.13",
          "form": "지금 당근에 스벅생쿠 팔려고요 ㅋㅋ",
          "original_form": "지금 당근에 스벅생쿠 팔려고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100037089.1.14",
          "form": "유용해요",
          "original_form": "유용해요",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100037089.1.15",
          "form": "당근에서 얼마 정도 받을 수 있어요?",
          "original_form": "당근에서 얼마 정도 받을 수 있어요?",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100037089.1.16",
          "form": "기프티스타보다 많이받아요?",
          "original_form": "기프티스타보다 많이받아요?",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100037089.1.17",
          "form": "보통 아메가격 받는거 같아요",
          "original_form": "보통 아메가격 받는거 같아요",
          "speaker_id": "2",
          "time": "20210831 21:12"
        }
      ]
