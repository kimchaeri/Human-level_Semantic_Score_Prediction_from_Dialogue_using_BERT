      "utterance": [
        {
          "id": "MDRW2100006806.1.1",
          "form": "최근에 무엇을 사셨나요?",
          "original_form": "최근에 무엇을 사셨나요?",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006806.1.2",
          "form": "마스크와 멀티툴이요",
          "original_form": "마스크와 멀티툴이요",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006806.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006806.1.4",
          "form": "멀티툴은 무엇이지요",
          "original_form": "멀티툴은 무엇이지요",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006806.1.5",
          "form": "저는 무알콜 맥주요ㅎ",
          "original_form": "저는 무알콜 맥주요ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100006806.1.6",
          "form": "생존도구 아이템이요 맥가이버 칼같은 거요",
          "original_form": "생존도구 아이템이요 맥가이버 칼같은 거요",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006806.1.7",
          "form": "와 멋지네요.",
          "original_form": "와 멋지네요.",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006806.1.8",
          "form": "칼 종류는 어디서 구입해요?",
          "original_form": "칼 종류는 어디서 구입해요?",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006806.1.9",
          "form": "그냥 쇼핑몰에서 검색해서 나온 것 사요 예비용으로 가격 저렴한 걸로요",
          "original_form": "그냥 쇼핑몰에서 검색해서 나온 것 사요 예비용으로 가격 저렴한 걸로요",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100006806.1.10",
          "form": "무알콜 맥주는 어디서 구입하세요?",
          "original_form": "무알콜 맥주는 어디서 구입하세요?",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100006806.1.11",
          "form": "저도 그냥 핫딜 주문하지요",
          "original_form": "저도 그냥 핫딜 주문하지요",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100006806.1.12",
          "form": "멀티툴 같은 거 파는 전문쇼핑몰이 있나요?",
          "original_form": "멀티툴 같은 거 파는 전문쇼핑몰이 있나요?",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100006806.1.13",
          "form": "캠핑 좋아하시나",
          "original_form": "캠핑 좋아하시나",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100006806.1.14",
          "form": "아뇨 재난대비용으로 구입했어요",
          "original_form": "아뇨 재난대비용으로 구입했어요",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100006806.1.15",
          "form": "와 아예 비상식량 같은거도?",
          "original_form": "와 아예 비상식량 같은거도?",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100006806.1.16",
          "form": "그냥 시리얼이나 초코바 정도 사놨어요",
          "original_form": "그냥 시리얼이나 초코바 정도 사놨어요",
          "speaker_id": "2",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100006806.1.17",
          "form": "혹시 모를 재난때문에? 아님 코로나?",
          "original_form": "혹시 모를 재난때문에? 아님 코로나?",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100006806.1.18",
          "form": "재난용으로요 지진이나 태풍 같은",
          "original_form": "재난용으로요 지진이나 태풍 같은",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100006806.1.19",
          "form": "아 올해 이제 태풍 시즌이 오네요",
          "original_form": "아 올해 이제 태풍 시즌이 오네요",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100006806.1.20",
          "form": "지방에 사시나봐요",
          "original_form": "지방에 사시나봐요",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100006806.1.21",
          "form": "누구나 대비는 하는 게 좋죠 하나 사 놓으면 안심도 되구요",
          "original_form": "누구나 대비는 하는 게 좋죠 하나 사 놓으면 안심도 되구요",
