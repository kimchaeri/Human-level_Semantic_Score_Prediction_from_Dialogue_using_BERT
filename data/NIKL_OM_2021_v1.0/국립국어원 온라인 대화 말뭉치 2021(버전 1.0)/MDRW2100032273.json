      "utterance": [
        {
          "id": "MDRW2100032273.1.1",
          "form": "복수전공이면 엄청 힘들었을 것 같은데요",
          "original_form": "복수전공이면 엄청 힘들었을 것 같은데요",
          "speaker_id": "1",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032273.1.2",
          "form": "엄마 집에 있으니까 단점이 있어요",
          "original_form": "엄마 집에 있으니까 단점이 있어요",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032273.1.3",
          "form": "어떤 단점이지요",
          "original_form": "어떤 단점이지요",
          "speaker_id": "1",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032273.1.4",
          "form": "화장실 이용",
          "original_form": "화장실 이용",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032273.1.5",
          "form": "계속 말 것이나요",
          "original_form": "계속 말 것이나요",
          "speaker_id": "1",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032273.1.6",
          "form": "엄마가 맨날 거실에 계시는데",
          "original_form": "엄마가 맨날 거실에 계시는데",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032273.1.7",
          "form": "아 겹치나요",
          "original_form": "아 겹치나요",
          "speaker_id": "1",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032273.1.8",
          "form": "거실에 화장실이 있거든요",
          "original_form": "거실에 화장실이 있거든요",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032273.1.9",
          "form": "큰 볼일을 못 보겠어요",
          "original_form": "큰 볼일을 못 보겠어요",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032273.1.10",
          "form": "아 이럴 수가",
          "original_form": "아 이럴 수가",
          "speaker_id": "1",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032273.1.11",
          "form": "제가 화장실을 가리거든요",
          "original_form": "제가 화장실을 가리거든요",
          "speaker_id": "2",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032273.1.12",
          "form": "음악을 크게 틀어 놓는 건 어떤가요",
          "original_form": "음악을 크게 틀어 놓는 건 어떤가요",
          "speaker_id": "1",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032273.1.13",
          "form": "배 아프면 일상 생활이 잘 안 되잖아요",
          "original_form": "배 아프면 일상 생활이 잘 안 되잖아요",
          "speaker_id": "1",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032273.1.14",
          "form": "그게 더 티나잖아요",
          "original_form": "그게 더 티나잖아요",
          "speaker_id": "2",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032273.1.15",
          "form": "그렇다면 차라리 방귀 뀌는음을 다운 받아서 틀어 놓고 편하게 사세요",
          "original_form": "그렇다면 차라리 방귀 뀌는음을 다운 받아서 틀어 놓고 편하게 사세요",
          "speaker_id": "1",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032273.1.16",
          "form": "음파가 다르답니다 뚫고나와요",
          "original_form": "음파가 다르답니다 뚫고나와요",
          "speaker_id": "2",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032273.1.17",
          "form": "그래서 제가 아침 일찍",
          "original_form": "그래서 제가 아침 일찍",
          "speaker_id": "2",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032273.1.18",
          "form": "누가 방귀 뀌는 g 똥 싸는지 모르게 한번 틀어 놓고 싸 보세요",
          "original_form": "누가 방귀 뀌는 g 똥 싸는지 모르게 한번 틀어 놓고 싸 보세요",
          "speaker_id": "1",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032273.1.19",
          "form": "카페로 옵니다",
          "original_form": "카페로 옵니다",
          "speaker_id": "2",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032273.1.20",
          "form": "화장실이 세 칸 있어요",
          "original_form": "화장실이 세 칸 있어요",
          "speaker_id": "2",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032273.1.21",
          "form": "카페 등 편한 가요",
          "original_form": "카페 등 편한 가요",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032273.1.22",
          "form": "화장실에 사람 많이 없네요",
          "original_form": "화장실에 사람 많이 없네요",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032273.1.23",
          "form": "여기서 편하게 봐요",
          "original_form": "여기서 편하게 봐요",
          "speaker_id": "2",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032273.1.24",
          "form": "아침엔 카페에 사람이 없어요",
          "original_form": "아침엔 카페에 사람이 없어요",
          "speaker_id": "2",
          "time": "20210905 14:48"
        }
