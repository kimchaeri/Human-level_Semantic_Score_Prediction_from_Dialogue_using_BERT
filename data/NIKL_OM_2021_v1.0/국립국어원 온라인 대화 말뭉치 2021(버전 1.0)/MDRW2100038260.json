      "utterance": [
        {
          "id": "MDRW2100038260.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 04:31"
        },
        {
          "id": "MDRW2100038260.1.2",
          "form": "블투 키보드가 블로그 글쓸때 좋죠ㅎㅎ",
          "original_form": "블투 키보드가 블로그 글쓸때 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 04:32"
        },
        {
          "id": "MDRW2100038260.1.3",
          "form": "맞아요!  그럴 때 쓰고 있어요",
          "original_form": "맞아요!  그럴 때 쓰고 있어요",
          "speaker_id": "2",
          "time": "20210902 04:32"
        },
        {
          "id": "MDRW2100038260.1.4",
          "form": "근데 가끔 뻑이 나더라고요",
          "original_form": "근데 가끔 뻑이 나더라고요",
          "speaker_id": "1",
          "time": "20210902 04:33"
        },
        {
          "id": "MDRW2100038260.1.5",
          "form": "제껀ㅠ",
          "original_form": "제껀ㅠ",
          "speaker_id": "1",
          "time": "20210902 04:33"
        },
        {
          "id": "MDRW2100038260.1.6",
          "form": "뻑이난다는게 어떤거 말하죠?",
          "original_form": "뻑이난다는게 어떤거 말하죠?",
          "speaker_id": "2",
          "time": "20210902 04:34"
        },
        {
          "id": "MDRW2100038260.1.7",
          "form": "갑자기 연타가 쳐질때가 있었어요ㅋㅋ",
          "original_form": "갑자기 연타가 쳐질때가 있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 04:34"
        },
        {
          "id": "MDRW2100038260.1.8",
          "form": "아ㅋㅋㅋㅋ 그 뜻이군요",
          "original_form": "아ㅋㅋㅋㅋ 그 뜻이군요",
          "speaker_id": "2",
          "time": "20210902 04:35"
        },
        {
          "id": "MDRW2100038260.1.9",
          "form": "name2님껀 정상인가요?",
          "original_form": "&name2&님껀 정상인가요?",
          "speaker_id": "1",
          "time": "20210902 04:35"
        },
        {
          "id": "MDRW2100038260.1.10",
          "form": "네 저는 그런 부분은 없어요",
          "original_form": "네 저는 그런 부분은 없어요",
          "speaker_id": "2",
          "time": "20210902 04:36"
        },
        {
          "id": "MDRW2100038260.1.11",
          "form": "오 좀 이상하다 싶은점은요?ㅠㅠ",
          "original_form": "오 좀 이상하다 싶은점은요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 04:37"
        },
        {
          "id": "MDRW2100038260.1.12",
          "form": "아직까지는 못 발견했는데",
          "original_form": "아직까지는 못 발견했는데",
          "speaker_id": "2",
          "time": "20210902 04:37"
        },
        {
          "id": "MDRW2100038260.1.13",
          "form": "부러워요ㅠ",
          "original_form": "부러워요ㅠ",
          "speaker_id": "1",
          "time": "20210902 04:37"
        },
        {
          "id": "MDRW2100038260.1.14",
          "form": "아!  그 채팅 쓰면서 전송버튼을 누르지 못 해서 아쉬워요",
          "original_form": "아!  그 채팅 쓰면서 전송버튼을 누르지 못 해서 아쉬워요",
          "speaker_id": "2",
          "time": "20210902 04:37"
        },
        {
          "id": "MDRW2100038260.1.15",
          "form": "엔터누르면 줄만 바껴서ㅜㅜ",
          "original_form": "엔터누르면 줄만 바껴서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 04:37"
        },
        {
          "id": "MDRW2100038260.1.16",
          "form": "그거 단축기 될걸요?",
          "original_form": "그거 단축기 될걸요?",
          "speaker_id": "1",
          "time": "20210902 04:37"
        },
        {
          "id": "MDRW2100038260.1.17",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210902 04:38"
        },
        {
          "id": "MDRW2100038260.1.18",
