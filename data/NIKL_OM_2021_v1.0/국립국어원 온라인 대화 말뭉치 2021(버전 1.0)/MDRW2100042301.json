      "utterance": [
        {
          "id": "MDRW2100042301.1.1",
          "form": "날씨에 이어",
          "original_form": "날씨에 이어",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042301.1.2",
          "form": "연애근황을 여쭤봐도될가요!!!  \r\n결혼하셨다구하셯나요...",
          "original_form": "연애근황을 여쭤봐도될가요!!!  \r\n결혼하셨다구하셯나요...",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042301.1.3",
          "form": "기억못하면죄송합니닺....ㅋㄱㅋㅋㄱㅜㅜ",
          "original_form": "기억못하면죄송합니닺....ㅋㄱㅋㅋㄱㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042301.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042301.1.5",
          "form": "결혼안했어요..",
          "original_form": "결혼안했어요..",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042301.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042301.1.7",
          "form": "우리 너무 많은 사람과 대화를 하니까요 ㅎㅎ",
          "original_form": "우리 너무 많은 사람과 대화를 하니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042301.1.8",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042301.1.9",
          "form": "아ㅋㅋ결혼안하셨군요!  저도안했습니다!\r\nㅋㅋㅋㅋ 만나시는분있으신가요!",
          "original_form": "아ㅋㅋ결혼안하셨군요!  저도안했습니다!\r\nㅋㅋㅋㅋ 만나시는분있으신가요!",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042301.1.10",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042301.1.11",
          "form": "연인이 있으신가요?",
          "original_form": "연인이 있으신가요?",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042301.1.12",
          "form": "저도없습니다...",
          "original_form": "저도없습니다...",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042301.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042301.1.14",
          "form": "소개팅같은거좋아하시나용?",
          "original_form": "소개팅같은거좋아하시나용?",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042301.1.15",
          "form": "자만추...?",
          "original_form": "자만추...?",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042301.1.16",
          "form": "자기만족추구인가요?",
          "original_form": "자기만족추구인가요?",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042301.1.17",
          "form": "자연스러운만남추구!",
          "original_form": "자연스러운만남추구!",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042301.1.18",
          "form": ">\\\\\\<",
          "original_form": ">\\\\\\<",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042301.1.19",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042301.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042301.1.21",
          "form": "어떤파이신가용!",
          "original_form": "어떤파이신가용!",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042301.1.22",
          "form": "그게 좋긴하죠 ㅋㅋㄱ",
          "original_form": "그게 좋긴하죠 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042301.1.23",
          "form": "그쵸역시..?ㅋㅋㅋㅋ",
          "original_form": "그쵸역시..?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042301.1.24",
          "form": "소개팅은어색한면이..",
          "original_form": "소개팅은어색한면이..",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042301.1.25",
          "form": "소개팅에서 대화가 되는 사람이 없었어요",
          "original_form": "소개팅에서 대화가 되는 사람이 없었어요",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042301.1.26",
          "form": "그런것도 있구요ㅋㅋㅋㅋ",
          "original_form": "그런것도 있구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042301.1.27",
          "form": "악ㅋㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042301.1.28",
          "form": "대화가되는사람이없으셨다니",
          "original_form": "대화가되는사람이없으셨다니",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042301.1.29",
          "form": "저 원래 딱히 기준이라는 게 없는 사람이였는데",
          "original_form": "저 원래 딱히 기준이라는 게 없는 사람이였는데",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042301.1.30",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042301.1.31",
          "form": "소개팅 몆번하고서 사람대사람으로 대화가 되는 사람이 없었요..",
          "original_form": "소개팅 몆번하고서 사람대사람으로 대화가 되는 사람이 없었요..",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042301.1.32",
          "form": "헛다리 짓는 분도 있고",
          "original_form": "헛다리 짓는 분도 있고",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042301.1.33",
          "form": "혼자 진도를 나가시는 분도있고ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "혼자 진도를 나가시는 분도있고ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042301.1.34",
          "form": "아..난감한데요ㅋㅋㅋㄱㅋ..",
          "original_form": "아..난감한데요ㅋㅋㅋㄱㅋ..",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042301.1.35",
          "form": "그쵸? 완전 난감했어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸? 완전 난감했어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042301.1.36",
          "form": "혼자진도나가시는분싫습니다.ㅈㅋㅋㅋㅋ",
          "original_form": "혼자진도나가시는분싫습니다.ㅈㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042301.1.37",
          "form": "근거없는자신감만빵이신분도",
          "original_form": "근거없는자신감만빵이신분도",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042301.1.38",
          "form": "그니까요.. 지인들과 이야기하다 알았는데 제가 대화하다 어색하면 잘 웃는데... 상대가 그걸로 오해한거 같다고요 ㅡㅡ",
          "original_form": "그니까요.. 지인들과 이야기하다 알았는데 제가 대화하다 어색하면 잘 웃는데... 상대가 그걸로 오해한거 같다고요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210906 22:27"
