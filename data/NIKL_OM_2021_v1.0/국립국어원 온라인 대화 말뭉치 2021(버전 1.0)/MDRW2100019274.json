        {
          "id": "MDRW2100019274.1.1",
          "form": "이번에 신작 나오는 도깨비 트레일러봤나?",
          "original_form": "이번에 신작 나오는 도깨비 트레일러봤나?",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019274.1.2",
          "form": "게임?",
          "original_form": "게임?",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019274.1.3",
          "form": "응 검은사막 만든회사꺼",
          "original_form": "응 검은사막 만든회사꺼",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019274.1.4",
          "form": "아니..",
          "original_form": "아니..",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019274.1.5",
          "form": "완전쩔더라",
          "original_form": "완전쩔더라",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019274.1.6",
          "form": "그냥 엔씨 망한거같던데",
          "original_form": "그냥 엔씨 망한거같던데",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019274.1.7",
          "form": "그건 또 퀄좋은가보네",
          "original_form": "그건 또 퀄좋은가보네",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019274.1.8",
          "form": "블소2는 망할만했지..",
          "original_form": "블소2는 망할만했지..",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019274.1.9",
          "form": "ㅋㅋㅋ ㅋㅋㅋ 차기작도 구린가봐",
          "original_form": "ㅋㅋㅋ ㅋㅋㅋ 차기작도 구린가봐",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019274.1.10",
          "form": "기무라횬타이도 없고..",
          "original_form": "기무라횬타이도 없고..",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019274.1.11",
          "form": "이제 펄어비스 주식 사야되",
          "original_form": "이제 펄어비스 주식 사야되",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019274.1.12",
          "form": "리니지 토대로 그냥 게임복제",
          "original_form": "리니지 토대로 그냥 게임복제",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019274.1.13",
          "form": "엔씨는 가망이없어",
          "original_form": "엔씨는 가망이없어",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019274.1.14",
          "form": "펄어비스 너무 올랏던데 지금타면 죽어",
          "original_form": "펄어비스 너무 올랏던데 지금타면 죽어",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019274.1.15",
          "form": "아..ㅋㅋㅋㅋ ㅜㅜ",
          "original_form": "아..ㅋㅋㅋㅋ ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
