      "utterance": [
        {
          "id": "MDRW2100029647.1.1",
          "form": "3번째미션깼어요",
          "original_form": "3번째미션깼어요",
          "speaker_id": "2",
          "time": "20210903 11:19"
        },
        {
          "id": "MDRW2100029647.1.2",
          "form": "곧 끝나겠군요",
          "original_form": "곧 끝나겠군요",
          "speaker_id": "1",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029647.1.3",
          "form": "나머지도 완료하세요",
          "original_form": "나머지도 완료하세요",
          "speaker_id": "1",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029647.1.4",
          "form": "이제 네번째에요",
          "original_form": "이제 네번째에요",
          "speaker_id": "2",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029647.1.5",
          "form": "아마지막이네요",
          "original_form": "아마지막이네요",
          "speaker_id": "2",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029647.1.6",
          "form": "화이팅입니다",
          "original_form": "화이팅입니다",
          "speaker_id": "1",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029647.1.7",
          "form": "하고왔습니다",
          "original_form": "하고왔습니다",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029647.1.8",
          "form": "성공이네요",
          "original_form": "성공이네요",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029647.1.9",
          "form": "오 무얼주나요",
          "original_form": "오 무얼주나요",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.10",
          "form": "미션 클리어뒤에는 뽑기를한ㅇㄴ데",
          "original_form": "미션 클리어뒤에는 뽑기를한ㅇㄴ데",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.11",
          "form": "성공ㅊㅋㅊㅋ",
          "original_form": "성공ㅊㅋㅊㅋ",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.12",
          "form": "캐릭터뽑기 메달",
          "original_form": "캐릭터뽑기 메달",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.13",
          "form": "1~3개",
          "original_form": "1~3개",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.14",
          "form": "뭐받았지요",
          "original_form": "뭐받았지요",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.15",
          "form": "랜덤인데",
          "original_form": "랜덤인데",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.16",
          "form": "몇개받았죠",
          "original_form": "몇개받았죠",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.17",
          "form": "뭘눌러도 꼭 하나만 걸리네요",
          "original_form": "뭘눌러도 꼭 하나만 걸리네요",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.18",
          "form": "색을 분명 매번",
          "original_form": "색을 분명 매번",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.19",
          "form": "좋은거받았나요",
          "original_form": "좋은거받았나요",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.20",
          "form": "다르게 눌러도",
          "original_form": "다르게 눌러도",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.21",
          "form": "메달로 캐릭터 사나요",
          "original_form": "메달로 캐릭터 사나요",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.22",
          "form": "1개만걸립니다",
          "original_form": "1개만걸립니다",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.23",
          "form": "그거 다 한개들었어요",
          "original_form": "그거 다 한개들었어요",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029647.1.24",
          "form": "뻔한 멍수작",
          "original_form": "뻔한 멍수작",
          "speaker_id": "1",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029647.1.25",
          "form": "치사해요",
          "original_form": "치사해요",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029647.1.26",
          "form": "유혹하죠",
          "original_form": "유혹하죠",
          "speaker_id": "1",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029647.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029647.1.28",
          "form": "결과는 같음",
          "original_form": "결과는 같음",
