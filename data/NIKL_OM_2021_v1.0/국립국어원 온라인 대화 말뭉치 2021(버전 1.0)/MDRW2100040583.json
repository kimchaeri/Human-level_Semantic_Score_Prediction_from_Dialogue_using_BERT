      "utterance": [
        {
          "id": "MDRW2100040583.1.1",
          "form": "공연좋아하세요?",
          "original_form": "공연좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040583.1.2",
          "form": "네네 좋아하죠 코로나라서 지금은 안가지만요 ㅠ",
          "original_form": "네네 좋아하죠 코로나라서 지금은 안가지만요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100040583.1.3",
          "form": "그쵸 코로나 끝나면 보러가고싶어요",
          "original_form": "그쵸 코로나 끝나면 보러가고싶어요",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040583.1.4",
          "form": "어떤류의 공연 좋아하세요?",
          "original_form": "어떤류의 공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040583.1.5",
          "form": "저 진짜 다 좋아해요ㅋㅋㅋㅋ 뮤지컬도 연극도 영화도 콘서트도 전시회도 박물관 다요 ㅋㅋㅋ",
          "original_form": "저 진짜 다 좋아해요ㅋㅋㅋㅋ 뮤지컬도 연극도 영화도 콘서트도 전시회도 박물관 다요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040583.1.6",
          "form": "근데 뮤지컬은 비싸서 두번 밖에 못 봤어요",
          "original_form": "근데 뮤지컬은 비싸서 두번 밖에 못 봤어요",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040583.1.7",
          "form": "저도 비싸서 몇번 못봤어요",
          "original_form": "저도 비싸서 몇번 못봤어요",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040583.1.8",
          "form": "어떤 뮤지컬이 제일 좋았어요?",
          "original_form": "어떤 뮤지컬이 제일 좋았어요?",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040583.1.9",
          "form": "저 엘리자벳이랑 드라큘라 봤거든요 친구가 김준수 좋아해서 봤는데 저는 엘리자벳이 더 재밌었어요 ㅋㅋㅋㅋ",
          "original_form": "저 엘리자벳이랑 드라큘라 봤거든요 친구가 김준수 좋아해서 봤는데 저는 엘리자벳이 더 재밌었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040583.1.10",
          "form": "돈만 많으면 더 많이 자주 보고 싶은데",
          "original_form": "돈만 많으면 더 많이 자주 보고 싶은데",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040583.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040583.1.12",
          "form": "너무 비싸요",
          "original_form": "너무 비싸요",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040583.1.13",
          "form": "저는 모차르트가 제일 좋았어요",
          "original_form": "저는 모차르트가 제일 좋았어요",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040583.1.14",
          "form": "엘리자벳은 무슨 내용이예요?",
          "original_form": "엘리자벳은 무슨 내용이예요?",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040583.1.15",
          "form": "오!! 그것도 김준수 나온거잖아요 엘리자벳은 여주이름이에요",
          "original_form": "오!! 그것도 김준수 나온거잖아요 엘리자벳은 여주이름이에요",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040583.1.16",
          "form": "자유롭고 싶은 영혼인데",
          "original_form": "자유롭고 싶은 영혼인데",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040583.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040583.1.18",
          "form": "신분때문에 항상 뭐도 안돼 뭐도 안돼 넌 항상 완벽해야해 이런 분위기에서 살아요 그러면서 엘리자벳 내면은 점점 죽어가는거죠",
          "original_form": "신분때문에 항상 뭐도 안돼 뭐도 안돼 넌 항상 완벽해야해 이런 분위기에서 살아요 그러면서 엘리자벳 내면은 점점 죽어가는거죠",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100040583.1.19",
          "form": "오 심오한 내용이네요",
          "original_form": "오 심오한 내용이네요",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100040583.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100040583.1.21",
          "form": "그렇게 피폐해지면서",
          "original_form": "그렇게 피폐해지면서",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100040583.1.22",
          "form": "죽음이라는 김준수를 만나죵",
          "original_form": "죽음이라는 김준수를 만나죵",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040583.1.23",
          "form": "오 재밌을거같아요",
          "original_form": "오 재밌을거같아요",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040583.1.24",
          "form": "암튼 판타지적 요소가 커요",
          "original_form": "암튼 판타지적 요소가 커요",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040583.1.25",
          "form": "네네 재밌었어요",
          "original_form": "네네 재밌었어요",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040583.1.26",
          "form": "모차르트 김준수나온지 몰랐어요 ㅋㅋ",
          "original_form": "모차르트 김준수나온지 몰랐어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040583.1.27",
          "form": "아니에요???",
          "original_form": "아니에요???",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040583.1.28",
          "form": "저는 박은태님 회차봤어요",
          "original_form": "저는 박은태님 회차봤어요",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040583.1.29",
          "form": "어 맞다맞다 찾아봤는데 맞아요 ㅋㅋㅋ",
          "original_form": "어 맞다맞다 찾아봤는데 맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040583.1.30",
          "form": "박은태님이 그렇게 뮤지컬판에서 유명하시다구",
          "original_form": "박은태님이 그렇게 뮤지컬판에서 유명하시다구",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040583.1.31",
          "form": "들었어요",
          "original_form": "들었어요",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040583.1.32",
          "form": "네네 목소리 너무 좋아요",
          "original_form": "네네 목소리 너무 좋아요",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040583.1.33",
          "form": "진짜 목소리가 너무 맑으세요",
          "original_form": "진짜 목소리가 너무 맑으세요",
          "speaker_id": "1",
          "time": "20210904 23:09"
        }
      ]
