      "utterance": [
        {
          "id": "MDRW2100041000.1.1",
          "form": "ㅎ이요 네네",
          "original_form": "ㅎ이요 네네",
          "speaker_id": "2",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100041000.1.2",
          "form": "국내랑 해외 중에 어디가 좋아요?",
          "original_form": "국내랑 해외 중에 어디가 좋아요?",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100041000.1.3",
          "form": "국내요 ㅎㅎ",
          "original_form": "국내요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041000.1.4",
          "form": "저도 여행 좋아하는데 많이는 못가봤거든요",
          "original_form": "저도 여행 좋아하는데 많이는 못가봤거든요",
          "speaker_id": "1",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100041000.1.5",
          "form": "코로나 괜찮아지면 국내여행 다니고 싶네요~",
          "original_form": "코로나 괜찮아지면 국내여행 다니고 싶네요~",
          "speaker_id": "1",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100041000.1.6",
          "form": "최근에 여행 갔던때가 언제예요?",
          "original_form": "최근에 여행 갔던때가 언제예요?",
          "speaker_id": "1",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100041000.1.7",
          "form": "코로나전이유 ㅜㅁ ㅜ",
          "original_form": "코로나전이유 ㅜㅁ ㅜ",
          "speaker_id": "2",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100041000.1.8",
          "form": "으아~ ㅠㅠㅠ 그쵸",
          "original_form": "으아~ ㅠㅠㅠ 그쵸",
          "speaker_id": "1",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100041000.1.9",
          "form": "어디 간게 마지막이었어요?",
          "original_form": "어디 간게 마지막이었어요?",
          "speaker_id": "1",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100041000.1.10",
          "form": "남해요",
          "original_form": "남해요",
          "speaker_id": "2",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100041000.1.11",
          "form": "남해면 땅끝마을이 생각나네요!!",
          "original_form": "남해면 땅끝마을이 생각나네요!!",
          "speaker_id": "1",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100041000.1.12",
          "form": "ㅎㅎ전라도쪽 말구요",
          "original_form": "ㅎㅎ전라도쪽 말구요",
          "speaker_id": "2",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100041000.1.13",
          "form": "경상도쪽이요 ㄹㅎ",
          "original_form": "경상도쪽이요 ㄹㅎ",
          "speaker_id": "2",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100041000.1.14",
          "form": "아!!!! 이럴수가 ㅋㅋㅋ",
          "original_form": "아!!!! 이럴수가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100041000.1.15",
          "form": "남해 땅끝마을밖에 몰랐어요",
          "original_form": "남해 땅끝마을밖에 몰랐어요",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100041000.1.16",
          "form": "경상도 남해는 어때요?",
          "original_form": "경상도 남해는 어때요?",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100041000.1.17",
          "form": "조용하고 ㅎㅎ아름다워요 ㅎㅎ",
          "original_form": "조용하고 ㅎㅎ아름다워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100041000.1.18",
          "form": "자연보러 간거예요?",
          "original_form": "자연보러 간거예요?",
