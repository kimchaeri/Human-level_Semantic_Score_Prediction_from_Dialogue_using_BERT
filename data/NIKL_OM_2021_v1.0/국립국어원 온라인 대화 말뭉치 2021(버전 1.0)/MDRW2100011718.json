      "utterance": [
        {
          "id": "MDRW2100011718.1.1",
          "form": "명품이좋다",
          "original_form": "명품이좋다",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011718.1.2",
          "form": "샤넬 많잖아~",
          "original_form": "샤넬 많잖아~",
          "speaker_id": "1",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100011718.1.3",
          "form": "싼거 밖에없다",
          "original_form": "싼거 밖에없다",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011718.1.4",
          "form": "샤넬은 정말 잘들게된다 산지오래되어도 있어보여",
          "original_form": "샤넬은 정말 잘들게된다 산지오래되어도 있어보여",
          "speaker_id": "2",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011718.1.5",
          "form": "그렇데 엄마 금반지 사고싶데",
          "original_form": "그렇데 엄마 금반지 사고싶데",
          "speaker_id": "1",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011718.1.6",
          "form": "언니야도 샤넬하나 사라",
          "original_form": "언니야도 샤넬하나 사라",
          "speaker_id": "2",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011718.1.7",
          "form": "사고싶다",
          "original_form": "사고싶다",
          "speaker_id": "1",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011718.1.8",
          "form": "엄마 반지사는데 따라가줘라",
          "original_form": "엄마 반지사는데 따라가줘라",
          "speaker_id": "1",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011718.1.9",
          "form": "반지는 갑자기 왜?",
          "original_form": "반지는 갑자기 왜?",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011718.1.10",
          "form": "데일리로 끼고싶은가보더라",
          "original_form": "데일리로 끼고싶은가보더라",
          "speaker_id": "1",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011718.1.11",
          "form": "그래서name3 금 반지인줄알고 살라고 한듯",
          "original_form": "그래서&name3& 금 반지인줄알고 살라고 한듯",
          "speaker_id": "1",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011718.1.12",
          "form": "아 데일리반지",
          "original_form": "아 데일리반지",
          "speaker_id": "2",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011718.1.13",
          "form": "같이 보러가자",
          "original_form": "같이 보러가자",
          "speaker_id": "2",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011718.1.14",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011718.1.15",
          "form": "샤넬은 물건 없다던데ㅜ",
          "original_form": "샤넬은 물건 없다던데ㅜ",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011718.1.16",
          "form": "웨이팅을 걸어놓던지",
          "original_form": "웨이팅을 걸어놓던지",
          "speaker_id": "2",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011718.1.17",
          "form": "구하기가 힘들긴하다 백화점 내랑 가보자 일단",
          "original_form": "구하기가 힘들긴하다 백화점 내랑 가보자 일단",
          "speaker_id": "2",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011718.1.18",
          "form": "아침부터 뛰어야한단다ㅜ",
          "original_form": "아침부터 뛰어야한단다ㅜ",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011718.1.19",
          "form": "어 그렇지",
          "original_form": "어 그렇지",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011718.1.20",
          "form": "입고되는 날맞춰서 가야해",
          "original_form": "입고되는 날맞춰서 가야해",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011718.1.21",
          "form": "미리전화해보면 알수는 있지",
          "original_form": "미리전화해보면 알수는 있지",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011718.1.22",
          "form": "입고는 언제?",
          "original_form": "입고는 언제?",
          "speaker_id": "1",
          "time": "20210824 16:16"
        }
      ]
