        },
        {
          "id": "MDRW2100009635.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210823 02:01"
        },
        {
          "id": "MDRW2100009635.1.3",
          "form": "집안일 잘하시나요??",
          "original_form": "집안일 잘하시나요??",
          "speaker_id": "1",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009635.1.4",
          "form": "음 요리는 못하는데",
          "original_form": "음 요리는 못하는데",
          "speaker_id": "2",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009635.1.5",
          "form": "청소는 좀 하는 것 같아요!",
          "original_form": "청소는 좀 하는 것 같아요!",
          "speaker_id": "2",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009635.1.6",
          "form": "저도 요리에는 소질이 없어요..",
          "original_form": "저도 요리에는 소질이 없어요..",
          "speaker_id": "1",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009635.1.7",
          "form": "청소를 자주 하세요?",
          "original_form": "청소를 자주 하세요?",
          "speaker_id": "1",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009635.1.8",
          "form": "네ㅎㅎ 바닥 먼지 쓸고 닦는거 자주해요",
          "original_form": "네ㅎㅎ 바닥 먼지 쓸고 닦는거 자주해요",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009635.1.9",
          "form": "요즘 무선청소기 많이 나오던데",
          "original_form": "요즘 무선청소기 많이 나오던데",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009635.1.10",
          "form": "하나 장만해야하나 싶어요",
          "original_form": "하나 장만해야하나 싶어요",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009635.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ좋네요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ좋네요!",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009635.1.12",
          "form": "저는 음악 틀고 설거지 하는 걸 좋아해요",
          "original_form": "저는 음악 틀고 설거지 하는 걸 좋아해요",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009635.1.13",
          "form": "오!!ㅋㅋㅋ 흥겨울 것 같아요",
          "original_form": "오!!ㅋㅋㅋ 흥겨울 것 같아요",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009635.1.14",
          "form": "설거지만하기엔 사실 너무 지루하죠ㅎㅎ",
          "original_form": "설거지만하기엔 사실 너무 지루하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009635.1.15",
          "form": "ㅎㅎ맞아요 집안일을 즐거워하는 사람은 그렇게 많지 않은 거 같아요...",
          "original_form": "ㅎㅎ맞아요 집안일을 즐거워하는 사람은 그렇게 많지 않은 거 같아요...",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009635.1.16",
          "form": "ㅋㅋ맞아요... 너무 힘들어요 가사노동",
          "original_form": "ㅋㅋ맞아요... 너무 힘들어요 가사노동",
          "speaker_id": "2",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009635.1.17",
          "form": "빨래 너는거 해보셨나요?",
          "original_form": "빨래 너는거 해보셨나요?",
          "speaker_id": "2",
          "time": "20210823 02:07"
        },
        {
          "id": "MDRW2100009635.1.18",
          "form": "그럼요!!",
          "original_form": "그럼요!!",
          "speaker_id": "1",
          "time": "20210823 02:08"
        },
        {
          "id": "MDRW2100009635.1.19",
          "form": "빨래 널면 집안이 습해지는 것 같아서",
          "original_form": "빨래 널면 집안이 습해지는 것 같아서",
          "speaker_id": "2",
          "time": "20210823 02:09"
        },
        {
          "id": "MDRW2100009635.1.20",
          "form": "고민이에요",
          "original_form": "고민이에요",
          "speaker_id": "2",
          "time": "20210823 02:10"
        },
        {
          "id": "MDRW2100009635.1.21",
          "form": "아..그쵸 요즘 비도 많이 오고 하니까요",
          "original_form": "아..그쵸 요즘 비도 많이 오고 하니까요",
          "speaker_id": "1",
          "time": "20210823 02:10"
        },
        {
          "id": "MDRW2100009635.1.22",
          "form": "저도 기숙사 살 때 베란다가 따로 없었어서",
          "original_form": "저도 기숙사 살 때 베란다가 따로 없었어서",
          "speaker_id": "1",
          "time": "20210823 02:11"
        },
        {
          "id": "MDRW2100009635.1.23",
          "form": "방에서 빨래 한 번 널었다가 넘 습하고 잘 마르지도 않아서 긱사에 있는 건조기를 주로 이용했었어요",
          "original_form": "방에서 빨래 한 번 널었다가 넘 습하고 잘 마르지도 않아서 긱사에 있는 건조기를 주로 이용했었어요",
          "speaker_id": "1",
          "time": "20210823 02:11"
        },
        {
          "id": "MDRW2100009635.1.24",
          "form": "건조기를 하나 장만하시는 건 어떨까요?",
          "original_form": "건조기를 하나 장만하시는 건 어떨까요?",
          "speaker_id": "1",
          "time": "20210823 02:11"
        },
        {
          "id": "MDRW2100009635.1.25",
          "form": "오 건조기를 사용하셨군요! 좋은 방법이네요!!",
          "original_form": "오 건조기를 사용하셨군요! 좋은 방법이네요!!",
          "speaker_id": "2",
          "time": "20210823 02:12"
        }
      ]
    }
