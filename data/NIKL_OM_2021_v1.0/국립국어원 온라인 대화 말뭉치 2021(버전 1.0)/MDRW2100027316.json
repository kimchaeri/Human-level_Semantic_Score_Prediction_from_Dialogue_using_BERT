      "utterance": [
        {
          "id": "MDRW2100027316.1.1",
          "form": "즐기진 않아요",
          "original_form": "즐기진 않아요",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.2",
          "form": "후폭풍이 심해서",
          "original_form": "후폭풍이 심해서",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.3",
          "form": "제주 밤막걸리",
          "original_form": "제주 밤막걸리",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.4",
          "form": "근데 맛있어요",
          "original_form": "근데 맛있어요",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.5",
          "form": "대존맛이요",
          "original_form": "대존맛이요",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.6",
          "form": "땅콩이었나",
          "original_form": "땅콩이었나",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.7",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.8",
          "form": "땅콩막걸리요",
          "original_form": "땅콩막걸리요",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.9",
          "form": "편의점에 파나요?",
          "original_form": "편의점에 파나요?",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.10",
          "form": "우도막걸리요?",
          "original_form": "우도막걸리요?",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.11",
          "form": "잘모르겠어요",
          "original_form": "잘모르겠어요",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027316.1.12",
          "form": "파전이랑먹음짱이에요",
          "original_form": "파전이랑먹음짱이에요",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027316.1.13",
          "form": "밤막걸리요",
          "original_form": "밤막걸리요",
          "speaker_id": "2",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027316.1.14",
          "form": "곶감에 치즈 넣어서",
          "original_form": "곶감에 치즈 넣어서",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027316.1.15",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027316.1.16",
          "form": "자랑?ㅋ",
          "original_form": "자랑?ㅋ",
          "speaker_id": "2",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027316.1.17",
          "form": "안주로 파는데",
          "original_form": "안주로 파는데",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027316.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱㅋ",
