      "utterance": [
        {
          "id": "MDRW2100002146.1.1",
          "form": "떡볶이",
          "original_form": "떡볶이",
          "speaker_id": "2",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002146.1.2",
          "form": "오늘은 치킨 먹긴했는데",
          "original_form": "오늘은 치킨 먹긴했는데",
          "speaker_id": "2",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002146.1.3",
          "form": "엽떡 먹고싶더라 오능",
          "original_form": "엽떡 먹고싶더라 오능",
          "speaker_id": "2",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002146.1.4",
          "form": "어휴 근데 우리동네 엽떡은",
          "original_form": "어휴 근데 우리동네 엽떡은",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002146.1.5",
          "form": "위생상태가 너무 별로야",
          "original_form": "위생상태가 너무 별로야",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002146.1.6",
          "form": "위생이 조금 그렇지",
          "original_form": "위생이 조금 그렇지",
          "speaker_id": "2",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002146.1.7",
          "form": "맞아 손톱이랑 머리카락이랑 벌레 나온건 너무했다.",
          "original_form": "맞아 손톱이랑 머리카락이랑 벌레 나온건 너무했다.",
          "speaker_id": "2",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002146.1.8",
          "form": "ㅜㅜㅜ나올수 있는건 다나왔네",
          "original_form": "ㅜㅜㅜ나올수 있는건 다나왔네",
          "speaker_id": "1",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002146.1.9",
          "form": "그러게..",
          "original_form": "그러게..",
          "speaker_id": "2",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002146.1.10",
          "form": "그리고 사장님이 너무 무례하셔서 가기가 싫더라",
          "original_form": "그리고 사장님이 너무 무례하셔서 가기가 싫더라",
          "speaker_id": "2",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002146.1.11",
          "form": "엽떡은 체인점답게 지점 관리를 좀 더 해줬으면 좋겠어",
          "original_form": "엽떡은 체인점답게 지점 관리를 좀 더 해줬으면 좋겠어",
          "speaker_id": "1",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002146.1.12",
          "form": "내가 무례한 상황을 맞닥뜨린건 아니지만",
          "original_form": "내가 무례한 상황을 맞닥뜨린건 아니지만",
          "speaker_id": "2",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002146.1.13",
          "form": "맞아 소문이 너무 안좋아",
          "original_form": "맞아 소문이 너무 안좋아",
          "speaker_id": "1",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002146.1.14",
          "form": "본사에서 좀 더 철저히 관리해줬으면 해.",
          "original_form": "본사에서 좀 더 철저히 관리해줬으면 해.",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002146.1.15",
          "form": "엽떡이 너무 먹고싶은데",
          "original_form": "엽떡이 너무 먹고싶은데",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002146.1.16",
          "form": "이런 이유로 먹지 못해서 아쉬워",
          "original_form": "이런 이유로 먹지 못해서 아쉬워",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002146.1.17",
          "form": "다음에 다른지점껄 시도해보자",
          "original_form": "다음에 다른지점껄 시도해보자",
          "speaker_id": "1",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002146.1.18",
          "form": "아니면 우리가 직접 차려버리자구",
          "original_form": "아니면 우리가 직접 차려버리자구",
          "speaker_id": "1",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002146.1.19",
          "form": "그래.",
          "original_form": "그래.",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002146.1.20",
          "form": "또 나쁜말이래..",
          "original_form": "또 나쁜말이래..",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002146.1.21",
          "form": "다음에 놀러가서 먹자.",
          "original_form": "다음에 놀러가서 먹자.",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002146.1.22",
          "form": "좋아 엽떡 맛집을 찾아서",
          "original_form": "좋아 엽떡 맛집을 찾아서",
          "speaker_id": "1",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100002146.1.23",
          "form": "다른지역도 떠나보자",
          "original_form": "다른지역도 떠나보자",
          "speaker_id": "1",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100002146.1.24",
          "form": "일단 너희집 놀러갈게",
          "original_form": "일단 너희집 놀러갈게",
          "speaker_id": "2",
          "time": "20210820 00:02"
        }
