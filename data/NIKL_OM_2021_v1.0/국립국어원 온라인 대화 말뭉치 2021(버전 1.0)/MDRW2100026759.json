      "utterance": [
        {
          "id": "MDRW2100026759.1.1",
          "form": "정말 어쨰서라는 생각밖에 안드는",
          "original_form": "정말 어쨰서라는 생각밖에 안드는",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026759.1.2",
          "form": "업무가 많지 않길 바랍니다...",
          "original_form": "업무가 많지 않길 바랍니다...",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026759.1.3",
          "form": "저도요...",
          "original_form": "저도요...",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026759.1.4",
          "form": "적게 일하고 싸게싸게 퇴근하시길 바랍니다....",
          "original_form": "적게 일하고 싸게싸게 퇴근하시길 바랍니다....",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026759.1.5",
          "form": "연락이 오긴했는데",
          "original_form": "연락이 오긴했는데",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026759.1.6",
          "form": "뭐 별거 없대 장비 옮기고 영상 저장하고 메모리 관리하고...",
          "original_form": "뭐 별거 없대 장비 옮기고 영상 저장하고 메모리 관리하고...",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.7",
          "form": "그럼..왜 급히 구하는거지",
          "original_form": "그럼..왜 급히 구하는거지",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.8",
          "form": "나참눼",
          "original_form": "나참눼",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.9",
          "form": "근데 돌발상황에서 대처만 잘하면 된대",
          "original_form": "근데 돌발상황에서 대처만 잘하면 된대",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.11",
          "form": "그게 뭔데요",
          "original_form": "그게 뭔데요",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.12",
          "form": "뭔데요",
          "original_form": "뭔데요",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.13",
          "form": "그걸얘기해줘야되는거 아닌가요 뭔데요",
          "original_form": "그걸얘기해줘야되는거 아닌가요 뭔데요",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.14",
          "form": "교수는 이게 문제야",
          "original_form": "교수는 이게 문제야",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.15",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.16",
          "form": "맨날 중요한걸 이야기 안해주ㅜ",
          "original_form": "맨날 중요한걸 이야기 안해주ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026759.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아개웃겨 (ㅋㅋㅋ ㅠㅠㅠㅠㅠㅠㅠㅠㅠ)희대의 명언",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아개웃겨 (ㅋㅋㅋ ㅠㅠㅠㅠㅠㅠㅠㅠㅠ)희대의 명언",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026759.1.18",
          "form": "맞아..보험사 약관에 조그맣게 쓰여있는 문장처럼...",
          "original_form": "맞아..보험사 약관에 조그맣게 쓰여있는 문장처럼...",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026759.1.19",
          "form": "가장중요한얘기안해주기",
          "original_form": "가장중요한얘기안해주기",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026759.1.20",
          "form": "일단오라고하기",
          "original_form": "일단오라고하기",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026759.1.21",
          "form": "힘들기",
          "original_form": "힘들기",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026759.1.22",
          "form": "사기로 팔려당하는 기분",
          "original_form": "사기로 팔려당하는 기분",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026759.1.23",
          "form": "교수님에게 팔려가고 싶지 않았는데",
          "original_form": "교수님에게 팔려가고 싶지 않았는데",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026759.1.24",
          "form": "와 말 두개 섞였다",
          "original_form": "와 말 두개 섞였다",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026759.1.25",
