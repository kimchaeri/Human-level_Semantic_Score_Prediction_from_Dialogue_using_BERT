      "utterance": [
        {
          "id": "MDRW2100044459.1.1",
          "form": "응 다들 잘춘다",
          "original_form": "응 다들 잘춘다",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044459.1.2",
          "form": "저게 자기 혼자 떨어지고 끝이 아니라",
          "original_form": "저게 자기 혼자 떨어지고 끝이 아니라",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044459.1.3",
          "form": "팀에 계속 마이너스가 간다는게 큰거같아",
          "original_form": "팀에 계속 마이너스가 간다는게 큰거같아",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044459.1.4",
          "form": "아 팀에 마이너스면ㅠ",
          "original_form": "아 팀에 마이너스면ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044459.1.5",
          "form": "타격이 크겠네",
          "original_form": "타격이 크겠네",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044459.1.6",
          "form": "그래도 지금 지목받은 훅 아이키씨 넘 멋져",
          "original_form": "그래도 지금 지목받은 훅 아이키씨 넘 멋져",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044459.1.7",
          "form": "저기 크루원이 다 자기 학생일텐데",
          "original_form": "저기 크루원이 다 자기 학생일텐데",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044459.1.8",
          "form": "지키는 모습이 좋구만",
          "original_form": "지키는 모습이 좋구만",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044459.1.9",
          "form": "응 아이키 멋있어",
          "original_form": "응 아이키 멋있어",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044459.1.10",
          "form": "나랑 동갑인가 그런데",
          "original_form": "나랑 동갑인가 그런데",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044459.1.11",
          "form": "리더미가 있어",
          "original_form": "리더미가 있어",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044459.1.12",
          "form": "오 너랑 동갑이든가?",
          "original_form": "오 너랑 동갑이든가?",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044459.1.13",
          "form": "비디오 보자보자",
          "original_form": "비디오 보자보자",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044459.1.14",
          "form": "응 그랬어",
          "original_form": "응 그랬어",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044459.1.15",
          "form": "오 비 내리는 배경이네",
          "original_form": "오 비 내리는 배경이네",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044459.1.16",
          "form": "와 찍을때 엄청 고생했겠다",
          "original_form": "와 찍을때 엄청 고생했겠다",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044459.1.17",
          "form": "응 물까지 있어서",
          "original_form": "응 물까지 있어서",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044459.1.18",
          "form": "보아 얼굴이 좀 바뀌었네",
          "original_form": "보아 얼굴이 좀 바뀌었네",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044459.1.19",
          "form": "그런가?",
          "original_form": "그런가?",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044459.1.20",
          "form": "앵글땜에 그른가 화장이나",
          "original_form": "앵글땜에 그른가 화장이나",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044459.1.21",
          "form": "그럴수도 있지",
          "original_form": "그럴수도 있지",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044459.1.22",
          "form": "앞에서 지목하다니",
          "original_form": "앞에서 지목하다니",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044459.1.23",
          "form": "잔인하다",
          "original_form": "잔인하다",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044459.1.24",
          "form": "스우파 무섭다",
          "original_form": "스우파 무섭다",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044459.1.25",
          "form": "편집이 한층 무섭게하지",
          "original_form": "편집이 한층 무섭게하지",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044459.1.26",
          "form": "다들 자존심 센 댄서들인데",
          "original_form": "다들 자존심 센 댄서들인데",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044459.1.27",
          "form": "또 용어도 워스트댄서라",
          "original_form": "또 용어도 워스트댄서라",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044459.1.28",
          "form": "응 그리고 크루들도 화나지",
          "original_form": "응 그리고 크루들도 화나지",
          "speaker_id": "2",
          "time": "20210907 22:59"
        }
      ]
    }
  ]
