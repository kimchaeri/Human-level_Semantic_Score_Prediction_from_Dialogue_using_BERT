      "utterance": [
        {
          "id": "MDRW2100013505.1.1",
          "form": "패션에 관심 많으세요?",
          "original_form": "패션에 관심 많으세요?",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013505.1.2",
          "form": "네 관심있죠",
          "original_form": "네 관심있죠",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013505.1.3",
          "form": "그럼 옷도 자주 구매하세요?",
          "original_form": "그럼 옷도 자주 구매하세요?",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013505.1.4",
          "form": "네 옷사는것도 조아해요",
          "original_form": "네 옷사는것도 조아해요",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013505.1.5",
          "form": "오오 그럼 여러 스타일에 도전하시는 타입인ㄱㅏ요?",
          "original_form": "오오 그럼 여러 스타일에 도전하시는 타입인ㄱㅏ요?",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013505.1.6",
          "form": "그냥 입고싶은거입어요",
          "original_form": "그냥 입고싶은거입어요",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013505.1.7",
          "form": "전 좀 무채색을 선호해서 밝은 색상의 옷이 별로 없어요",
          "original_form": "전 좀 무채색을 선호해서 밝은 색상의 옷이 별로 없어요",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013505.1.8",
          "form": "저는 무난하게 입고 다니고 있어요",
          "original_form": "저는 무난하게 입고 다니고 있어요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013505.1.9",
          "form": "저도요 튀는 스타일링은 조금 겁이 난달까요ㅋㅋ",
          "original_form": "저도요 튀는 스타일링은 조금 겁이 난달까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013505.1.10",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013505.1.11",
          "form": "걷기하세요?",
          "original_form": "걷기하세요?",
          "speaker_id": "1",
          "time": "20210825 15:58"
        },
        {
          "id": "MDRW2100013505.1.12",
          "form": "네 가까운거리는 주로 걸어서 이동해요 운동겸",
          "original_form": "네 가까운거리는 주로 걸어서 이동해요 운동겸",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013505.1.13",
          "form": "걷기운동에좋죠",
          "original_form": "걷기운동에좋죠",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013505.1.14",
          "form": "달리기가 정말 좋다고 하는데 전 관절이 조금 안좋아서.. 못해요ㅠㅠㅋㅋ",
          "original_form": "달리기가 정말 좋다고 하는데 전 관절이 조금 안좋아서.. 못해요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013505.1.15",
          "form": "저도요.....ㅜㅜ그냥 걷기도 운도이니깐",
          "original_form": "저도요.....ㅜㅜ그냥 걷기도 운도이니깐",
