        },
        {
          "id": "MDRW2100046140.1.2",
          "form": "나 어제 하루종일 청소했어",
          "original_form": "나 어제 하루종일 청소했어",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046140.1.3",
          "form": "대청소 했구나ㅜ",
          "original_form": "대청소 했구나ㅜ",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046140.1.4",
          "form": "맞아맞아 이제 가을이니까",
          "original_form": "맞아맞아 이제 가을이니까",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046140.1.5",
          "form": "헐 가을맞이 대청소",
          "original_form": "헐 가을맞이 대청소",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046140.1.6",
          "form": "나는화장실청소가싫어",
          "original_form": "나는화장실청소가싫어",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046140.1.7",
          "form": "맞아 그게 제일 힘든거같아",
          "original_form": "맞아 그게 제일 힘든거같아",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046140.1.8",
          "form": "그리고 머리카락도 많이빠져",
          "original_form": "그리고 머리카락도 많이빠져",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046140.1.9",
          "form": "배수구 청소 나돔 싫어",
          "original_form": "배수구 청소 나돔 싫어",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046140.1.10",
          "form": "어제 청소했더니 뿌듯하긴하더라",
          "original_form": "어제 청소했더니 뿌듯하긴하더라",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046140.1.11",
          "form": "나도 대청소 한번 해야겠다",
          "original_form": "나도 대청소 한번 해야겠다",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046140.1.12",
          "form": "나 어제 룸메이트가 코로나 자기격리라 검사도받고왔어",
          "original_form": "나 어제 룸메이트가 코로나 자기격리라 검사도받고왔어",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046140.1.13",
          "form": "다른 가족들은 괜찮아?",
          "original_form": "다른 가족들은 괜찮아?",
