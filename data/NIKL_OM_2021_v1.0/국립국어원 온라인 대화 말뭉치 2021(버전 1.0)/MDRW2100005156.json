      "utterance": [
        {
          "id": "MDRW2100005156.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005156.1.2",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005156.1.3",
          "form": "네, 연극이랑 콘서트만 가보았어요",
          "original_form": "네, 연극이랑 콘서트만 가보았어요",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005156.1.4",
          "form": "네 저는 연극 좋아합니다",
          "original_form": "네 저는 연극 좋아합니다",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005156.1.5",
          "form": "네, 그러시군요",
          "original_form": "네, 그러시군요",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005156.1.6",
          "form": "연극은 어떤 연극을 보셨는지",
          "original_form": "연극은 어떤 연극을 보셨는지",
          "speaker_id": "1",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005156.1.7",
          "form": "요즘은 직접 공연 보기가 힘든데 어떻게 하시나요",
          "original_form": "요즘은 직접 공연 보기가 힘든데 어떻게 하시나요",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005156.1.8",
          "form": "그냥 이것저것 봐요",
          "original_form": "그냥 이것저것 봐요",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005156.1.9",
          "form": "티켓 예약은 인터파크죠",
          "original_form": "티켓 예약은 인터파크죠",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005156.1.10",
          "form": "저도 인터파크 이용하고 있어요",
          "original_form": "저도 인터파크 이용하고 있어요",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005156.1.11",
          "form": "인기 있는 공연은 티켓팅이 정말 어렵던데, 티켓팅하는 노하우 있으신가요",
          "original_form": "인기 있는 공연은 티켓팅이 정말 어렵던데, 티켓팅하는 노하우 있으신가요",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005156.1.12",
          "form": "대기타서 클릭질이요",
          "original_form": "대기타서 클릭질이요",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005156.1.13",
          "form": "저는 폰으로 주로 해요",
          "original_form": "저는 폰으로 주로 해요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005156.1.14",
          "form": "보구 가장 와~했던 공연 있으신가요",
          "original_form": "보구 가장 와~했던 공연 있으신가요",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005156.1.15",
          "form": "아 네. 폰으로도 할만하죠",
          "original_form": "아 네. 폰으로도 할만하죠",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005156.1.16",
          "form": "글쎄요 잘",
          "original_form": "글쎄요 잘",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005156.1.17",
          "form": "네예",
          "original_form": "네예",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005156.1.18",
          "form": "저는 뮤지컬은 본적이 없는데 본적 있으신가요",
          "original_form": "저는 뮤지컬은 본적이 없는데 본적 있으신가요",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005156.1.19",
          "form": "네 있어요 음",
          "original_form": "네 있어요 음",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005156.1.20",
          "form": "롯데 어디서 봣ㅈ습니당",
          "original_form": "롯데 어디서 봣ㅈ습니당",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005156.1.21",
          "form": "네 갑자기 흥미가 없네요",
          "original_form": "네 갑자기 흥미가 없네요",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005156.1.22",
          "form": "마지막으로 본 공연은 언제세요",
          "original_form": "마지막으로 본 공연은 언제세요",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005156.1.23",
          "form": "기억안나요",
          "original_form": "기억안나요",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005156.1.24",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "2",
          "time": "20210821 21:25"
        }
      ]
    }
