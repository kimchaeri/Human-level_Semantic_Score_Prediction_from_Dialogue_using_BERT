      "utterance": [
        {
          "id": "MDRW2100013066.1.1",
          "form": "밥드셨나요",
          "original_form": "밥드셨나요",
          "speaker_id": "2",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013066.1.2",
          "form": "네 아침늦게먹었어요",
          "original_form": "네 아침늦게먹었어요",
          "speaker_id": "1",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013066.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013066.1.4",
          "form": "저도 먹었어용",
          "original_form": "저도 먹었어용",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100013066.1.5",
          "form": "시켜드세요?",
          "original_form": "시켜드세요?",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100013066.1.6",
          "form": "아뇨 ㅎㅎ",
          "original_form": "아뇨 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100013066.1.7",
          "form": "점심은 뭐드실거에요?",
          "original_form": "점심은 뭐드실거에요?",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013066.1.8",
          "form": "아점으로 먹은거라 점심은 건너뛰고",
          "original_form": "아점으로 먹은거라 점심은 건너뛰고",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013066.1.9",
          "form": "저녁에 간단히 먹으려구욥",
          "original_form": "저녁에 간단히 먹으려구욥",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013066.1.10",
          "form": "네 ㅎㅎ 배민자주쓰시나요?",
          "original_form": "네 ㅎㅎ 배민자주쓰시나요?",
          "speaker_id": "1",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100013066.1.11",
          "form": "네네 배달어플들 많이써욥 ㅎㅎ 할인쿠폰도 주고해서요",
          "original_form": "네네 배달어플들 많이써욥 ㅎㅎ 할인쿠폰도 주고해서요",
          "speaker_id": "2",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100013066.1.12",
          "form": "네 ㅎㅎ 요기요 만원할인 이거",
          "original_form": "네 ㅎㅎ 요기요 만원할인 이거",
          "speaker_id": "1",
          "time": "20210825 12:17"
        },
        {
          "id": "MDRW2100013066.1.13",
          "form": "진짜 만원할인이에요?",
          "original_form": "진짜 만원할인이에요?",
          "speaker_id": "1",
          "time": "20210825 12:17"
        },
        {
          "id": "MDRW2100013066.1.14",
          "form": "네네 아마 첫주문 만원 말씀하시는거같은데!",
          "original_form": "네네 아마 첫주문 만원 말씀하시는거같은데!",
          "speaker_id": "2",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100013066.1.15",
          "form": "넹넹 그거요!",
          "original_form": "넹넹 그거요!",
          "speaker_id": "1",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100013066.1.16",
          "form": "맞아요 ㅎㅎ 꼭사용하세요 15000원이상인가 쿠폰이라",
          "original_form": "맞아요 ㅎㅎ 꼭사용하세요 15000원이상인가 쿠폰이라",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100013066.1.17",
          "form": "저렴하게 시킹수있어용",
          "original_form": "저렴하게 시킹수있어용",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013066.1.18",
          "form": "오오 네 먹고싶을때 가입해야겠어요",
          "original_form": "오오 네 먹고싶을때 가입해야겠어요",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013066.1.19",
          "form": "자꾸 까먹어요 ㅠㅠ",
          "original_form": "자꾸 까먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 12:20"
        }
      ]
