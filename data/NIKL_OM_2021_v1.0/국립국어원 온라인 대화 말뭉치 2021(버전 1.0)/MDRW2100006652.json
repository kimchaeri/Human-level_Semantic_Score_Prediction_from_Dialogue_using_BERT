      "utterance": [
        {
          "id": "MDRW2100006652.1.1",
          "form": "디즈니랜드! 거긴 저도 가보고 싶어요ㅠㅠ",
          "original_form": "디즈니랜드! 거긴 저도 가보고 싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100006652.1.2",
          "form": "디즈니랜드 무조건 가야죠",
          "original_form": "디즈니랜드 무조건 가야죠",
          "speaker_id": "2",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100006652.1.3",
          "form": "미국 여행 필수사항이에요",
          "original_form": "미국 여행 필수사항이에요",
          "speaker_id": "2",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100006652.1.4",
          "form": "본토의 디즈니는 엄청 나겠죠?ㅠㅠ",
          "original_form": "본토의 디즈니는 엄청 나겠죠?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100006652.1.5",
          "form": "상상만해도 두근거리네요",
          "original_form": "상상만해도 두근거리네요",
          "speaker_id": "2",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100006652.1.6",
          "form": "어디 가보고싶으신 곳 없으신가요??",
          "original_form": "어디 가보고싶으신 곳 없으신가요??",
          "speaker_id": "2",
          "time": "20210908 05:28"
        },
        {
          "id": "MDRW2100006652.1.7",
          "form": "저는 해리포터 있다는 곳 거기 가보고 싶어요ㅠㅠ",
          "original_form": "저는 해리포터 있다는 곳 거기 가보고 싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:29"
        },
        {
          "id": "MDRW2100006652.1.8",
          "form": "아마 유니버셜 일거에요",
          "original_form": "아마 유니버셜 일거에요",
          "speaker_id": "2",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100006652.1.9",
          "form": "네ㅠㅠ 거기요ㅠㅠ 가서 버터비어 마시고 싶어요ㅠㅠ",
          "original_form": "네ㅠㅠ 거기요ㅠㅠ 가서 버터비어 마시고 싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:31"
        },
        {
          "id": "MDRW2100006652.1.10",
          "form": "꼭 가보세요 전 오사카에서 가보긴했는데 재미있더라고요",
          "original_form": "꼭 가보세요 전 오사카에서 가보긴했는데 재미있더라고요",
          "speaker_id": "2",
          "time": "20210908 05:31"
        },
        {
          "id": "MDRW2100006652.1.11",
          "form": "미국으로 가면 규모가 훨씬 크지않을까싶기도 해요",
          "original_form": "미국으로 가면 규모가 훨씬 크지않을까싶기도 해요",
          "speaker_id": "2",
          "time": "20210908 05:32"
        },
        {
          "id": "MDRW2100006652.1.12",
          "form": "생각만해도 두근두근하네요!",
          "original_form": "생각만해도 두근두근하네요!",
          "speaker_id": "1",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100006652.1.13",
          "form": "파랑님은 그런곳 가시면 기념품같은거 사오시나요?",
          "original_form": "파랑님은 그런곳 가시면 기념품같은거 사오시나요?",
          "speaker_id": "1",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100006652.1.14",
          "form": "네네 사오는 편이에요",
          "original_form": "네네 사오는 편이에요",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100006652.1.15",
          "form": "당장 제 책상만 봐도 지팡이가 있답니다 ㅋㅋㅋ",
          "original_form": "당장 제 책상만 봐도 지팡이가 있답니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100006652.1.16",
          "form": "우와아ㅠㅠ 부러워요ㅠㅠㅠㅠ 공식 굿즈!!!",
          "original_form": "우와아ㅠㅠ 부러워요ㅠㅠㅠㅠ 공식 굿즈!!!",
          "speaker_id": "1",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100006652.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ기념품 샵 들어가니 벽에 빼곡하게 있는데 너무 예뻐서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ기념품 샵 들어가니 벽에 빼곡하게 있는데 너무 예뻐서",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100006652.1.18",
          "form": "하나 안 살 수가 없더라고요",
          "original_form": "하나 안 살 수가 없더라고요",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100006652.1.19",
          "form": "저였어도 그랬을 것 같아요ㅠㅠㅠㅠ",
          "original_form": "저였어도 그랬을 것 같아요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:40"
        },
        {
          "id": "MDRW2100006652.1.20",
          "form": "하지만 다 살 수는 없으니 엄청 고민하셨겠어요ㅠㅠ",
          "original_form": "하지만 다 살 수는 없으니 엄청 고민하셨겠어요ㅠㅠ",
          "speaker_id": "1",
