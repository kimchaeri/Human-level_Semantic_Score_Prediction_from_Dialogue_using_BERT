      "utterance": [
        {
          "id": "MDRW2100013379.1.1",
          "form": "즐겨보는 프로그램 있으신가요",
          "original_form": "즐겨보는 프로그램 있으신가요",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013379.1.2",
          "form": "저 나혼산이랑 놀라운토요일 봐요!",
          "original_form": "저 나혼산이랑 놀라운토요일 봐요!",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013379.1.3",
          "form": "저도 놀라운토요일 좋아해요!",
          "original_form": "저도 놀라운토요일 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013379.1.4",
          "form": "너무 재밌지않나요",
          "original_form": "너무 재밌지않나요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013379.1.5",
          "form": "ㅋㄱㅋ맞아요! 받아쓰기도 재밌고",
          "original_form": "ㅋㄱㅋ맞아요! 받아쓰기도 재밌고",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013379.1.6",
          "form": "다들 너무 웃겨욬ㅋㅋ",
          "original_form": "다들 너무 웃겨욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013379.1.7",
          "form": "그리고 음식들이",
          "original_form": "그리고 음식들이",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013379.1.8",
          "form": "진짜 기가막히더라구요",
          "original_form": "진짜 기가막히더라구요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013379.1.9",
          "form": "그리고 중간에 간식 게임 너무 웃기지않나요",
          "original_form": "그리고 중간에 간식 게임 너무 웃기지않나요",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013379.1.10",
          "form": "ㅋㄱㅋ아 공감이에요! 간식게임이 솔직히 더 재밌어요",
          "original_form": "ㅋㄱㅋ아 공감이에요! 간식게임이 솔직히 더 재밌어요",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013379.1.11",
          "form": "음식들도 너무 먹고싶어요....",
          "original_form": "음식들도 너무 먹고싶어요....",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013379.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013379.1.13",
          "form": "저는 김동현씨 마지막 남을 때가 제일 웃겨요",
          "original_form": "저는 김동현씨 마지막 남을 때가 제일 웃겨요",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013379.1.14",
          "form": "ㅋㄱㅋㄱㅋㄱㅋ맞아욬",
          "original_form": "ㅋㄱㅋㄱㅋㄱㅋ맞아욬",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013379.1.15",
          "form": "김동현 진짜 웃겨욬ㅋㅋㄱ",
          "original_form": "김동현 진짜 웃겨욬ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013379.1.16",
          "form": "거의 맨날 마지막에 남지 않나욬ㅋㅋ",
          "original_form": "거의 맨날 마지막에 남지 않나욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013379.1.17",
          "form": "진짜 뒤집어집니다ㅋㅋ",
          "original_form": "진짜 뒤집어집니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013379.1.18",
          "form": "오답파티할 때 제일 웃긴데",
          "original_form": "오답파티할 때 제일 웃긴데",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013379.1.19",
          "form": "그 답이 엉뚱해서 더 웃겨요",
          "original_form": "그 답이 엉뚱해서 더 웃겨요",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013379.1.20",
          "form": "맞아요ㅋㅋㅋ 어떻게 그렇게 생각할 수 있는건짘",
          "original_form": "맞아요ㅋㅋㅋ 어떻게 그렇게 생각할 수 있는건짘",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013379.1.21",
          "form": "진짜 놀토의 보물",
          "original_form": "진짜 놀토의 보물",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013379.1.22",
          "form": "인정이요ㅋㅋ 김동현씨는 집사부에서도 웃기더라고요ㅋㅋ",
          "original_form": "인정이요ㅋㅋ 김동현씨는 집사부에서도 웃기더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013379.1.23",
          "form": "진짜 진정한 예능인같아요",
          "original_form": "진짜 진정한 예능인같아요",
          "speaker_id": "1",
          "time": "20210825 14:40"
        }
      ]
