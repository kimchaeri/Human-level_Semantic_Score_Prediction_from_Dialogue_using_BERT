      "utterance": [
        {
          "id": "MDRW2100017192.1.1",
          "form": "다이어트 하기 싫어",
          "original_form": "다이어트 하기 싫어",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100017192.1.2",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017192.1.3",
          "form": "다이어트?",
          "original_form": "다이어트?",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017192.1.4",
          "form": "개나줘",
          "original_form": "개나줘",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017192.1.5",
          "form": "근데 나 저번 운동할따보다",
          "original_form": "근데 나 저번 운동할따보다",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017192.1.6",
          "form": "살 빠짐",
          "original_form": "살 빠짐",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017192.1.7",
          "form": "안나가고",
          "original_form": "안나가고",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017192.1.8",
          "form": "잘 안먹어서",
          "original_form": "잘 안먹어서",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017192.1.9",
          "form": "요즘 하루에 몇 끼 먹어?",
          "original_form": "요즘 하루에 몇 끼 먹어?",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100017192.1.10",
          "form": "많으면 2끼",
          "original_form": "많으면 2끼",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017192.1.11",
          "form": "보통 1끼?",
          "original_form": "보통 1끼?",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017192.1.12",
          "form": "건강 챙겨..",
          "original_form": "건강 챙겨..",
          "speaker_id": "1",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017192.1.13",
          "form": "제발..ㅠㅠㅠ",
          "original_form": "제발..ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017192.1.14",
          "form": "한 끼가 말이 되니",
          "original_form": "한 끼가 말이 되니",
          "speaker_id": "1",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017192.1.15",
          "form": "24시간 동안 1끼? 그건 학대야ㅠㅠ",
          "original_form": "24시간 동안 1끼? 그건 학대야ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017192.1.16",
          "form": ".....",
          "original_form": ".....",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017192.1.17",
          "form": "아냐...",
          "original_form": "아냐...",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017192.1.18",
          "form": "괜찮아..",
          "original_form": "괜찮아..",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017192.1.19",
          "form": "그래두ㅠㅠ",
          "original_form": "그래두ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017192.1.20",
          "form": "난 건강해",
          "original_form": "난 건강해",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017192.1.21",
          "form": "얼마나 멀쩡해",
          "original_form": "얼마나 멀쩡해",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017192.1.22",
          "form": "헬스는 어땠어? 피티 받은 거지?",
          "original_form": "헬스는 어땠어? 피티 받은 거지?",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017192.1.23",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017192.1.24",
          "form": "피티 받으면 식단도 도와주나?",
          "original_form": "피티 받으면 식단도 도와주나?",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017192.1.25",
          "form": "난 아니",
          "original_form": "난 아니",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017192.1.26",
          "form": "그 코오롱에",
          "original_form": "그 코오롱에",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017192.1.27",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017192.1.28",
          "form": "참누리 그 센트럴 거기에 있느데",
          "original_form": "참누리 그 센트럴 거기에 있느데",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017192.1.29",
          "form": "식단도 해준다고 했었어",
          "original_form": "식단도 해준다고 했었어",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017192.1.30",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017192.1.31",
          "form": "거기서",
          "original_form": "거기서",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017192.1.32",
          "form": "인바디도 재봤어?",
          "original_form": "인바디도 재봤어?",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017192.1.33",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017192.1.34",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017192.1.35",
          "form": "다해줘",
          "original_form": "다해줘",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017192.1.36",
          "form": "거기 스트레칭 풀어줬는데",
          "original_form": "거기 스트레칭 풀어줬는데",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017192.1.37",
          "form": "개아픈데",
          "original_form": "개아픈데",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017192.1.38",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017192.1.39",
          "form": "시원해?",
          "original_form": "시원해?",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017192.1.40",
          "form": "그 허리 숙여서 닿는거",
          "original_form": "그 허리 숙여서 닿는거",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017192.1.41",
          "form": "시원하기보다 아파",
          "original_form": "시원하기보다 아파",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017192.1.42",
          "form": "너무 아파",
          "original_form": "너무 아파",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017192.1.43",
          "form": "얍",
          "original_form": "얍",
          "speaker_id": "1",
          "time": "20210826 21:05"
        }
      ]
