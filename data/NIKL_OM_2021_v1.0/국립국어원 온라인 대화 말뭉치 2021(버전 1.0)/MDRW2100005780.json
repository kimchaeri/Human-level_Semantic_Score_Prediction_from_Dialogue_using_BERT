      "utterance": [
        {
          "id": "MDRW2100005780.1.1",
          "form": "갤플립진짜 너무 예쁘죠",
          "original_form": "갤플립진짜 너무 예쁘죠",
          "speaker_id": "2",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100005780.1.2",
          "form": "호시탐탐 보구 있어요",
          "original_form": "호시탐탐 보구 있어요",
          "speaker_id": "1",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100005780.1.3",
          "form": "색깔은 어떤게 이쁘던가요",
          "original_form": "색깔은 어떤게 이쁘던가요",
          "speaker_id": "1",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100005780.1.4",
          "form": "저 그린이랑 라벤더요",
          "original_form": "저 그린이랑 라벤더요",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100005780.1.5",
          "form": "지금 제가 마침 폰바꿀때긴 하거든요? 근데 막상 플립사려니 그 뭐죠",
          "original_form": "지금 제가 마침 폰바꿀때긴 하거든요? 근데 막상 플립사려니 그 뭐죠",
          "speaker_id": "2",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100005780.1.6",
          "form": "사전예약 끝나서 아까워요",
          "original_form": "사전예약 끝나서 아까워요",
          "speaker_id": "2",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100005780.1.7",
          "form": "아 그러네요 ㅎㅎ",
          "original_form": "아 그러네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100005780.1.8",
          "form": "근데 약간 불편하다고도해요",
          "original_form": "근데 약간 불편하다고도해요",
          "speaker_id": "1",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100005780.1.9",
          "form": "어떤점이요?",
          "original_form": "어떤점이요?",
          "speaker_id": "2",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100005780.1.10",
          "form": "핸드폰을 열어야해서요",
          "original_form": "핸드폰을 열어야해서요",
          "speaker_id": "1",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100005780.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100005780.1.12",
          "form": "ㅋㅋㅋㅋㅋ와...단순..!",
          "original_form": "ㅋㅋㅋㅋㅋ와...단순..!",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100005780.1.13",
          "form": "그렇군욬ㅋㅋㅋㅋㅋ",
          "original_form": "그렇군욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100005780.1.14",
          "form": "닫는 기능이 없을땐 일자인대",
          "original_form": "닫는 기능이 없을땐 일자인대",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100005780.1.15",
          "form": "아 저는 제 친구가 잠깐 플립1 썼는데 갑자기 화면 반이 지지직거리더니 나가는거 보고",
          "original_form": "아 저는 제 친구가 잠깐 플립1 썼는데 갑자기 화면 반이 지지직거리더니 나가는거 보고",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100005780.1.16",
          "form": "매번 열려면 귀찮다구",
          "original_form": "매번 열려면 귀찮다구",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100005780.1.17",
          "form": "아...갖고싶은데 망설여지더라구요",
          "original_form": "아...갖고싶은데 망설여지더라구요",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100005780.1.18",
          "form": "이번에 방수기능도 있고 뭔가 더 튼튼해보이던데",
          "original_form": "이번에 방수기능도 있고 뭔가 더 튼튼해보이던데",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100005780.1.19",
          "form": "그쵸 쓰리니까 업그레이드 해야죠",
          "original_form": "그쵸 쓰리니까 업그레이드 해야죠",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100005780.1.20",
          "form": "정신차려 삼성",
          "original_form": "정신차려 삼성",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100005780.1.21",
          "form": "제 친구가 폴드2로 최근에 바꿧는데",
          "original_form": "제 친구가 폴드2로 최근에 바꿧는데",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005780.1.22",
          "form": "그건 이제 옆으로 커지잖아요",
          "original_form": "그건 이제 옆으로 커지잖아요",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005780.1.23",
          "form": "엄청 크더라고요 진짜 미니북같음 ㅋㅋㅋ",
