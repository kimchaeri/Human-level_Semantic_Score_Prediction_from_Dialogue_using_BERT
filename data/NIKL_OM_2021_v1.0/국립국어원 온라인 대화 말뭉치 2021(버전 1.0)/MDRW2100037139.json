      "utterance": [
        {
          "id": "MDRW2100037139.1.1",
          "form": "카페 좋아하세오?",
          "original_form": "카페 좋아하세오?",
          "speaker_id": "1",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100037139.1.2",
          "form": "네 기분전환하고 싶을 때 가요",
          "original_form": "네 기분전환하고 싶을 때 가요",
          "speaker_id": "2",
          "time": "20210831 20:33"
        },
        {
          "id": "MDRW2100037139.1.3",
          "form": "name1님은 어떠신가요?",
          "original_form": "&name1&님은 어떠신가요?",
          "speaker_id": "2",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100037139.1.4",
          "form": "저는 케이크 먹고싶을때 가용!",
          "original_form": "저는 케이크 먹고싶을때 가용!",
          "speaker_id": "1",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100037139.1.5",
          "form": "케이크 좋아하시나봐요",
          "original_form": "케이크 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100037139.1.6",
          "form": "네! 그렇게 안좋아했는데 갈수록 단게 땡기네요",
          "original_form": "네! 그렇게 안좋아했는데 갈수록 단게 땡기네요",
          "speaker_id": "1",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037139.1.7",
          "form": "저랑 입맛이 반대시네요 전 단 것을 점점 안 먹게 되네요",
          "original_form": "저랑 입맛이 반대시네요 전 단 것을 점점 안 먹게 되네요",
          "speaker_id": "2",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100037139.1.8",
          "form": "그러게욥 헣헣",
          "original_form": "그러게욥 헣헣",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100037139.1.9",
          "form": "그럼 카페가시면 어떤거 많이드세요?",
          "original_form": "그럼 카페가시면 어떤거 많이드세요?",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100037139.1.10",
          "form": "허브티요 아니면 전통차 같은 것 먹어요",
          "original_form": "허브티요 아니면 전통차 같은 것 먹어요",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037139.1.11",
          "form": "차 좋아하시는군요~",
          "original_form": "차 좋아하시는군요~",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100037139.1.12",
          "form": "허브티 한번마시기 시작하면 생각나는거같아요",
          "original_form": "허브티 한번마시기 시작하면 생각나는거같아요",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100037139.1.13",
          "form": "네 name1님도 허브티 좋아하세요?",
          "original_form": "네 &name1&님도 허브티 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037139.1.14",
          "form": "넹~ 좋아해욤!",
          "original_form": "넹~ 좋아해욤!",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037139.1.15",
          "form": "홍차나 얼그레이티 좋아해요",
          "original_form": "홍차나 얼그레이티 좋아해요",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037139.1.16",
          "form": "저도 홍차 좋아해요 레이디 그레이 최애입니다",
          "original_form": "저도 홍차 좋아해요 레이디 그레이 최애입니다",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037139.1.17",
          "form": "그건 안마셔본거같네욥",
          "original_form": "그건 안마셔본거같네욥",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037139.1.18",
          "form": "어떤가요??",
          "original_form": "어떤가요??",
          "speaker_id": "1",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100037139.1.19",
          "form": "얼그레이에 시트러스가 가미되어 전 더 맛있더라고요 \r\n얼그레이 좋아하시면 잘 드실거에요",
          "original_form": "얼그레이에 시트러스가 가미되어 전 더 맛있더라고요 \r\n얼그레이 좋아하시면 잘 드실거에요",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100037139.1.20",
          "form": "오오 담에 마셔봐야겠네요!",
          "original_form": "오오 담에 마셔봐야겠네요!",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037139.1.21",
          "form": "향이 진짜 좋죠~",
          "original_form": "향이 진짜 좋죠~",
          "speaker_id": "1",
          "time": "20210831 21:55"
        }
      ]
