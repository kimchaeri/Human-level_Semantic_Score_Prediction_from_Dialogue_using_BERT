          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031904.1.3",
          "form": "정모 해보셨나요",
          "original_form": "정모 해보셨나요",
          "speaker_id": "1",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031904.1.4",
          "form": "코로나 전엔 몇번 했는데",
          "original_form": "코로나 전엔 몇번 했는데",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031904.1.5",
          "form": "요즘은 안해요",
          "original_form": "요즘은 안해요",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031904.1.6",
          "form": "요새",
          "original_form": "요새",
          "speaker_id": "1",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031904.1.7",
          "form": "시기가 그렇긴",
          "original_form": "시기가 그렇긴",
          "speaker_id": "1",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031904.1.8",
          "form": "하네요 아쉽게",
          "original_form": "하네요 아쉽게",
          "speaker_id": "1",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031904.1.9",
          "form": "네 온라인 커뮤니티 하는 것 있으세요?",
          "original_form": "네 온라인 커뮤니티 하는 것 있으세요?",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031904.1.10",
          "form": "네이버 카페정도?",
          "original_form": "네이버 카페정도?",
          "speaker_id": "1",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031904.1.11",
          "form": "정보 얻으려구요 ㅎㅎ",
          "original_form": "정보 얻으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031904.1.12",
          "form": "아 어떤 카페 활동하세요?",
          "original_form": "아 어떤 카페 활동하세요?",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031904.1.13",
          "form": "부동산 정보 좀 ㅎㅎ",
          "original_form": "부동산 정보 좀 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:25"
        },
        {
          "id": "MDRW2100031904.1.14",
          "form": "청약해보려구해서요",
          "original_form": "청약해보려구해서요",
          "speaker_id": "1",
          "time": "20210905 09:25"
        },
        {
          "id": "MDRW2100031904.1.15",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210905 09:26"
        },
        {
          "id": "MDRW2100031904.1.16",
          "form": "저도 최근에 부동산 카페 가입했어요",
          "original_form": "저도 최근에 부동산 카페 가입했어요",
          "speaker_id": "2",
          "time": "20210905 09:26"
        },
        {
          "id": "MDRW2100031904.1.17",
          "form": "ㅎㅎ 정보가 있긴한데 잘 고르는게 일이라",
          "original_form": "ㅎㅎ 정보가 있긴한데 잘 고르는게 일이라",
          "speaker_id": "1",
          "time": "20210905 10:01"
        },
        {
          "id": "MDRW2100031904.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 10:04"
        },
        {
          "id": "MDRW2100031904.1.19",
          "form": "카페는 정보의 홍수라",
