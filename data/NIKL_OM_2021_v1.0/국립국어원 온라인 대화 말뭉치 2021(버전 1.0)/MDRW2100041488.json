      "utterance": [
        {
          "id": "MDRW2100041488.1.1",
          "form": "좋아하는 음식이 뭐에요?",
          "original_form": "좋아하는 음식이 뭐에요?",
          "speaker_id": "1",
          "time": "20210906 02:43"
        },
        {
          "id": "MDRW2100041488.1.2",
          "form": "저는... 비빔밥 ?! ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는... 비빔밥 ?! ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 02:44"
        },
        {
          "id": "MDRW2100041488.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 02:44"
        },
        {
          "id": "MDRW2100041488.1.4",
          "form": "저는 떡볶이요!",
          "original_form": "저는 떡볶이요!",
          "speaker_id": "1",
          "time": "20210906 02:47"
        },
        {
          "id": "MDRW2100041488.1.5",
          "form": "매운거 잘 드시나요?!",
          "original_form": "매운거 잘 드시나요?!",
          "speaker_id": "2",
          "time": "20210906 02:47"
        },
        {
          "id": "MDRW2100041488.1.6",
          "form": "네 좀 먹어요!",
          "original_form": "네 좀 먹어요!",
          "speaker_id": "1",
          "time": "20210906 02:48"
        },
        {
          "id": "MDRW2100041488.1.7",
          "form": "님은 잘 드세오ㅡ??",
          "original_form": "님은 잘 드세오ㅡ??",
          "speaker_id": "1",
          "time": "20210906 02:48"
        },
        {
          "id": "MDRW2100041488.1.8",
          "form": "드세요??",
          "original_form": "드세요??",
          "speaker_id": "1",
          "time": "20210906 02:48"
        },
        {
          "id": "MDRW2100041488.1.9",
          "form": "저는 맵찔이에용 ㅋㅋㅋ",
          "original_form": "저는 맵찔이에용 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 02:48"
        },
        {
          "id": "MDRW2100041488.1.10",
          "form": "로제떡볶이 먹어봤어요?!",
          "original_form": "로제떡볶이 먹어봤어요?!",
          "speaker_id": "2",
          "time": "20210906 02:48"
        },
        {
          "id": "MDRW2100041488.1.11",
          "form": "앜ㅋㅋ아니요 아직 못먹어봤어요ㅠ",
          "original_form": "앜ㅋㅋ아니요 아직 못먹어봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210906 02:49"
        },
        {
          "id": "MDRW2100041488.1.12",
          "form": "무슨맛이에요??",
          "original_form": "무슨맛이에요??",
          "speaker_id": "1",
          "time": "20210906 02:49"
        },
        {
          "id": "MDRW2100041488.1.13",
          "form": "로제가 토마토 + 크림 소스인데 떡볶이라서 매콤하기도 한?!?!",
          "original_form": "로제가 토마토 + 크림 소스인데 떡볶이라서 매콤하기도 한?!?!",
          "speaker_id": "2",
          "time": "20210906 02:51"
        },
        {
          "id": "MDRW2100041488.1.14",
          "form": "맛있더라구요 하도 유행이라 한번 먹어봤어요",
          "original_form": "맛있더라구요 하도 유행이라 한번 먹어봤어요",
          "speaker_id": "2",
          "time": "20210906 02:51"
        },
        {
          "id": "MDRW2100041488.1.15",
          "form": "오 그럼 토마토 맛인데 약간 매콤하기도 하는거에요??",
          "original_form": "오 그럼 토마토 맛인데 약간 매콤하기도 하는거에요??",
          "speaker_id": "1",
          "time": "20210906 02:52"
        },
        {
          "id": "MDRW2100041488.1.16",
          "form": "신기하네요?!",
          "original_form": "신기하네요?!",
          "speaker_id": "1",
          "time": "20210906 02:52"
        },
        {
          "id": "MDRW2100041488.1.17",
          "form": "넹넹!! ㅋㅋㅋㅋ 로제찜닭도 맛있어용ㅋㅋㅋ",
          "original_form": "넹넹!! ㅋㅋㅋㅋ 로제찜닭도 맛있어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 02:57"
        },
        {
          "id": "MDRW2100041488.1.18",
          "form": "치킨 좋아해용?",
          "original_form": "치킨 좋아해용?",
          "speaker_id": "2",
          "time": "20210906 02:58"
        },
        {
          "id": "MDRW2100041488.1.19",
          "form": "왕 찜닭도 나왔다고요?!",
          "original_form": "왕 찜닭도 나왔다고요?!",
          "speaker_id": "1",
          "time": "20210906 02:58"
        },
        {
          "id": "MDRW2100041488.1.20",
          "form": "당근이죠 치느님이신데요!!",
          "original_form": "당근이죠 치느님이신데요!!",
          "speaker_id": "1",
          "time": "20210906 02:58"
        },
        {
          "id": "MDRW2100041488.1.21",
          "form": "최애 치킨 뭐에요?",
          "original_form": "최애 치킨 뭐에요?",
          "speaker_id": "2",
          "time": "20210906 02:59"
        },
        {
          "id": "MDRW2100041488.1.22",
