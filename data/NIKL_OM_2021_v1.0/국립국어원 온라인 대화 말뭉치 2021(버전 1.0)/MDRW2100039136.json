        },
        {
          "id": "MDRW2100039136.1.2",
          "form": "깜박햇슈",
          "original_form": "깜박햇슈",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100039136.1.3",
          "form": "영상미쳤어요",
          "original_form": "영상미쳤어요",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100039136.1.4",
          "form": "기대주게임이에요",
          "original_form": "기대주게임이에요",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100039136.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100039136.1.6",
          "form": "기대주 기대되네요",
          "original_form": "기대주 기대되네요",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100039136.1.7",
          "form": "영상보고 사람들이 엄청기다려요",
          "original_form": "영상보고 사람들이 엄청기다려요",
          "speaker_id": "1",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100039136.1.8",
          "form": "아프간영상 믄저봐야하는디",
          "original_form": "아프간영상 믄저봐야하는디",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100039136.1.9",
          "form": "그거 먼저보세요",
          "original_form": "그거 먼저보세요",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100039136.1.10",
          "form": "아프칸이 더 중요하죠",
          "original_form": "아프칸이 더 중요하죠",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100039136.1.11",
          "form": "제가드린 차지연영상은 보셨슈?",
          "original_form": "제가드린 차지연영상은 보셨슈?",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100039136.1.12",
          "form": "봤쥬",
          "original_form": "봤쥬",
          "speaker_id": "1",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100039136.1.13",
          "form": "잘하쥬",
          "original_form": "잘하쥬",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100039136.1.14",
          "form": "잘하쥬",
          "original_form": "잘하쥬",
          "speaker_id": "1",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100039136.1.15",
          "form": "최고쥬",
          "original_form": "최고쥬",
          "speaker_id": "1",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100039136.1.16",
          "form": "최고야유",
          "original_form": "최고야유",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100039136.1.17",
          "form": "근데 오늘은 쟈철서 영상안보구 겜하면서왔쥬",
          "original_form": "근데 오늘은 쟈철서 영상안보구 겜하면서왔쥬",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100039136.1.18",
          "form": "ㅋㅋㅋㅋㅋ어다갔어요?",
          "original_form": "ㅋㅋㅋㅋㅋ어다갔어요?",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100039136.1.19",
          "form": "영상편집사무실여",
          "original_form": "영상편집사무실여",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100039136.1.20",
          "form": "? 취업했슈?",
          "original_form": "? 취업했슈?",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100039136.1.21",
          "form": "봉사요",
          "original_form": "봉사요",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100039136.1.22",
          "form": "이것저것함서 외주하지유",
          "original_form": "이것저것함서 외주하지유",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100039136.1.23",
          "form": "아 꾸준하시네요",
          "original_form": "아 꾸준하시네요",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100039136.1.24",
          "form": "집에선 겜만하기때문에 어짜피",
          "original_form": "집에선 겜만하기때문에 어짜피",
          "speaker_id": "2",
