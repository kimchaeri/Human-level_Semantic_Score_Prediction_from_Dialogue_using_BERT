      "utterance": [
        {
          "id": "MDRW2100004911.1.1",
          "form": "병아리 같구먼ㅋㅋㅋ",
          "original_form": "병아리 같구먼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004911.1.2",
          "form": "진짜 삐약이 뺙뺙",
          "original_form": "진짜 삐약이 뺙뺙",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004911.1.3",
          "form": "정준하 오랜만이네ㅋㅋㅋ",
          "original_form": "정준하 오랜만이네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004911.1.4",
          "form": "무도 느낌 나서 자꾸 놀뭐를 무도라 할뻔 ㅋㅋ",
          "original_form": "무도 느낌 나서 자꾸 놀뭐를 무도라 할뻔 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004911.1.5",
          "form": "나도 무도같다 생각함ㅋㅋㅋㅋ",
          "original_form": "나도 무도같다 생각함ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004911.1.6",
          "form": "유재석이랑 유빈 선수 아버님이 동갑이라니 ㅋㅋㅋ 멤버들도 나이 많이 먹었다 ㅋㅋㅋ",
          "original_form": "유재석이랑 유빈 선수 아버님이 동갑이라니 ㅋㅋㅋ 멤버들도 나이 많이 먹었다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004911.1.7",
          "form": "아빠뻘이네ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아빠뻘이네ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004911.1.8",
          "form": "아까 떡볶이 ppl하는데 유빈쓰 멘트도 잘치고 못하는게 없더라",
          "original_form": "아까 떡볶이 ppl하는데 유빈쓰 멘트도 잘치고 못하는게 없더라",
          "speaker_id": "1",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004911.1.9",
          "form": "꾕가리로도 탁구 잘치네ㅋㅋ 진짜 못하는게 없어ㅋㅋㅋ",
          "original_form": "꾕가리로도 탁구 잘치네ㅋㅋ 진짜 못하는게 없어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:30"
        },
        {
          "id": "MDRW2100004911.1.10",
          "form": "걍 하는 짓도 예쁜듯 애긔라 그런가 ㅋㅋㅋㅋㅋ",
          "original_form": "걍 하는 짓도 예쁜듯 애긔라 그런가 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004911.1.11",
          "form": "볼살 통통ㅋㅋ 사탕물고 있나ㅋㅋ",
          "original_form": "볼살 통통ㅋㅋ 사탕물고 있나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004911.1.12",
          "form": "10대 선수들 다 같이 나오면 재밌었을듯 ㅋㅋ",
          "original_form": "10대 선수들 다 같이 나오면 재밌었을듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004911.1.13",
          "form": "그러면 시청률 대박날것같은디ㅋㅋ",
          "original_form": "그러면 시청률 대박날것같은디ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004911.1.14",
          "form": "어린애기들 친하게 이야기하는것만 봐도 흐뭇",
          "original_form": "어린애기들 친하게 이야기하는것만 봐도 흐뭇",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004911.1.15",
          "form": "좋을때다 이럼서 보겠지ㅋㅋ",
          "original_form": "좋을때다 이럼서 보겠지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
