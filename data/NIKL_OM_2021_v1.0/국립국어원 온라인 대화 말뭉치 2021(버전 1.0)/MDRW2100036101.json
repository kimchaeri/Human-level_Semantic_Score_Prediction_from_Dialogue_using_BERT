      "utterance": [
        {
          "id": "MDRW2100036101.1.1",
          "form": "구매 예정 물품이 있으신가요?",
          "original_form": "구매 예정 물품이 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100036101.1.2",
          "form": "전.. 결명차",
          "original_form": "전.. 결명차",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036101.1.3",
          "form": "이런 거 구매 고민중이에요 ㅎ",
          "original_form": "이런 거 구매 고민중이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036101.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036101.1.5",
          "form": "저는 폰스트랩이랑 케이스요!",
          "original_form": "저는 폰스트랩이랑 케이스요!",
          "speaker_id": "1",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036101.1.6",
          "form": "오 어떤 스타일의",
          "original_form": "오 어떤 스타일의",
          "speaker_id": "2",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036101.1.7",
          "form": "케이스 사셨어요",
          "original_form": "케이스 사셨어요",
          "speaker_id": "2",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100036101.1.8",
          "form": "아직 고민중이에요!",
          "original_form": "아직 고민중이에요!",
          "speaker_id": "1",
          "time": "20210830 16:36"
        },
        {
          "id": "MDRW2100036101.1.9",
          "form": "아 ㅎ 고민이시라고 하셨군요",
          "original_form": "아 ㅎ 고민이시라고 하셨군요",
          "speaker_id": "2",
          "time": "20210830 16:37"
        },
        {
          "id": "MDRW2100036101.1.10",
          "form": "투명케이스를 사고 스트랩에 색을 넣을지, 케이스를 이쁜걸로 라고 스트랩을 단색으로 살지 고민중이에요",
          "original_form": "투명케이스를 사고 스트랩에 색을 넣을지, 케이스를 이쁜걸로 라고 스트랩을 단색으로 살지 고민중이에요",
          "speaker_id": "1",
          "time": "20210830 16:38"
        },
        {
          "id": "MDRW2100036101.1.11",
          "form": "케이스를 예쁜걸로 구매하시죠!",
          "original_form": "케이스를 예쁜걸로 구매하시죠!",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100036101.1.12",
          "form": "그 편이 역시 낫겠죠??",
          "original_form": "그 편이 역시 낫겠죠??",
          "speaker_id": "1",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100036101.1.13",
          "form": "네 ㅋㅋㅋㅋ 케이스가 중심이니까요!!",
          "original_form": "네 ㅋㅋㅋㅋ 케이스가 중심이니까요!!",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100036101.1.14",
          "form": "제꺼는 케이스가 별로 없어서",
          "original_form": "제꺼는 케이스가 별로 없어서",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100036101.1.15",
          "form": "살게 없는 거 같아요. ㅜ 못찾는 건가 싶기도 하고",
          "original_form": "살게 없는 거 같아요. ㅜ 못찾는 건가 싶기도 하고",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100036101.1.16",
          "form": "인터넷에도 별로 없나요?",
          "original_form": "인터넷에도 별로 없나요?",
          "speaker_id": "1",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100036101.1.17",
          "form": "네 제가 원하는게.. 흑 ㅠ",
          "original_form": "네 제가 원하는게.. 흑 ㅠ",
          "speaker_id": "2",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100036101.1.18",
          "form": "그래서 걍 투명 끼고 있어요",
          "original_form": "그래서 걍 투명 끼고 있어요",
          "speaker_id": "2",
          "time": "20210830 17:14"
        },
        {
          "id": "MDRW2100036101.1.19",
          "form": "케이스는 아이폰이 진짜 많은데 전 갤럭시라ㅠ",
          "original_form": "케이스는 아이폰이 진짜 많은데 전 갤럭시라ㅠ",
          "speaker_id": "1",
          "time": "20210830 17:20"
        },
        {
          "id": "MDRW2100036101.1.20",
          "form": "그러니까요 ㅠ",
          "original_form": "그러니까요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 17:21"
        },
        {
          "id": "MDRW2100036101.1.21",
          "form": "저도 갤럭시....",
          "original_form": "저도 갤럭시....",
