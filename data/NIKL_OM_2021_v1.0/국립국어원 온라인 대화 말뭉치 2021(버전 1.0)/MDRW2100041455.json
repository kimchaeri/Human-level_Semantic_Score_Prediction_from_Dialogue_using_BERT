      "utterance": [
        {
          "id": "MDRW2100041455.1.1",
          "form": "ㅎㅇ",
          "original_form": "ㅎㅇ",
          "speaker_id": "2",
          "time": "20210906 00:23"
        },
        {
          "id": "MDRW2100041455.1.2",
          "form": "요즘 무슨 드라마 보고 있니",
          "original_form": "요즘 무슨 드라마 보고 있니",
          "speaker_id": "1",
          "time": "20210906 00:24"
        },
        {
          "id": "MDRW2100041455.1.3",
          "form": "요즘 갯마을 차차차 가 유명하던데",
          "original_form": "요즘 갯마을 차차차 가 유명하던데",
          "speaker_id": "2",
          "time": "20210906 00:24"
        },
        {
          "id": "MDRW2100041455.1.4",
          "form": "아 그 김선호 나오는거 맞나",
          "original_form": "아 그 김선호 나오는거 맞나",
          "speaker_id": "1",
          "time": "20210906 00:24"
        },
        {
          "id": "MDRW2100041455.1.5",
          "form": "응 맞아 신민아도 나와",
          "original_form": "응 맞아 신민아도 나와",
          "speaker_id": "2",
          "time": "20210906 00:24"
        },
        {
          "id": "MDRW2100041455.1.6",
          "form": "주인공들 비주얼이 너무 좋네",
          "original_form": "주인공들 비주얼이 너무 좋네",
          "speaker_id": "1",
          "time": "20210906 00:24"
        },
        {
          "id": "MDRW2100041455.1.7",
          "form": "맞아 게다가 드라마도 재미있는 것 같더라고",
          "original_form": "맞아 게다가 드라마도 재미있는 것 같더라고",
          "speaker_id": "2",
          "time": "20210906 00:24"
        },
        {
          "id": "MDRW2100041455.1.8",
          "form": "로코 같은 거 너무 좋아",
          "original_form": "로코 같은 거 너무 좋아",
          "speaker_id": "1",
          "time": "20210906 00:24"
        },
        {
          "id": "MDRW2100041455.1.9",
          "form": "김선호 가 웃는 게 설레더라고",
          "original_form": "김선호 가 웃는 게 설레더라고",
          "speaker_id": "2",
          "time": "20210906 00:24"
        },
        {
          "id": "MDRW2100041455.1.10",
          "form": "그건 티비 엔 드라마 지",
          "original_form": "그건 티비 엔 드라마 지",
          "speaker_id": "1",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041455.1.11",
          "form": "응 맞어 주말에 하는 듯",
          "original_form": "응 맞어 주말에 하는 듯",
          "speaker_id": "2",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041455.1.12",
          "form": "나도 그거 좀 봐볼까",
          "original_form": "나도 그거 좀 봐볼까",
          "speaker_id": "1",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041455.1.13",
          "form": "시청률이 꾸준히 오르고 있어",
          "original_form": "시청률이 꾸준히 오르고 있어",
          "speaker_id": "2",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041455.1.14",
          "form": "오진짜 재밌나 보네",
          "original_form": "오진짜 재밌나 보네",
          "speaker_id": "1",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041455.1.15",
          "form": "그런가봐 관련 글도 많이 봤어",
          "original_form": "그런가봐 관련 글도 많이 봤어",
          "speaker_id": "2",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041455.1.16",
          "form": "역시 김선호는 대세 구나",
          "original_form": "역시 김선호는 대세 구나",
          "speaker_id": "1",
          "time": "20210906 00:25"
        },
        {
          "id": "MDRW2100041455.1.17",
          "form": "웃는 게 너무 예뻐",
          "original_form": "웃는 게 너무 예뻐",
          "speaker_id": "2",
          "time": "20210906 00:25"
        }
      ]
