      "utterance": [
        {
          "id": "MDRW2100002964.1.1",
          "form": "나 전에 킹덤 봤었는데 지금 뒷시리즈 기다리고 있어",
          "original_form": "나 전에 킹덤 봤었는데 지금 뒷시리즈 기다리고 있어",
          "speaker_id": "2",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002964.1.2",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002964.1.3",
          "form": "난 킹덤은 안봤네",
          "original_form": "난 킹덤은 안봤네",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002964.1.4",
          "form": "재미있어?",
          "original_form": "재미있어?",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002964.1.5",
          "form": "응 좀비물이긴한데 꽤 볼만해",
          "original_form": "응 좀비물이긴한데 꽤 볼만해",
          "speaker_id": "2",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002964.1.6",
          "form": "잔인한 장면 나와?",
          "original_form": "잔인한 장면 나와?",
          "speaker_id": "1",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002964.1.7",
          "form": "좀비물이니까 약간 좀 있어",
          "original_form": "좀비물이니까 약간 좀 있어",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002964.1.8",
          "form": "어떤식의 재미가 있는지 궁금하다",
          "original_form": "어떤식의 재미가 있는지 궁금하다",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002964.1.9",
          "form": "심장이 두근두근해?",
          "original_form": "심장이 두근두근해?",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002964.1.10",
          "form": "좀 갑툭튀도 있어",
          "original_form": "좀 갑툭튀도 있어",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002964.1.11",
          "form": "근데 진짜 재밋더라고",
          "original_form": "근데 진짜 재밋더라고",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002964.1.12",
          "form": "추천해",
          "original_form": "추천해",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002964.1.13",
          "form": "한번 볼게",
          "original_form": "한번 볼게",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002964.1.14",
          "form": "어차피 넷플 보니깐",
          "original_form": "어차피 넷플 보니깐",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002964.1.15",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002964.1.16",
          "form": "넌 혹시 추천할거 있어?",
          "original_form": "넌 혹시 추천할거 있어?",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002964.1.17",
          "form": "나는 테일러 스위프트 좋아해서",
          "original_form": "나는 테일러 스위프트 좋아해서",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002964.1.18",
          "form": "최근에 테일러 스위프트 다큐멘터리랑 콘서트 영상 봤어",
          "original_form": "최근에 테일러 스위프트 다큐멘터리랑 콘서트 영상 봤어",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002964.1.19",
          "form": "그건 무슨 내용이야?",
          "original_form": "그건 무슨 내용이야?",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002964.1.20",
          "form": "테일러 스위프트가 10년이 넘는 시간동안",
          "original_form": "테일러 스위프트가 10년이 넘는 시간동안",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002964.1.21",
          "form": "어떻게 미국에서 살아남았는지에 대한 내용이야",
          "original_form": "어떻게 미국에서 살아남았는지에 대한 내용이야",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002964.1.22",
          "form": "그리고 또 재미있게 본건 굿플레이스",
          "original_form": "그리고 또 재미있게 본건 굿플레이스",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002964.1.23",
          "form": "생윤 윤사 내용 나와",
          "original_form": "생윤 윤사 내용 나와",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002964.1.24",
          "form": "아하 나중에 한번 봐볼까 싶네",
          "original_form": "아하 나중에 한번 봐볼까 싶네",
          "speaker_id": "2",
          "time": "20210820 17:24"
        }
      ]
    }
