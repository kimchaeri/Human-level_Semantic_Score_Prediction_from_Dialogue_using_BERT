      "utterance": [
        {
          "id": "MDRW2100003135.1.1",
          "form": "이불 새로 사고 싶다",
          "original_form": "이불 새로 사고 싶다",
          "speaker_id": "2",
          "time": "20210820 19:14"
        },
        {
          "id": "MDRW2100003135.1.2",
          "form": "덮는거?",
          "original_form": "덮는거?",
          "speaker_id": "1",
          "time": "20210820 19:14"
        },
        {
          "id": "MDRW2100003135.1.3",
          "form": "덮는거 까는거 둘 다 ㅎㅎ",
          "original_form": "덮는거 까는거 둘 다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 19:14"
        },
        {
          "id": "MDRW2100003135.1.4",
          "form": "너무 오래되긴 했지",
          "original_form": "너무 오래되긴 했지",
          "speaker_id": "1",
          "time": "20210820 19:14"
        },
        {
          "id": "MDRW2100003135.1.5",
          "form": "패드도 침대보다 작아서 좀 넉넉한 걸로 사고 싶어",
          "original_form": "패드도 침대보다 작아서 좀 넉넉한 걸로 사고 싶어",
          "speaker_id": "2",
          "time": "20210820 19:15"
        },
        {
          "id": "MDRW2100003135.1.6",
          "form": "사요 베게커버도 바꿔야겠던데",
          "original_form": "사요 베게커버도 바꿔야겠던데",
          "speaker_id": "1",
          "time": "20210820 19:15"
        },
        {
          "id": "MDRW2100003135.1.7",
          "form": "ㅇㅇ 요즘엔 세트로 파니깐",
          "original_form": "ㅇㅇ 요즘엔 세트로 파니깐",
          "speaker_id": "2",
          "time": "20210820 19:15"
        },
        {
          "id": "MDRW2100003135.1.8",
          "form": "어머님이 만들어주신 걸로 오래 버텼네 ㅎㅎ",
          "original_form": "어머님이 만들어주신 걸로 오래 버텼네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:15"
        },
        {
          "id": "MDRW2100003135.1.9",
          "form": "그러게~ 10년 넘게 썼으니 이제 좀 예쁜 디자인으로 바꾸고 싶네",
          "original_form": "그러게~ 10년 넘게 썼으니 이제 좀 예쁜 디자인으로 바꾸고 싶네",
          "speaker_id": "2",
          "time": "20210820 19:16"
        },
        {
          "id": "MDRW2100003135.1.10",
          "form": "맘에 드는 걸로 바꿔 ㅎㅎ",
          "original_form": "맘에 드는 걸로 바꿔 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:16"
        },
        {
          "id": "MDRW2100003135.1.11",
          "form": "그래도 될까?",
          "original_form": "그래도 될까?",
          "speaker_id": "2",
          "time": "20210820 19:16"
        },
        {
          "id": "MDRW2100003135.1.12",
          "form": "고럼 되고 말고 ㅋㅋ",
          "original_form": "고럼 되고 말고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:16"
        },
        {
          "id": "MDRW2100003135.1.13",
          "form": "요즘 은근히 나가는 돈이 많아서리 ㅎㅎ",
          "original_form": "요즘 은근히 나가는 돈이 많아서리 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 19:16"
        },
        {
          "id": "MDRW2100003135.1.14",
          "form": "어차피 사야 되는 건데 뭘~ 매일 사용하기도 하고",
          "original_form": "어차피 사야 되는 건데 뭘~ 매일 사용하기도 하고",
