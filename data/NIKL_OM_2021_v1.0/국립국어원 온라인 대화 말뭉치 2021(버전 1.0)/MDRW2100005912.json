      "utterance": [
        {
          "id": "MDRW2100005912.1.1",
          "form": "자소서....그건 너무 어렵긴 해 나한테...",
          "original_form": "자소서....그건 너무 어렵긴 해 나한테...",
          "speaker_id": "2",
          "time": "20210907 16:18"
        },
        {
          "id": "MDRW2100005912.1.2",
          "form": "근데 왜 자소서가 아니라 이력서야..??",
          "original_form": "근데 왜 자소서가 아니라 이력서야..??",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100005912.1.3",
          "form": "이력서에 적어야돼",
          "original_form": "이력서에 적어야돼",
          "speaker_id": "1",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005912.1.4",
          "form": "존경하는인물과 사유",
          "original_form": "존경하는인물과 사유",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100005912.1.5",
          "form": "취미 특기도 적고",
          "original_form": "취미 특기도 적고",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100005912.1.6",
          "form": "자소서보다 난감해 그냥 아빠라고할까",
          "original_form": "자소서보다 난감해 그냥 아빠라고할까",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100005912.1.7",
          "form": "아직 시간 있으니꺼",
          "original_form": "아직 시간 있으니꺼",
          "speaker_id": "2",
          "time": "20210907 17:44"
        },
        {
          "id": "MDRW2100005912.1.8",
          "form": "천천히 생각해봐 엄청 길게 쓰는 그런건 아니니까",
          "original_form": "천천히 생각해봐 엄청 길게 쓰는 그런건 아니니까",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005912.1.9",
          "form": "김연경 올해 많을것 같다구 하더람,,",
          "original_form": "김연경 올해 많을것 같다구 하더람,,",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100005912.1.10",
          "form": "삼성은 왜이런걸 물어보는지",
          "original_form": "삼성은 왜이런걸 물어보는지",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100005912.1.11",
          "form": "아무래도 핫한 인물은 많이 뜨지",
          "original_form": "아무래도 핫한 인물은 많이 뜨지",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100005912.1.12",
          "form": "이력서 주제에",
          "original_form": "이력서 주제에",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100005912.1.13",
          "form": "태양의 후예 방영했을 때 직업군인 지원자 엄청 늘었대잖아",
          "original_form": "태양의 후예 방영했을 때 직업군인 지원자 엄청 늘었대잖아",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100005912.1.14",
          "form": "잘 할 수 있어 천천히 생각해봐",
          "original_form": "잘 할 수 있어 천천히 생각해봐",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100005912.1.15",
          "form": "응?? 그거랑 무슨 상관이야?? ㅋㅋㅋㅋ 나는 삼성 지원하는건데",
          "original_form": "응?? 그거랑 무슨 상관이야?? ㅋㅋㅋㅋ 나는 삼성 지원하는건데",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100005912.1.16",
          "form": "아니 그냥 그 시기에 핫한 키워드나 그런게 취업하고 직결되는 것 같다고",
          "original_form": "아니 그냥 그 시기에 핫한 키워드나 그런게 취업하고 직결되는 것 같다고",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100005912.1.17",
          "form": "자소서든 지원하는 직업군이든 그럴때",
          "original_form": "자소서든 지원하는 직업군이든 그럴때",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100005912.1.18",
          "form": "있었었지",
          "original_form": "있었었지",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100005912.1.19",
          "form": "아?? 구랭?",
          "original_form": "아?? 구랭?",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100005912.1.20",
          "form": "전역 안하려했었잖아",
          "original_form": "전역 안하려했었잖아",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100005912.1.21",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100005912.1.22",
          "form": "직업 군인은 그래도 자소서 안쓰지?",
          "original_form": "직업 군인은 그래도 자소서 안쓰지?",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100005912.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ대신 체력 테스트 보는건가",
          "original_form": "ㅋㅋㅋㅋㅋㅋ대신 체력 테스트 보는건가",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100005912.1.24",
          "form": "안 쓰지 ㅋㅋㅋㅋ맞아",
          "original_form": "안 쓰지 ㅋㅋㅋㅋ맞아",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100005912.1.25",
          "form": "면접도 보고",
          "original_form": "면접도 보고",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100005912.1.26",
          "form": "역사나 그런 시험도 보긴 해 난이도는 낮지만",
          "original_form": "역사나 그런 시험도 보긴 해 난이도는 낮지만",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100005912.1.27",
          "form": "면접은 거기도 보는구낭",
          "original_form": "면접은 거기도 보는구낭",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100005912.1.28",
          "form": "그럼그럼 다 같지 그런건",
          "original_form": "그럼그럼 다 같지 그런건",
          "speaker_id": "2",
          "time": "20210908 00:27"
        }
      ]
