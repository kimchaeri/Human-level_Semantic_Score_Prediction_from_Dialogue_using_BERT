      "utterance": [
        {
          "id": "MDRW2100027263.1.1",
          "form": "어..그건 너무 활동적이라 그런 거잖아욧!!",
          "original_form": "어..그건 너무 활동적이라 그런 거잖아욧!!",
          "speaker_id": "1",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100027263.1.2",
          "form": "한동안 운동안하다가",
          "original_form": "한동안 운동안하다가",
          "speaker_id": "2",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100027263.1.3",
          "form": "아 그때 이직한다고",
          "original_form": "아 그때 이직한다고",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027263.1.4",
          "form": "이직 축하경기로",
          "original_form": "이직 축하경기로",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027263.1.5",
          "form": "했엇거든요 축구를...",
          "original_form": "했엇거든요 축구를...",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027263.1.6",
          "form": "이직 축하경기ㅋㅋㅋ 유쾌 그 자체였는데",
          "original_form": "이직 축하경기ㅋㅋㅋ 유쾌 그 자체였는데",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027263.1.7",
          "form": "ㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027263.1.8",
          "form": "속상합니다",
          "original_form": "속상합니다",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027263.1.9",
          "form": "지금도 축구 골프 수영 다 하시는거죠?",
          "original_form": "지금도 축구 골프 수영 다 하시는거죠?",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027263.1.10",
          "form": "오십견 36에 오네요",
          "original_form": "오십견 36에 오네요",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027263.1.11",
          "form": "몸으로 1등하는 님도 오는데ㅜ",
          "original_form": "몸으로 1등하는 님도 오는데ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027263.1.12",
          "form": "네 지금은",
          "original_form": "네 지금은",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027263.1.13",
          "form": "축구랑 철봉만 ㅎㅎ",
          "original_form": "축구랑 철봉만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027263.1.14",
          "form": "수영은 잠이와서 포기",
          "original_form": "수영은 잠이와서 포기",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027263.1.15",
          "form": "2등부터 찌끄러기 순위는 또 얼마나 비실대겠어요ㅜ",
          "original_form": "2등부터 찌끄러기 순위는 또 얼마나 비실대겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027263.1.16",
          "form": "철봉ㄷㄷㄷ",
          "original_form": "철봉ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027263.1.17",
          "form": "아니에요 ㅋㅋㅋ",
          "original_form": "아니에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027263.1.18",
          "form": "진짜 건강하시군요ㅎ",
          "original_form": "진짜 건강하시군요ㅎ",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027263.1.19",
          "form": "철봉 추천합니다",
          "original_form": "철봉 추천합니다",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027263.1.20",
          "form": "전신운동이에요",
          "original_form": "전신운동이에요",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027263.1.21",
          "form": "코어도 좋아지구",
          "original_form": "코어도 좋아지구",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027263.1.22",
          "form": "ㅎㅎ 제가 고소공포증이 있어서리",
          "original_form": "ㅎㅎ 제가 고소공포증이 있어서리",
