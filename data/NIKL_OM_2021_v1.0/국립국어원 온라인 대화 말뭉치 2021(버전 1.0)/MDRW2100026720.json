      "utterance": [
        {
          "id": "MDRW2100026720.1.1",
          "form": "네 무려 900화가 넘은 소설....",
          "original_form": "네 무려 900화가 넘은 소설....",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026720.1.2",
          "form": "근데 아직 최종보스도 안 나온...",
          "original_form": "근데 아직 최종보스도 안 나온...",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026720.1.3",
          "form": "헐 역시 무협쪽은 볼륨이랑 스케일 장난 없나부당 ㅇㅁㅇ",
          "original_form": "헐 역시 무협쪽은 볼륨이랑 스케일 장난 없나부당 ㅇㅁㅇ",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026720.1.4",
          "form": "최소 1000화잖아요 그럼",
          "original_form": "최소 1000화잖아요 그럼",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026720.1.5",
          "form": "지금 970화 넘었고.... 아직 천마가 안 나왔어요 기승전결중 전 초반부 쯤 아닐까?",
          "original_form": "지금 970화 넘었고.... 아직 천마가 안 나왔어요 기승전결중 전 초반부 쯤 아닐까?",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026720.1.6",
          "form": "헐 천마 아직도???",
          "original_form": "헐 천마 아직도???",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026720.1.7",
          "form": "이거 완전 0.5멸살법 찍겠네요",
          "original_form": "이거 완전 0.5멸살법 찍겠네요",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026720.1.8",
          "form": "근데 재밌죵",
          "original_form": "근데 재밌죵",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026720.1.9",
          "form": "그쵸 주인공 제 할아버지 됐어요",
          "original_form": "그쵸 주인공 제 할아버지 됐어요",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026720.1.10",
          "form": "효도할거야 용서못해",
          "original_form": "효도할거야 용서못해",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026720.1.11",
          "form": "할아버지가 주인공이에요?ㅠㅠㅠㅠㅠㅠㅠ아 머야",
          "original_form": "할아버지가 주인공이에요?ㅠㅠㅠㅠㅠㅠㅠ아 머야",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026720.1.12",
          "form": "주인공 아닌줄 알았다구요",
          "original_form": "주인공 아닌줄 알았다구요",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026720.1.13",
          "form": "사존같은 건 줄 알았다구요~!~!~",
          "original_form": "사존같은 건 줄 알았다구요~!~!~",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026720.1.14",
          "form": "효도ㅋㅋㅋㅋㅋㅋ",
          "original_form": "효도ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026720.1.15",
          "form": "80살인데 무공이 고강해서 30대 외양으로 살다가",
          "original_form": "80살인데 무공이 고강해서 30대 외양으로 살다가",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026720.1.16",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026720.1.17",
          "form": "천마 죽이고 죽어서 환생했더니",
          "original_form": "천마 죽이고 죽어서 환생했더니",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026720.1.18",
          "form": "문파가 쫄딱 거지가 됏어요",
          "original_form": "문파가 쫄딱 거지가 됏어요",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026720.1.19",
          "form": "잉 똑땅해",
          "original_form": "잉 똑땅해",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026720.1.20",
          "form": "그래서 다시 문파 일으키는 건가요",
          "original_form": "그래서 다시 문파 일으키는 건가요",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026720.1.21",
          "form": "그래서 까마득한 사존인데 문파 막내로 들어가서 윗놈들 두들겨 패면서 키워서 문파 일으켜요",
          "original_form": "그래서 까마득한 사존인데 문파 막내로 들어가서 윗놈들 두들겨 패면서 키워서 문파 일으켜요",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026720.1.22",
          "form": "그래서..사존인데 막내야",
          "original_form": "그래서..사존인데 막내야",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026720.1.23",
          "form": "?? 막내가 윗놈을 패",
          "original_form": "?? 막내가 윗놈을 패",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026720.1.24",
          "form": "제법 멋진데",
          "original_form": "제법 멋진데",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026720.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ멋진 장르잖아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ멋진 장르잖아요",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026720.1.26",
          "form": "막내가 패서 가르치는 실질적 스승이라 서열이 망했답니다",
          "original_form": "막내가 패서 가르치는 실질적 스승이라 서열이 망했답니다",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026720.1.27",
          "form": "할배... 그 나이에 고생해",
          "original_form": "할배... 그 나이에 고생해",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026720.1.28",
          "form": "천마한테 가족같은 사형제들 다 잃고...",
          "original_form": "천마한테 가족같은 사형제들 다 잃고...",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026720.1.29",
          "form": "나라도 효도해야..",
          "original_form": "나라도 효도해야..",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026720.1.30",
          "form": "큐ㅠㅠㅠㅠㅠㅠㅠ 효심 지극하게 하네",
          "original_form": "큐ㅠㅠㅠㅠㅠㅠㅠ 효심 지극하게 하네",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026720.1.31",
          "form": "서열망한 것도 웃겨요",
          "original_form": "서열망한 것도 웃겨요",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026720.1.32",
          "form": "저 무협알못인데",
          "original_form": "저 무협알못인데",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026720.1.33",
          "form": "원래 서열 빡세지 않냐구요ㅠㅠ",
          "original_form": "원래 서열 빡세지 않냐구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026720.1.34",
          "form": "아.... 천마 아직도 안 나온 거 정말",
          "original_form": "아.... 천마 아직도 안 나온 거 정말",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026720.1.35",
          "form": "애태우겠는데요...",
          "original_form": "애태우겠는데요...",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026720.1.36",
          "form": "빡세죠... 사실 저도 무협알못이지만ㅎ 할배가 생태파괴범 같은 거인 것 같아...",
