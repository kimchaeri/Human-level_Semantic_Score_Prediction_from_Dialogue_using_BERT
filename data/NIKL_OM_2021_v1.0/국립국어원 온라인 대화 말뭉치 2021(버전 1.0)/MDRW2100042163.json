      "utterance": [
        {
          "id": "MDRW2100042163.1.1",
          "form": "저녁 먹었어?",
          "original_form": "저녁 먹었어?",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100042163.1.2",
          "form": "왜 치킨 먹으러가지ㅠㅠ",
          "original_form": "왜 치킨 먹으러가지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100042163.1.3",
          "form": "엉??",
          "original_form": "엉??",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042163.1.4",
          "form": "나는 아직ㅠㅠ 다이어트 중이라",
          "original_form": "나는 아직ㅠㅠ 다이어트 중이라",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042163.1.5",
          "form": "그렇구나ㅠㅠㅠ",
          "original_form": "그렇구나ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042163.1.6",
          "form": "이제 겨울인데 다이어트 그만해ㅠㅠ",
          "original_form": "이제 겨울인데 다이어트 그만해ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042163.1.7",
          "form": "ㅋㅋㅋㅋㅋ 걱정 고마워",
          "original_form": "ㅋㅋㅋㅋㅋ 걱정 고마워",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042163.1.8",
          "form": "무슨 치킨 좋아해?",
          "original_form": "무슨 치킨 좋아해?",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042163.1.9",
          "form": "난 굽네나 지코바 쥬아해 너는?",
          "original_form": "난 굽네나 지코바 쥬아해 너는?",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042163.1.10",
          "form": "나도 굽네 좋아하고 처갓집도 요즘 많이 먹고 있어",
          "original_form": "나도 굽네 좋아하고 처갓집도 요즘 많이 먹고 있어",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042163.1.11",
          "form": "고바삭... 맛있어",
          "original_form": "고바삭... 맛있어",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042163.1.12",
          "form": "와 처갓집 무슨일이야 정날 말있겠다",
          "original_form": "와 처갓집 무슨일이야 정날 말있겠다",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042163.1.13",
          "form": "근데 고바삭 줄임말 좋다",
          "original_form": "근데 고바삭 줄임말 좋다",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042163.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 많이 애용하고 있어...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 많이 애용하고 있어...",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042163.1.15",
          "form": "슈프림 치킨은 오리지널이 제일 맛있더라고~",
          "original_form": "슈프림 치킨은 오리지널이 제일 맛있더라고~",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042163.1.16",
          "form": "어제 자담치킨 그라파다노 치킨도 먹어봤는데 뿌링클 좋아하면 맛있게 먹을 거 같아",
          "original_form": "어제 자담치킨 그라파다노 치킨도 먹어봤는데 뿌링클 좋아하면 맛있게 먹을 거 같아",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042163.1.17",
          "form": "와 슈프림치킨 !! 이름부터 맛있다",
          "original_form": "와 슈프림치킨 !! 이름부터 맛있다",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042163.1.18",
          "form": "아아 자담은 아직 못먹어봤어ㅠㅠ",
          "original_form": "아아 자담은 아직 못먹어봤어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042163.1.19",
          "form": "나도 어제 처음 먹어봤는데 괜찮더라고 ㅎㅎ",
          "original_form": "나도 어제 처음 먹어봤는데 괜찮더라고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042163.1.20",
          "form": "자담이 평이 진짜 좋더라 주변에서도. 조정석이 광고하디?",
          "original_form": "자담이 평이 진짜 좋더라 주변에서도. 조정석이 광고하디?",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042163.1.21",
          "form": "응ㅋㅋㅋㅋㅋ 나도 그 브랜드 조정석이 광고하는 거 보고 알았어",
          "original_form": "응ㅋㅋㅋㅋㅋ 나도 그 브랜드 조정석이 광고하는 거 보고 알았어",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042163.1.22",
          "form": "맵슐랭이 제일 유명한 거 같더라",
          "original_form": "맵슐랭이 제일 유명한 거 같더라",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042163.1.23",
          "form": "아 맵슐랭 ㅋㅋㅋㅋㅋ",
          "original_form": "아 맵슐랭 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042163.1.24",
          "form": "치킨 이름들이 다 너무 ㄱ ㅣ엽네",
          "original_form": "치킨 이름들이 다 너무 ㄱ ㅣ엽네",
