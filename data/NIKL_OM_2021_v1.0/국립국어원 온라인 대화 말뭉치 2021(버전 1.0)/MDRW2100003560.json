      "utterance": [
        {
          "id": "MDRW2100003560.1.1",
          "form": "신나서 ㅋㅋ 시간 가는줄 모르네여",
          "original_form": "신나서 ㅋㅋ 시간 가는줄 모르네여",
          "speaker_id": "1",
          "time": "20210821 01:25"
        },
        {
          "id": "MDRW2100003560.1.2",
          "form": "역시 추억의 힘!",
          "original_form": "역시 추억의 힘!",
          "speaker_id": "1",
          "time": "20210821 01:25"
        },
        {
          "id": "MDRW2100003560.1.3",
          "form": "그러니깐요ㅎㅎㅎ 추억은 너무좋아요ㅎㅎ",
          "original_form": "그러니깐요ㅎㅎㅎ 추억은 너무좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 01:25"
        },
        {
          "id": "MDRW2100003560.1.4",
          "form": "옛날이 그립긴 해요",
          "original_form": "옛날이 그립긴 해요",
          "speaker_id": "1",
          "time": "20210821 01:25"
        },
        {
          "id": "MDRW2100003560.1.5",
          "form": "그때는 사귀는 사람하고 헤어지면 미니홈피 사진이랑 케릭터가 불쌍했었어요ㅎㅎ",
          "original_form": "그때는 사귀는 사람하고 헤어지면 미니홈피 사진이랑 케릭터가 불쌍했었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 01:26"
        },
        {
          "id": "MDRW2100003560.1.6",
          "form": "눈물사진에",
          "original_form": "눈물사진에",
          "speaker_id": "2",
          "time": "20210821 01:26"
        },
        {
          "id": "MDRW2100003560.1.7",
          "form": "아 저도 마지막이 좀 백업 안한게",
          "original_form": "아 저도 마지막이 좀 백업 안한게",
          "speaker_id": "1",
          "time": "20210821 01:26"
        },
        {
          "id": "MDRW2100003560.1.8",
          "form": "캐릭터는 벽에붙어있었나?ㅎ없어졌나 그랬을꺼에요ㅎ",
          "original_form": "캐릭터는 벽에붙어있었나?ㅎ없어졌나 그랬을꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210821 01:26"
        },
        {
          "id": "MDRW2100003560.1.9",
          "form": "오래 사귄남친 ㅎㅎ 사진이 그대로 잇더라구요",
          "original_form": "오래 사귄남친 ㅎㅎ 사진이 그대로 잇더라구요",
          "speaker_id": "1",
          "time": "20210821 01:26"
        },
        {
          "id": "MDRW2100003560.1.10",
          "form": "저도 후회되네요ㅎㅎ",
          "original_form": "저도 후회되네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 01:26"
        },
        {
          "id": "MDRW2100003560.1.11",
          "form": "그래도 그때 사진보면 풋풋하잖아요ㅎ",
          "original_form": "그래도 그때 사진보면 풋풋하잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003560.1.12",
          "form": "ㅋㅋ 애기애기 햇어요",
          "original_form": "ㅋㅋ 애기애기 햇어요",
          "speaker_id": "1",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003560.1.13",
          "form": "저도 아마 있을껀데ㅎㅎ",
          "original_form": "저도 아마 있을껀데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003560.1.14",
          "form": "그런건 진짜 추억인데ㄹㅎ",
          "original_form": "그런건 진짜 추억인데ㄹㅎ",
          "speaker_id": "2",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003560.1.15",
          "form": "남친이랑 헤어지면서 아예 싸이를 안한서 같아요",
          "original_form": "남친이랑 헤어지면서 아예 싸이를 안한서 같아요",
          "speaker_id": "1",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003560.1.16",
          "form": "정리도 못하고 너무 어이없데 헤어져서 ㅋ",
          "original_form": "정리도 못하고 너무 어이없데 헤어져서 ㅋ",
          "speaker_id": "1",
          "time": "20210821 01:28"
        },
        {
          "id": "MDRW2100003560.1.17",
          "form": "그렇죠ㅎ 싸우고헤어지면 무조건 안하죠ㄹ",
          "original_form": "그렇죠ㅎ 싸우고헤어지면 무조건 안하죠ㄹ",
          "speaker_id": "2",
          "time": "20210821 01:28"
        },
        {
          "id": "MDRW2100003560.1.18",
          "form": "지금이야 추억이지만욮ㅋ",
          "original_form": "지금이야 추억이지만욮ㅋ",
          "speaker_id": "1",
          "time": "20210821 01:28"
        },
        {
          "id": "MDRW2100003560.1.19",
          "form": "그때는 그랬죠ㅎ",
          "original_form": "그때는 그랬죠ㅎ",
          "speaker_id": "2",
          "time": "20210821 01:28"
        },
        {
          "id": "MDRW2100003560.1.20",
          "form": "어렷잖아요ㅎㅎ 그럴수있어요ㅎㅎ",
          "original_form": "어렷잖아요ㅎㅎ 그럴수있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 01:28"
        },
        {
          "id": "MDRW2100003560.1.21",
          "form": "싸이월드 부활하면 그때 그대로 부활하게 해줬으면 좋겟어요",
          "original_form": "싸이월드 부활하면 그때 그대로 부활하게 해줬으면 좋겟어요",
          "speaker_id": "2",
          "time": "20210821 01:29"
        },
        {
          "id": "MDRW2100003560.1.22",
          "form": "내가 원하는 시점으로ㅎㅎ name1님은 어디로가실꺼에요?",
          "original_form": "내가 원하는 시점으로ㅎㅎ &name1&님은 어디로가실꺼에요?",
          "speaker_id": "2",
          "time": "20210821 01:29"
        },
        {
          "id": "MDRW2100003560.1.23",
          "form": "음 ~  기대도되고 걱정도 되는 ㅎㅎ?",
          "original_form": "음 ~  기대도되고 걱정도 되는 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210821 01:29"
        },
        {
          "id": "MDRW2100003560.1.24",
          "form": "가장행복했던 때로 돌아가고싶네요ㅎ 정말ㄹㅎ",
          "original_form": "가장행복했던 때로 돌아가고싶네요ㅎ 정말ㄹㅎ",
          "speaker_id": "2",
          "time": "20210821 01:29"
        },
        {
          "id": "MDRW2100003560.1.25",
          "form": "음 전 고3 원서 쓰기전이요 ㅎ",
          "original_form": "음 전 고3 원서 쓰기전이요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 01:30"
        },
        {
          "id": "MDRW2100003560.1.26",
          "form": "그때도 진짜 좋죠ㅎ",
          "original_form": "그때도 진짜 좋죠ㅎ",
          "speaker_id": "2",
          "time": "20210821 01:31"
        },
        {
          "id": "MDRW2100003560.1.27",
          "form": "메가스터디할때ㅎ",
          "original_form": "메가스터디할때ㅎ",
          "speaker_id": "2",
          "time": "20210821 01:31"
        },
        {
          "id": "MDRW2100003560.1.28",
          "form": "메가스터티 들어봣는뎀",
          "original_form": "메가스터티 들어봣는뎀",
          "speaker_id": "1",
          "time": "20210821 01:31"
        },
        {
          "id": "MDRW2100003560.1.29",
          "form": "전 수시합격해서 메가스터디인가 싸이트에서 대학교 모의지원했었는데ㅎ",
          "original_form": "전 수시합격해서 메가스터디인가 싸이트에서 대학교 모의지원했었는데ㅎ",
          "speaker_id": "2",
          "time": "20210821 01:31"
        },
        {
          "id": "MDRW2100003560.1.30",
          "form": "왜전 기억이ㅜ없죠  ㅋ",
          "original_form": "왜전 기억이ㅜ없죠  ㅋ",
          "speaker_id": "1",
          "time": "20210821 01:32"
        },
        {
          "id": "MDRW2100003560.1.31",
          "form": "그게 돈주고하는 학습지같은거여서그래요ㅎ",
          "original_form": "그게 돈주고하는 학습지같은거여서그래요ㅎ",
