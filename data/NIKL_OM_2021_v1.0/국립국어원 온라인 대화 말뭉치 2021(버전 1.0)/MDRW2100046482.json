      "utterance": [
        {
          "id": "MDRW2100046482.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211025 17:28"
        },
        {
          "id": "MDRW2100046482.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211025 17:30"
        },
        {
          "id": "MDRW2100046482.1.3",
          "form": "혹시 곰표 맥주 드셔보셨어요?",
          "original_form": "혹시 곰표 맥주 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20211025 17:30"
        },
        {
          "id": "MDRW2100046482.1.4",
          "form": "네 먹어 봤습니다",
          "original_form": "네 먹어 봤습니다",
          "speaker_id": "1",
          "time": "20211025 17:31"
        },
        {
          "id": "MDRW2100046482.1.5",
          "form": "오 드셔보셨군요!",
          "original_form": "오 드셔보셨군요!",
          "speaker_id": "2",
          "time": "20211025 17:31"
        },
        {
          "id": "MDRW2100046482.1.6",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20211025 17:31"
        },
        {
          "id": "MDRW2100046482.1.7",
          "form": "안먹어봤는데",
          "original_form": "안먹어봤는데",
          "speaker_id": "2",
          "time": "20211025 17:31"
        },
        {
          "id": "MDRW2100046482.1.8",
          "form": "무슨 맛인가용",
          "original_form": "무슨 맛인가용",
          "speaker_id": "2",
          "time": "20211025 17:31"
        },
        {
          "id": "MDRW2100046482.1.9",
          "form": "혹시 곰표 복숭아맛 한번 드셔보세요",
          "original_form": "혹시 곰표 복숭아맛 한번 드셔보세요",
          "speaker_id": "1",
          "time": "20211025 17:31"
        },
        {
          "id": "MDRW2100046482.1.10",
          "form": "아 그게 맛이 다양하게 있는건가요?!",
          "original_form": "아 그게 맛이 다양하게 있는건가요?!",
          "speaker_id": "2",
          "time": "20211025 17:31"
        },
        {
          "id": "MDRW2100046482.1.11",
          "form": "몰랐어요",
          "original_form": "몰랐어요",
          "speaker_id": "2",
          "time": "20211025 17:32"
        },
        {
          "id": "MDRW2100046482.1.12",
          "form": "복숭아맛이 실패하기 어려운데",
          "original_form": "복숭아맛이 실패하기 어려운데",
          "speaker_id": "2",
          "time": "20211025 17:32"
        },
        {
          "id": "MDRW2100046482.1.13",
          "form": "먹어볼게요 ㅎ",
          "original_form": "먹어볼게요 ㅎ",
          "speaker_id": "2",
          "time": "20211025 17:32"
        },
        {
          "id": "MDRW2100046482.1.14",
          "form": "네 맛이 여러개 있어요",
          "original_form": "네 맛이 여러개 있어요",
          "speaker_id": "1",
          "time": "20211025 17:32"
        },
        {
          "id": "MDRW2100046482.1.15",
          "form": "복숭아맛이 따봉인가요",
          "original_form": "복숭아맛이 따봉인가요",
          "speaker_id": "2",
          "time": "20211025 17:32"
        },
        {
          "id": "MDRW2100046482.1.16",
          "form": "네네 조금 음료같기는 한데 가끔씩 그냥 맥주는 질릴때 복숭아맛이 제일 맛있더라구요",
          "original_form": "네네 조금 음료같기는 한데 가끔씩 그냥 맥주는 질릴때 복숭아맛이 제일 맛있더라구요",
          "speaker_id": "1",
          "time": "20211025 17:33"
        },
        {
          "id": "MDRW2100046482.1.17",
          "form": "오호 갑자기 궁금해지네요",
          "original_form": "오호 갑자기 궁금해지네요",
          "speaker_id": "2",
          "time": "20211025 17:33"
        },
        {
          "id": "MDRW2100046482.1.18",
          "form": "먹어봐야겠어요...!",
          "original_form": "먹어봐야겠어요...!",
          "speaker_id": "2",
          "time": "20211025 17:33"
        },
        {
          "id": "MDRW2100046482.1.19",
          "form": "곰표 팝콘도 있더라구요",
          "original_form": "곰표 팝콘도 있더라구요",
          "speaker_id": "2",
          "time": "20211025 17:33"
        },
        {
          "id": "MDRW2100046482.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211025 17:33"
        },
        {
          "id": "MDRW2100046482.1.21",
          "form": "곰표가 참 이래저래",
          "original_form": "곰표가 참 이래저래",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.22",
          "form": "콜라보를 많이하네요",
          "original_form": "콜라보를 많이하네요",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.23",
          "form": "네네 봤어요",
          "original_form": "네네 봤어요",
          "speaker_id": "1",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.24",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.25",
          "form": "드셔보시지는",
          "original_form": "드셔보시지는",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.26",
          "form": "않으신건가용",
          "original_form": "않으신건가용",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.27",
          "form": "네 아직 팝콘은 못먹어봤습니다",
          "original_form": "네 아직 팝콘은 못먹어봤습니다",
          "speaker_id": "1",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.28",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.29",
          "form": "편의점 팝콘 중",
          "original_form": "편의점 팝콘 중",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.30",
          "form": "1등은...",
          "original_form": "1등은...",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.31",
          "form": "씨유 콘소메맛팝콘인데",
          "original_form": "씨유 콘소메맛팝콘인데",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.32",
          "form": "나중에 꼭...꼭 드셔보세요",
          "original_form": "나중에 꼭...꼭 드셔보세요",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.33",
          "form": "벌크업되는맛입니다",
          "original_form": "벌크업되는맛입니다",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.34",
          "form": "맥주가 술술ㄹ",
          "original_form": "맥주가 술술ㄹ",
          "speaker_id": "2",
          "time": "20211025 17:34"
        },
        {
          "id": "MDRW2100046482.1.35",
          "form": "이게 편의점에서 팔다보니...없는지점이 많더라구요",
          "original_form": "이게 편의점에서 팔다보니...없는지점이 많더라구요",
          "speaker_id": "1",
          "time": "20211025 17:35"
        },
        {
          "id": "MDRW2100046482.1.36",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "2",
          "time": "20211025 17:35"
        },
        {
          "id": "MDRW2100046482.1.37",
          "form": "하긴 발주 넣는건 점장 맘이니..",
          "original_form": "하긴 발주 넣는건 점장 맘이니..",
          "speaker_id": "2",
          "time": "20211025 17:35"
        },
        {
          "id": "MDRW2100046482.1.38",
          "form": "흑흑\r\n",
          "original_form": "흑흑\r\n",
          "speaker_id": "2",
          "time": "20211025 17:35"
        }
      ]
