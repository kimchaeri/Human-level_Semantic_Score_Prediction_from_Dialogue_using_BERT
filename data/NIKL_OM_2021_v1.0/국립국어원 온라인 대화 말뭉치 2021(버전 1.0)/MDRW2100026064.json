      "utterance": [
        {
          "id": "MDRW2100026064.1.1",
          "form": "밥드셨나요",
          "original_form": "밥드셨나요",
          "speaker_id": "1",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100026064.1.2",
          "form": "아뇨 이제 먹어야함!",
          "original_form": "아뇨 이제 먹어야함!",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.3",
          "form": "아까 타이 푸드를 시켜서",
          "original_form": "아까 타이 푸드를 시켜서",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.4",
          "form": "밥드세요",
          "original_form": "밥드세요",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.5",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.6",
          "form": "쫌잇다 먹으려구요",
          "original_form": "쫌잇다 먹으려구요",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.7",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.8",
          "form": "오늘 name1님 식단은?!",
          "original_form": "오늘 &name1&님 식단은?!",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.9",
          "form": "뭐시키셨나요",
          "original_form": "뭐시키셨나요",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.10",
          "form": "저는 몰라요",
          "original_form": "저는 몰라요",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.11",
          "form": "아니 왜 몰라요 이사람아",
          "original_form": "아니 왜 몰라요 이사람아",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.12",
          "form": "안드셧구만!",
          "original_form": "안드셧구만!",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026064.1.13",
          "form": "왜냐면.... 가족이 나가자고해야 밥먹는데",
          "original_form": "왜냐면.... 가족이 나가자고해야 밥먹는데",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026064.1.14",
          "form": "옆방의 아부지가 나갈생각이 없어보여요",
          "original_form": "옆방의 아부지가 나갈생각이 없어보여요",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026064.1.15",
          "form": "파인애플 볶음밥 어쩌구 시켯어요 사실 잘 기억나지않음 ㅋㅋ",
          "original_form": "파인애플 볶음밥 어쩌구 시켯어요 사실 잘 기억나지않음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026064.1.16",
          "form": "이럴수가 아부지 ㅠ",
          "original_form": "이럴수가 아부지 ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026064.1.17",
          "form": "마싓겠다",
          "original_form": "마싓겠다",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026064.1.18",
          "form": "name1님 고기드세요... 마싯는거 많이묵쟈",
          "original_form": "&name1&님 고기드세요... 마싯는거 많이묵쟈",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026064.1.19",
          "form": "딸랑구 간식도 다 털어드셔서 배 안고프실듯...",
          "original_form": "딸랑구 간식도 다 털어드셔서 배 안고프실듯...",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026064.1.20",
          "form": "저 오늘 낮에 갈비탕 먹었어요(발그레",
          "original_form": "저 오늘 낮에 갈비탕 먹었어요(발그레",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026064.1.21",
          "form": "이럴수가 마싯는거 드셨잖아",
          "original_form": "이럴수가 마싯는거 드셨잖아",
          "speaker_id": "2",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100026064.1.22",
          "form": "그치만 저녁두 맛난거드셔야한다",
          "original_form": "그치만 저녁두 맛난거드셔야한다",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026064.1.23",
          "form": "name1님 맞다 혹쉬",
          "original_form": "&name1&님 맞다 혹쉬",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026064.1.24",
          "form": "싫어하는 음식 같은거 잇나요?!",
          "original_form": "싫어하는 음식 같은거 잇나요?!",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026064.1.25",
          "form": "저 디따많아요",
          "original_form": "저 디따많아요",
          "speaker_id": "1",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100026064.1.26",
          "form": "걍 노답인듯(...)",
          "original_form": "걍 노답인듯(...)",
          "speaker_id": "1",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100026064.1.27",
          "form": "매운거 싫어하고 떫은거 싫어하고 단거 별루 안좋아해요",
          "original_form": "매운거 싫어하고 떫은거 싫어하고 단거 별루 안좋아해요",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100026064.1.28",
          "form": "음식대는것보다 맛말하는게 빠른편이죠()",
          "original_form": "음식대는것보다 맛말하는게 빠른편이죠()",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100026064.1.29",
          "form": "name2님은 무슨 음식 좋아하시나요",
          "original_form": "&name2&님은 무슨 음식 좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100026064.1.30",
          "form": "ㅋㅋㅋㅋㅋ아 저두 매운거 진짜 못먹어요ㅋㅋㅋ 하 우리 맵찔이들... 나중에 마싯는거 함 먹으러 가야겟다 쥔자",
          "original_form": "ㅋㅋㅋㅋㅋ아 저두 매운거 진짜 못먹어요ㅋㅋㅋ 하 우리 맵찔이들... 나중에 마싯는거 함 먹으러 가야겟다 쥔자",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026064.1.31",
          "form": "저는 담백한거~",
          "original_form": "저는 담백한거~",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026064.1.32",
          "form": "원체 짜고 맵고 이런걸 안 좋아해서 백숙(ㅋㅋ) 죽 칼국수 이런것도 잘먹구요",
          "original_form": "원체 짜고 맵고 이런걸 안 좋아해서 백숙(ㅋㅋ) 죽 칼국수 이런것도 잘먹구요",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026064.1.33",
          "form": "헐 저두 백숙좋아해요",
          "original_form": "헐 저두 백숙좋아해요",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026064.1.34",
          "form": "술안주도 조와함..(소근",
          "original_form": "술안주도 조와함..(소근",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026064.1.35",
          "form": "남의 내장.. 덜매운걸루다가...",
          "original_form": "남의 내장.. 덜매운걸루다가...",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026064.1.36",
          "form": "저기 남한산성쪽에 누룽지 백숙 맛있는 곳 많음요",
          "original_form": "저기 남한산성쪽에 누룽지 백숙 맛있는 곳 많음요",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026064.1.37",
          "form": "이시국에 살아남았을지 모르겠네요....",
          "original_form": "이시국에 살아남았을지 모르겠네요....",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026064.1.38",
          "form": "안돼 ㅠ ㅠ\r\nㅠ ㅠㅠㅠㅠㅠ ㅠ",
          "original_form": "안돼 ㅠ ㅠ\r\nㅠ ㅠㅠㅠㅠㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026064.1.39",
          "form": "name1님이란 꼭 먹으러가야만...",
          "original_form": "&name1&님이란 꼭 먹으러가야만...",
