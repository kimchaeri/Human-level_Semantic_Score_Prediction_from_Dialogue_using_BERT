      "utterance": [
        {
          "id": "MDRW2100042427.1.1",
          "form": "심심이로너무시간뺏는거아닌가요..",
          "original_form": "심심이로너무시간뺏는거아닌가요..",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042427.1.2",
          "form": "내일뵐까요..?ㅋㅋㅋㅋ",
          "original_form": "내일뵐까요..?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042427.1.3",
          "form": "편하신대루..",
          "original_form": "편하신대루..",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042427.1.4",
          "form": "*_*",
          "original_form": "*_*",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042427.1.5",
          "form": "ㅋㅋㅋ저는 열시반에 잘생각이에용",
          "original_form": "ㅋㅋㅋ저는 열시반에 잘생각이에용",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042427.1.6",
          "form": "낼 출근해서 또 이어나가용><",
          "original_form": "낼 출근해서 또 이어나가용><",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042427.1.7",
          "form": "화이팅!!!!!",
          "original_form": "화이팅!!!!!",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042427.1.8",
          "form": "아하 ㅋㅋㅋㄱ 넹넹 >\\\\\\<",
          "original_form": "아하 ㅋㅋㅋㄱ 넹넹 >\\\\\\<",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042427.1.9",
          "form": "홧팅ㅋㅋㄲ굿밤되세용!!!!",
          "original_form": "홧팅ㅋㅋㄲ굿밤되세용!!!!",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042427.1.10",
          "form": "ㅎㅎ출근시간몇시이신가용",
          "original_form": "ㅎㅎ출근시간몇시이신가용",
          "speaker_id": "2",
          "time": "20210907 08:14"
        },
        {
          "id": "MDRW2100042427.1.11",
          "form": "저는 여덟시반까지에유",
          "original_form": "저는 여덟시반까지에유",
          "speaker_id": "2",
          "time": "20210907 08:14"
        },
        {
          "id": "MDRW2100042427.1.12",
          "form": "저는8시50까지요ㅎㅁㅎ!!",
          "original_form": "저는8시50까지요ㅎㅁㅎ!!",
          "speaker_id": "1",
          "time": "20210907 08:34"
        },
        {
          "id": "MDRW2100042427.1.13",
          "form": "ㅎㅎ출근해서",
          "original_form": "ㅎㅎ출근해서",
          "speaker_id": "2",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042427.1.14",
          "form": "아아때려넣엇어요",
          "original_form": "아아때려넣엇어요",
          "speaker_id": "2",
          "time": "20210907 08:38"
        },
        {
          "id": "MDRW2100042427.1.15",
          "form": "일찍자도 매일 아침 죽을꺼같아요",
          "original_form": "일찍자도 매일 아침 죽을꺼같아요",
          "speaker_id": "2",
          "time": "20210907 08:38"
        },
        {
          "id": "MDRW2100042427.1.16",
          "form": "오 아아 ㅋㅋㅋㄱ 아아좋죠\n빈속에커피안좋다해도  어쩔수없어요\n때려야 굴러가요",
          "original_form": "오 아아 ㅋㅋㅋㄱ 아아좋죠\n빈속에커피안좋다해도  어쩔수없어요\n때려야 굴러가요",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042427.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042427.1.18",
          "form": "올해 첨 위내시경 햇는데",
          "original_form": "올해 첨 위내시경 햇는데",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042427.1.19",
          "form": "하도 식습관이안좋아서 쫄앗는데",
          "original_form": "하도 식습관이안좋아서 쫄앗는데",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042427.1.20",
          "form": "아직 쌩쌩하더군요...ㅎㅎ",
          "original_form": "아직 쌩쌩하더군요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042427.1.21",
          "form": "건강검진받아보셧나요",
          "original_form": "건강검진받아보셧나요",
          "speaker_id": "2",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042427.1.22",
          "form": "저매번간단한직장인건강검진만했는데\n위내시경좀쫄립니다..ㅋㅋㅋ",
          "original_form": "저매번간단한직장인건강검진만했는데\n위내시경좀쫄립니다..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:20"
        },
        {
          "id": "MDRW2100042427.1.23",
          "form": "ㅋㅋ저두 결과듣기전에 두근두근햇어유",
          "original_form": "ㅋㅋ저두 결과듣기전에 두근두근햇어유",
          "speaker_id": "2",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042427.1.24",
          "form": "실비처리되고 엄청 간단하니까 꼭 받으세욤",
          "original_form": "실비처리되고 엄청 간단하니까 꼭 받으세욤",
          "speaker_id": "2",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042427.1.25",
          "form": "그러게요 한번해보긴해야한다구생각은하는데 늘생각에서그칩니다ㅜㅜㅋㅋㅋ",
          "original_form": "그러게요 한번해보긴해야한다구생각은하는데 늘생각에서그칩니다ㅜㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:22"
        },
        {
          "id": "MDRW2100042427.1.26",
          "form": "ㅜㅜ귀찮긴하져 ㅋㅋㅋㅋ",
          "original_form": "ㅜㅜ귀찮긴하져 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:31"
        },
        {
