        },
        {
          "id": "MDRW2100036643.1.2",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
          "speaker_id": "1",
          "time": "20210831 03:47"
        },
        {
          "id": "MDRW2100036643.1.3",
          "form": "기상이변 겪어봤나요?",
          "original_form": "기상이변 겪어봤나요?",
          "speaker_id": "2",
          "time": "20210831 03:47"
        },
        {
          "id": "MDRW2100036643.1.4",
          "form": "무슨 기상이변이죠?",
          "original_form": "무슨 기상이변이죠?",
          "speaker_id": "1",
          "time": "20210831 03:47"
        },
        {
          "id": "MDRW2100036643.1.5",
          "form": "지진 어땨요?",
          "original_form": "지진 어땨요?",
          "speaker_id": "2",
          "time": "20210831 03:47"
        },
        {
          "id": "MDRW2100036643.1.6",
          "form": "그거 완전 혐오해요",
          "original_form": "그거 완전 혐오해요",
          "speaker_id": "1",
          "time": "20210831 03:47"
        },
        {
          "id": "MDRW2100036643.1.7",
          "form": "그거 완전혐고해요",
          "original_form": "그거 완전혐고해요",
          "speaker_id": "1",
          "time": "20210831 03:48"
        },
        {
          "id": "MDRW2100036643.1.8",
          "form": "겨혐오",
          "original_form": "겨혐오",
          "speaker_id": "1",
          "time": "20210831 03:48"
        },
        {
          "id": "MDRW2100036643.1.9",
          "form": "재밋진 안ㄹ아요?",
          "original_form": "재밋진 안ㄹ아요?",
          "speaker_id": "2",
          "time": "20210831 03:48"
        },
        {
          "id": "MDRW2100036643.1.10",
          "form": "글쿤여",
          "original_form": "글쿤여",
          "speaker_id": "2",
          "time": "20210831 03:48"
        },
        {
          "id": "MDRW2100036643.1.11",
          "form": "어느날 문득 마구 흔들리는 그런걸 상상한적이잇는데\r\n정막 그런날이 두번이나왓다는게 너무나도 충격적이엇어요",
          "original_form": "어느날 문득 마구 흔들리는 그런걸 상상한적이잇는데\r\n정막 그런날이 두번이나왓다는게 너무나도 충격적이엇어요",
          "speaker_id": "1",
          "time": "20210831 03:48"
        },
        {
          "id": "MDRW2100036643.1.12",
          "form": "저는 잊을수앖는 사건이 있죠",
          "original_form": "저는 잊을수앖는 사건이 있죠",
          "speaker_id": "2",
          "time": "20210831 03:49"
        },
        {
          "id": "MDRW2100036643.1.13",
          "form": "오 뭔데여",
          "original_form": "오 뭔데여",
          "speaker_id": "1",
          "time": "20210831 03:49"
        },
        {
          "id": "MDRW2100036643.1.14",
          "form": "티비 붙잡았던거",
          "original_form": "티비 붙잡았던거",
          "speaker_id": "2",
          "time": "20210831 03:49"
        },
        {
          "id": "MDRW2100036643.1.15",
          "form": "아..막 쓰러질것같던가요?",
          "original_form": "아..막 쓰러질것같던가요?",
          "speaker_id": "1",
          "time": "20210831 03:49"
        },
        {
          "id": "MDRW2100036643.1.16",
          "form": "흔들렸져",
          "original_form": "흔들렸져",
          "speaker_id": "2",
          "time": "20210831 03:49"
        },
        {
          "id": "MDRW2100036643.1.17",
          "form": "주변에 안전 고리라도 달아야하는거아닌가요?",
