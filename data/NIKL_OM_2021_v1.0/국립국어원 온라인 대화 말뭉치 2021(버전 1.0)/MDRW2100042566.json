      "utterance": [
        {
          "id": "MDRW2100042566.1.1",
          "form": "파랑 포카칩이 담백하다니",
          "original_form": "파랑 포카칩이 담백하다니",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042566.1.2",
          "form": "그럼 초록 포카칩은 안 좋아하니?",
          "original_form": "그럼 초록 포카칩은 안 좋아하니?",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042566.1.3",
          "form": "그건 짭쪼름해서 좋아해",
          "original_form": "그건 짭쪼름해서 좋아해",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042566.1.4",
          "form": "근데 안짜서 더좋은것같아",
          "original_form": "근데 안짜서 더좋은것같아",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042566.1.5",
          "form": "파랑색이 덜 짜?",
          "original_form": "파랑색이 덜 짜?",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042566.1.6",
          "form": "응 더 고소해",
          "original_form": "응 더 고소해",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042566.1.7",
          "form": "헐 몰랐다 나란히 두고 먹어본 적이 없어서 몰랐어",
          "original_form": "헐 몰랐다 나란히 두고 먹어본 적이 없어서 몰랐어",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042566.1.8",
          "form": "너 제일 좋아하는 과자가 뭐니",
          "original_form": "너 제일 좋아하는 과자가 뭐니",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042566.1.9",
          "form": "나는 눈을 감자",
          "original_form": "나는 눈을 감자",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042566.1.10",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042566.1.11",
          "form": "그것도 칩인가",
          "original_form": "그것도 칩인가",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042566.1.12",
          "form": "스틱형인가",
          "original_form": "스틱형인가",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042566.1.13",
          "form": "기억이 안 나네",
          "original_form": "기억이 안 나네",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042566.1.14",
          "form": "어떻게 생긴 과자였더라",
          "original_form": "어떻게 생긴 과자였더라",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042566.1.15",
          "form": "맞아 바삭바삭한스틱형",
          "original_form": "맞아 바삭바삭한스틱형",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042566.1.16",
          "form": "언젠가 한 번 먹어본 것 같은데",
          "original_form": "언젠가 한 번 먹어본 것 같은데",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042566.1.17",
          "form": "너무 가물가물해",
          "original_form": "너무 가물가물해",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042566.1.18",
          "form": "너무 가물가물해",
          "original_form": "너무 가물가물해",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042566.1.19",
          "form": "그럼 싫어하는 과자 있어?",
          "original_form": "그럼 싫어하는 과자 있어?",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042566.1.20",
          "form": "응 하비스트나",
          "original_form": "응 하비스트나",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042566.1.21",
          "form": "참깨과자",
          "original_form": "참깨과자",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042566.1.22",
          "form": "그런 고소한 과자류 먹으면",
          "original_form": "그런 고소한 과자류 먹으면",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042566.1.23",
          "form": "입안이 너무 꾸덕해져",
          "original_form": "입안이 너무 꾸덕해져",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042566.1.24",
          "form": "특유의 단맛때문에 멀미나",
          "original_form": "특유의 단맛때문에 멀미나",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042566.1.25",
          "form": "알 것 같다 약간 답답한 맛이야",
          "original_form": "알 것 같다 약간 답답한 맛이야",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042566.1.26",
          "form": "헉 생라면 부숴먹고싶다",
          "original_form": "헉 생라면 부숴먹고싶다",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042566.1.27",
          "form": "맞아 비싼데 맛없어",
          "original_form": "맞아 비싼데 맛없어",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042566.1.28",
          "form": "맞아 값을 못 해",
          "original_form": "맞아 값을 못 해",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042566.1.29",
          "form": "요상한 과자야",
          "original_form": "요상한 과자야",
          "speaker_id": "1",
          "time": "20210906 23:04"
        }
      ]
    }
