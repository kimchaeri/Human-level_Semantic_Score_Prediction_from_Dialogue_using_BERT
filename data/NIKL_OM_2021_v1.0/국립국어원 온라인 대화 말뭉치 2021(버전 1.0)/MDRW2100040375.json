      "utterance": [
        {
          "id": "MDRW2100040375.1.1",
          "form": "하도 화내는사람이많아서",
          "original_form": "하도 화내는사람이많아서",
          "speaker_id": "2",
          "time": "20210904 17:33"
        },
        {
          "id": "MDRW2100040375.1.2",
          "form": "빠르게 대응한걸까요",
          "original_form": "빠르게 대응한걸까요",
          "speaker_id": "2",
          "time": "20210904 17:34"
        },
        {
          "id": "MDRW2100040375.1.3",
          "form": "그런걸수도요",
          "original_form": "그런걸수도요",
          "speaker_id": "1",
          "time": "20210904 17:34"
        },
        {
          "id": "MDRW2100040375.1.4",
          "form": "그게현명할수도..ㅎㅁㅎ",
          "original_form": "그게현명할수도..ㅎㅁㅎ",
          "speaker_id": "2",
          "time": "20210904 17:34"
        },
        {
          "id": "MDRW2100040375.1.5",
          "form": "애초에 재조리 얘기했을 때",
          "original_form": "애초에 재조리 얘기했을 때",
          "speaker_id": "1",
          "time": "20210904 17:34"
        },
        {
          "id": "MDRW2100040375.1.6",
          "form": "라이더한테 피해가는 거면 걍 오배송된 거 찾아 주셔도 된다 이렇게 말했었으니",
          "original_form": "라이더한테 피해가는 거면 걍 오배송된 거 찾아 주셔도 된다 이렇게 말했었으니",
          "speaker_id": "1",
          "time": "20210904 17:35"
        },
        {
          "id": "MDRW2100040375.1.7",
          "form": "라이더한테 이중 덤터기 씌운 건 아니겠죵",
          "original_form": "라이더한테 이중 덤터기 씌운 건 아니겠죵",
          "speaker_id": "1",
          "time": "20210904 17:35"
        },
        {
          "id": "MDRW2100040375.1.8",
          "form": "암튼 님도 이런일 있음",
          "original_form": "암튼 님도 이런일 있음",
          "speaker_id": "1",
          "time": "20210904 17:35"
        },
        {
          "id": "MDRW2100040375.1.9",
          "form": "환불보다 재배송을 추천합니다ㅎ",
          "original_form": "환불보다 재배송을 추천합니다ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:35"
        },
        {
          "id": "MDRW2100040375.1.10",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㄱㅋㅋ 참고해두겠습니당..ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㄱㅋㅋ 참고해두겠습니당..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100040375.1.11",
          "form": "가게 원인이 아니라 배민 시스템상 문제면",
          "original_form": "가게 원인이 아니라 배민 시스템상 문제면",
          "speaker_id": "1",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100040375.1.12",
          "form": "암튼가게나라이더님피해만없으면좋죵",
          "original_form": "암튼가게나라이더님피해만없으면좋죵",
          "speaker_id": "2",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100040375.1.13",
          "form": "보상 잘해줄듯",
          "original_form": "보상 잘해줄듯",
          "speaker_id": "1",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100040375.1.14",
          "form": "우리도좋고뭐...ㅎㅎ",
          "original_form": "우리도좋고뭐...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100040375.1.15",
          "form": "금융치료짱..",
          "original_form": "금융치료짱..",
          "speaker_id": "2",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100040375.1.16",
          "form": "자리안잡혀서 항의문의많을거같긴해요",
          "original_form": "자리안잡혀서 항의문의많을거같긴해요",
          "speaker_id": "2",
          "time": "20210904 17:37"
        },
        {
          "id": "MDRW2100040375.1.17",
          "form": "가게는 아예 배민에 연락하세요 이걸로 끝이고 거기도 라이더 연락처를 모르니",
          "original_form": "가게는 아예 배민에 연락하세요 이걸로 끝이고 거기도 라이더 연락처를 모르니",
          "speaker_id": "1",
          "time": "20210904 17:37"
        },
        {
          "id": "MDRW2100040375.1.18",
          "form": "이상한 시스템이긴 해요",
          "original_form": "이상한 시스템이긴 해요",
          "speaker_id": "1",
          "time": "20210904 17:37"
        },
        {
          "id": "MDRW2100040375.1.19",
          "form": "네네 그래서 지금은 초기단계 항의가 많아  매뉴얼된 보상체계인가 싶기도",
          "original_form": "네네 그래서 지금은 초기단계 항의가 많아  매뉴얼된 보상체계인가 싶기도",
          "speaker_id": "1",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100040375.1.20",
          "form": "아 가게도 배민에서관리하는거라 딱 선긋네여",
          "original_form": "아 가게도 배민에서관리하는거라 딱 선긋네여",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100040375.1.21",
          "form": "그런거같아요ㅋㅋ나중엔이렇게안해줄거같아요자리좀잡으면",
          "original_form": "그런거같아요ㅋㅋ나중엔이렇게안해줄거같아요자리좀잡으면",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100040375.1.22",
          "form": "네 재조리 주문도 바로 배민서 가게로 연락하는 거라ㅎ",
          "original_form": "네 재조리 주문도 바로 배민서 가게로 연락하는 거라ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100040375.1.23",
          "form": "이차 배달도 사십분 기다려도 안와 전화하니",
          "original_form": "이차 배달도 사십분 기다려도 안와 전화하니",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100040375.1.24",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100040375.1.25",
          "form": "가게선 이미 삼십분전 픽업해 갔대요",
          "original_form": "가게선 이미 삼십분전 픽업해 갔대요",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100040375.1.26",
          "form": "근데 역시 라이더 연락처는 모름 ㅋ",
          "original_form": "근데 역시 라이더 연락처는 모름 ㅋ",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100040375.1.27",
          "form": "진짜희한한구조ㅋㅋㄱㅋㅋ",
          "original_form": "진짜희한한구조ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100040375.1.28",
          "form": "삼십분전픽업해가셧음배달원어디가신거람",
          "original_form": "삼십분전픽업해가셧음배달원어디가신거람",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100040375.1.29",
          "form": "혹시나 해서 집앞보니 소리소문없이 두고 갔던데요",
          "original_form": "혹시나 해서 집앞보니 소리소문없이 두고 갔던데요",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100040375.1.30",
          "form": "한 이십분 집앞서 방치된듯ㅎ",
          "original_form": "한 이십분 집앞서 방치된듯ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100040375.1.31",
          "form": "악..ㅋㄱㅋㄱㄱㅋ",
          "original_form": "악..ㅋㄱㅋㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100040375.1.32",
          "form": "방치하니생각나요",
          "original_form": "방치하니생각나요",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100040375.1.33",
          "form": "저배달시켜놓고잠들어서",
          "original_form": "저배달시켜놓고잠들어서",
          "speaker_id": "2",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100040375.1.34",
          "form": "금융치료를 받은 상태라 화는 안 났쥬",
          "original_form": "금융치료를 받은 상태라 화는 안 났쥬",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100040375.1.35",
          "form": "앜!",
          "original_form": "앜!",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100040375.1.36",
          "form": "음식 문앞에두시간방치ㅋㅋㅋㅋ",
          "original_form": "음식 문앞에두시간방치ㅋㅋㅋㅋ",
