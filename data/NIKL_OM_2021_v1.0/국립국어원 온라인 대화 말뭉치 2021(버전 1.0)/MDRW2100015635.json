      "utterance": [
        {
          "id": "MDRW2100015635.1.1",
          "form": "좋아하시는 동물이나 키우고 있는 반려동물 있으세요?",
          "original_form": "좋아하시는 동물이나 키우고 있는 반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015635.1.2",
          "form": "동물이야 다 좋죠",
          "original_form": "동물이야 다 좋죠",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015635.1.3",
          "form": "키우고있는 반려동물은 없습니다",
          "original_form": "키우고있는 반려동물은 없습니다",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015635.1.4",
          "form": "저두 없지만 동물 다 좋아해요ㅎㅎ",
          "original_form": "저두 없지만 동물 다 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015635.1.5",
          "form": "제일 좋아하는 동물 있나용?",
          "original_form": "제일 좋아하는 동물 있나용?",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015635.1.6",
          "form": "음 생각해본적 없는데 name1님은요?",
          "original_form": "음 생각해본적 없는데 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015635.1.7",
          "form": "음....강아지요",
          "original_form": "음....강아지요",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015635.1.8",
          "form": "제가 변덕이 심해서 자주 바뀌긴 하지만",
          "original_form": "제가 변덕이 심해서 자주 바뀌긴 하지만",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015635.1.9",
          "form": "강아지의 어떤점이 좋으신가요?",
          "original_form": "강아지의 어떤점이 좋으신가요?",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015635.1.10",
          "form": "따뜻하고 몰캉한 다른 생명체?ㅠㅠ인것 자체만으로 귀염댕이...",
          "original_form": "따뜻하고 몰캉한 다른 생명체?ㅠㅠ인것 자체만으로 귀염댕이...",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015635.1.11",
          "form": "발소리 차칵차칵 나는 것도 긔엽구",
          "original_form": "발소리 차칵차칵 나는 것도 긔엽구",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015635.1.12",
          "form": "다 귀여워요ㅎㅎ",
          "original_form": "다 귀여워요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015635.1.13",
          "form": "어우 너무 좋죠ㅠㅠ",
          "original_form": "어우 너무 좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015635.1.14",
          "form": "발꼬락꼬순내가 그렇게 좋다던데요",
          "original_form": "발꼬락꼬순내가 그렇게 좋다던데요",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015635.1.15",
          "form": "같이 걸을때 나를 보면서 걷는것도 너무 사랑스러워요ㅠ",
          "original_form": "같이 걸을때 나를 보면서 걷는것도 너무 사랑스러워요ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015635.1.16",
          "form": "전 맡아보지 못해서",
          "original_form": "전 맡아보지 못해서",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015635.1.17",
          "form": "무슨 냄샌지 상상이 안가요ㅠㅠㅠ",
          "original_form": "무슨 냄샌지 상상이 안가요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015635.1.18",
          "form": "어떨까요...",
          "original_form": "어떨까요...",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015635.1.19",
          "form": "룸메가 강아지를 본가에서 키웠었는데",
          "original_form": "룸메가 강아지를 본가에서 키웠었는데",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015635.1.20",
          "form": "그렇게 꼬순내를 그리워해요",
          "original_form": "그렇게 꼬순내를 그리워해요",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015635.1.21",
          "form": "저도 상상이안가는데 부러워요",
          "original_form": "저도 상상이안가는데 부러워요",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015635.1.22",
          "form": "부럽지만 키우는건 두려워요ㅋㅋㅋㅋ",
          "original_form": "부럽지만 키우는건 두려워요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015635.1.23",
          "form": "제가 잘 못키울까봐ㅠㅠㅠ",
          "original_form": "제가 잘 못키울까봐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015635.1.24",
          "form": "그냥 남의 집 강아지 구경하는게 그나마 삶의 낙...ㅠ",
          "original_form": "그냥 남의 집 강아지 구경하는게 그나마 삶의 낙...ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015635.1.25",
          "form": "저두요ㅠㅠ 그친구의 평생을 책임질 자신이 있을때 데려오려구요ㅠㅠ",
          "original_form": "저두요ㅠㅠ 그친구의 평생을 책임질 자신이 있을때 데려오려구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015635.1.26",
          "form": "맞죠...생명체를 다루는거니까ㅠ",
          "original_form": "맞죠...생명체를 다루는거니까ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015635.1.27",
          "form": "키우면 저 초대해주세요",
          "original_form": "키우면 저 초대해주세요",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015635.1.28",
          "form": "와랄라하게",
          "original_form": "와랄라하게",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015635.1.29",
          "form": "앜ㅋㅋㅋㅋ저희 애긔 소즁하게 대해주세요",
          "original_form": "앜ㅋㅋㅋㅋ저희 애긔 소즁하게 대해주세요",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015635.1.30",
          "form": "그만 귀여웠으면ㅠ",
          "original_form": "그만 귀여웠으면ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:11"
        }
      ]
