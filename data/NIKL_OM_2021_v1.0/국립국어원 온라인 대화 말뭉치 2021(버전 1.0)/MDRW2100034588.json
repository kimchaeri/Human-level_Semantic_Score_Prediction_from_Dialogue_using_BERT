        {
          "id": "MDRW2100034588.1.3",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100034588.1.4",
          "form": "왜 돈벌면 막 아낀다하던데",
          "original_form": "왜 돈벌면 막 아낀다하던데",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100034588.1.5",
          "form": "전 먹는거에 다 써요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 먹는거에 다 써요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100034588.1.6",
          "form": "그니까요 내가번거 아까워서 못쓴다고 하는데",
          "original_form": "그니까요 내가번거 아까워서 못쓴다고 하는데",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100034588.1.7",
          "form": "저도 아니더라구요",
          "original_form": "저도 아니더라구요",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100034588.1.8",
          "form": "정반대에요 ㅋㅋㅋㅋ",
          "original_form": "정반대에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100034588.1.9",
          "form": "더 잘쓰는거같아요ㅋㅋㅋ특히 먹는거에ㅋㅋ",
          "original_form": "더 잘쓰는거같아요ㅋㅋㅋ특히 먹는거에ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100034588.1.10",
          "form": "내가 이만큼 일했으니",
          "original_form": "내가 이만큼 일했으니",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100034588.1.11",
          "form": "먹기라도해야지 하는 보상심리가 ㅋㅋㅋ",
          "original_form": "먹기라도해야지 하는 보상심리가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100034588.1.12",
          "form": "마자요 딱 그거예요ㅋㅋㅋㅋ",
          "original_form": "마자요 딱 그거예요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100034588.1.13",
          "form": "마치 운동했을때와 같은 마음이네요ㅋㅋㅋ",
          "original_form": "마치 운동했을때와 같은 마음이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100034588.1.14",
          "form": "다같네요 ㅋㅋㅋ",
