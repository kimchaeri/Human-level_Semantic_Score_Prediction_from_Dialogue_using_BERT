      "utterance": [
        {
          "id": "MDRW2100038752.1.1",
          "form": "쇼핑과상품 이야기에이어",
          "original_form": "쇼핑과상품 이야기에이어",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038752.1.2",
          "form": "ㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038752.1.3",
          "form": "오늘 좀 선선하네요",
          "original_form": "오늘 좀 선선하네요",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038752.1.4",
          "form": "맞아요선선해요",
          "original_form": "맞아요선선해요",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038752.1.5",
          "form": "산책다녀와쏘요?",
          "original_form": "산책다녀와쏘요?",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038752.1.6",
          "form": "네네다녀왔죠",
          "original_form": "네네다녀왔죠",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038752.1.7",
          "form": "그리곤 목욕시켰어요 강쥐",
          "original_form": "그리곤 목욕시켰어요 강쥐",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038752.1.8",
          "form": "오늘도부지런한하루를보냇네요",
          "original_form": "오늘도부지런한하루를보냇네요",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038752.1.9",
          "form": "낮에도나갔다왔었어용?",
          "original_form": "낮에도나갔다왔었어용?",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038752.1.10",
          "form": "네네 나갔다왔져",
          "original_form": "네네 나갔다왔져",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038752.1.11",
          "form": "그땐비안맞았쬬?",
          "original_form": "그땐비안맞았쬬?",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038752.1.12",
          "form": "네네 들어올때 왔던거같아요",
          "original_form": "네네 들어올때 왔던거같아요",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038752.1.13",
          "form": "다행이예요 비맞으면머리빠져요",
          "original_form": "다행이예요 비맞으면머리빠져요",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038752.1.14",
          "form": "맞아요 머리 빠짐 안되어요",
          "original_form": "맞아요 머리 빠짐 안되어요",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038752.1.15",
          "form": "관리하는제품같은거있나요?",
          "original_form": "관리하는제품같은거있나요?",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100038752.1.16",
          "form": "샴푸랑 김희애아주머니 두피에센스?",
          "original_form": "샴푸랑 김희애아주머니 두피에센스?",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100038752.1.17",
          "form": "백신에 이어",
          "original_form": "백신에 이어",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100038752.1.18",
