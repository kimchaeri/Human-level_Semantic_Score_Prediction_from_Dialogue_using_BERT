      "utterance": [
        {
          "id": "MDRW2100042533.1.1",
          "form": "게임하시나요",
          "original_form": "게임하시나요",
          "speaker_id": "2",
          "time": "20210907 00:02"
        },
        {
          "id": "MDRW2100042533.1.2",
          "form": "저 쿠키런이라는 게인했었어요 name2님은요?",
          "original_form": "저 쿠키런이라는 게인했었어요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 00:03"
        },
        {
          "id": "MDRW2100042533.1.3",
          "form": "전 게임을 잘 안함니다",
          "original_form": "전 게임을 잘 안함니다",
          "speaker_id": "2",
          "time": "20210907 00:03"
        },
        {
          "id": "MDRW2100042533.1.4",
          "form": "아하ㅜ 한번도 안해봤나요?!",
          "original_form": "아하ㅜ 한번도 안해봤나요?!",
          "speaker_id": "1",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042533.1.5",
          "form": "애니팡은 해봤네요",
          "original_form": "애니팡은 해봤네요",
          "speaker_id": "2",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042533.1.6",
          "form": "헉 애니팡 너무 오랜만에 들어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 애니팡 너무 오랜만에 들어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042533.1.7",
          "form": "애니팡 저도 오래했었어요!",
          "original_form": "애니팡 저도 오래했었어요!",
          "speaker_id": "1",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042533.1.8",
          "form": "애니팡 재밌는데 중독되는 기분이 싫어서 삭제했아요",
          "original_form": "애니팡 재밌는데 중독되는 기분이 싫어서 삭제했아요",
          "speaker_id": "2",
          "time": "20210907 00:09"
        },
        {
          "id": "MDRW2100042533.1.9",
          "form": "아 맞아요ㅠ 게임할때 그런기분 들면 게임 그만하고싶어지죠",
          "original_form": "아 맞아요ㅠ 게임할때 그런기분 들면 게임 그만하고싶어지죠",
          "speaker_id": "1",
          "time": "20210907 00:09"
        },
        {
          "id": "MDRW2100042533.1.10",
          "form": "게임 자주하시나요",
          "original_form": "게임 자주하시나요",
          "speaker_id": "2",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042533.1.11",
          "form": "아뇨ㅠ 자주하지는 않아요!",
          "original_form": "아뇨ㅠ 자주하지는 않아요!",
          "speaker_id": "1",
          "time": "20210907 00:11"
        },
        {
          "id": "MDRW2100042533.1.12",
          "form": "그렇군요 그럼 요즘하는 게임은요?",
          "original_form": "그렇군요 그럼 요즘하는 게임은요?",
          "speaker_id": "2",
          "time": "20210907 00:11"
        },
        {
          "id": "MDRW2100042533.1.13",
          "form": "저 쿠키런킹덤해보려고 하고있어요",
          "original_form": "저 쿠키런킹덤해보려고 하고있어요",
          "speaker_id": "1",
          "time": "20210907 00:12"
        },
        {
          "id": "MDRW2100042533.1.14",
          "form": "그 게임도 재밌나봐요",
          "original_form": "그 게임도 재밌나봐요",
          "speaker_id": "2",
          "time": "20210907 00:12"
        },
        {
          "id": "MDRW2100042533.1.15",
          "form": "네ㅠ 재밋다고 하는데 전 안해봐서 모르겠어요 어려워보이는거예요ㅠㅠ~!~!~!",
          "original_form": "네ㅠ 재밋다고 하는데 전 안해봐서 모르겠어요 어려워보이는거예요ㅠㅠ~!~!~!",
          "speaker_id": "1",
          "time": "20210907 00:17"
        }
