      "utterance": [
        {
          "id": "MDRW2100047165.1.1",
          "form": "제일 싫어하는 집안일이 뭐예요?",
          "original_form": "제일 싫어하는 집안일이 뭐예요?",
          "speaker_id": "1",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100047165.1.2",
          "form": "저는... 설거지가싫은거같아요ㅎㅎ",
          "original_form": "저는... 설거지가싫은거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100047165.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100047165.1.4",
          "form": "저는 욕실청소요",
          "original_form": "저는 욕실청소요",
          "speaker_id": "1",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100047165.1.5",
          "form": "맞아요 욕실청소도 싫어요ㅠㅠ..",
          "original_form": "맞아요 욕실청소도 싫어요ㅠㅠ..",
          "speaker_id": "2",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100047165.1.6",
          "form": "곰팡이도싫고..",
          "original_form": "곰팡이도싫고..",
          "speaker_id": "2",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100047165.1.7",
          "form": "그래서그런지 건식화장실도 좋은거같아요",
          "original_form": "그래서그런지 건식화장실도 좋은거같아요",
          "speaker_id": "2",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100047165.1.8",
          "form": "그죠 저도 최대한 건식으로 사용하려고하는데",
          "original_form": "그죠 저도 최대한 건식으로 사용하려고하는데",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100047165.1.9",
          "form": "좁다보니 잘안돼요",
          "original_form": "좁다보니 잘안돼요",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100047165.1.10",
          "form": "설거지 너무 귀찮죠...기름기있는 설거지거리 너무 힘들어요",
          "original_form": "설거지 너무 귀찮죠...기름기있는 설거지거리 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210901 15:30"
        },
        {
          "id": "MDRW2100047165.1.11",
          "form": "네 한번 닦아야하고..",
          "original_form": "네 한번 닦아야하고..",
          "speaker_id": "2",
          "time": "20210901 15:30"
        },
        {
          "id": "MDRW2100047165.1.12",
          "form": "미루지않고 하면되는걸 아는데",
          "original_form": "미루지않고 하면되는걸 아는데",
          "speaker_id": "2",
          "time": "20210901 15:30"
        },
        {
          "id": "MDRW2100047165.1.13",
          "form": "그게 어려운거같아요",
          "original_form": "그게 어려운거같아요",
          "speaker_id": "2",
          "time": "20210901 15:30"
        },
        {
          "id": "MDRW2100047165.1.14",
          "form": "그죠 깨끗하게 뒷정리까지 하고나서 물한잔마시면",
          "original_form": "그죠 깨끗하게 뒷정리까지 하고나서 물한잔마시면",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100047165.1.15",
          "form": "바로 또 생겨서 끝이없어요",
          "original_form": "바로 또 생겨서 끝이없어요",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100047165.1.16",
          "form": "정말 그말이 맞는거같아요 끝이없어요ㅠㅠ",
          "original_form": "정말 그말이 맞는거같아요 끝이없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100047165.1.17",
          "form": "욕실도 그렇고.. 안씻을수가없으니까요",
          "original_form": "욕실도 그렇고.. 안씻을수가없으니까요",
          "speaker_id": "2",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100047165.1.18",
          "form": "집안일은 끝이없어서 별로인거 같아요",
          "original_form": "집안일은 끝이없어서 별로인거 같아요",
          "speaker_id": "1",
          "time": "20210901 15:37"
        },
        {
          "id": "MDRW2100047165.1.19",
          "form": "식기세척기 사고싶어요",
          "original_form": "식기세척기 사고싶어요",
          "speaker_id": "1",
          "time": "20210901 15:37"
        },
        {
          "id": "MDRW2100047165.1.20",
          "form": "요즘건 정말 잘나온다고하더라구요!",
          "original_form": "요즘건 정말 잘나온다고하더라구요!",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100047165.1.21",
          "form": "저도 있으면 좋을거같아요ㅎㅎ",
          "original_form": "저도 있으면 좋을거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100047165.1.22",
          "form": "건조기는 사용하세요?",
          "original_form": "건조기는 사용하세요?",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100047165.1.23",
          "form": "건조기가 없어요...본가에서는 써봤어요",
          "original_form": "건조기가 없어요...본가에서는 써봤어요",
          "speaker_id": "1",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100047165.1.24",
          "form": "사용하세요?",
          "original_form": "사용하세요?",
          "speaker_id": "1",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100047165.1.25",
          "form": "아뇨 저도 아직이에요ㅎㅎ",
          "original_form": "아뇨 저도 아직이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100047165.1.26",
          "form": "많이 편한가요?",
          "original_form": "많이 편한가요?",
          "speaker_id": "2",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100047165.1.27",
          "form": "네! 날씨 신경쓸필요도 없구요",
          "original_form": "네! 날씨 신경쓸필요도 없구요",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100047165.1.28",
          "form": "수건같은건 확실히 부드러워져요",
          "original_form": "수건같은건 확실히 부드러워져요",
          "speaker_id": "1",
          "time": "20210901 15:42"
        }
      ]
    }
