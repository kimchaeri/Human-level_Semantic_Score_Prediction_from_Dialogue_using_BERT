      "utterance": [
        {
          "id": "MDRW2100025317.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210901 09:15"
        },
        {
          "id": "MDRW2100025317.1.2",
          "form": "네ㅎㅎ 어느 정도는요?",
          "original_form": "네ㅎㅎ 어느 정도는요?",
          "speaker_id": "2",
          "time": "20210901 09:20"
        },
        {
          "id": "MDRW2100025317.1.3",
          "form": "제가 발에 뭐 밟히는걸 싫어해서",
          "original_form": "제가 발에 뭐 밟히는걸 싫어해서",
          "speaker_id": "2",
          "time": "20210901 09:20"
        },
        {
          "id": "MDRW2100025317.1.4",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210901 09:20"
        },
        {
          "id": "MDRW2100025317.1.5",
          "form": "저희는 담당까진 없네요 알아서 대충",
          "original_form": "저희는 담당까진 없네요 알아서 대충",
          "speaker_id": "2",
          "time": "20210901 09:21"
        },
        {
          "id": "MDRW2100025317.1.6",
          "form": "빨래 널고 개기는 싫습니다",
          "original_form": "빨래 널고 개기는 싫습니다",
          "speaker_id": "2",
          "time": "20210901 09:22"
        },
        {
          "id": "MDRW2100025317.1.7",
          "form": "매번..",
          "original_form": "매번..",
          "speaker_id": "2",
          "time": "20210901 09:22"
        },
        {
          "id": "MDRW2100025317.1.8",
          "form": "귀찮죠",
          "original_form": "귀찮죠",
          "speaker_id": "1",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025317.1.9",
          "form": "젤 싫어요ㅋㅋ",
          "original_form": "젤 싫어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100025317.1.10",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100025317.1.11",
          "form": "화장실은 군대에서 많이해서 그런지 괜찮아요",
          "original_form": "화장실은 군대에서 많이해서 그런지 괜찮아요",
          "speaker_id": "2",
          "time": "20210901 09:27"
        },
        {
          "id": "MDRW2100025317.1.12",
          "form": "수챗구멍이 좀 고비긴한데",
          "original_form": "수챗구멍이 좀 고비긴한데",
          "speaker_id": "2",
          "time": "20210901 09:27"
        },
        {
          "id": "MDRW2100025317.1.13",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025317.1.14",
          "form": "대신 깨끗하잖아요 소독도 되고",
          "original_form": "대신 깨끗하잖아요 소독도 되고",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025317.1.15",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210901 09:35"
        },
        {
          "id": "MDRW2100025317.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210901 09:36"
        },
        {
          "id": "MDRW2100025317.1.17",
          "form": "락스가 세제랑 반응해서 독성물질 나와요",
          "original_form": "락스가 세제랑 반응해서 독성물질 나와요",
          "speaker_id": "2",
          "time": "20210901 09:36"
        },
        {
          "id": "MDRW2100025317.1.18",
