      "utterance": [
        {
          "id": "MDRW2100005711.1.1",
          "form": "영화는 즐겨 보시나요?",
          "original_form": "영화는 즐겨 보시나요?",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005711.1.2",
          "form": "자주보는편이였는데 티켓값이 은근쓸쩍 올라서 부담스러워서 넷플로 떼우곤 해요 최근에는 블랙위도우 봤어요",
          "original_form": "자주보는편이였는데 티켓값이 은근쓸쩍 올라서 부담스러워서 넷플로 떼우곤 해요 최근에는 블랙위도우 봤어요",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005711.1.3",
          "form": "오 저도 블랙위도우 봤어요",
          "original_form": "오 저도 블랙위도우 봤어요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005711.1.4",
          "form": "영화 티켓값 정말 비싸더라고요",
          "original_form": "영화 티켓값 정말 비싸더라고요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005711.1.5",
          "form": "진짜요 은근슬쩍 엄청 올라서ㅠㅠ",
          "original_form": "진짜요 은근슬쩍 엄청 올라서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 08:39"
        },
        {
          "id": "MDRW2100005711.1.6",
          "form": "뮤지컬이나 연극은 좋아하세요?",
          "original_form": "뮤지컬이나 연극은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 08:40"
        },
        {
          "id": "MDRW2100005711.1.7",
          "form": "뮤지컬 좋아하는데 너무 비싸서 ㅜㅠ",
          "original_form": "뮤지컬 좋아하는데 너무 비싸서 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100005711.1.8",
          "form": "정말 기대되는 거 하나 내한 오면 보려고 기다리고 있어요",
          "original_form": "정말 기대되는 거 하나 내한 오면 보려고 기다리고 있어요",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100005711.1.9",
          "form": "기대하시는게 뭔가요?",
          "original_form": "기대하시는게 뭔가요?",
          "speaker_id": "2",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100005711.1.10",
          "form": "시카고요!!",
          "original_form": "시카고요!!",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100005711.1.11",
          "form": "헉 제애인이 제일 좋아하는 뮤지컬이라",
          "original_form": "헉 제애인이 제일 좋아하는 뮤지컬이라",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100005711.1.12",
          "form": "저도 몇번봤어요",
          "original_form": "저도 몇번봤어요",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100005711.1.13",
          "form": "내한오시면 저도 가보고싶어요 그렇게 퀄리티가 좋다던데ㅠㅠ",
          "original_form": "내한오시면 저도 가보고싶어요 그렇게 퀄리티가 좋다던데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100005711.1.14",
          "form": "오 그렇군요!!",
          "original_form": "오 그렇군요!!",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100005711.1.15",
          "form": "맞아요ㅜㅜ 그래서 꼭",
          "original_form": "맞아요ㅜㅜ 그래서 꼭",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100005711.1.16",
          "form": "내한 왔을 때 비싼 돈이라도 안 아깝게 내고 관람하고 싶은 맘이에요",
          "original_form": "내한 왔을 때 비싼 돈이라도 안 아깝게 내고 관람하고 싶은 맘이에요",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100005711.1.17",
          "form": "저두요!!",
          "original_form": "저두요!!",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100005711.1.18",
          "form": "코로나시국 끝나면 한번쯤 내한 하겠죠...?",
          "original_form": "코로나시국 끝나면 한번쯤 내한 하겠죠...?",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100005711.1.19",
          "form": "그러길 바라요ㅜ",
          "original_form": "그러길 바라요ㅜ",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100005711.1.20",
          "form": "뮤지컬도 그렇고 전시회도 얼른 활발히 열리면 좋겠어요",
          "original_form": "뮤지컬도 그렇고 전시회도 얼른 활발히 열리면 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100005711.1.21",
          "form": "전시회 정말 가고 싶은 것 같아요",
          "original_form": "전시회 정말 가고 싶은 것 같아요",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100005711.1.22",
          "form": "원래 즐기는 편은 아니었는데",
          "original_form": "원래 즐기는 편은 아니었는데",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100005711.1.23",
          "form": "코로나 터진 이후로 전시회가 왜 이리 가고 싶은지...",
          "original_form": "코로나 터진 이후로 전시회가 왜 이리 가고 싶은지...",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100005711.1.24",
          "form": "어우 너무 공감해요",
          "original_form": "어우 너무 공감해요",
          "speaker_id": "2",
          "time": "20210822 16:31"
        }
      ]
    }
  ]
