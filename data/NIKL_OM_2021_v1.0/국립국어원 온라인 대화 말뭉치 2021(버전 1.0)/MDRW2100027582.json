      "utterance": [
        {
          "id": "MDRW2100027582.1.1",
          "form": "지금은 좀 한가하니",
          "original_form": "지금은 좀 한가하니",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027582.1.2",
          "form": "ㅇㅇ 좀 한가해 그래서 게임... 틀었어...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 좀 한가해 그래서 게임... 틀었어...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027582.1.3",
          "form": "배도 덜 아파서 좀 살거 같은",
          "original_form": "배도 덜 아파서 좀 살거 같은",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027582.1.4",
          "form": "오 다행 ㅋㅋㅋㅋ",
          "original_form": "오 다행 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027582.1.5",
          "form": "적당히 농땡이를 쳐주면서 일해야해",
          "original_form": "적당히 농땡이를 쳐주면서 일해야해",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027582.1.6",
          "form": "ㅋㅋㅋㅋㅋ 맞아.. 근데 덜 바빠 하자마자",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아.. 근데 덜 바빠 하자마자",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027582.1.7",
          "form": "바빠졌엌ㅋㅋㅋ",
          "original_form": "바빠졌엌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027582.1.8",
          "form": "아닠ㅋㅋㅋㅋㅋㅋ 역시",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋ 역시",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027582.1.9",
          "form": "일 할땐 그런 소리 하면 안돼..",
          "original_form": "일 할땐 그런 소리 하면 안돼..",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027582.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 일에 귀달린듯?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 일에 귀달린듯?",
          "speaker_id": "2",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100027582.1.11",
          "form": "양반은 못되는 일",
          "original_form": "양반은 못되는 일",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027582.1.12",
          "form": "데리",
          "original_form": "데리",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027582.1.13",
          "form": "24일낭",
          "original_form": "24일낭",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027582.1.14",
          "form": "날",
          "original_form": "날",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027582.1.15",
          "form": "휴가 낼 수 있어?",
          "original_form": "휴가 낼 수 있어?",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027582.1.16",
          "form": "모르겠엉.. 24일 언제지",
          "original_form": "모르겠엉.. 24일 언제지",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027582.1.17",
          "form": "추석 주구나",
          "original_form": "추석 주구나",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027582.1.18",
          "form": "금욜",
          "original_form": "금욜",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027582.1.19",
          "form": "ㅇ ㅏ",
          "original_form": "ㅇ ㅏ",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027582.1.20",
          "form": "그렇긴...한데...",
          "original_form": "그렇긴...한데...",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100027582.1.21",
          "form": "좀있다 함 물어봐야겠..",
          "original_form": "좀있다 함 물어봐야겠..",
          "speaker_id": "2",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100027582.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100027582.1.23",
          "form": "다음주 주말에 놀긴 내 몸이 글렀을까 ((((잠시 생각ㅎ))",
          "original_form": "다음주 주말에 놀긴 내 몸이 글렀을까 ((((잠시 생각ㅎ))",
          "speaker_id": "2",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100027582.1.24",
