      "utterance": [
        {
          "id": "MDRW2100032510.1.1",
          "form": "초등학교 들어가기 전이었는데",
          "original_form": "초등학교 들어가기 전이었는데",
          "speaker_id": "2",
          "time": "20210905 19:52"
        },
        {
          "id": "MDRW2100032510.1.2",
          "form": "몰라 한 5 6살??",
          "original_form": "몰라 한 5 6살??",
          "speaker_id": "2",
          "time": "20210905 19:52"
        },
        {
          "id": "MDRW2100032510.1.3",
          "form": "애기얐군",
          "original_form": "애기얐군",
          "speaker_id": "1",
          "time": "20210905 19:52"
        },
        {
          "id": "MDRW2100032510.1.4",
          "form": "귀엽군",
          "original_form": "귀엽군",
          "speaker_id": "1",
          "time": "20210905 19:52"
        },
        {
          "id": "MDRW2100032510.1.5",
          "form": "근데 초등학교들어가기전에했었으면",
          "original_form": "근데 초등학교들어가기전에했었으면",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032510.1.6",
          "form": "좀 무서웠겠는걸",
          "original_form": "좀 무서웠겠는걸",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032510.1.7",
          "form": "안무서웠어?!",
          "original_form": "안무서웠어?!",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032510.1.8",
          "form": "첨엔 무서워 했다는데",
          "original_form": "첨엔 무서워 했다는데",
          "speaker_id": "2",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032510.1.9",
          "form": "난 기억도 안 나...",
          "original_form": "난 기억도 안 나...",
          "speaker_id": "2",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032510.1.10",
          "form": "귀여워!!!",
          "original_form": "귀여워!!!",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032510.1.11",
          "form": "내가 기억하는 건 이미 물에 익숙해진 이후 ㅋㅋㅋㅋ",
          "original_form": "내가 기억하는 건 이미 물에 익숙해진 이후 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032510.1.12",
          "form": "솧씨는 수영 할 줄 알아??",
          "original_form": "솧씨는 수영 할 줄 알아??",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032510.1.13",
          "form": "오올",
          "original_form": "오올",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032510.1.14",
          "form": "귀엽군",
          "original_form": "귀엽군",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032510.1.15",
          "form": "나도 다이빙까지배웠었엉",
          "original_form": "나도 다이빙까지배웠었엉",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032510.1.16",
          "form": "딩초땐가",
          "original_form": "딩초땐가",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032510.1.17",
          "form": "근데그이후로안하고...",
          "original_form": "근데그이후로안하고...",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032510.1.18",
          "form": "뭔가 캐리비안베이갈때마다",
          "original_form": "뭔가 캐리비안베이갈때마다",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032510.1.19",
          "form": "파도풀장에서",
          "original_form": "파도풀장에서",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.20",
          "form": "오 다이빙...",
          "original_form": "오 다이빙...",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.21",
          "form": "내가 물을 냅다 벌컥벌컥마셨어서",
          "original_form": "내가 물을 냅다 벌컥벌컥마셨어서",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.22",
          "form": "접배평자는 다 배운거??",
          "original_form": "접배평자는 다 배운거??",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.23",
          "form": "아닠ㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.24",
          "form": "그이후로 수영장싫은..",
          "original_form": "그이후로 수영장싫은..",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.25",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.26",
          "form": "ㅇㅇㅇ접배평자 다배웠어",
          "original_form": "ㅇㅇㅇ접배평자 다배웠어",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.27",
          "form": "근데모대",
          "original_form": "근데모대",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.28",
          "form": "그래도 수영은 한 번 하면 안 까먹는다던데",
          "original_form": "그래도 수영은 한 번 하면 안 까먹는다던데",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.29",
          "form": "바다도...넘 싫어졌어...",
          "original_form": "바다도...넘 싫어졌어...",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.30",
          "form": "예아",
          "original_form": "예아",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.31",
          "form": "안까먹긴하도라구",
          "original_form": "안까먹긴하도라구",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032510.1.32",
          "form": "냅다 떠있을수도있고",
          "original_form": "냅다 떠있을수도있고",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.33",
          "form": "아니몇년전에바다갔을때",
          "original_form": "아니몇년전에바다갔을때",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.34",
          "form": "손을 들어올렸더니",
          "original_form": "손을 들어올렸더니",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.35",
          "form": "비닐같은게",
          "original_form": "비닐같은게",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.36",
          "form": "말랑한게붙어잇는거야",
          "original_form": "말랑한게붙어잇는거야",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.37",
          "form": "근데그게 이곳저곳에떠다니는거야",
          "original_form": "근데그게 이곳저곳에떠다니는거야",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.38",
          "form": "알고보니까 죽은해파리였던",
          "original_form": "알고보니까 죽은해파리였던",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.39",
          "form": "난그거땨문에 바닷가는좋아하는데 들어가는게",
          "original_form": "난그거땨문에 바닷가는좋아하는데 들어가는게",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.40",
          "form": "으...",
          "original_form": "으...",
          "speaker_id": "2",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.41",
          "form": "너무에반거같아",
          "original_form": "너무에반거같아",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100032510.1.42",
          "form": "name2는 수영하는거 좋아행?",
          "original_form": "&name2&는 수영하는거 좋아행?",
