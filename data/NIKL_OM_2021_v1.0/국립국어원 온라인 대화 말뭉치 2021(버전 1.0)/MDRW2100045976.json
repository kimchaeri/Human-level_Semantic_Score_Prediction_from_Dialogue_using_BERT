      "utterance": [
        {
          "id": "MDRW2100045976.1.1",
          "form": "네 허리 중간 쯤? 까지 와요",
          "original_form": "네 허리 중간 쯤? 까지 와요",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045976.1.2",
          "form": "오 많이기시네요",
          "original_form": "오 많이기시네요",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045976.1.3",
          "form": "스트레이트세요?",
          "original_form": "스트레이트세요?",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045976.1.4",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045976.1.5",
          "form": "관리하기힘드신가요",
          "original_form": "관리하기힘드신가요",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045976.1.6",
          "form": "직모라 그냥 감고 말리고 밖에 안 해요",
          "original_form": "직모라 그냥 감고 말리고 밖에 안 해요",
          "speaker_id": "2",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045976.1.7",
          "form": "ㅎㅎ염색하셨어요?",
          "original_form": "ㅎㅎ염색하셨어요?",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045976.1.8",
          "form": "했었어요",
          "original_form": "했었어요",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045976.1.9",
          "form": "지금은무슨색이세요",
          "original_form": "지금은무슨색이세요",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045976.1.10",
          "form": "지금은 탈색모 다 잘라서 검은색이애요",
          "original_form": "지금은 탈색모 다 잘라서 검은색이애요",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045976.1.11",
          "form": "오 저는아직멀었는데부러워요",
          "original_form": "오 저는아직멀었는데부러워요",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045976.1.12",
          "form": "오 무슨 색이새요?",
          "original_form": "오 무슨 색이새요?",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045976.1.13",
          "form": "저는갈색인데블랙으로덮었어요",
          "original_form": "저는갈색인데블랙으로덮었어요",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045976.1.14",
          "form": "블랙으로 덮으면 다시 염색이나 탈색 못 하잖아요ㅠㅠ",
          "original_form": "블랙으로 덮으면 다시 염색이나 탈색 못 하잖아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045976.1.15",
          "form": "ㅜㅜ힝면접때라덮었어요",
