      "utterance": [
        {
          "id": "MDRW2100024483.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "1",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100024483.1.2",
          "form": "해야하는데 안하구있어요.ㅠ 홈트해보려고 집에 이것저것 있는데",
          "original_form": "해야하는데 안하구있어요.ㅠ 홈트해보려고 집에 이것저것 있는데",
          "speaker_id": "2",
          "time": "20210831 11:22"
        },
        {
          "id": "MDRW2100024483.1.3",
          "form": "오 선선해지면 시작해보셔요",
          "original_form": "오 선선해지면 시작해보셔요",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100024483.1.4",
          "form": "지금도 충분히 선선한데 의지박약인거 같아요^^;",
          "original_form": "지금도 충분히 선선한데 의지박약인거 같아요^^;",
          "speaker_id": "2",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100024483.1.5",
          "form": "아 요즘 선선해요?",
          "original_form": "아 요즘 선선해요?",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024483.1.6",
          "form": "여긴 아직 한여름이에요ㅜㅠ",
          "original_form": "여긴 아직 한여름이에요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024483.1.7",
          "form": "더워서 밤에 깨요",
          "original_form": "더워서 밤에 깨요",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024483.1.8",
          "form": "그래요? 밤에 춥던데ㅎㅎ신기하네요.",
          "original_form": "그래요? 밤에 춥던데ㅎㅎ신기하네요.",
          "speaker_id": "2",
          "time": "20210831 15:14"
        },
        {
          "id": "MDRW2100024483.1.9",
          "form": "오 정말요 너무 더워요 ㅠㅠ흑흑",
          "original_form": "오 정말요 너무 더워요 ㅠㅠ흑흑",
          "speaker_id": "1",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100024483.1.10",
          "form": "시원해져서 가을 옷 입고싶습니다",
          "original_form": "시원해져서 가을 옷 입고싶습니다",
          "speaker_id": "1",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100024483.1.11",
          "form": "더운데 운동중이세요?^^",
          "original_form": "더운데 운동중이세요?^^",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100024483.1.12",
          "form": "강아지 키워서 산책이용^^",
          "original_form": "강아지 키워서 산책이용^^",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024483.1.13",
          "form": "주말엔 댄스레슨 배워요",
          "original_form": "주말엔 댄스레슨 배워요",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024483.1.14",
          "form": "오 저도 강아지랑 산책은해요ㅎㅎ\r\n댄스레슨 재미있으시겠네요.",
          "original_form": "오 저도 강아지랑 산책은해요ㅎㅎ\r\n댄스레슨 재미있으시겠네요.",
          "speaker_id": "2",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100024483.1.15",
          "form": "오 강아지키우시나요",
          "original_form": "오 강아지키우시나요",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024483.1.16",
          "form": "네네 아이돌 댄스 운동 대신 해요",
          "original_form": "네네 아이돌 댄스 운동 대신 해요",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024483.1.17",
          "form": "와 아이돌 댄스 대박~~ 옛날신입사원때 연말공연하느라 잠깐 배워서 한게 다인데 저는 재능이없더라구요.",
          "original_form": "와 아이돌 댄스 대박~~ 옛날신입사원때 연말공연하느라 잠깐 배워서 한게 다인데 저는 재능이없더라구요.",
          "speaker_id": "2",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024483.1.18",
          "form": "우와 그러셨구나",
          "original_form": "우와 그러셨구나",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024483.1.19",
          "form": "저는 개인취미로 해요 고딩때부터 하고싶었어서",
          "original_form": "저는 개인취미로 해요 고딩때부터 하고싶었어서",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024483.1.20",
          "form": "^^  좋은 취미같아요. 근데 비용이 비싸지 않나요?",
          "original_form": "^^  좋은 취미같아요. 근데 비용이 비싸지 않나요?",
          "speaker_id": "2",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024483.1.21",
          "form": "친구랑 2인으로 하면 1시간에 3만원이에요",
          "original_form": "친구랑 2인으로 하면 1시간에 3만원이에요",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100024483.1.22",
          "form": "한곡 배우는데 9만원정도요 1인당",
          "original_form": "한곡 배우는데 9만원정도요 1인당",
