      "utterance": [
        {
          "id": "MDRW2100009363.1.1",
          "form": "집안일 좋아하세요?",
          "original_form": "집안일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009363.1.2",
          "form": "깔꼼한거 좋아해서..",
          "original_form": "깔꼼한거 좋아해서..",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009363.1.3",
          "form": "평생 주부하고 싶어요",
          "original_form": "평생 주부하고 싶어요",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009363.1.4",
          "form": "청소 자주 하시나요?",
          "original_form": "청소 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009363.1.5",
          "form": "엇 저는 청소는 그렇게 좋아하지않아요ㅠㅠ",
          "original_form": "엇 저는 청소는 그렇게 좋아하지않아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009363.1.6",
          "form": "설거지하는 거 좋아해요!",
          "original_form": "설거지하는 거 좋아해요!",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009363.1.7",
          "form": "신기하시네요..",
          "original_form": "신기하시네요..",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009363.1.8",
          "form": "설거지 보통 다 싫어하잖아요..",
          "original_form": "설거지 보통 다 싫어하잖아요..",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009363.1.9",
          "form": "노래 틀어놓고 설거지하면 너무좋더라구요ㅋㅋ",
          "original_form": "노래 틀어놓고 설거지하면 너무좋더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009363.1.10",
          "form": "해치우는 기분?",
          "original_form": "해치우는 기분?",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009363.1.11",
          "form": "오... 전 돈 벌면 식기세척기부터 살꺼에요.. ㅋㅋ",
          "original_form": "오... 전 돈 벌면 식기세척기부터 살꺼에요.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009363.1.12",
          "form": "청소는 좋은데 설거지가...",
          "original_form": "청소는 좋은데 설거지가...",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009363.1.13",
          "form": "자취이세요?",
          "original_form": "자취이세요?",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009363.1.14",
          "form": "네! 자취하고있어요!",
          "original_form": "네! 자취하고있어요!",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009363.1.15",
          "form": "청소기 추천해주세요!",
          "original_form": "청소기 추천해주세요!",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009363.1.16",
          "form": "청소기는... 빗자루가 와따..ㅋㅋㅋ",
          "original_form": "청소기는... 빗자루가 와따..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009363.1.17",
          "form": "지금 청소기 쓰세요?",
          "original_form": "지금 청소기 쓰세요?",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009363.1.18",
          "form": "아니요ㅋㅋ 자취생은 돌돌이와 물티슈죠!ㅋㅋ",
          "original_form": "아니요ㅋㅋ 자취생은 돌돌이와 물티슈죠!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009363.1.19",
          "form": "아.. 저도 그거.. 물티슈 청소포써요..",
          "original_form": "아.. 저도 그거.. 물티슈 청소포써요..",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009363.1.20",
          "form": "다이소에서 파는거..",
          "original_form": "다이소에서 파는거..",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009363.1.21",
          "form": "청소기나 빗자루는 번거롭고",
          "original_form": "청소기나 빗자루는 번거롭고",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009363.1.22",
          "form": "청소잘되나용?",
          "original_form": "청소잘되나용?",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009363.1.23",
          "form": "너무싼거 그렇고.. \r\n3m꺼가 튼튼하고 좋은거 같아요..",
          "original_form": "너무싼거 그렇고.. \r\n3m꺼가 튼튼하고 좋은거 같아요..",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009363.1.24",
          "form": "저렴이는 대가 약하더라구요.. \r\n청소포는 싼거도 괜찮고..",
          "original_form": "저렴이는 대가 약하더라구요.. \r\n청소포는 싼거도 괜찮고..",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009363.1.25",
          "form": "구경하러 가봐야겠습니다 ㅎㅎ",
          "original_form": "구경하러 가봐야겠습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:13"
        }
      ]
    }
