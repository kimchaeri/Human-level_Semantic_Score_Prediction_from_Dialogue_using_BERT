      "utterance": [
        {
          "id": "MDRW2100013575.1.1",
          "form": "커뮤 하시는거 있어요?",
          "original_form": "커뮤 하시는거 있어요?",
          "speaker_id": "1",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013575.1.2",
          "form": "글이나 댓글쓰면서 활동하는건 없구 그냥 웃긴자료 눈팅만 하는데",
          "original_form": "글이나 댓글쓰면서 활동하는건 없구 그냥 웃긴자료 눈팅만 하는데",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013575.1.3",
          "form": "넌 하는거 있어?",
          "original_form": "넌 하는거 있어?",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013575.1.4",
          "form": "난 웃대랑 클리앙 같은거 눈팅해",
          "original_form": "난 웃대랑 클리앙 같은거 눈팅해",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013575.1.5",
          "form": "웃대 알아?",
          "original_form": "웃대 알아?",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013575.1.6",
          "form": "오ㅋㅋㅋㅋ 웃대 유명하잖아",
          "original_form": "오ㅋㅋㅋㅋ 웃대 유명하잖아",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013575.1.7",
          "form": "클리앙은 사실 잘 모르구",
          "original_form": "클리앙은 사실 잘 모르구",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013575.1.8",
          "form": "웃대는 어릴때 많이 본듯!",
          "original_form": "웃대는 어릴때 많이 본듯!",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013575.1.9",
          "form": "ㅋㅋㅋ 요즘도 웃대 눈팅 재밌엉.. ㅋㅋ",
          "original_form": "ㅋㅋㅋ 요즘도 웃대 눈팅 재밌엉.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013575.1.10",
          "form": "그럼 페북 주로해?",
          "original_form": "그럼 페북 주로해?",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013575.1.11",
          "form": "페북도 눈팅을 주로 해ㅋㅋ",
          "original_form": "페북도 눈팅을 주로 해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013575.1.12",
          "form": "페북은 예전엔 좋아했는데",
          "original_form": "페북은 예전엔 좋아했는데",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013575.1.13",
          "form": "요즘 좀 많이 변했지ㅠㅠ",
          "original_form": "요즘 좀 많이 변했지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013575.1.14",
          "form": "페북이 왜?",
          "original_form": "페북이 왜?",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013575.1.15",
          "form": "광고도 많아지구",
          "original_form": "광고도 많아지구",
          "speaker_id": "2",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013575.1.16",
          "form": "친구들도 활동 뜸해지구..",
          "original_form": "친구들도 활동 뜸해지구..",
          "speaker_id": "2",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013575.1.17",
          "form": "아.. 그럼 틱톡으로 많이강건가?",
          "original_form": "아.. 그럼 틱톡으로 많이강건가?",
          "speaker_id": "1",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013575.1.18",
          "form": "틱톡은 너무 인싸아닌가.. ㅋㅋㅋ",
          "original_form": "틱톡은 너무 인싸아닌가.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013575.1.19",
          "form": "틱톡 넘나 인싸.... ㅋㅋㅋㅋㅋㅋㅋ 따라갈수가없어",
          "original_form": "틱톡 넘나 인싸.... ㅋㅋㅋㅋㅋㅋㅋ 따라갈수가없어",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013575.1.20",
          "form": "ㅋㅋㅋ 너도 아싸?",
          "original_form": "ㅋㅋㅋ 너도 아싸?",
          "speaker_id": "1",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013575.1.21",
          "form": "웅ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 아싸이면서 집순이이면서 커뮤눈팅러",
          "original_form": "웅ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 아싸이면서 집순이이면서 커뮤눈팅러",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013575.1.22",
          "form": "웃긴자료는 어서봐?",
          "original_form": "웃긴자료는 어서봐?",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013575.1.23",
          "form": "나 뽐뿌나 펨코나 오유 이런거 유머게시판?",
          "original_form": "나 뽐뿌나 펨코나 오유 이런거 유머게시판?",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013575.1.24",
          "form": "근데 요즘 커뮤사람들이 화가많고 너무 서로 싸워대서... ㅠ",
          "original_form": "근데 요즘 커뮤사람들이 화가많고 너무 서로 싸워대서... ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013575.1.25",
          "form": "맞어.. 갈등조장 심하지..",
          "original_form": "맞어.. 갈등조장 심하지..",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013575.1.26",
          "form": "국정원이랑 보수놈들이 작업친게 자리잡은듯..",
          "original_form": "국정원이랑 보수놈들이 작업친게 자리잡은듯..",
          "speaker_id": "1",
          "time": "20210825 15:28"
        }
      ]
    }
