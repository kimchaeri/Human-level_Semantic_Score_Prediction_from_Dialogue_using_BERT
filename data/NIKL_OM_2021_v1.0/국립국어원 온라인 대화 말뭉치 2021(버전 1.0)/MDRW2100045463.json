        {
          "id": "MDRW2100045463.1.1",
          "form": "일단 암은 걸리기 전에 죽엇을듯",
          "original_form": "일단 암은 걸리기 전에 죽엇을듯",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045463.1.2",
          "form": "엌",
          "original_form": "엌",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045463.1.3",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ그건 그래",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ그건 그래",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045463.1.4",
          "form": "그거는 약간 인간이",
          "original_form": "그거는 약간 인간이",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045463.1.5",
          "form": "원래 야생에서 막살던때보다 수명 길어지니까",
          "original_form": "원래 야생에서 막살던때보다 수명 길어지니까",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045463.1.6",
          "form": "세포변이 일어나는 느낌이잔아",
          "original_form": "세포변이 일어나는 느낌이잔아",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045463.1.7",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045463.1.8",
          "form": "치매도 사실",
          "original_form": "치매도 사실",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045463.1.9",
          "form": "대부분 그 전에 죽었을거같고",
          "original_form": "대부분 그 전에 죽었을거같고",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045463.1.10",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045463.1.11",
          "form": "고기 거의 못먹어서 성인병 없을거고",
          "original_form": "고기 거의 못먹어서 성인병 없을거고",
          "speaker_id": "2",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045463.1.12",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045463.1.13",
          "form": "자동차 컴퓨터 없어서",
          "original_form": "자동차 컴퓨터 없어서",
          "speaker_id": "2",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045463.1.14",
          "form": "하긴",
          "original_form": "하긴",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045463.1.15",
          "form": "거북목이나 측만 없엇을거같고",
          "original_form": "거북목이나 측만 없엇을거같고",
          "speaker_id": "2",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045463.1.16",
          "form": "와 그럼",
          "original_form": "와 그럼",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045463.1.17",
          "form": "당뇨 없을거고",
          "original_form": "당뇨 없을거고",
          "speaker_id": "2",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045463.1.18",
          "form": "어깨결림도",
          "original_form": "어깨결림도",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045463.1.19",
          "form": "없었으려나",
          "original_form": "없었으려나",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045463.1.20",
          "form": "하긴 먹자마자 누울수 없는 환경이라",
          "original_form": "하긴 먹자마자 누울수 없는 환경이라",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045463.1.21",
          "form": "ㅋㅋㅋㅋㅋ 그건 잇엇을듯",
          "original_form": "ㅋㅋㅋㅋㅋ 그건 잇엇을듯",
