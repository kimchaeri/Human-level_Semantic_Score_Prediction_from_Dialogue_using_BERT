      "utterance": [
        {
          "id": "MDRW2100034177.1.1",
          "form": "어디까지 말했죠 까묵",
          "original_form": "어디까지 말했죠 까묵",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100034177.1.2",
          "form": "음식물",
          "original_form": "음식물",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100034177.1.3",
          "form": "쓰레기요 ㅎㅎ",
          "original_form": "쓰레기요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100034177.1.4",
          "form": "저 오늘 배달했더니 ㅜㅜ다 버리고",
          "original_form": "저 오늘 배달했더니 ㅜㅜ다 버리고",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100034177.1.5",
          "form": "아아 음쓰를 무게를 달아서 버려서 이젠 무게별로 돈내요 ㅠㅠㅋㅋ",
          "original_form": "아아 음쓰를 무게를 달아서 버려서 이젠 무게별로 돈내요 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100034177.1.6",
          "form": "담엔 안전한거로 배달할래요",
          "original_form": "담엔 안전한거로 배달할래요",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100034177.1.7",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100034177.1.8",
          "form": "여긴 봉지 사서 버려야 해요",
          "original_form": "여긴 봉지 사서 버려야 해요",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100034177.1.9",
          "form": "그리고 봉지 다 찰때까지ㅏ 기다리는게 넘 힘들어요",
          "original_form": "그리고 봉지 다 찰때까지ㅏ 기다리는게 넘 힘들어요",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100034177.1.10",
          "form": "그렇죠 냄새",
          "original_form": "그렇죠 냄새",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034177.1.11",
          "form": "호박죽 은 직접 하셨어요?",
          "original_form": "호박죽 은 직접 하셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034177.1.12",
          "form": "엄마가 해주신걸 싸왔어요,,,",
          "original_form": "엄마가 해주신걸 싸왔어요,,,",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034177.1.13",
          "form": "ㅎㅎㅎㅎ저라면 그냥 호박째로먹을 것 같아요 ㅋㅋ",
          "original_form": "ㅎㅎㅎㅎ저라면 그냥 호박째로먹을 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034177.1.14",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100034177.1.15",
          "form": "호박죽 싫어하는분 많긴해요",
          "original_form": "호박죽 싫어하는분 많긴해요",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100034177.1.16",
          "form": "저도 단호박 그냥 쪄먹어요",
          "original_form": "저도 단호박 그냥 쪄먹어요",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100034177.1.17",
          "form": "쪄먹는게 간편하고 맛있어요 죽은 밥이나 찹쌀이 들어가서요",
          "original_form": "쪄먹는게 간편하고 맛있어요 죽은 밥이나 찹쌀이 들어가서요",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100034177.1.18",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100034177.1.19",
          "form": "저도 찐거 좋아해요",
          "original_form": "저도 찐거 좋아해요",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100034177.1.20",
          "form": "편하고 맛있고",
          "original_form": "편하고 맛있고",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100034177.1.21",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100034177.1.22",
          "form": "전자레인지에 쪄도 좋아요",
          "original_form": "전자레인지에 쪄도 좋아요",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100034177.1.23",
          "form": "저도 렌지를 ㅡ애용해요 좀 익힌담에 자르면 잘잘려요",
          "original_form": "저도 렌지를 ㅡ애용해요 좀 익힌담에 자르면 잘잘려요",
