      "utterance": [
        {
          "id": "MDRW2100033459.1.1",
          "form": "아침은 막장이죠",
          "original_form": "아침은 막장이죠",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100033459.1.2",
          "form": "아 금요 드라마 남궁민 나오는거 한데요",
          "original_form": "아 금요 드라마 남궁민 나오는거 한데요",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100033459.1.3",
          "form": "완전 잼나보이던데",
          "original_form": "완전 잼나보이던데",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100033459.1.4",
          "form": "제목이 생각 안나네요",
          "original_form": "제목이 생각 안나네요",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100033459.1.5",
          "form": "아직 시작은 안한거예요?",
          "original_form": "아직 시작은 안한거예요?",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100033459.1.6",
          "form": "네 9월요",
          "original_form": "네 9월요",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100033459.1.7",
          "form": "요즘 본방송을 안보니 뭐가 끝나고 뭐가 시작하는지 잘 모르겠네요",
          "original_form": "요즘 본방송을 안보니 뭐가 끝나고 뭐가 시작하는지 잘 모르겠네요",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100033459.1.8",
          "form": "챙겨서 봐야겠어요 !!\r\n감사합니다^^",
          "original_form": "챙겨서 봐야겠어요 !!\r\n감사합니다^^",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100033459.1.9",
          "form": "검은태양요",
          "original_form": "검은태양요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100033459.1.10",
          "form": "완전 카리스마 넘치던데 ㅋㅋ",
          "original_form": "완전 카리스마 넘치던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100033459.1.11",
          "form": "잼날것 같아요",
          "original_form": "잼날것 같아요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100033459.1.12",
          "form": "찾아보세요",
          "original_form": "찾아보세요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100033459.1.13",
          "form": "저 모범택시도 잘봤는데 ㅋㅋ",
          "original_form": "저 모범택시도 잘봤는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100033459.1.14",
          "form": "아아아 !! 재밌겠어요!\r\n저도 모범택시는 매주 매주 봤어요 ㅎㅎ",
          "original_form": "아아아 !! 재밌겠어요!\r\n저도 모범택시는 매주 매주 봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100033459.1.15",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100033459.1.16",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033459.1.17",
          "form": "이제훈 와 멋져요",
          "original_form": "이제훈 와 멋져요",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033459.1.18",
          "form": "스토리도 잼나고",
          "original_form": "스토리도 잼나고",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033459.1.19",
          "form": "그런사람들이 복수해주면 한이 풀릴것 같아요",
          "original_form": "그런사람들이 복수해주면 한이 풀릴것 같아요",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033459.1.20",
          "form": "그쵸 ! 내용이 획기적이였어요 \r\n복수를 한다는게",
          "original_form": "그쵸 ! 내용이 획기적이였어요 \r\n복수를 한다는게",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033459.1.21",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100033459.1.22",
          "form": "그 게임 목소리 이영애래요",
          "original_form": "그 게임 목소리 이영애래요",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100033459.1.23",
          "form": "어? 몰랐어요!",
          "original_form": "어? 몰랐어요!",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100033459.1.24",
          "form": "복수하겠습니가?!",
          "original_form": "복수하겠습니가?!",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033459.1.25",
          "form": "그거 말이죠?",
          "original_form": "그거 말이죠?",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033459.1.26",
          "form": "네 ㅋㅋ잘들어보면 알아요",
          "original_form": "네 ㅋㅋ잘들어보면 알아요",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033459.1.27",
          "form": "난 특별출연 어디 나왔나 했어요 ㅋㅋ",
          "original_form": "난 특별출연 어디 나왔나 했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033459.1.28",
          "form": "아.. 저는 아무생각없이 들어서 그런가봐요 !\r\n다시한번 봐야겠어요 ㅎㅎㅎ",
          "original_form": "아.. 저는 아무생각없이 들어서 그런가봐요 !\r\n다시한번 봐야겠어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033459.1.29",
          "form": "봐보세요 알면 잼나요",
          "original_form": "봐보세요 알면 잼나요",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100033459.1.30",
          "form": "아 왜 친절한 금자씨 생각나죠 ㅋㅋ",
          "original_form": "아 왜 친절한 금자씨 생각나죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100033459.1.31",
          "form": "맞다.. 친절한 금자씨도 재밌게 봤었는데..",
          "original_form": "맞다.. 친절한 금자씨도 재밌게 봤었는데..",
          "speaker_id": "1",
          "time": "20210823 23:23"
        }
      ]
    }
