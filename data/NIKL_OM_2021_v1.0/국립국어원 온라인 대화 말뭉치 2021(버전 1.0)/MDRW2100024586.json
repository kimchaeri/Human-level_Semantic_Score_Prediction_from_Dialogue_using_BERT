      "utterance": [
        {
          "id": "MDRW2100024586.1.1",
          "form": "쇼핑은 자주하시나요?",
          "original_form": "쇼핑은 자주하시나요?",
          "speaker_id": "1",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100024586.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100024586.1.3",
          "form": "물욕이 없는 편인지 자주하지는 않아요",
          "original_form": "물욕이 없는 편인지 자주하지는 않아요",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100024586.1.4",
          "form": "최근에는 뭘 사셨나요?",
          "original_form": "최근에는 뭘 사셨나요?",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024586.1.5",
          "form": "최근엔",
          "original_form": "최근엔",
          "speaker_id": "2",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100024586.1.6",
          "form": "아 이거 좀 충동구매이긴한데",
          "original_form": "아 이거 좀 충동구매이긴한데",
          "speaker_id": "2",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100024586.1.7",
          "form": "김연경 선수 유니폼 티",
          "original_form": "김연경 선수 유니폼 티",
          "speaker_id": "2",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100024586.1.8",
          "form": "샀어욬ㅋㅋㅋㅋ",
          "original_form": "샀어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100024586.1.9",
          "form": "어머 ㅋㅋㅋㅋㅋ 애국하셨네요",
          "original_form": "어머 ㅋㅋㅋㅋㅋ 애국하셨네요",
          "speaker_id": "1",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100024586.1.10",
          "form": "뭔가 멋진옷일거 같네요!",
          "original_form": "뭔가 멋진옷일거 같네요!",
          "speaker_id": "1",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100024586.1.11",
          "form": "ㅋㅋㅌㅌㅋㅋ",
          "original_form": "ㅋㅋㅌㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100024586.1.12",
          "form": "도쿄올림픽 보고",
          "original_form": "도쿄올림픽 보고",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100024586.1.13",
          "form": "감명받아서",
          "original_form": "감명받아서",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100024586.1.14",
          "form": "샀으나 지금은 살짝 후회중이에요ㅜ",
          "original_form": "샀으나 지금은 살짝 후회중이에요ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100024586.1.15",
          "form": "왜요?ㅎㅎㅎ",
          "original_form": "왜요?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024586.1.16",
          "form": "디자인이 별로인가요?",
          "original_form": "디자인이 별로인가요?",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024586.1.17",
          "form": "아니요 그게 음",
          "original_form": "아니요 그게 음",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024586.1.18",
          "form": "제가 산게 김연경선수 유니폼이거든요",
          "original_form": "제가 산게 김연경선수 유니폼이거든요",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024586.1.19",
          "form": "도쿄올임픽 배구 재밌게봐서",
          "original_form": "도쿄올임픽 배구 재밌게봐서",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024586.1.20",
          "form": "그당시에는 기분좋게 질렀는데!",
          "original_form": "그당시에는 기분좋게 질렀는데!",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024586.1.21",
          "form": "지금은 저걸 내가 입을까, 살짝 후회되네요ㅋㅋㅋ",
          "original_form": "지금은 저걸 내가 입을까, 살짝 후회되네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100024586.1.22",
          "form": "아. ....",
          "original_form": "아. ....",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100024586.1.23",
          "form": "입기가..코디하기가 상당히 난감하네요ㅋㅋ",
          "original_form": "입기가..코디하기가 상당히 난감하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024586.1.24",
          "form": "운동할때 입을 수 있을까요?ㅎㅎ",
          "original_form": "운동할때 입을 수 있을까요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024586.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024586.1.26",
          "form": "사실 운동할때 입으력고 샀어요",
          "original_form": "사실 운동할때 입으력고 샀어요",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024586.1.27",
          "form": "내가 김연경선수다!",
          "original_form": "내가 김연경선수다!",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024586.1.28",
          "form": "이런느낌으로",
          "original_form": "이런느낌으로",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024586.1.29",
          "form": "근데 운동을 잘 안할것같아서ㅜ",
          "original_form": "근데 운동을 잘 안할것같아서ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100024586.1.30",
          "form": "..아...그럼 일단 입으시면 운동하고 싶어지지 않을까요?",
          "original_form": "..아...그럼 일단 입으시면 운동하고 싶어지지 않을까요?",
          "speaker_id": "1",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100024586.1.31",
          "form": "그렇겠죠?",
          "original_form": "그렇겠죠?",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100024586.1.32",
          "form": "...ㅜ",
          "original_form": "...ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100024586.1.33",
          "form": "그게 35000원이거든요",
          "original_form": "그게 35000원이거든요",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100024586.1.34",
          "form": "그 값을 생각허면서 해야겠어요",
          "original_form": "그 값을 생각허면서 해야겠어요",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100024586.1.35",
          "form": "와우...걷기운동이라도...고고고",
          "original_form": "와우...걷기운동이라도...고고고",
          "speaker_id": "1",
          "time": "20210831 22:28"
        }
      ]
    }
