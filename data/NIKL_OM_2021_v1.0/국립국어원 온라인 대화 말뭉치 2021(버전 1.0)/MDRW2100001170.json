        },
        {
          "id": "MDRW2100001170.1.2",
          "form": "오 하이",
          "original_form": "오 하이",
          "speaker_id": "2",
          "time": "20210804 16:51"
        },
        {
          "id": "MDRW2100001170.1.3",
          "form": "오랜만",
          "original_form": "오랜만",
          "speaker_id": "2",
          "time": "20210804 16:51"
        },
        {
          "id": "MDRW2100001170.1.4",
          "form": "아고 요새 덥다ㅋ 그지?",
          "original_form": "아고 요새 덥다ㅋ 그지?",
          "speaker_id": "1",
          "time": "20210804 16:51"
        },
        {
          "id": "MDRW2100001170.1.5",
          "form": "응 넘 더워가지고 아무데도 못가겠어",
          "original_form": "응 넘 더워가지고 아무데도 못가겠어",
          "speaker_id": "2",
          "time": "20210804 16:52"
        },
        {
          "id": "MDRW2100001170.1.6",
          "form": "습고 덥고 장난아냐",
          "original_form": "습고 덥고 장난아냐",
          "speaker_id": "2",
          "time": "20210804 16:52"
        },
        {
          "id": "MDRW2100001170.1.7",
          "form": "언닌 어때",
          "original_form": "언닌 어때",
          "speaker_id": "2",
          "time": "20210804 16:54"
        },
        {
          "id": "MDRW2100001170.1.8",
          "form": "응 넘 덥다ㅋㅋ 나도 몸이 쳐지구\r\n암것도 못하겠다",
          "original_form": "응 넘 덥다ㅋㅋ 나도 몸이 쳐지구\r\n암것도 못하겠다",
          "speaker_id": "1",
          "time": "20210804 16:54"
        },
        {
          "id": "MDRW2100001170.1.9",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210804 16:55"
        },
        {
          "id": "MDRW2100001170.1.10",
          "form": "입맛도 없고",
          "original_form": "입맛도 없고",
          "speaker_id": "2",
          "time": "20210804 16:55"
        },
        {
          "id": "MDRW2100001170.1.11",
          "form": "나는 시원한 물만 달고 살자너 ㅋㅋㅋ",
          "original_form": "나는 시원한 물만 달고 살자너 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210804 16:55"
        },
        {
          "id": "MDRW2100001170.1.12",
          "form": "응 건강도 챙겨야되는데",
          "original_form": "응 건강도 챙겨야되는데",
          "speaker_id": "1",
          "time": "20210804 16:55"
        },
        {
          "id": "MDRW2100001170.1.13",
          "form": "식사 챙기기도 힘들구ㅜ",
          "original_form": "식사 챙기기도 힘들구ㅜ",
          "speaker_id": "1",
          "time": "20210804 16:56"
        },
        {
          "id": "MDRW2100001170.1.14",
          "form": "그래도 잘 챙겨먹어야지",
          "original_form": "그래도 잘 챙겨먹어야지",
          "speaker_id": "2",
          "time": "20210804 16:57"
        },
        {
          "id": "MDRW2100001170.1.15",
          "form": "오늘은 뭐 먹었어",
          "original_form": "오늘은 뭐 먹었어",
          "speaker_id": "2",
          "time": "20210804 16:57"
        },
        {
          "id": "MDRW2100001170.1.16",
          "form": "입맛 없어가지구",
          "original_form": "입맛 없어가지구",
          "speaker_id": "1",
          "time": "20210804 16:57"
        },
        {
          "id": "MDRW2100001170.1.17",
          "form": "아침에 간단하게 먹게 되더라구",
          "original_form": "아침에 간단하게 먹게 되더라구",
          "speaker_id": "1",
          "time": "20210804 16:57"
        },
        {
          "id": "MDRW2100001170.1.18",
          "form": "선식이랑 음 뭐더라",
          "original_form": "선식이랑 음 뭐더라",
          "speaker_id": "1",
          "time": "20210804 16:57"
        },
        {
          "id": "MDRW2100001170.1.19",
          "form": "선식 같은거 타먹어",
          "original_form": "선식 같은거 타먹어",
          "speaker_id": "1",
          "time": "20210804 16:57"
        },
        {
          "id": "MDRW2100001170.1.20",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210804 16:58"
        },
        {
          "id": "MDRW2100001170.1.21",
          "form": "올 ㅋㅋ 나도 그거 먹어봐야겠다",
          "original_form": "올 ㅋㅋ 나도 그거 먹어봐야겠다",
          "speaker_id": "2",
          "time": "20210804 17:19"
        },
        {
          "id": "MDRW2100001170.1.22",
          "form": "어릴때 많이 먹었는데",
          "original_form": "어릴때 많이 먹었는데",
          "speaker_id": "2",
          "time": "20210804 17:19"
        },
        {
          "id": "MDRW2100001170.1.23",
          "form": "나는 아침 그냥 대충 거르고 점저 먹어",
          "original_form": "나는 아침 그냥 대충 거르고 점저 먹어",
          "speaker_id": "2",
          "time": "20210804 17:20"
        },
        {
          "id": "MDRW2100001170.1.24",
          "form": "응ㅋ 입맛없고 기운없을때\r\n진짜 좋더라고",
          "original_form": "응ㅋ 입맛없고 기운없을때\r\n진짜 좋더라고",
          "speaker_id": "1",
          "time": "20210804 17:20"
        },
        {
          "id": "MDRW2100001170.1.25",
          "form": "볶음밥 해먹어야지",
          "original_form": "볶음밥 해먹어야지",
          "speaker_id": "2",
          "time": "20210804 17:20"
        },
        {
          "id": "MDRW2100001170.1.26",
          "form": "두유에 타서 먹어봐",
          "original_form": "두유에 타서 먹어봐",
          "speaker_id": "1",
          "time": "20210804 17:20"
        }
