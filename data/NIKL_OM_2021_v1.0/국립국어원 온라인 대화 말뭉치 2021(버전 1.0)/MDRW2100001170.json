          "speaker_id": "1",
          "time": "20210804 17:20"
        },
        {
          "id": "MDRW2100001170.2.3",
          "form": "오키 알았어 마셔볼게",
          "original_form": "오키 알았어 마셔볼게",
          "speaker_id": "2",
          "time": "20210804 17:20"
        },
