      "utterance": [
        {
          "id": "MDRW2100014497.1.1",
          "form": "제일 좋아하는 음식이 뭐예요?",
          "original_form": "제일 좋아하는 음식이 뭐예요?",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100014497.1.2",
          "form": "전 단 음식 좋아해요ㅎㅎ",
          "original_form": "전 단 음식 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100014497.1.3",
          "form": "디저트류요",
          "original_form": "디저트류요",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100014497.1.4",
          "form": "오옹 마시는거요 아님 먹는거요?",
          "original_form": "오옹 마시는거요 아님 먹는거요?",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100014497.1.5",
          "form": "주로 먹는거요",
          "original_form": "주로 먹는거요",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100014497.1.6",
          "form": "케이크나 초콜릿 이런거 좋아해요ㅎㅎ",
          "original_form": "케이크나 초콜릿 이런거 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100014497.1.7",
          "form": "아하 그럼 초코 케이크 좋아하세요?ㅎㅎ",
          "original_form": "아하 그럼 초코 케이크 좋아하세요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100014497.1.8",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100014497.1.9",
          "form": "name2님은 디저트 좋아하세요??",
          "original_form": "&name2&님은 디저트 좋아하세요??",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100014497.1.10",
          "form": "네! 좋아하죠ㅠ 저는 주로 음료를 많이 마셔요",
          "original_form": "네! 좋아하죠ㅠ 저는 주로 음료를 많이 마셔요",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100014497.1.11",
          "form": "오 혹시 카페에서 자주 시키는 음료 있으세요??",
          "original_form": "오 혹시 카페에서 자주 시키는 음료 있으세요??",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100014497.1.12",
          "form": "저는 아이 입맛이라 단 음료를 찾게 되는 것같아요",
          "original_form": "저는 아이 입맛이라 단 음료를 찾게 되는 것같아요",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100014497.1.13",
          "form": "커피도 연유라떼 이런거요ㅋㅋㅋ",
          "original_form": "커피도 연유라떼 이런거요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100014497.1.14",
          "form": "저도 그래요ㅋㅋ 커피 안좋아하고 카페가면 아이스초코 이런거 시켜요ㅋㅋㅋ",
          "original_form": "저도 그래요ㅋㅋ 커피 안좋아하고 카페가면 아이스초코 이런거 시켜요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100014497.1.15",
          "form": "아앗ㅋㅋㅋㅋ",
          "original_form": "아앗ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100014497.1.16",
          "form": "헉 디저트 얘기하다보니",
          "original_form": "헉 디저트 얘기하다보니",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100014497.1.17",
          "form": "오랜만에 허니브레드...그거...먹고 싶어요...",
          "original_form": "오랜만에 허니브레드...그거...먹고 싶어요...",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100014497.1.18",
          "form": "아 그거 맛있죠ㅋㅋ",
          "original_form": "아 그거 맛있죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100014497.1.19",
          "form": "한때 되게 많이팔았는데 요즘은 좀 덜보이더라고요",
          "original_form": "한때 되게 많이팔았는데 요즘은 좀 덜보이더라고요",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100014497.1.20",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100014497.1.21",
          "form": "그거 먹을땐 조금 삼삼한 음료마셔주면 딱이에요",
          "original_form": "그거 먹을땐 조금 삼삼한 음료마셔주면 딱이에요",
          "speaker_id": "2",
          "time": "20210826 00:29"
        }
      ]
