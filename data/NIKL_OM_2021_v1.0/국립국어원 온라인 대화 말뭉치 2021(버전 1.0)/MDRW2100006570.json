      "utterance": [
        {
          "id": "MDRW2100006570.1.1",
          "form": "학창시절 추억 있나뇨",
          "original_form": "학창시절 추억 있나뇨",
          "speaker_id": "1",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100006570.1.2",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100006570.1.3",
          "form": "추억 있나요?",
          "original_form": "추억 있나요?",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100006570.1.4",
          "form": "전 친구들이랑 학교 끝나고 먹으러 다녔던게",
          "original_form": "전 친구들이랑 학교 끝나고 먹으러 다녔던게",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006570.1.5",
          "form": "추억이라면 추억이네요 ㅋㅋㅋㅋ",
          "original_form": "추억이라면 추억이네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006570.1.6",
          "form": "그렇죠 매점 있었나요",
          "original_form": "그렇죠 매점 있었나요",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006570.1.7",
          "form": "아뇨 저흰 매점 없어서 다들 밖에서 사먹었너요",
          "original_form": "아뇨 저흰 매점 없어서 다들 밖에서 사먹었너요",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006570.1.8",
          "form": "아 그랬군요 무슨 가게들이.있었나요",
          "original_form": "아 그랬군요 무슨 가게들이.있었나요",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100006570.1.9",
          "form": "젤 흔한건 분식집이었어요",
          "original_form": "젤 흔한건 분식집이었어요",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100006570.1.10",
          "form": "그렇죠 학교앞 분식점 필수.\r\n.",
          "original_form": "그렇죠 학교앞 분식점 필수.\r\n.",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100006570.1.11",
          "form": "야자 땡땡이치고 갔던게",
          "original_form": "야자 땡땡이치고 갔던게",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006570.1.12",
          "form": "지금 생각해보면 다 추억이네요",
          "original_form": "지금 생각해보면 다 추억이네요",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006570.1.13",
          "form": "저도 야자중간에 먹던 간식들이 생각나요",
          "original_form": "저도 야자중간에 먹던 간식들이 생각나요",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006570.1.14",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100006570.1.15",
          "form": "피자한판..치킨한마리..이렇게요..ㅎㅎㅎ",
          "original_form": "피자한판..치킨한마리..이렇게요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100006570.1.16",
          "form": "몰래 드신건가요?",
          "original_form": "몰래 드신건가요?",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100006570.1.17",
          "form": "아뇨 간식으로 들어온것들이요 ㅎ",
          "original_form": "아뇨 간식으로 들어온것들이요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 02:55"
        }
      ]
