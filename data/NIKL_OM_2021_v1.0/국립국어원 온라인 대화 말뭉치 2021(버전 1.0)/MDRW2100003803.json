      "utterance": [
        {
          "id": "MDRW2100003803.1.1",
          "form": "대구도 비오나요?",
          "original_form": "대구도 비오나요?",
          "speaker_id": "1",
          "time": "20210821 12:19"
        },
        {
          "id": "MDRW2100003803.1.2",
          "form": "네 비와요 서울도 오나요?",
          "original_form": "네 비와요 서울도 오나요?",
          "speaker_id": "2",
          "time": "20210821 12:19"
        },
        {
          "id": "MDRW2100003803.1.3",
          "form": "여기도 흐리긴한데 왔다 그친거같아요",
          "original_form": "여기도 흐리긴한데 왔다 그친거같아요",
          "speaker_id": "1",
          "time": "20210821 12:19"
        },
        {
          "id": "MDRW2100003803.1.4",
          "form": "가을 장마오나봐요",
          "original_form": "가을 장마오나봐요",
          "speaker_id": "1",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003803.1.5",
          "form": "네 가을장마시작이라고 하더라구요",
          "original_form": "네 가을장마시작이라고 하더라구요",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003803.1.6",
          "form": "부산은 많이 온데요 일본에 천미리 그비가 온거같아요",
          "original_form": "부산은 많이 온데요 일본에 천미리 그비가 온거같아요",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003803.1.7",
          "form": "비오고나면 선선해지겠어요",
          "original_form": "비오고나면 선선해지겠어요",
          "speaker_id": "1",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003803.1.8",
          "form": "점점 비가 많이 오려는지",
          "original_form": "점점 비가 많이 오려는지",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003803.1.9",
          "form": "올해는 큰태풍도 없이 지나간거같은데",
          "original_form": "올해는 큰태풍도 없이 지나간거같은데",
          "speaker_id": "1",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003803.1.10",
          "form": "이러다 여름 끝나고 가을 훅 짧게 해서 겨울올꺼같아요",
          "original_form": "이러다 여름 끝나고 가을 훅 짧게 해서 겨울올꺼같아요",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003803.1.11",
          "form": "그니까요 간절기 옷살려고하면 계절바뀌더라구요",
          "original_form": "그니까요 간절기 옷살려고하면 계절바뀌더라구요",
          "speaker_id": "1",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003803.1.12",
          "form": "이젠 간절기옷 사기 좀 아깝더라구요",
          "original_form": "이젠 간절기옷 사기 좀 아깝더라구요",
          "speaker_id": "2",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003803.1.13",
          "form": "트렌치코트 몇번안입고 겨울오니 ㅋㅋ",
          "original_form": "트렌치코트 몇번안입고 겨울오니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003803.1.14",
          "form": "맞아요 트렌치코드 2-3번입금 추워져서",
          "original_form": "맞아요 트렌치코드 2-3번입금 추워져서",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003803.1.15",
          "form": "가을엔 트렌치 좀 입어줘야 제맛인데 말이죠 ㅎ",
          "original_form": "가을엔 트렌치 좀 입어줘야 제맛인데 말이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003803.1.16",
          "form": "단풍구경하면서ㅎㅎ",
          "original_form": "단풍구경하면서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003803.1.17",
          "form": "패딩꺼낼 준비해야죠 뭐ㅎ",
          "original_form": "패딩꺼낼 준비해야죠 뭐ㅎ",
          "speaker_id": "1",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003803.1.18",
          "form": "아.., 단풍 이야기하시니 산행 가고싶네요",
          "original_form": "아.., 단풍 이야기하시니 산행 가고싶네요",
          "speaker_id": "2",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003803.1.19",
          "form": "가을을 제일 좋아하는데 단풍구경 갈 수있을지 모르겠네요",
          "original_form": "가을을 제일 좋아하는데 단풍구경 갈 수있을지 모르겠네요",
          "speaker_id": "1",
          "time": "20210821 12:26"
        }
