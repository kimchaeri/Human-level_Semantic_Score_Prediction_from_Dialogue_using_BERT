      "utterance": [
        {
          "id": "MDRW2100037886.1.1",
          "form": "울첫찌도 할미집가믄 드라마본다 ㅋㅋㅋ",
          "original_form": "울첫찌도 할미집가믄 드라마본다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037886.1.2",
          "form": "그랴~한방에 성공하이소 ㅋㅋㅋㅋ",
          "original_form": "그랴~한방에 성공하이소 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037886.1.3",
          "form": "아ㅡㅡ짜증",
          "original_form": "아ㅡㅡ짜증",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037886.1.4",
          "form": "1시간째 저런다",
          "original_form": "1시간째 저런다",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037886.1.5",
          "form": "누가??",
          "original_form": "누가??",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037886.1.6",
          "form": "안 자는겨??",
          "original_form": "안 자는겨??",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037886.1.7",
          "form": "두찌",
          "original_form": "두찌",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037886.1.8",
          "form": "아 진짜 스트레스받아서못재우겠다",
          "original_form": "아 진짜 스트레스받아서못재우겠다",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037886.1.9",
          "form": "아하 오늘 에너지가 안 빠졌는갑다",
          "original_form": "아하 오늘 에너지가 안 빠졌는갑다",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037886.1.10",
          "form": "매일밤마다",
          "original_form": "매일밤마다",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037886.1.11",
          "form": "11~12시넘어서잔다",
          "original_form": "11~12시넘어서잔다",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037886.1.12",
          "form": "우째 생각하노",
          "original_form": "우째 생각하노",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037886.1.13",
          "form": "승질난다",
          "original_form": "승질난다",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037886.1.14",
          "form": "미소에서 활동량이 별로 없어서  그런가",
          "original_form": "미소에서 활동량이 별로 없어서  그런가",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037886.1.15",
          "form": "낮잠을 안자 야 되는거 아니야?",
          "original_form": "낮잠을 안자 야 되는거 아니야?",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037886.1.16",
          "form": "몰라ㅡㅡ진짜 짜증 ㅡㅡ",
          "original_form": "몰라ㅡㅡ진짜 짜증 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037886.1.17",
          "form": "낮잠안자기에는ㅜ",
          "original_form": "낮잠안자기에는ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037886.1.18",
          "form": "아직어리니..",
          "original_form": "아직어리니..",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037886.1.19",
          "form": "아 근데 맨날저카니깐",
          "original_form": "아 근데 맨날저카니깐",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037886.1.20",
          "form": "아 맞다 지오 세상 이제;",
          "original_form": "아 맞다 지오 세상 이제;",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037886.1.21",
          "form": "체력이 좋은 갑다ㅠ",
          "original_form": "체력이 좋은 갑다ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037886.1.22",
          "form": "오늘 비 와서 밖에서 못 놀아서 더 그런가",
          "original_form": "오늘 비 와서 밖에서 못 놀아서 더 그런가",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037886.1.23",
          "form": "그런가보다.",
          "original_form": "그런가보다.",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037886.1.24",
          "form": "집에서 에너지가 안빠지니ㅜ",
          "original_form": "집에서 에너지가 안빠지니ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037886.1.25",
          "form": "그래 나가서 좀 놀아야 되는데 그 자",
          "original_form": "그래 나가서 좀 놀아야 되는데 그 자",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037886.1.26",
          "form": "비 오는데 근데 어떡하노",
          "original_form": "비 오는데 근데 어떡하노",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037886.1.27",
          "form": "그러니깐ㅡㅡ 아효 답답네ㅋ",
          "original_form": "그러니깐ㅡㅡ 아효 답답네ㅋ",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037886.1.28",
          "form": "짜증나서 달래기도싫음",
          "original_form": "짜증나서 달래기도싫음",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037886.1.29",
          "form": "그 마음 잘 알지ㅠ",
          "original_form": "그 마음 잘 알지ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100037886.1.30",
          "form": "잠 안 자면 정말  힘들다 ㅠ",
          "original_form": "잠 안 자면 정말  힘들다 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037886.1.31",
          "form": "그러니깐ㅜ",
          "original_form": "그러니깐ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:40"
        }
      ]
    }
  ]
