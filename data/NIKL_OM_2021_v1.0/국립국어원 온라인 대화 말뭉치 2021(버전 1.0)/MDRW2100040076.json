      "utterance": [
        {
          "id": "MDRW2100040076.1.1",
          "form": "어 이제 곧 돌이라며",
          "original_form": "어 이제 곧 돌이라며",
          "speaker_id": "2",
          "time": "20210904 06:27"
        },
        {
          "id": "MDRW2100040076.1.2",
          "form": "얼마 전에 사진 봤는데 애기 이빨 났더라",
          "original_form": "얼마 전에 사진 봤는데 애기 이빨 났더라",
          "speaker_id": "1",
          "time": "20210904 06:28"
        },
        {
          "id": "MDRW2100040076.1.3",
          "form": "애들은 원래 금방 커",
          "original_form": "애들은 원래 금방 커",
          "speaker_id": "2",
          "time": "20210904 06:28"
        },
        {
          "id": "MDRW2100040076.1.4",
          "form": "너는 돌 되기 전에 8키로 넘었어",
          "original_form": "너는 돌 되기 전에 8키로 넘었어",
          "speaker_id": "2",
          "time": "20210904 06:28"
        },
        {
          "id": "MDRW2100040076.1.5",
          "form": "ㅋㅋㅋ애기 머리숱도 진짜 많구 넘 귀엽더라구",
          "original_form": "ㅋㅋㅋ애기 머리숱도 진짜 많구 넘 귀엽더라구",
          "speaker_id": "1",
          "time": "20210904 06:28"
        },
        {
          "id": "MDRW2100040076.1.6",
          "form": "이제 걸음마 하겠네",
          "original_form": "이제 걸음마 하겠네",
          "speaker_id": "2",
          "time": "20210904 06:29"
        },
        {
          "id": "MDRW2100040076.1.7",
          "form": "어 근데 요새 애기용 걷기 기구? 그런 거 장난 아니더라",
          "original_form": "어 근데 요새 애기용 걷기 기구? 그런 거 장난 아니더라",
          "speaker_id": "1",
          "time": "20210904 06:29"
        },
        {
          "id": "MDRW2100040076.1.8",
          "form": "완전 첨단 기술적이야ㅋㅋㅋㅋ",
          "original_form": "완전 첨단 기술적이야ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 06:29"
        },
        {
          "id": "MDRW2100040076.1.9",
          "form": "너희 키울 땐 보행기가 전부였는데",
          "original_form": "너희 키울 땐 보행기가 전부였는데",
          "speaker_id": "2",
          "time": "20210904 06:30"
        },
        {
          "id": "MDRW2100040076.1.10",
          "form": "요즘엔 다양한 육아용품들이 많더라고",
          "original_form": "요즘엔 다양한 육아용품들이 많더라고",
          "speaker_id": "2",
          "time": "20210904 06:31"
        },
        {
          "id": "MDRW2100040076.1.11",
          "form": "그치~ 친구네 애기도 지금 딱 돌아다니는 거 좋아한대",
          "original_form": "그치~ 친구네 애기도 지금 딱 돌아다니는 거 좋아한대",
          "speaker_id": "1",
          "time": "20210904 06:32"
        },
        {
          "id": "MDRW2100040076.1.12",
          "form": "호기심도 많고 ㅋㅋㅋㅋ",
          "original_form": "호기심도 많고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 06:32"
        },
        {
          "id": "MDRW2100040076.1.13",
          "form": "맞아 그때쯤이면 이제 낯도 가리기 시작해 애기들이",
          "original_form": "맞아 그때쯤이면 이제 낯도 가리기 시작해 애기들이",
          "speaker_id": "2",
          "time": "20210904 06:33"
        },
        {
          "id": "MDRW2100040076.1.14",
          "form": "그니까.. 애기는 혼자 소파 잡고 일어나서 엄마를 깜짝깜짝 놀라게 한대",
          "original_form": "그니까.. 애기는 혼자 소파 잡고 일어나서 엄마를 깜짝깜짝 놀라게 한대",
          "speaker_id": "1",
          "time": "20210904 06:34"
        },
        {
          "id": "MDRW2100040076.1.15",
          "form": "여기저기 많이 걸어야 근육 발달도 되고 좋아 아기한테도",
          "original_form": "여기저기 많이 걸어야 근육 발달도 되고 좋아 아기한테도",
          "speaker_id": "2",
          "time": "20210904 06:36"
        },
        {
          "id": "MDRW2100040076.1.16",
          "form": "아직 어린이집도 안 가고 하니까 집에서 많이 걷게 하나봐",
          "original_form": "아직 어린이집도 안 가고 하니까 집에서 많이 걷게 하나봐",
          "speaker_id": "1",
          "time": "20210904 06:38"
        },
        {
          "id": "MDRW2100040076.1.17",
          "form": "애기 잘 키우고 있네. 그때는 뭘해도 애기가 반짝반짝 예쁠 때야.",
          "original_form": "애기 잘 키우고 있네. 그때는 뭘해도 애기가 반짝반짝 예쁠 때야.",
          "speaker_id": "2",
          "time": "20210904 06:39"
        }
      ]
