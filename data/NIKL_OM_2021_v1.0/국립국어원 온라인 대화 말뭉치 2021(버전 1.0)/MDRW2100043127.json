      "utterance": [
        {
          "id": "MDRW2100043127.1.1",
          "form": "샌드위치요!",
          "original_form": "샌드위치요!",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043127.1.2",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043127.1.3",
          "form": "점심에 감자채볶음이랑 가지볶음 해서 먹었어요",
          "original_form": "점심에 감자채볶음이랑 가지볶음 해서 먹었어요",
          "speaker_id": "1",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100043127.1.4",
          "form": "완전 건강식이네요! 집밥",
          "original_form": "완전 건강식이네요! 집밥",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043127.1.5",
          "form": "네 제가 집밥을 좋아하죠",
          "original_form": "네 제가 집밥을 좋아하죠",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043127.1.6",
          "form": "ㅋㅋ가장 평범한게 베스트인거같아요",
          "original_form": "ㅋㅋ가장 평범한게 베스트인거같아요",
          "speaker_id": "2",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100043127.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100043127.1.8",
          "form": "집에서 이런 저런 요리하는 걸 좋아하다보니",
          "original_form": "집에서 이런 저런 요리하는 걸 좋아하다보니",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100043127.1.9",
          "form": "밖에선 잘 안사먹게 되더라구요",
          "original_form": "밖에선 잘 안사먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100043127.1.10",
          "form": "집밥 먹어야 속이 편해요",
          "original_form": "집밥 먹어야 속이 편해요",
          "speaker_id": "2",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100043127.1.11",
          "form": "집밥이 속이 편해요 진짜",
          "original_form": "집밥이 속이 편해요 진짜",
          "speaker_id": "1",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100043127.1.12",
          "form": "저는 또 직접 요리하니까 딱 내 입맛에 맞게 할 수도 있고요",
          "original_form": "저는 또 직접 요리하니까 딱 내 입맛에 맞게 할 수도 있고요",
          "speaker_id": "1",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100043127.1.13",
          "form": "맞아요 그리고 물리지않아요!!!!",
          "original_form": "맞아요 그리고 물리지않아요!!!!",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043127.1.14",
          "form": "집김밥먹고싶네요",
          "original_form": "집김밥먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043127.1.15",
          "form": "만두랑",
          "original_form": "만두랑",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043127.1.16",
          "form": "김밥은 제대로 만들려고 하면 손이 정말 많이 가는데",
          "original_form": "김밥은 제대로 만들려고 하면 손이 정말 많이 가는데",
          "speaker_id": "1",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100043127.1.17",
          "form": "이번에 식중독 사태 터진거 보니까",
          "original_form": "이번에 식중독 사태 터진거 보니까",
          "speaker_id": "1",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100043127.1.18",
          "form": "김밥도 집에서 해먹어야겠단 생각이 들더라구요",
          "original_form": "김밥도 집에서 해먹어야겠단 생각이 들더라구요",
          "speaker_id": "1",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100043127.1.19",
          "form": "집김밥 진짜 무제한으로 먹을 수 있어요ㅋㅋ",
          "original_form": "집김밥 진짜 무제한으로 먹을 수 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043127.1.20",
          "form": "만두도 힘든데 너무 맛있어요!",
          "original_form": "만두도 힘든데 너무 맛있어요!",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043127.1.21",
          "form": "만두는 묵은지 많을 때 시도하는데 가족들을 다 동원해야 해요",
          "original_form": "만두는 묵은지 많을 때 시도하는데 가족들을 다 동원해야 해요",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043127.1.22",
          "form": "반죽하고 밀고 싸고 찌고",
          "original_form": "반죽하고 밀고 싸고 찌고",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043127.1.23",
          "form": "진짜 힘든데 진짜 물리지않고 속도 편해요!",
          "original_form": "진짜 힘든데 진짜 물리지않고 속도 편해요!",
          "speaker_id": "2",
          "time": "20210907 15:18"
        }
      ]
    }
