      "utterance": [
        {
          "id": "MDRW2100042815.1.1",
          "form": "원래는 본가에 같이 살다가",
          "original_form": "원래는 본가에 같이 살다가",
          "speaker_id": "2",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042815.1.2",
          "form": "혼자 내려가신 거예요??",
          "original_form": "혼자 내려가신 거예요??",
          "speaker_id": "2",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042815.1.3",
          "form": "육지에서 온거요",
          "original_form": "육지에서 온거요",
          "speaker_id": "1",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042815.1.4",
          "form": "아뇨 서울살아가요",
          "original_form": "아뇨 서울살아가요",
          "speaker_id": "1",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042815.1.5",
          "form": "서울선 여자셋이 같이 살았구요",
          "original_form": "서울선 여자셋이 같이 살았구요",
          "speaker_id": "1",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042815.1.6",
          "form": "아 그랬군요",
          "original_form": "아 그랬군요",
          "speaker_id": "2",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042815.1.7",
          "form": "맞아요 제주는",
          "original_form": "맞아요 제주는",
          "speaker_id": "2",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042815.1.8",
          "form": "싱글이고 결혼생각도 없어서",
          "original_form": "싱글이고 결혼생각도 없어서",
          "speaker_id": "1",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042815.1.9",
          "form": "다 비싸더라고요",
          "original_form": "다 비싸더라고요",
          "speaker_id": "2",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042815.1.10",
          "form": "네 배송료더 많이.나오고요",
          "original_form": "네 배송료더 많이.나오고요",
          "speaker_id": "1",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042815.1.11",
          "form": "혼자 오롯이 즐길 수 있는 시간이겠어요",
          "original_form": "혼자 오롯이 즐길 수 있는 시간이겠어요",
          "speaker_id": "2",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042815.1.12",
          "form": "네 어색하기도 해요",
          "original_form": "네 어색하기도 해요",
          "speaker_id": "1",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042815.1.13",
          "form": "아기는 몇살이예요",
          "original_form": "아기는 몇살이예요",
          "speaker_id": "1",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042815.1.14",
          "form": "여덟살 다섯 살 두 명입니다",
          "original_form": "여덟살 다섯 살 두 명입니다",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042815.1.15",
          "form": "귀엽겠어요",
          "original_form": "귀엽겠어요",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042815.1.16",
          "form": "네 아직은 귀여울 나이라 ㅎㅎ",
          "original_form": "네 아직은 귀여울 나이라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042815.1.17",
          "form": "아기가 있으면 활기가 있죠 집안이",
          "original_form": "아기가 있으면 활기가 있죠 집안이",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042815.1.18",
          "form": "밥도 거의 해주시나요",
          "original_form": "밥도 거의 해주시나요",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042815.1.19",
          "form": "아무래도  조용할 시간이 없죠",
          "original_form": "아무래도  조용할 시간이 없죠",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042815.1.20",
          "form": "제가 일을 해서 웬만하면 해주려고 하는데",
          "original_form": "제가 일을 해서 웬만하면 해주려고 하는데",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042815.1.21",
          "form": "국은 제가 끓이고",
          "original_form": "국은 제가 끓이고",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042815.1.22",
          "form": "반찬은 종종 사 먹는 편이에요",
          "original_form": "반찬은 종종 사 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042815.1.23",
          "form": "아 애기있으면 유기농 많이 드시던데",
          "original_form": "아 애기있으면 유기농 많이 드시던데",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042815.1.24",
          "form": "식재료는 자연드림에서 사 먹는 편이에요",
          "original_form": "식재료는 자연드림에서 사 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042815.1.25",
          "form": "여자아이가 있어서",
          "original_form": "여자아이가 있어서",
          "speaker_id": "2",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042815.1.26",
          "form": "아 그렇군",
          "original_form": "아 그렇군",
          "speaker_id": "1",
          "time": "20210907 09:39"
        }
      ]
    }
  ]
