      "utterance": [
        {
          "id": "MDRW2100039576.1.1",
          "form": "와 예상하지 못했던 나라야",
          "original_form": "와 예상하지 못했던 나라야",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039576.1.2",
          "form": "또 가보고 싶어요!",
          "original_form": "또 가보고 싶어요!",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039576.1.3",
          "form": "헝가리 여행갔던 이야기 처음 들어!",
          "original_form": "헝가리 여행갔던 이야기 처음 들어!",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039576.1.4",
          "form": "언제 갔었어?",
          "original_form": "언제 갔었어?",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039576.1.5",
          "form": "아 예전에 갔었어용",
          "original_form": "아 예전에 갔었어용",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039576.1.6",
          "form": "17년도였나?",
          "original_form": "17년도였나?",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039576.1.7",
          "form": "우왕 좋을 때 갔었다ㅠㅠ",
          "original_form": "우왕 좋을 때 갔었다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039576.1.8",
          "form": "몇 박으로 갔다왔어??",
          "original_form": "몇 박으로 갔다왔어??",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039576.1.9",
          "form": "그쵸ㅠㅠ 완전 체력 좋을때...ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅠㅠ 완전 체력 좋을때...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039576.1.10",
          "form": "ㅋㅋㅋㅋ지금도 충분히 젊어~",
          "original_form": "ㅋㅋㅋㅋ지금도 충분히 젊어~",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039576.1.11",
          "form": "헝가리는 하루인가 있었고",
          "original_form": "헝가리는 하루인가 있었고",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039576.1.12",
          "form": "다른 유럽도 돌아다녔었어요!",
          "original_form": "다른 유럽도 돌아다녔었어요!",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039576.1.13",
          "form": "오 유럽 여행으로 꽤 길게 갔다왔구나?!",
          "original_form": "오 유럽 여행으로 꽤 길게 갔다왔구나?!",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039576.1.14",
          "form": "맞아요 지금도 아직 체력이 남아있을 때라 언넝 돌아댕겨야해요ㅜㅜ",
          "original_form": "맞아요 지금도 아직 체력이 남아있을 때라 언넝 돌아댕겨야해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039576.1.15",
          "form": "맞아... 확실히 20대일 때가 체력이",
          "original_form": "맞아... 확실히 20대일 때가 체력이",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039576.1.16",
          "form": "네네 한 5일 정도?",
          "original_form": "네네 한 5일 정도?",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039576.1.17",
          "form": "짱짱해... 달라...",
          "original_form": "짱짱해... 달라...",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039576.1.18",
          "form": "맞아요ㅠㅠ 흑흑",
          "original_form": "맞아요ㅠㅠ 흑흑",
          "speaker_id": "1",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039576.1.19",
          "form": "우와 너무 좋았겠다ㅠㅠ",
          "original_form": "우와 너무 좋았겠다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039576.1.20",
          "form": "넘 좋았죠... 추천이에요...",
          "original_form": "넘 좋았죠... 추천이에요...",
          "speaker_id": "1",
          "time": "20210903 19:17"
        }
      ]
