      "utterance": [
        {
          "id": "MDRW2100011656.1.1",
          "form": "비오나?",
          "original_form": "비오나?",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011656.1.2",
          "form": "대구에 비엄청내린다",
          "original_form": "대구에 비엄청내린다",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011656.1.3",
          "form": "여기도 비온다",
          "original_form": "여기도 비온다",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011656.1.4",
          "form": "많이내리나",
          "original_form": "많이내리나",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011656.1.5",
          "form": "좀 많이",
          "original_form": "좀 많이",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011656.1.6",
          "form": "비많이오니 깨끗해지는기분",
          "original_form": "비많이오니 깨끗해지는기분",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011656.1.7",
          "form": "찝찝한데",
          "original_form": "찝찝한데",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011656.1.8",
          "form": "이제 더운여름은 지나간거같아 좋은데",
          "original_form": "이제 더운여름은 지나간거같아 좋은데",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011656.1.9",
          "form": "비오는거 좋아해?",
          "original_form": "비오는거 좋아해?",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011656.1.10",
          "form": "코로나때는 좋드라",
          "original_form": "코로나때는 좋드라",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011656.1.11",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011656.1.12",
          "form": "날씨좋으면 더짜쯩",
          "original_form": "날씨좋으면 더짜쯩",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011656.1.13",
          "form": "더 짜증난다",
          "original_form": "더 짜증난다",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011656.1.14",
          "form": "맞나~ㅋㅋ",
          "original_form": "맞나~ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011656.1.15",
          "form": "많이 못나가자잖아",
          "original_form": "많이 못나가자잖아",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011656.1.16",
          "form": "난 초여름이 좋은데",
          "original_form": "난 초여름이 좋은데",
          "speaker_id": "1",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011656.1.17",
          "form": "갑자기 더워졌다ㅜ비 안오니",
          "original_form": "갑자기 더워졌다ㅜ비 안오니",
