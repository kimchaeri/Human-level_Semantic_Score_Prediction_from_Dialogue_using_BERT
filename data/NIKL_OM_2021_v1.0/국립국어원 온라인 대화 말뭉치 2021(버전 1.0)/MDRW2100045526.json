      "utterance": [
        {
          "id": "MDRW2100045526.1.1",
          "form": "색깔에 너무 구애받지 마세여!",
          "original_form": "색깔에 너무 구애받지 마세여!",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100045526.1.2",
          "form": "까만 비닐에도 넣어서 팔아요",
          "original_form": "까만 비닐에도 넣어서 팔아요",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100045526.1.3",
          "form": "진짜 감자는 아니죠..?",
          "original_form": "진짜 감자는 아니죠..?",
          "speaker_id": "1",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100045526.1.4",
          "form": "감자의 귀환",
          "original_form": "감자의 귀환",
          "speaker_id": "1",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100045526.1.5",
          "form": "아뇨 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100045526.1.6",
          "form": "세글자예요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "세글자예요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100045526.1.7",
          "form": "세글자 야채!",
          "original_form": "세글자 야채!",
          "speaker_id": "2",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100045526.1.8",
          "form": "재배하기 쉽고 국끓여먹능거!",
          "original_form": "재배하기 쉽고 국끓여먹능거!",
          "speaker_id": "2",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100045526.1.9",
          "form": "탄수화물 종류인가요???",
          "original_form": "탄수화물 종류인가요???",
          "speaker_id": "1",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100045526.1.10",
          "form": "아뇨 섬유질과 수분입니다!",
          "original_form": "아뇨 섬유질과 수분입니다!",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100045526.1.11",
          "form": "노란색도 섞여있어요",
          "original_form": "노란색도 섞여있어요",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100045526.1.12",
          "form": "으으으으으으으음",
          "original_form": "으으으으으으으음",
          "speaker_id": "1",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045526.1.13",
          "form": "전으로도 부쳐먹나요?",
          "original_form": "전으로도 부쳐먹나요?",
          "speaker_id": "1",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045526.1.14",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045526.1.15",
          "form": "국끓이거나 무쳐먹어요",
          "original_form": "국끓이거나 무쳐먹어요",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045526.1.16",
          "form": "아 혹시",
          "original_form": "아 혹시",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045526.1.17",
          "form": "콩나물????",
          "original_form": "콩나물????",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045526.1.18",
          "form": "와 머리속이 탁 트이는 느낌",
          "original_form": "와 머리속이 탁 트이는 느낌",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045526.1.19",
          "form": "맞죠 콩나물!!",
          "original_form": "맞죠 콩나물!!",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045526.1.20",
          "form": "네 맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045526.1.21",
          "form": "종류가 좀 애매하긴 했어요 ㅋㅋㅋ큐ㅠㅠ",
          "original_form": "종류가 좀 애매하긴 했어요 ㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045526.1.22",
          "form": "ㅌㅋㅋㅋㅋ아 그래서 뿌리채소인가 긴가민가 하셨구나",
          "original_form": "ㅌㅋㅋㅋㅋ아 그래서 뿌리채소인가 긴가민가 하셨구나",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045526.1.23",
          "form": "그럴만 해요 진짜 ㅋㅋ",
          "original_form": "그럴만 해요 진짜 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045526.1.24",
          "form": "네 ㅋㅋㅋㅋ 뿌리는 뿌리니까요 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ 뿌리는 뿌리니까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045526.1.25",
          "form": "ㅌㅋㅋㅋㅋㅋㅋ광장히 하찮은 뿌리",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋ광장히 하찮은 뿌리",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045526.1.26",
          "form": "그쵸 ㅋㅋㅋㅋ 딱히 제철이랄 것도 없고....",
          "original_form": "그쵸 ㅋㅋㅋㅋ 딱히 제철이랄 것도 없고....",
