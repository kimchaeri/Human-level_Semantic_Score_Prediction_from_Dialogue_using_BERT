      "utterance": [
        {
          "id": "MDRW2100003322.1.1",
          "form": "맛집 아는 곳 있어?",
          "original_form": "맛집 아는 곳 있어?",
          "speaker_id": "1",
          "time": "20210820 21:46"
        },
        {
          "id": "MDRW2100003322.1.2",
          "form": "음 많ㅇㅣ 가보긴 했는데 막 엄청난 곳은 못본 것 같아",
          "original_form": "음 많ㅇㅣ 가보긴 했는데 막 엄청난 곳은 못본 것 같아",
          "speaker_id": "2",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003322.1.3",
          "form": "나도 그때그때 먹고 싶은 것 찾아가니까",
          "original_form": "나도 그때그때 먹고 싶은 것 찾아가니까",
          "speaker_id": "1",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003322.1.4",
          "form": "나는 내 친구가 맛집 많이 알아",
          "original_form": "나는 내 친구가 맛집 많이 알아",
          "speaker_id": "2",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003322.1.5",
          "form": "오 좋겠네",
          "original_form": "오 좋겠네",
          "speaker_id": "1",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003322.1.6",
          "form": "여기저기 데리고 가주니까 편해",
          "original_form": "여기저기 데리고 가주니까 편해",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003322.1.7",
          "form": "아 메뉴 정하기 힘들어",
          "original_form": "아 메뉴 정하기 힘들어",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003322.1.8",
          "form": "그니까ㅋㅋㅋ",
          "original_form": "그니까ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003322.1.9",
          "form": "그냥 주변에서 유명하다고 하면 한 번씩 가보는 중",
          "original_form": "그냥 주변에서 유명하다고 하면 한 번씩 가보는 중",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003322.1.10",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003322.1.11",
          "form": "요즘 맛집 많은 거 같지?",
          "original_form": "요즘 맛집 많은 거 같지?",
          "speaker_id": "1",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003322.1.12",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003322.1.13",
          "form": "세상에 먹을 건 많다",
          "original_form": "세상에 먹을 건 많다",
          "speaker_id": "1",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003322.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아",
          "speaker_id": "2",
