      "utterance": [
        {
          "id": "MDRW2100008287.1.1",
          "form": "유플러스 넷플릭스 보세요?",
          "original_form": "유플러스 넷플릭스 보세요?",
          "speaker_id": "1",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008287.1.2",
          "form": "웨이브 봐용",
          "original_form": "웨이브 봐용",
          "speaker_id": "2",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008287.1.3",
          "form": "넷플은 네명이 깨져서 ㅋㅋ 그이후 결제 안했네여",
          "original_form": "넷플은 네명이 깨져서 ㅋㅋ 그이후 결제 안했네여",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008287.1.4",
          "form": "아 전 3명이 해요",
          "original_form": "아 전 3명이 해요",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008287.1.5",
          "form": "깨짐 난감해요",
          "original_form": "깨짐 난감해요",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008287.1.6",
          "form": "웨이브 전 예전에 애니 보려고 봤었어요",
          "original_form": "웨이브 전 예전에 애니 보려고 봤었어요",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008287.1.7",
          "form": "뭐보세요 ㅎㅎ?",
          "original_form": "뭐보세요 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008287.1.8",
          "form": "거긴 티비 프로 재방송 많이 볼수있져?",
          "original_form": "거긴 티비 프로 재방송 많이 볼수있져?",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008287.1.9",
          "form": "전 요새 예전드라마가 가끔 보고싶어요",
          "original_form": "전 요새 예전드라마가 가끔 보고싶어요",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008287.1.10",
          "form": "예전에 유투브에서 볼수있었는데 사라졌더아구요",
          "original_form": "예전에 유투브에서 볼수있었는데 사라졌더아구요",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008287.1.11",
          "form": "전 요새 웨이브 영드 쪽에 푹 빠졌어요",
          "original_form": "전 요새 웨이브 영드 쪽에 푹 빠졌어요",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008287.1.12",
          "form": "진짜 하루가 순삭됨ㄷㄷ",
          "original_form": "진짜 하루가 순삭됨ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008287.1.13",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008287.1.14",
          "form": "시리즈 보다보면 하루 금방가요",
          "original_form": "시리즈 보다보면 하루 금방가요",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008287.1.15",
          "form": "전 넷플 왓쳐가 기다려져요",
          "original_form": "전 넷플 왓쳐가 기다려져요",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008287.1.16",
          "form": "12월에 다시한데요 ㅜㅜ",
          "original_form": "12월에 다시한데요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008287.1.17",
          "form": "아 넘 멀었네요",
          "original_form": "아 넘 멀었네요",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008287.1.18",
          "form": "넷플은 그 킹덤인가 한국좀비나오는게 유명한거같던데 보셨어요?",
          "original_form": "넷플은 그 킹덤인가 한국좀비나오는게 유명한거같던데 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008287.1.19",
          "form": "다봤어요",
          "original_form": "다봤어요",
          "speaker_id": "1",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008287.1.20",
          "form": "아신전 까지요",
          "original_form": "아신전 까지요",
          "speaker_id": "1",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008287.1.21",
          "form": "전 스위트홈도 재밌더라구여",
          "original_form": "전 스위트홈도 재밌더라구여",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008287.1.22",
          "form": "넷플 에어리어니스트도 잼나요",
          "original_form": "넷플 에어리어니스트도 잼나요",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008287.1.23",
          "form": "에엉",
          "original_form": "에엉",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008287.1.24",
          "form": "에얼",
          "original_form": "에얼",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008287.1.25",
          "form": "저는 유투브에서 정리해놓은거라도 보면서 아쉬움 달래여 ㅋㅋ",
          "original_form": "저는 유투브에서 정리해놓은거라도 보면서 아쉬움 달래여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008287.1.26",
          "form": "네 사실 별거ㅠ없어요 ㅜㅜ",
          "original_form": "네 사실 별거ㅠ없어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008287.1.27",
          "form": "왓챠 2주 공짜예요",
          "original_form": "왓챠 2주 공짜예요",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008287.1.28",
          "form": "이마트24팝콘사면 이용권도 줘요",
          "original_form": "이마트24팝콘사면 이용권도 줘요",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008287.1.29",
          "form": "그래서 왓 챠도 보고있어요",
          "original_form": "그래서 왓 챠도 보고있어요",
          "speaker_id": "1",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100008287.1.30",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008287.1.31",
          "form": "사실 하루종일 폰만잡고있어서ㅜ",
          "original_form": "사실 하루종일 폰만잡고있어서ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008287.1.32",
          "form": "줄여야겠단 생각도 들더라고요",
          "original_form": "줄여야겠단 생각도 들더라고요",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008287.1.33",
          "form": "멎아요",
          "original_form": "멎아요",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008287.1.34",
          "form": "저도요 ㅜㅜ",
          "original_form": "저도요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008287.1.35",
          "form": "근데 잘안돼요 ㅜㅜ",
          "original_form": "근데 잘안돼요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008287.1.36",
          "form": "지금도 이렇게 하루종일 하네여",
          "original_form": "지금도 이렇게 하루종일 하네여",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008287.1.37",
          "form": "시리즈는 보면 하루종일보고",
          "original_form": "시리즈는 보면 하루종일보고",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008287.1.38",
          "form": "뭔가 시리즈는 한번에 못끝내면 잠도안오더라고요",
          "original_form": "뭔가 시리즈는 한번에 못끝내면 잠도안오더라고요",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008287.1.39",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008287.1.40",
          "form": "저 진짜 잠많은데 ㅋㅋ",
          "original_form": "저 진짜 잠많은데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008287.1.41",
          "form": "드라마는 다보고 딱 그때되면졸림 ㅋㅋ",
          "original_form": "드라마는 다보고 딱 그때되면졸림 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008287.1.42",
          "form": "공부를 이만큼했으면 하버드갔을듯",
          "original_form": "공부를 이만큼했으면 하버드갔을듯",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008287.1.43",
          "form": "ㅋㅋ좋은건 원래",
          "original_form": "ㅋㅋ좋은건 원래",
          "speaker_id": "1",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008287.1.44",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008287.1.45",
          "form": "잠도 안와요",
          "original_form": "잠도 안와요",
          "speaker_id": "1",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008287.1.46",
          "form": "할때",
          "original_form": "할때",
          "speaker_id": "1",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008287.1.47",
          "form": "전 스릴러도 좋고 판타지도 좋아요",
          "original_form": "전 스릴러도 좋고 판타지도 좋아요",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008287.1.48",
          "form": "카카오페이지도 봐요",
          "original_form": "카카오페이지도 봐요",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008287.1.49",
          "form": "웹툰 이벤에서 것도 응모하고요",
          "original_form": "웹툰 이벤에서 것도 응모하고요",
          "speaker_id": "1",
