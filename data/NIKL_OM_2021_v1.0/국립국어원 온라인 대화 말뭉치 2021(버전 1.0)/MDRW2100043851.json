      "utterance": [
        {
          "id": "MDRW2100043851.1.1",
          "form": "가고 싶은데 너무 멀고 가게가 너무 바빠서 포기입니당..ㅠㅋㅋㅋ 과자 뭐 좋아하세요",
          "original_form": "가고 싶은데 너무 멀고 가게가 너무 바빠서 포기입니당..ㅠㅋㅋㅋ 과자 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043851.1.2",
          "form": "과자 저 감자칩 제일 좋아하고",
          "original_form": "과자 저 감자칩 제일 좋아하고",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043851.1.3",
          "form": "과자 안가려요 ㅋㅋㅋㅋㅋ",
          "original_form": "과자 안가려요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043851.1.4",
          "form": "저 여기서 추천받은거 알려드릴까요",
          "original_form": "저 여기서 추천받은거 알려드릴까요",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043851.1.5",
          "form": "뭐 추천받으셨어요??",
          "original_form": "뭐 추천받으셨어요??",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043851.1.6",
          "form": "콰삭칩이요!!!!",
          "original_form": "콰삭칩이요!!!!",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043851.1.7",
          "form": "제가 그래서 먹어봤거든요??",
          "original_form": "제가 그래서 먹어봤거든요??",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043851.1.8",
          "form": "쫀맛",
          "original_form": "쫀맛",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043851.1.9",
          "form": "트러플 뭐시기랑 어니언 뭐시기 두개 있었는데",
          "original_form": "트러플 뭐시기랑 어니언 뭐시기 두개 있었는데",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043851.1.10",
          "form": "몰라서 둘다 샀는데 둘다 맛있어요",
          "original_form": "몰라서 둘다 샀는데 둘다 맛있어요",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043851.1.11",
          "form": "아 저 먹어봤어요!!",
          "original_form": "아 저 먹어봤어요!!",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043851.1.12",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043851.1.13",
          "form": "트러플!!",
          "original_form": "트러플!!",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043851.1.14",
          "form": "할!!!",
          "original_form": "할!!!",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043851.1.15",
          "form": "이미 드셔보셨군요",
          "original_form": "이미 드셔보셨군요",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043851.1.16",
          "form": "맛있져!!",
          "original_form": "맛있져!!",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043851.1.17",
          "form": "오징어땅콩 이마트에서만 파는",
          "original_form": "오징어땅콩 이마트에서만 파는",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043851.1.18",
          "form": "치즈매콤맛인가 그거도 나름 맛있더라고요",
          "original_form": "치즈매콤맛인가 그거도 나름 맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043851.1.19",
          "form": "므야!! 저 그런 맛있는거 못봤는데!!!!",
          "original_form": "므야!! 저 그런 맛있는거 못봤는데!!!!",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043851.1.20",
          "form": "이름만으로도 맛있는데요!",
          "original_form": "이름만으로도 맛있는데요!",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043851.1.21",
          "form": "그거 맛있어요 맥주랑 먹으면 딱일거 같았어요",
          "original_form": "그거 맛있어요 맥주랑 먹으면 딱일거 같았어요",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043851.1.22",
          "form": "술을 안마시지만 저는 안주가 좋으니까 보이면 살게요!",
          "original_form": "술을 안마시지만 저는 안주가 좋으니까 보이면 살게요!",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043851.1.23",
          "form": "하 맛있겠다",
          "original_form": "하 맛있겠다",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043851.1.24",
          "form": "저도 술을 잘 안하는데 그냥 먹어도 맛있었어요!!",
          "original_form": "저도 술을 잘 안하는데 그냥 먹어도 맛있었어요!!",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043851.1.25",
          "form": "name2님 갑작스럽지먼",
          "original_form": "&name2&님 갑작스럽지먼",
