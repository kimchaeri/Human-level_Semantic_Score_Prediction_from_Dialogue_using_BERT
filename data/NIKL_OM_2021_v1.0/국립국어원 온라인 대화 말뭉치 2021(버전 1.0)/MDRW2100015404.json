      "utterance": [
        {
          "id": "MDRW2100015404.1.1",
          "form": "푸라닭 투움바 뭐시기도 맛있더라",
          "original_form": "푸라닭 투움바 뭐시기도 맛있더라",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015404.1.2",
          "form": "난 근데 갠적으론 허니콤보가 제일 맛있어",
          "original_form": "난 근데 갠적으론 허니콤보가 제일 맛있어",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015404.1.3",
          "form": "푸라닭은 대표메뉴 뭐지 그거 한번먹고 안먹었어ㅋㅋㅋ",
          "original_form": "푸라닭은 대표메뉴 뭐지 그거 한번먹고 안먹었어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015404.1.4",
          "form": "나한테 치킨은 술안주라서",
          "original_form": "나한테 치킨은 술안주라서",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015404.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ아하",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아하",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015404.1.6",
          "form": "난 치킨 무조건 순살 양많고 맛있는거",
          "original_form": "난 치킨 무조건 순살 양많고 맛있는거",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015404.1.7",
          "form": "푸라닭 최고야",
          "original_form": "푸라닭 최고야",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015404.1.8",
          "form": "파닭 먹어본적 있어?",
          "original_form": "파닭 먹어본적 있어?",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015404.1.9",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015404.1.10",
          "form": "파닭도 맛있는데 넘 매운 파 걸리면 코 찡해",
          "original_form": "파닭도 맛있는데 넘 매운 파 걸리면 코 찡해",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015404.1.11",
          "form": "파닭이랑 쏘주랑 먹으면",
          "original_form": "파닭이랑 쏘주랑 먹으면",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015404.1.12",
          "form": "진짜 술술 들어가는데",
          "original_form": "진짜 술술 들어가는데",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015404.1.13",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015404.1.14",
          "form": "아쉽게도 난 술찌라",
          "original_form": "아쉽게도 난 술찌라",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015404.1.15",
          "form": "소주는 껍떼기랑",
          "original_form": "소주는 껍떼기랑",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015404.1.16",
          "form": "먹어야지",
          "original_form": "먹어야지",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015404.1.17",
          "form": "오... 으르신입맛",
          "original_form": "오... 으르신입맛",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015404.1.18",
          "form": "ㅋㄱㅋㄱㅋㄱㅋ혹시 껍떼기 안먹어..?",
          "original_form": "ㅋㄱㅋㄱㅋㄱㅋ혹시 껍떼기 안먹어..?",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015404.1.19",
          "form": "가리는 음식은 없는데",
          "original_form": "가리는 음식은 없는데",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015404.1.20",
          "form": "자주는 안먹어ㅋㅋㅋ",
          "original_form": "자주는 안먹어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015404.1.21",
          "form": "아휴 나도 자주는 안먹어ㅋㅋㅋ",
          "original_form": "아휴 나도 자주는 안먹어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015404.1.22",
          "form": "가끔 생각날때 먹어야 맛있다구",
          "original_form": "가끔 생각날때 먹어야 맛있다구",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015404.1.23",
          "form": "닭발은 어때",
          "original_form": "닭발은 어때",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015404.1.24",
          "form": "나는 닭발 뼈 없는것만 먹어",
          "original_form": "나는 닭발 뼈 없는것만 먹어",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015404.1.25",
          "form": "뼈 있는거 안먹어봤어",
          "original_form": "뼈 있는거 안먹어봤어",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015404.1.26",
