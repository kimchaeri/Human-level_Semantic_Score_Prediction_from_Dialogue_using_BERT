      "utterance": [
        {
          "id": "MDRW2100047517.1.1",
          "form": "나는 항상 테이크아웃함",
          "original_form": "나는 항상 테이크아웃함",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100047517.1.2",
          "form": "매장 안들어가고",
          "original_form": "매장 안들어가고",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100047517.1.3",
          "form": "카드 안들고 다녀서 심지어 매번 계좌이체해줌",
          "original_form": "카드 안들고 다녀서 심지어 매번 계좌이체해줌",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100047517.1.4",
          "form": "더좋지 사장님은",
          "original_form": "더좋지 사장님은",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100047517.1.5",
          "form": "근데 내가 그렇게 많이 사먹었는데",
          "original_form": "근데 내가 그렇게 많이 사먹었는데",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.6",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.7",
          "form": "단 한번도 쿠폰 안찍어줌",
          "original_form": "단 한번도 쿠폰 안찍어줌",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.8",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.9",
          "form": "쿠폰 찍어드릴까요",
          "original_form": "쿠폰 찍어드릴까요",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.10",
          "form": "물어보지도않음",
          "original_form": "물어보지도않음",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.11",
          "form": "1음료 1쿠폰이자나",
          "original_form": "1음료 1쿠폰이자나",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.12",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.13",
          "form": "나는 늘 도로 쪽에서 테이크아웃해서",
          "original_form": "나는 늘 도로 쪽에서 테이크아웃해서",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.14",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.15",
          "form": "그집 쿠폰없을거라생각함",
          "original_form": "그집 쿠폰없을거라생각함",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.16",
          "form": "도장이 읍나",
          "original_form": "도장이 읍나",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047517.1.17",
          "form": "1리터 아아가 1500임",
          "original_form": "1리터 아아가 1500임",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047517.1.18",
          "form": "니가 달라해봄?",
          "original_form": "니가 달라해봄?",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047517.1.19",
          "form": "싸네",
          "original_form": "싸네",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047517.1.20",
          "form": "아 너무저렴하니 쿠폰없구나라고 생각했던듭",
          "original_form": "아 너무저렴하니 쿠폰없구나라고 생각했던듭",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047517.1.21",
          "form": "나혼자그냥",
          "original_form": "나혼자그냥",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047517.1.22",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047517.1.23",
          "form": "권한적도없고해서 물어보지도않음",
          "original_form": "권한적도없고해서 물어보지도않음",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047517.1.24",
          "form": "이제 챙기믄안댐?",
          "original_form": "이제 챙기믄안댐?",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047517.1.25",
          "form": "아아는 쿠폰안주나",
          "original_form": "아아는 쿠폰안주나",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047517.1.26",
          "form": "근데 어느날 누가 쿠폰으로 공짜음료 먹는거봄",
          "original_form": "근데 어느날 누가 쿠폰으로 공짜음료 먹는거봄",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047517.1.27",
          "form": "아아를?",
          "original_form": "아아를?",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047517.1.28",
          "form": "나는 맨날그집에서 비싼거 먹었음 3900버블티나 프라페나",
          "original_form": "나는 맨날그집에서 비싼거 먹었음 3900버블티나 프라페나",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047517.1.29",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047517.1.30",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047517.1.31",
          "form": "아아를",
          "original_form": "아아를",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047517.1.32",
          "form": "오잉!",
          "original_form": "오잉!",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047517.1.33",
          "form": "와 갑자기 졸라배신감 느낌",
          "original_form": "와 갑자기 졸라배신감 느낌",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047517.1.34",
          "form": "주야징ㅜㅜ",
          "original_form": "주야징ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047517.1.35",
          "form": "사장님ㅜㅜㅜ",
          "original_form": "사장님ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047517.1.36",
          "form": "내가 그마이 팔아줬는데",
          "original_form": "내가 그마이 팔아줬는데",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047517.1.37",
          "form": "섭섭하오",
          "original_form": "섭섭하오",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047517.1.38",
          "form": "그것도 그쪽계좌에 직접 꽂아줬음 수수료없게",
          "original_form": "그것도 그쪽계좌에 직접 꽂아줬음 수수료없게",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047517.1.39",
          "form": "에잉",
          "original_form": "에잉",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047517.1.40",
          "form": "그리고 젤싼1500아아도 도장 찍어주는데",
          "original_form": "그리고 젤싼1500아아도 도장 찍어주는데",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047517.1.41",
          "form": "난 맨날 비싼거 먹었고",
          "original_form": "난 맨날 비싼거 먹었고",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047517.1.42",
          "form": "다른가게읍나",
          "original_form": "다른가게읍나",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047517.1.43",
          "form": "하루 두잔 사먹은적도 있었음",
          "original_form": "하루 두잔 사먹은적도 있었음",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047517.1.44",
          "form": "배신감에 초부들부들했음",
          "original_form": "배신감에 초부들부들했음",
          "speaker_id": "2",
