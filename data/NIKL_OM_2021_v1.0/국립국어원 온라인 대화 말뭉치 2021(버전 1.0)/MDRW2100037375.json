        },
        {
          "id": "MDRW2100037375.1.2",
          "form": "패션에 관심 있으새요?",
          "original_form": "패션에 관심 있으새요?",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037375.1.3",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037375.1.4",
          "form": "지금 어떤 패션이새요?",
          "original_form": "지금 어떤 패션이새요?",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037375.1.5",
          "form": "여름이죠",
          "original_form": "여름이죠",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037375.1.6",
          "form": "반팔 반바지?",
          "original_form": "반팔 반바지?",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100037375.1.7",
          "form": "네.맞아요.잘아시네",
          "original_form": "네.맞아요.잘아시네",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037375.1.8",
          "form": "맞춰봤어뇨",
          "original_form": "맞춰봤어뇨",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037375.1.9",
          "form": "오 돗자리ㅡ얼른 까세욧!",
          "original_form": "오 돗자리ㅡ얼른 까세욧!",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037375.1.10",
          "form": "돗자리 사줘요",
          "original_form": "돗자리 사줘요",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037375.1.11",
          "form": "집에 돗자리 없나요.\r\n바닷가가면 필요한 은색돗자리 ㅎ",
          "original_form": "집에 돗자리 없나요.\r\n바닷가가면 필요한 은색돗자리 ㅎ",
          "speaker_id": "2",
