      "utterance": [
        {
          "id": "MDRW2100028679.1.1",
          "form": "무슨색이 젤 맘에드셨어요??",
          "original_form": "무슨색이 젤 맘에드셨어요??",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028679.1.2",
          "form": "저는 크림이요!!",
          "original_form": "저는 크림이요!!",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028679.1.3",
          "form": "무슨색이 젤 맘에 드세요?",
          "original_form": "무슨색이 젤 맘에 드세요?",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028679.1.4",
          "form": "저는 라벤더가 이쁘더라구요",
          "original_form": "저는 라벤더가 이쁘더라구요",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028679.1.5",
          "form": "그리고 케이스 미쳤따리 ㅠㅠ...",
          "original_form": "그리고 케이스 미쳤따리 ㅠㅠ...",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028679.1.6",
          "form": "헐 맞아요 케이스!!!",
          "original_form": "헐 맞아요 케이스!!!",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028679.1.7",
          "form": "엄청 신기한거 많더라구요",
          "original_form": "엄청 신기한거 많더라구요",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028679.1.8",
          "form": "진짜 바꾸고싶네요ㅠㅠ",
          "original_form": "진짜 바꾸고싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028679.1.9",
          "form": "저 노티드도넛 케이스 보고 기절 ㅋㅋㅋ",
          "original_form": "저 노티드도넛 케이스 보고 기절 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028679.1.10",
          "form": "하 아직 2년 못채웠어여 ...",
          "original_form": "하 아직 2년 못채웠어여 ...",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028679.1.11",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028679.1.12",
          "form": "노티드도넛 케이스 처음 봤는데",
          "original_form": "노티드도넛 케이스 처음 봤는데",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028679.1.13",
          "form": "진짜 귀엽네요ㅠㅠㅠㅠㅠㅠ",
          "original_form": "진짜 귀엽네요ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028679.1.14",
          "form": "진짜.. 가져야만한다는 생각이",
          "original_form": "진짜.. 가져야만한다는 생각이",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028679.1.15",
          "form": "하지만 2년이 되지않았어여",
          "original_form": "하지만 2년이 되지않았어여",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028679.1.16",
          "form": "저두",
          "original_form": "저두",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028679.1.17",
          "form": "담에 플립4라도 나오면 그거 사야겠어요ㅠㅠ",
          "original_form": "담에 플립4라도 나오면 그거 사야겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028679.1.18",
          "form": "ㅎ ㅏ 또 이쁜거 나오겠져",
          "original_form": "ㅎ ㅏ 또 이쁜거 나오겠져",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028679.1.19",
          "form": "갠적으로 폴드는 별로에여 아재폰 느낌",
          "original_form": "갠적으로 폴드는 별로에여 아재폰 느낌",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028679.1.20",
          "form": "그쵸 폴드는 안땡겨요",
          "original_form": "그쵸 폴드는 안땡겨요",
          "speaker_id": "1",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028679.1.21",
          "form": "플립이 딱인데 담에도 이렇게 이뻤음 좋겠네요",
          "original_form": "플립이 딱인데 담에도 이렇게 이뻤음 좋겠네요",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028679.1.22",
          "form": "전 버즈프로 쓰는데",
          "original_form": "전 버즈프로 쓰는데",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028679.1.23",
          "form": "나중에케이스만이라도 사볼까 고민이에여 ㅋㅋ",
          "original_form": "나중에케이스만이라도 사볼까 고민이에여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028679.1.24",
          "form": "오 그것도 괜찮네요!!",
          "original_form": "오 그것도 괜찮네요!!",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028679.1.25",
          "form": "플립은 바깥쪽 작은화면 나오는게 너무 귀엽고 이쁜거같아요ㅠㅠㅠㅠ",
          "original_form": "플립은 바깥쪽 작은화면 나오는게 너무 귀엽고 이쁜거같아요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028679.1.26",
          "form": "맞아요 거기에 움짤 넣는거 보고 사람들 창의력에 감탄함 ㅠㅠㅋㅋ",
