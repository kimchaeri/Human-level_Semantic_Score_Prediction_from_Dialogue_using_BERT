      "utterance": [
        {
          "id": "MDRW2100002242.1.1",
          "form": "요새 플립이 너무 사고싶어져요ㅠㅜ",
          "original_form": "요새 플립이 너무 사고싶어져요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210820 01:18"
        },
        {
          "id": "MDRW2100002242.1.2",
          "form": "저...플립3 사전예약했답니다",
          "original_form": "저...플립3 사전예약했답니다",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002242.1.3",
          "form": "와 대박ㅋㅋㅋ",
          "original_form": "와 대박ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002242.1.4",
          "form": "내일 배송되요ㅋㅋㅋㅋ",
          "original_form": "내일 배송되요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002242.1.5",
          "form": "무슨 색 하셨어요?",
          "original_form": "무슨 색 하셨어요?",
          "speaker_id": "1",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002242.1.6",
          "form": "무슨색원하시나여ㅋㅋㅋ",
          "original_form": "무슨색원하시나여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002242.1.7",
          "form": "헉 너무 부럽네요ㅠㅜ",
          "original_form": "헉 너무 부럽네요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002242.1.8",
          "form": "초록이!",
          "original_form": "초록이!",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002242.1.9",
          "form": "유행안타고 오래쓸꺼같더라구요",
          "original_form": "유행안타고 오래쓸꺼같더라구요",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002242.1.10",
          "form": "저 라벤더나 그린이요!",
          "original_form": "저 라벤더나 그린이요!",
          "speaker_id": "1",
          "time": "20210820 01:21"
        },
        {
          "id": "MDRW2100002242.1.11",
          "form": "요번에 진짜 색 잘 뽑은 것 같아요ㅋㄱㅈㄲ",
          "original_form": "요번에 진짜 색 잘 뽑은 것 같아요ㅋㄱㅈㄲ",
          "speaker_id": "1",
          "time": "20210820 01:21"
        },
        {
          "id": "MDRW2100002242.1.12",
          "form": "근데 사전예약 딱히 혜택없더라구요ㅜㅜ",
          "original_form": "근데 사전예약 딱히 혜택없더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 01:21"
        },
        {
          "id": "MDRW2100002242.1.13",
          "form": "엥? 진짜요? 걍 얼리어답터가 되라 이건가ㅜ",
          "original_form": "엥? 진짜요? 걍 얼리어답터가 되라 이건가ㅜ",
          "speaker_id": "1",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002242.1.14",
          "form": "아무튼 부러워요ㅠㅜ 전 갤s21로 바꾼지 얼마 안되서ㅠㅜ",
          "original_form": "아무튼 부러워요ㅠㅜ 전 갤s21로 바꾼지 얼마 안되서ㅠㅜ",
          "speaker_id": "1",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002242.1.15",
          "form": "110만원이라는 큰금액을 썼지만 열심히 써보겠습니다 ㅎㅎ 갤21도 좋자나요? 그 사진배경지워지는거 진짜되나요?",
          "original_form": "110만원이라는 큰금액을 썼지만 열심히 써보겠습니다 ㅎㅎ 갤21도 좋자나요? 그 사진배경지워지는거 진짜되나요?",
          "speaker_id": "2",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002242.1.16",
          "form": "네ㅋㅋㅋ신기한데 그닥 안 써요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ신기한데 그닥 안 써요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002242.1.17",
          "form": "좀더쓰시다가 중고로 팔고 기변하시죠ㅎㅎ",
          "original_form": "좀더쓰시다가 중고로 팔고 기변하시죠ㅎㅎ",
