      "utterance": [
        {
          "id": "MDRW2100006195.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006195.1.2",
          "form": "학교 다니고 계신가요?",
          "original_form": "학교 다니고 계신가요?",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006195.1.3",
          "form": "네 고등학생이에요",
          "original_form": "네 고등학생이에요",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006195.1.4",
          "form": "요새 수업은 다 집에서 하고 계세요?",
          "original_form": "요새 수업은 다 집에서 하고 계세요?",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006195.1.5",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006195.1.6",
          "form": "저희는 올해부터 계속 학교 가고 있어요",
          "original_form": "저희는 올해부터 계속 학교 가고 있어요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006195.1.7",
          "form": "아 정말요? 마스크 하루 종일 쓰고 있겠네요",
          "original_form": "아 정말요? 마스크 하루 종일 쓰고 있겠네요",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006195.1.8",
          "form": "네 그렇죠...",
          "original_form": "네 그렇죠...",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006195.1.9",
          "form": "마스크 진짜 너무 답답해요 ㅠㅠ",
          "original_form": "마스크 진짜 너무 답답해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006195.1.10",
          "form": "공부보다 마스크가 더 스트레스겠네요ㅠ",
          "original_form": "공부보다 마스크가 더 스트레스겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006195.1.11",
          "form": "시국이 시국이다보니 답답한게 너무 많아요",
          "original_form": "시국이 시국이다보니 답답한게 너무 많아요",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006195.1.12",
          "form": "수능 준비중이신거에요 그럼?",
          "original_form": "수능 준비중이신거에요 그럼?",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006195.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100006195.1.14",
          "form": "이과요 문과요?",
          "original_form": "이과요 문과요?",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100006195.1.15",
          "form": "저 문과요",
          "original_form": "저 문과요",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100006195.1.16",
          "form": "혹시 name1님은 졸업하셨나요??",
          "original_form": "혹시 &name1&님은 졸업하셨나요??",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100006195.1.17",
          "form": "오 네네 저는 졸업하고 사회인이에요",
          "original_form": "오 네네 저는 졸업하고 사회인이에요",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006195.1.18",
          "form": "전 이과 출신입니다ㅋㅋㅋ사회 국사를 못해서..",
          "original_form": "전 이과 출신입니다ㅋㅋㅋ사회 국사를 못해서..",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006195.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006195.1.20",
          "form": "저는 수학을 너무 못해서...",
          "original_form": "저는 수학을 너무 못해서...",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006195.1.21",
          "form": "혹시 실례가 안 된다면 무슨 학과 나오셨나요...?",
          "original_form": "혹시 실례가 안 된다면 무슨 학과 나오셨나요...?",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006195.1.22",
          "form": "전 화학과 거쳐서 약학과요ㅎㅎ",
          "original_form": "전 화학과 거쳐서 약학과요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006195.1.23",
          "form": "친구들보면 잘하는 과목따라 문이과 정하기도 하던데ㅋㅋㅋ대부분은 못하는 과목 반대로 가죠ㅋㅋㅋ",
          "original_form": "친구들보면 잘하는 과목따라 문이과 정하기도 하던데ㅋㅋㅋ대부분은 못하는 과목 반대로 가죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006195.1.24",
          "form": "가려고 하는 학과 있어요?",
          "original_form": "가려고 하는 학과 있어요?",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006195.1.25",
          "form": "철학과 생각하다",
          "original_form": "철학과 생각하다",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006195.1.26",
          "form": "종교학과라는 곳이 있다는 거 알고 관심 가지게 됐어요!",
          "original_form": "종교학과라는 곳이 있다는 거 알고 관심 가지게 됐어요!",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006195.1.27",
          "form": "오 철학과 뭔가 저한테는 생소하네요",
          "original_form": "오 철학과 뭔가 저한테는 생소하네요",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006195.1.28",
          "form": "종교학과는 세부전공으로 기독교 천주교 이런게 나뉘는 걸까요?",
          "original_form": "종교학과는 세부전공으로 기독교 천주교 이런게 나뉘는 걸까요?",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006195.1.29",
          "form": "전체적인 종교사적인 걸 배우는 곳이에요",
          "original_form": "전체적인 종교사적인 걸 배우는 곳이에요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006195.1.30",
          "form": "특정 종교 위주로 배우는 곳은 신학과 같은 곳이고",
          "original_form": "특정 종교 위주로 배우는 곳은 신학과 같은 곳이고",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006195.1.31",
          "form": "근데 많이들 헷갈려 하시더라고요",
          "original_form": "근데 많이들 헷갈려 하시더라고요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006195.1.32",
          "form": "그죠 보통 사람들은 모르는 경우가 많으니까요 :)",
          "original_form": "그죠 보통 사람들은 모르는 경우가 많으니까요 :)",
          "speaker_id": "1",
          "time": "20210907 21:45"
        }
      ]
