        },
        {
          "id": "MDRW2100024216.1.2",
          "form": "포털에 들국화 검색하는데 진짜 깜짝 놀랐어요",
          "original_form": "포털에 들국화 검색하는데 진짜 깜짝 놀랐어요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024216.1.3",
          "form": "전인권 그사람이 들국화엿ㄱ던가요",
          "original_form": "전인권 그사람이 들국화엿ㄱ던가요",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024216.1.4",
          "form": "전인권씨.. 잘 계시겠죠?",
          "original_form": "전인권씨.. 잘 계시겠죠?",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024216.1.5",
          "form": "네 ㅎㅎ 1985년도에 데뷔하셨네요",
          "original_form": "네 ㅎㅎ 1985년도에 데뷔하셨네요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024216.1.6",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024216.1.7",
          "form": "님 중고등학교 땐 무슨 노래 많이 들었어요?",
          "original_form": "님 중고등학교 땐 무슨 노래 많이 들었어요?",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024216.1.8",
          "form": "저 중고등학교땐 동방신기가 워낙 인기엿는데",
          "original_form": "저 중고등학교땐 동방신기가 워낙 인기엿는데",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024216.1.9",
          "form": "중학생땐 버즈",
          "original_form": "중학생땐 버즈",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024216.1.10",
          "form": "고등학교땐 동방신기",
          "original_form": "고등학교땐 동방신기",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024216.1.11",
          "form": "고3에 빅뱅 원더걸스",
          "original_form": "고3에 빅뱅 원더걸스",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024216.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024216.1.13",
          "form": "오 ㅋㅋㅋㅋㅋ저도요!!",
          "original_form": "오 ㅋㅋㅋㅋㅋ저도요!!",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024216.1.14",
          "form": "혹시 동방신기의 허그 노래 아시나요 ㅎㅎㅎ",
          "original_form": "혹시 동방신기의 허그 노래 아시나요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024216.1.15",
          "form": "네네 알죠",
          "original_form": "네네 알죠",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024216.1.16",
          "form": "남자애들이 그 노래로 장난 짖궃게 많이 치구 했는데",
          "original_form": "남자애들이 그 노래로 장난 짖궃게 많이 치구 했는데",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024216.1.17",
          "form": "맛있는을 마싯는으로 읽냐 마딛는으로 읽냐",
          "original_form": "맛있는을 마싯는으로 읽냐 마딛는으로 읽냐",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024216.1.18",
          "form": "국어시간의 소소한 논쟁거리였어요",
          "original_form": "국어시간의 소소한 논쟁거리였어요",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024216.1.19",
          "form": "마싯는이 맞지 않습니까",
          "original_form": "마싯는이 맞지 않습니까",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024216.1.20",
          "form": "ㅋㅋㅋㅋㅋ최강창민이 마딛는으로 노래를 불러서",
          "original_form": "ㅋㅋㅋㅋㅋ최강창민이 마딛는으로 노래를 불러서",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024216.1.21",
          "form": "ㅋㅋㅋㅋㅋ아 진짜요?ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아 진짜요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024216.1.22",
          "form": "마싯는이 맞지않나 하는 논쟁이 잇엇죠",
          "original_form": "마싯는이 맞지않나 하는 논쟁이 잇엇죠",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.23",
          "form": "아 진짜 너무 웃긴다",
          "original_form": "아 진짜 너무 웃긴다",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.24",
          "form": "그 가사가 허그에 있다구용?",
          "original_form": "그 가사가 허그에 있다구용?",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.25",
          "form": "ㅋㅋㅋㅋㅋ넹ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ넹ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.26",
          "form": "아 찾았어요 ㅋㅋㅋㅋ",
          "original_form": "아 찾았어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.27",
          "form": "니가 주는 맛있는 우유와 부드러운~",
          "original_form": "니가 주는 맛있는 우유와 부드러운~",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.28",
          "form": "ㅋㅋㅋㅋㅋ넼ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.29",
          "form": "여기 부분ㅋㅋㅋ 지금 음원 들으면 있어용?",
          "original_form": "여기 부분ㅋㅋㅋ 지금 음원 들으면 있어용?",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.30",
          "form": "고양이가 되어서 우유마신다고",
          "original_form": "고양이가 되어서 우유마신다고",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.31",
          "form": "네네 잇어요",
          "original_form": "네네 잇어요",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.32",
          "form": "아 진짜 너무 웃긴다 저 처음 알았어요 그 논란",
          "original_form": "아 진짜 너무 웃긴다 저 처음 알았어요 그 논란",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024216.1.33",
          "form": "아 진짜욬ㅋㅋㅋㅋㅋ",
          "original_form": "아 진짜욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024216.1.34",
          "form": "데뷔때부터 논란의 그룹",
          "original_form": "데뷔때부터 논란의 그룹",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024216.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024216.1.36",
