        {
          "id": "MDRW2100002403.1.1",
          "form": "name2님음 이상형이랑 결혼 하셨나요 ㅎㅎ?",
          "original_form": "&name2&님음 이상형이랑 결혼 하셨나요 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002403.1.2",
          "form": "아뇨 전혀 아니예요 이상형",
          "original_form": "아뇨 전혀 아니예요 이상형",
          "speaker_id": "2",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002403.1.3",
          "form": "앜 ㅎㅎㅎ  저도요 ㅎㅎㅎ",
          "original_form": "앜 ㅎㅎㅎ  저도요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002403.1.4",
          "form": "후회중요 ㅋㅋ",
          "original_form": "후회중요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002403.1.5",
          "form": "인생은 도전과 후회의 연속이죠",
          "original_form": "인생은 도전과 후회의 연속이죠",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002403.1.6",
          "form": "동감합니다 ㅎㅎ",
          "original_form": "동감합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002403.1.7",
          "form": "전 성격이 유한데 남편이 불이예요 ㅜㅜ",
          "original_form": "전 성격이 유한데 남편이 불이예요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002403.1.8",
          "form": "헐 저희랑 비스무리 !!!",
          "original_form": "헐 저희랑 비스무리 !!!",
          "speaker_id": "1",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002403.1.9",
          "form": "제가 속이 썩죠 ㅋㅋ",
          "original_form": "제가 속이 썩죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002403.1.10",
          "form": "이제 해탈 막이래요 ㅋㅋㅋ",
          "original_form": "이제 해탈 막이래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002403.1.11",
          "form": "전 쭉 성격이 일관된게 좋은거 같아요",
          "original_form": "전 쭉 성격이 일관된게 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002403.1.12",
          "form": "해탈 아직전 못했네요 ㅋㅋ",
          "original_form": "해탈 아직전 못했네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002403.1.13",
          "form": "그래도 결혼 결심한 계기 잇으셔요 ?",
          "original_form": "그래도 결혼 결심한 계기 잇으셔요 ?",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002403.1.14",
          "form": "엄청 잘했죠 그땐",
          "original_form": "엄청 잘했죠 그땐",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002403.1.15",
          "form": "친구들이 감동의 물결이라 했어요 ㅋㅋ",
          "original_form": "친구들이 감동의 물결이라 했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002403.1.16",
          "form": "왜 연애땐 잘하고 결혼하면 달라질까요",
          "original_form": "왜 연애땐 잘하고 결혼하면 달라질까요",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002403.1.17",
          "form": "인간의 본성이죠",
          "original_form": "인간의 본성이죠",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002403.1.18",
          "form": "잡은 물고기",
          "original_form": "잡은 물고기",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002403.1.19",
          "form": "남편분 어떠세요? 잘하나요?",
          "original_form": "남편분 어떠세요? 잘하나요?",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002403.1.20",
          "form": "잘하다가 가끔 ㅎㅎㅎ우우욱 해요",
          "original_form": "잘하다가 가끔 ㅎㅎㅎ우우욱 해요",
          "speaker_id": "1",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100002403.1.21",
          "form": "ㅋㅋ맞아요 잘하다 어느부분에 꽂혀서 팍",
          "original_form": "ㅋㅋ맞아요 잘하다 어느부분에 꽂혀서 팍",
          "speaker_id": "2",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100002403.1.22",
          "form": "참나 성질하곤",
          "original_form": "참나 성질하곤",
          "speaker_id": "2",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100002403.1.23",
          "form": "가끔 화날 타이밍 아닌데  왜저러지 싶은 ㅠㅠㅋ",
          "original_form": "가끔 화날 타이밍 아닌데  왜저러지 싶은 ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100002403.1.24",
          "form": "오 맞아요 이해불가",
          "original_form": "오 맞아요 이해불가",
          "speaker_id": "2",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100002403.1.25",
          "form": "이건 뭐지 당황해요",
          "original_form": "이건 뭐지 당황해요",
          "speaker_id": "2",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100002403.1.26",
          "form": "애들도 있는데 참 성질이 ㅡㅡ",
          "original_form": "애들도 있는데 참 성질이 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210820 11:30"
        },
        {
          "id": "MDRW2100002403.1.27",
          "form": "ㅋㅋㅋㅋ 혼나야댐 남편들 !!!",
          "original_form": "ㅋㅋㅋㅋ 혼나야댐 남편들 !!!",
          "speaker_id": "1",
          "time": "20210820 11:30"
        },
        {
          "id": "MDRW2100002403.1.28",
          "form": "네 아주 북어 때리듯 맞아야해요",
          "original_form": "네 아주 북어 때리듯 맞아야해요",
          "speaker_id": "2",
          "time": "20210820 11:30"
        },
        {
          "id": "MDRW2100002403.1.29",
          "form": "저 자다 가끔 툭 치고는 했었는데",
          "original_form": "저 자다 가끔 툭 치고는 했었는데",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100002403.1.30",
          "form": "요샌 귀찮네요 ㅋㅋ",
          "original_form": "요샌 귀찮네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100002403.1.31",
          "form": "ㅋㅋㅋㅋ 소심한 복수",
          "original_form": "ㅋㅋㅋㅋ 소심한 복수",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
