      "utterance": [
        {
          "id": "MDRW2100019051.1.1",
          "form": "네 그건 좀 이상해요",
          "original_form": "네 그건 좀 이상해요",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019051.1.2",
          "form": "어떻게 이상한가요?",
          "original_form": "어떻게 이상한가요?",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019051.1.3",
          "form": "그냥 탄산수에 사카린 탄 맛??",
          "original_form": "그냥 탄산수에 사카린 탄 맛??",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019051.1.4",
          "form": "헉 ㅋㅋ",
          "original_form": "헉 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019051.1.5",
          "form": "말만 들어도 안 먹고 싶네요",
          "original_form": "말만 들어도 안 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019051.1.6",
          "form": "네 ㅋㅋ 별루..",
          "original_form": "네 ㅋㅋ 별루..",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019051.1.7",
          "form": "나랑드는 괜찮던데요",
          "original_form": "나랑드는 괜찮던데요",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019051.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019051.1.9",
          "form": "차라리 나랑드가 나아요",
          "original_form": "차라리 나랑드가 나아요",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.10",
          "form": "천연 사이다 아세요?",
          "original_form": "천연 사이다 아세요?",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.11",
          "form": "아 그거 ㅋㅋ",
          "original_form": "아 그거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.12",
          "form": "소다맛!!",
          "original_form": "소다맛!!",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.13",
          "form": "네 뽕따맛 ㅎㅎ",
          "original_form": "네 뽕따맛 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.14",
          "form": "그거 맛있어요",
          "original_form": "그거 맛있어요",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.15",
          "form": "약간 초딩맛? ㅋㅋ",
          "original_form": "약간 초딩맛? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.16",
          "form": "저도 ㅎㅎ",
          "original_form": "저도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.17",
          "form": "근데 사고 싶진 않은? ㅎ",
          "original_form": "근데 사고 싶진 않은? ㅎ",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.18",
          "form": "네 다 먹고싶진 않은데",
          "original_form": "네 다 먹고싶진 않은데",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.19",
          "form": "음식점 가서 먹음 맛있더라고요 ㅋㅋㅋ",
          "original_form": "음식점 가서 먹음 맛있더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019051.1.20",
          "form": "오늘은 뭐드셨어요?",
          "original_form": "오늘은 뭐드셨어요?",
