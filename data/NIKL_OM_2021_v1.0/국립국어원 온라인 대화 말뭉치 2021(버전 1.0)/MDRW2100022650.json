      "utterance": [
        {
          "id": "MDRW2100022650.1.1",
          "form": "아무튼 그래서",
          "original_form": "아무튼 그래서",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022650.1.2",
          "form": "와 반 분위기 살벌하네싶엇고",
          "original_form": "와 반 분위기 살벌하네싶엇고",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022650.1.3",
          "form": "사실 짝꿍이랑 첨엔 사이 별로 안좋앗더든요",
          "original_form": "사실 짝꿍이랑 첨엔 사이 별로 안좋앗더든요",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022650.1.4",
          "form": "왜냐면 서로 쪽지질하다가 자꾸 혼나고",
          "original_form": "왜냐면 서로 쪽지질하다가 자꾸 혼나고",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022650.1.5",
          "form": "그랬다니",
          "original_form": "그랬다니",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022650.1.6",
          "form": "좀 서로탓도하다가",
          "original_form": "좀 서로탓도하다가",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022650.1.7",
          "form": "마지ㅏㅁㄱ에 앞에 나가면서 허ㅏ해하고",
          "original_form": "마지ㅏㅁㄱ에 앞에 나가면서 허ㅏ해하고",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022650.1.8",
          "form": "괜히 더 우정돈독해진거임 지금",
          "original_form": "괜히 더 우정돈독해진거임 지금",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.9",
          "form": "그래서 친해지고 나니까 반 분위기가 너무",
          "original_form": "그래서 친해지고 나니까 반 분위기가 너무",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.10",
          "form": "좀 그런거임..",
          "original_form": "좀 그런거임..",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.11",
          "form": "헐왜..",
          "original_form": "헐왜..",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.12",
          "form": "그래서 그냥 수업을 쨌어요 ㅋ",
          "original_form": "그래서 그냥 수업을 쨌어요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.13",
          "form": "이유는 모름",
          "original_form": "이유는 모름",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.14",
          "form": "아니꼬와해서?",
          "original_form": "아니꼬와해서?",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.16",
          "form": "다음 수업을 그냥 째야겟다 싶엇음",
          "original_form": "다음 수업을 그냥 째야겟다 싶엇음",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.17",
          "form": "그래서 수업 재끼고",
          "original_form": "그래서 수업 재끼고",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.18",
          "form": "짝궁이랑",
          "original_form": "짝궁이랑",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.19",
          "form": "뒷자리 애들한테",
          "original_form": "뒷자리 애들한테",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022650.1.20",
          "form": "매점 갓다올게 하고",
          "original_form": "매점 갓다올게 하고",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.21",
          "form": "혼자 나왔는데",
          "original_form": "혼자 나왔는데",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.22",
          "form": "멋있겠다",
          "original_form": "멋있겠다",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.23",
          "form": "맛",
          "original_form": "맛",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.24",
          "form": "식당이 지하인데",
          "original_form": "식당이 지하인데",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.25",
          "form": "지하에 숨으면 안들킬거같아서",
          "original_form": "지하에 숨으면 안들킬거같아서",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.26",
          "form": "지하로 내려가다가",
          "original_form": "지하로 내려가다가",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.27",
          "form": "어떤 엔터테이너 ceo를 만난거임",
          "original_form": "어떤 엔터테이너 ceo를 만난거임",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.28",
          "form": "박진영 양현석같은 사람",
          "original_form": "박진영 양현석같은 사람",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ왜..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ왜..",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.30",
          "form": "그래서 왜 학교에 있나 싶엇는데",
          "original_form": "그래서 왜 학교에 있나 싶엇는데",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.31",
          "form": "이벤트로",
          "original_form": "이벤트로",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022650.1.32",
          "form": "몇몇 학생에게",
          "original_form": "몇몇 학생에게",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.33",
          "form": "놀 수 있는 펜트하우스와 간식을 제공하겠대요",
          "original_form": "놀 수 있는 펜트하우스와 간식을 제공하겠대요",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.34",
          "form": "근데 내가 지나가다 걸린거임",
          "original_form": "근데 내가 지나가다 걸린거임",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.35",
          "form": "그래서 혹시",
          "original_form": "그래서 혹시",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.36",
          "form": "우리반ㅇ애들만",
          "original_form": "우리반ㅇ애들만",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.37",
          "form": "데리고 와도 되냐고 물어봣음",
          "original_form": "데리고 와도 되냐고 물어봣음",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.38",
          "form": "와 잼겟다",
          "original_form": "와 잼겟다",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.39",
          "form": "근데 된대요",
          "original_form": "근데 된대요",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.40",
          "form": "근데 가능 방법이",
          "original_form": "근데 가능 방법이",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.41",
          "form": "채팅창을 키고",
          "original_form": "채팅창을 키고",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.42",
          "form": "이들이 꼬와했는데",
          "original_form": "이들이 꼬와했는데",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022650.1.43",
          "form": "반을챙기다니",
          "original_form": "반을챙기다니",
          "speaker_id": "1",
          "time": "20210829 16:47"
        }
      ]
