      "utterance": [
        {
          "id": "MDRW2100029298.1.1",
          "form": "탄산좋아하세요?",
          "original_form": "탄산좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029298.1.2",
          "form": "저 좋아합니다!",
          "original_form": "저 좋아합니다!",
          "speaker_id": "1",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029298.1.3",
          "form": "콜라 /  사이다",
          "original_form": "콜라 /  사이다",
          "speaker_id": "2",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029298.1.4",
          "form": "사이다 좋아해요 ㅎㅎ",
          "original_form": "사이다 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029298.1.5",
          "form": "name2은요?!",
          "original_form": "&name2&은요?!",
          "speaker_id": "1",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029298.1.6",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029298.1.7",
          "form": "전콜라팝니다",
          "original_form": "전콜라팝니다",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029298.1.8",
          "form": "오 그럼 펩시/코콜",
          "original_form": "오 그럼 펩시/코콜",
          "speaker_id": "1",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029298.1.9",
          "form": "아그건 너무 가혹해요",
          "original_form": "아그건 너무 가혹해요",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029298.1.10",
          "form": "전 콜라 안가려요ㅠ",
          "original_form": "전 콜라 안가려요ㅠ",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029298.1.11",
          "form": "헐 의외이신데요",
          "original_form": "헐 의외이신데요",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029298.1.12",
          "form": "대부분은 코카콜라만 취급하시던데",
          "original_form": "대부분은 코카콜라만 취급하시던데",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029298.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029298.1.14",
          "form": "한창 코카만 마셨는데",
          "original_form": "한창 코카만 마셨는데",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029298.1.15",
          "form": "어느순간 펩시도 맛있더라고요,..",
          "original_form": "어느순간 펩시도 맛있더라고요,..",
          "speaker_id": "2",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029298.1.16",
          "form": "ㅍ저는 펩시만 먹었었어요",
          "original_form": "ㅍ저는 펩시만 먹었었어요",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029298.1.17",
          "form": "예전에... 좋아하던 아이돌이 펩시 광고를 했어가지고",
          "original_form": "예전에... 좋아하던 아이돌이 펩시 광고를 했어가지고",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029298.1.18",
          "form": "ㅋㄱㅋㄱㅋ헉",
          "original_form": "ㅋㄱㅋㄱㅋ헉",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029298.1.19",
          "form": "누구에요?!",
          "original_form": "누구에요?!",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029298.1.20",
          "form": "인피니트 라고 예전에... 2015년쯤",
          "original_form": "인피니트 라고 예전에... 2015년쯤",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029298.1.21",
          "form": "했었어요!",
