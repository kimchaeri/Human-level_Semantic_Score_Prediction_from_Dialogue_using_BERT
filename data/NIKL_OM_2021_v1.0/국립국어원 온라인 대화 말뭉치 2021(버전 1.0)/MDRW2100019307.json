      "utterance": [
        {
          "id": "MDRW2100019307.1.1",
          "form": "안녕하세요~ 그쪽 날씨는 어떤가요?",
          "original_form": "안녕하세요~ 그쪽 날씨는 어떤가요?",
          "speaker_id": "1",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019307.1.2",
          "form": "안녕하세요~ 아침엔 비가 조금 오더니 지금은 흐려요",
          "original_form": "안녕하세요~ 아침엔 비가 조금 오더니 지금은 흐려요",
          "speaker_id": "2",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019307.1.3",
          "form": "여기는 슬슬 흐려지는거 같아요~",
          "original_form": "여기는 슬슬 흐려지는거 같아요~",
          "speaker_id": "1",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019307.1.4",
          "form": "우산챙기셨나요?",
          "original_form": "우산챙기셨나요?",
          "speaker_id": "1",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019307.1.5",
          "form": "우산을 안가지고 나가서 비 좀 맞았어요~",
          "original_form": "우산을 안가지고 나가서 비 좀 맞았어요~",
          "speaker_id": "2",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019307.1.6",
          "form": "주말에도 비가 오려나요?",
          "original_form": "주말에도 비가 오려나요?",
          "speaker_id": "2",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019307.1.7",
          "form": "저희지역은 계속 올거같아요 ㅠㅠ",
          "original_form": "저희지역은 계속 올거같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100019307.1.8",
          "form": "날은 안더운데 습기가 심하네요",
          "original_form": "날은 안더운데 습기가 심하네요",
          "speaker_id": "1",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100019307.1.9",
          "form": "맞아요 너무 습해서 계속 에어컨 켜놓고 있어요 ㅠㅠ",
          "original_form": "맞아요 너무 습해서 계속 에어컨 켜놓고 있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100019307.1.10",
          "form": "저두요 에어컨 없으면 어쨌을까 싶어요",
          "original_form": "저두요 에어컨 없으면 어쨌을까 싶어요",
          "speaker_id": "1",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100019307.1.11",
          "form": "비오는데 저녁은 뭐 드세요?",
          "original_form": "비오는데 저녁은 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100019307.1.12",
          "form": "창문 열어두면 시원한 바람이 부는데 습도가 높아져서 다시 에어컨 틀게되네요~",
          "original_form": "창문 열어두면 시원한 바람이 부는데 습도가 높아져서 다시 에어컨 틀게되네요~",
          "speaker_id": "2",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100019307.1.13",
          "form": "비오는 날은 역시 파전이랑 막걸리가 딱인데~",
          "original_form": "비오는 날은 역시 파전이랑 막걸리가 딱인데~",
          "speaker_id": "2",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019307.1.14",
          "form": "마침 불금이니까 막걸리 한잔 할까 해요 ㅋㅋ",
          "original_form": "마침 불금이니까 막걸리 한잔 할까 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019307.1.15",
          "form": "아 진찌 좋죠 ㅠㅠㅠ 저도 편의점가서 사올까봐요",
          "original_form": "아 진찌 좋죠 ㅠㅠㅠ 저도 편의점가서 사올까봐요",
          "speaker_id": "1",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019307.1.16",
          "form": "마침 집에 부추도 있어서 해보려구요",
          "original_form": "마침 집에 부추도 있어서 해보려구요",
          "speaker_id": "1",
          "time": "20210827 17:18"
        },
        {
          "id": "MDRW2100019307.1.17",
          "form": "오 부추전 맛있겠어요!!",
          "original_form": "오 부추전 맛있겠어요!!",
          "speaker_id": "2",
          "time": "20210827 17:18"
        },
        {
          "id": "MDRW2100019307.1.18",
          "form": "그래도 비오니까 미세먼지는 좀 없어서 다행이에요",
          "original_form": "그래도 비오니까 미세먼지는 좀 없어서 다행이에요",
          "speaker_id": "2",
          "time": "20210827 17:18"
        },
        {
          "id": "MDRW2100019307.1.19",
          "form": "네네 여름이 미세먼지 없는건 참 좋아요",
          "original_form": "네네 여름이 미세먼지 없는건 참 좋아요",
          "speaker_id": "1",
          "time": "20210827 17:18"
        },
        {
          "id": "MDRW2100019307.1.20",
          "form": "비 끝나면 다시 더워질까요?",
          "original_form": "비 끝나면 다시 더워질까요?",
          "speaker_id": "1",
          "time": "20210827 17:19"
        },
        {
          "id": "MDRW2100019307.1.21",
          "form": "가을 날씨면 좋겠는데 ㅠㅠ",
          "original_form": "가을 날씨면 좋겠는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:19"
        },
        {
          "id": "MDRW2100019307.1.22",
          "form": "그러게요~ 이제 곧 9월인데 선선한 가을바람이 불면 좋겠어요~",
          "original_form": "그러게요~ 이제 곧 9월인데 선선한 가을바람이 불면 좋겠어요~",
          "speaker_id": "2",
          "time": "20210827 17:19"
        },
        {
          "id": "MDRW2100019307.1.23",
          "form": "맞아요 코로나도 잠잠해지면 놀러가고 싶네요ㅎㅎㅎ",
          "original_form": "맞아요 코로나도 잠잠해지면 놀러가고 싶네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:20"
        },
        {
          "id": "MDRW2100019307.1.24",
