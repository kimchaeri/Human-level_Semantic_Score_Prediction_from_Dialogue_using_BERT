      "utterance": [
        {
          "id": "MDRW2100021796.1.1",
          "form": "ㅜㅋㅋ팬들은 더 속상하죠?ㅜㅠ",
          "original_form": "ㅜㅋㅋ팬들은 더 속상하죠?ㅜㅠ",
          "speaker_id": "1",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021796.1.2",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021796.1.3",
          "form": "책 좋아해여 ㅎㅎ",
          "original_form": "책 좋아해여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021796.1.4",
          "form": "추천해주세요!",
          "original_form": "추천해주세요!",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021796.1.5",
          "form": "아 전 좋아하는게 독특해서 ㅎㅎ",
          "original_form": "아 전 좋아하는게 독특해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021796.1.6",
          "form": "전 동화책 좋아해요",
          "original_form": "전 동화책 좋아해요",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021796.1.7",
          "form": "백희나 작가님 팬이에요 ㅎㅎ",
          "original_form": "백희나 작가님 팬이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021796.1.8",
          "form": "아 멋져요",
          "original_form": "아 멋져요",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021796.1.9",
          "form": "이렇게 확고한 취향 가진 분들 너무 멋져요",
          "original_form": "이렇게 확고한 취향 가진 분들 너무 멋져요",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021796.1.10",
          "form": "ㅋㅋ 도서관가면  어린이도서관부터 갑니다요 ㅋ",
          "original_form": "ㅋㅋ 도서관가면  어린이도서관부터 갑니다요 ㅋ",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021796.1.11",
          "form": "저도 초딩때 보던 동화책 얼마전에 찾아봤어ㅇㆍ",
          "original_form": "저도 초딩때 보던 동화책 얼마전에 찾아봤어ㅇㆍ",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021796.1.12",
          "form": "내일가서 한번 찾아봐야겠네요",
          "original_form": "내일가서 한번 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021796.1.13",
          "form": "좋아하는 이유가 따로 있으세요?",
          "original_form": "좋아하는 이유가 따로 있으세요?",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021796.1.14",
          "form": "짧게 읽는데 뭔가 더 여운이 남아요",
          "original_form": "짧게 읽는데 뭔가 더 여운이 남아요",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021796.1.15",
          "form": "솔직히 길게 읽을틈이  핑계긴 하지만 없기도 하고 ㅎㅎ",
          "original_form": "솔직히 길게 읽을틈이  핑계긴 하지만 없기도 하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021796.1.16",
          "form": "반전같은게 있는건 아니죠?",
          "original_form": "반전같은게 있는건 아니죠?",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021796.1.17",
          "form": "반전도 있구여",
          "original_form": "반전도 있구여",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021796.1.18",
          "form": "순수해서  더 끌려요 ㅎㄹ",
          "original_form": "순수해서  더 끌려요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021796.1.19",
          "form": "전 미국작가 스릴러나 로맨스 자주봐요",
          "original_form": "전 미국작가 스릴러나 로맨스 자주봐요",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021796.1.20",
          "form": "노란장판감성 싫어해서ㅋㅋ",
          "original_form": "노란장판감성 싫어해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021796.1.21",
          "form": "오  ㅎㅎㅎ",
          "original_form": "오  ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021796.1.22",
          "form": "ㅁㅣ비포유 작가 책들 좋더라고요",
          "original_form": "ㅁㅣ비포유 작가 책들 좋더라고요",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021796.1.23",
          "form": "도서관 가면 봐바야 겟네요 ㅎㅎ",
          "original_form": "도서관 가면 봐바야 겟네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021796.1.24",
          "form": "미비포유 3부작인데 첫편이랑 마지막이 좋대요",
          "original_form": "미비포유 3부작인데 첫편이랑 마지막이 좋대요",
