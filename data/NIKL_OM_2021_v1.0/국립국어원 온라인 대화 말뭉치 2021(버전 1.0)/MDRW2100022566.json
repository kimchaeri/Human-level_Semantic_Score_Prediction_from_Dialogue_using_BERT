      "utterance": [
        {
          "id": "MDRW2100022566.1.1",
          "form": "식음료랑 쇼핑상품 이거 은근 경계가 모호해",
          "original_form": "식음료랑 쇼핑상품 이거 은근 경계가 모호해",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022566.1.2",
          "form": "ㅋㅋㅋㅋㅋ배라 민초단 반가워요",
          "original_form": "ㅋㅋㅋㅋㅋ배라 민초단 반가워요",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022566.1.3",
          "form": "민초단이세요?",
          "original_form": "민초단이세요?",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022566.1.4",
          "form": "ㅋㅋㅋ저도 민초단 진짜 반갑네요",
          "original_form": "ㅋㅋㅋ저도 민초단 진짜 반갑네요",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022566.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ민초최고 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ민초최고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022566.1.6",
          "form": "아이스크림만 민초 먹어요?",
          "original_form": "아이스크림만 민초 먹어요?",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022566.1.7",
          "form": "다른민초도많긴한데",
          "original_form": "다른민초도많긴한데",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022566.1.8",
          "form": "배라가 진짜맛잇더라구요",
          "original_form": "배라가 진짜맛잇더라구요",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022566.1.9",
          "form": "오~저는 카페 가서도 민트초코 찾아요 ㅋㅋ",
          "original_form": "오~저는 카페 가서도 민트초코 찾아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022566.1.10",
          "form": "아뇨 다른민초도 즐겨먹어요 ㅋㅋㅋㅋ",
          "original_form": "아뇨 다른민초도 즐겨먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022566.1.11",
          "form": "민트초코 초콜렛도 좋더라구요 ㅜㅜ",
          "original_form": "민트초코 초콜렛도 좋더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022566.1.12",
          "form": "크 맞아요 그 뭐더라 유명한거잇자나요",
          "original_form": "크 맞아요 그 뭐더라 유명한거잇자나요",
          "speaker_id": "2",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022566.1.13",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022566.1.14",
          "form": "이름이기억이잘안나네요 ㅠㅠ 그거맛나는데",
          "original_form": "이름이기억이잘안나네요 ㅠㅠ 그거맛나는데",
          "speaker_id": "2",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022566.1.15",
          "form": "치약 맛이요?",
          "original_form": "치약 맛이요?",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022566.1.16",
          "form": "네네 민트초코!",
          "original_form": "네네 민트초코!",
          "speaker_id": "2",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022566.1.17",
          "form": "ㅋㅋㅋ잘 모르겠어요 ㅜㅜ",
          "original_form": "ㅋㅋㅋ잘 모르겠어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022566.1.18",
          "form": "요즘 과자업체에서 민트초코맛 출시하던데 그건 어떻게 생각해요?",
          "original_form": "요즘 과자업체에서 민트초코맛 출시하던데 그건 어떻게 생각해요?",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022566.1.19",
          "form": "아 그러면 피자에 파인애플드간거 좋아해요?",
          "original_form": "아 그러면 피자에 파인애플드간거 좋아해요?",
          "speaker_id": "2",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022566.1.20",
          "form": "피자에 파인애플은 사랑 아닌가요?ㅠ",
          "original_form": "피자에 파인애플은 사랑 아닌가요?ㅠ",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022566.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저두좋아해요 맛잇던데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저두좋아해요 맛잇던데",
          "speaker_id": "2",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022566.1.22",
          "form": "근데 싫어하는 사람들도 있더라구요",
          "original_form": "근데 싫어하는 사람들도 있더라구요",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022566.1.23",
          "form": "과자회사에서 수요를 잘 고려한다고 생각해요^^*",
          "original_form": "과자회사에서 수요를 잘 고려한다고 생각해요^^*",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022566.1.24",
          "form": "ㅋㅋㅋㅋ수요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ수요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022566.1.25",
          "form": "틈새시장노리는거죠",
          "original_form": "틈새시장노리는거죠",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022566.1.26",
          "form": "저는 아무리 그래도 민트초코 과자는.. 선넘..",
          "original_form": "저는 아무리 그래도 민트초코 과자는.. 선넘..",
          "speaker_id": "1",
