        },
        {
          "id": "MDRW2100022566.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022566.1.28",
          "form": "ㅠㅠㅠ한번도 사본 적 없어요",
          "original_form": "ㅠㅠㅠ한번도 사본 적 없어요",
