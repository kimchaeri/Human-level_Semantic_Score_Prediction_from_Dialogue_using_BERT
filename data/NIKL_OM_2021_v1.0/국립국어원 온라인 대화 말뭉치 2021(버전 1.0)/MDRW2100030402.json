      "utterance": [
        {
          "id": "MDRW2100030402.1.1",
          "form": "북부 지역의 흑발의 미 공작",
          "original_form": "북부 지역의 흑발의 미 공작",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.2",
          "form": "웹툰이다 드라마로 나오더라고요",
          "original_form": "웹툰이다 드라마로 나오더라고요",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.3",
          "form": "금발의 왕세자",
          "original_form": "금발의 왕세자",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.4",
          "form": "제목인가요",
          "original_form": "제목인가요",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.5",
          "form": "이상한 제목이네요",
          "original_form": "이상한 제목이네요",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.6",
          "form": "어릴 때부터 친구이지만 설레는 미 남자",
          "original_form": "어릴 때부터 친구이지만 설레는 미 남자",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.7",
          "form": "소설인가요",
          "original_form": "소설인가요",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.8",
          "form": "어떤 캐릭터가 취향이신가요",
          "original_form": "어떤 캐릭터가 취향이신가요",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.9",
          "form": "남주가 세 명이나 오는데 누가 진정한 남주가 될까요",
          "original_form": "남주가 세 명이나 오는데 누가 진정한 남주가 될까요",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.10",
          "form": "잘생긴 남자",
          "original_form": "잘생긴 남자",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.11",
          "form": "지금 읽고 있는 책에서 나오는 캐릭터입니다",
          "original_form": "지금 읽고 있는 책에서 나오는 캐릭터입니다",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.12",
          "form": "됐다 잘생겼어요",
          "original_form": "됐다 잘생겼어요",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030402.1.13",
          "form": "셋다",
          "original_form": "셋다",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030402.1.14",
          "form": "세 번째 사람",
          "original_form": "세 번째 사람",
          "speaker_id": "1",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030402.1.15",
          "form": "진짜 대리 만족이네요",
          "original_form": "진짜 대리 만족이네요",
          "speaker_id": "1",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030402.1.16",
          "form": "민지 그럴 것 같아서요",
          "original_form": "민지 그럴 것 같아서요",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030402.1.17",
          "form": "왠지",
          "original_form": "왠지",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030402.1.18",
          "form": "그런 거 보면 다른 거 못 볼 거 같아요",
          "original_form": "그런 거 보면 다른 거 못 볼 거 같아요",
          "speaker_id": "1",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030402.1.19",
          "form": "친구고를것같았어묘",
          "original_form": "친구고를것같았어묘",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030402.1.20",
          "form": "아기와 나 기억하시나요",
          "original_form": "아기와 나 기억하시나요",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030402.1.21",
          "form": "금방 읽겠네요",
          "original_form": "금방 읽겠네요",
          "speaker_id": "1",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030402.1.22",
          "form": "정말 재밌게 봤던 만화인데",
          "original_form": "정말 재밌게 봤던 만화인데",
