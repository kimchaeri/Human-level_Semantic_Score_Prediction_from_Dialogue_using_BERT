      "utterance": [
        {
          "id": "MDRW2100026492.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026492.1.2",
          "form": "네 지금도 하고 있어요",
          "original_form": "네 지금도 하고 있어요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026492.1.3",
          "form": "하는 운동 있나요?",
          "original_form": "하는 운동 있나요?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026492.1.4",
          "form": "홈트요",
          "original_form": "홈트요",
          "speaker_id": "1",
          "time": "20210902 07:19"
        },
        {
          "id": "MDRW2100026492.1.5",
          "form": "홈트 좋죠",
          "original_form": "홈트 좋죠",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100026492.1.6",
          "form": "주말에 한번씩 하는데",
          "original_form": "주말에 한번씩 하는데",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100026492.1.7",
          "form": "집이 편안해서 그런지",
          "original_form": "집이 편안해서 그런지",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100026492.1.8",
          "form": "하다가도 눕게되요ㅋㅋㅋ",
          "original_form": "하다가도 눕게되요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100026492.1.9",
          "form": "운동은 자주하나요?",
          "original_form": "운동은 자주하나요?",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100026492.1.10",
          "form": "매일료",
          "original_form": "매일료",
          "speaker_id": "1",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100026492.1.11",
          "form": "오 멋져요",
          "original_form": "오 멋져요",
          "speaker_id": "2",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100026492.1.12",
          "form": "운동하는 삶 대단하고 멋져요",
          "original_form": "운동하는 삶 대단하고 멋져요",
          "speaker_id": "2",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100026492.1.13",
          "form": "저도 열심히는 하는데 건강한 돼지가 되고 있어요ㅋㅋㅋ",
          "original_form": "저도 열심히는 하는데 건강한 돼지가 되고 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100026492.1.14",
          "form": "유투브봐요",
          "original_form": "유투브봐요",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100026492.1.15",
          "form": "유투브 보냐구요?",
          "original_form": "유투브 보냐구요?",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100026492.1.16",
          "form": "네 요새 안보는 사람 찾기 힘들죠ㅋㅋㅋ",
          "original_form": "네 요새 안보는 사람 찾기 힘들죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100026492.1.17",
          "form": "좋아하는 유투버 있나요?",
          "original_form": "좋아하는 유투버 있나요?",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100026492.1.18",
          "form": "땅크봐요",
          "original_form": "땅크봐요",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100026492.1.19",
          "form": "땅끄부부! 알죠알죠",
          "original_form": "땅끄부부! 알죠알죠",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100026492.1.20",
          "form": "두 분 케미가 귀엽잖아요ㅋㅋㅋ",
          "original_form": "두 분 케미가 귀엽잖아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100026492.1.21",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100026492.1.22",
          "form": "효과가있는",
          "original_form": "효과가있는",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100026492.1.23",
          "form": "꾸준히요",
          "original_form": "꾸준히요",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100026492.1.24",
          "form": "힘들다요",
          "original_form": "힘들다요",
          "speaker_id": "1",
          "time": "20210903 14:34"
        },
        {
          "id": "MDRW2100026492.1.25",
          "form": "아이고 답장을 못 했네요",
          "original_form": "아이고 답장을 못 했네요",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100026492.1.26",
          "form": "땅끄부부 괜찮으면 유투버 중에 비타민 신지니도 추천할게요",
          "original_form": "땅끄부부 괜찮으면 유투버 중에 비타민 신지니도 추천할게요",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100026492.1.27",
          "form": "이 분도 홈트 영상 올리는 분인데",
          "original_form": "이 분도 홈트 영상 올리는 분인데",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100026492.1.28",
          "form": "텐션이 하늘을 찔러서 재밌게 할 수 있더라구요",
          "original_form": "텐션이 하늘을 찔러서 재밌게 할 수 있더라구요",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100026492.1.29",
          "form": "운동 안가는 주말에 한 번 씩 듣고 따라하고 있어요",
          "original_form": "운동 안가는 주말에 한 번 씩 듣고 따라하고 있어요",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100026492.1.30",
          "form": "오ㅜ할 맛 나겠어요",
          "original_form": "오ㅜ할 맛 나겠어요",
          "speaker_id": "1",
          "time": "20210904 08:39"
        }
      ]
