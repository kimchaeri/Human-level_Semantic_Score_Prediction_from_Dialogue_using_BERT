      "utterance": [
        {
          "id": "MDRW2100014556.1.1",
          "form": "그거 h패션몰에서 쓰면되죠ㅎ",
          "original_form": "그거 h패션몰에서 쓰면되죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014556.1.2",
          "form": "h패션몰 이번에 출첵이벤트해서 2만포인트 이벤트했었는데",
          "original_form": "h패션몰 이번에 출첵이벤트해서 2만포인트 이벤트했었는데",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014556.1.3",
          "form": "제가 h패션몰을 잘 안써서 몰랐네요",
          "original_form": "제가 h패션몰을 잘 안써서 몰랐네요",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014556.1.4",
          "form": "거긴 옷 사는덴가요?",
          "original_form": "거긴 옷 사는덴가요?",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014556.1.5",
          "form": "그렇군요ㅍ",
          "original_form": "그렇군요ㅍ",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014556.1.6",
          "form": "너무 아깝네요ㅠ",
          "original_form": "너무 아깝네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014556.1.7",
          "form": "아울렛도있고 브랜드몰도있는 어플이에요ㅍ",
          "original_form": "아울렛도있고 브랜드몰도있는 어플이에요ㅍ",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014556.1.8",
          "form": "이제라도 알았으니 일단 앱을 깔아보려고요",
          "original_form": "이제라도 알았으니 일단 앱을 깔아보려고요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014556.1.9",
          "form": "옷쇼핑하는 어플이요",
          "original_form": "옷쇼핑하는 어플이요",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014556.1.10",
          "form": "거기서 옷 자주 사시나요?",
          "original_form": "거기서 옷 자주 사시나요?",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014556.1.11",
          "form": "그거 회원가입하면 스벅주는 이벤트했았는데",
          "original_form": "그거 회원가입하면 스벅주는 이벤트했았는데",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014556.1.12",
          "form": "지금은 안할꺼같애요ㅎ",
          "original_form": "지금은 안할꺼같애요ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014556.1.13",
          "form": "저는 이벤트로만 사요ㅎ",
          "original_form": "저는 이벤트로만 사요ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014556.1.14",
          "form": "그건 예전에 이미 타 먹은듯요",
          "original_form": "그건 예전에 이미 타 먹은듯요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014556.1.15",
          "form": "원랜 살 마음이 없는데 포인트를 모아서 그 포인트에 맞춰서 쇼핑하시는 거군요",
          "original_form": "원랜 살 마음이 없는데 포인트를 모아서 그 포인트에 맞춰서 쇼핑하시는 거군요",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014556.1.16",
          "form": "꼭 그런건아닌데 딸옷이필요하니깐 사는거죠ㅎ",
          "original_form": "꼭 그런건아닌데 딸옷이필요하니깐 사는거죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014556.1.17",
          "form": "아이가 자라니깐ㅎ 돈이많이들어서 이번에도 이벤트하니깐 189000원짜리 7만원에 샀네여ㅎ",
          "original_form": "아이가 자라니깐ㅎ 돈이많이들어서 이번에도 이벤트하니깐 189000원짜리 7만원에 샀네여ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014556.1.18",
          "form": "우와!!! 좋네요~ 맞아요 애들은 너무 빨리 커서 옷을 자꾸 자꾸 사줘야 되더라고요",
          "original_form": "우와!!! 좋네요~ 맞아요 애들은 너무 빨리 커서 옷을 자꾸 자꾸 사줘야 되더라고요",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014556.1.19",
          "form": "애들은 브랜드 또 안사주면 좀그래서ㅎ",
          "original_form": "애들은 브랜드 또 안사주면 좀그래서ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014556.1.20",
          "form": "빨리자라지만 또 어린이집가야해서ㅠ",
          "original_form": "빨리자라지만 또 어린이집가야해서ㅠ",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014556.1.21",
          "form": "어린이집도 교복이 있으면 좋겠죠? ㅋ",
          "original_form": "어린이집도 교복이 있으면 좋겠죠? ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014556.1.22",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014556.1.23",
          "form": "진짜 왜없는지ㅠㅍ",
          "original_form": "진짜 왜없는지ㅠㅍ",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014556.1.24",
          "form": "매일 다른옷입혀야하니ㅎ 옷값이 장난아니에요ㅠ",
          "original_form": "매일 다른옷입혀야하니ㅎ 옷값이 장난아니에요ㅠ",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014556.1.25",
          "form": "몇번 못 입는데 사주려니 아까워서 전 중고로도 많이 사요",
          "original_form": "몇번 못 입는데 사주려니 아까워서 전 중고로도 많이 사요",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014556.1.26",
          "form": "아 그러세요?ㅎ",
          "original_form": "아 그러세요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014556.1.27",
          "form": "중고는 얼마나 싸요?",
          "original_form": "중고는 얼마나 싸요?",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014556.1.28",
          "form": "올라오기 나름인데 10만원 넘는건 7~8만원대로 살 수 있어요.",
