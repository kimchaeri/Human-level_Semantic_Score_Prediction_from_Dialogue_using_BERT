      "utterance": [
        {
          "id": "MDRW2100018014.1.1",
          "form": "요즘 날이 쌀쌀해져서 긴팔을 많아 찾네요 저는",
          "original_form": "요즘 날이 쌀쌀해져서 긴팔을 많아 찾네요 저는",
          "speaker_id": "2",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100018014.1.2",
          "form": "벌써 긴팔 꺼내셨나요!",
          "original_form": "벌써 긴팔 꺼내셨나요!",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100018014.1.3",
          "form": "저는 위에 후드걸쳤더니 좀 덥던데",
          "original_form": "저는 위에 후드걸쳤더니 좀 덥던데",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100018014.1.4",
          "form": "아 완전 두꺼운 건 아니고",
          "original_form": "아 완전 두꺼운 건 아니고",
          "speaker_id": "2",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100018014.1.5",
          "form": "얇은 긴팔 남방이나 셔츠요!!!",
          "original_form": "얇은 긴팔 남방이나 셔츠요!!!",
          "speaker_id": "2",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100018014.1.6",
          "form": "아하 딱 그거입기 좋은 좋은 계절이죠",
          "original_form": "아하 딱 그거입기 좋은 좋은 계절이죠",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100018014.1.7",
          "form": "오늘은 좀 시원하던데 ㅎㅎ",
          "original_form": "오늘은 좀 시원하던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100018014.1.8",
          "form": "오 여긴 약간 습해서 더웠어요",
          "original_form": "오 여긴 약간 습해서 더웠어요",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100018014.1.9",
          "form": "기온 자체는 낮은데 습도가...",
          "original_form": "기온 자체는 낮은데 습도가...",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100018014.1.10",
          "form": "아아 ㅠㅠ 끈적거리는개 더 힘들죠",
          "original_form": "아아 ㅠㅠ 끈적거리는개 더 힘들죠",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100018014.1.11",
          "form": "뛸때 긴팔은 너무 덥더라고요",
          "original_form": "뛸때 긴팔은 너무 덥더라고요",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100018014.1.12",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100018014.1.13",
          "form": "겨울에도 뛸 때는",
          "original_form": "겨울에도 뛸 때는",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100018014.1.14",
          "form": "반팔에 외투 걸치는 게 좋더라구요",
          "original_form": "반팔에 외투 걸치는 게 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100018014.1.15",
          "form": "뛰고나면 몸에 열이 쫙 오르죠",
          "original_form": "뛰고나면 몸에 열이 쫙 오르죠",
          "speaker_id": "1",
          "time": "20210829 07:04"
        },
        {
          "id": "MDRW2100018014.1.16",
          "form": "뛸때 물건 어떻게하새요?",
          "original_form": "뛸때 물건 어떻게하새요?",
          "speaker_id": "1",
          "time": "20210829 07:05"
        },
        {
          "id": "MDRW2100018014.1.17",
          "form": "물건을 안 가죠가고나",
          "original_form": "물건을 안 가죠가고나",
          "speaker_id": "2",
          "time": "20210829 08:02"
        },
        {
          "id": "MDRW2100018014.1.18",
          "form": "힙색 챙기는데 왠만하면 폰만 들고 뛰어요",
          "original_form": "힙색 챙기는데 왠만하면 폰만 들고 뛰어요",
          "speaker_id": "2",
          "time": "20210829 08:03"
        },
        {
          "id": "MDRW2100018014.1.19",
          "form": "그렇군요 ..",
          "original_form": "그렇군요 ..",
          "speaker_id": "1",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100018014.1.20",
          "form": "요즘 고민거리였거든요",
          "original_form": "요즘 고민거리였거든요",
          "speaker_id": "1",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100018014.1.21",
          "form": "저도 그냥 다 두고 나가야겠내요 ㅎㅎ",
          "original_form": "저도 그냥 다 두고 나가야겠내요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100018014.1.22",
          "form": "오 그 전엔 뭘 들고 나가샸어요?",
          "original_form": "오 그 전엔 뭘 들고 나가샸어요?",
          "speaker_id": "2",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100018014.1.23",
          "form": "가방에 작은 물 하나 챙기고 다녔는데",
          "original_form": "가방에 작은 물 하나 챙기고 다녔는데",
          "speaker_id": "1",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100018014.1.24",
          "form": "걷기에서 달리기로 바꾸니까 불편하더라고요",
          "original_form": "걷기에서 달리기로 바꾸니까 불편하더라고요",
          "speaker_id": "1",
          "time": "20210830 12:23"
        }
      ]
    }
