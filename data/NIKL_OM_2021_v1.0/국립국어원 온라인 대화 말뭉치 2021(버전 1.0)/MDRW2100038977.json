      "utterance": [
        {
          "id": "MDRW2100038977.1.1",
          "form": "역시 맛잘알 name1님ㅠㅠ",
          "original_form": "역시 맛잘알 &name1&님ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100038977.1.2",
          "form": "단짠은 정말 최고죠ㅠㅠㅠㅠ",
          "original_form": "단짠은 정말 최고죠ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100038977.1.3",
          "form": "드실주아시네요 전 핫도그도 설탕완전 소스왕창ㅜ",
          "original_form": "드실주아시네요 전 핫도그도 설탕완전 소스왕창ㅜ",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100038977.1.4",
          "form": "자두요ㅠㅠㅠㅠㅠ",
          "original_form": "자두요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100038977.1.5",
          "form": "내일 핫도그사먹으러가야겠어여....",
          "original_form": "내일 핫도그사먹으러가야겠어여....",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100038977.1.6",
          "form": "그리고 거기에 식혜가 있다면 식혜도 한 잔 같이 마시면!!!ㅜㅠㅜㅠㅠ",
          "original_form": "그리고 거기에 식혜가 있다면 식혜도 한 잔 같이 마시면!!!ㅜㅠㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100038977.1.7",
          "form": "옆집이 또 츄러스도팔거든요",
          "original_form": "옆집이 또 츄러스도팔거든요",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100038977.1.8",
          "form": "허억ㅠㅠ 츄러스ㅠㅠㅠ",
          "original_form": "허억ㅠㅠ 츄러스ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100038977.1.9",
          "form": "크으 식혜한잔 음료수필수죠",
          "original_form": "크으 식혜한잔 음료수필수죠",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100038977.1.10",
          "form": "아이스크림과 같이 먹으면 맛있는 츄러스!!!",
          "original_form": "아이스크림과 같이 먹으면 맛있는 츄러스!!!",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100038977.1.11",
          "form": "초코딥으로 하나 사고 일반핫도그 치즈핫도그 종류별로사야겠어요ㅜ",
          "original_form": "초코딥으로 하나 사고 일반핫도그 치즈핫도그 종류별로사야겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100038977.1.12",
          "form": "저는 식혜는 다 맛있는데 시골집가서 겨울에 자연적으로 생긴 살얼음 식혜랑 찜질방식혜가 제일 맛있는거 같아요ㅠㅠ",
          "original_form": "저는 식혜는 다 맛있는데 시골집가서 겨울에 자연적으로 생긴 살얼음 식혜랑 찜질방식혜가 제일 맛있는거 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100038977.1.13",
          "form": "맞아요맞아요ㅜㅜㅜㅜ",
          "original_form": "맞아요맞아요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100038977.1.14",
          "form": "그래서 전 반찬가게에서식혜사요",
          "original_form": "그래서 전 반찬가게에서식혜사요",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100038977.1.15",
          "form": "아아ㅠㅠ 종류별로 하나씩ㅠㅠ 최고시네요ㅠㅠ",
          "original_form": "아아ㅠㅠ 종류별로 하나씩ㅠㅠ 최고시네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100038977.1.16",
          "form": "반찬가게에서도 식혜를 파나요??",
          "original_form": "반찬가게에서도 식혜를 파나요??",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038977.1.17",
          "form": "음료수가게나 일반판매용은 그맛이안나니까",
          "original_form": "음료수가게나 일반판매용은 그맛이안나니까",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038977.1.18",
          "form": "네네 식혜팔더라구여 직접만든거",
          "original_form": "네네 식혜팔더라구여 직접만든거",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038977.1.19",
          "form": "맞아요ㅜㅠㅠㅠㅠ 그맛이 안나요ㅠㅠ",
          "original_form": "맞아요ㅜㅠㅠㅠㅠ 그맛이 안나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038977.1.20",
          "form": "그래서 돈으로 그맛을느낍니다..",
          "original_form": "그래서 돈으로 그맛을느낍니다..",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038977.1.21",
          "form": "허류ㅠㅠㅠㅠ 반찬가게는 반찬만 팔 줄알고 안가봤는데 집근처 반찬가게 얼렁 찾아봐야겠어오ㅠㅠㅠㅠ",
          "original_form": "허류ㅠㅠㅠㅠ 반찬가게는 반찬만 팔 줄알고 안가봤는데 집근처 반찬가게 얼렁 찾아봐야겠어오ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038977.1.22",
          "form": "역시 돈의 맛이란... 맛있는 맛....ㅠㅠ",
          "original_form": "역시 돈의 맛이란... 맛있는 맛....ㅠㅠ",
