          "speaker_id": "1",
          "time": "20210827 18:19"
        },
        {
          "id": "MDRW2100019358.1.7",
          "form": "똑똑하고 잘생긴사람",
          "original_form": "똑똑하고 잘생긴사람",
          "speaker_id": "2",
          "time": "20210827 18:19"
