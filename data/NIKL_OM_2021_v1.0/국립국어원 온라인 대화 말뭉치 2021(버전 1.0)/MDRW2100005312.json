      "utterance": [
        {
          "id": "MDRW2100005312.1.1",
          "form": "일 안하고 놀고싶다",
          "original_form": "일 안하고 놀고싶다",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005312.1.2",
          "form": "돈많은 백수",
          "original_form": "돈많은 백수",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005312.1.3",
          "form": "모두의 꿈",
          "original_form": "모두의 꿈",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005312.1.4",
          "form": "진짜로....",
          "original_form": "진짜로....",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005312.1.5",
          "form": "살려줘!",
          "original_form": "살려줘!",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005312.1.6",
          "form": "돈벌어야해 ㅠ",
          "original_form": "돈벌어야해 ㅠ",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005312.1.7",
          "form": "맞아 사람은 왜 돈을 벌어야 할까",
          "original_form": "맞아 사람은 왜 돈을 벌어야 할까",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005312.1.8",
          "form": "그치 돈 없이 각자 하고싶은걸하면좋을텐데",
          "original_form": "그치 돈 없이 각자 하고싶은걸하면좋을텐데",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005312.1.9",
          "form": "infp스러운 발상인듯",
          "original_form": "infp스러운 발상인듯",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005312.1.10",
          "form": "나도 같은 생각을 했어",
          "original_form": "나도 같은 생각을 했어",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005312.1.11",
          "form": "세상사람들이 다 infp라면",
          "original_form": "세상사람들이 다 infp라면",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005312.1.12",
          "form": "얼마나좋을까",
          "original_form": "얼마나좋을까",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005312.1.13",
          "form": "name1님은 스트레스가 많군",
          "original_form": "&name1&님은 스트레스가 많군",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005312.1.14",
          "form": "저요? 그렇죠. 님도 일 스트레스 많짆아요",
          "original_form": "저요? 그렇죠. 님도 일 스트레스 많짆아요",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005312.1.15",
          "form": "죽겠습니다 알바만 하고싶습니다",
          "original_form": "죽겠습니다 알바만 하고싶습니다",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
