      "utterance": [
        {
          "id": "MDRW2100046139.1.1",
          "form": "뭐가 부러워요??",
          "original_form": "뭐가 부러워요??",
          "speaker_id": "2",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100046139.1.2",
          "form": "교수님들이 나이가 좀 있으시면",
          "original_form": "교수님들이 나이가 좀 있으시면",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100046139.1.3",
          "form": "다그런건아니지만",
          "original_form": "다그런건아니지만",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100046139.1.4",
          "form": "기준이 명확한게 있어요",
          "original_form": "기준이 명확한게 있어요",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100046139.1.5",
          "form": "음.. 그게 무슨..",
          "original_form": "음.. 그게 무슨..",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046139.1.6",
          "form": "봐주시는게 없다고요ㅋㅋㅋㅋㅋ",
          "original_form": "봐주시는게 없다고요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046139.1.7",
          "form": "성적도 칼이고",
          "original_form": "성적도 칼이고",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046139.1.8",
          "form": "시험 난이도도 높고ㅠ",
          "original_form": "시험 난이도도 높고ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046139.1.9",
          "form": "아아...",
          "original_form": "아아...",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046139.1.10",
          "form": "경력이 많으시니까",
          "original_form": "경력이 많으시니까",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046139.1.11",
          "form": "대신ㅋㅋㅋ시험스타일이 잘 안바뀌세요",
          "original_form": "대신ㅋㅋㅋ시험스타일이 잘 안바뀌세요",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046139.1.12",
          "form": "그래서 족보가 돌죠ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 족보가 돌죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046139.1.13",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046139.1.14",
          "form": "말로만 듣던 족보",
          "original_form": "말로만 듣던 족보",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046139.1.15",
          "form": "말이 족보지 선배들이 정리한 요약본인경우도 많아요ㅋㅋ",
          "original_form": "말이 족보지 선배들이 정리한 요약본인경우도 많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046139.1.16",
          "form": "그래도 그런게 있으면",
          "original_form": "그래도 그런게 있으면",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046139.1.17",
          "form": "시험공부에 도움될거같아요",
          "original_form": "시험공부에 도움될거같아요",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046139.1.18",
          "form": "선배들한테",
          "original_form": "선배들한테",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046139.1.19",
          "form": "도움요청해보세요!",
          "original_form": "도움요청해보세요!",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046139.1.20",
          "form": "저희과는 2학년때 전과필수라",
          "original_form": "저희과는 2학년때 전과필수라",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046139.1.21",
          "form": "선배가 없답니다ㅜ",
          "original_form": "선배가 없답니다ㅜ",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046139.1.22",
          "form": "음....안타깝ㅜㅜ",
          "original_form": "음....안타깝ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046139.1.23",
          "form": "아 근데어차피 창의수업이라",
          "original_form": "아 근데어차피 창의수업이라",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046139.1.24",
          "form": "정답은 없다면서요ㅋㅋㅋㅋ",
          "original_form": "정답은 없다면서요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046139.1.25",
          "form": "그렇긴해요ㅋ",
          "original_form": "그렇긴해요ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:44"
        }
      ]
    }
