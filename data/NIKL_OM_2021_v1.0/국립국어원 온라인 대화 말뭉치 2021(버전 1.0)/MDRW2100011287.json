      "utterance": [
        {
          "id": "MDRW2100011287.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011287.1.2",
          "form": "종종하는데 간단한것만 하게되네요",
          "original_form": "종종하는데 간단한것만 하게되네요",
          "speaker_id": "2",
          "time": "20210824 10:45"
        },
        {
          "id": "MDRW2100011287.1.3",
          "form": "무슨게임인가용?ㅎㅎ",
          "original_form": "무슨게임인가용?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:47"
        },
        {
          "id": "MDRW2100011287.1.4",
          "form": "그냥 심슨타일이나 그런 플래시게임 가끔해요ㅎㅎ 하시는 게임 있으세요?",
          "original_form": "그냥 심슨타일이나 그런 플래시게임 가끔해요ㅎㅎ 하시는 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210824 10:47"
        },
        {
          "id": "MDRW2100011287.1.5",
          "form": "오 심슨타일! 찾아봐야겠네요ㅎㅎ",
          "original_form": "오 심슨타일! 찾아봐야겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:48"
        },
        {
          "id": "MDRW2100011287.1.6",
          "form": "저는 꿈의집 하고있어요~",
          "original_form": "저는 꿈의집 하고있어요~",
          "speaker_id": "1",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100011287.1.7",
          "form": "그런 퍼즐게임 좋아해서요!",
          "original_form": "그런 퍼즐게임 좋아해서요!",
          "speaker_id": "1",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100011287.1.8",
          "form": "아 꿈의집ㅋㅋ 저도 한창 했었는데ㅎㅎ",
          "original_form": "아 꿈의집ㅋㅋ 저도 한창 했었는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100011287.1.9",
          "form": "ㅋㅋㅋㅋㅋ뭔가 끊을수 없더라구요",
          "original_form": "ㅋㅋㅋㅋㅋ뭔가 끊을수 없더라구요",
          "speaker_id": "1",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011287.1.10",
          "form": "이벤트도 계속하고",
          "original_form": "이벤트도 계속하고",
          "speaker_id": "1",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011287.1.11",
          "form": "오랜만에가면 오랜만에 왓다고 무제한 하트주고,,,,ㅎ",
          "original_form": "오랜만에가면 오랜만에 왓다고 무제한 하트주고,,,,ㅎ",
          "speaker_id": "1",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011287.1.12",
          "form": "맞아요 중독성있어요 끝도 없구ㅋㅋㅋ",
          "original_form": "맞아요 중독성있어요 끝도 없구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100011287.1.13",
          "form": "심슨타일은 어떤 종류 게임이에요?",
          "original_form": "심슨타일은 어떤 종류 게임이에요?",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100011287.1.14",
          "form": "짝맞추기같은거에요ㅎㅎ 간단해요ㅎㅎ",
          "original_form": "짝맞추기같은거에요ㅎㅎ 간단해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100011287.1.15",
          "form": "ㅋㅋㅋㅋ게임은 간단한게 좋은것같아요",
          "original_form": "ㅋㅋㅋㅋ게임은 간단한게 좋은것같아요",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011287.1.16",
          "form": "다른 복잡한일도 많은데 게임까지 복잡하면 하기싫어요ㅋㅋ",
          "original_form": "다른 복잡한일도 많은데 게임까지 복잡하면 하기싫어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011287.1.17",
          "form": "맞아요 귀찮아지더라구요ㅋㅋ",
          "original_form": "맞아요 귀찮아지더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100011287.1.18",
          "form": "ㅋㄱㅋㅋㅋㅋㅋ아 쿠키런도 열심히 하다가 점점 똑같은점수만 나오니까 질려서 삭제했어요",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋ아 쿠키런도 열심히 하다가 점점 똑같은점수만 나오니까 질려서 삭제했어요",
          "speaker_id": "1",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100011287.1.19",
          "form": "아 저도 쿠키런 그래서 접었는데ㅎㅎ 신기하네요",
