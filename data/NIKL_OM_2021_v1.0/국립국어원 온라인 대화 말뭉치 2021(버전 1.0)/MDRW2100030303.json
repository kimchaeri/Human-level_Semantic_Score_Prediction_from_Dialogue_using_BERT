      "utterance": [
        {
          "id": "MDRW2100030303.1.1",
          "form": "ㅎㅎㅎ 낼 아웃백 스테이크",
          "original_form": "ㅎㅎㅎ 낼 아웃백 스테이크",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030303.1.2",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030303.1.3",
          "form": "누나가 예약 해뒀다",
          "original_form": "누나가 예약 해뒀다",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030303.1.4",
          "form": "애들한테 자랑함",
          "original_form": "애들한테 자랑함",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030303.1.5",
          "form": "고기 썰러 가자",
          "original_form": "고기 썰러 가자",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030303.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030303.1.7",
          "form": "욜~ ㅋ애들이 뭐래? 많이 자랑하구 와 ㅋㅋㅋ",
          "original_form": "욜~ ㅋ애들이 뭐래? 많이 자랑하구 와 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030303.1.8",
          "form": "ㅎㅎ가서 아웃백 빵도 배터지게 먹고 오자",
          "original_form": "ㅎㅎ가서 아웃백 빵도 배터지게 먹고 오자",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030303.1.9",
          "form": "토요일이라 애들이 많ㅇ이 없어서 반응이 영",
          "original_form": "토요일이라 애들이 많ㅇ이 없어서 반응이 영",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030303.1.10",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030303.1.11",
          "form": "아웃백은 빵이 맛니",
          "original_form": "아웃백은 빵이 맛니",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030303.1.12",
          "form": "아 미리미리 고기썰러 간다고 자랑 했었어야지",
          "original_form": "아 미리미리 고기썰러 간다고 자랑 했었어야지",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.13",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.14",
          "form": "빵 몇 개까지 싸올 수 있지?",
          "original_form": "빵 몇 개까지 싸올 수 있지?",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.15",
          "form": "세 개였나.>?",
          "original_form": "세 개였나.>?",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.16",
          "form": "낼 함 봐야될듯 ㅠㅠ",
          "original_form": "낼 함 봐야될듯 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.17",
          "form": "생각보다 적네",
          "original_form": "생각보다 적네",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.18",
          "form": "근데 일단 난 안 가져갈거고 너랑 윤수만 좀 주려고",
          "original_form": "근데 일단 난 안 가져갈거고 너랑 윤수만 좀 주려고",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.20",
          "form": "땡스",
          "original_form": "땡스",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.21",
          "form": "난 할머니 있어서 글케 스테이크 사먹는다고 고기 갖고 다니는거 걸리면 혼나 ㅋㅋ",
          "original_form": "난 할머니 있어서 글케 스테이크 사먹는다고 고기 갖고 다니는거 걸리면 혼나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030303.1.22",
          "form": "ㅋㅋㅋㅋㄱㄱㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030303.1.23",
          "form": "나도 안걸리게",
          "original_form": "나도 안걸리게",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030303.1.24",
          "form": "바로 먹어야하나",
          "original_form": "바로 먹어야하나",
          "speaker_id": "2",
          "time": "20210903 23:41"
        }
      ]
