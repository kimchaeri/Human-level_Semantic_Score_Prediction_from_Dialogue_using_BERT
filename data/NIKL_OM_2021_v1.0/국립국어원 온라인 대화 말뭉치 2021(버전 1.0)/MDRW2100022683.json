      "utterance": [
        {
          "id": "MDRW2100022683.1.1",
          "form": "아침 뭐먹엇음",
          "original_form": "아침 뭐먹엇음",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.2",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.3",
          "form": "저아직안먹음",
          "original_form": "저아직안먹음",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.4",
          "form": "개배고파",
          "original_form": "개배고파",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.5",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.6",
          "form": "아까 아침에",
          "original_form": "아까 아침에",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.7",
          "form": "님은먹음?",
          "original_form": "님은먹음?",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.9",
          "form": "짜장면늘 먹엇어요",
          "original_form": "짜장면늘 먹엇어요",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.10",
          "form": "갑자기 11시에 시켜서",
          "original_form": "갑자기 11시에 시켜서",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.11",
          "form": "맛나겠다",
          "original_form": "맛나겠다",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022683.1.12",
          "form": "근데 너무 졸려서",
          "original_form": "근데 너무 졸려서",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022683.1.13",
          "form": "눈도 못뚜고 먹고",
          "original_form": "눈도 못뚜고 먹고",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022683.1.14",
          "form": "다시 잣음",
          "original_form": "다시 잣음",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022683.1.15",
          "form": "그리고 지금 전 먹는데",
          "original_form": "그리고 지금 전 먹는데",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022683.1.16",
          "form": "전에 엄마가 건새우를 넣어서",
          "original_form": "전에 엄마가 건새우를 넣어서",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022683.1.17",
          "form": "나는 건새우 안좋아해가지고",
          "original_form": "나는 건새우 안좋아해가지고",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022683.1.18",
          "form": "전에 해물을 되게 많이 넣네",
          "original_form": "전에 해물을 되게 많이 넣네",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022683.1.19",
          "form": "전 자체는 맛잇는데",
          "original_form": "전 자체는 맛잇는데",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022683.1.20",
          "form": "건새우땜에 너무 비려개제고",
          "original_form": "건새우땜에 너무 비려개제고",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022683.1.21",
          "form": "지금 입에 비린내 쩔",
          "original_form": "지금 입에 비린내 쩔",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022683.1.22",
          "form": "ㄱㄴㄲ비리겠다",
          "original_form": "ㄱㄴㄲ비리겠다",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022683.1.23",
          "form": "건새우라니",
          "original_form": "건새우라니",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022683.1.24",
          "form": "우욱..",
          "original_form": "우욱..",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022683.1.25",
          "form": "엄마아빠는 ..",
          "original_form": "엄마아빠는 ..",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022683.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022683.1.27",
          "form": "너무 해산물을 사랑해",
          "original_form": "너무 해산물을 사랑해",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022683.1.28",
