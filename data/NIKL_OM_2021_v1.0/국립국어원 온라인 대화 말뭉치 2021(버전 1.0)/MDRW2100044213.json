      "utterance": [
        {
          "id": "MDRW2100044213.1.1",
          "form": "저는 요즘 날이 따뜻한 곳 가서 스킨스쿠버 배우고 싶더라고요 name2님은 가고 싶은 곳 있으신가요?",
          "original_form": "저는 요즘 날이 따뜻한 곳 가서 스킨스쿠버 배우고 싶더라고요 &name2&님은 가고 싶은 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044213.1.2",
          "form": "오~저도 도전해보고싶어요!",
          "original_form": "오~저도 도전해보고싶어요!",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044213.1.3",
          "form": "여행 가면 외국이요",
          "original_form": "여행 가면 외국이요",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044213.1.4",
          "form": "어디든 좋을것같네요",
          "original_form": "어디든 좋을것같네요",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044213.1.5",
          "form": "그쵸ㅋㅋㅋ요즘은 정말 외국 너무 가고 싶더라고요ㅜㅜ",
          "original_form": "그쵸ㅋㅋㅋ요즘은 정말 외국 너무 가고 싶더라고요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044213.1.6",
          "form": "원래 따뜻한 나라 좋아하세요 아님 추운 나라 좋아하세요?",
          "original_form": "원래 따뜻한 나라 좋아하세요 아님 추운 나라 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044213.1.7",
          "form": "ㅋㅋ둘다 싫어요",
          "original_form": "ㅋㅋ둘다 싫어요",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044213.1.8",
          "form": "지금같은 날씨인 나라 여행가고싶어요",
          "original_form": "지금같은 날씨인 나라 여행가고싶어요",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044213.1.9",
          "form": "아ㅋㅋㅋㅋ지금 날씨 너무 괜찮죠 딱 선선하고",
          "original_form": "아ㅋㅋㅋㅋ지금 날씨 너무 괜찮죠 딱 선선하고",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044213.1.10",
          "form": "저는 아주 추운 곳 가서 오로라 한 번 보고 싶긴 하더라고요",
          "original_form": "저는 아주 추운 곳 가서 오로라 한 번 보고 싶긴 하더라고요",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044213.1.11",
          "form": "오 아일랜드?",
          "original_form": "오 아일랜드?",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044213.1.12",
          "form": "오디로 가야하죠",
          "original_form": "오디로 가야하죠",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044213.1.13",
          "form": "어 어디였지",
          "original_form": "어 어디였지",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044213.1.14",
          "form": "잠시만요ㅋㅋㅋㅋㅋ",
          "original_form": "잠시만요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044213.1.15",
          "form": "캐나다도가고 그 유우리사막?",
          "original_form": "캐나다도가고 그 유우리사막?",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100044213.1.16",
          "form": "거기도 볼수있고",
          "original_form": "거기도 볼수있고",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100044213.1.17",
          "form": "아이고 죄송해요 답이 늦었죠",
          "original_form": "아이고 죄송해요 답이 늦었죠",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100044213.1.18",
          "form": "아이슬란드였더라고요ㅋㅋㅋ",
          "original_form": "아이슬란드였더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100044213.1.19",
          "form": "아ㅋㅋㅋㅋㅋ 긴휴가내고가야돼여",
          "original_form": "아ㅋㅋㅋㅋㅋ 긴휴가내고가야돼여",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100044213.1.20",
          "form": "멀죠?",
          "original_form": "멀죠?",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100044213.1.21",
          "form": "네네ㅋㅋㅋㅋㅋ먼 것 같아요",
          "original_form": "네네ㅋㅋㅋㅋㅋ먼 것 같아요",
          "speaker_id": "1",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100044213.1.22",
          "form": "저도 그거 버킷리스트에있긴해요",
          "original_form": "저도 그거 버킷리스트에있긴해요",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100044213.1.23",
          "form": "티비에 나온 적 있는데 되게 부럽더라고요",
          "original_form": "티비에 나온 적 있는데 되게 부럽더라고요",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100044213.1.24",
          "form": "맞아요 언젠가 갈수있겠죠",
          "original_form": "맞아요 언젠가 갈수있겠죠",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100044213.1.25",
          "form": "맞아요 코로나 끝나면 꼭 가보고 싶어요ㅋㅋㅋ",
          "original_form": "맞아요 코로나 끝나면 꼭 가보고 싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100044213.1.26",
          "form": "요즘은 국내여행만가능하니깐요",
          "original_form": "요즘은 국내여행만가능하니깐요",
          "speaker_id": "2",
          "time": "20210908 11:13"
        }
      ]
    }
