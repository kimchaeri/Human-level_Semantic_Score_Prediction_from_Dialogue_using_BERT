      "utterance": [
        {
          "id": "MDRW2100002111.1.1",
          "form": "안녕하세요 요즘 재미있는 프로그램 있나요",
          "original_form": "안녕하세요 요즘 재미있는 프로그램 있나요",
          "speaker_id": "1",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002111.1.2",
          "form": "저는 요즘 악마판사 정주행중이예요",
          "original_form": "저는 요즘 악마판사 정주행중이예요",
          "speaker_id": "2",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002111.1.3",
          "form": "아 그것도 재미잇나요? 제목만 들어봣어요",
          "original_form": "아 그것도 재미잇나요? 제목만 들어봣어요",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002111.1.4",
          "form": "저는 재밌게 보고 있어요ㅋㅋ",
          "original_form": "저는 재밌게 보고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002111.1.5",
          "form": "저는 몰아보는편이라 한번에 쭉ㅋ",
          "original_form": "저는 몰아보는편이라 한번에 쭉ㅋ",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002111.1.6",
          "form": "저도 그래요ㅎㅎ",
          "original_form": "저도 그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002111.1.7",
          "form": "요즘  보고 계신거 있으세요?",
          "original_form": "요즘  보고 계신거 있으세요?",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002111.1.8",
          "form": "전 요즘 슬기로운의사생활 보려고하는중이에요",
          "original_form": "전 요즘 슬기로운의사생활 보려고하는중이에요",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002111.1.9",
          "form": "슬의는 1편부터 보셔야쥬",
          "original_form": "슬의는 1편부터 보셔야쥬",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002111.1.10",
          "form": "사실 저는 조정석과 지성을 좋아해요ㅎㅎ",
          "original_form": "사실 저는 조정석과 지성을 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002111.1.11",
          "form": "그래서 슬의도 보고 악마판사도 보는거죠",
          "original_form": "그래서 슬의도 보고 악마판사도 보는거죠",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100002111.1.12",
          "form": "아 저도 지성팬이에요",
          "original_form": "아 저도 지성팬이에요",
          "speaker_id": "1",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100002111.1.13",
          "form": "넘 좋음",
          "original_form": "넘 좋음",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002111.1.14",
          "form": "정말요?  ㅎㅎ 찌찌뽕",
          "original_form": "정말요?  ㅎㅎ 찌찌뽕",
          "speaker_id": "2",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002111.1.15",
          "form": "저는 킬미힐미때 반해서",
          "original_form": "저는 킬미힐미때 반해서",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002111.1.16",
          "form": "쭉 보고 있죠ㅎㅎ",
          "original_form": "쭉 보고 있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002111.1.17",
          "form": "저도 그거랑 아 무슨 범죄물인데",
          "original_form": "저도 그거랑 아 무슨 범죄물인데",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002111.1.18",
          "form": "제목이 기억이 잘 안나네요 그것도 재밋엇는데",
          "original_form": "제목이 기억이 잘 안나네요 그것도 재밋엇는데",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002111.1.19",
          "form": "엄기준 나오는거였나요?",
          "original_form": "엄기준 나오는거였나요?",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002111.1.20",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002111.1.21",
          "form": "피고인?",
          "original_form": "피고인?",
          "speaker_id": "2",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002111.1.22",
          "form": "재미있었죠ㅋㅋㅋ",
          "original_form": "재미있었죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002111.1.23",
          "form": "엄기준은 그때도 나쁜놈이였네요ㅋㅋ",
          "original_form": "엄기준은 그때도 나쁜놈이였네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002111.1.24",
          "form": "ㅋ아 맞아요 그거다시 보고싶더라구요",
          "original_form": "ㅋ아 맞아요 그거다시 보고싶더라구요",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002111.1.25",
          "form": "ㅋㅋ  저도 굳이 따지자면 알콩달콩보다 범죄물을 더 좋아해요",
          "original_form": "ㅋㅋ  저도 굳이 따지자면 알콩달콩보다 범죄물을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002111.1.26",
          "form": "시그널. 보이스 요런거요",
          "original_form": "시그널. 보이스 요런거요",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002111.1.27",
          "form": "시그널 재미잇게 봣어요 저도",
          "original_form": "시그널 재미잇게 봣어요 저도",
          "speaker_id": "1",
          "time": "20210820 00:17"
        }
      ]
