      "utterance": [
        {
          "id": "MDRW2100032636.1.1",
          "form": "앗ㅎㅎ여행은좋아하세요?",
          "original_form": "앗ㅎㅎ여행은좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032636.1.2",
          "form": "여행은 좋아하는데\r\n코시국 이후로 가본적이 없네요",
          "original_form": "여행은 좋아하는데\r\n코시국 이후로 가본적이 없네요",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032636.1.3",
          "form": "가장 최근 여행이 언제셨어요?",
          "original_form": "가장 최근 여행이 언제셨어요?",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032636.1.4",
          "form": "저는지난달에담양다녀왔어요",
          "original_form": "저는지난달에담양다녀왔어요",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032636.1.5",
          "form": "ㅜ코로나때문에가기어렵죠",
          "original_form": "ㅜ코로나때문에가기어렵죠",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032636.1.6",
          "form": "와우 담양.....이면",
          "original_form": "와우 담양.....이면",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032636.1.7",
          "form": "담양이면...",
          "original_form": "담양이면...",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032636.1.8",
          "form": "패러글라이딩 하셨나요?",
          "original_form": "패러글라이딩 하셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032636.1.9",
          "form": "앗ㅎㅎㅎ담양에선못했는데",
          "original_form": "앗ㅎㅎㅎ담양에선못했는데",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032636.1.10",
          "form": "스위스갔을때해봤어요",
          "original_form": "스위스갔을때해봤어요",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032636.1.11",
          "form": "우왕 스위스 하늘은 어땠나요 .",
          "original_form": "우왕 스위스 하늘은 어땠나요 {emoji:♡}.{emoji:♡}",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032636.1.12",
          "form": "ㅎㅎ너무좋았어요",
          "original_form": "ㅎㅎ너무좋았어요",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032636.1.13",
          "form": "고프로 영상도",
          "original_form": "고프로 영상도",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032636.1.14",
          "form": "남기셨어요?",
          "original_form": "남기셨어요?",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032636.1.15",
          "form": "ㅎㅎㅎ앗 그건안샀어요ㅠ",
          "original_form": "ㅎㅎㅎ앗 그건안샀어요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032636.1.16",
          "form": "가장 인상깊었던 곳이 어딘가요?",
          "original_form": "가장 인상깊었던 곳이 어딘가요?",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032636.1.17",
          "form": "아쉽 ㅜㅜ",
          "original_form": "아쉽 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032636.1.18",
          "form": "음.... 저는 하와이요 ㅋㅋ",
          "original_form": "음.... 저는 하와이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032636.1.19",
          "form": "와 너무 좋았을것같아요",
          "original_form": "와 너무 좋았을것같아요",
          "speaker_id": "1",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032636.1.20",
          "form": "네:) 하와이 너무 좋았어요\r\n아이들 생기면 다시 가야지 했는데\r\n코시국이네요ㅠㅋ",
          "original_form": "네:) 하와이 너무 좋았어요\r\n아이들 생기면 다시 가야지 했는데\r\n코시국이네요ㅠㅋ",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032636.1.21",
          "form": "큐우ㅡㅜ곧끝날꺼에오ㅡ",
          "original_form": "큐우ㅡㅜ곧끝날꺼에오ㅡ",
          "speaker_id": "1",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100032636.1.22",
          "form": "하와이 바다 이쁘고 밥도맛있을것같아요",
