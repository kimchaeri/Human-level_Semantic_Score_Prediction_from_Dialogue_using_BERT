      "utterance": [
        {
          "id": "MDRW2100030484.1.1",
          "form": "제가 알기로는 센텀시티가",
          "original_form": "제가 알기로는 센텀시티가",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030484.1.2",
          "form": "부산의 부촌이라고 알고있어요!",
          "original_form": "부산의 부촌이라고 알고있어요!",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030484.1.3",
          "form": "아하...!",
          "original_form": "아하...!",
          "speaker_id": "2",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030484.1.4",
          "form": "제가 여기서 하도 여행 주제일 때 부산 가보고싶다고 하니까 다른 분들이 말씀해주셨어요ㅠㅋㅋㅋㅋ",
          "original_form": "제가 여기서 하도 여행 주제일 때 부산 가보고싶다고 하니까 다른 분들이 말씀해주셨어요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030484.1.5",
          "form": "헉 그렇군요ㅋㅋㅋ",
          "original_form": "헉 그렇군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030484.1.6",
          "form": "'부자들이 바닷가에는 어떤 건물을 짓고 살까?' 라는 질문에 보여주기 좋은 동네라고...",
          "original_form": "'부자들이 바닷가에는 어떤 건물을 짓고 살까?' 라는 질문에 보여주기 좋은 동네라고...",
          "speaker_id": "1",
          "time": "20210904 02:00"
        },
        {
          "id": "MDRW2100030484.1.7",
          "form": "네네 너무 궁금해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 너무 궁금해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.8",
          "form": "다음번엔 꼭 센텀시티가봐야겠네요",
          "original_form": "다음번엔 꼭 센텀시티가봐야겠네요",
          "speaker_id": "2",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.9",
          "form": "네네 꼭 가보세요!!! 저도 갈거랍니다",
          "original_form": "네네 꼭 가보세요!!! 저도 갈거랍니다",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.10",
          "form": "그럼 혹시 해외여행은",
          "original_form": "그럼 혹시 해외여행은",
          "speaker_id": "2",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.11",
          "form": "그리고 부산은 일본이랑도 가까워서",
          "original_form": "그리고 부산은 일본이랑도 가까워서",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.12",
          "form": "뭔가 캐릭터 상품도 많고...",
          "original_form": "뭔가 캐릭터 상품도 많고...",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.13",
          "form": "코로나때문에 어렵겠지만ㅜ  가고싶은곳이 있으신가요?",
          "original_form": "코로나때문에 어렵겠지만ㅜ  가고싶은곳이 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.14",
          "form": "일본?",
          "original_form": "일본?",
          "speaker_id": "2",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.15",
          "form": "오 정말 많아요!",
          "original_form": "오 정말 많아요!",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.16",
          "form": "사실 일본은",
          "original_form": "사실 일본은",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.17",
          "form": "정말 가고는 싶은데",
          "original_form": "정말 가고는 싶은데",
          "speaker_id": "1",
          "time": "20210904 02:01"
        },
        {
          "id": "MDRW2100030484.1.18",
          "form": "사실 방사능 때문에...",
          "original_form": "사실 방사능 때문에...",
          "speaker_id": "1",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030484.1.19",
          "form": "꺼려지더라구요 ㅠㅠㅠㅠ",
          "original_form": "꺼려지더라구요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030484.1.20",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030484.1.21",
          "form": "제가 친구들한테 항상 일본가자고하는데",
          "original_form": "제가 친구들한테 항상 일본가자고하는데",
          "speaker_id": "2",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030484.1.22",
          "form": "항상 그런이유로 거절당해요",
          "original_form": "항상 그런이유로 거절당해요",
          "speaker_id": "2",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030484.1.23",
          "form": "앗 네네!",
          "original_form": "앗 네네!",
          "speaker_id": "1",
          "time": "20210904 02:03"
        },
        {
          "id": "MDRW2100030484.1.24",
          "form": "앜ㅋㅋㅋㅋㅋ 아이고 ㅠㅠㅠ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 아이고 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 02:03"
        }
      ]
    }
  ]
