      "utterance": [
        {
          "id": "MDRW2100038447.1.1",
          "form": "네, 그쵸",
          "original_form": "네, 그쵸",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100038447.1.2",
          "form": "계란샐러드를 좋아하긴해서",
          "original_form": "계란샐러드를 좋아하긴해서",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100038447.1.3",
          "form": "가끔 제가 맘먹고 만들때도 있어요",
          "original_form": "가끔 제가 맘먹고 만들때도 있어요",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100038447.1.4",
          "form": "ㅎㅎ 저두 사라다는 안좋아하는데",
          "original_form": "ㅎㅎ 저두 사라다는 안좋아하는데",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100038447.1.5",
          "form": "에그마요?는 괜찮아요 ㅋㅋㅋ",
          "original_form": "에그마요?는 괜찮아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100038447.1.6",
          "form": "사라다에 당근이 싫더라구여ㅜㅜ",
          "original_form": "사라다에 당근이 싫더라구여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100038447.1.7",
          "form": "아... 저도 당근은 최대한 작게",
          "original_form": "아... 저도 당근은 최대한 작게",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100038447.1.8",
          "form": "색감은 필요한데 딱히 맛은 없으니까요",
          "original_form": "색감은 필요한데 딱히 맛은 없으니까요",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100038447.1.9",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100038447.1.10",
          "form": "오이도 좋아하지만 사라다에는 작게 있는게 좋아요 ㅜ",
          "original_form": "오이도 좋아하지만 사라다에는 작게 있는게 좋아요 ㅜ",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100038447.1.11",
          "form": "전 오이는 좋아해요",
          "original_form": "전 오이는 좋아해요",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100038447.1.12",
          "form": "소금에 절여서 꾹 짜서 넣어야하지만",
          "original_form": "소금에 절여서 꾹 짜서 넣어야하지만",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100038447.1.13",
          "form": "안넣으면 좀 밋밋하달까",
          "original_form": "안넣으면 좀 밋밋하달까",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100038447.1.14",
          "form": "아 소금에 절여서 넣어요????",
          "original_form": "아 소금에 절여서 넣어요????",
          "speaker_id": "1",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100038447.1.15",
          "form": "전 사라다는 안만들어봤거든요 !",
          "original_form": "전 사라다는 안만들어봤거든요 !",
          "speaker_id": "1",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100038447.1.16",
          "form": "네, 생으로 넣으면 맛도 겉돌고 뚝뚝 부러져서 좀 별로에요",
          "original_form": "네, 생으로 넣으면 맛도 겉돌고 뚝뚝 부러져서 좀 별로에요",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038447.1.17",
          "form": "물기도 많이 생겨서 질척거리구요",
          "original_form": "물기도 많이 생겨서 질척거리구요",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038447.1.18",
          "form": "오오 담에 만들면 해볼께요!!!",
          "original_form": "오오 담에 만들면 해볼께요!!!",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038447.1.19",
          "form": "오이 소금에 절이기 ㅎㅎ",
          "original_form": "오이 소금에 절이기 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038447.1.20",
          "form": "기록해놨어요 ㅋㅋㅋ",
          "original_form": "기록해놨어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100038447.1.21",
          "form": "그리고나서 물에 한번 헹구고 꾹 짜야해요",
          "original_form": "그리고나서 물에 한번 헹구고 꾹 짜야해요",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038447.1.22",
          "form": "절인거 바로 쓰면 짜요",
          "original_form": "절인거 바로 쓰면 짜요",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038447.1.23",
          "form": "네네 !",
          "original_form": "네네 !",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100038447.1.24",
          "form": "그걸로 오이지무침해도 맛있겠어요 ㅋㅋㅋ",
          "original_form": "그걸로 오이지무침해도 맛있겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100038447.1.25",
          "form": "오? 생각도 못했는데?",
          "original_form": "오? 생각도 못했는데?",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100038447.1.26",
          "form": "야매로 오이지 무칠때 한번쯤 써볼만하네요",
          "original_form": "야매로 오이지 무칠때 한번쯤 써볼만하네요",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100038447.1.27",
          "form": "맞죠 !!! ㅋㅋㅋ",
          "original_form": "맞죠 !!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100038447.1.28",
          "form": "뜬금없이 생각이 나더라구여 ㅋㄱㄲ",
          "original_form": "뜬금없이 생각이 나더라구여 ㅋㄱㄲ",
          "speaker_id": "1",
