      "utterance": [
        {
          "id": "MDRW2100023763.1.1",
          "form": "ㄹㅇ?",
          "original_form": "ㄹㅇ?",
          "speaker_id": "2",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100023763.1.2",
          "form": "근데 왤케 느리지",
          "original_form": "근데 왤케 느리지",
          "speaker_id": "2",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100023763.1.3",
          "form": "그니까...",
          "original_form": "그니까...",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023763.1.4",
          "form": "이게 그리고",
          "original_form": "이게 그리고",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023763.1.5",
          "form": "패치파일 다운받으면서 다른거 하면 속도 겁나 느려짐...",
          "original_form": "패치파일 다운받으면서 다른거 하면 속도 겁나 느려짐...",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023763.1.6",
          "form": "아 그건 내껏도...",
          "original_form": "아 그건 내껏도...",
          "speaker_id": "2",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023763.1.7",
          "form": "아까 넘 빡쳐서 껐다가 다시 켰더니",
          "original_form": "아까 넘 빡쳐서 껐다가 다시 켰더니",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023763.1.8",
          "form": "아직도 300메가나 더 받아야해...",
          "original_form": "아직도 300메가나 더 받아야해...",
          "speaker_id": "1",
          "time": "20210830 17:17"
        },
        {
          "id": "MDRW2100023763.1.9",
          "form": "용량 과부하야...",
          "original_form": "용량 과부하야...",
          "speaker_id": "2",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100023763.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 18:11"
        },
        {
          "id": "MDRW2100023763.1.11",
          "form": "주간보스 잡는거 깜빡했다..",
          "original_form": "주간보스 잡는거 깜빡했다..",
          "speaker_id": "1",
          "time": "20210830 18:11"
        },
        {
          "id": "MDRW2100023763.1.12",
          "form": "아ㅡ",
          "original_form": "아ㅡ",
          "speaker_id": "2",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100023763.1.13",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100023763.1.14",
          "form": "뭔가 보상이 획기적이지 않아서 그런가",
          "original_form": "뭔가 보상이 획기적이지 않아서 그런가",
          "speaker_id": "1",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100023763.1.15",
          "form": "귀찮아..",
          "original_form": "귀찮아..",
          "speaker_id": "1",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100023763.1.16",
          "form": "ㅇㅇ......무기 원형이라도",
          "original_form": "ㅇㅇ......무기 원형이라도",
          "speaker_id": "2",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100023763.1.17",
          "form": "확정이면",
          "original_form": "확정이면",
          "speaker_id": "2",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100023763.1.18",
          "form": "좀 돌면서 4성 풀돌",
          "original_form": "좀 돌면서 4성 풀돌",
          "speaker_id": "2",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100023763.1.19",
          "form": "할텐데",
          "original_form": "할텐데",
          "speaker_id": "2",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100023763.1.20",
          "form": "ㅇㅇ 원형이라도 확정해주지",
          "original_form": "ㅇㅇ 원형이라도 확정해주지",
          "speaker_id": "1",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100023763.1.21",
          "form": "근데 무기 재련은",
          "original_form": "근데 무기 재련은",
          "speaker_id": "1",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100023763.1.22",
          "form": "효과 그냥 그런가봐",
          "original_form": "효과 그냥 그런가봐",
          "speaker_id": "1",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100023763.1.23",
          "form": "글킨한데",
          "original_form": "글킨한데",
          "speaker_id": "2",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100023763.1.24",
          "form": "4성 대검",
          "original_form": "4성 대검",
          "speaker_id": "2",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100023763.1.25",
          "form": "죽어라고 안나오고",
          "original_form": "죽어라고 안나오고",
          "speaker_id": "2",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100023763.1.26",
          "form": "한손검도",
          "original_form": "한손검도",
          "speaker_id": "2",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100023763.1.27",
          "form": "죽어라고 안나와서",
          "original_form": "죽어라고 안나와서",
          "speaker_id": "2",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100023763.1.28",
          "form": "어쩔수없었어....",
          "original_form": "어쩔수없었어....",
          "speaker_id": "2",
          "time": "20210830 18:26"
        },
        {
          "id": "MDRW2100023763.1.29",
          "form": "한손검 6개 만들어서 풀돌하는동안",
          "original_form": "한손검 6개 만들어서 풀돌하는동안",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023763.1.30",
          "form": "가챠로는",
          "original_form": "가챠로는",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023763.1.31",
          "form": "한개나와서",
          "original_form": "한개나와서",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023763.1.32",
          "form": "지금 하나는 3성검 쓰고잇잔아....",
          "original_form": "지금 하나는 3성검 쓰고잇잔아....",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023763.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023763.1.34",
          "form": "풀돌할 원형으로",
          "original_form": "풀돌할 원형으로",
          "speaker_id": "1",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023763.1.35",
          "form": "하나만 만들어주지 그랬어...",
          "original_form": "하나만 만들어주지 그랬어...",
          "speaker_id": "1",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023763.1.36",
          "form": "왜냐면 그 3성검도",
          "original_form": "왜냐면 그 3성검도",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100023763.1.37",
          "form": "풀돌 80렙이라",
          "original_form": "풀돌 80렙이라",
          "speaker_id": "2",
          "time": "20210830 18:28"
        },
        {
          "id": "MDRW2100023763.1.38",
          "form": "아까워서.....",
          "original_form": "아까워서.....",
          "speaker_id": "2",
          "time": "20210830 18:28"
        },
        {
          "id": "MDRW2100023763.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:29"
        },
        {
          "id": "MDRW2100023763.1.40",
          "form": "아 용학살창 꾸준히 만들어야하는데",
          "original_form": "아 용학살창 꾸준히 만들어야하는데",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100023763.1.41",
          "form": "이빨 파밍하기 매우 귀찮...",
          "original_form": "이빨 파밍하기 매우 귀찮...",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100023763.1.42",
          "form": "ㅇㅏ그거",
          "original_form": "ㅇㅏ그거",
          "speaker_id": "2",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100023763.1.43",
          "form": "난 포기햇서",
          "original_form": "난 포기햇서",
          "speaker_id": "2",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100023763.1.44",
          "form": "걍머",
          "original_form": "걍머",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023763.1.45",
          "form": "때되면 그때의 내가",
          "original_form": "때되면 그때의 내가",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023763.1.46",
          "form": "알아서 하겠지....",
          "original_form": "알아서 하겠지....",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023763.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023763.1.48",
          "form": "하기야 일단 장병기 캐를 뽑을 수 있어야",
          "original_form": "하기야 일단 장병기 캐를 뽑을 수 있어야",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023763.1.49",
          "form": "장병기를 쓰든 말든 하지...",
          "original_form": "장병기를 쓰든 말든 하지...",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023763.1.50",
          "form": "ㅇㅇ.........",
          "original_form": "ㅇㅇ.........",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023763.1.51",
          "form": "나한테 있는 장병기캐",
          "original_form": "나한테 있는 장병기캐",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023763.1.52",
          "form": "항릉이랑 로자리아였나",
          "original_form": "항릉이랑 로자리아였나",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023763.1.53",
          "form": "걔밖에 없어",
          "original_form": "걔밖에 없어",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023763.1.54",
          "form": "나도 ㅇㅇㅋㅋㅋㅋ",
          "original_form": "나도 ㅇㅇㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100023763.1.55",
          "form": "엥간해선 없기 힘든 기본지급 캐릭터들 뿐...",
          "original_form": "엥간해선 없기 힘든 기본지급 캐릭터들 뿐...",
