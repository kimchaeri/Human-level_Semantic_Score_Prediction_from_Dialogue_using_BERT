      "utterance": [
        {
          "id": "MDRW2100040314.1.1",
          "form": "숙소위치 알려쥬세요",
          "original_form": "숙소위치 알려쥬세요",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040314.1.2",
          "form": "기반으로 알려드릴게요",
          "original_form": "기반으로 알려드릴게요",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040314.1.3",
          "form": "캡쳐하려는데 지워졌어요",
          "original_form": "캡쳐하려는데 지워졌어요",
          "speaker_id": "2",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040314.1.4",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040314.1.5",
          "form": "한화리조트요",
          "original_form": "한화리조트요",
          "speaker_id": "2",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040314.1.6",
          "form": "마라훠궈 : 서면 라라관",
          "original_form": "마라훠궈 : 서면 라라관",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040314.1.7",
          "form": "해운대",
          "original_form": "해운대",
          "speaker_id": "2",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040314.1.8",
          "form": "고기 : 거대갈비, 해운대 암소갈비",
          "original_form": "고기 : 거대갈비, 해운대 암소갈비",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040314.1.9",
          "form": "조개찜 : 해운대 참새방앗간",
          "original_form": "조개찜 : 해운대 참새방앗간",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040314.1.10",
          "form": "생선구이정식 : 해운대 새좋은아침?",
          "original_form": "생선구이정식 : 해운대 새좋은아침?",
          "speaker_id": "1",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100040314.1.11",
          "form": "캡쳐했어요",
          "original_form": "캡쳐했어요",
          "speaker_id": "2",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100040314.1.12",
          "form": "와..오늘행운이네요",
          "original_form": "와..오늘행운이네요",
          "speaker_id": "2",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100040314.1.13",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "1",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100040314.1.14",
          "form": "부산은 결혼식잠깐가보고 수학여행가보고 이번에첨가거든요",
          "original_form": "부산은 결혼식잠깐가보고 수학여행가보고 이번에첨가거든요",
          "speaker_id": "2",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100040314.1.15",
          "form": "해운대 초원 복국",
          "original_form": "해운대 초원 복국",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100040314.1.16",
          "form": "엑스더스카이 야경 보시고",
          "original_form": "엑스더스카이 야경 보시고",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100040314.1.17",
          "form": "여긴 대전이라 부산가려면 2박3일은가야해서",
          "original_form": "여긴 대전이라 부산가려면 2박3일은가야해서",
          "speaker_id": "2",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100040314.1.18",
          "form": "스카이캡슐도 타셔요",
          "original_form": "스카이캡슐도 타셔요",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100040314.1.19",
          "form": "아 저 대전 가봘어요",
          "original_form": "아 저 대전 가봘어요",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100040314.1.20",
          "form": "엑스포 공원 !",
          "original_form": "엑스포 공원 !",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100040314.1.21",
          "form": "종이에 다 적었어요",
          "original_form": "종이에 다 적었어요",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100040314.1.22",
          "form": "엑스포 공원 한빛탑남아있고 나머지 부지에 몇일전에 신세계백화점 아주크게들어왔어요",
          "original_form": "엑스포 공원 한빛탑남아있고 나머지 부지에 몇일전에 신세계백화점 아주크게들어왔어요",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100040314.1.23",
          "form": "호텔.아쿠아리움등 다양하게들어와서 볼걸이생겼네요",
          "original_form": "호텔.아쿠아리움등 다양하게들어와서 볼걸이생겼네요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100040314.1.24",
          "form": "호텔은 37층정도 되는것같아 대전야경을 볼수있구요..바로 갑천이라는 강이흘러서 멋있어요",
          "original_form": "호텔은 37층정도 되는것같아 대전야경을 볼수있구요..바로 갑천이라는 강이흘러서 멋있어요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100040314.1.25",
          "form": "오 신세계 군요",
          "original_form": "오 신세계 군요",
          "speaker_id": "1",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100040314.1.26",
          "form": "잘적어놨으니 10월에 꼭 가볼께요",
          "original_form": "잘적어놨으니 10월에 꼭 가볼께요",
