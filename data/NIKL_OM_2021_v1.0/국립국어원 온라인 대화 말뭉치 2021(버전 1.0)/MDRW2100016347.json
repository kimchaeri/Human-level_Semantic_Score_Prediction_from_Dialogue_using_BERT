        },
        {
          "id": "MDRW2100016347.1.2",
          "form": "안녕하세요 미용에 관심있으세요?",
          "original_form": "안녕하세요 미용에 관심있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016347.1.3",
          "form": "아니요 님은요?",
          "original_form": "아니요 님은요?",
          "speaker_id": "1",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016347.1.4",
          "form": "미용실 간 지 좀 돼서 가려고요",
          "original_form": "미용실 간 지 좀 돼서 가려고요",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016347.1.5",
          "form": "어떤 헤어스타일 원하세요?",
          "original_form": "어떤 헤어스타일 원하세요?",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016347.1.6",
          "form": "이제 가을이니 웨이브하려고요",
          "original_form": "이제 가을이니 웨이브하려고요",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016347.1.7",
          "form": "자주가는 미용실있나요?",
          "original_form": "자주가는 미용실있나요?",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016347.1.8",
          "form": "집앞 상가에 가요",
          "original_form": "집앞 상가에 가요",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016347.1.9",
          "form": "건강 챙기시나요?",
          "original_form": "건강 챙기시나요?",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016347.1.10",
          "form": "비타민 챙겨먹어요",
          "original_form": "비타민 챙겨먹어요",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016347.1.11",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
