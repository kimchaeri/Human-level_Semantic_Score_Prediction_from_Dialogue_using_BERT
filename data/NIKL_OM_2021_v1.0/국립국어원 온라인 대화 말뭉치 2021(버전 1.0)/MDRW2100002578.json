      "utterance": [
        {
          "id": "MDRW2100002578.1.1",
          "form": "날씨어때요?",
          "original_form": "날씨어때요?",
          "speaker_id": "1",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002578.1.2",
          "form": "여기 날씨 흐립니다!",
          "original_form": "여기 날씨 흐립니다!",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002578.1.3",
          "form": "2일째 흐려요 ㅠㅠ",
          "original_form": "2일째 흐려요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002578.1.4",
          "form": "여긴 맑아요",
          "original_form": "여긴 맑아요",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002578.1.5",
          "form": "부럽네요~",
          "original_form": "부럽네요~",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002578.1.6",
          "form": "햇빛 좀 보고 싶어요 힝",
          "original_form": "햇빛 좀 보고 싶어요 힝",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002578.1.7",
          "form": "소나기도 많이와요",
          "original_form": "소나기도 많이와요",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002578.1.8",
          "form": "요즘 날씨가 변덕스러워서",
          "original_form": "요즘 날씨가 변덕스러워서",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002578.1.9",
          "form": "소나기가 너무 급작스럽게 와요~ㅠㅠ",
          "original_form": "소나기가 너무 급작스럽게 와요~ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002578.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002578.1.11",
          "form": "그나마 요즘",
          "original_form": "그나마 요즘",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002578.1.12",
          "form": "여름 슬슬",
          "original_form": "여름 슬슬",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002578.1.13",
          "form": "탈출할려는거 같아서",
          "original_form": "탈출할려는거 같아서",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002578.1.14",
          "form": "좀 다행이지만요~",
          "original_form": "좀 다행이지만요~",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002578.1.15",
          "form": "네 선선하죠",
          "original_form": "네 선선하죠",
          "speaker_id": "1",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002578.1.16",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002578.1.17",
          "form": "슬슬 가을날씨 올려나봐요 ㅎㅎ",
          "original_form": "슬슬 가을날씨 올려나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002578.1.18",
          "form": "낮에도 선선했으면",
          "original_form": "낮에도 선선했으면",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002578.1.19",
          "form": "낮에는 여전히 덥긴하죠 ㅠㅠ",
          "original_form": "낮에는 여전히 덥긴하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002578.1.20",
          "form": "얼른 여름지나갔으면 좋겠어요~",
          "original_form": "얼른 여름지나갔으면 좋겠어요~",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002578.1.21",
          "form": "저두",
