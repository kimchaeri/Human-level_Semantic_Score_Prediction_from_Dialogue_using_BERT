      "utterance": [
        {
          "id": "MDRW2100025362.1.1",
          "form": "요즘 곰표 마케팅이 대세더라구요",
          "original_form": "요즘 곰표 마케팅이 대세더라구요",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025362.1.2",
          "form": "예 그렇더라구요",
          "original_form": "예 그렇더라구요",
          "speaker_id": "2",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025362.1.3",
          "form": "맥주가 맛있다던데 저는 맥주를 못먹어서 아쉽네요",
          "original_form": "맥주가 맛있다던데 저는 맥주를 못먹어서 아쉽네요",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025362.1.4",
          "form": "저도 못마셔봤어요 인기가많아 없어서 마시지못한다는 얘기들었어요",
          "original_form": "저도 못마셔봤어요 인기가많아 없어서 마시지못한다는 얘기들었어요",
          "speaker_id": "2",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025362.1.5",
          "form": "그러게요 무슨 시멘트 회사 콜라보 팝콘도 봤어요",
          "original_form": "그러게요 무슨 시멘트 회사 콜라보 팝콘도 봤어요",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025362.1.6",
          "form": "그런가요? 신선하네요",
          "original_form": "그런가요? 신선하네요",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025362.1.7",
          "form": "시멘트 포대만큼 크더라구요",
          "original_form": "시멘트 포대만큼 크더라구요",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025362.1.8",
          "form": "진로 두꺼비도 귀여워서 인기가 많더라구요",
          "original_form": "진로 두꺼비도 귀여워서 인기가 많더라구요",
          "speaker_id": "1",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025362.1.9",
          "form": "와 눅눅해지진않을까요",
          "original_form": "와 눅눅해지진않을까요",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025362.1.10",
          "form": "그러게요 피규어로도 판매하고있던데 귀엽네요",
          "original_form": "그러게요 피규어로도 판매하고있던데 귀엽네요",
          "speaker_id": "2",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025362.1.11",
          "form": "소주병처럼 생긴 방향제 있던데 그건 쫌 위험한거 같아요",
          "original_form": "소주병처럼 생긴 방향제 있던데 그건 쫌 위험한거 같아요",
          "speaker_id": "1",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025362.1.12",
          "form": "맞아요 저도봤어요",
          "original_form": "맞아요 저도봤어요",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025362.1.13",
          "form": "너무 똑같이 생겨서 모르고 마실것같아요",
          "original_form": "너무 똑같이 생겨서 모르고 마실것같아요",
          "speaker_id": "2",
          "time": "20210901 10:29"
        },
        {
          "id": "MDRW2100025362.1.14",
          "form": "나이있으신 분들은 글자 작아서 읽기 힘드실것같네요",
          "original_form": "나이있으신 분들은 글자 작아서 읽기 힘드실것같네요",
          "speaker_id": "1",
          "time": "20210901 10:30"
        },
        {
          "id": "MDRW2100025362.1.15",
          "form": "너무 과도한 콜라보는 금지했으면 하네요",
          "original_form": "너무 과도한 콜라보는 금지했으면 하네요",
          "speaker_id": "1",
          "time": "20210901 10:30"
        },
        {
          "id": "MDRW2100025362.1.16",
          "form": "맞아요 소비자를 더욱 생각하고 마케팅에 임했으면 좋겠네요",
          "original_form": "맞아요 소비자를 더욱 생각하고 마케팅에 임했으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025362.1.17",
          "form": "스팸 에어팟 케이스도 귀엽더라구요",
          "original_form": "스팸 에어팟 케이스도 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210901 10:35"
        }
