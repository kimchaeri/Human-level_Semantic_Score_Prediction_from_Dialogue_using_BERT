      "utterance": [
        {
          "id": "MDRW2100045979.1.1",
          "form": "시작하려면 한과도 배워야 할 텐데 어떤 거부터 시작해야 될지 모르겠네요ㅠㅠ",
          "original_form": "시작하려면 한과도 배워야 할 텐데 어떤 거부터 시작해야 될지 모르겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045979.1.2",
          "form": "그런 점도 있겠군요.. 한과장인 수제자로 받아달라 해야 하나",
          "original_form": "그런 점도 있겠군요.. 한과장인 수제자로 받아달라 해야 하나",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045979.1.3",
          "form": "졸업했어도 학교 교수님한테 연락 안 되료나요ㅋㅌㅌㅋ",
          "original_form": "졸업했어도 학교 교수님한테 연락 안 되료나요ㅋㅌㅌㅋ",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045979.1.4",
          "form": "아 아 학교에 전화하고 싶지 않아요 정말 전화하고 싶지 않아요 학교랑은 모른 척하고 싶어요",
          "original_form": "아 아 학교에 전화하고 싶지 않아요 정말 전화하고 싶지 않아요 학교랑은 모른 척하고 싶어요",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045979.1.5",
          "form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045979.1.6",
          "form": "엄청난 부정",
          "original_form": "엄청난 부정",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045979.1.7",
          "form": "저 졸업하고 나서 학교에 전화 와 가지고 차단했어요ㅋㅋㅋㅋ",
          "original_form": "저 졸업하고 나서 학교에 전화 와 가지고 차단했어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045979.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ취업조사하려고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ취업조사하려고",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045979.1.9",
          "form": "전화 겁나 오지 않나요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전화 겁나 오지 않나요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045979.1.10",
          "form": "저도 그래서 차단했는디",
          "original_form": "저도 그래서 차단했는디",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045979.1.11",
          "form": "헐 저만 그런게 아니었군요 맞아요 취업 때문에 엄청 전화 와요...",
          "original_form": "헐 저만 그런게 아니었군요 맞아요 취업 때문에 엄청 전화 와요...",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045979.1.12",
          "form": "그니까요 일 알아서 구하게 놔 둬 아악",
          "original_form": "그니까요 일 알아서 구하게 놔 둬 아악",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045979.1.13",
          "form": "그니까요ㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045979.1.14",
          "form": "알마나 많은 사람들이 전공으로 간다고 그걸 전화해 가지고 아휴...",
          "original_form": "알마나 많은 사람들이 전공으로 간다고 그걸 전화해 가지고 아휴...",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045979.1.15",
          "form": "긍께요ㅋㅋㅋㅋ 주책이여",
          "original_form": "긍께요ㅋㅋㅋㅋ 주책이여",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045979.1.16",
          "form": "하 벌써 해가 다 떴어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "하 벌써 해가 다 떴어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045979.1.17",
          "form": "아침 먹어야 하는데",
          "original_form": "아침 먹어야 하는데",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045979.1.18",
          "form": "name2님은 아침 뭐 드실거예요?",
          "original_form": "&name2&님은 아침 뭐 드실거예요?",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045979.1.19",
          "form": "또 맞춰볼까요 ㅋㅌㅌㅋ",
          "original_form": "또 맞춰볼까요 ㅋㅌㅌㅋ",
