      "utterance": [
        {
          "id": "MDRW2100044200.1.1",
          "form": "과자 다먹어가니까 하나 더 먹고싶다..",
          "original_form": "과자 다먹어가니까 하나 더 먹고싶다..",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044200.1.2",
          "form": "돼지야?",
          "original_form": "돼지야?",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044200.1.3",
          "form": "돼지 아닌데 ㅠㅠ",
          "original_form": "돼지 아닌데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044200.1.4",
          "form": "살찐다",
          "original_form": "살찐다",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044200.1.5",
          "form": "그렇겠지?ㅠㅠ",
          "original_form": "그렇겠지?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044200.1.6",
          "form": "그래 참아라",
          "original_form": "그래 참아라",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044200.1.7",
          "form": "하나만 더 먹을게..",
          "original_form": "하나만 더 먹을게..",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044200.1.8",
          "form": "과자가 더 있나?",
          "original_form": "과자가 더 있나?",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044200.1.9",
          "form": "웅웅.. 작은거..",
          "original_form": "웅웅.. 작은거..",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044200.1.10",
          "form": "그래 그거만 먹고 양치해",
          "original_form": "그래 그거만 먹고 양치해",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044200.1.11",
          "form": "그래야겠다 ㅋㅋ",
          "original_form": "그래야겠다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044200.1.12",
          "form": "딸램 살빼야지",
          "original_form": "딸램 살빼야지",
