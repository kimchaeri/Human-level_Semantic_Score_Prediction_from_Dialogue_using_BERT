      "utterance": [
        {
          "id": "MDRW2100017858.1.1",
          "form": "안뇽하세요",
          "original_form": "안뇽하세요",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017858.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017858.1.3",
          "form": "코로나 때문에 여행을 못가고 있는데\n코로나가 끝나면 어디로 가고싶으세요?",
          "original_form": "코로나 때문에 여행을 못가고 있는데\n코로나가 끝나면 어디로 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017858.1.4",
          "form": "해외로 갈까 생각중이야",
          "original_form": "해외로 갈까 생각중이야",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017858.1.5",
          "form": "해외 어디로 가고싶으신데요?",
          "original_form": "해외 어디로 가고싶으신데요?",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017858.1.6",
          "form": "캐나다",
          "original_form": "캐나다",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017858.1.7",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017858.1.8",
          "form": "왜 캐나다로 가고싶으세요?",
          "original_form": "왜 캐나다로 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017858.1.9",
          "form": "겨울에 오로라를 보러 가고 싶어",
          "original_form": "겨울에 오로라를 보러 가고 싶어",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017858.1.10",
          "form": "오로라 좋죠",
          "original_form": "오로라 좋죠",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017858.1.11",
          "form": "오로라 보는거 말고 캐나다 다른곳 가고싶은 곳은 없나요?",
          "original_form": "오로라 보는거 말고 캐나다 다른곳 가고싶은 곳은 없나요?",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017858.1.12",
          "form": "나이아가라 폭포도 가고 싶어",
          "original_form": "나이아가라 폭포도 가고 싶어",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017858.1.13",
          "form": "미국도 나이아가라 폭포가 있는데 캐나다에서 가고싶은 이유가 있으신가요?",
          "original_form": "미국도 나이아가라 폭포가 있는데 캐나다에서 가고싶은 이유가 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017858.1.14",
          "form": "미국보다 캐나다에서 보는 나이아가라 폭포가 예쁘다고 해서 그래",
          "original_form": "미국보다 캐나다에서 보는 나이아가라 폭포가 예쁘다고 해서 그래",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017858.1.15",
          "form": "저도 나이아가라 폭포 보러 가고싶네요!",
          "original_form": "저도 나이아가라 폭포 보러 가고싶네요!",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017858.1.16",
          "form": "코로나 끝나면 같이 가자^^",
          "original_form": "코로나 끝나면 같이 가자^^",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017858.1.17",
          "form": "좋아요~!",
          "original_form": "좋아요~!",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017858.1.18",
          "form": "그리고 몬트리올 성당도 가고싶어",
          "original_form": "그리고 몬트리올 성당도 가고싶어",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017858.1.19",
          "form": "오 검색해 보니 정말 멋진 성당이네요~",
          "original_form": "오 검색해 보니 정말 멋진 성당이네요~",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017858.1.20",
          "form": "캐나다 여행가면 다양하게 즐길 수 있어서 벌써부터 기대돼~",
          "original_form": "캐나다 여행가면 다양하게 즐길 수 있어서 벌써부터 기대돼~",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017858.1.21",
          "form": "캐나다는 겨울인 이미지가 강해서 이렇게 다양하게 관광할 수 있는지 처음 알았어요",
          "original_form": "캐나다는 겨울인 이미지가 강해서 이렇게 다양하게 관광할 수 있는지 처음 알았어요",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017858.1.22",
          "form": "예전에 언니가 캐나다 가서 사다줬던 캐나다 초콜릿도 맛있었는데~!",
          "original_form": "예전에 언니가 캐나다 가서 사다줬던 캐나다 초콜릿도 맛있었는데~!",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017858.1.23",
          "form": "맞아! 맛있어서 아직도 기억에 남는 맛이야!",
          "original_form": "맞아! 맛있어서 아직도 기억에 남는 맛이야!",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017858.1.24",
          "form": "코로나 끝나고 가게되면 그 초콜릿 다시 먹어보고 싶어요~",
          "original_form": "코로나 끝나고 가게되면 그 초콜릿 다시 먹어보고 싶어요~",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017858.1.25",
          "form": "그래! 코로나 끝나면 꼭 그 초콜릿 맛보고 관광하러 가자^^",
          "original_form": "그래! 코로나 끝나면 꼭 그 초콜릿 맛보고 관광하러 가자^^",
          "speaker_id": "2",
          "time": "20210826 21:37"
        }
      ]
