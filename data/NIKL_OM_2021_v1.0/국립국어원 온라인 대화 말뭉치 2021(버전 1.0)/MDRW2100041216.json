        },
        {
          "id": "MDRW2100041216.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇군욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇군욬ㅋㅋㅋ",
