          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100006202.1.8",
          "form": "자긴 그렇게 폰이 좋아?",
          "original_form": "자긴 그렇게 폰이 좋아?",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100006202.1.9",
          "form": "응 ㅋㅋㅋ",
          "original_form": "응 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100006202.1.10",
          "form": "난 이런 새기기 쓰는거 좋더라",
          "original_form": "난 이런 새기기 쓰는거 좋더라",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100006202.1.11",
          "form": "플립이 이쁜거 같애",
          "original_form": "플립이 이쁜거 같애",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006202.1.12",
          "form": "나도 그런거 같아 ㅋㅋㅋ",
          "original_form": "나도 그런거 같아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006202.1.13",
          "form": "이번에  디자인 잘뽑았어",
          "original_form": "이번에  디자인 잘뽑았어",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006202.1.14",
          "form": "그런거 같긴 해",
          "original_form": "그런거 같긴 해",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006202.1.15",
          "form": "없어서 못산다며",
          "original_form": "없어서 못산다며",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006202.1.16",
          "form": "난 사전예약으로 샀지",
          "original_form": "난 사전예약으로 샀지",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006202.1.17",
          "form": "진짜 많이 팔렸나봐",
          "original_form": "진짜 많이 팔렸나봐",
