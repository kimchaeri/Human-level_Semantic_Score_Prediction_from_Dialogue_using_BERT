      "utterance": [
        {
          "id": "MDRW2100039849.1.1",
          "form": "학생이신가요",
          "original_form": "학생이신가요",
          "speaker_id": "1",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039849.1.2",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "2",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039849.1.3",
          "form": "과제 중이신가요?",
          "original_form": "과제 중이신가요?",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039849.1.4",
          "form": "다 하셨나요?",
          "original_form": "다 하셨나요?",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039849.1.5",
          "form": "아뇨 수다중이요",
          "original_form": "아뇨 수다중이요",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039849.1.6",
          "form": "교수님이 선하신가 봐요",
          "original_form": "교수님이 선하신가 봐요",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039849.1.7",
          "form": "전혀요",
          "original_form": "전혀요",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039849.1.8",
          "form": "이제 할거에요 밤새서",
          "original_form": "이제 할거에요 밤새서",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039849.1.9",
          "form": "젊게 사시네요 밤도 새고",
          "original_form": "젊게 사시네요 밤도 새고",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039849.1.10",
          "form": "넹 좀 젊어요 제가 혹시 늙으셨어요?",
          "original_form": "넹 좀 젊어요 제가 혹시 늙으셨어요?",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039849.1.11",
          "form": "먹을 만큼 먹었어요 저는 회사원이에요",
          "original_form": "먹을 만큼 먹었어요 저는 회사원이에요",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039849.1.12",
          "form": "반모 되나요?",
          "original_form": "반모 되나요?",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039849.1.13",
          "form": "반모가 뭐죠",
          "original_form": "반모가 뭐죠",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039849.1.14",
          "form": "반 모임인가요",
          "original_form": "반 모임인가요",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039849.1.15",
          "form": "나이가 좀 있으신가 보네요",
          "original_form": "나이가 좀 있으신가 보네요",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039849.1.16",
          "form": "신조어 못 알아듣고",
          "original_form": "신조어 못 알아듣고",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039849.1.17",
          "form": "기분이 나쁘군요",
          "original_form": "기분이 나쁘군요",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039849.1.18",
          "form": "반모는 반말모드입니다",
          "original_form": "반모는 반말모드입니다",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039849.1.19",
          "form": "발표 준비는 잘 되고 있죠?",
          "original_form": "발표 준비는 잘 되고 있죠?",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039849.1.20",
          "form": "네 대충 발표할 수 있을 정도만 해놓고 퇴근했습니다",
          "original_form": "네 대충 발표할 수 있을 정도만 해놓고 퇴근했습니다",
          "speaker_id": "2",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100039849.1.21",
          "form": "일단 구색은 갖춘거죠",
          "original_form": "일단 구색은 갖춘거죠",
          "speaker_id": "2",
          "time": "20210904 01:42"
        }
