      "utterance": [
        {
          "id": "MDRW2100004649.1.1",
          "form": "안녕하세요 옷 좋아하세요?",
          "original_form": "안녕하세요 옷 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004649.1.2",
          "form": "안녕하세요!! 막 꾸며입는 편은 아닌데 좋아하는 스타일은 있어요",
          "original_form": "안녕하세요!! 막 꾸며입는 편은 아닌데 좋아하는 스타일은 있어요",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004649.1.3",
          "form": "무슨 스타일 좋아하세요??",
          "original_form": "무슨 스타일 좋아하세요??",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004649.1.4",
          "form": "저도 꾸안꾸 느낌 그런거 좋아해요 ㅎㅎ",
          "original_form": "저도 꾸안꾸 느낌 그런거 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004649.1.5",
          "form": "저 그냥 체크셔츠랑 후드티 청바지 이런거 좋아하는데 친구들이 제가 체크셔츠 입은거 보면 엄청 싫어해요",
          "original_form": "저 그냥 체크셔츠랑 후드티 청바지 이런거 좋아하는데 친구들이 제가 체크셔츠 입은거 보면 엄청 싫어해요",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.6",
          "form": "ㅋㅋㅋ 공대패션이라서 그런거 아닐까요",
          "original_form": "ㅋㅋㅋ 공대패션이라서 그런거 아닐까요",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.7",
          "form": "공대생인거 티내냐고",
          "original_form": "공대생인거 티내냐고",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.8",
          "form": "저도 전에 체크셔츠 한 번 입었다가",
          "original_form": "저도 전에 체크셔츠 한 번 입었다가",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.9",
          "form": "공대생소리들었네요",
          "original_form": "공대생소리들었네요",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.10",
          "form": "아니 근데 진짜 이쁘지 않아요?",
          "original_form": "아니 근데 진짜 이쁘지 않아요?",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.11",
          "form": "공대생이세요?",
          "original_form": "공대생이세요?",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.12",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.13",
          "form": "그니까요 이쁜건 이쁘던데 ㅠ",
          "original_form": "그니까요 이쁜건 이쁘던데 ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.14",
          "form": "학교 갈때는 그래서 잘 안입게 돼요",
          "original_form": "학교 갈때는 그래서 잘 안입게 돼요",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.15",
          "form": "그리고 저도 후드티 엄청 좋아해요ㅎㅎ",
          "original_form": "그리고 저도 후드티 엄청 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004649.1.16",
          "form": "ㅋㅋㅋ전 그런거 신경안쓰고",
          "original_form": "ㅋㅋㅋ전 그런거 신경안쓰고",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004649.1.17",
          "form": "공대 건물 들어가기 눈치보여요...",
          "original_form": "공대 건물 들어가기 눈치보여요...",
          "speaker_id": "2",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004649.1.18",
          "form": "그냥 입어요",
          "original_form": "그냥 입어요",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004649.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004649.1.20",
          "form": "아 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004649.1.21",
          "form": "근데 확실히",
          "original_form": "근데 확실히",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004649.1.22",
          "form": "전보다는 안사게되긴하더라구요",
          "original_form": "전보다는 안사게되긴하더라구요",
          "speaker_id": "1",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004649.1.23",
          "form": "맞아요ㅋㅋㅋㅋ 여전히 체크셔츠보면 이쁘당...갖고싶당...하기는 하는데 잘사지는 않는거 같아요",
          "original_form": "맞아요ㅋㅋㅋㅋ 여전히 체크셔츠보면 이쁘당...갖고싶당...하기는 하는데 잘사지는 않는거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004649.1.24",
          "form": "친구들이랑 같이 가면",
          "original_form": "친구들이랑 같이 가면",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004649.1.25",
          "form": "애들이 안돼~이러고 다른곳으로 끌고가요ㅋㄱㅋ",
          "original_form": "애들이 안돼~이러고 다른곳으로 끌고가요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004649.1.26",
          "form": "아고 ㅋㅋㅋ ㅠㅠ",
          "original_form": "아고 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004649.1.27",
          "form": "쇼핑하실때 오프라인으로 자주 하시나봐요!",
          "original_form": "쇼핑하실때 오프라인으로 자주 하시나봐요!",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004649.1.28",
          "form": "뭐가나쁜말이지",
          "original_form": "뭐가나쁜말이지",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004649.1.29",
          "form": "네네 인터넷보다는 직접 입어보고 사는걸 더 선호해요",
          "original_form": "네네 인터넷보다는 직접 입어보고 사는걸 더 선호해요",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004649.1.30",
          "form": "제가 섬유재질에 좀 민감해서",
          "original_form": "제가 섬유재질에 좀 민감해서",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004649.1.31",
          "form": "아 저는 인터넷쇼핑을 더 자주해요",
          "original_form": "아 저는 인터넷쇼핑을 더 자주해요",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004649.1.32",
          "form": "만져보고 사야해요",
          "original_form": "만져보고 사야해요",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004649.1.33",
          "form": "오 섬유재질 중요하죠",
          "original_form": "오 섬유재질 중요하죠",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004649.1.34",
          "form": "저는 그런거 다 귀찮아서 ㅋㅋㅋ",
          "original_form": "저는 그런거 다 귀찮아서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004649.1.35",
          "form": "상세보기같은걸로만 대충 보고사는편이네요",
          "original_form": "상세보기같은걸로만 대충 보고사는편이네요",
          "speaker_id": "1",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004649.1.36",
          "form": "직접보고 사는게 더 좋긴하죠 ㅠㅜ",
          "original_form": "직접보고 사는게 더 좋긴하죠 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004649.1.37",
          "form": "아아ㅋㅋㅋㅋㅋㅋ 저는 좀 알러지가 올라와서",
          "original_form": "아아ㅋㅋㅋㅋㅋㅋ 저는 좀 알러지가 올라와서",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004649.1.38",
          "form": "오프에서 입어보고 좀 따가우면",
          "original_form": "오프에서 입어보고 좀 따가우면",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004649.1.39",
          "form": "바로 내려놔요",
          "original_form": "바로 내려놔요",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004649.1.40",
          "form": "아 그럼 중요하죠 ㅜㅜ",
          "original_form": "아 그럼 중요하죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004649.1.41",
          "form": "요새 모달소재라고 또 나오던데 한 번 찾아보세요!",
          "original_form": "요새 모달소재라고 또 나오던데 한 번 찾아보세요!",
          "speaker_id": "1",
          "time": "20210821 18:18"
        }
      ]
