      "utterance": [
        {
          "id": "MDRW2100018012.1.1",
          "form": "지마켓 쓰시는구나",
          "original_form": "지마켓 쓰시는구나",
          "speaker_id": "2",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100018012.1.2",
          "form": "어디 쓰시나요?",
          "original_form": "어디 쓰시나요?",
          "speaker_id": "1",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100018012.1.3",
          "form": "저도 네이버쇼핑 잘 이용해요 네이버쇼핑이 편해서요",
          "original_form": "저도 네이버쇼핑 잘 이용해요 네이버쇼핑이 편해서요",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100018012.1.4",
          "form": "위메프를 이용할때도 있구요",
          "original_form": "위메프를 이용할때도 있구요",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100018012.1.5",
          "form": "위메프 전 가입이 안되어있고 티몬 씁니다",
          "original_form": "위메프 전 가입이 안되어있고 티몬 씁니다",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100018012.1.6",
          "form": "최근에 지마켓에서 사신거 있으신가요?",
          "original_form": "최근에 지마켓에서 사신거 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018012.1.7",
          "form": "지금 막 닭갈비 주문할까 생각중이에요",
          "original_form": "지금 막 닭갈비 주문할까 생각중이에요",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100018012.1.8",
          "form": "저는 얼마전에 옷을 샀어요:)",
          "original_form": "저는 얼마전에 옷을 샀어요:)",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100018012.1.9",
          "form": "위아래로 받쳐서 입을 수 있게요",
          "original_form": "위아래로 받쳐서 입을 수 있게요",
          "speaker_id": "2",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018012.1.10",
          "form": "오! 가을맞이!!!",
          "original_form": "오! 가을맞이!!!",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018012.1.11",
          "form": "언데웨어라서 가을맞이는 아니예요",
          "original_form": "언데웨어라서 가을맞이는 아니예요",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018012.1.12",
          "form": "뭐든 사면 기분이 좋죠!",
          "original_form": "뭐든 사면 기분이 좋죠!",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018012.1.13",
          "form": "네 인정합니다",
          "original_form": "네 인정합니다",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018012.1.14",
          "form": "소소하게 구매하는게 가끔은 기분좋을 때도 있지요",
          "original_form": "소소하게 구매하는게 가끔은 기분좋을 때도 있지요",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018012.1.15",
          "form": "선물받아서 기분좋았던게 있으신가요.",
          "original_form": "선물받아서 기분좋았던게 있으신가요.",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018012.1.16",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018012.1.17",
          "form": "현금이랄까요...",
          "original_form": "현금이랄까요...",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018012.1.18",
          "form": "너무 솔직했죠..",
          "original_form": "너무 솔직했죠..",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018012.1.19",
          "form": "아니요ㅋㅋㅋㅋㅋ 현금을 싫어하는 사람은 없죠 ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋ 현금을 싫어하는 사람은 없죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018012.1.20",
          "form": "귀엽네요 ㅎㅎ",
          "original_form": "귀엽네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018012.1.21",
          "form": "닭갈비는 사셨나요?",
          "original_form": "닭갈비는 사셨나요?",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018012.1.22",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018012.1.23",
          "form": "엄마 영양제 보고있어요",
          "original_form": "엄마 영양제 보고있어요",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018012.1.24",
          "form": "영양제요?",
          "original_form": "영양제요?",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018012.1.25",
          "form": "어떤걸 보시나요? 비타민인가요?",
          "original_form": "어떤걸 보시나요? 비타민인가요?",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018012.1.26",
          "form": "네 비타민디 다 드셨다고..",
          "original_form": "네 비타민디 다 드셨다고..",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018012.1.27",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210826 22:41"
        }
      ]
    }
  ]
