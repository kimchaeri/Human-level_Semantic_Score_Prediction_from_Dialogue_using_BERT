      "utterance": [
        {
          "id": "MDRW2100030679.1.1",
          "form": "매우 당황하셨을 것 같아요",
          "original_form": "매우 당황하셨을 것 같아요",
          "speaker_id": "2",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030679.1.2",
          "form": "아빠가 예약할 때 통화한 내용을 녹음해 두었어요",
          "original_form": "아빠가 예약할 때 통화한 내용을 녹음해 두었어요",
          "speaker_id": "1",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030679.1.3",
          "form": "녹음한 내용에 생선이름은 없었지요",
          "original_form": "녹음한 내용에 생선이름은 없었지요",
          "speaker_id": "1",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.4",
          "form": "그래서 어떻게 되었나요",
          "original_form": "그래서 어떻게 되었나요",
          "speaker_id": "2",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.5",
          "form": "그쪽에 들려 줘 않나요",
          "original_form": "그쪽에 들려 줘 않나요",
          "speaker_id": "2",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.6",
          "form": "이미 먹었으니 다 먹고 난 후에 금액을 받았지요",
          "original_form": "이미 먹었으니 다 먹고 난 후에 금액을 받았지요",
          "speaker_id": "1",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.7",
          "form": "먹었으니 냈지요",
          "original_form": "먹었으니 냈지요",
          "speaker_id": "1",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.8",
          "form": "아 저런 안타까운 일이",
          "original_form": "아 저런 안타까운 일이",
          "speaker_id": "2",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.9",
          "form": "아빠가 또 그런 건 잘못 따져요",
          "original_form": "아빠가 또 그런 건 잘못 따져요",
          "speaker_id": "1",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.10",
          "form": "13만 원",
          "original_form": "13만 원",
          "speaker_id": "1",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.11",
          "form": "안타깝네요",
          "original_form": "안타깝네요",
          "speaker_id": "2",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.12",
          "form": "너무 비싸요",
          "original_form": "너무 비싸요",
          "speaker_id": "2",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.13",
          "form": "네 접시는 먹겠네요",
          "original_form": "네 접시는 먹겠네요",
          "speaker_id": "1",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.14",
          "form": "무슨 생선인지 궁금하네요",
          "original_form": "무슨 생선인지 궁금하네요",
          "speaker_id": "1",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030679.1.15",
          "form": "너는 시키면 회사 작은 걸로만 시켜요",
          "original_form": "너는 시키면 회사 작은 걸로만 시켜요",
          "speaker_id": "2",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030679.1.16",
          "form": "그래서 아빠가 배달 해 먹자고 하네요",
          "original_form": "그래서 아빠가 배달 해 먹자고 하네요",
          "speaker_id": "1",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030679.1.17",
          "form": "생선 구이 였나요 왜 그렇게 비싸죠",
          "original_form": "생선 구이 였나요 왜 그렇게 비싸죠",
          "speaker_id": "2",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030679.1.18",
          "form": "가격 그대로 나오니까",
          "original_form": "가격 그대로 나오니까",
          "speaker_id": "1",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030679.1.19",
          "form": "회를 먹었어요",
          "original_form": "회를 먹었어요",
          "speaker_id": "1",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030679.1.20",
          "form": "특별히 주문한 회라고 한 그 생선",
          "original_form": "특별히 주문한 회라고 한 그 생선",
          "speaker_id": "1",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030679.1.21",
          "form": "아 회를 생선 이 였군요",
          "original_form": "아 회를 생선 이 였군요",
          "speaker_id": "2",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030679.1.22",
          "form": "그렇다면 원래 주문한 사람은 뭘 먹었을까요",
          "original_form": "그렇다면 원래 주문한 사람은 뭘 먹었을까요",
          "speaker_id": "1",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030679.1.23",
          "form": "맛없는 식당도 많지요",
          "original_form": "맛없는 식당도 많지요",
          "speaker_id": "1",
          "time": "20210904 11:22"
        }
      ]
