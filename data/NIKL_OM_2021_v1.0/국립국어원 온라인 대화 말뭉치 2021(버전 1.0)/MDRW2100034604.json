      "utterance": [
        {
          "id": "MDRW2100034604.1.1",
          "form": "그니까요 딱 걸리기 좋은 시즌이죠ㅠ",
          "original_form": "그니까요 딱 걸리기 좋은 시즌이죠ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034604.1.2",
          "form": "가족들도 모이고",
          "original_form": "가족들도 모이고",
          "speaker_id": "2",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034604.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034604.1.4",
          "form": "코로나가 언제끝날지 ㅠㅜ",
          "original_form": "코로나가 언제끝날지 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034604.1.5",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100034604.1.6",
          "form": "그럼 2차는",
          "original_form": "그럼 2차는",
          "speaker_id": "2",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100034604.1.7",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100034604.1.8",
          "form": "10월 중순에 맞으시겠네요?",
          "original_form": "10월 중순에 맞으시겠네요?",
          "speaker_id": "2",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100034604.1.9",
          "form": "네 원래 11월초였는데",
          "original_form": "네 원래 11월초였는데",
          "speaker_id": "1",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100034604.1.10",
          "form": "그쯤으로 바뀌었죠",
          "original_form": "그쯤으로 바뀌었죠",
          "speaker_id": "1",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100034604.1.11",
          "form": "진짜 많이 당겨지긴했네요 다행히도ㅠ",
          "original_form": "진짜 많이 당겨지긴했네요 다행히도ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100034604.1.12",
          "form": "맞아요 11월은 너무해요 ㅋㅋㅋ",
          "original_form": "맞아요 11월은 너무해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100034604.1.13",
          "form": "주변에 코로나확진되신분 있으세요?",
          "original_form": "주변에 코로나확진되신분 있으세요?",
          "speaker_id": "2",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100034604.1.14",
          "form": "마자요 너무 늦죠ㅠ",
          "original_form": "마자요 너무 늦죠ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100034604.1.15",
          "form": "다행히 없어요",
          "original_form": "다행히 없어요",
          "speaker_id": "1",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100034604.1.16",
          "form": "있으세요??",
          "original_form": "있으세요??",
          "speaker_id": "1",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100034604.1.17",
          "form": "아니요 다행히도 없어요",
          "original_form": "아니요 다행히도 없어요",
          "speaker_id": "2",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100034604.1.18",
          "form": "근데 건너건너 얘기는 들리더라구요ㅠ",
          "original_form": "근데 건너건너 얘기는 들리더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100034604.1.19",
          "form": "아 ㅜㅜ 무섭겠어요",
          "original_form": "아 ㅜㅜ 무섭겠어요",
          "speaker_id": "1",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100034604.1.20",
          "form": "이제 점점 먼얘기가 아닌거같아요ㅠ",
          "original_form": "이제 점점 먼얘기가 아닌거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:13"
        },
        {
          "id": "MDRW2100034604.1.21",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
