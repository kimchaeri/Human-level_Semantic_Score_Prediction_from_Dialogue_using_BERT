      "utterance": [
        {
          "id": "MDRW2100024825.1.1",
          "form": "키우는 동물있으신가요",
          "original_form": "키우는 동물있으신가요",
          "speaker_id": "2",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100024825.1.2",
          "form": "아뇨 요즘 반려동물 산업이 엄청 커지고 있대요",
          "original_form": "아뇨 요즘 반려동물 산업이 엄청 커지고 있대요",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024825.1.3",
          "form": "저도 키워본적이없어요",
          "original_form": "저도 키워본적이없어요",
          "speaker_id": "2",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024825.1.4",
          "form": "개털알러지가 있거든요",
          "original_form": "개털알러지가 있거든요",
          "speaker_id": "2",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024825.1.5",
          "form": "알러지 있는 사람들은 반려 동물 키우기 힘들 거 같아요",
          "original_form": "알러지 있는 사람들은 반려 동물 키우기 힘들 거 같아요",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024825.1.6",
          "form": "이웃이 고양이를 키우는데 아이가 좋아해요",
          "original_form": "이웃이 고양이를 키우는데 아이가 좋아해요",
          "speaker_id": "2",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100024825.1.7",
          "form": "아이들 정서에는 좋은듯해요",
          "original_form": "아이들 정서에는 좋은듯해요",
          "speaker_id": "2",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100024825.1.8",
          "form": "네 아이들 정서에 좋은 거 같아요 그런데 은근히 동물 키우면 밥 주고 똥 싸고 집안 살림",
          "original_form": "네 아이들 정서에 좋은 거 같아요 그런데 은근히 동물 키우면 밥 주고 똥 싸고 집안 살림",
          "speaker_id": "1",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100024825.1.9",
          "form": "동물들이 긁어서 부수고 안 좋은 점도 많죠 그런데 귀엽고 사랑스러우니 키우는 거 같더라고요",
          "original_form": "동물들이 긁어서 부수고 안 좋은 점도 많죠 그런데 귀엽고 사랑스러우니 키우는 거 같더라고요",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024825.1.10",
          "form": "그래도 지나가는 동물들보면넘귀여워요",
          "original_form": "그래도 지나가는 동물들보면넘귀여워요",
          "speaker_id": "2",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100024825.1.11",
          "form": "근데 목줄안하는사람 은근많더라구요",
          "original_form": "근데 목줄안하는사람 은근많더라구요",
          "speaker_id": "2",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100024825.1.12",
          "form": "우리개는 안물어요 족들 말씀이시군요",
          "original_form": "우리개는 안물어요 족들 말씀이시군요",
          "speaker_id": "1",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100024825.1.13",
          "form": "네 언제물지 무섭드라구요",
          "original_form": "네 언제물지 무섭드라구요",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100024825.1.14",
          "form": "네 어릴때 학교가는 길에",
          "original_form": "네 어릴때 학교가는 길에",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100024825.1.15",
          "form": "개가 무서웠던 기억이 있어서요. 작은 개였는데 저도 어렸으니까요...",
          "original_form": "개가 무서웠던 기억이 있어서요. 작은 개였는데 저도 어렸으니까요...",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100024825.1.16",
          "form": "아 많이 놀라셨을듯요",
          "original_form": "아 많이 놀라셨을듯요",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100024825.1.17",
          "form": "일종의 트라우마죠ㅠ",
          "original_form": "일종의 트라우마죠ㅠ",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100024825.1.18",
          "form": "네 거기를 피해서 멀리 돌아갔어요",
          "original_form": "네 거기를 피해서 멀리 돌아갔어요",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100024825.1.19",
          "form": "개보면 항상 조심하게되요",
          "original_form": "개보면 항상 조심하게되요",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100024825.1.20",
          "form": "혹시모르니",
          "original_form": "혹시모르니",
          "speaker_id": "2",
          "time": "20210901 20:44"
        }
      ]
    }
