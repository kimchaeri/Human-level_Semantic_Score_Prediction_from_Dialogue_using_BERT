      "utterance": [
        {
          "id": "MDRW2100016203.1.1",
          "form": "네 악뮤좋아하죠",
          "original_form": "네 악뮤좋아하죠",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016203.1.2",
          "form": "저도요 지금도 듣고있어요",
          "original_form": "저도요 지금도 듣고있어요",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016203.1.3",
          "form": "저는 아이유 노래 듣고있어요",
          "original_form": "저는 아이유 노래 듣고있어요",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016203.1.4",
          "form": "정말 콘서트 가고싶네요ㅜㅜ",
          "original_form": "정말 콘서트 가고싶네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016203.1.5",
          "form": "저도요 아이유 콘서트 악뮤콘서트  다 가고싶네요",
          "original_form": "저도요 아이유 콘서트 악뮤콘서트  다 가고싶네요",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016203.1.6",
          "form": "지금 열심히 문화생활비 모으고있어요ㅋㅋ",
          "original_form": "지금 열심히 문화생활비 모으고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016203.1.7",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016203.1.8",
          "form": "코로나가 저금을 시켜주네요...",
          "original_form": "코로나가 저금을 시켜주네요...",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016203.1.9",
          "form": "ㅜㅜ 안시켜줘도되는데....",
          "original_form": "ㅜㅜ 안시켜줘도되는데....",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016203.1.10",
          "form": "콘서트는 불가능하지만 소규모 공연같은 걸 볼까 생각중이에요",
          "original_form": "콘서트는 불가능하지만 소규모 공연같은 걸 볼까 생각중이에요",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016203.1.11",
          "form": "어떤종류요?",
          "original_form": "어떤종류요?",
          "speaker_id": "2",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016203.1.12",
          "form": "음 창극도 좋고 난타공연도 신나겠죠?",
          "original_form": "음 창극도 좋고 난타공연도 신나겠죠?",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016203.1.13",
          "form": "난타라니 너무 좋네요ㅋㅋ",
          "original_form": "난타라니 너무 좋네요ㅋㅋ",
