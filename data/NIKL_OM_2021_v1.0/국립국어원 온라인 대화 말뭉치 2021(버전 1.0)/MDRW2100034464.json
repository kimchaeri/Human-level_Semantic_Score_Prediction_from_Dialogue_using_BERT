      "utterance": [
        {
          "id": "MDRW2100034464.1.1",
          "form": "점심드셧어요?",
          "original_form": "점심드셧어요?",
          "speaker_id": "2",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100034464.1.2",
          "form": "네! 오리고기 먹었네요",
          "original_form": "네! 오리고기 먹었네요",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034464.1.3",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034464.1.4",
          "form": "네 떡만둣국 먹엇어요",
          "original_form": "네 떡만둣국 먹엇어요",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034464.1.5",
          "form": "오 맛있죠",
          "original_form": "오 맛있죠",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100034464.1.6",
          "form": "만두 고기 김치 중에 어떤걸 더 좋아하시나요",
          "original_form": "만두 고기 김치 중에 어떤걸 더 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100034464.1.7",
          "form": "둘다 좋아하지만 고기를 좀더 좋아합니다",
          "original_form": "둘다 좋아하지만 고기를 좀더 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100034464.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100034464.1.9",
          "form": "저도 둘다 좋아하는데 김치를 좀더 좋아해요 ..!",
          "original_form": "저도 둘다 좋아하는데 김치를 좀더 좋아해요 ..!",
          "speaker_id": "1",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100034464.1.10",
          "form": "오오 취향이 나뉘네요",
          "original_form": "오오 취향이 나뉘네요",
          "speaker_id": "2",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100034464.1.11",
          "form": "그럼 군만두 찐만두 중에선?",
          "original_form": "그럼 군만두 찐만두 중에선?",
          "speaker_id": "2",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100034464.1.12",
          "form": "이것도 둘다 좋은데 저는 찐만두 파입니다...",
          "original_form": "이것도 둘다 좋은데 저는 찐만두 파입니다...",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100034464.1.13",
          "form": "어떤거 좋아하시나요",
          "original_form": "어떤거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100034464.1.14",
          "form": "저도 둘다 좋지만 군만두파인데 ㅋㅋㅋㅋ만두를 좋아하는 사람들 사이에서도 미묘하게 갈리네요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 둘다 좋지만 군만두파인데 ㅋㅋㅋㅋ만두를 좋아하는 사람들 사이에서도 미묘하게 갈리네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100034464.1.15",
          "form": "ㅋㅋㅋㅋ 그러게요 신기하네요",
          "original_form": "ㅋㅋㅋㅋ 그러게요 신기하네요",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034464.1.16",
          "form": "그래도 만두는 항상 맛있죠 다른 음식과도 잘 어울리구요!",
          "original_form": "그래도 만두는 항상 맛있죠 다른 음식과도 잘 어울리구요!",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034464.1.17",
          "form": "맞아요 만두 너무 좋아해요",
          "original_form": "맞아요 만두 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034464.1.18",
          "form": "딤섬도 좋아하세요? 소롱포 존맛탱...",
          "original_form": "딤섬도 좋아하세요? 소롱포 존맛탱...",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034464.1.19",
          "form": "맛있죠 따끈해서 그 육즙 넘 좋아요...",
          "original_form": "맛있죠 따끈해서 그 육즙 넘 좋아요...",
          "speaker_id": "1",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100034464.1.20",
          "form": "맞아요 ㅠㅠ 후릅 후릅 마셔야해요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 후릅 후릅 마셔야해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100034464.1.21",
          "form": "그쵸그쵸ㅋㅋ 조심해서 먹어야합니다..",
          "original_form": "그쵸그쵸ㅋㅋ 조심해서 먹어야합니다..",
          "speaker_id": "1",
          "time": "20210828 15:37"
        }
      ]
