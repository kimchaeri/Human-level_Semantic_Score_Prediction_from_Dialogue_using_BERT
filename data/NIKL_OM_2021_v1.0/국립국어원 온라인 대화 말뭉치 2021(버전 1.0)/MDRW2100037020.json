      "utterance": [
        {
          "id": "MDRW2100037020.1.1",
          "form": "운동하시나여",
          "original_form": "운동하시나여",
          "speaker_id": "1",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100037020.1.2",
          "form": "아니요 해야하는데 생각만해요",
          "original_form": "아니요 해야하는데 생각만해요",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100037020.1.3",
          "form": "저도요..ㅜ전 수영하고싶은데 코로나라 언제할수있을지..",
          "original_form": "저도요..ㅜ전 수영하고싶은데 코로나라 언제할수있을지..",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100037020.1.4",
          "form": "하시고싶은운동있으시나요",
          "original_form": "하시고싶은운동있으시나요",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100037020.1.5",
          "form": "아뇨 운동을 별로 안 좋아해서요",
          "original_form": "아뇨 운동을 별로 안 좋아해서요",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100037020.1.6",
          "form": "예전엔 그래도 새로나오는 운동은 다해보려고 했는데",
          "original_form": "예전엔 그래도 새로나오는 운동은 다해보려고 했는데",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100037020.1.7",
          "form": "요즘은 너무 귀찮아서",
          "original_form": "요즘은 너무 귀찮아서",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100037020.1.8",
          "form": "그쳐 저도 요즘 왜케 귀찮은지모르겠어요",
          "original_form": "그쳐 저도 요즘 왜케 귀찮은지모르겠어요",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100037020.1.9",
          "form": "새로나온운동어떤거 해보셨어요",
          "original_form": "새로나온운동어떤거 해보셨어요",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100037020.1.10",
          "form": "스피닝도 나오자 마자 찾아가서 배우고",
          "original_form": "스피닝도 나오자 마자 찾아가서 배우고",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037020.1.11",
          "form": "핫요가도 한때 했었고",
          "original_form": "핫요가도 한때 했었고",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037020.1.12",
          "form": "스쿼시도 배우다 말고",
          "original_form": "스쿼시도 배우다 말고",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037020.1.13",
          "form": "와아",
          "original_form": "와아",
          "speaker_id": "1",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037020.1.14",
          "form": "넘많이하셔서 이제 질리신건지 진짜 여러가지배우셨네요",
          "original_form": "넘많이하셔서 이제 질리신건지 진짜 여러가지배우셨네요",
          "speaker_id": "1",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037020.1.15",
          "form": "제대로 하는건 없어요",
          "original_form": "제대로 하는건 없어요",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037020.1.16",
          "form": "한땐 피겨스케이팅도 ㅋㅋ",
          "original_form": "한땐 피겨스케이팅도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037020.1.17",
          "form": "어머어머",
          "original_form": "어머어머",
          "speaker_id": "1",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037020.1.18",
          "form": "ㅋㅋㅋㅋ진짜 다양하게하셨어요",
          "original_form": "ㅋㅋㅋㅋ진짜 다양하게하셨어요",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037020.1.19",
          "form": "피겨 배우는거어땠어요? 균형잘잡아야되나요?",
          "original_form": "피겨 배우는거어땠어요? 균형잘잡아야되나요?",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037020.1.20",
          "form": "피겨는 우선 균형잡기 편한 스케이트여서",
          "original_form": "피겨는 우선 균형잡기 편한 스케이트여서",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037020.1.21",
          "form": "단 넘어지는 용기가 가장 중요했어요",
          "original_form": "단 넘어지는 용기가 가장 중요했어요",
          "speaker_id": "2",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037020.1.22",
          "form": "아 넘어지는용기!",
          "original_form": "아 넘어지는용기!",
          "speaker_id": "1",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100037020.1.23",
          "form": "멍많이들거같아요ㅜ",
          "original_form": "멍많이들거같아요ㅜ",
          "speaker_id": "1",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100037020.1.24",
          "form": "넹. 멍 많이 들었어요 엉덩이도 아작나고",
          "original_form": "넹. 멍 많이 들었어요 엉덩이도 아작나고",
          "speaker_id": "2",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100037020.1.25",
          "form": "ㅜㅜ크읍 역시쉬운운동은없네요",
          "original_form": "ㅜㅜ크읍 역시쉬운운동은없네요",
          "speaker_id": "1",
          "time": "20210831 17:44"
        }
      ]
    }
  ]
