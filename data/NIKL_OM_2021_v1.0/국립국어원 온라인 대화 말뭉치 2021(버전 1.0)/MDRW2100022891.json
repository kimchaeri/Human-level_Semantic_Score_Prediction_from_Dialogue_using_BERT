      "utterance": [
        {
          "id": "MDRW2100022891.1.1",
          "form": "저는 빅뱅이론이나 미란다같은",
          "original_form": "저는 빅뱅이론이나 미란다같은",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022891.1.2",
          "form": "그냥코미디 미드영드 많이봐요ㅋㅋ",
          "original_form": "그냥코미디 미드영드 많이봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022891.1.3",
          "form": "아하 해외드라마 취향이시구나!",
          "original_form": "아하 해외드라마 취향이시구나!",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022891.1.4",
          "form": "저는 국내 작품 위주로 봐용ㅎㅎㅎ",
          "original_form": "저는 국내 작품 위주로 봐용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022891.1.5",
          "form": "최근에 영화 뭐 보셨어요?",
          "original_form": "최근에 영화 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022891.1.6",
          "form": "거의 못보긴했는데",
          "original_form": "거의 못보긴했는데",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022891.1.7",
          "form": "이것도 좀 된얘기지만",
          "original_form": "이것도 좀 된얘기지만",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022891.1.8",
          "form": "아이들은즐겁다 봤어요!",
          "original_form": "아이들은즐겁다 봤어요!",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022891.1.9",
          "form": "저도 영화관은 거의 못가고 집에서 봤어용ㅠ",
          "original_form": "저도 영화관은 거의 못가고 집에서 봤어용ㅠ",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022891.1.10",
          "form": "오 누구 나오는건가용?",
          "original_form": "오 누구 나오는건가용?",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022891.1.11",
          "form": "해외작품인가용?",
          "original_form": "해외작품인가용?",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022891.1.12",
          "form": "웹툰원작영화라 국내작품이구요!",
          "original_form": "웹툰원작영화라 국내작품이구요!",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022891.1.13",
          "form": "아하 저는 왜 몰랐을까욬ㅋㅋㅋ",
          "original_form": "아하 저는 왜 몰랐을까욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022891.1.14",
          "form": "거의 아역배우들이라 유명배우는 안나온거같았어요ㅋㅋ",
          "original_form": "거의 아역배우들이라 유명배우는 안나온거같았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022891.1.15",
          "form": "애기들 연기 잘하나용?",
          "original_form": "애기들 연기 잘하나용?",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022891.1.16",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022891.1.17",
          "form": "잘하더라구요~",
          "original_form": "잘하더라구요~",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022891.1.18",
          "form": "너무울어서ㅜㅜ",
          "original_form": "너무울어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022891.1.19",
          "form": "추천합니다!",
          "original_form": "추천합니다!",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022891.1.20",
          "form": "아아ㅠㅠㅠ",
          "original_form": "아아ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022891.1.21",
          "form": "분위기는 어둡진않죠?",
          "original_form": "분위기는 어둡진않죠?",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022891.1.22",
          "form": "조금 어둡긴한데 그래도 희망찬거같아요!",
          "original_form": "조금 어둡긴한데 그래도 희망찬거같아요!",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022891.1.23",
          "form": "추천해주실것있으신가요?",
          "original_form": "추천해주실것있으신가요?",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022891.1.24",
          "form": "감사해요 꼭 볼게용ㅎㅎ",
          "original_form": "감사해요 꼭 볼게용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022891.1.25",
          "form": "음 사실 벨기에 드라마를 추천해드리고싶은데",
          "original_form": "음 사실 벨기에 드라마를 추천해드리고싶은데",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022891.1.26",
          "form": "이것도 넷플릭스 작품이네요ㅠㅠ",
          "original_form": "이것도 넷플릭스 작품이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022891.1.27",
          "form": "나중에 꼭 볼게요!",
          "original_form": "나중에 꼭 볼게요!",
          "speaker_id": "2",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022891.1.28",
          "form": "이거 말고는 음",
          "original_form": "이거 말고는 음",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022891.1.29",
          "form": "호우텔",
          "original_form": "호우텔",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022891.1.30",
          "form": "뭄바이",
          "original_form": "뭄바이",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022891.1.31",
          "form": "이 영화 재밌어요",
          "original_form": "이 영화 재밌어요",
          "speaker_id": "1",
          "time": "20210829 18:59"
        }
      ]
    }
  ]
