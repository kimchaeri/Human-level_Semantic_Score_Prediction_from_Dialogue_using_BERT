      "utterance": [
        {
          "id": "MDRW2100029724.1.1",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:19"
        },
        {
          "id": "MDRW2100029724.1.2",
          "form": "안녕하세요 운동하시나요",
          "original_form": "안녕하세요 운동하시나요",
          "speaker_id": "2",
          "time": "20210903 12:19"
        },
        {
          "id": "MDRW2100029724.1.3",
          "form": "아뇨ㅎㅎ 안좋아해요ㅋ",
          "original_form": "아뇨ㅎㅎ 안좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210903 12:19"
        },
        {
          "id": "MDRW2100029724.1.4",
          "form": "게을러서ㅠㅋㅋㅋ",
          "original_form": "게을러서ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:19"
        },
        {
          "id": "MDRW2100029724.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210903 12:19"
        },
        {
          "id": "MDRW2100029724.1.6",
          "form": "ㅜㅇㅜ 이해합니다",
          "original_form": "ㅜㅇㅜ 이해합니다",
          "speaker_id": "2",
          "time": "20210903 12:20"
        },
        {
          "id": "MDRW2100029724.1.7",
          "form": "전 숨쉬기운동합니다",
          "original_form": "전 숨쉬기운동합니다",
          "speaker_id": "2",
          "time": "20210903 12:20"
        },
        {
          "id": "MDRW2100029724.1.8",
          "form": "건강관리  위해 별도로 하는건 있으신가요",
          "original_form": "건강관리  위해 별도로 하는건 있으신가요",
          "speaker_id": "2",
          "time": "20210903 12:20"
        },
        {
          "id": "MDRW2100029724.1.9",
          "form": "ㅎㅎㅎ저랑 같으신데요",
          "original_form": "ㅎㅎㅎ저랑 같으신데요",
          "speaker_id": "1",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100029724.1.10",
          "form": "백신 맞으셨나요",
          "original_form": "백신 맞으셨나요",
          "speaker_id": "2",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100029724.1.11",
          "form": "아뇨ㅋㅋ다음주예악이에요ㅋ",
          "original_form": "아뇨ㅋㅋ다음주예악이에요ㅋ",
          "speaker_id": "1",
          "time": "20210903 13:08"
        },
        {
          "id": "MDRW2100029724.1.12",
          "form": "예약ㅋ",
          "original_form": "예약ㅋ",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100029724.1.13",
          "form": "아 화이자인가요?",
          "original_form": "아 화이자인가요?",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100029724.1.14",
          "form": "그냥 모더나 혹은 화이자로 써있던데 미리알수있어요?",
          "original_form": "그냥 모더나 혹은 화이자로 써있던데 미리알수있어요?",
          "speaker_id": "1",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100029724.1.15",
          "form": "아 맞기전에 확정된걸로 다시 연락 와요",
          "original_form": "아 맞기전에 확정된걸로 다시 연락 와요",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100029724.1.16",
          "form": "모더나가 들어왔으니 님은 모더나일수도 있겠네요",
          "original_form": "모더나가 들어왔으니 님은 모더나일수도 있겠네요",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100029724.1.17",
          "form": "일찍 맞은 분들은 화이자였어요 대개",
          "original_form": "일찍 맞은 분들은 화이자였어요 대개",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100029724.1.18",
          "form": "저 10일이에요ㅋ 저도 화이자 맞고픈데ㅜㅋ",
          "original_form": "저 10일이에요ㅋ 저도 화이자 맞고픈데ㅜㅋ",
          "speaker_id": "1",
          "time": "20210903 13:30"
        },
        {
          "id": "MDRW2100029724.1.19",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210903 13:30"
        },
        {
          "id": "MDRW2100029724.1.20",
          "form": "모더나가 변이에 더 강하대요ㅎ",
          "original_form": "모더나가 변이에 더 강하대요ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:31"
        },
        {
          "id": "MDRW2100029724.1.21",
          "form": "전 모더나 맞고 싶었지만..",
          "original_form": "전 모더나 맞고 싶었지만..",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100029724.1.22",
          "form": "어제 화이자 맞았습니다ㅎ",
          "original_form": "어제 화이자 맞았습니다ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100029724.1.23",
          "form": "아그래요?ㅜ뭐 무튼 잘 예방해야죠ㅋ",
          "original_form": "아그래요?ㅜ뭐 무튼 잘 예방해야죠ㅋ",
