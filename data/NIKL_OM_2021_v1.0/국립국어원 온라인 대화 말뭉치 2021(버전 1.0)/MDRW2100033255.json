      "utterance": [
        {
          "id": "MDRW2100033255.1.1",
          "form": "뭐든괜찮아욤",
          "original_form": "뭐든괜찮아욤",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033255.1.2",
          "form": "근로 장려금.. 혹시 신청 하셨나요??",
          "original_form": "근로 장려금.. 혹시 신청 하셨나요??",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033255.1.3",
          "form": "아마 아직 안 하셨을 수도 있을 것 같은데",
          "original_form": "아마 아직 안 하셨을 수도 있을 것 같은데",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033255.1.4",
          "form": "저는 작년부터",
          "original_form": "저는 작년부터",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100033255.1.5",
          "form": "꾸준히 받고있지요!!!",
          "original_form": "꾸준히 받고있지요!!!",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100033255.1.6",
          "form": "이번에도 1일부터신청이길래",
          "original_form": "이번에도 1일부터신청이길래",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100033255.1.7",
          "form": "했져!",
          "original_form": "했져!",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100033255.1.8",
          "form": "엇 하고 계셨군요!",
          "original_form": "엇 하고 계셨군요!",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100033255.1.9",
          "form": "물론리죠!!",
          "original_form": "물론리죠!!",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100033255.1.10",
          "form": "형제 자매중엔 한명만 받을 수 있다고 본 것 같아서 이번엔  신청 못했거든요 ㅠ",
          "original_form": "형제 자매중엔 한명만 받을 수 있다고 본 것 같아서 이번엔  신청 못했거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100033255.1.11",
          "form": "우와 되게 정보들 잘 찾아보시나봐요!",
          "original_form": "우와 되게 정보들 잘 찾아보시나봐요!",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100033255.1.12",
          "form": "아아앙 저는 사실 20살때부터",
          "original_form": "아아앙 저는 사실 20살때부터",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100033255.1.13",
          "form": "집에서 용돈안받고",
          "original_form": "집에서 용돈안받고",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100033255.1.14",
          "form": "등록금 생활비 교통비 휴대폰 등등 다 제가내서",
          "original_form": "등록금 생활비 교통비 휴대폰 등등 다 제가내서",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100033255.1.15",
          "form": "열심히 찾아먹고있죠,,,ㅋㅋㅋㅋ",
          "original_form": "열심히 찾아먹고있죠,,,ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100033255.1.16",
          "form": "진짜 멋쟁이네요!!",
          "original_form": "진짜 멋쟁이네요!!",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100033255.1.17",
          "form": "알바 하면서 공부 동시에 하기 힘들지 않아요?",
          "original_form": "알바 하면서 공부 동시에 하기 힘들지 않아요?",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100033255.1.18",
          "form": "아 혹시 장학금.. 받으시는... 수석?",
          "original_form": "아 혹시 장학금.. 받으시는... 수석?",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100033255.1.19",
          "form": "에이...아니에요",
          "original_form": "에이...아니에요",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033255.1.20",
          "form": "3.8...평범평범남인걸여",
          "original_form": "3.8...평범평범남인걸여",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033255.1.21",
          "form": "에이... 돈벌면서 학점 챙기기 어려운건 다 아는데",
          "original_form": "에이... 돈벌면서 학점 챙기기 어려운건 다 아는데",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033255.1.22",
          "form": "열심히 하시는게 느껴져요",
          "original_form": "열심히 하시는게 느껴져요",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033255.1.23",
          "form": "후라....감사합니당 ㅠㅠ",
          "original_form": "후라....감사합니당 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100033255.1.24",
          "form": "스스로 20살부터 어떻게 알아서 할 생각을 하셨어요?",
          "original_form": "스스로 20살부터 어떻게 알아서 할 생각을 하셨어요?",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100033255.1.25",
          "form": "그때부턴 성인이니까요!",
          "original_form": "그때부턴 성인이니까요!",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100033255.1.26",
          "form": "라는생각이었던거같아요",
          "original_form": "라는생각이었던거같아요",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100033255.1.27",
          "form": "고등학교 3년도",
          "original_form": "고등학교 3년도",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100033255.1.28",
          "form": "매년 삼성장학재던에서 장학금받고 학교다녀서",
          "original_form": "매년 삼성장학재던에서 장학금받고 학교다녀서",
          "speaker_id": "2",
          "time": "20210906 22:00"
        }
      ]
