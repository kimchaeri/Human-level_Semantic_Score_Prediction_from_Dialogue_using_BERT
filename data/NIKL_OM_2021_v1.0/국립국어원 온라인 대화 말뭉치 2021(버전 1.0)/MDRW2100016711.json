      "utterance": [
        {
          "id": "MDRW2100016711.1.1",
          "form": "키프트콘 중고거래판매처가 많잖아요 어디사용하셨나요?",
          "original_form": "키프트콘 중고거래판매처가 많잖아요 어디사용하셨나요?",
          "speaker_id": "1",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016711.1.2",
          "form": "저는 기프티스타같은 어플도 해봤고 당근마켓이나 중고나라 다양하게 이용해요",
          "original_form": "저는 기프티스타같은 어플도 해봤고 당근마켓이나 중고나라 다양하게 이용해요",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016711.1.3",
          "form": "기프티스타 저도 자주 사용해요",
          "original_form": "기프티스타 저도 자주 사용해요",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016711.1.4",
          "form": "아무래도 알아서 가격매겨 사주는게 편하더라구요",
          "original_form": "아무래도 알아서 가격매겨 사주는게 편하더라구요",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016711.1.5",
          "form": "거기가 젤 유명한 거 같아요 어플중에서는",
          "original_form": "거기가 젤 유명한 거 같아요 어플중에서는",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016711.1.6",
          "form": "맞아요 니콘내콘도 이용해보셨어요?",
          "original_form": "맞아요 니콘내콘도 이용해보셨어요?",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016711.1.7",
          "form": "들어는봤어요!",
          "original_form": "들어는봤어요!",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016711.1.8",
          "form": "거긴 어때요?",
          "original_form": "거긴 어때요?",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016711.1.9",
          "form": "거긴 기프티스타보다 가격을 더 쳐줘서 좋아요 ㅋㅋㅋ",
          "original_form": "거긴 기프티스타보다 가격을 더 쳐줘서 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016711.1.10",
          "form": "거기도 자동인가요?",
          "original_form": "거기도 자동인가요?",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016711.1.11",
          "form": "네 ㅋㅋㅋ 대신 3일 지나야 출금 가능해요 그거빼곤 같아요",
          "original_form": "네 ㅋㅋㅋ 대신 3일 지나야 출금 가능해요 그거빼곤 같아요",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016711.1.12",
          "form": "오...꿀정보네요 갈아타야겠어요",
          "original_form": "오...꿀정보네요 갈아타야겠어요",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016711.1.13",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016711.1.14",
          "form": "전 두곳 중에서 더 비싸게 쳐주는데서 팔아요 ㅋㅋㅋ 그때그때 달라서요",
          "original_form": "전 두곳 중에서 더 비싸게 쳐주는데서 팔아요 ㅋㅋㅋ 그때그때 달라서요",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016711.1.15",
          "form": "주로 어디서 중고거래하세요?",
          "original_form": "주로 어디서 중고거래하세요?",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016711.1.16",
          "form": "당근이랑 기프트스타요",
          "original_form": "당근이랑 기프트스타요",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016711.1.17",
          "form": "그 두곳이 젤 잘나가나봐요",
          "original_form": "그 두곳이 젤 잘나가나봐요",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016711.1.18",
          "form": "그런가봐요 쓰기 편해서 저는 잘쓰고있어요",
          "original_form": "그런가봐요 쓰기 편해서 저는 잘쓰고있어요",
          "speaker_id": "1",
