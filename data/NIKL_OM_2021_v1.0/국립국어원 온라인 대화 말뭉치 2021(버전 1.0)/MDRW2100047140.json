      "utterance": [
        {
          "id": "MDRW2100047140.1.1",
          "form": "이런거 아세요?",
          "original_form": "이런거 아세요?",
          "speaker_id": "1",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100047140.1.2",
          "form": "콜라보 제품요??",
          "original_form": "콜라보 제품요??",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100047140.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100047140.1.4",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100047140.1.5",
          "form": "맥주만 먹어본 것 같아요",
          "original_form": "맥주만 먹어본 것 같아요",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100047140.1.6",
          "form": "곰표맥주 말표맥주",
          "original_form": "곰표맥주 말표맥주",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100047140.1.7",
          "form": "앗 저도 먹어봤어요!!",
          "original_form": "앗 저도 먹어봤어요!!",
          "speaker_id": "1",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100047140.1.8",
          "form": "그냥 그냥 무난한?",
          "original_form": "그냥 그냥 무난한?",
          "speaker_id": "1",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100047140.1.9",
          "form": "네 그냥 무난한ㄷ",
          "original_form": "네 그냥 무난한ㄷ",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047140.1.10",
          "form": "향이 좀 더 강한 것 같았어요",
          "original_form": "향이 좀 더 강한 것 같았어요",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047140.1.11",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047140.1.12",
          "form": "말표맥주가 엄청 향이 강해서",
          "original_form": "말표맥주가 엄청 향이 강해서",
          "speaker_id": "1",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047140.1.13",
          "form": "처음에 좀 거부감 들더라구요",
          "original_form": "처음에 좀 거부감 들더라구요",
          "speaker_id": "1",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047140.1.14",
          "form": "네 말표는 구두약인데 ㅎㅎ",
          "original_form": "네 말표는 구두약인데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047140.1.15",
          "form": "좀 이상해요",
          "original_form": "좀 이상해요",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047140.1.16",
          "form": "웃기기도 하고 ㅎㅎ",
          "original_form": "웃기기도 하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:16"
        },
        {
          "id": "MDRW2100047140.1.17",
          "form": "네 ㅋㅋ 재밌긴 한데",
          "original_form": "네 ㅋㅋ 재밌긴 한데",
          "speaker_id": "1",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100047140.1.18",
          "form": "정작 맛은 그냥 그랬어서",
          "original_form": "정작 맛은 그냥 그랬어서",
          "speaker_id": "1",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100047140.1.19",
          "form": "실망..",
          "original_form": "실망..",
          "speaker_id": "1",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100047140.1.20",
          "form": "네 다른 건 잘 모르겠네요",
          "original_form": "네 다른 건 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100047140.1.21",
          "form": "네 막 그런건 봤어요",
          "original_form": "네 막 그런건 봤어요",
          "speaker_id": "1",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100047140.1.22",
          "form": "케찹 포장지 안에 다른 내용물 들어있는?",
          "original_form": "케찹 포장지 안에 다른 내용물 들어있는?",
          "speaker_id": "1",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100047140.1.23",
          "form": "아 뭔지 알 것 같아요",
          "original_form": "아 뭔지 알 것 같아요",
          "speaker_id": "2",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100047140.1.24",
          "form": "네 밀키스 빵 이런것도 있고",
          "original_form": "네 밀키스 빵 이런것도 있고",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100047140.1.25",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100047140.1.26",
          "form": "별로 땡기진 않더라구요",
          "original_form": "별로 땡기진 않더라구요",
          "speaker_id": "2",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100047140.1.27",
          "form": "네 저도 사먹진 않았어요 ㅋㅋ",
          "original_form": "네 저도 사먹진 않았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:18"
        }
