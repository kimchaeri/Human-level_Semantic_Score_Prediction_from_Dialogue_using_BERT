      "utterance": [
        {
          "id": "MDRW2100002118.1.1",
          "form": "오늘 갑자기 비오더라",
          "original_form": "오늘 갑자기 비오더라",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002118.1.2",
          "form": "맞아 갑자기 비오더라 오늘 ㅠㅠ",
          "original_form": "맞아 갑자기 비오더라 오늘 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002118.1.3",
          "form": "그러다가 갑자기 추워지겠지?",
          "original_form": "그러다가 갑자기 추워지겠지?",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002118.1.4",
          "form": "응 지금도 좀 추운거같아",
          "original_form": "응 지금도 좀 추운거같아",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002118.1.5",
          "form": "그러게ㅋㅋㅋ 곧 패딩꺼내야될지도",
          "original_form": "그러게ㅋㅋㅋ 곧 패딩꺼내야될지도",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002118.1.6",
          "form": "패딩ㅋㅋㅋ 하긴 비오고나면 더 추워지니깐 ㅠㅠ",
          "original_form": "패딩ㅋㅋㅋ 하긴 비오고나면 더 추워지니깐 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002118.1.7",
          "form": "주말에 여행가기로했는데 비온대서 걱정",
          "original_form": "주말에 여행가기로했는데 비온대서 걱정",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002118.1.8",
          "form": "오 여행가는구나 비오면 좀 그렇겠다 ㅠㅠ",
          "original_form": "오 여행가는구나 비오면 좀 그렇겠다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002118.1.9",
          "form": "웅 그래서 짐싸는데",
          "original_form": "웅 그래서 짐싸는데",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002118.1.10",
          "form": "긴팔 반팔 뭐챙길지 모르겠어",
          "original_form": "긴팔 반팔 뭐챙길지 모르겠어",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002118.1.11",
          "form": "그러게 바람막이같은거 하나 챙겨가라 혹시 추워지면 입게",
          "original_form": "그러게 바람막이같은거 하나 챙겨가라 혹시 추워지면 입게",
          "speaker_id": "2",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002118.1.12",
          "form": "너희동네도 주말에 비온대?",
          "original_form": "너희동네도 주말에 비온대?",
          "speaker_id": "1",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002118.1.13",
          "form": "웅 온다더라구 집에만 있을라구",
          "original_form": "웅 온다더라구 집에만 있을라구",
          "speaker_id": "2",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002118.1.14",
          "form": "맞아 요즘 날씨 좀 오락가락해서",
          "original_form": "맞아 요즘 날씨 좀 오락가락해서",
          "speaker_id": "1",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002118.1.15",
          "form": "집에있는게 최고임ㅋㅋ",
          "original_form": "집에있는게 최고임ㅋㅋ",
          "speaker_id": "1",
