      "utterance": [
        {
          "id": "MDRW2100037778.1.1",
          "form": "요즘 많이 선선해진 거 같아요",
          "original_form": "요즘 많이 선선해진 거 같아요",
          "speaker_id": "2",
          "time": "20210905 17:24"
        },
        {
          "id": "MDRW2100037778.1.2",
          "form": "네 시원해졌어요 많이!",
          "original_form": "네 시원해졌어요 많이!",
          "speaker_id": "1",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100037778.1.3",
          "form": "여름에 엄청 더웠는데 그게 오래된 일이 되어버렸어요",
          "original_form": "여름에 엄청 더웠는데 그게 오래된 일이 되어버렸어요",
          "speaker_id": "2",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100037778.1.4",
          "form": "아 맞아요 시간이 빠르네요ㅠ",
          "original_form": "아 맞아요 시간이 빠르네요ㅠ",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100037778.1.5",
          "form": "가을이왔어요",
          "original_form": "가을이왔어요",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100037778.1.6",
          "form": "가을에 하고 싶은 거 있으셨어요?",
          "original_form": "가을에 하고 싶은 거 있으셨어요?",
          "speaker_id": "2",
          "time": "20210905 17:30"
        },
        {
          "id": "MDRW2100037778.1.7",
          "form": "단풍구경이라거나...",
          "original_form": "단풍구경이라거나...",
          "speaker_id": "2",
          "time": "20210905 17:30"
        },
        {
          "id": "MDRW2100037778.1.8",
          "form": "저는 한강가는거 좋아해요",
          "original_form": "저는 한강가는거 좋아해요",
          "speaker_id": "1",
          "time": "20210905 17:30"
        },
        {
          "id": "MDRW2100037778.1.9",
          "form": "가을에 한강 구경",
          "original_form": "가을에 한강 구경",
          "speaker_id": "2",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100037778.1.10",
          "form": "좋을 거 같아요",
          "original_form": "좋을 거 같아요",
          "speaker_id": "2",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100037778.1.11",
          "form": "시~~~~원하고좋아요!",
          "original_form": "시~~~~원하고좋아요!",
          "speaker_id": "1",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100037778.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100037778.1.13",
          "form": "저는 산에 단풍 보러 가고 싶어요",
          "original_form": "저는 산에 단풍 보러 가고 싶어요",
          "speaker_id": "2",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100037778.1.14",
          "form": "단풍너무이쁘죠ㅠ",
          "original_form": "단풍너무이쁘죠ㅠ",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100037778.1.15",
          "form": "올해는 아직 단풍이 안든것같아요",
          "original_form": "올해는 아직 단풍이 안든것같아요",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100037778.1.16",
          "form": "그쵸?!",
          "original_form": "그쵸?!",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100037778.1.17",
          "form": "네 아직은 안든 거 같아요",
          "original_form": "네 아직은 안든 거 같아요",
          "speaker_id": "2",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100037778.1.18",
          "form": "조금 지나야 색이 나오지 않을까요",
          "original_form": "조금 지나야 색이 나오지 않을까요",
          "speaker_id": "2",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100037778.1.19",
          "form": "원래 등산도 좋아하세요?",
          "original_form": "원래 등산도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100037778.1.20",
          "form": "아니요 등산은 그렇게 좋아하진 않아요",
          "original_form": "아니요 등산은 그렇게 좋아하진 않아요",
          "speaker_id": "2",
          "time": "20210905 17:35"
        }
