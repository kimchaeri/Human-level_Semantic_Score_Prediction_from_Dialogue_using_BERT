      "utterance": [
        {
          "id": "MDRW2100002073.1.1",
          "form": "좋아하는 계절있으세요?",
          "original_form": "좋아하는 계절있으세요?",
          "speaker_id": "1",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002073.1.2",
          "form": "전 겨울좋아해요!",
          "original_form": "전 겨울좋아해요!",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002073.1.3",
          "form": "오 전 가을좋아하는데 겨울좋아하시는 이유있으실까여!",
          "original_form": "오 전 가을좋아하는데 겨울좋아하시는 이유있으실까여!",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002073.1.4",
          "form": "싸하고 추운게 좋더라구요 ㅎㅎㅎㅎ 해도 짧고..",
          "original_form": "싸하고 추운게 좋더라구요 ㅎㅎㅎㅎ 해도 짧고..",
          "speaker_id": "2",
          "time": "20210820 08:30"
        },
        {
          "id": "MDRW2100002073.1.5",
          "form": "가을도 좋아해요!",
          "original_form": "가을도 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 08:31"
        },
        {
          "id": "MDRW2100002073.1.6",
          "form": "지금보다 조금 더 쌀쌀해진 가을이 좋아요",
          "original_form": "지금보다 조금 더 쌀쌀해진 가을이 좋아요",
          "speaker_id": "2",
          "time": "20210820 08:31"
        },
        {
          "id": "MDRW2100002073.1.7",
          "form": "오 전 추위를 너무 타서 봄 가을을 좋아해요",
          "original_form": "오 전 추위를 너무 타서 봄 가을을 좋아해요",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100002073.1.8",
          "form": "겨울에 눈오는것도 좋아하시나요?",
          "original_form": "겨울에 눈오는것도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100002073.1.9",
          "form": "전 더위를 많이 타서..ㅎㅎ 겨울이 좋아요",
          "original_form": "전 더위를 많이 타서..ㅎㅎ 겨울이 좋아요",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002073.1.10",
          "form": "눈 좋아해요!",
          "original_form": "눈 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002073.1.11",
          "form": "근데 부산사람이라 눈이 귀해요..",
          "original_form": "근데 부산사람이라 눈이 귀해요..",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002073.1.12",
          "form": "오 부산이셨구나 눈진짜 보기힘들져 저도 남부지방이라 쌓이는 눈 보기 진짜 힘들더라구요",
          "original_form": "오 부산이셨구나 눈진짜 보기힘들져 저도 남부지방이라 쌓이는 눈 보기 진짜 힘들더라구요",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002073.1.13",
          "form": "그래서 눈오는게좋아요 ㅋㅋㅋ많이 안쌓이기도하고",
          "original_form": "그래서 눈오는게좋아요 ㅋㅋㅋ많이 안쌓이기도하고",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002073.1.14",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ 귀하니까 더 좋은거같기도해요 ㅎㅎㅎ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ 귀하니까 더 좋은거같기도해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002073.1.15",
          "form": "오늘은 좀 더운것같네요",
          "original_form": "오늘은 좀 더운것같네요",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002073.1.16",
          "form": "담주부터 장마래요..",
          "original_form": "담주부터 장마래요..",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002073.1.17",
          "form": "아이고 여긴어제도 비가쏟아졌는데",
          "original_form": "아이고 여긴어제도 비가쏟아졌는데",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002073.1.18",
          "form": "꾸리한날씨 계속 가나보네요ㅜ",
          "original_form": "꾸리한날씨 계속 가나보네요ㅜ",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002073.1.19",
          "form": "부산도 어제 스콜처럼 소나기가 몇번 왔었어요",
          "original_form": "부산도 어제 스콜처럼 소나기가 몇번 왔었어요",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002073.1.20",
          "form": "올여름은 날씨가.. 동남아같았죠",
          "original_form": "올여름은 날씨가.. 동남아같았죠",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002073.1.21",
          "form": "진짜 그래요 동남아처럼 변하나봐요",
          "original_form": "진짜 그래요 동남아처럼 변하나봐요",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002073.1.22",
          "form": "비가왕창왔다가 아예안왔다가ㅜ왔다리갔다리",
          "original_form": "비가왕창왔다가 아예안왔다가ㅜ왔다리갔다리",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002073.1.23",
          "form": "예전에 사계절 뚜렷할때가 좋았죠,,",
          "original_form": "예전에 사계절 뚜렷할때가 좋았죠,,",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002073.1.24",
          "form": "그니까여 이제는 계절이 뭔 두개밖에없는것같아요",
          "original_form": "그니까여 이제는 계절이 뭔 두개밖에없는것같아요",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002073.1.25",
          "form": "봄과가을은 진짜 스쳐지나가는 기분",
          "original_form": "봄과가을은 진짜 스쳐지나가는 기분",
          "speaker_id": "1",
          "time": "20210820 14:34"
        }
      ]
    }
