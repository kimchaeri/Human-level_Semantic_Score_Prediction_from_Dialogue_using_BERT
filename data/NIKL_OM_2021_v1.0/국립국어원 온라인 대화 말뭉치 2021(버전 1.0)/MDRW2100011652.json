        {
          "id": "MDRW2100011652.1.1",
          "form": "식사 하셨나요",
          "original_form": "식사 하셨나요",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011652.1.2",
          "form": "네 식사 하셨나요???",
          "original_form": "네 식사 하셨나요???",
          "speaker_id": "2",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100011652.1.3",
          "form": "네 라면에 만두 넣어서 먹었어요",
          "original_form": "네 라면에 만두 넣어서 먹었어요",
          "speaker_id": "1",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100011652.1.4",
          "form": "name2님은 뭐 드셨나요",
          "original_form": "&name2&님은 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100011652.1.5",
          "form": "오 맛있겠네요",
          "original_form": "오 맛있겠네요",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011652.1.6",
          "form": "저 참치회덮밥이요!",
          "original_form": "저 참치회덮밥이요!",
          "speaker_id": "2",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011652.1.7",
          "form": "참치회  덮밥도 맛있었을 거 같아요",
          "original_form": "참치회  덮밥도 맛있었을 거 같아요",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011652.1.8",
          "form": "회 좋아하시나요",
          "original_form": "회 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011652.1.9",
          "form": "네! 회 좋아하시나요?",
          "original_form": "네! 회 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011652.1.10",
          "form": "내 저도 회 좋아해요. 어떤 회 좋아하시나요",
          "original_form": "내 저도 회 좋아해요. 어떤 회 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011652.1.11",
          "form": "다 좋아합니다 소맥이랑 먹으면 다 맛있더라구요",
          "original_form": "다 좋아합니다 소맥이랑 먹으면 다 맛있더라구요",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011652.1.12",
          "form": "가리는 건 딱히 없으시군요ㅎㅎ",
          "original_form": "가리는 건 딱히 없으시군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011652.1.13",
          "form": "저는 전어 회 좋아해요",
          "original_form": "저는 전어 회 좋아해요",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011652.1.14",
          "form": "한번도 안 먹어봣네요 맛있나요???",
          "original_form": "한번도 안 먹어봣네요 맛있나요???",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011652.1.15",
          "form": "내 약간 기름진 맛인데 되게 고소해요",
          "original_form": "내 약간 기름진 맛인데 되게 고소해요",
          "speaker_id": "1",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011652.1.16",
          "form": "폰 오류때문에 계속 오타가 나서요 이해좀 부탁드려요",
          "original_form": "폰 오류때문에 계속 오타가 나서요 이해좀 부탁드려요",
          "speaker_id": "1",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011652.1.17",
          "form": "곧 가을 다가와서 전어철이니 기회가 되신다면 한 번 드셔보세요",
          "original_form": "곧 가을 다가와서 전어철이니 기회가 되신다면 한 번 드셔보세요",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011652.1.18",
          "form": "괜찮아요ㅋㅋ",
          "original_form": "괜찮아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011652.1.19",
          "form": "전어가 글케 맛있다는데",
          "original_form": "전어가 글케 맛있다는데",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011652.1.20",
          "form": "한번도 안먹어봤네요",
          "original_form": "한번도 안먹어봤네요",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011652.1.21",
          "form": "어디서 드셨나요",
          "original_form": "어디서 드셨나요",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011652.1.22",
          "form": "전어는 구워도 회로 먹어도 진짜 맛있는데 어디 지역가서 먹은건 아니고 집 근처 식당가서.. 먹었어요",
          "original_form": "전어는 구워도 회로 먹어도 진짜 맛있는데 어디 지역가서 먹은건 아니고 집 근처 식당가서.. 먹었어요",
