        },
        {
          "id": "MDRW2100033519.1.2",
          "form": "하시는 게임 있으세요",
          "original_form": "하시는 게임 있으세요",
          "speaker_id": "2",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100033519.1.3",
          "form": "음 모바일게임 카트정도요ㅋㅋㅋ",
          "original_form": "음 모바일게임 카트정도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100033519.1.4",
          "form": "카트라이더가 아직 있나요? ㅎㅎ",
          "original_form": "카트라이더가 아직 있나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100033519.1.5",
          "form": "네네ㅋㅋㅋ 피씨가 모바일로나왔고",
          "original_form": "네네ㅋㅋㅋ 피씨가 모바일로나왔고",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100033519.1.6",
          "form": "여전히 있답니다",
          "original_form": "여전히 있답니다",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100033519.1.7",
          "form": "저는 폰게임 해요",
          "original_form": "저는 폰게임 해요",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100033519.1.8",
          "form": "앱테크랑 게임 기능 같이있는",
          "original_form": "앱테크랑 게임 기능 같이있는",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100033519.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100033519.1.10",
          "form": "이름이 뭔가요?",
          "original_form": "이름이 뭔가요?",
          "speaker_id": "1",
          "time": "20210824 11:28"
        },
