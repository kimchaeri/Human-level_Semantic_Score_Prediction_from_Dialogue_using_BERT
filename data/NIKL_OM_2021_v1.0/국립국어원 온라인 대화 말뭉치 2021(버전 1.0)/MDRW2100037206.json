        {
          "id": "MDRW2100037206.1.2",
          "form": "지금 네이버 멤버십 할인 기간이야",
          "original_form": "지금 네이버 멤버십 할인 기간이야",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037206.1.3",
          "form": "안 그래도 나도 어제 쇼핑했어",
          "original_form": "안 그래도 나도 어제 쇼핑했어",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037206.1.4",
          "form": "아이 뭐 샀어",
          "original_form": "아이 뭐 샀어",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037206.1.5",
          "form": "풀무원 주먹밥 샀어",
          "original_form": "풀무원 주먹밥 샀어",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037206.1.6",
          "form": "혹시 김치볶음밥 샀니",
          "original_form": "혹시 김치볶음밥 샀니",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100037206.1.7",
          "form": "맞아 김치 치즈가 맛있대",
          "original_form": "맞아 김치 치즈가 맛있대",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100037206.1.8",
          "form": "또 먹어봤는데 맛있더라고",
          "original_form": "또 먹어봤는데 맛있더라고",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100037206.1.9",
          "form": "빨리 왔으면 좋겠다",
          "original_form": "빨리 왔으면 좋겠다",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100037206.1.10",
          "form": "나도 만두를 살까 고민 중이야",
          "original_form": "나도 만두를 살까 고민 중이야",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100037206.1.11",
          "form": "이번에 냉동식품이 살 만한 게 많더라",
          "original_form": "이번에 냉동식품이 살 만한 게 많더라",
          "speaker_id": "2",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100037206.1.12",
          "form": "햇반도 싸게 팔더라",
          "original_form": "햇반도 싸게 팔더라",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100037206.1.13",
          "form": "이번 네이버 되게 행사 크게  해",
