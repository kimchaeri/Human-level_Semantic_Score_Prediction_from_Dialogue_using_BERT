      "utterance": [
        {
          "id": "MDRW2100003608.1.1",
          "form": "name2님은요 더운게 좋아요 추운게 좋아요",
          "original_form": "&name2&님은요 더운게 좋아요 추운게 좋아요",
          "speaker_id": "1",
          "time": "20210821 08:20"
        },
        {
          "id": "MDRW2100003608.1.2",
          "form": "이번겨울엔...",
          "original_form": "이번겨울엔...",
          "speaker_id": "2",
          "time": "20210821 08:27"
        },
        {
          "id": "MDRW2100003608.1.3",
          "form": "추울까요?",
          "original_form": "추울까요?",
          "speaker_id": "2",
          "time": "20210821 08:27"
        },
        {
          "id": "MDRW2100003608.1.4",
          "form": "아무래도 그러지않을까요?",
          "original_form": "아무래도 그러지않을까요?",
          "speaker_id": "1",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003608.1.5",
          "form": "이상기후 때문에 사실 짐작이 안가요",
          "original_form": "이상기후 때문에 사실 짐작이 안가요",
          "speaker_id": "1",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003608.1.6",
          "form": "어떨 거 같으세요?",
          "original_form": "어떨 거 같으세요?",
          "speaker_id": "1",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003608.1.7",
          "form": "겁나 추웠으면 좋겠는데",
          "original_form": "겁나 추웠으면 좋겠는데",
          "speaker_id": "2",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003608.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003608.1.9",
          "form": "겁나 따뜻할거 같아요",
          "original_form": "겁나 따뜻할거 같아요",
          "speaker_id": "2",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003608.1.10",
          "form": "그건 좀ㅎ",
          "original_form": "그건 좀ㅎ",
          "speaker_id": "1",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003608.1.11",
          "form": "너무 추우면 좀 그래요",
          "original_form": "너무 추우면 좀 그래요",
          "speaker_id": "1",
          "time": "20210821 08:29"
        },
        {
          "id": "MDRW2100003608.1.12",
          "form": "겨울은 추워야 제맛인데...ㅋㅋㅋ",
          "original_form": "겨울은 추워야 제맛인데...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:29"
        },
        {
          "id": "MDRW2100003608.1.13",
          "form": "추운거 시러하세요?",
          "original_form": "추운거 시러하세요?",
          "speaker_id": "2",
          "time": "20210821 08:29"
        },
        {
          "id": "MDRW2100003608.1.14",
          "form": "너무 추운 건 좀 그렇더라구요 손 발 떨어질 거 같아서ㅎ",
          "original_form": "너무 추운 건 좀 그렇더라구요 손 발 떨어질 거 같아서ㅎ",
          "speaker_id": "1",
          "time": "20210821 08:29"
        },
        {
          "id": "MDRW2100003608.1.15",
          "form": "진짜 여름이 싫을수밖에 없는게",
          "original_form": "진짜 여름이 싫을수밖에 없는게",
          "speaker_id": "2",
          "time": "20210821 08:30"
        },
        {
          "id": "MDRW2100003608.1.16",
          "form": "겨울은 막 껴입어도 되는데",
          "original_form": "겨울은 막 껴입어도 되는데",
          "speaker_id": "2",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.18",
          "form": "직장인에게 여름이란....",
          "original_form": "직장인에게 여름이란....",
          "speaker_id": "2",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.19",
          "form": "긴바지에",
          "original_form": "긴바지에",
          "speaker_id": "2",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.20",
          "form": "아으....",
          "original_form": "아으....",
          "speaker_id": "2",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.21",
          "form": "저도 그 이유 때문에 여름이 싫긴해요",
          "original_form": "저도 그 이유 때문에 여름이 싫긴해요",
          "speaker_id": "1",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.22",
          "form": "반바지도 못입고",
          "original_form": "반바지도 못입고",
          "speaker_id": "2",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.23",
          "form": "근데 여자분들은",
          "original_form": "근데 여자분들은",
          "speaker_id": "2",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.24",
          "form": "그리고 벌레도 많고",
          "original_form": "그리고 벌레도 많고",
          "speaker_id": "1",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.25",
          "form": "반바지 입을수 있지 않아요?",
          "original_form": "반바지 입을수 있지 않아요?",
          "speaker_id": "2",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003608.1.26",
          "form": "흠.. 더운건 똑같아요ㅎ 그리고 반바지도 입는 사람만 입어서",
          "original_form": "흠.. 더운건 똑같아요ㅎ 그리고 반바지도 입는 사람만 입어서",
          "speaker_id": "1",
          "time": "20210821 08:31"
        }
      ]
    }
