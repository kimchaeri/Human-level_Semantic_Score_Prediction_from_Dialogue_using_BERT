      "utterance": [
        {
          "id": "MDRW2100005850.1.1",
          "form": "여행가구싶다",
          "original_form": "여행가구싶다",
          "speaker_id": "1",
          "time": "20210907 14:12"
        },
        {
          "id": "MDRW2100005850.1.2",
          "form": "최근에 여행 어디갔어?",
          "original_form": "최근에 여행 어디갔어?",
          "speaker_id": "1",
          "time": "20210907 14:12"
        },
        {
          "id": "MDRW2100005850.1.3",
          "form": "여행 마지막으로 간게 어디지",
          "original_form": "여행 마지막으로 간게 어디지",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100005850.1.4",
          "form": "사실 제주도나 어디 타지가서 승마할 계획 이었는데",
          "original_form": "사실 제주도나 어디 타지가서 승마할 계획 이었는데",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100005850.1.5",
          "form": "제주도 코로나 확산으로 여행 계획 취소함",
          "original_form": "제주도 코로나 확산으로 여행 계획 취소함",
          "speaker_id": "2",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100005850.1.6",
          "form": "헐 ㅠ",
          "original_form": "헐 ㅠ",
          "speaker_id": "1",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100005850.1.7",
          "form": "해외는 가본적있어?",
          "original_form": "해외는 가본적있어?",
          "speaker_id": "1",
          "time": "20210907 14:21"
        },
        {
          "id": "MDRW2100005850.1.8",
          "form": "어쩌다보니 마지막 여행이 일본이네",
          "original_form": "어쩌다보니 마지막 여행이 일본이네",
          "speaker_id": "2",
          "time": "20210907 14:21"
        },
        {
          "id": "MDRW2100005850.1.9",
          "form": "마지막으로간거 언제?",
          "original_form": "마지막으로간거 언제?",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100005850.1.10",
          "form": "2년전? 작년? 도쿄돔",
          "original_form": "2년전? 작년? 도쿄돔",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100005850.1.11",
          "form": "그때가 아이마스 15주년 전에 있던 반남 몇십주년 콘서트 였으니까",
          "original_form": "그때가 아이마스 15주년 전에 있던 반남 몇십주년 콘서트 였으니까",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100005850.1.12",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100005850.1.13",
          "form": "아 재밌겠다 ㅏㅏㅏㅏㅏㅏ",
          "original_form": "아 재밌겠다 ㅏㅏㅏㅏㅏㅏ",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100005850.1.14",
          "form": "아 일본가고싶어 ㅠㅠㅠㅠ",
          "original_form": "아 일본가고싶어 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100005850.1.15",
          "form": "쌍안경 없으면 멀리서는 얼굴 보기도 힘들더라 콘서트",
          "original_form": "쌍안경 없으면 멀리서는 얼굴 보기도 힘들더라 콘서트",
          "speaker_id": "2",
          "time": "20210907 14:37"
        },
        {
          "id": "MDRW2100005850.1.16",
          "form": "일본 여러번 가보니까 사전조사하면 갈만하더라",
          "original_form": "일본 여러번 가보니까 사전조사하면 갈만하더라",
          "speaker_id": "2",
          "time": "20210907 14:37"
        },
        {
          "id": "MDRW2100005850.1.17",
          "form": "그치그치..",
          "original_form": "그치그치..",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100005850.1.18",
          "form": "아 어디든 좋으니 놀러가고싶네",
          "original_form": "아 어디든 좋으니 놀러가고싶네",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100005850.1.19",
          "form": "아니메이트 조지기",
          "original_form": "아니메이트 조지기",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100005850.1.20",
          "form": "나도 놀러가고싶어ㅠㅠ",
          "original_form": "나도 놀러가고싶어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100005850.1.21",
          "form": "같이 갈 사람이 없는거지",
          "original_form": "같이 갈 사람이 없는거지",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100005850.1.22",
          "form": "혼자가는것도 좋아 ㅋㅋㅋㅋ",
          "original_form": "혼자가는것도 좋아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100005850.1.23",
          "form": "나는 혼자 너무가서...",
          "original_form": "나는 혼자 너무가서...",
          "speaker_id": "2",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100005850.1.24",
          "form": "그리고 말 많은데 대화 할 상대 없으면 쓸쓸함",
          "original_form": "그리고 말 많은데 대화 할 상대 없으면 쓸쓸함",
          "speaker_id": "2",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100005850.1.25",
          "form": "아님 현지친구사귀기",
          "original_form": "아님 현지친구사귀기",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100005850.1.26",
          "form": "게하 같은데서 친구만드는건?!",
          "original_form": "게하 같은데서 친구만드는건?!",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100005850.1.27",
          "form": "와우 아무리 외향인 이어도 아직 거기까지는 무리인듯 싶음ㅋㅋ",
          "original_form": "와우 아무리 외향인 이어도 아직 거기까지는 무리인듯 싶음ㅋㅋ",
