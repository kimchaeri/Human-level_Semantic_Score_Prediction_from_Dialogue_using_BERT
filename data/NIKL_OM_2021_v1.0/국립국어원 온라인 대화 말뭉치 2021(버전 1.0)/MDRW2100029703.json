      "utterance": [
        {
          "id": "MDRW2100029703.1.1",
          "form": "안녕하세요 최근에 여행 갔다오신 데 있으세요?",
          "original_form": "안녕하세요 최근에 여행 갔다오신 데 있으세요?",
          "speaker_id": "1",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029703.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029703.1.3",
          "form": "코로나 때문에 어디 숙박은 못했고",
          "original_form": "코로나 때문에 어디 숙박은 못했고",
          "speaker_id": "2",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029703.1.4",
          "form": "가까운 양수리 다녀왔어요",
          "original_form": "가까운 양수리 다녀왔어요",
          "speaker_id": "2",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029703.1.5",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029703.1.6",
          "form": "저도 그냥 드라이브만 했는데",
          "original_form": "저도 그냥 드라이브만 했는데",
          "speaker_id": "1",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029703.1.7",
          "form": "속초 갔다왔었네요!",
          "original_form": "속초 갔다왔었네요!",
          "speaker_id": "1",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100029703.1.8",
          "form": "오오 속초 너무 좋죠!",
          "original_form": "오오 속초 너무 좋죠!",
          "speaker_id": "2",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100029703.1.9",
          "form": "먹을 것도 많구요!!!",
          "original_form": "먹을 것도 많구요!!!",
          "speaker_id": "2",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100029703.1.10",
          "form": "바다가 있어서 제일 좋았네요",
          "original_form": "바다가 있어서 제일 좋았네요",
          "speaker_id": "1",
          "time": "20210903 14:07"
        },
        {
          "id": "MDRW2100029703.1.11",
          "form": "다들 마스크 쓰고 놀고 계시더라고요....이상한 풍경",
          "original_form": "다들 마스크 쓰고 놀고 계시더라고요....이상한 풍경",
          "speaker_id": "1",
          "time": "20210903 14:07"
        },
        {
          "id": "MDRW2100029703.1.12",
          "form": "와 바다에서요??!",
          "original_form": "와 바다에서요??!",
          "speaker_id": "2",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100029703.1.13",
          "form": "저는 그 물 속에",
          "original_form": "저는 그 물 속에",
          "speaker_id": "2",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100029703.1.14",
          "form": "마스크를 쓰고 들어간다는 게 정말 이상해요",
          "original_form": "마스크를 쓰고 들어간다는 게 정말 이상해요",
          "speaker_id": "2",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100029703.1.15",
          "form": "워터파크도 그렇곸ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "워터파크도 그렇곸ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100029703.1.16",
          "form": "그니까요 다들 마스크 스고 물 속에 있는데 자꾸 인지부조화 왔네요 ㅋㅋㅋㅋㅋ",
          "original_form": "그니까요 다들 마스크 스고 물 속에 있는데 자꾸 인지부조화 왔네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029703.1.17",
          "form": "와... 안답답하나?!",
          "original_form": "와... 안답답하나?!",
          "speaker_id": "2",
          "time": "20210903 14:18"
        },
        {
          "id": "MDRW2100029703.1.18",
          "form": "숨막혀 죽을 거 같은데요?!?!",
          "original_form": "숨막혀 죽을 거 같은데요?!?!",
          "speaker_id": "2",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100029703.1.19",
          "form": "그니까요 ㅋㅋㅋㅋㅋ",
          "original_form": "그니까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100029703.1.20",
          "form": "거기서 유명한 기장떡?도 샀는데 맛잇더라고요",
          "original_form": "거기서 유명한 기장떡?도 샀는데 맛잇더라고요",
          "speaker_id": "1",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100029703.1.21",
          "form": "아 기장떡 알아요!!!!",
          "original_form": "아 기장떡 알아요!!!!",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029703.1.22",
          "form": "코로나만 아니면 여행 어디든지 다닐텐데요ㅠ",
          "original_form": "코로나만 아니면 여행 어디든지 다닐텐데요ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029703.1.23",
          "form": "해외는 ....바라지도 않아요,,,",
          "original_form": "해외는 ....바라지도 않아요,,,",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029703.1.24",
          "form": "국내로요?",
          "original_form": "국내로요?",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029703.1.25",
          "form": "국내 어디요?",
          "original_form": "국내 어디요?",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029703.1.26",
          "form": "제주도 가고싶어요~!~!",
          "original_form": "제주도 가고싶어요~!~!",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029703.1.27",
          "form": "제주도에는 맛집도 많고,,,",
          "original_form": "제주도에는 맛집도 많고,,,",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029703.1.28",
          "form": "갈 데가 많아보이더라구요~!",
          "original_form": "갈 데가 많아보이더라구요~!",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029703.1.29",
          "form": "제주도 전에 딱 한 번 가봤는데",
          "original_form": "제주도 전에 딱 한 번 가봤는데",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100029703.1.30",
          "form": "먹을 곳 진짜 많아요!!",
          "original_form": "먹을 곳 진짜 많아요!!",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100029703.1.31",
          "form": "비수기때 가서 사람도 없었고요",
          "original_form": "비수기때 가서 사람도 없었고요",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100029703.1.32",
          "form": "우와 비수기면 보통",
          "original_form": "우와 비수기면 보통",
          "speaker_id": "2",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100029703.1.33",
          "form": "4~6월 이 때일까요?",
          "original_form": "4~6월 이 때일까요?",
          "speaker_id": "2",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100029703.1.34",
          "form": "아니면 가을?!",
          "original_form": "아니면 가을?!",
          "speaker_id": "2",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100029703.1.35",
          "form": "네 딱 4~6월요!",
          "original_form": "네 딱 4~6월요!",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100029703.1.36",
          "form": "전 5월 말쯤 다녀왔는데 좋았어요 너무 ㅠㅠ",
          "original_form": "전 5월 말쯤 다녀왔는데 좋았어요 너무 ㅠㅠ",
