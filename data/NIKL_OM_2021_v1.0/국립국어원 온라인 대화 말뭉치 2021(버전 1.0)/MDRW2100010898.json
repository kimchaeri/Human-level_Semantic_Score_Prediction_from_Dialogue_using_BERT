      "utterance": [
        {
          "id": "MDRW2100010898.1.1",
          "form": "좋아하는 여행지가 있으신가요?",
          "original_form": "좋아하는 여행지가 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100010898.1.2",
          "form": "저는 해외는 안 가봐서",
          "original_form": "저는 해외는 안 가봐서",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100010898.1.3",
          "form": "서울이 최애입니다",
          "original_form": "서울이 최애입니다",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100010898.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100010898.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ요즘에 정말 귀중한 마인드세요..서울이 최고죠ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ요즘에 정말 귀중한 마인드세요..서울이 최고죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100010898.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 게다가 제가 지방 사람인지라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 게다가 제가 지방 사람인지라",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010898.1.7",
          "form": "더 그러는 거 같아요",
          "original_form": "더 그러는 거 같아요",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010898.1.8",
          "form": "저도 강남이랑 명동에 갈 때마다 새로워요ㅋㅋㅋ 그 자체가 관광지죠ㅋㅋ",
          "original_form": "저도 강남이랑 명동에 갈 때마다 새로워요ㅋㅋㅋ 그 자체가 관광지죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010898.1.9",
          "form": "진짜 건물 높다는 게",
          "original_form": "진짜 건물 높다는 게",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010898.1.10",
          "form": "뭔말인지 알겠더라구요",
          "original_form": "뭔말인지 알겠더라구요",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010898.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010898.1.12",
          "form": "약간 건물이 무너지면 아무리 달려도 살아나기 힘든 높이예요ㅋㅋㅋ",
          "original_form": "약간 건물이 무너지면 아무리 달려도 살아나기 힘든 높이예요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010898.1.13",
          "form": "정말요 지진이라도 크게 나면 ㅠㅠ",
          "original_form": "정말요 지진이라도 크게 나면 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100010898.1.14",
          "form": "저는 근데 높은 건물 많은 도시에",
          "original_form": "저는 근데 높은 건물 많은 도시에",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100010898.1.15",
          "form": "로망이 있어서",
          "original_form": "로망이 있어서",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100010898.1.16",
          "form": "뉴욕 같은데 가보고 싶어요",
          "original_form": "뉴욕 같은데 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100010898.1.17",
          "form": "뉴욕 스파이더맨 보고 더 로망이 생겼어요",
          "original_form": "뉴욕 스파이더맨 보고 더 로망이 생겼어요",
          "speaker_id": "2",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100010898.1.18",
          "form": "아 그 파프롬홈이던가요?",
          "original_form": "아 그 파프롬홈이던가요?",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100010898.1.19",
          "form": "저는 요즘 몽골에 별이 그렇게 예쁘대서 몽골에 로망이 생겼습니다",
          "original_form": "저는 요즘 몽골에 별이 그렇게 예쁘대서 몽골에 로망이 생겼습니다",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100010898.1.20",
          "form": "몽골은 정말 밤하늘이 기대돼요...",
          "original_form": "몽골은 정말 밤하늘이 기대돼요...",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100010898.1.21",
          "form": "정말요... 별 박힌 하늘이 정말 그렇게 예쁘다더라고요",
          "original_form": "정말요... 별 박힌 하늘이 정말 그렇게 예쁘다더라고요",
          "speaker_id": "1",
