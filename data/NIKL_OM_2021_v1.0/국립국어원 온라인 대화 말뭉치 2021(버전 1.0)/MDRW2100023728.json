        },
        {
          "id": "MDRW2100023728.1.2",
          "form": "오늘 저녁은 뭐 먹습니까",
          "original_form": "오늘 저녁은 뭐 먹습니까",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023728.1.3",
          "form": "나한테 배상 겹살",
          "original_form": "나한테 배상 겹살",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023728.1.4",
          "form": "ㅋㅋㅋ 난 대패삼겹살",
          "original_form": "ㅋㅋㅋ 난 대패삼겹살",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023728.1.5",
          "form": "난 된찌 밥",
          "original_form": "난 된찌 밥",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100023728.1.6",
          "form": "치우고 설거지 산이다ㅡㅡ",
          "original_form": "치우고 설거지 산이다ㅡㅡ",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100023728.1.7",
          "form": "아 진짜 설거지가 제일 싫음",
          "original_form": "아 진짜 설거지가 제일 싫음",
          "speaker_id": "2",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023728.1.8",
          "form": "이제 애들 재우러 갑니다",
          "original_form": "이제 애들 재우러 갑니다",
          "speaker_id": "2",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023728.1.9",
          "form": "육회 하고 만납시다",
          "original_form": "육회 하고 만납시다",
          "speaker_id": "2",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023728.1.10",
          "form": "육퇴 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "육퇴 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023728.1.11",
          "form": "벌써?",
          "original_form": "벌써?",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100023728.1.12",
          "form": "빠르네ㅋ",
          "original_form": "빠르네ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100023728.1.13",
          "form": "육퇴를\n한건아니고",
          "original_form": "육퇴를\n한건아니고",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100023728.1.14",
          "form": "육퇴를 하러 간다고 했지",
          "original_form": "육퇴를 하러 간다고 했지",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100023728.1.15",
          "form": "자나몰라",
          "original_form": "자나몰라",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100023728.1.16",
          "form": "아ㅋ",
          "original_form": "아ㅋ",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100023728.1.17",
          "form": "우린 잔다 오늘 디게 안잘라하네 월요일인데",
          "original_form": "우린 잔다 오늘 디게 안잘라하네 월요일인데",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100023728.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 언니도 어여주무슈",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 언니도 어여주무슈",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100023728.1.19",
          "form": "나도 이제 자러갑니다",
          "original_form": "나도 이제 자러갑니다",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100023728.1.20",
          "form": "눈뜨슈 ㅎㅎ",
          "original_form": "눈뜨슈 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 09:09"
        },
        {
          "id": "MDRW2100023728.1.21",
          "form": "출근했슈??",
          "original_form": "출근했슈??",
          "speaker_id": "2",
          "time": "20210831 09:09"
        },
        {
          "id": "MDRW2100023728.1.22",
