      "utterance": [
        {
          "id": "MDRW2100006399.1.1",
          "form": "동인비 팩 써보셨어요?",
          "original_form": "동인비 팩 써보셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006399.1.2",
          "form": "아뇨. 얼굴에 잘 안발라요",
          "original_form": "아뇨. 얼굴에 잘 안발라요",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006399.1.3",
          "form": "전에 받은거  팩하는데 좀바꼈네요",
          "original_form": "전에 받은거  팩하는데 좀바꼈네요",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006399.1.4",
          "form": "동인비 좋다고는 하더라구요",
          "original_form": "동인비 좋다고는 하더라구요",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006399.1.5",
          "form": "예전은 좀 젤리같은 거였는데",
          "original_form": "예전은 좀 젤리같은 거였는데",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006399.1.6",
          "form": "일반시트에 에센스 줄줄 흐르네요ㅋ",
          "original_form": "일반시트에 에센스 줄줄 흐르네요ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006399.1.7",
          "form": "에센스만 많이 넣었나보네요",
          "original_form": "에센스만 많이 넣었나보네요",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006399.1.8",
          "form": "젤리시트가 더 좋을꺼같아요",
          "original_form": "젤리시트가 더 좋을꺼같아요",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006399.1.9",
          "form": "가을오면 건조하니 팩을 하긴해야겠어요",
          "original_form": "가을오면 건조하니 팩을 하긴해야겠어요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006399.1.10",
          "form": "얼굴이 많이 상할듯은해요",
          "original_form": "얼굴이 많이 상할듯은해요",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006399.1.11",
          "form": "덥다 추워지니",
          "original_form": "덥다 추워지니",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006399.1.12",
          "form": "마스크써서 더그래요",
          "original_form": "마스크써서 더그래요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006399.1.13",
          "form": "여드름처럼 올라오긴 하더라구요",
          "original_form": "여드름처럼 올라오긴 하더라구요",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006399.1.14",
          "form": "트러블 없어질 날이 없네요",
          "original_form": "트러블 없어질 날이 없네요",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006399.1.15",
          "form": "많이쓰는편도 아닌데 그래요",
          "original_form": "많이쓰는편도 아닌데 그래요",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006399.1.16",
          "form": "저도 출퇴근길 잠깐쓰는데 그러네요",
          "original_form": "저도 출퇴근길 잠깐쓰는데 그러네요",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006399.1.17",
          "form": "하루종일 쓰고계심 힘드시겠어요",
          "original_form": "하루종일 쓰고계심 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006399.1.18",
          "form": "사무실에서 벗고있어요ㅋ",
          "original_form": "사무실에서 벗고있어요ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006399.1.19",
          "form": "출퇴근에만 쓰세요?",
          "original_form": "출퇴근에만 쓰세요?",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
