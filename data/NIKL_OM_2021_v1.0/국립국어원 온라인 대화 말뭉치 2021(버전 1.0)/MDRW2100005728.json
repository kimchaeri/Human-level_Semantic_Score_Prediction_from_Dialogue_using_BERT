      "utterance": [
        {
          "id": "MDRW2100005728.1.1",
          "form": "쇼핑?",
          "original_form": "쇼핑?",
          "speaker_id": "2",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100005728.1.2",
          "form": "머 샀어요?",
          "original_form": "머 샀어요?",
          "speaker_id": "2",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100005728.1.3",
          "form": "아 저 펩시제로삼",
          "original_form": "아 저 펩시제로삼",
          "speaker_id": "2",
          "time": "20210907 10:09"
        },
        {
          "id": "MDRW2100005728.1.4",
          "form": "사실 음식하려다 잘못누름 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사실 음식하려다 잘못누름 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:11"
        },
        {
          "id": "MDRW2100005728.1.5",
          "form": "아 어제 샐러드삿움",
          "original_form": "아 어제 샐러드삿움",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100005728.1.6",
          "form": "포케다 포케",
          "original_form": "포케다 포케",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100005728.1.7",
          "form": "졸맛 ㅠ",
          "original_form": "졸맛 ㅠ",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100005728.1.8",
          "form": "포케?? 그게머지",
          "original_form": "포케?? 그게머지",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005728.1.9",
          "form": "샐러드 얼마짜리에요?",
          "original_form": "샐러드 얼마짜리에요?",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005728.1.10",
          "form": "한끼?",
          "original_form": "한끼?",
          "speaker_id": "2",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100005728.1.11",
          "form": "아 이거",
          "original_form": "아 이거",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005728.1.12",
          "form": "회샐러드ㅋㅋㅋ",
          "original_form": "회샐러드ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005728.1.13",
          "form": "종원이형이 만드는거 봄요",
          "original_form": "종원이형이 만드는거 봄요",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005728.1.14",
          "form": "회샐러드라니 ㅋㅋ전어제 불고기먹음요",
          "original_form": "회샐러드라니 ㅋㅋ전어제 불고기먹음요",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005728.1.15",
          "form": "8900짜리",
          "original_form": "8900짜리",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100005728.1.16",
          "form": "뚝불?",
          "original_form": "뚝불?",
          "speaker_id": "2",
          "time": "20210907 10:17"
        },
        {
          "id": "MDRW2100005728.1.17",
          "form": "어제 아부지 아는분이 꽃게줘서",
          "original_form": "어제 아부지 아는분이 꽃게줘서",
          "speaker_id": "2",
          "time": "20210907 10:17"
        },
        {
          "id": "MDRW2100005728.1.18",
          "form": "아니 불고기포케",
          "original_form": "아니 불고기포케",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.19",
          "form": "꽃게탕이랑 라면먹음",
          "original_form": "꽃게탕이랑 라면먹음",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.21",
          "form": "갑각류 안좋아함",
          "original_form": "갑각류 안좋아함",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.22",
          "form": "게 맛있어요?",
          "original_form": "게 맛있어요?",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.23",
          "form": "ㄴㄴ",
          "original_form": "ㄴㄴ",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.24",
          "form": "인건비도 안나옴",
          "original_form": "인건비도 안나옴",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.25",
          "form": "게 먹다보면 짱남",
          "original_form": "게 먹다보면 짱남",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.26",
          "form": "먹은것도 아니고",
          "original_form": "먹은것도 아니고",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.27",
          "form": "안먹는것도아니고",
          "original_form": "안먹는것도아니고",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100005728.1.29",
          "form": "랍스타 킹크랩도 안먹어요?",
          "original_form": "랍스타 킹크랩도 안먹어요?",
          "speaker_id": "1",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100005728.1.30",
          "form": "주면 먹어요",
          "original_form": "주면 먹어요",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100005728.1.31",
          "form": "가리는거 없어요",
          "original_form": "가리는거 없어요",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100005728.1.32",
          "form": "썩은거 빼고",
          "original_form": "썩은거 빼고",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100005728.1.33",
          "form": "맛이 매우 없거나",
          "original_form": "맛이 매우 없거나",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100005728.1.34",
          "form": "그건다행이네요ㅕ",
          "original_form": "그건다행이네요ㅕ",
          "speaker_id": "1",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100005728.1.35",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100005728.1.36",
          "form": "껍데기는 안먹음",
          "original_form": "껍데기는 안먹음",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100005728.1.37",
          "form": "알죠그건 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "알죠그건 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100005728.1.38",
          "form": "포케 저거 장사 잘한듯",
          "original_form": "포케 저거 장사 잘한듯",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100005728.1.39",
          "form": "가격도 다른거에 비해서",
          "original_form": "가격도 다른거에 비해서",
          "speaker_id": "2",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100005728.1.40",
          "form": "그렇게 비싼 느낌도 아니고",
          "original_form": "그렇게 비싼 느낌도 아니고",
          "speaker_id": "2",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100005728.1.41",
          "form": "근데 놀라운건 전에도 이런거했었는데 다 승인됐음",
          "original_form": "근데 놀라운건 전에도 이런거했었는데 다 승인됐음",
          "speaker_id": "1",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100005728.1.42",
          "form": "어제진짜 감격하면서먹었음 ㅠㅠ졸맛",
          "original_form": "어제진짜 감격하면서먹었음 ㅠㅠ졸맛",
          "speaker_id": "1",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100005728.1.43",
          "form": "제가보기엔 이거 알바생이 그냥 올라오면 확인버튼만 겁나 누름",
          "original_form": "제가보기엔 이거 알바생이 그냥 올라오면 확인버튼만 겁나 누름",
          "speaker_id": "2",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100005728.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋ",
