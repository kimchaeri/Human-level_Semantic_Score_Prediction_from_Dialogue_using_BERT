      "utterance": [
        {
          "id": "MDRW2100005325.1.1",
          "form": "굿모닝~ 오늘 아침은 코울슬로인가?",
          "original_form": "굿모닝~ 오늘 아침은 코울슬로인가?",
          "speaker_id": "2",
          "time": "20210822 09:12"
        },
        {
          "id": "MDRW2100005325.1.2",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100005325.1.3",
          "form": "코울슬로 맛있더라",
          "original_form": "코울슬로 맛있더라",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100005325.1.4",
          "form": "코울슬로 이번 여름 맛템이지",
          "original_form": "코울슬로 이번 여름 맛템이지",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100005325.1.5",
          "form": "그러게 시원하고 아삭아삭하고",
          "original_form": "그러게 시원하고 아삭아삭하고",
          "speaker_id": "1",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100005325.1.6",
          "form": "양배추 활용하려고 만든 메뉴인데",
          "original_form": "양배추 활용하려고 만든 메뉴인데",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100005325.1.7",
          "form": "식감이 좋아",
          "original_form": "식감이 좋아",
          "speaker_id": "1",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100005325.1.8",
          "form": "채소도 많이 섭취하고 좋네",
          "original_form": "채소도 많이 섭취하고 좋네",
          "speaker_id": "1",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100005325.1.9",
          "form": "양배추 씻고 써는 작업만 하면",
          "original_form": "양배추 씻고 써는 작업만 하면",
          "speaker_id": "2",
          "time": "20210822 11:20"
        },
        {
          "id": "MDRW2100005325.1.10",
          "form": "아 써는 게 좀 힘들지",
          "original_form": "아 써는 게 좀 힘들지",
          "speaker_id": "1",
          "time": "20210822 11:20"
        },
        {
          "id": "MDRW2100005325.1.11",
          "form": "나머지 재료 썰어서 드레싱에 버무리면 되니까",
          "original_form": "나머지 재료 썰어서 드레싱에 버무리면 되니까",
          "speaker_id": "2",
          "time": "20210822 11:20"
        },
        {
          "id": "MDRW2100005325.1.12",
          "form": "점심은 전어에 밥! 그리고 밑반찬",
          "original_form": "점심은 전어에 밥! 그리고 밑반찬",
          "speaker_id": "2",
          "time": "20210822 11:21"
        },
        {
          "id": "MDRW2100005325.1.13",
          "form": "오랜만에 밥 먹겠네",
          "original_form": "오랜만에 밥 먹겠네",
          "speaker_id": "2",
          "time": "20210822 11:21"
        },
        {
          "id": "MDRW2100005325.1.14",
          "form": "밥 우리 잡채밥도 먹었는데",
          "original_form": "밥 우리 잡채밥도 먹었는데",
          "speaker_id": "1",
          "time": "20210822 11:22"
        },
        {
          "id": "MDRW2100005325.1.15",
          "form": "엄청 오랜만은 아님",
          "original_form": "엄청 오랜만은 아님",
          "speaker_id": "1",
          "time": "20210822 11:22"
        },
        {
          "id": "MDRW2100005325.1.16",
          "form": "아니 집에서 한 잡곡밥 말이야~",
          "original_form": "아니 집에서 한 잡곡밥 말이야~",
          "speaker_id": "2",
          "time": "20210822 11:23"
        },
        {
          "id": "MDRW2100005325.1.17",
          "form": "아 오랜만인가?",
          "original_form": "아 오랜만인가?",
          "speaker_id": "1",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100005325.1.18",
          "form": "며칠 전에 푸팟퐁커리랑 먹고",
          "original_form": "며칠 전에 푸팟퐁커리랑 먹고",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100005325.1.19",
          "form": "열무비빔밥 먹은게 꽤 되었나..? 그렇게 오래 된 기분이 안드네",
          "original_form": "열무비빔밥 먹은게 꽤 되었나..? 그렇게 오래 된 기분이 안드네",
          "speaker_id": "1",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100005325.1.20",
          "form": "아 카레도 먹었지",
          "original_form": "아 카레도 먹었지",
          "speaker_id": "1",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100005325.1.21",
          "form": "응 밑반찬 잔뜩이랑",
          "original_form": "응 밑반찬 잔뜩이랑",
          "speaker_id": "2",
          "time": "20210822 11:24"
        }
      ]
