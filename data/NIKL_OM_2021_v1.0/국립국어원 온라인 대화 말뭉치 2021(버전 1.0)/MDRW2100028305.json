      "utterance": [
        {
          "id": "MDRW2100028305.1.1",
          "form": "이제와서 생각하면",
          "original_form": "이제와서 생각하면",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028305.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028305.1.3",
          "form": "학점에 신경을 쓰든 안쓰든..결과가 그닥 달라지는 것 같진 않더라구요",
          "original_form": "학점에 신경을 쓰든 안쓰든..결과가 그닥 달라지는 것 같진 않더라구요",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028305.1.4",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028305.1.5",
          "form": "글고 저 전공살리지도않았고...",
          "original_form": "글고 저 전공살리지도않았고...",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028305.1.6",
          "form": "와 진짜 전공 살리기가 장난 아니더라고요",
          "original_form": "와 진짜 전공 살리기가 장난 아니더라고요",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028305.1.7",
          "form": "맞아여맞아여ㅜ",
          "original_form": "맞아여맞아여ㅜ",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028305.1.8",
          "form": "저는 취업이 정해져있는 과라 괜찮았는데 다른 친구 이야기 들어보면",
          "original_form": "저는 취업이 정해져있는 과라 괜찮았는데 다른 친구 이야기 들어보면",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028305.1.9",
          "form": "너무 고생 많이하더라고요",
          "original_form": "너무 고생 많이하더라고요",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028305.1.10",
          "form": "그쳐 전공살릴수있는 과가 몇없어요ㅜ",
          "original_form": "그쳐 전공살릴수있는 과가 몇없어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028305.1.11",
          "form": "팀플 과제하실때 포지션은 보통뭐하셨어요",
          "original_form": "팀플 과제하실때 포지션은 보통뭐하셨어요",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028305.1.12",
          "form": "저는.......아득한 기억이지만",
          "original_form": "저는.......아득한 기억이지만",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028305.1.13",
          "form": "약간 올플레이어라 맞춰갔던 것 같아요",
          "original_form": "약간 올플레이어라 맞춰갔던 것 같아요",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028305.1.14",
          "form": "자료조사가 젬병이라 그것만 빼고...",
          "original_form": "자료조사가 젬병이라 그것만 빼고...",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028305.1.15",
          "form": "오오 ㅋㅋㅋㅋㅋ멋있으세요",
          "original_form": "오오 ㅋㅋㅋㅋㅋ멋있으세요",
