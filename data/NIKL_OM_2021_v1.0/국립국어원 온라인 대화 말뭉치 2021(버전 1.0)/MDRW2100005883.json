      "utterance": [
        {
          "id": "MDRW2100005883.1.1",
          "form": "사실 책을 빌리기만하고",
          "original_form": "사실 책을 빌리기만하고",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.2",
          "form": "안읽고반납해서",
          "original_form": "안읽고반납해서",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.3",
          "form": "그게 쌓이니까",
          "original_form": "그게 쌓이니까",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.4",
          "form": "name2씨도 책 추천해 줘!!",
          "original_form": "&name2&씨도 책 추천해 줘!!",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.5",
          "form": "다독상을 받게되었다",
          "original_form": "다독상을 받게되었다",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.6",
          "form": "아니 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.7",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.8",
          "form": "나아는",
          "original_form": "나아는",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.9",
          "form": "그 단편집인데",
          "original_form": "그 단편집인데",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.10",
          "form": "칵테일 러브 좀비",
          "original_form": "칵테일 러브 좀비",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005883.1.11",
          "form": "라는책을 읽었는데 너무 재밌었어서",
          "original_form": "라는책을 읽었는데 너무 재밌었어서",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005883.1.12",
          "form": "일상판타지라고해야하나",
          "original_form": "일상판타지라고해야하나",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005883.1.13",
          "form": "엄청짧아",
          "original_form": "엄청짧아",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005883.1.14",
          "form": "집중하면",
          "original_form": "집중하면",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005883.1.15",
          "form": "3시간만에읽을걸",
          "original_form": "3시간만에읽을걸",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005883.1.16",
          "form": "물론 더 빨리읽을수도있고",
          "original_form": "물론 더 빨리읽을수도있고",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005883.1.17",
          "form": "외국 소설이야??",
          "original_form": "외국 소설이야??",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005883.1.18",
          "form": "노노",
          "original_form": "노노",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005883.1.19",
          "form": "한국소설이양",
          "original_form": "한국소설이양",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005883.1.20",
          "form": "앗 혹시 작가가 누구...??",
          "original_form": "앗 혹시 작가가 누구...??",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005883.1.21",
          "form": "조예은",
          "original_form": "조예은",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005883.1.22",
          "form": "작가님이여",
          "original_form": "작가님이여",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005883.1.23",
          "form": "첨 들어보는 분이네..",
          "original_form": "첨 들어보는 분이네..",
          "speaker_id": "1",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100005883.1.24",
          "form": "예아...",
          "original_form": "예아...",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100005883.1.25",
          "form": "나도친구가",
          "original_form": "나도친구가",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100005883.1.26",
          "form": "탐라에서 봤다면서",
          "original_form": "탐라에서 봤다면서",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100005883.1.27",
          "form": "나보고읽어보라고",
          "original_form": "나보고읽어보라고",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.28",
          "form": "추천해줬는데",
          "original_form": "추천해줬는데",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.29",
          "form": "너무재밌게잘읽었던...",
          "original_form": "너무재밌게잘읽었던...",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.30",
          "form": "이후에 친구랑",
          "original_form": "이후에 친구랑",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.31",
          "form": "템플스테이가서",
          "original_form": "템플스테이가서",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.32",
          "form": "너무할거없어서",
          "original_form": "너무할거없어서",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.33",
          "form": "그걸 이북으로 저장해놨거든",
          "original_form": "그걸 이북으로 저장해놨거든",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.34",
          "form": "그래서 친구한테",
          "original_form": "그래서 친구한테",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.35",
          "form": "심심하면",
          "original_form": "심심하면",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.36",
          "form": "이거읽어봐라",
          "original_form": "이거읽어봐라",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.37",
          "form": "하고냅다줬더니",
          "original_form": "하고냅다줬더니",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.38",
          "form": "너무잘읽는거야",
          "original_form": "너무잘읽는거야",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.39",
          "form": "엄청재밌다고했엉",
          "original_form": "엄청재밌다고했엉",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.40",
          "form": "함 읽어봐야겠군...",
          "original_form": "함 읽어봐야겠군...",
          "speaker_id": "1",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100005883.1.41",
          "form": "아 템플스테이때 읽은거야?? ㅋㅋㅋ",
          "original_form": "아 템플스테이때 읽은거야?? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100005883.1.42",
          "form": "아 나는 그 전에읽었구",
          "original_form": "아 나는 그 전에읽었구",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100005883.1.43",
          "form": "친구는 템플스테이에서읽은ㅋㅋㅋㅋ",
          "original_form": "친구는 템플스테이에서읽은ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100005883.1.44",
          "form": "너무할게없다보니",
          "original_form": "너무할게없다보니",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100005883.1.45",
          "form": "저절로책도읽게되더라구...",
          "original_form": "저절로책도읽게되더라구...",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100005883.1.46",
          "form": "아하아하...",
          "original_form": "아하아하...",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100005883.1.47",
          "form": "껄껄",
          "original_form": "껄껄",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100005883.1.48",
          "form": "난독증 ㅁㅊ...",
          "original_form": "난독증 ㅁㅊ...",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005883.1.49",
          "form": "^^...",
          "original_form": "^^...",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005883.1.50",
          "form": "아냐",
          "original_form": "아냐",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005883.1.51",
          "form": "내가 설명을",
          "original_form": "내가 설명을",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005883.1.52",
          "form": "개떡같이해서그랴",
          "original_form": "개떡같이해서그랴",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005883.1.53",
          "form": "요즘 읽고있는책은있엉?",
          "original_form": "요즘 읽고있는책은있엉?",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005883.1.54",
          "form": "나는 ..",
          "original_form": "나는 ..",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005883.1.55",
          "form": "짜라투스트라는 이렇게 말했다였나",
          "original_form": "짜라투스트라는 이렇게 말했다였나",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005883.1.56",
          "form": "제목 아무리 말해도 외워지지않는",
          "original_form": "제목 아무리 말해도 외워지지않는",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005883.1.57",
          "form": "암튼 그거 100분의 1도 못읽은듯",
          "original_form": "암튼 그거 100분의 1도 못읽은듯",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005883.1.58",
          "form": "젠장 책 도대체 언제읽지",
          "original_form": "젠장 책 도대체 언제읽지",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005883.1.59",
          "form": "책을 도대체 언제읽어야하는걸까",
          "original_form": "책을 도대체 언제읽어야하는걸까",
