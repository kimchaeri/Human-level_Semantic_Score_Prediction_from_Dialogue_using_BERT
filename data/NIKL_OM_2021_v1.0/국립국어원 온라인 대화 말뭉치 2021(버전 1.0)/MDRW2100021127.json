      "utterance": [
        {
          "id": "MDRW2100021127.1.1",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100021127.1.2",
          "form": "피 안와요:) 거기는요?",
          "original_form": "피 안와요:) 거기는요?",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021127.1.3",
          "form": "비",
          "original_form": "비",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021127.1.4",
          "form": "비는 안와도 축축하네요",
          "original_form": "비는 안와도 축축하네요",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021127.1.5",
          "form": "습기가 많아요~ 오늘은 그래도 덜한거 같아요~ \n그런데 담주도 쭉 습할 거 같아요 ㅜ",
          "original_form": "습기가 많아요~ 오늘은 그래도 덜한거 같아요~ \n그런데 담주도 쭉 습할 거 같아요 ㅜ",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100021127.1.6",
          "form": "새벽에 비 마구와서 갑자기 추워짐.. ㅎㅎ",
          "original_form": "새벽에 비 마구와서 갑자기 추워짐.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100021127.1.7",
          "form": "아 그래요?? 여기는 안오는데 \n춥겠어요",
          "original_form": "아 그래요?? 여기는 안오는데 \n춥겠어요",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100021127.1.8",
          "form": "일기예보 비 없엇는데.. 새벽에 빗소리에.. 깼어요",
          "original_form": "일기예보 비 없엇는데.. 새벽에 빗소리에.. 깼어요",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100021127.1.9",
          "form": "아 그래서 일찍 일어나셨군요",
          "original_form": "아 그래서 일찍 일어나셨군요",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100021127.1.10",
          "form": "ㅋㅋ 지금도 선풍기 트시나여?",
          "original_form": "ㅋㅋ 지금도 선풍기 트시나여?",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100021127.1.11",
          "form": "네 약풍정도는 막 움직이면 더워서 ㅋㅋㅋㅋ",
          "original_form": "네 약풍정도는 막 움직이면 더워서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100021127.1.12",
          "form": "요즘 날씨 너무 최악이에요.. \n선풍기틀면 춥고.. 끄면 덥고..",
          "original_form": "요즘 날씨 너무 최악이에요.. \n선풍기틀면 춥고.. 끄면 덥고..",
          "speaker_id": "1",
          "time": "20210829 11:57"
        },
        {
          "id": "MDRW2100021127.1.13",
          "form": "너무 습하면 긴옷입고 에어컨 클도.  ㅋㅋ",
          "original_form": "너무 습하면 긴옷입고 에어컨 클도.  ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:57"
        },
        {
          "id": "MDRW2100021127.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 제습기 해도 습하죠? ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 제습기 해도 습하죠? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100021127.1.15",
          "form": "멍청이같지만 인센스나 캔들 키고 더워지면 ㅋㅋㅋㅋㅋㅋ 선풍기 틀어욬ㅋ 뭐하는짓인가 싶지만",
          "original_form": "멍청이같지만 인센스나 캔들 키고 더워지면 ㅋㅋㅋㅋㅋㅋ 선풍기 틀어욬ㅋ 뭐하는짓인가 싶지만",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100021127.1.16",
          "form": "인센스가 뭐애요?",
          "original_form": "인센스가 뭐애요?",
          "speaker_id": "1",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100021127.1.17",
          "form": "인센스 스틱이요 ㅋㅋㅋㅋ",
          "original_form": "인센스 스틱이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:03"
        },
        {
          "id": "MDRW2100021127.1.18",
          "form": "아.. 저 막대기 방향제 이른이 그거구나ㅡㅡ",
          "original_form": "아.. 저 막대기 방향제 이른이 그거구나ㅡㅡ",
