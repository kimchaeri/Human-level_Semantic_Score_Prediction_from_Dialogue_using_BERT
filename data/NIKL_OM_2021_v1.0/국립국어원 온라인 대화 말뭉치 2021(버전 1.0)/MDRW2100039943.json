      "utterance": [
        {
          "id": "MDRW2100039943.1.1",
          "form": "가디건 입으려고 하나 샀죠",
          "original_form": "가디건 입으려고 하나 샀죠",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039943.1.2",
          "form": "니트도 대기중입니다",
          "original_form": "니트도 대기중입니다",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039943.1.3",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039943.1.4",
          "form": "배색카라있는 검은 니트 살까요 없는거 살까요",
          "original_form": "배색카라있는 검은 니트 살까요 없는거 살까요",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039943.1.5",
          "form": "있는게 이쁜거 같아요",
          "original_form": "있는게 이쁜거 같아요",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039943.1.6",
          "form": "포인트가 살아서",
          "original_form": "포인트가 살아서",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039943.1.7",
          "form": "오 그런가요 포인트 !",
          "original_form": "오 그런가요 포인트 !",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039943.1.8",
          "form": "그리고 배색카라 있는게 좀 더 귀여운거 같아요",
          "original_form": "그리고 배색카라 있는게 좀 더 귀여운거 같아요",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039943.1.9",
          "form": "배색으로 샀어요 감사합니다",
          "original_form": "배색으로 샀어요 감사합니다",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039943.1.10",
          "form": "방금 주문헸어요",
          "original_form": "방금 주문헸어요",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039943.1.11",
          "form": "ㅎㅎ 이쁘게 입으시길 바래요",
          "original_form": "ㅎㅎ 이쁘게 입으시길 바래요",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039943.1.12",
          "form": "네네 얼른오길!!",
          "original_form": "네네 얼른오길!!",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100039943.1.13",
          "form": "그런 스타일 좋아하시나봐요",
          "original_form": "그런 스타일 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100039943.1.14",
          "form": "뭔가 프렌치 시크 추구하지먼",
          "original_form": "뭔가 프렌치 시크 추구하지먼",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100039943.1.15",
          "form": "그게그거에요",
          "original_form": "그게그거에요",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100039943.1.16",
          "form": "ㅋㅋ 그런가요",
          "original_form": "ㅋㅋ 그런가요",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100039943.1.17",
          "form": "네넼ㅋㅋㅋㅋ",
          "original_form": "네넼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039943.1.18",
          "form": "그냥 제옷이에요ㅠㅜ 항상 고민해서 사도 ㅋㅋ",
          "original_form": "그냥 제옷이에요ㅠㅜ 항상 고민해서 사도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039943.1.19",
          "form": "고민해서 구매하는게 어딘가요",
          "original_form": "고민해서 구매하는게 어딘가요",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100039943.1.20",
          "form": "저는 꽂히면 지르고 끝나요ㅋㅋㅋ",
          "original_form": "저는 꽂히면 지르고 끝나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100039943.1.21",
          "form": "아정말요 그게 좋은데",
          "original_form": "아정말요 그게 좋은데",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100039943.1.22",
          "form": "고민은 고민대로 돈은 돈대로 랍니다",
          "original_form": "고민은 고민대로 돈은 돈대로 랍니다",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100039943.1.23",
          "form": "시원하게 지르는 비결좀 ㅋㅋㅋㅋ어차피살거 ㅋㅋㅋ",
          "original_form": "시원하게 지르는 비결좀 ㅋㅋㅋㅋ어차피살거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100039943.1.24",
          "form": "돈 모을 때에는 쇼핑 검색 1도 안하다가",
          "original_form": "돈 모을 때에는 쇼핑 검색 1도 안하다가",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100039943.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100039943.1.26",
          "form": "어느 정도 모이면 그때 찾아봐요",
          "original_form": "어느 정도 모이면 그때 찾아봐요",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100039943.1.27",
          "form": "아아 돈 모을때는 빡세게 하시는군요",
          "original_form": "아아 돈 모을때는 빡세게 하시는군요",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100039943.1.28",
          "form": "저도 그래여겠다",
          "original_form": "저도 그래여겠다",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100039943.1.29",
          "form": "인스타를 안둘어가야겠어요ㅠㅠ",
          "original_form": "인스타를 안둘어가야겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 00:37"
        }
      ]
    }
