      "utterance": [
        {
          "id": "MDRW2100005917.1.1",
          "form": "이슬람교의 창시자 무함마드는",
          "original_form": "이슬람교의 창시자 무함마드는",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100005917.1.2",
          "form": "동굴에서 기도하던 중에 대천사 가브리엘의 계시를 받았는데",
          "original_form": "동굴에서 기도하던 중에 대천사 가브리엘의 계시를 받았는데",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100005917.1.3",
          "form": "그 계시가 이슬람 경전 쿠란이야",
          "original_form": "그 계시가 이슬람 경전 쿠란이야",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100005917.1.4",
          "form": "바쁘게살았군",
          "original_form": "바쁘게살았군",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005917.1.5",
          "form": "아까 유대교는 예수를 이단으로 보는 반면",
          "original_form": "아까 유대교는 예수를 이단으로 보는 반면",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005917.1.6",
          "form": "기독교에선 신으로 생각한다 했잖아",
          "original_form": "기독교에선 신으로 생각한다 했잖아",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100005917.1.7",
          "form": "예아",
          "original_form": "예아",
          "speaker_id": "2",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100005917.1.8",
          "form": "이슬람교에선 예수님을 예언자들 중 한 명으로 봐",
          "original_form": "이슬람교에선 예수님을 예언자들 중 한 명으로 봐",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100005917.1.9",
          "form": "모세 아브라함 등등과 같은 급으로",
          "original_form": "모세 아브라함 등등과 같은 급으로",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100005917.1.10",
          "form": "그래서 심지어 예수님의 재림도 믿는데",
          "original_form": "그래서 심지어 예수님의 재림도 믿는데",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100005917.1.11",
          "form": "유일하게 부활만 안 믿는대",
          "original_form": "유일하게 부활만 안 믿는대",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005917.1.12",
          "form": "헤엑",
          "original_form": "헤엑",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005917.1.13",
          "form": "신이 아닌 사람이 죽었다 살아나는 건 불가능하기 때문에",
          "original_form": "신이 아닌 사람이 죽었다 살아나는 건 불가능하기 때문에",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005917.1.14",
          "form": "오오오옹",
          "original_form": "오오오옹",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005917.1.15",
          "form": "그래서 예수가 십자가에 못 박힌게 아니고",
          "original_form": "그래서 예수가 십자가에 못 박힌게 아니고",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005917.1.16",
          "form": "예아",
          "original_form": "예아",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005917.1.17",
          "form": "사실 십자가에 매달린건 가롯 유다인데",
          "original_form": "사실 십자가에 매달린건 가롯 유다인데",
          "speaker_id": "1",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100005917.1.18",
          "form": "신이 환상을 써서 유다를 예수처럼 보이게 했다고 믿는다더라...",
          "original_form": "신이 환상을 써서 유다를 예수처럼 보이게 했다고 믿는다더라...",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005917.1.19",
          "form": "오오오오오ㅗ",
          "original_form": "오오오오오ㅗ",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005917.1.20",
          "form": "대박이다",
          "original_form": "대박이다",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005917.1.21",
          "form": "신기하군",
          "original_form": "신기하군",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005917.1.22",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005917.1.23",
          "form": "예수님은 죽지 않은 채 하늘로 올라가셔서",
          "original_form": "예수님은 죽지 않은 채 하늘로 올라가셔서",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005917.1.24",
          "form": "성경도 하나의 역사서같은거라고 생각하는데",
          "original_form": "성경도 하나의 역사서같은거라고 생각하는데",
          "speaker_id": "2",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005917.1.25",
          "form": "종말의 날에 다시 내려와 심판을 한다고 믿는거지",
          "original_form": "종말의 날에 다시 내려와 심판을 한다고 믿는거지",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100005917.1.26",
          "form": "오오옹오오ㅗㅌ오",
          "original_form": "오오옹오오ㅗㅌ오",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005917.1.27",
          "form": "종말의날이",
          "original_form": "종말의날이",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005917.1.28",
          "form": "빨리다가왔으면좋겠다",
          "original_form": "빨리다가왔으면좋겠다",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005917.1.29",
          "form": "많은것들을볼수있을듯",
          "original_form": "많은것들을볼수있을듯",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100005917.1.30",
          "form": "고곳은 천국에 갈 수 있다는 전제하에...",
          "original_form": "고곳은 천국에 갈 수 있다는 전제하에...",
          "speaker_id": "1",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100005917.1.31",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100005917.1.32",
          "form": "불교는",
          "original_form": "불교는",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100005917.1.33",
          "form": "관심없어?!",
          "original_form": "관심없어?!",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100005917.1.34",
          "form": "난 나태지옥갈거같아",
          "original_form": "난 나태지옥갈거같아",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100005917.1.35",
          "form": "근데 무간도보니까",
          "original_form": "근데 무간도보니까",
          "speaker_id": "2",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100005917.1.36",
          "form": "무간지옥있데",
          "original_form": "무간지옥있데",
          "speaker_id": "2",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100005917.1.37",
          "form": "그리고 뭐였지 누가 묘사한 9옥도있었는데",
          "original_form": "그리고 뭐였지 누가 묘사한 9옥도있었는데",
          "speaker_id": "2",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100005917.1.38",
          "form": "불교 지옥은 잘 몰라...",
          "original_form": "불교 지옥은 잘 몰라...",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100005917.1.39",
          "form": "불교 교리도 너무 다양한 거 같더라고",
          "original_form": "불교 교리도 너무 다양한 거 같더라고",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100005917.1.40",
          "form": "초기 불교부터 동아시아에 전파된 불교까지 너무 다양해",
          "original_form": "초기 불교부터 동아시아에 전파된 불교까지 너무 다양해",
          "speaker_id": "1",
