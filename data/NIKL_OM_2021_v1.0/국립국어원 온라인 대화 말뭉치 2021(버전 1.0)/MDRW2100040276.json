      "utterance": [
        {
          "id": "MDRW2100040276.1.1",
          "form": "네 식욕이 솟아나요",
          "original_form": "네 식욕이 솟아나요",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040276.1.2",
          "form": "저도 진짜 딱 가을이면 식욕이 늘 솟는 편이라 ㅋㅋ",
          "original_form": "저도 진짜 딱 가을이면 식욕이 늘 솟는 편이라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040276.1.3",
          "form": "요즘 자주 드신 음식은 뭔가요?",
          "original_form": "요즘 자주 드신 음식은 뭔가요?",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040276.1.4",
          "form": "용가리...? ㅋㅋㅋㅋㅋ 마침 지금 먹었어요 ㅋㅋㅋ",
          "original_form": "용가리...? ㅋㅋㅋㅋㅋ 마침 지금 먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040276.1.5",
          "form": "용가리 치킨이요?ㅎㅎ",
          "original_form": "용가리 치킨이요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:33"
        },
        {
          "id": "MDRW2100040276.1.6",
          "form": "저는 방금 늙은호박전이라는 걸 부쳐 먹었어요",
          "original_form": "저는 방금 늙은호박전이라는 걸 부쳐 먹었어요",
          "speaker_id": "2",
          "time": "20210904 17:33"
        },
        {
          "id": "MDRW2100040276.1.7",
          "form": "아 뭔지 알아요 저도 어렸을때 먹었었는데 달달하니 넘 맛있죠",
          "original_form": "아 뭔지 알아요 저도 어렸을때 먹었었는데 달달하니 넘 맛있죠",
          "speaker_id": "1",
          "time": "20210904 20:02"
        },
        {
          "id": "MDRW2100040276.1.8",
          "form": "오 아시는구나",
          "original_form": "오 아시는구나",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100040276.1.9",
          "form": "근데 집에 온 늙은호박이 너무 커서 며칠동안 호박전을 먹어야할 것 같아요ㅜㅎㅎ",
          "original_form": "근데 집에 온 늙은호박이 너무 커서 며칠동안 호박전을 먹어야할 것 같아요ㅜㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040276.1.10",
          "form": "맞아요 늙은 호박의 단점은 너무 커서 양이 많다는거 ㅜㅜ ㅋㅋ",
          "original_form": "맞아요 늙은 호박의 단점은 너무 커서 양이 많다는거 ㅜㅜ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100040276.1.11",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100040276.1.12",
          "form": "선물로 받았는데",
          "original_form": "선물로 받았는데",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100040276.1.13",
          "form": "저걸 언제 다 먹지 싶은 생각이ㅠ",
          "original_form": "저걸 언제 다 먹지 싶은 생각이ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100040276.1.14",
          "form": "저희집도 늙은 호박 하나면 죽도 해먹고 전도 굽고 그랬던거같아요",
          "original_form": "저희집도 늙은 호박 하나면 죽도 해먹고 전도 굽고 그랬던거같아요",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100040276.1.15",
          "form": "네 맛있기는 한데",
          "original_form": "네 맛있기는 한데",
          "speaker_id": "2",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100040276.1.16",
          "form": "활용방법을 고민해봐야하는 식재료",
          "original_form": "활용방법을 고민해봐야하는 식재료",
          "speaker_id": "2",
          "time": "20210904 20:19"
        },
        {
          "id": "MDRW2100040276.1.17",
          "form": "조금 먹으면 맛있는데 해 치워야지 생각하면 ㅜㅜ",
          "original_form": "조금 먹으면 맛있는데 해 치워야지 생각하면 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100040276.1.18",
          "form": "네 근데 아직까진 맛있게 잘먹었어요ㅎㅎ",
          "original_form": "네 근데 아직까진 맛있게 잘먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100040276.1.19",
          "form": "내일도 모레도 먹을만큼 남았지만ㅠ",
          "original_form": "내일도 모레도 먹을만큼 남았지만ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:24"
        }
      ]
