      "utterance": [
        {
          "id": "MDRW2100034016.1.1",
          "form": "그래서 맛집메이트 찾기가 어려워요ㅜ",
          "original_form": "그래서 맛집메이트 찾기가 어려워요ㅜ",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100034016.1.2",
          "form": "회까지는 먹어도 해산물 별로 안 좋아하는 사람이 많고",
          "original_form": "회까지는 먹어도 해산물 별로 안 좋아하는 사람이 많고",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100034016.1.3",
          "form": "곱창은 드세요?",
          "original_form": "곱창은 드세요?",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100034016.1.4",
          "form": "곱창 먹죠ㅋㅋㅋㅋ",
          "original_form": "곱창 먹죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100034016.1.5",
          "form": "사실먹긴먹어요 곱창 막창 대창 곱막대",
          "original_form": "사실먹긴먹어요 곱창 막창 대창 곱막대",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100034016.1.6",
          "form": "소 돼지 어느쪽 선호하세요?",
          "original_form": "소 돼지 어느쪽 선호하세요?",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100034016.1.7",
          "form": "근데 ... 회랑초밥을 더 좋아할뿐이죠 빈도가ㅋㅋㅋ",
          "original_form": "근데 ... 회랑초밥을 더 좋아할뿐이죠 빈도가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100034016.1.8",
          "form": "곱창은",
          "original_form": "곱창은",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100034016.1.9",
          "form": "소곱창이죠!!!!!",
          "original_form": "소곱창이죠!!!!!",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100034016.1.10",
          "form": "생간 천엽만 안 드시는 거군요ㅎ",
          "original_form": "생간 천엽만 안 드시는 거군요ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100034016.1.11",
          "form": "맞아요! 돼지는 간혹 누린내가ㅜ",
          "original_form": "맞아요! 돼지는 간혹 누린내가ㅜ",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100034016.1.12",
          "form": "그렇게몸에 안좋대요",
          "original_form": "그렇게몸에 안좋대요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100034016.1.13",
          "form": "ㅎ  원래 몸에 안좋은 게 맛있지요",
          "original_form": "ㅎ  원래 몸에 안좋은 게 맛있지요",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100034016.1.14",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100034016.1.15",
          "form": "술은 잘드세요?",
          "original_form": "술은 잘드세요?",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100034016.1.16",
          "form": "소주 두병 주량이요",
          "original_form": "소주 두병 주량이요",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100034016.1.17",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100034016.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100034016.1.19",
          "form": "잘드시네요",
          "original_form": "잘드시네요",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100034016.1.20",
          "form": "저는 알쓰랍니다",
          "original_form": "저는 알쓰랍니다",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100034016.1.21",
          "form": "ㅎㅎ 근데 솔직히 술맛을 잘 모르겠어요",
          "original_form": "ㅎㅎ 근데 솔직히 술맛을 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100034016.1.22",
          "form": "술자리 분위기와 안주 때문에 먹는 거지",
          "original_form": "술자리 분위기와 안주 때문에 먹는 거지",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100034016.1.23",
          "form": "좋아하는 음식들이 다들 술과 궁합이 좋긴 하네요",
          "original_form": "좋아하는 음식들이 다들 술과 궁합이 좋긴 하네요",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100034016.1.24",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100034016.1.25",
          "form": "저는 하이볼제일좋아해요",
          "original_form": "저는 하이볼제일좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100034016.1.26",
          "form": "오오. 예전에 하이볼가든 자주 갔었어요",
          "original_form": "오오. 예전에 하이볼가든 자주 갔었어요",
