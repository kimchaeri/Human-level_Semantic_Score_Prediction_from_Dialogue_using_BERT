      "utterance": [
        {
          "id": "MDRW2100041205.1.1",
          "form": "저는 최근에 충전선 샀어요!",
          "original_form": "저는 최근에 충전선 샀어요!",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100041205.1.2",
          "form": "초고속 충전선!ㅋㅋㅋㅋ",
          "original_form": "초고속 충전선!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100041205.1.3",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100041205.1.4",
          "form": "정말초고속이었던가요?!",
          "original_form": "정말초고속이었던가요?!",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100041205.1.5",
          "form": "네 ㅋㅋㅋㅋ 초고속이면 파란 동그라미가 돌아가거든요",
          "original_form": "네 ㅋㅋㅋㅋ 초고속이면 파란 동그라미가 돌아가거든요",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100041205.1.6",
          "form": "파란동그라미 잘 나왔어요ㅋㅋㅋ",
          "original_form": "파란동그라미 잘 나왔어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100041205.1.7",
          "form": "오옹오",
          "original_form": "오옹오",
          "speaker_id": "1",
          "time": "20210906 02:16"
        },
        {
          "id": "MDRW2100041205.1.8",
          "form": "파란동그라밐ㅋㅋㅋㅋㅋ",
          "original_form": "파란동그라밐ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 02:16"
        },
        {
          "id": "MDRW2100041205.1.9",
          "form": "신기하네용",
          "original_form": "신기하네용",
          "speaker_id": "1",
          "time": "20210906 02:16"
        },
        {
          "id": "MDRW2100041205.1.10",
          "form": "저는 요즘 유선이어폰이 가지고 싶어요",
          "original_form": "저는 요즘 유선이어폰이 가지고 싶어요",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041205.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041205.1.12",
          "form": "저요즘 유선이어폰쓴답니다",
          "original_form": "저요즘 유선이어폰쓴답니다",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041205.1.13",
          "form": "에어팟도써보고 버즈도써봤는데",
          "original_form": "에어팟도써보고 버즈도써봤는데",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041205.1.14",
          "form": "충전너무귀찮아요",
          "original_form": "충전너무귀찮아요",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041205.1.15",
          "form": "킹받아요",
          "original_form": "킹받아요",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041205.1.16",
          "form": "헐!!! 뭐쓰세요?",
          "original_form": "헐!!! 뭐쓰세요?",
          "speaker_id": "2",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100041205.1.17",
          "form": "저도 에어팟시작으로 지금 버즈 사용중이긴한데",
          "original_form": "저도 에어팟시작으로 지금 버즈 사용중이긴한데",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100041205.1.18",
          "form": "유선이어폰 필요해요ㅠ",
          "original_form": "유선이어폰 필요해요ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100041205.1.19",
          "form": "저 유선이어폰쓰는중인뎈ㅋㅋㅋ",
          "original_form": "저 유선이어폰쓰는중인뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100041205.1.20",
          "form": "뭔가 확실히 거슬리긴하더라구요",
          "original_form": "뭔가 확실히 거슬리긴하더라구요",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100041205.1.21",
          "form": "근데충전너무귀찮구...",
          "original_form": "근데충전너무귀찮구...",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100041205.1.22",
          "form": "버즈는 그냥 버즈....1세대",
          "original_form": "버즈는 그냥 버즈....1세대",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100041205.1.23",
          "form": "쓰고있는거같네용",
          "original_form": "쓰고있는거같네용",
          "speaker_id": "1",
          "time": "20210906 19:25"
        },
        {
          "id": "MDRW2100041205.1.24",
          "form": "아 마스크만 없었어도 유선이어폰 그래도 좀 덜 귀찮을텐데 그쵸?",
          "original_form": "아 마스크만 없었어도 유선이어폰 그래도 좀 덜 귀찮을텐데 그쵸?",
          "speaker_id": "2",
          "time": "20210906 19:25"
        },
        {
          "id": "MDRW2100041205.1.25",
          "form": "아그쵸ㅠ",
          "original_form": "아그쵸ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100041205.1.26",
          "form": "그리고 진짜",
          "original_form": "그리고 진짜",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100041205.1.27",
          "form": "노트북쓸때도",
          "original_form": "노트북쓸때도",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100041205.1.28",
          "form": "유선이어폰쓰는데",
          "original_form": "유선이어폰쓰는데",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100041205.1.29",
          "form": "무선쓰다가 유선쓰니까",
          "original_form": "무선쓰다가 유선쓰니까",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100041205.1.30",
          "form": "딱",
          "original_form": "딱",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100041205.1.31",
          "form": "자리에서일어날때",
          "original_form": "자리에서일어날때",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100041205.1.32",
          "form": "항상 노트북도 따라오게되네욬큐ㅠ",
          "original_form": "항상 노트북도 따라오게되네욬큐ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100041205.1.33",
          "form": "아악",
          "original_form": "아악",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100041205.1.34",
          "form": "저 예전에 에어팟 없을때ㅋㅋㅋㅋ 카페에서 노트북 쓰다가 이어폰이 노트북 들었어요",
          "original_form": "저 예전에 에어팟 없을때ㅋㅋㅋㅋ 카페에서 노트북 쓰다가 이어폰이 노트북 들었어요",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100041205.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100041205.1.36",
          "form": "다행히 테이블위에 살짝 떠서 그렇지 옆이었으면 떨어졌을거예용",
          "original_form": "다행히 테이블위에 살짝 떠서 그렇지 옆이었으면 떨어졌을거예용",
          "speaker_id": "2",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100041205.1.37",
          "form": "아앜ㅋㅋㅋㅋㅋ",
          "original_form": "아앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:12"
        },
        {
          "id": "MDRW2100041205.1.38",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210907 14:12"
        },
        {
          "id": "MDRW2100041205.1.39",
          "form": "그럴때는 무선이어폰이 좋은편 ㅠㅠㅠ",
          "original_form": "그럴때는 무선이어폰이 좋은편 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:12"
        },
        {
          "id": "MDRW2100041205.1.40",
          "form": "맞죠맞죠 확실히 편리함은 무선이어폰이 짱인데",
          "original_form": "맞죠맞죠 확실히 편리함은 무선이어폰이 짱인데",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100041205.1.41",
          "form": "충전땜에 귀찮은게 너무 큰 단점이에요ㅜㅠ",
          "original_form": "충전땜에 귀찮은게 너무 큰 단점이에요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100041205.1.42",
          "form": "핸드폰도 배터리 없는데 ㅋㅋㅋ",
          "original_form": "핸드폰도 배터리 없는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100041205.1.43",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100041205.1.44",
          "form": "무선헤드폰은",
          "original_form": "무선헤드폰은",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100041205.1.45",
          "form": "써보셨나요",
          "original_form": "써보셨나요",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100041205.1.46",
          "form": "전 애플에서나온거 좀 구형?한번써봤는데",
          "original_form": "전 애플에서나온거 좀 구형?한번써봤는데",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100041205.1.47",
          "form": "애플은 왜 애플끼리만 연동되는지....",
          "original_form": "애플은 왜 애플끼리만 연동되는지....",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100041205.1.48",
          "form": "하지만 ㅇ무선이어폰보다는",
          "original_form": "하지만 ㅇ무선이어폰보다는",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100041205.1.49",
          "form": "오래가더라구요",
          "original_form": "오래가더라구요",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100041205.1.50",
          "form": "귀도안아프고",
          "original_form": "귀도안아프고",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100041205.1.51",
          "form": "저 갤럭시라서 눈길도 안주고 있어요..ㅠㅋㅋㅋ 얼마전에 갤럭시로 넘어오면서 기기 다 바꿨거든요 ㅋㅋㅋ 못돌아갑니다",
          "original_form": "저 갤럭시라서 눈길도 안주고 있어요..ㅠㅋㅋㅋ 얼마전에 갤럭시로 넘어오면서 기기 다 바꿨거든요 ㅋㅋㅋ 못돌아갑니다",
