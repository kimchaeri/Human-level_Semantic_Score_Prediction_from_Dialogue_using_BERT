      "utterance": [
        {
          "id": "MDRW2100019364.1.1",
          "form": "암튼 결혼은 상상으로 충분하다",
          "original_form": "암튼 결혼은 상상으로 충분하다",
          "speaker_id": "2",
          "time": "20210827 18:21"
        },
        {
          "id": "MDRW2100019364.1.2",
          "form": "만약에 하면",
          "original_form": "만약에 하면",
          "speaker_id": "1",
          "time": "20210827 18:21"
        },
        {
          "id": "MDRW2100019364.1.3",
          "form": "엄마처럼 살거같아섳무서움",
          "original_form": "엄마처럼 살거같아섳무서움",
          "speaker_id": "1",
          "time": "20210827 18:21"
        },
        {
          "id": "MDRW2100019364.1.4",
          "form": "이미 식 한번 올리고 이혼까지 했다 난",
          "original_form": "이미 식 한번 올리고 이혼까지 했다 난",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.5",
          "form": "엄마처럼?",
          "original_form": "엄마처럼?",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.6",
          "form": "딸.들.은 엄ㅁㅁ  마 인...생ㅇ",
          "original_form": "딸.들.은 엄ㅁㅁ  마 인...생ㅇ",
          "speaker_id": "1",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.7",
          "form": "닮는다잖아",
          "original_form": "닮는다잖아",
          "speaker_id": "1",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.8",
          "form": "난",
          "original_form": "난",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.9",
          "form": "너같은",
          "original_form": "너같은",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.10",
          "form": "딸",
          "original_form": "딸",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.11",
          "form": "있으면",
          "original_form": "있으면",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.12",
          "form": "너무 좋을듯",
          "original_form": "너무 좋을듯",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.14",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.15",
          "form": "착하지ㅠ공부 잘하지",
          "original_form": "착하지ㅠ공부 잘하지",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019364.1.16",
          "form": "어버버...",
          "original_form": "어버버...",
          "speaker_id": "1",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019364.1.17",
          "form": "너도 그 이야기 들음",
          "original_form": "너도 그 이야기 들음",
          "speaker_id": "2",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019364.1.18",
          "form": "보고 배운게 그거라서 결국 아빠같은 남자 만나게 된다",
          "original_form": "보고 배운게 그거라서 결국 아빠같은 남자 만나게 된다",
          "speaker_id": "2",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019364.1.19",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019364.1.20",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019364.1.21",
          "form": "그런 이야기 들었음",
          "original_form": "그런 이야기 들었음",
          "speaker_id": "2",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019364.1.22",
          "form": "너도가 아니고 나도임",
          "original_form": "너도가 아니고 나도임",
          "speaker_id": "2",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019364.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019364.1.24",
          "form": "맞아 결국 딸들은",
          "original_form": "맞아 결국 딸들은",
          "speaker_id": "1",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100019364.1.25",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019364.1.26",
          "form": "필터 때리고 싶다",
          "original_form": "필터 때리고 싶다",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019364.1.27",
          "form": "엄ㅁ마같은 인ㄴ생 살게된더고",
          "original_form": "엄ㅁ마같은 인ㄴ생 살게된더고",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019364.1.28",
          "form": "그말이 너무무서웟음",
          "original_form": "그말이 너무무서웟음",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019364.1.29",
          "form": "어우 다 그런건 아니겠지만",
          "original_form": "어우 다 그런건 아니겠지만",
          "speaker_id": "2",
          "time": "20210827 18:24"
        }
      ]
    }
  ]
