      "utterance": [
        {
          "id": "MDRW2100030257.1.1",
          "form": "다른밀키트 괜찮았던거 있으신가요!",
          "original_form": "다른밀키트 괜찮았던거 있으신가요!",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030257.1.2",
          "form": "뭐 감바스알아히요?",
          "original_form": "뭐 감바스알아히요?",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030257.1.3",
          "form": "이건 노말하죠 ㅋㅋ",
          "original_form": "이건 노말하죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030257.1.4",
          "form": "그거 밀키트로는 안사봤어요 ㅋㅋㅋㅋ 새우 신선하게 오나요.. ? ㅋㅋㅋㅋ의심이 많은타입 ㅋㅋㅋ",
          "original_form": "그거 밀키트로는 안사봤어요 ㅋㅋㅋㅋ 새우 신선하게 오나요.. ? ㅋㅋㅋㅋ의심이 많은타입 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030257.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ진공포장되서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진공포장되서",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030257.1.6",
          "form": "맛있음니다",
          "original_form": "맛있음니다",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030257.1.7",
          "form": "집밥은 맛있게 드셨나요 ㅋㅋ",
          "original_form": "집밥은 맛있게 드셨나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030257.1.8",
          "form": "ㅋㅋ돼지고기 그냥 뭐 먹었죠",
          "original_form": "ㅋㅋ돼지고기 그냥 뭐 먹었죠",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030257.1.9",
          "form": "역시 본가에 가면 일단 고기부터 ㅋㅋㅋㅋㅋ",
          "original_form": "역시 본가에 가면 일단 고기부터 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030257.1.10",
          "form": "ㅋㅋ고기 싸주려는데 바로 출근이라ㅠ",
          "original_form": "ㅋㅋ고기 싸주려는데 바로 출근이라ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030257.1.11",
          "form": "지금 출근하셨나요?!",
          "original_form": "지금 출근하셨나요?!",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030257.1.12",
          "form": "네 ㅋㅋ 나와서 샐러디 먹었슴다",
          "original_form": "네 ㅋㅋ 나와서 샐러디 먹었슴다",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030257.1.13",
          "form": "오 야근이신가보군요! 어떤메뉴드시나요",
          "original_form": "오 야근이신가보군요! 어떤메뉴드시나요",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030257.1.14",
          "form": "아 이제 조금 이따 자는데요;;",
          "original_form": "아 이제 조금 이따 자는데요;;",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030257.1.15",
          "form": "ㅋㅋㅋㅋㅋ알수업는 업무시간 ㅋㅋㅌ 샐러디는 맛있게드셨어요? ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ알수업는 업무시간 ㅋㅋㅌ 샐러디는 맛있게드셨어요? ㅋㅋㅋ",
