      "utterance": [
        {
          "id": "MDRW2100038006.1.1",
          "form": "소울푸드 있으세요?",
          "original_form": "소울푸드 있으세요?",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038006.1.2",
          "form": "저는 떡볶이요 name1님은요?",
          "original_form": "저는 떡볶이요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100038006.1.3",
          "form": "저는 피자 좋아해요",
          "original_form": "저는 피자 좋아해요",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100038006.1.4",
          "form": "지금 다이어트중인데 먹는거 이야기라도 실컷하고싶어요 ㅋㅋ",
          "original_form": "지금 다이어트중인데 먹는거 이야기라도 실컷하고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100038006.1.5",
          "form": "ㅋㅋㅋㅋㅋ어떤 브랜드 피자 좋아하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋ어떤 브랜드 피자 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038006.1.6",
          "form": "저는 피자라면 다좋아해요",
          "original_form": "저는 피자라면 다좋아해요",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100038006.1.7",
          "form": "떡볶이도 좋아해요",
          "original_form": "떡볶이도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100038006.1.8",
          "form": "로제 떡볶이는 사랑이죠ㅜ",
          "original_form": "로제 떡볶이는 사랑이죠ㅜ",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100038006.1.9",
          "form": "로제 저 짱 좋아해요\r\n어떤 로제떡볶이 가장 좋아하시나요?",
          "original_form": "로제 저 짱 좋아해요\r\n어떤 로제떡볶이 가장 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100038006.1.10",
          "form": "전 배민에서 진짜 한동안 이틀에한번씩 로제시켜먹었어요 ㅋㅋ",
          "original_form": "전 배민에서 진짜 한동안 이틀에한번씩 로제시켜먹었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100038006.1.11",
          "form": "음 전 떡참이랑 신참 ㅋㅋ 좋았어요",
          "original_form": "음 전 떡참이랑 신참 ㅋㅋ 좋았어요",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100038006.1.12",
          "form": "저희 주변에 신참있었는데 사라졌어요\r\n신참은 어떤 맛인가요?",
          "original_form": "저희 주변에 신참있었는데 사라졌어요\r\n신참은 어떤 맛인가요?",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038006.1.13",
          "form": "신참은 일단 저희동네는 5프로 포장할인을새줘서 ㅋㅋㅋ",
          "original_form": "신참은 일단 저희동네는 5프로 포장할인을새줘서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100038006.1.14",
          "form": "그리고 달고마 떡볶이 강추요",
          "original_form": "그리고 달고마 떡볶이 강추요",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038006.1.15",
          "form": "그렇군요 신참 달고마떡볶이는 언젠가 먹어보고 싶었는데 사라져버렸어요",
          "original_form": "그렇군요 신참 달고마떡볶이는 언젠가 먹어보고 싶었는데 사라져버렸어요",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038006.1.16",
          "form": "일반 떡볶이와 어떻게 다른가요?",
          "original_form": "일반 떡볶이와 어떻게 다른가요?",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100038006.1.17",
          "form": "일반떡볶이 위에 치즈 고구마 무스 조합이요 ㅋㅋ",
          "original_form": "일반떡볶이 위에 치즈 고구마 무스 조합이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038006.1.18",
          "form": "맛이 없을수가없어요 ㅋㅋㅋ",
          "original_form": "맛이 없을수가없어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038006.1.19",
          "form": "5000원인가? 그정도 돈 내고 먹을만 한가요?",
          "original_form": "5000원인가? 그정도 돈 내고 먹을만 한가요?",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100038006.1.20",
          "form": "네네 그정도 가치는 충분히있어요",
          "original_form": "네네 그정도 가치는 충분히있어요",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100038006.1.21",
          "form": "6천원인가 9천원인가 그랬던듯요",
          "original_form": "6천원인가 9천원인가 그랬던듯요",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100038006.1.22",
          "form": "신참 찾으면 꼭 먹어봐야겠네요",
          "original_form": "신참 찾으면 꼭 먹어봐야겠네요",
