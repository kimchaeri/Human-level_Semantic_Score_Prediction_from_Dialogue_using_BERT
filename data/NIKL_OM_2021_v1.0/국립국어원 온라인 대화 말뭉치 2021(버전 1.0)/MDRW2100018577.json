      "utterance": [
        {
          "id": "MDRW2100018577.1.1",
          "form": "회사가는중이에요 가기싫어요",
          "original_form": "회사가는중이에요 가기싫어요",
          "speaker_id": "1",
          "time": "20210827 08:38"
        },
        {
          "id": "MDRW2100018577.1.2",
          "form": "저는 이미 회사인데",
          "original_form": "저는 이미 회사인데",
          "speaker_id": "2",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100018577.1.3",
          "form": "집 가고 싶어요",
          "original_form": "집 가고 싶어요",
          "speaker_id": "2",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100018577.1.4",
          "form": "ㅋㅋㅋ출근 화이팅",
          "original_form": "ㅋㅋㅋ출근 화이팅",
          "speaker_id": "2",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100018577.1.5",
          "form": "저도요 퇴근하고싶어요",
          "original_form": "저도요 퇴근하고싶어요",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018577.1.6",
          "form": "점심 맛있는거 나오나요",
          "original_form": "점심 맛있는거 나오나요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018577.1.7",
          "form": "점심 오징어숙회나왔어요",
          "original_form": "점심 오징어숙회나왔어요",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018577.1.8",
          "form": "점심 뭐에요",
          "original_form": "점심 뭐에요",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018577.1.9",
          "form": "점심 반각오븐구이라서 뭔가 했는데",
          "original_form": "점심 반각오븐구이라서 뭔가 했는데",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018577.1.10",
          "form": "옛날통닭 반마리였어요",
          "original_form": "옛날통닭 반마리였어요",
          "speaker_id": "2",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018577.1.11",
          "form": "반각오븐구잌ㅋㅋㅋㅋㅋ",
          "original_form": "반각오븐구잌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018577.1.12",
          "form": "넘나 배부른 것",
          "original_form": "넘나 배부른 것",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018577.1.13",
          "form": "치킨데이 커피는요",
          "original_form": "치킨데이 커피는요",
          "speaker_id": "1",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018577.1.14",
          "form": "커피는 아침에 돌체라떼 테이크아웃",
          "original_form": "커피는 아침에 돌체라떼 테이크아웃",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018577.1.15",
          "form": "근데 직업 얘기해놓고 먹는 얘기 더 하게 되네여",
          "original_form": "근데 직업 얘기해놓고 먹는 얘기 더 하게 되네여",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018577.1.16",
          "form": "그러네요 오늘 일는 어떠세요",
          "original_form": "그러네요 오늘 일는 어떠세요",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100018577.1.17",
          "form": "저는 업무스트레스가 덜한 날이에요",
          "original_form": "저는 업무스트레스가 덜한 날이에요",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100018577.1.18",
          "form": "저두 오늘은 수월하게 하루가 흘러가고 있네요",
          "original_form": "저두 오늘은 수월하게 하루가 흘러가고 있네요",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018577.1.19",
          "form": "업무 스트레스가 덜 하다니 정말 다행이에요",
          "original_form": "업무 스트레스가 덜 하다니 정말 다행이에요",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018577.1.20",
          "form": "왜녀면 부장님이 없고",
          "original_form": "왜녀면 부장님이 없고",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018577.1.21",
          "form": "그나쁜언니가 일이 없기때문이에요",
          "original_form": "그나쁜언니가 일이 없기때문이에요",
          "speaker_id": "1",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100018577.1.22",
          "form": "직장사람에의한 스트레스는 어쩔슈없어서 내가 잘 이겨내야하는데 방법이있을까요",
          "original_form": "직장사람에의한 스트레스는 어쩔슈없어서 내가 잘 이겨내야하는데 방법이있을까요",
          "speaker_id": "1",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100018577.1.23",
          "form": "그냥 업무를 업무로만 보고 사람을 끌어오지 않아야한달까",
          "original_form": "그냥 업무를 업무로만 보고 사람을 끌어오지 않아야한달까",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100018577.1.24",
          "form": "맞아요 그냥 거렁뱅이가 업무를 주는구나",
          "original_form": "맞아요 그냥 거렁뱅이가 업무를 주는구나",
          "speaker_id": "1",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100018577.1.25",
          "form": "왜 저럴까 왜이리 배려가없지 이런 생각 노노",
          "original_form": "왜 저럴까 왜이리 배려가없지 이런 생각 노노",
          "speaker_id": "1",
          "time": "20210827 15:08"
        }
      ]
    }
