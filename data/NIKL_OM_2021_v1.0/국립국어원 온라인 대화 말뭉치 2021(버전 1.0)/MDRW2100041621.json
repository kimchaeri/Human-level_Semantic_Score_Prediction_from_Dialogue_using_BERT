      "utterance": [
        {
          "id": "MDRW2100041621.1.1",
          "form": "점심 드셧어요,",
          "original_form": "점심 드셧어요,",
          "speaker_id": "1",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100041621.1.2",
          "form": "네~ 점심 어떤거 드셨어요?",
          "original_form": "네~ 점심 어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041621.1.3",
          "form": "저는 참치 김밥 먹었어요!",
          "original_form": "저는 참치 김밥 먹었어요!",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041621.1.4",
          "form": "어떤거 드셧어요?",
          "original_form": "어떤거 드셧어요?",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041621.1.5",
          "form": "저는 점심에 두부부침이랑 달걀후라이먹어ㅛ어요!",
          "original_form": "저는 점심에 두부부침이랑 달걀후라이먹어ㅛ어요!",
          "speaker_id": "2",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100041621.1.6",
          "form": "이제 저녁시간이 다가오네요!",
          "original_form": "이제 저녁시간이 다가오네요!",
          "speaker_id": "2",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100041621.1.7",
          "form": "저녁 계획 있으새요?",
          "original_form": "저녁 계획 있으새요?",
          "speaker_id": "2",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100041621.1.8",
          "form": "네네 저는 간단하게 볶음밥 해서 먹을려구용",
          "original_form": "네네 저는 간단하게 볶음밥 해서 먹을려구용",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041621.1.9",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041621.1.10",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100041621.1.11",
          "form": "라면정도만.. 끌일줄 압니다..",
          "original_form": "라면정도만.. 끌일줄 압니다..",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100041621.1.12",
          "form": "주로 배달해먹는거같아요 ㅠㅜ",
          "original_form": "주로 배달해먹는거같아요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100041621.1.13",
          "form": "와 저두요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 저두요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100041621.1.14",
          "form": "레시피 대로 해도 맛이 안나오드라구요 ㅠㅠ",
          "original_form": "레시피 대로 해도 맛이 안나오드라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100041621.1.15",
          "form": "배달 어떤거 주로 드세요",
          "original_form": "배달 어떤거 주로 드세요",
          "speaker_id": "1",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100041621.1.16",
          "form": "저는 치킨! 이나 피자 족발!",
          "original_form": "저는 치킨! 이나 피자 족발!",
          "speaker_id": "2",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100041621.1.17",
          "form": "아니면 아침 점심에는 그냥 집밥도 많이 시켜먹어요!",
          "original_form": "아니면 아침 점심에는 그냥 집밥도 많이 시켜먹어요!",
          "speaker_id": "2",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100041621.1.18",
          "form": "오오 피자 너무 맛있죠 ㅠㅠ 가격이 비싸서 ..",
          "original_form": "오오 피자 너무 맛있죠 ㅠㅠ 가격이 비싸서 ..",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041621.1.19",
          "form": "피자 어떤 메뉴 좋아하세요??",
          "original_form": "피자 어떤 메뉴 좋아하세요??",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041621.1.20",
          "form": "저는 포테이토요!!",
          "original_form": "저는 포테이토요!!",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100041621.1.21",
          "form": "어떤거 제일 좋아하세요?",
          "original_form": "어떤거 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100041621.1.22",
          "form": "오 저도요!!! 무난하고 젤 맛있어요 ㅋㅋㅋ",
          "original_form": "오 저도요!!! 무난하고 젤 맛있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100041621.1.23",
          "form": "치즈크러스트 꼭 해여 저는 ㅋㅋ",
          "original_form": "치즈크러스트 꼭 해여 저는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100041621.1.24",
          "form": "헐저는",
          "original_form": "헐저는",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100041621.1.25",
