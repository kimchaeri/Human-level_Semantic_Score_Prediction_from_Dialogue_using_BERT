      "utterance": [
        {
          "id": "MDRW2100025659.1.1",
          "form": "name1은 종교 있으신가요?",
          "original_form": "&name1&은 종교 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025659.1.2",
          "form": "무교에요!",
          "original_form": "무교에요!",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100025659.1.3",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100025659.1.4",
          "form": "저도 무교입니다",
          "original_form": "저도 무교입니다",
          "speaker_id": "2",
          "time": "20210901 16:45"
        },
        {
          "id": "MDRW2100025659.1.5",
          "form": "저는 믿음이 안생기더라구요 ㅎㄹ",
          "original_form": "저는 믿음이 안생기더라구요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025659.1.6",
          "form": "다들 그럴 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "다들 그럴 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025659.1.7",
          "form": "아니에요 또 기독교신자보면 믿음이 대단합니다",
          "original_form": "아니에요 또 기독교신자보면 믿음이 대단합니다",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025659.1.8",
          "form": "기독교는 전도도 엄청 열심히잖아요 ㅎㅎ",
          "original_form": "기독교는 전도도 엄청 열심히잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025659.1.9",
          "form": "요즘 기독교는 기독교가 아닌 것 같아요 ㅎㅎ",
          "original_form": "요즘 기독교는 기독교가 아닌 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025659.1.10",
          "form": "다들 눈빛이 너무 무섭습니다",
          "original_form": "다들 눈빛이 너무 무섭습니다",
          "speaker_id": "1",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025659.1.11",
          "form": "공감해요",
          "original_form": "공감해요",
          "speaker_id": "2",
          "time": "20210902 07:50"
        },
        {
          "id": "MDRW2100025659.1.12",
          "form": "종교 자체도 한국에 넘어오면서 약간은 변질 된듯한 느낌이 들어요",
          "original_form": "종교 자체도 한국에 넘어오면서 약간은 변질 된듯한 느낌이 들어요",
          "speaker_id": "2",
          "time": "20210902 07:51"
        },
        {
          "id": "MDRW2100025659.1.13",
          "form": "그럴까요 외국에도 사이비화 된 곳이 많아서 그런게 넘어왔나 싶었어요 저는",
          "original_form": "그럴까요 외국에도 사이비화 된 곳이 많아서 그런게 넘어왔나 싶었어요 저는",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100025659.1.14",
          "form": "아 그렇군요!",
          "original_form": "아 그렇군요!",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100025659.1.15",
          "form": "기독교만 놓고 봤을때 미국이나 이런데선 전도 이런걸 별로 안하는거같아서 저는 한국입맛에 맞게 조금씩 변형됐다고 느꼈거든요",
          "original_form": "기독교만 놓고 봤을때 미국이나 이런데선 전도 이런걸 별로 안하는거같아서 저는 한국입맛에 맞게 조금씩 변형됐다고 느꼈거든요",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100025659.1.16",
          "form": "외국에도 사이비화된 곳이 많군요!!!",
          "original_form": "외국에도 사이비화된 곳이 많군요!!!",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100025659.1.17",
          "form": "뭐든지 지나치면 안좋은거같아요",
          "original_form": "뭐든지 지나치면 안좋은거같아요",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100025659.1.18",
          "form": "맞아요 예수님 입장에서도 쟤들은 왜 저러지 싶으실 것 같아요 ㅎㅎ",
          "original_form": "맞아요 예수님 입장에서도 쟤들은 왜 저러지 싶으실 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100025659.1.19",
          "form": "맞아요 ㅎㅎㅎ같은 종파여도 싸우고 다투고..ㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎ같은 종파여도 싸우고 다투고..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100025659.1.20",
          "form": "그럴때보면 저는 무교인게 마음이 안정될 때가 있더라구요",
          "original_form": "그럴때보면 저는 무교인게 마음이 안정될 때가 있더라구요",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100025659.1.21",
          "form": "맞아요 ㅋㅋㅋㅋ 제가 종교인이었다면 뉴스에서 소식 접할때마다 화나고 심란했을 것 같아요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 제가 종교인이었다면 뉴스에서 소식 접할때마다 화나고 심란했을 것 같아요",
          "speaker_id": "1",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100025659.1.22",
          "form": "진짜 그럴거같아요!",
          "original_form": "진짜 그럴거같아요!",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100025659.1.23",
          "form": "저는 어떤 종교든 존중하는 마음은 가지고잇는데 남에게 강요만 하지 않는 다면 좋은거같아요 ㅎ",
          "original_form": "저는 어떤 종교든 존중하는 마음은 가지고잇는데 남에게 강요만 하지 않는 다면 좋은거같아요 ㅎ",
          "speaker_id": "2",
