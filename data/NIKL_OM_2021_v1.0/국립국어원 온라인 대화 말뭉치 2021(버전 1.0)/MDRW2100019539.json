      "utterance": [
        {
          "id": "MDRW2100019539.1.1",
          "form": "돈까스 좋아하시나요?",
          "original_form": "돈까스 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019539.1.2",
          "form": "돈까스 좋아하죠~",
          "original_form": "돈까스 좋아하죠~",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019539.1.3",
          "form": "옛날돈까스로 된 것도 좋고 치즈돈까스도 좋아요 ㅎㅎ",
          "original_form": "옛날돈까스로 된 것도 좋고 치즈돈까스도 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019539.1.4",
          "form": "저 오늘 저녁이 치즈돈까스였는데 맛있어서 기분좋았어요 ㅎㅎㅎ",
          "original_form": "저 오늘 저녁이 치즈돈까스였는데 맛있어서 기분좋았어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019539.1.5",
          "form": "치즈돈까스 시켰는데 돈까스 위에 치즈 한 장 놓여져있으면 어떨 거 같아요?ㅋㅋㅋ",
          "original_form": "치즈돈까스 시켰는데 돈까스 위에 치즈 한 장 놓여져있으면 어떨 거 같아요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019539.1.6",
          "form": "헐 그건 거의 마음속 고소단겨인데요",
          "original_form": "헐 그건 거의 마음속 고소단겨인데요",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019539.1.7",
          "form": "단계",
          "original_form": "단계",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019539.1.8",
          "form": "ㅋㅋㅋㅋ저 예전에 분식집에 그렇게 당해본 적 있어요",
          "original_form": "ㅋㅋㅋㅋ저 예전에 분식집에 그렇게 당해본 적 있어요",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019539.1.9",
          "form": "어떻게 슬라이스 치즈 한 장만 올려주냐구요 ㅜㅜ",
          "original_form": "어떻게 슬라이스 치즈 한 장만 올려주냐구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019539.1.10",
          "form": "자매로 치즈라볶이가 있습니다..",
          "original_form": "자매로 치즈라볶이가 있습니다..",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019539.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ치즈라볶이는 저도 그런적많아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ치즈라볶이는 저도 그런적많아요",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019539.1.12",
          "form": "슬라이스치즈 1개차이에 1000원차이가 나다니",
          "original_form": "슬라이스치즈 1개차이에 1000원차이가 나다니",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019539.1.13",
          "form": "ㅋㅋㅋ 아진짜 그래서 몇번 당하고 그 뒤로 치즈 안봐요 ㅜㅜ",
          "original_form": "ㅋㅋㅋ 아진짜 그래서 몇번 당하고 그 뒤로 치즈 안봐요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019539.1.14",
          "form": "모짜렐라 치즈를 뿌려줘야되는 거 아니에요?ㅠㅠ",
          "original_form": "모짜렐라 치즈를 뿌려줘야되는 거 아니에요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019539.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ돈까스는 진짜 심했네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ돈까스는 진짜 심했네요",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019539.1.16",
          "form": "돈까스.. 아직도 충격적이에요.",
          "original_form": "돈까스.. 아직도 충격적이에요.",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019539.1.17",
          "form": "요새 그렇게 장사하는 가게는 없겠죠?",
          "original_form": "요새 그렇게 장사하는 가게는 없겠죠?",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019539.1.18",
          "form": "ㅋㅋㅋㅋㅋ혹시 보게된다면 알려드릴게요",
          "original_form": "ㅋㅋㅋㅋㅋ혹시 보게된다면 알려드릴게요",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019539.1.19",
          "form": "옛날돈까스도 맛있죠",
          "original_form": "옛날돈까스도 맛있죠",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019539.1.20",
          "form": "남산 돈까스 먹어보셨어요?",
          "original_form": "남산 돈까스 먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019539.1.21",
          "form": "로망이긴해요! 맛있나요?",
          "original_form": "로망이긴해요! 맛있나요?",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019539.1.22",
          "form": "저 아주 어렸을 때 자주 먹었던 기억이 나는데",
          "original_form": "저 아주 어렸을 때 자주 먹었던 기억이 나는데",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019539.1.23",
          "form": "무한도전 출연이후 가격만 비싸지고 맛은 없어졌다고 들었어요..",
          "original_form": "무한도전 출연이후 가격만 비싸지고 맛은 없어졌다고 들었어요..",
          "speaker_id": "2",
          "time": "20210827 20:23"
        }
      ]
