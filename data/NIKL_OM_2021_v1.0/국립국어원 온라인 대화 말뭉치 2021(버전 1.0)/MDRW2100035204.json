      "utterance": [
        {
          "id": "MDRW2100035204.1.1",
          "form": "식사하셨나여?",
          "original_form": "식사하셨나여?",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035204.1.2",
          "form": "네 어죽 먹었어요",
          "original_form": "네 어죽 먹었어요",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100035204.1.3",
          "form": "name3님은요?",
          "original_form": "&name3&님은요?",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100035204.1.4",
          "form": "앗 name2님은요?",
          "original_form": "앗 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100035204.1.5",
          "form": "저도 먹었어요 미역국ㅎㅎ",
          "original_form": "저도 먹었어요 미역국ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100035204.1.6",
          "form": "가족 누가 생일인가요?",
          "original_form": "가족 누가 생일인가요?",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100035204.1.7",
          "form": "그냥 끓였어용. 국으로",
          "original_form": "그냥 끓였어용. 국으로",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100035204.1.8",
          "form": "아ㅎ 직접 끓이셨어요?",
          "original_form": "아ㅎ 직접 끓이셨어요?",
          "speaker_id": "1",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100035204.1.9",
          "form": "네~ㅎㅎ. 맛있게됬더라구요",
          "original_form": "네~ㅎㅎ. 맛있게됬더라구요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100035204.1.10",
          "form": "와 요리솜씨가 좋으신가봐요!!",
          "original_form": "와 요리솜씨가 좋으신가봐요!!",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100035204.1.11",
          "form": "그런건 아닌데 ㅎㅎㅎ 그냥 주부 11년차니 미역국 정도는.. ㅋㅋ 잘되네요 ㅎㅎ",
          "original_form": "그런건 아닌데 ㅎㅎㅎ 그냥 주부 11년차니 미역국 정도는.. ㅋㅋ 잘되네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100035204.1.12",
          "form": "주부 만렙이시네요ㄷㄷ",
          "original_form": "주부 만렙이시네요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100035204.1.13",
          "form": "소고기 넣으셨나요?",
          "original_form": "소고기 넣으셨나요?",
          "speaker_id": "1",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100035204.1.14",
          "form": "네 ㅎㅎ 소고기로 ㅋㅋ 근데 또 하는것만 해서 ㅎ 다른건 잘 안되요",
          "original_form": "네 ㅎㅎ 소고기로 ㅋㅋ 근데 또 하는것만 해서 ㅎ 다른건 잘 안되요",
          "speaker_id": "2",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100035204.1.15",
          "form": "다른 시도도 해보셨나 봐요",
          "original_form": "다른 시도도 해보셨나 봐요",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035204.1.16",
          "form": "전 미역국전문점서 갈치미역국? 을 먹어봤는데 여러모로 신기했어요ㅎ",
          "original_form": "전 미역국전문점서 갈치미역국? 을 먹어봤는데 여러모로 신기했어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035204.1.17",
          "form": "첨들어봐요ㅋㅋ 미역국은 바지락. 북어 요렇게만 해봤어요",
          "original_form": "첨들어봐요ㅋㅋ 미역국은 바지락. 북어 요렇게만 해봤어요",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100035204.1.18",
          "form": "북어를 넣고도 가능하군요!",
          "original_form": "북어를 넣고도 가능하군요!",
          "speaker_id": "1",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100035204.1.19",
          "form": "북엇국에 들어가는 그 북어가 미역국에 들어가는 거죠?",
          "original_form": "북엇국에 들어가는 그 북어가 미역국에 들어가는 거죠?",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100035204.1.20",
          "form": "나머지는 같고",
          "original_form": "나머지는 같고",
          "speaker_id": "1",
          "time": "20210829 23:19"
        }
      ]
