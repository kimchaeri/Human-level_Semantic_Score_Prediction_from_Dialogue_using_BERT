      "utterance": [
        {
          "id": "MDRW2100046492.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20211021 15:24"
        },
        {
          "id": "MDRW2100046492.1.2",
          "form": "안녕하세요~!!",
          "original_form": "안녕하세요~!!",
          "speaker_id": "2",
          "time": "20211021 15:35"
        },
        {
          "id": "MDRW2100046492.1.3",
          "form": "날이 춥네요 ㅠㅠ",
          "original_form": "날이 춥네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211021 15:35"
        },
        {
          "id": "MDRW2100046492.1.4",
          "form": "ㅜㅜ 그러게용 ㅜㅜ",
          "original_form": "ㅜㅜ 그러게용 ㅜㅜ",
          "speaker_id": "1",
          "time": "20211021 15:55"
        },
        {
          "id": "MDRW2100046492.1.5",
          "form": "벌써 겨울이네요ㅜㅜ",
          "original_form": "벌써 겨울이네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20211021 15:55"
        },
        {
          "id": "MDRW2100046492.1.6",
          "form": "그러니까요 ㅠㅠ 가을을 즐기고 싶었는데 아쉬워요 ㅠㅠ",
          "original_form": "그러니까요 ㅠㅠ 가을을 즐기고 싶었는데 아쉬워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211021 16:05"
        },
        {
          "id": "MDRW2100046492.1.7",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20211021 16:05"
        },
        {
          "id": "MDRW2100046492.1.8",
          "form": "전 봄이 제일 좋아요!",
          "original_form": "전 봄이 제일 좋아요!",
          "speaker_id": "1",
          "time": "20211021 22:04"
        },
        {
          "id": "MDRW2100046492.1.9",
          "form": "벚꽃이 너무 예쁘고 좋아서요",
          "original_form": "벚꽃이 너무 예쁘고 좋아서요",
          "speaker_id": "1",
          "time": "20211021 22:05"
        },
        {
          "id": "MDRW2100046492.1.10",
          "form": "앗 맞아요 ㅎㅎㅎ",
          "original_form": "앗 맞아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211021 22:25"
        },
        {
          "id": "MDRW2100046492.1.11",
          "form": "전 선선한 가을을 좋아해요 ㅎㅎ",
          "original_form": "전 선선한 가을을 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211021 22:26"
        },
        {
          "id": "MDRW2100046492.1.12",
          "form": "작년 벚꽃 구경 가셨어요?",
          "original_form": "작년 벚꽃 구경 가셨어요?",
          "speaker_id": "2",
          "time": "20211021 22:26"
        },
        {
          "id": "MDRW2100046492.1.13",
          "form": "아뇽ㅜㅠ 못갔어요 코로나 때문에 무서워서요",
          "original_form": "아뇽ㅜㅠ 못갔어요 코로나 때문에 무서워서요",
          "speaker_id": "1",
          "time": "20211021 22:35"
        },
        {
          "id": "MDRW2100046492.1.14",
          "form": "그냥 집앞에 벚꽃나무 몇그루 있는걸로 만족했어요",
          "original_form": "그냥 집앞에 벚꽃나무 몇그루 있는걸로 만족했어요",
          "speaker_id": "1",
          "time": "20211021 22:36"
        },
        {
          "id": "MDRW2100046492.1.15",
          "form": "아 저는 드라이브스루로 봤는데 그것도 좋더라구요 ㅎㅎ",
          "original_form": "아 저는 드라이브스루로 봤는데 그것도 좋더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211021 22:46"
        },
        {
          "id": "MDRW2100046492.1.16",
          "form": "백신은 맞으셨어요?",
          "original_form": "백신은 맞으셨어요?",
          "speaker_id": "2",
          "time": "20211021 22:46"
        },
        {
          "id": "MDRW2100046492.1.17",
          "form": "넹 저번주에 2차까지 다 맞았어요",
          "original_form": "넹 저번주에 2차까지 다 맞았어요",
          "speaker_id": "1",
          "time": "20211022 08:33"
        },
        {
          "id": "MDRW2100046492.1.18",
          "form": "백신 맞으셨어용?",
          "original_form": "백신 맞으셨어용?",
          "speaker_id": "1",
          "time": "20211022 08:34"
        },
        {
          "id": "MDRW2100046492.1.19",
          "form": "네 ! 2차에 엄청 아파서 힘들었어요 ㅠㅠ",
          "original_form": "네 ! 2차에 엄청 아파서 힘들었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211022 09:05"
        },
        {
          "id": "MDRW2100046492.1.20",
          "form": "괜찮으셨어요?",
          "original_form": "괜찮으셨어요?",
          "speaker_id": "2",
          "time": "20211022 09:05"
        },
        {
          "id": "MDRW2100046492.1.21",
          "form": "아뇨 저도 아팠어요",
          "original_form": "아뇨 저도 아팠어요",
          "speaker_id": "1",
          "time": "20211022 09:06"
        },
        {
          "id": "MDRW2100046492.1.22",
          "form": "저는 모더나 맞았는데 2차가 원래 아프다더라구요",
          "original_form": "저는 모더나 맞았는데 2차가 원래 아프다더라구요",
          "speaker_id": "1",
          "time": "20211022 09:06"
        },
        {
          "id": "MDRW2100046492.1.23",
          "form": "혹시 뭐 맞으셨어요?",
          "original_form": "혹시 뭐 맞으셨어요?",
          "speaker_id": "1",
          "time": "20211022 09:06"
        },
        {
          "id": "MDRW2100046492.1.24",
          "form": "저도 화이자 맞았는데 아팠어요!",
          "original_form": "저도 화이자 맞았는데 아팠어요!",
          "speaker_id": "2",
          "time": "20211022 09:06"
        },
        {
          "id": "MDRW2100046492.1.25",
          "form": "얼른 코로나가 사라졌음 좋겠네요 ㅠㅠ",
          "original_form": "얼른 코로나가 사라졌음 좋겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211022 09:07"
        }
      ]
    }
