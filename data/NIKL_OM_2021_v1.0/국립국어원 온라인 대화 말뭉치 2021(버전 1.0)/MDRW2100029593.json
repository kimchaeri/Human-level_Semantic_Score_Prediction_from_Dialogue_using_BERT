      "utterance": [
        {
          "id": "MDRW2100029593.1.1",
          "form": "ㅆㅏ게 살수도있죠",
          "original_form": "ㅆㅏ게 살수도있죠",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029593.1.2",
          "form": "편법인데 돈차이가 좀나요",
          "original_form": "편법인데 돈차이가 좀나요",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029593.1.3",
          "form": "세일도 한번씩 하구요",
          "original_form": "세일도 한번씩 하구요",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029593.1.4",
          "form": "음 게임전문가같아요",
          "original_form": "음 게임전문가같아요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029593.1.5",
          "form": "사람들이랑 함께하는 게임",
          "original_form": "사람들이랑 함께하는 게임",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029593.1.6",
          "form": "해보셨나요",
          "original_form": "해보셨나요",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029593.1.7",
          "form": "요즘 인기게임이 뭐죠",
          "original_form": "요즘 인기게임이 뭐죠",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029593.1.8",
          "form": "전 주키퍼 로 대전하거나",
          "original_form": "전 주키퍼 로 대전하거나",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029593.1.9",
          "form": "넷마블해봤나요",
          "original_form": "넷마블해봤나요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029593.1.10",
          "form": "넷마블은 안해봤어요",
          "original_form": "넷마블은 안해봤어요",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029593.1.11",
          "form": "넷마블로 사천성  틀린그림 했어요",
          "original_form": "넷마블로 사천성  틀린그림 했어요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029593.1.12",
          "form": "사천성도 재밌어요",
          "original_form": "사천성도 재밌어요",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029593.1.13",
          "form": "고스톱도 쳤죠",
          "original_form": "고스톱도 쳤죠",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029593.1.14",
          "form": "틀린그림해봤어요",
          "original_form": "틀린그림해봤어요",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029593.1.15",
          "form": "기억나네요",
          "original_form": "기억나네요",
          "speaker_id": "2",
