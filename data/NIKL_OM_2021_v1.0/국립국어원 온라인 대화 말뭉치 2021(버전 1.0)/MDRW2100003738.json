      "utterance": [
        {
          "id": "MDRW2100003738.1.1",
          "form": "연애 중인 분이랑은 잘 맞으신가요?",
          "original_form": "연애 중인 분이랑은 잘 맞으신가요?",
          "speaker_id": "1",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003738.1.2",
          "form": "연애 몇년하고 결혼하셨어요?",
          "original_form": "연애 몇년하고 결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003738.1.3",
          "form": "아뇨 진짜 안맞아요ㅋㅋㅋㅋ",
          "original_form": "아뇨 진짜 안맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003738.1.4",
          "form": "2년차인데 진짜 안맞아서 더 결혼생각 없어졌어요",
          "original_form": "2년차인데 진짜 안맞아서 더 결혼생각 없어졌어요",
          "speaker_id": "2",
          "time": "20210821 11:33"
        },
        {
          "id": "MDRW2100003738.1.5",
          "form": "연애만하려구요",
          "original_form": "연애만하려구요",
          "speaker_id": "2",
          "time": "20210821 11:33"
        },
        {
          "id": "MDRW2100003738.1.6",
          "form": "헙 그러시구나!!!",
          "original_form": "헙 그러시구나!!!",
          "speaker_id": "1",
          "time": "20210821 11:34"
        },
        {
          "id": "MDRW2100003738.1.7",
          "form": "일단 나이차도 많아요ㅎㅎ",
          "original_form": "일단 나이차도 많아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003738.1.8",
          "form": "잘맞는데 진짜 안맞는편이에요 약간 모순적이죠",
          "original_form": "잘맞는데 진짜 안맞는편이에요 약간 모순적이죠",
          "speaker_id": "2",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003738.1.9",
          "form": "ㅎㅎ 그러네요",
          "original_form": "ㅎㅎ 그러네요",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003738.1.10",
          "form": "저는 일단 관심사랑 취미가 같고 말이 통하는 사람이 좋을것 같아요",
          "original_form": "저는 일단 관심사랑 취미가 같고 말이 통하는 사람이 좋을것 같아요",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003738.1.11",
          "form": "아 관심사 취미 둘 다 안맞아서 제가 맞추고있어요ㅋㅋㅋ",
          "original_form": "아 관심사 취미 둘 다 안맞아서 제가 맞추고있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003738.1.12",
          "form": "저희남편은 저랑 정반대라 결혼생활이 힘드네요",
          "original_form": "저희남편은 저랑 정반대라 결혼생활이 힘드네요",
          "speaker_id": "1",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003738.1.13",
          "form": "아이고ㅜㅜ",
          "original_form": "아이고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003738.1.14",
          "form": "말은 통하세요?",
          "original_form": "말은 통하세요?",
          "speaker_id": "2",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003738.1.15",
          "form": "저는 말은 통하거든요",
          "original_form": "저는 말은 통하거든요",
          "speaker_id": "2",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003738.1.16",
          "form": "말도 잘 안통해요",
          "original_form": "말도 잘 안통해요",
          "speaker_id": "1",
          "time": "20210821 11:38"
        },
        {
          "id": "MDRW2100003738.1.17",
          "form": "그래서 결혼초기에 엄청 싸웠어요",
          "original_form": "그래서 결혼초기에 엄청 싸웠어요",
          "speaker_id": "1",
          "time": "20210821 11:38"
        },
        {
          "id": "MDRW2100003738.1.18",
          "form": "완전 정반대인데 어떻게 결혼하셨어요?",
          "original_form": "완전 정반대인데 어떻게 결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003738.1.19",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003738.1.20",
          "form": "연애중에 모르셨던거예요?",
          "original_form": "연애중에 모르셨던거예요?",
          "speaker_id": "2",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003738.1.21",
          "form": "이게 결혼하니 사람이 확 바뀌더라고요",
          "original_form": "이게 결혼하니 사람이 확 바뀌더라고요",
          "speaker_id": "1",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003738.1.22",
          "form": "헐 연애때랑 다르게요?",
          "original_form": "헐 연애때랑 다르게요?",
          "speaker_id": "2",
          "time": "20210821 11:40"
        },
        {
          "id": "MDRW2100003738.1.23",
          "form": "네~~엄청 다정다감하고 저한테 다 맞춰주는 사람이었어요",
          "original_form": "네~~엄청 다정다감하고 저한테 다 맞춰주는 사람이었어요",
          "speaker_id": "1",
          "time": "20210821 11:40"
        },
        {
          "id": "MDRW2100003738.1.24",
          "form": "결혼하려고 맞춰준거였나보다",
          "original_form": "결혼하려고 맞춰준거였나보다",
          "speaker_id": "2",
          "time": "20210821 11:41"
        }
      ]
    }
