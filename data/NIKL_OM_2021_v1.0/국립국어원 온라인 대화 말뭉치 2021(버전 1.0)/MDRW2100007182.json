      "utterance": [
        {
          "id": "MDRW2100007182.1.1",
          "form": "저녁은 먹었어?",
          "original_form": "저녁은 먹었어?",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007182.1.2",
          "form": "응 김치찌개!",
          "original_form": "응 김치찌개!",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007182.1.3",
          "form": "너는 뭐먹었어?",
          "original_form": "너는 뭐먹었어?",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007182.1.4",
          "form": "나는 치킨 배달 시켜먹었어",
          "original_form": "나는 치킨 배달 시켜먹었어",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007182.1.5",
          "form": "오 어디치킨??",
          "original_form": "오 어디치킨??",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007182.1.6",
          "form": "푸라닭 양념 반 후라이드 반!",
          "original_form": "푸라닭 양념 반 후라이드 반!",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007182.1.7",
          "form": "헐 푸라닭 진짜 맛있는데!!",
          "original_form": "헐 푸라닭 진짜 맛있는데!!",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007182.1.8",
          "form": "맛있는데 부족했어! 그래서 밥도 먹었어",
          "original_form": "맛있는데 부족했어! 그래서 밥도 먹었어",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007182.1.9",
          "form": "나는 고ㅊㅜ마요 최애인데 오랜만에 먹고싶다 ㅠㅠ",
          "original_form": "나는 고ㅊㅜ마요 최애인데 오랜만에 먹고싶다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007182.1.10",
          "form": "그건 안 먹어봤는데 다음에 먹어볼게\r\n원래 치킨을 제일 좋아해?",
          "original_form": "그건 안 먹어봤는데 다음에 먹어볼게\r\n원래 치킨을 제일 좋아해?",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007182.1.11",
          "form": "나는 원래는 치킨보다 피자!",
          "original_form": "나는 원래는 치킨보다 피자!",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007182.1.12",
          "form": "어디 피자 좋아해?",
          "original_form": "어디 피자 좋아해?",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007182.1.13",
          "form": "나는 반올림 피자샵!",
          "original_form": "나는 반올림 피자샵!",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007182.1.14",
          "form": "반올림피자 좋아해 ㅎㅎ 거기 갈릭소스가 맛있어!",
          "original_form": "반올림피자 좋아해 ㅎㅎ 거기 갈릭소스가 맛있어!",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007182.1.15",
          "form": "헐 통했다!",
          "original_form": "헐 통했다!",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007182.1.16",
          "form": "헉 진짜 맛있어",
          "original_form": "헉 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007182.1.17",
          "form": "나는 콤비네이션 피자! 진짜 푸짐하더라",
          "original_form": "나는 콤비네이션 피자! 진짜 푸짐하더라",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007182.1.18",
          "form": "원플러스원도 해주고..(감동)",
          "original_form": "원플러스원도 해주고..(감동)",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007182.1.19",
          "form": "진짜 토핑도 실하고 좋앟ㅎㅎ",
          "original_form": "진짜 토핑도 실하고 좋앟ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007182.1.20",
          "form": "원래 그런 피자집이 있는지도 몰랐는데 광희 유튜브 보고 알았어\r\n난 매번 핫치킨이랑 콤비네이션 시켜ㅋㅋ",
          "original_form": "원래 그런 피자집이 있는지도 몰랐는데 광희 유튜브 보고 알았어\r\n난 매번 핫치킨이랑 콤비네이션 시켜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007182.1.21",
          "form": "오 광희 유튜브에서 나왔었구나! 나는 아이유 광고로 알아서 먹어봤는데 맛있더라구 ㅠㅠ",
          "original_form": "오 광희 유튜브에서 나왔었구나! 나는 아이유 광고로 알아서 먹어봤는데 맛있더라구 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007182.1.22",
          "form": "맞아\r\n나도 배달 시켰더니 아이유 포토카드를 받았는데 쓸데가 없었어ㅋㅋ",
          "original_form": "맞아\r\n나도 배달 시켰더니 아이유 포토카드를 받았는데 쓸데가 없었어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:58"
        }
      ]
    }
