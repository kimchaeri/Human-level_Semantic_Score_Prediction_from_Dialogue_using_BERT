      "utterance": [
        {
          "id": "MDRW2100036567.1.1",
          "form": "어제도 더워서 에어컨 틀었다",
          "original_form": "어제도 더워서 에어컨 틀었다",
          "speaker_id": "2",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100036567.1.2",
          "form": "그러게 잘려고 하니 덥더라",
          "original_form": "그러게 잘려고 하니 덥더라",
          "speaker_id": "1",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100036567.1.3",
          "form": "응 애들이 더워서 못잔다고",
          "original_form": "응 애들이 더워서 못잔다고",
          "speaker_id": "2",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036567.1.4",
          "form": "시원해지나 했더니 덥다",
          "original_form": "시원해지나 했더니 덥다",
          "speaker_id": "1",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036567.1.5",
          "form": "더운거보다 습도가 있어서 그렇다",
          "original_form": "더운거보다 습도가 있어서 그렇다",
          "speaker_id": "2",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036567.1.6",
          "form": "응 제습기를 계속 켜놓고 있어",
          "original_form": "응 제습기를 계속 켜놓고 있어",
          "speaker_id": "1",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036567.1.7",
          "form": "제습기때문에 더 더운거 아니야",
          "original_form": "제습기때문에 더 더운거 아니야",
          "speaker_id": "2",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036567.1.8",
          "form": "ㅋㅋ 그럴지도 모르지",
          "original_form": "ㅋㅋ 그럴지도 모르지",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100036567.1.9",
          "form": "비소식은 없어",
          "original_form": "비소식은 없어",
          "speaker_id": "2",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100036567.1.10",
          "form": "목 금요일 비가 온다는데 모르지",
          "original_form": "목 금요일 비가 온다는데 모르지",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100036567.1.11",
          "form": "맞다 실시간예보이니",
          "original_form": "맞다 실시간예보이니",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100036567.1.12",
          "form": "엉 날씨앱을 볼때마다 날씨가 틀리다 ㅋㅋ",
          "original_form": "엉 날씨앱을 볼때마다 날씨가 틀리다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100036567.1.13",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100036567.1.14",
          "form": "날씨가 계속 덥네",
          "original_form": "날씨가 계속 덥네",
