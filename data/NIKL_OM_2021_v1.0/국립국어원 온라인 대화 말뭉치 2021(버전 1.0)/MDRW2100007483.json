      "utterance": [
        {
          "id": "MDRW2100007483.1.1",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007483.1.2",
          "form": "저는 제가 생각한 범위 안에 사람 들어오는걸",
          "original_form": "저는 제가 생각한 범위 안에 사람 들어오는걸",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007483.1.3",
          "form": "별로 안 좋아해서..",
          "original_form": "별로 안 좋아해서..",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007483.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007483.1.5",
          "form": "기숙사도 쓰다가 나왔거든요..ㅎㅎ",
          "original_form": "기숙사도 쓰다가 나왔거든요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007483.1.6",
          "form": "저도 선은 있어요ㅋㅋㅋㅋㅋ 내성적이기도 하고",
          "original_form": "저도 선은 있어요ㅋㅋㅋㅋㅋ 내성적이기도 하고",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007483.1.7",
          "form": "나빼고 다 싸워...",
          "original_form": "나빼고 다 싸워...",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007483.1.8",
          "form": "기숙사는 모르는 사람이랑 사니깐",
          "original_form": "기숙사는 모르는 사람이랑 사니깐",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007483.1.9",
          "form": "확실히 불편하더라구요",
          "original_form": "확실히 불편하더라구요",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007483.1.10",
          "form": "나중에 결혼 계획 있으세요?",
          "original_form": "나중에 결혼 계획 있으세요?",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007483.1.11",
          "form": "아니면 비혼?",
          "original_form": "아니면 비혼?",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007483.1.12",
          "form": "위와 같은 사유로 결혼 생각 없어요!",
          "original_form": "위와 같은 사유로 결혼 생각 없어요!",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007483.1.13",
          "form": "아직까지 굳이 해야할 이유를 못 찾았어요!",
          "original_form": "아직까지 굳이 해야할 이유를 못 찾았어요!",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007483.1.14",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007483.1.15",
          "form": "저도 비혼은 아닌데",
          "original_form": "저도 비혼은 아닌데",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007483.1.16",
          "form": "꼭 결혼해야한다 이것도 아니에요",
          "original_form": "꼭 결혼해야한다 이것도 아니에요",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007483.1.17",
          "form": "요새는 약간",
          "original_form": "요새는 약간",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007483.1.18",
          "form": "난 무조건 결혼 안해!!! 보단 굳이 결혼 해야해? 가 많아지는거 같아요",
          "original_form": "난 무조건 결혼 안해!!! 보단 굳이 결혼 해야해? 가 많아지는거 같아요",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007483.1.19",
          "form": "근데 전 결혼 안한다고 하면",
          "original_form": "근데 전 결혼 안한다고 하면",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007483.1.20",
          "form": "부모님이반대하세요",
          "original_form": "부모님이반대하세요",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007483.1.21",
          "form": "부모님 반대를 이겨내고 하는 결혼이 아닌..",
          "original_form": "부모님 반대를 이겨내고 하는 결혼이 아닌..",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007483.1.22",
          "form": "부모님의 반대를 이겨내고 하는 비혼...",
          "original_form": "부모님의 반대를 이겨내고 하는 비혼...",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007483.1.23",
          "form": "아이러니하네요",
          "original_form": "아이러니하네요",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007483.1.24",
          "form": "뭐 좋은 사람있으면 결혼하는 것도 좋은데",
          "original_form": "뭐 좋은 사람있으면 결혼하는 것도 좋은데",
          "speaker_id": "1",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007483.1.25",
          "form": "결혼해도 전 제 방을 갖고 싶어요",
          "original_form": "결혼해도 전 제 방을 갖고 싶어요",
          "speaker_id": "1",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007483.1.26",
          "form": "제가 좋아하는 것들로 가득한 방",
          "original_form": "제가 좋아하는 것들로 가득한 방",
          "speaker_id": "2",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007483.1.27",
          "form": "들어갔을때 심리적 안정을 가질 수 있는 그런 공간",
          "original_form": "들어갔을때 심리적 안정을 가질 수 있는 그런 공간",
          "speaker_id": "2",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007483.1.28",
          "form": "무조건 있어야해요",
          "original_form": "무조건 있어야해요",
          "speaker_id": "2",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007483.1.29",
          "form": "맞아요 혼자서 충전할 수 있는 방",
          "original_form": "맞아요 혼자서 충전할 수 있는 방",
          "speaker_id": "1",
          "time": "20210822 02:49"
        }
      ]
    }
