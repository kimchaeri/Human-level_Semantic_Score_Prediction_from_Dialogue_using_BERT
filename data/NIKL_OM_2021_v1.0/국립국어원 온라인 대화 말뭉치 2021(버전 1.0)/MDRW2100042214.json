        },
        {
          "id": "MDRW2100042214.1.2",
          "form": "name1님은 어디꺼 쓰세요?",
          "original_form": "&name1&님은 어디꺼 쓰세요?",
          "speaker_id": "2",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100042214.1.3",
          "form": "아 저는 데이지크란 곳",
          "original_form": "아 저는 데이지크란 곳",
          "speaker_id": "1",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100042214.1.4",
          "form": "인스타 광고 보고 샀는데 조아여",
          "original_form": "인스타 광고 보고 샀는데 조아여",
          "speaker_id": "1",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042214.1.5",
          "form": "그것도 처음 들어요~",
          "original_form": "그것도 처음 들어요~",
          "speaker_id": "2",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042214.1.6",
          "form": "안뭉치나요?",
          "original_form": "안뭉치나요?",
          "speaker_id": "2",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042214.1.7",
          "form": "네네 뭉치진않고 갈색이라 이뻐요",
          "original_form": "네네 뭉치진않고 갈색이라 이뻐요",
          "speaker_id": "1",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042214.1.8",
          "form": "그제 새로 산 로퍼 오늘 신고갔는데",
          "original_form": "그제 새로 산 로퍼 오늘 신고갔는데",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042214.1.9",
          "form": "편하고 좋았어요",
          "original_form": "편하고 좋았어요",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042214.1.10",
          "form": "우왕!! 갈색 마스카라~^^ \r\n그 로퍼는 어디껀가요?",
          "original_form": "우왕!! 갈색 마스카라~^^ \r\n그 로퍼는 어디껀가요?",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042214.1.11",
          "form": "로퍼 인터넷 마켓으로 샀어요",
          "original_form": "로퍼 인터넷 마켓으로 샀어요",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100042214.1.12",
          "form": "유기견 기부도 되는 마켓이라 좋았어요",
          "original_form": "유기견 기부도 되는 마켓이라 좋았어요",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100042214.1.13",
          "form": "쇼핑도 하고 좋은 일도 하셨군요^-^",
          "original_form": "쇼핑도 하고 좋은 일도 하셨군요^-^",
          "speaker_id": "2",
          "time": "20210906 20:48"
        },
        {
          "id": "MDRW2100042214.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042214.1.15",
          "form": "저렴한데 질도 좋아서",
          "original_form": "저렴한데 질도 좋아서",
          "speaker_id": "1",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042214.1.16",
          "form": "잘 산거같아요",
          "original_form": "잘 산거같아요",
          "speaker_id": "1",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042214.1.17",
          "form": "오~ 최고네요^^ 신발은 편해야 신어지더라고요",
          "original_form": "오~ 최고네요^^ 신발은 편해야 신어지더라고요",
          "speaker_id": "2",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042214.1.18",
          "form": "맞아요 아무리 이뻐도 아프면 끝 ㅠㅠ",
          "original_form": "맞아요 아무리 이뻐도 아프면 끝 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042214.1.19",
          "form": "저 처녀때 산 굽 높은 구두 못신잖아요. 그래서 굽 낮은 신발로 다 다시 샀어요",
          "original_form": "저 처녀때 산 굽 높은 구두 못신잖아요. 그래서 굽 낮은 신발로 다 다시 샀어요",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042214.1.20",
          "form": "맞아요ㅠ저도 이제 높은 건 안사요ㅠㅠ",
          "original_form": "맞아요ㅠ저도 이제 높은 건 안사요ㅠㅠ",
