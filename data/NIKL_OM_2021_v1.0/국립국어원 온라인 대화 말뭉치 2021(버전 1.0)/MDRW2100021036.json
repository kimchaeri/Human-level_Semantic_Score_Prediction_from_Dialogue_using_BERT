          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100021036.1.13",
          "form": "아 저도 팔때주로쓰는데",
          "original_form": "아 저도 팔때주로쓰는데",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100021036.1.14",
          "form": "사기도해요 ㅋㅋ",
          "original_form": "사기도해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100021036.1.15",
          "form": "오 주로 뭐사세요?",
