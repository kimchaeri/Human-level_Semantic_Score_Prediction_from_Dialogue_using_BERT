        {
          "id": "MDRW2100021036.1.1",
          "form": "무료 생각보다 자주 하더라구여 ㅋㅋㅋㅋ",
          "original_form": "무료 생각보다 자주 하더라구여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100021036.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100021036.1.3",
          "form": "넘좋아요",
          "original_form": "넘좋아요",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021036.1.4",
          "form": "은근 이벤트 많이해요",
          "original_form": "은근 이벤트 많이해요",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021036.1.5",
          "form": "맞아요 꿀ㅋㅋㅋㅋ",
          "original_form": "맞아요 꿀ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021036.1.6",
          "form": "네이버페이가 여기저기 다 돼서 쇼핑하기 좋아요",
          "original_form": "네이버페이가 여기저기 다 돼서 쇼핑하기 좋아요",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021036.1.7",
          "form": "저도 이벤트 많이 챙기는편이라",
          "original_form": "저도 이벤트 많이 챙기는편이라",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021036.1.8",
          "form": "자주해요 ㅋㅋ",
          "original_form": "자주해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021036.1.9",
          "form": "아하 ㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021036.1.10",
          "form": "저두여 ㅋㅋㅋㅋ",
          "original_form": "저두여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021036.1.11",
          "form": "당근은 사용하세요?",
          "original_form": "당근은 사용하세요?",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100021036.1.12",
          "form": "네 ㅋㅋㅋㅋ 가끔팔아요",
          "original_form": "네 ㅋㅋㅋㅋ 가끔팔아요",
