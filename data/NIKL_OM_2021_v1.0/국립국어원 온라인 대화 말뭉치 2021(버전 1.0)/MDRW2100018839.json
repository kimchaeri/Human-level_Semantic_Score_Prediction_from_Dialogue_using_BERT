      "utterance": [
        {
          "id": "MDRW2100018839.1.1",
          "form": "왜 뭔데???",
          "original_form": "왜 뭔데???",
          "speaker_id": "2",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018839.1.2",
          "form": "아니 책 저자가 브라운 어쩌고 였거든",
          "original_form": "아니 책 저자가 브라운 어쩌고 였거든",
          "speaker_id": "1",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018839.1.3",
          "form": "당연히 외국인인줄 알았는데",
          "original_form": "당연히 외국인인줄 알았는데",
          "speaker_id": "1",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018839.1.4",
          "form": "한국남자였음",
          "original_form": "한국남자였음",
          "speaker_id": "1",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018839.1.5",
          "form": "당황스러움",
          "original_form": "당황스러움",
          "speaker_id": "1",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100018839.1.6",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018839.1.7",
          "form": "므ㅓ야",
          "original_form": "므ㅓ야",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018839.1.8",
          "form": "브라운은 누가봐도 외국인 이름 아니야?? 뭐야",
          "original_form": "브라운은 누가봐도 외국인 이름 아니야?? 뭐야",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018839.1.9",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018839.1.10",
          "form": "원래저자이름에",
          "original_form": "원래저자이름에",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018839.1.11",
          "form": "본명들어가는거아니야?",
          "original_form": "본명들어가는거아니야?",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018839.1.12",
          "form": "본명아니어도됨?",
          "original_form": "본명아니어도됨?",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018839.1.13",
          "form": "아 그런 경우가 있음",
          "original_form": "아 그런 경우가 있음",
          "speaker_id": "2",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018839.1.14",
          "form": "필명 쓰는 사람이 있으니까",
          "original_form": "필명 쓰는 사람이 있으니까",
          "speaker_id": "2",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018839.1.15",
          "form": "근데 보통 국적 판별에 혼동을 주는 필명은 잘 안쓰지 않나?",
          "original_form": "근데 보통 국적 판별에 혼동을 주는 필명은 잘 안쓰지 않나?",
          "speaker_id": "2",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018839.1.16",
          "form": "아 필명은 작가가 정하는거야?",
          "original_form": "아 필명은 작가가 정하는거야?",
          "speaker_id": "1",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018839.1.17",
          "form": "별명같은느낌인가?",
          "original_form": "별명같은느낌인가?",
          "speaker_id": "1",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018839.1.18",
          "form": "그러니까 당황스러움",
          "original_form": "그러니까 당황스러움",
          "speaker_id": "1",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018839.1.19",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018839.1.20",
          "form": "그니까 뭐여 진따",
          "original_form": "그니까 뭐여 진따",
          "speaker_id": "2",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018839.1.21",
          "form": "그냥도서관에서빌린거라서다행이지",
          "original_form": "그냥도서관에서빌린거라서다행이지",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018839.1.22",
          "form": "내돈주고산거였음",
          "original_form": "내돈주고산거였음",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018839.1.23",
          "form": "진짜화났을듯",
          "original_form": "진짜화났을듯",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018839.1.24",
          "form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018839.1.25",
          "form": "ㅇㅈ 출펀사에 글 썼재",
          "original_form": "ㅇㅈ 출펀사에 글 썼재",
          "speaker_id": "2",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018839.1.26",
          "form": "나오길래",
          "original_form": "나오길래",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018839.1.27",
          "form": "번역을 이런식으로도 하나",
          "original_form": "번역을 이런식으로도 하나",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018839.1.28",
          "form": "ㅋㅋㅋㅋㅋ나는 처음에 서울 이런 지역이름이",
          "original_form": "ㅋㅋㅋㅋㅋ나는 처음에 서울 이런 지역이름이",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018839.1.29",
          "form": "그랬음",
          "original_form": "그랬음",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018839.1.30",
          "form": "주식얘기하는데삼성전자나오고하길래",
          "original_form": "주식얘기하는데삼성전자나오고하길래",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018839.1.31",
          "form": "외국우량주를임의로번역한줄",
          "original_form": "외국우량주를임의로번역한줄",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018839.1.32",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018839.1.33",
          "form": "아 약간 투니버스가 했던 것 처럼??",
          "original_form": "아 약간 투니버스가 했던 것 처럼??",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018839.1.34",
          "form": "아 진심ㅋㅌㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 진심ㅋㅌㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018839.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ투니버슼ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ투니버슼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018839.1.36",
          "form": "그래서그냥적당히읽는중",
          "original_form": "그래서그냥적당히읽는중",
          "speaker_id": "1",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100018839.1.37",
          "form": "생각보다새로운내용도없어서",
          "original_form": "생각보다새로운내용도없어서",
          "speaker_id": "1",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100018839.1.38",
          "form": "ㅋㅋㅋㅋㅋㅌㅌㅋㅌㅌㅌㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅌㅋㅌㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210827 12:55"
        },
        {
          "id": "MDRW2100018839.1.39",
          "form": "아 그래??",
          "original_form": "아 그래??",
