      "utterance": [
        {
          "id": "MDRW2100045908.1.1",
          "form": "아침드셯나요",
          "original_form": "아침드셯나요",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045908.1.2",
          "form": "아침 먹고 있습니다!",
          "original_form": "아침 먹고 있습니다!",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045908.1.3",
          "form": "식빵에 쨈 발라서요!",
          "original_form": "식빵에 쨈 발라서요!",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045908.1.4",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045908.1.5",
          "form": "오 저는 아직요",
          "original_form": "오 저는 아직요",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045908.1.6",
          "form": "쨈맛있겠네요",
          "original_form": "쨈맛있겠네요",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045908.1.7",
          "form": "아이구 어서 드셔요 맛있는거 드셔야 할텐데",
          "original_form": "아이구 어서 드셔요 맛있는거 드셔야 할텐데",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045908.1.8",
          "form": "쨈 직접 만들어서 먹는건데 시중쨈보다 맛있는거 같아요 ㅋㅋㅋㅋ",
          "original_form": "쨈 직접 만들어서 먹는건데 시중쨈보다 맛있는거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045908.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045908.1.10",
          "form": "무슨쨈이에요?",
          "original_form": "무슨쨈이에요?",
          "speaker_id": "1",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100045908.1.11",
          "form": "딸기쨈이요!",
          "original_form": "딸기쨈이요!",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045908.1.12",
          "form": "ㅎㅎ맛있겠어요",
          "original_form": "ㅎㅎ맛있겠어요",
          "speaker_id": "1",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045908.1.13",
          "form": "네! ㅋㅋㅋ 그래서 맛나게 먹었습니다! 딱 아침으로 좋은거 같아요",
          "original_form": "네! ㅋㅋㅋ 그래서 맛나게 먹었습니다! 딱 아침으로 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045908.1.14",
          "form": "아침 뭐드실 예정이신가요?",
          "original_form": "아침 뭐드실 예정이신가요?",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045908.1.15",
          "form": "저는 ㅎㅎㅎ 만두랑 사과중에 고민이에요",
          "original_form": "저는 ㅎㅎㅎ 만두랑 사과중에 고민이에요",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045908.1.16",
          "form": "아앗 아침사과가 좋다곤 하는데",
          "original_form": "아앗 아침사과가 좋다곤 하는데",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045908.1.17",
          "form": "만두도 맛있겠네요",
          "original_form": "만두도 맛있겠네요",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045908.1.18",
          "form": "ㅎㅎㅎ고민되네요",
          "original_form": "ㅎㅎㅎ고민되네요",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045908.1.19",
          "form": "맛집좋아하세요?",
          "original_form": "맛집좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045908.1.20",
          "form": "둘다..는 넘 많으려나요 아침에 ㅋ큐ㅠ",
          "original_form": "둘다..는 넘 많으려나요 아침에 ㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045908.1.21",
          "form": "맛집 좋아하죠!",
          "original_form": "맛집 좋아하죠!",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045908.1.22",
          "form": "식생활이 좋은 활력소인걸요!",
          "original_form": "식생활이 좋은 활력소인걸요!",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045908.1.23",
          "form": "ㅎㅎㅎ가능하죵",
          "original_form": "ㅎㅎㅎ가능하죵",
          "speaker_id": "1",
          "time": "20210908 09:27"
        }
      ]
