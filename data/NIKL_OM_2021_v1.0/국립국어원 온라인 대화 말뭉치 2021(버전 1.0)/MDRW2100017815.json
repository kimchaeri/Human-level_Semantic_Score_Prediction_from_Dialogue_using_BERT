      "utterance": [
        {
          "id": "MDRW2100017815.1.1",
          "form": "동물농장 보신다고 하셨나요?",
          "original_form": "동물농장 보신다고 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017815.1.2",
          "form": "아 네네 ㅋㅋㅋㅋ",
          "original_form": "아 네네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017815.1.3",
          "form": "방송사 채널 구독해서 예능클립들도 많이 봐요",
          "original_form": "방송사 채널 구독해서 예능클립들도 많이 봐요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017815.1.4",
          "form": "오오 진짜 동물 좋아하시나봐요",
          "original_form": "오오 진짜 동물 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017815.1.5",
          "form": "저는 방송사 관련된건 문명특급 뿐이에욬ㅋㅋ",
          "original_form": "저는 방송사 관련된건 문명특급 뿐이에욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017815.1.6",
          "form": "귀여운 동물도 많이 나오고 은근 동물농장도 괜히 궁금하게 영상을 올리는 게 많거든요",
          "original_form": "귀여운 동물도 많이 나오고 은근 동물농장도 괜히 궁금하게 영상을 올리는 게 많거든요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017815.1.7",
          "form": "아 문특 재밌죠",
          "original_form": "아 문특 재밌죠",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017815.1.8",
          "form": "지금은 좀 밀리긴 했지만",
          "original_form": "지금은 좀 밀리긴 했지만",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017815.1.9",
          "form": "자주 봤어요",
          "original_form": "자주 봤어요",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017815.1.10",
          "form": "최애편은 뭐예요?",
          "original_form": "최애편은 뭐예요?",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017815.1.11",
          "form": "최애라고 하긴 뭐하지만 지금 생각나는게 암욜맨 작곡가인가 작사가인가랑 인터뷰한 게 웃겼던 것 같아요",
          "original_form": "최애라고 하긴 뭐하지만 지금 생각나는게 암욜맨 작곡가인가 작사가인가랑 인터뷰한 게 웃겼던 것 같아요",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017815.1.12",
          "form": "숨듣명시리즈요 ㅋㅋㅋ",
          "original_form": "숨듣명시리즈요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017815.1.13",
          "form": "아아맞아요",
          "original_form": "아아맞아요",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017815.1.14",
          "form": "숨듣명 꿀잼이었어요",
          "original_form": "숨듣명 꿀잼이었어요",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017815.1.15",
          "form": "컴듣명도 꿀잼이고",
          "original_form": "컴듣명도 꿀잼이고",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017815.1.16",
          "form": "그쵸 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017815.1.17",
          "form": "아 걍 다 잼써요",
          "original_form": "아 걍 다 잼써요",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017815.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017815.1.19",
          "form": "맞아요!!ㅋㅋㄱㅋ",
          "original_form": "맞아요!!ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017815.1.20",
          "form": "재재가 진짜 인터뷰를 잘하는 것 같아요",
          "original_form": "재재가 진짜 인터뷰를 잘하는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017815.1.21",
          "form": "매번 유쾌하게",
          "original_form": "매번 유쾌하게",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017815.1.22",
          "form": "맞아요 인터뷰할때마다 팬되고",
          "original_form": "맞아요 인터뷰할때마다 팬되고",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017815.1.23",
          "form": "컴백한 가수들 괜히 한번씩찾아봐욬ㅋㅋ",
          "original_form": "컴백한 가수들 괜히 한번씩찾아봐욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017815.1.24",
          "form": "유튜브 말고 다른 거 구독하는 거 있으세요?",
          "original_form": "유튜브 말고 다른 거 구독하는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017815.1.25",
          "form": "넷플릭스 같은거요",
          "original_form": "넷플릭스 같은거요",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017815.1.26",
          "form": "넷플릭스는 최근에 구독해지했어요.",
          "original_form": "넷플릭스는 최근에 구독해지했어요.",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017815.1.27",
          "form": "아 그러셨군요 저는 지금 구독을 고민하고 있는데 ㅋㅋㅋ",
          "original_form": "아 그러셨군요 저는 지금 구독을 고민하고 있는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017815.1.28",
          "form": "뽕뽑고 해지했어요 ㅎㅎ",
          "original_form": "뽕뽑고 해지했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017815.1.29",
          "form": "크리스틴스튜어트 좋아하는데 미녀삼총사재미있게봤어요",
          "original_form": "크리스틴스튜어트 좋아하는데 미녀삼총사재미있게봤어요",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017815.1.30",
          "form": "샤를리즈테론꺼는 올드가드요 ㅎㅎ",
          "original_form": "샤를리즈테론꺼는 올드가드요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:29"
        }
      ]
