        {
          "id": "MDRW2100033853.1.1",
          "form": "열띈",
          "original_form": "열띈",
          "speaker_id": "2",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100033853.1.2",
          "form": "토론을 하고싶었는데떡볶이주제로",
          "original_form": "토론을 하고싶었는데떡볶이주제로",
          "speaker_id": "2",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100033853.1.3",
          "form": "ㅎㅎ 안타깝네요.",
          "original_form": "ㅎㅎ 안타깝네요.",
          "speaker_id": "1",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100033853.1.4",
          "form": "저는 포장마차서도 떡볶이보다 닭꼬치 순대파라",
          "original_form": "저는 포장마차서도 떡볶이보다 닭꼬치 순대파라",
          "speaker_id": "1",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100033853.1.5",
          "form": "최근 유행하는 로제떡볶이 먹어봤는데 이건 또 괜찮았어요",
          "original_form": "최근 유행하는 로제떡볶이 먹어봤는데 이건 또 괜찮았어요",
          "speaker_id": "1",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100033853.1.6",
          "form": "어디 떡볶이  좋아하시나요",
          "original_form": "어디 떡볶이  좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100033853.1.7",
          "form": "와우 순대파이시군요ㅋㅋㅋ",
          "original_form": "와우 순대파이시군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:02"
        },
        {
          "id": "MDRW2100033853.1.8",
          "form": "전다좋아하는데",
          "original_form": "전다좋아하는데",
          "speaker_id": "2",
          "time": "20210825 13:02"
        },
        {
          "id": "MDRW2100033853.1.9",
          "form": "요즘은 로제에 꽂혔어요",
          "original_form": "요즘은 로제에 꽂혔어요",
          "speaker_id": "2",
          "time": "20210825 13:02"
        },
        {
          "id": "MDRW2100033853.1.10",
          "form": "로제불닭당면 드셔보셨어요?",
          "original_form": "로제불닭당면 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:02"
        },
        {
          "id": "MDRW2100033853.1.11",
          "form": "납작당면 들어갔는데 떡보다 어울리더라고요",
          "original_form": "납작당면 들어갔는데 떡보다 어울리더라고요",
          "speaker_id": "1",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100033853.1.12",
          "form": "로제불닭",
          "original_form": "로제불닭",
          "speaker_id": "2",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100033853.1.13",
          "form": "당면은뭔가요?!",
          "original_form": "당면은뭔가요?!",
          "speaker_id": "2",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100033853.1.14",
          "form": "삼양 신상품인데 요새 나름 핫하대요",
          "original_form": "삼양 신상품인데 요새 나름 핫하대요",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033853.1.15",
          "form": "양은 좀 적지만 만족스러운 맛이에요",
          "original_form": "양은 좀 적지만 만족스러운 맛이에요",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033853.1.16",
          "form": "마라탕에 넣는 그 납작당면이에요ㅎ",
          "original_form": "마라탕에 넣는 그 납작당면이에요ㅎ",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033853.1.17",
          "form": "와우 그런게나왔군요?",
          "original_form": "와우 그런게나왔군요?",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100033853.1.18",
          "form": "신라면볶음면도 있다더니",
          "original_form": "신라면볶음면도 있다더니",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100033853.1.19",
          "form": "요즘은 별게다나오네요",
          "original_form": "요즘은 별게다나오네요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100033853.1.20",
          "form": "볶음면 궁금했는데 평이 워낙 안 좋더라고요",
          "original_form": "볶음면 궁금했는데 평이 워낙 안 좋더라고요",
          "speaker_id": "1",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100033853.1.21",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "1",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100033853.1.22",
          "form": "아진짜요?",
          "original_form": "아진짜요?",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033853.1.23",
          "form": "신라면볶음면 평이 괜찮던데ㄷㄷ",
          "original_form": "신라면볶음면 평이 괜찮던데ㄷㄷ",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033853.1.24",
          "form": "너무 짜고 자극적이라던데",
          "original_form": "너무 짜고 자극적이라던데",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033853.1.25",
          "form": "제 주변만 평이 이상한가봐요",
          "original_form": "제 주변만 평이 이상한가봐요",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033853.1.26",
          "form": "name2님 쪽은 평이 좋다니 혹하네요",
          "original_form": "&name2&님 쪽은 평이 좋다니 혹하네요",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100033853.1.27",
          "form": "생각보다 괜찮다고들해서",
          "original_form": "생각보다 괜찮다고들해서",
          "speaker_id": "2",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100033853.1.28",
          "form": "저는 한번먹어볼참이었거든요ㅋㅋㅋ",
          "original_form": "저는 한번먹어볼참이었거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100033853.1.29",
          "form": "맵고 짠 음식 좋아하면 괜찮을 것 같아요ㅎ",
          "original_form": "맵고 짠 음식 좋아하면 괜찮을 것 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100033853.1.30",
          "form": "사실 제가 딱 그런 입맛이라ㅎㅎ",
          "original_form": "사실 제가 딱 그런 입맛이라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100033853.1.31",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100033853.1.32",
          "form": "저도그런거좋아해요",
          "original_form": "저도그런거좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:58"
        }
      ]
