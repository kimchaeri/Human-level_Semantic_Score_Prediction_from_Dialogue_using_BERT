      "utterance": [
        {
          "id": "MDRW2100046783.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 17:23"
        },
        {
          "id": "MDRW2100046783.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 17:33"
        },
        {
          "id": "MDRW2100046783.1.3",
          "form": "지금 뭐하고 계시나요? ㅎㅎ",
          "original_form": "지금 뭐하고 계시나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 17:33"
        },
        {
          "id": "MDRW2100046783.1.4",
          "form": "안녕하세요 그냥 집에서 쉬는중이에요",
          "original_form": "안녕하세요 그냥 집에서 쉬는중이에요",
          "speaker_id": "1",
          "time": "20211104 17:40"
        },
        {
          "id": "MDRW2100046783.1.5",
          "form": "퇴근하고 오신 거예요~?",
          "original_form": "퇴근하고 오신 거예요~?",
          "speaker_id": "2",
          "time": "20211104 17:43"
        },
        {
          "id": "MDRW2100046783.1.6",
          "form": "아뇨 대학생이에요",
          "original_form": "아뇨 대학생이에요",
          "speaker_id": "1",
          "time": "20211104 17:44"
        },
        {
          "id": "MDRW2100046783.1.7",
          "form": "학생이시군요!",
          "original_form": "학생이시군요!",
          "speaker_id": "2",
          "time": "20211104 17:45"
        },
        {
          "id": "MDRW2100046783.1.8",
          "form": "무슨 전공이세요? ㅎㅎ",
          "original_form": "무슨 전공이세요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 17:45"
        },
        {
          "id": "MDRW2100046783.1.9",
          "form": "전자과에요",
          "original_form": "전자과에요",
          "speaker_id": "1",
          "time": "20211104 17:46"
        },
        {
          "id": "MDRW2100046783.1.10",
          "form": "이과생이시군요 ㅎㅎ",
          "original_form": "이과생이시군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 17:46"
        },
        {
          "id": "MDRW2100046783.1.11",
          "form": "제가 잘 모르는 분야네요! ㅋㅋㅋ",
          "original_form": "제가 잘 모르는 분야네요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 17:46"
        },
        {
          "id": "MDRW2100046783.1.12",
          "form": "저는 완전 문과생이여요",
          "original_form": "저는 완전 문과생이여요",
          "speaker_id": "2",
          "time": "20211104 17:46"
        },
        {
          "id": "MDRW2100046783.1.13",
          "form": "아 넵 무슨과세요?",
          "original_form": "아 넵 무슨과세요?",
          "speaker_id": "1",
          "time": "20211104 17:47"
        },
        {
          "id": "MDRW2100046783.1.14",
          "form": "국문과요! ㅎㅎ",
          "original_form": "국문과요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 17:47"
        },
        {
          "id": "MDRW2100046783.1.15",
          "form": "문과 중에 문과죠",
          "original_form": "문과 중에 문과죠",
          "speaker_id": "2",
          "time": "20211104 17:47"
        },
        {
          "id": "MDRW2100046783.1.16",
          "form": "ㅎㅎ 저녁은 드셨나요?",
          "original_form": "ㅎㅎ 저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20211104 17:48"
        },
        {
          "id": "MDRW2100046783.1.17",
          "form": "아뇨 아직 저는 퇴근을 못했답니다! ㅋㅋㅋ",
          "original_form": "아뇨 아직 저는 퇴근을 못했답니다! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 17:48"
        },
        {
          "id": "MDRW2100046783.1.18",
          "form": "좋아하는 전공 과목 있으세요~?",
          "original_form": "좋아하는 전공 과목 있으세요~?",
          "speaker_id": "2",
          "time": "20211104 17:48"
        },
        {
          "id": "MDRW2100046783.1.19",
          "form": "컴공이랑 복수전공중인데 지금은 프로그래밍 관련된게 재밌어요",
          "original_form": "컴공이랑 복수전공중인데 지금은 프로그래밍 관련된게 재밌어요",
          "speaker_id": "1",
          "time": "20211104 17:49"
        },
        {
          "id": "MDRW2100046783.1.20",
          "form": "복수전공까지 하시면 엄청 바쁘시겠네요~!",
          "original_form": "복수전공까지 하시면 엄청 바쁘시겠네요~!",
          "speaker_id": "2",
          "time": "20211104 17:50"
        },
        {
          "id": "MDRW2100046783.1.21",
          "form": "저는 요즘 코딩이 관심 가더라구요 ㅎㅎ",
          "original_form": "저는 요즘 코딩이 관심 가더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 17:50"
        },
        {
          "id": "MDRW2100046783.1.22",
          "form": "네 바쁘긴한데 과목들이 재밌어서 좋아요",
          "original_form": "네 바쁘긴한데 과목들이 재밌어서 좋아요",
          "speaker_id": "1",
          "time": "20211104 17:50"
        }
      ]
