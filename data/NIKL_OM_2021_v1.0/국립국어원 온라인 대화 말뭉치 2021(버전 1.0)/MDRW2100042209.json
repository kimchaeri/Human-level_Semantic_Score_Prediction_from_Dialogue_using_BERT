      "utterance": [
        {
          "id": "MDRW2100042209.1.1",
          "form": "얘기하니까배고프넹ㆍ",
          "original_form": "얘기하니까배고프넹ㆍ",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042209.1.2",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042209.1.3",
          "form": "야식이 땡길 시간이죠 ㅎㅎ",
          "original_form": "야식이 땡길 시간이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100042209.1.4",
          "form": "ㅎㅎ맛집찾아다니세요?",
          "original_form": "ㅎㅎ맛집찾아다니세요?",
          "speaker_id": "1",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042209.1.5",
          "form": "맛집 찾아가고 싶어서 정보는 많이 탐색하는데 막상 나가는 걸 귀찮아해서 많이 못가봤어요 ㅋㅋ",
          "original_form": "맛집 찾아가고 싶어서 정보는 많이 탐색하는데 막상 나가는 걸 귀찮아해서 많이 못가봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:40"
        },
        {
          "id": "MDRW2100042209.1.6",
          "form": "ㅎㅎㅎ저도요",
          "original_form": "ㅎㅎㅎ저도요",
          "speaker_id": "1",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100042209.1.7",
          "form": "뚫은곳만자주가요",
          "original_form": "뚫은곳만자주가요",
          "speaker_id": "1",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100042209.1.8",
          "form": "그쵸 결국 가던 데만 가게 되는 것 같아요",
          "original_form": "그쵸 결국 가던 데만 가게 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042209.1.9",
          "form": "가끔 그냥 사진만 예쁘게 찍히고 정작 맛은 평범한 데가 많더라구요",
          "original_form": "가끔 그냥 사진만 예쁘게 찍히고 정작 맛은 평범한 데가 많더라구요",
          "speaker_id": "2",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042209.1.10",
          "form": "아하 그런데도있어요",
          "original_form": "아하 그런데도있어요",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042209.1.11",
          "form": "분위기좋은곳도근데가고싶어요ㅎㅎ",
          "original_form": "분위기좋은곳도근데가고싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042209.1.12",
          "form": "맞아요 ㅋㅋ 가끔은 분위기에 마음에 드는 곳도 있긴해요",
          "original_form": "맞아요 ㅋㅋ 가끔은 분위기에 마음에 드는 곳도 있긴해요",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042209.1.13",
          "form": "그래도 맛도 챙겨줬으면 ㅋㅋㄱㄱ",
          "original_form": "그래도 맛도 챙겨줬으면 ㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042209.1.14",
          "form": "ㅎㅎㅎ맞아요",
          "original_form": "ㅎㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042209.1.15",
          "form": "양식한식중식",
          "original_form": "양식한식중식",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042209.1.16",
          "form": "어떤거좋아하세요",
          "original_form": "어떤거좋아하세요",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042209.1.17",
          "form": "선택지엔 없지만 일식 좋아해요 ㅋㅋ",
          "original_form": "선택지엔 없지만 일식 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042209.1.18",
          "form": "돈까스랑 초밥 좋아하거든요",
          "original_form": "돈까스랑 초밥 좋아하거든요",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042209.1.19",
          "form": "근데 본토에서 먹는 일식보다 한국에서 먹는 일식을 더 좋아해요",
          "original_form": "근데 본토에서 먹는 일식보다 한국에서 먹는 일식을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042209.1.20",
          "form": "아 뭔지알아요ㅎㅎ",
          "original_form": "아 뭔지알아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042209.1.21",
          "form": "저도그게간이잘맞아요",
