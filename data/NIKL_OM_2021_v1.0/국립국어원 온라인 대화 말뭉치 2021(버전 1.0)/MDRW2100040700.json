      "utterance": [
        {
          "id": "MDRW2100040700.1.1",
          "form": "오늘은 뭐하는 가요??",
          "original_form": "오늘은 뭐하는 가요??",
          "speaker_id": "1",
          "time": "20210905 11:14"
        },
        {
          "id": "MDRW2100040700.1.2",
          "form": "교회갔다가 집에간다",
          "original_form": "교회갔다가 집에간다",
          "speaker_id": "2",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100040700.1.3",
          "form": "니는 뭐하노",
          "original_form": "니는 뭐하노",
          "speaker_id": "2",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100040700.1.4",
          "form": "비오네ㅡㅡ",
          "original_form": "비오네ㅡㅡ",
          "speaker_id": "2",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100040700.1.5",
          "form": "비좀안오면좋것다",
          "original_form": "비좀안오면좋것다",
          "speaker_id": "2",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100040700.1.6",
          "form": "원은 친구 만나러 힐데스 가고",
          "original_form": "원은 친구 만나러 힐데스 가고",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100040700.1.7",
          "form": "투는 나랑 집에서 놀고",
          "original_form": "투는 나랑 집에서 놀고",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100040700.1.8",
          "form": "신랑은 일하러 가고",
          "original_form": "신랑은 일하러 가고",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100040700.1.9",
          "form": "원 좋네ㅋㅋ",
          "original_form": "원 좋네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100040700.1.10",
          "form": "나는 밥할거 고민하며있다",
          "original_form": "나는 밥할거 고민하며있다",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100040700.1.11",
          "form": "우리 오늘 저녁은 탕수육",
          "original_form": "우리 오늘 저녁은 탕수육",
          "speaker_id": "1",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100040700.1.12",
          "form": "쟁반짜장",
          "original_form": "쟁반짜장",
          "speaker_id": "1",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100040700.1.13",
          "form": "저녁 뭐 먹을 거라 요??",
          "original_form": "저녁 뭐 먹을 거라 요??",
          "speaker_id": "1",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100040700.1.14",
          "form": "우린 김밥먹었다ㅋ",
          "original_form": "우린 김밥먹었다ㅋ",
          "speaker_id": "2",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100040700.1.15",
          "form": "재료사서",
          "original_form": "재료사서",
          "speaker_id": "2",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100040700.1.16",
          "form": "이제설거지남음",
          "original_form": "이제설거지남음",
          "speaker_id": "2",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100040700.1.17",
          "form": "오 김밥도 싸고 대단하네",
          "original_form": "오 김밥도 싸고 대단하네",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100040700.1.18",
          "form": "어여 설거지하고 육퇴합시다",
          "original_form": "어여 설거지하고 육퇴합시다",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100040700.1.19",
          "form": "설거지 진짜;;",
          "original_form": "설거지 진짜;;",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100040700.1.20",
          "form": "식세기 지르고싶음ㅜ",
          "original_form": "식세기 지르고싶음ㅜ",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100040700.1.21",
          "form": "요즘너무싫은 집안일1위",
          "original_form": "요즘너무싫은 집안일1위",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100040700.1.22",
          "form": "ㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100040700.1.23",
          "form": "나도 설거지 제일 싫음",
          "original_form": "나도 설거지 제일 싫음",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100040700.1.24",
          "form": "그냥 부엌 자체가 안 맞다",
          "original_form": "그냥 부엌 자체가 안 맞다",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100040700.1.25",
          "form": "요리도 싫고 설거지도 싫고",
          "original_form": "요리도 싫고 설거지도 싫고",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100040700.1.26",
          "form": "나도 자체가안맞닷",
          "original_form": "나도 자체가안맞닷",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100040700.1.27",
          "form": "설거지겨우함",
          "original_form": "설거지겨우함",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100040700.1.28",
          "form": "12시에자야긋다",
          "original_form": "12시에자야긋다",
          "speaker_id": "2",
          "time": "20210905 23:29"
        },
        {
          "id": "MDRW2100040700.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 고생했네 언니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 고생했네 언니",
          "speaker_id": "1",
          "time": "20210906 10:07"
        },
        {
          "id": "MDRW2100040700.1.30",
          "form": "출근이 낫다 ㅋㅋㅋ",
          "original_form": "출근이 낫다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 10:07"
        },
        {
          "id": "MDRW2100040700.1.31",
          "form": "나도 출근",
          "original_form": "나도 출근",
          "speaker_id": "2",
          "time": "20210906 10:12"
        },
        {
          "id": "MDRW2100040700.1.32",
          "form": "우린 일하자계속ㅋ",
          "original_form": "우린 일하자계속ㅋ",
          "speaker_id": "2",
          "time": "20210906 10:13"
        },
        {
          "id": "MDRW2100040700.1.33",
          "form": "ㅋㅋㅋㅋㅋㄱ 그렇지 주부가 안맞다 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 그렇지 주부가 안맞다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 10:17"
        },
        {
          "id": "MDRW2100040700.1.34",
          "form": "쉬는건 좋은데\r\n주부는 아니지 ㅋㅋㄱ",
          "original_form": "쉬는건 좋은데\r\n주부는 아니지 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 10:17"
        }
      ]
