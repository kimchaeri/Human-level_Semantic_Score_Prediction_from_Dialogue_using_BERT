      "utterance": [
        {
          "id": "MDRW2100038296.1.1",
          "form": "혹시 차이교정 해보셨나요",
          "original_form": "혹시 차이교정 해보셨나요",
          "speaker_id": "1",
          "time": "20210902 08:56"
        },
        {
          "id": "MDRW2100038296.1.2",
          "form": "아뇨 하고 계세요?",
          "original_form": "아뇨 하고 계세요?",
          "speaker_id": "2",
          "time": "20210902 11:51"
        },
        {
          "id": "MDRW2100038296.1.3",
          "form": "저도 안하긴 하는데 요즘 관심이 생겨서요!",
          "original_form": "저도 안하긴 하는데 요즘 관심이 생겨서요!",
          "speaker_id": "1",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100038296.1.4",
          "form": "저 교정하려면 생니를 4개나 빼야한대요",
          "original_form": "저 교정하려면 생니를 4개나 빼야한대요",
          "speaker_id": "1",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100038296.1.5",
          "form": "헉 저도 관심 갖고있는데",
          "original_form": "헉 저도 관심 갖고있는데",
          "speaker_id": "2",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100038296.1.6",
          "form": "오 그러시군요.. 성인교정의 세계",
          "original_form": "오 그러시군요.. 성인교정의 세계",
          "speaker_id": "1",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100038296.1.7",
          "form": "험난해보이더라구요",
          "original_form": "험난해보이더라구요",
          "speaker_id": "1",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100038296.1.8",
          "form": "근데 나중에 치아 약해진다고 해서",
          "original_form": "근데 나중에 치아 약해진다고 해서",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100038296.1.9",
          "form": "고민이에요",
          "original_form": "고민이에요",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100038296.1.10",
          "form": "잇몸 뿌리가 내려앉는다고 하더라구요 ㅠ",
          "original_form": "잇몸 뿌리가 내려앉는다고 하더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100038296.1.11",
          "form": "저 갓던 병원은 교정해도 잇몸에 문제 없다고 하던데",
          "original_form": "저 갓던 병원은 교정해도 잇몸에 문제 없다고 하던데",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100038296.1.12",
          "form": "ㄷ교정하면 양치가 잘 안되서 잇몸이 상할순잇대요",
          "original_form": "ㄷ교정하면 양치가 잘 안되서 잇몸이 상할순잇대요",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100038296.1.13",
          "form": "그렇죠 ㅠ 충치도 꽤 생긴다고 하고",
          "original_form": "그렇죠 ㅠ 충치도 꽤 생긴다고 하고",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038296.1.14",
          "form": "전 그 블랙트라이앵글 생길까봐도",
          "original_form": "전 그 블랙트라이앵글 생길까봐도",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038296.1.15",
          "form": "헉 블랙트라이앵글은 처음들어봐요",
          "original_form": "헉 블랙트라이앵글은 처음들어봐요",
          "speaker_id": "1",
          "time": "20210903 07:44"
        },
        {
          "id": "MDRW2100038296.1.16",
          "form": "찾아보면 나올거에요!!",
          "original_form": "찾아보면 나올거에요!!",
          "speaker_id": "2",
          "time": "20210903 08:19"
        },
        {
          "id": "MDRW2100038296.1.17",
          "form": "와 지금 찾아보고 왔는데 대박무서워요",
          "original_form": "와 지금 찾아보고 왔는데 대박무서워요",
          "speaker_id": "1",
          "time": "20210903 08:24"
        },
        {
          "id": "MDRW2100038296.1.18",
          "form": "그쳐....",
          "original_form": "그쳐....",
          "speaker_id": "2",
          "time": "20210903 08:26"
        },
        {
          "id": "MDRW2100038296.1.19",
