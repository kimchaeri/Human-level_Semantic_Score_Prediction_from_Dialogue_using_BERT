      "utterance": [
        {
          "id": "MDRW2100006211.1.1",
          "form": "혼자 있으니까",
          "original_form": "혼자 있으니까",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006211.1.2",
          "form": "좋나요",
          "original_form": "좋나요",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006211.1.3",
          "form": "아 시댁에서 쉬나요?",
          "original_form": "아 시댁에서 쉬나요?",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006211.1.4",
          "form": "그럼 내일 못보겠군요",
          "original_form": "그럼 내일 못보겠군요",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006211.1.5",
          "form": "몰러요 언제 갈지",
          "original_form": "몰러요 언제 갈지",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006211.1.6",
          "form": "알겠어요",
          "original_form": "알겠어요",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006211.1.7",
          "form": "도련님 소개팅의사있는지 내일 만나면 물어보세요",
          "original_form": "도련님 소개팅의사있는지 내일 만나면 물어보세요",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006211.1.8",
          "form": "이상형이 어떻게",
          "original_form": "이상형이 어떻게",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006211.1.9",
          "form": "되시나요",
          "original_form": "되시나요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006211.1.10",
          "form": "전 목소리좋고 다정한사람인데요",
          "original_form": "전 목소리좋고 다정한사람인데요",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006211.1.11",
          "form": "저군요",
          "original_form": "저군요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006211.1.12",
          "form": "아닌데요",
          "original_form": "아닌데요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006211.1.13",
          "form": "맞는데요",
          "original_form": "맞는데요",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006211.1.14",
          "form": "다정한가요?",
          "original_form": "다정한가요?",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006211.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006211.1.16",
          "form": "모르겠는데요",
          "original_form": "모르겠는데요",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006211.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
