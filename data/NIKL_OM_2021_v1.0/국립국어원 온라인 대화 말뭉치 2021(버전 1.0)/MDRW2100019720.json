      "utterance": [
        {
          "id": "MDRW2100019720.1.1",
          "form": "요즘 읽는책이뭐야?",
          "original_form": "요즘 읽는책이뭐야?",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019720.1.2",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019720.1.3",
          "form": "어찌 찾응?",
          "original_form": "어찌 찾응?",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019720.1.4",
          "form": "도서는 히가시노 게이고 책이용",
          "original_form": "도서는 히가시노 게이고 책이용",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019720.1.5",
          "form": "히가시노게이고중 제일 좋아하는 책은뭐야?",
          "original_form": "히가시노게이고중 제일 좋아하는 책은뭐야?",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019720.1.6",
          "form": "나미야 잡화점의 기적",
          "original_form": "나미야 잡화점의 기적",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019720.1.7",
          "form": "그게 재밌어",
          "original_form": "그게 재밌어",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019720.1.8",
          "form": "추천해",
          "original_form": "추천해",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019720.1.9",
          "form": "난 추리쪽인 채깅더좋아",
          "original_form": "난 추리쪽인 채깅더좋아",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019720.1.10",
          "form": "악의같은거",
          "original_form": "악의같은거",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019720.1.11",
          "form": "추리소설이좋아",
          "original_form": "추리소설이좋아",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019720.1.12",
          "form": "히가시노게이고는 너무 다작이야",
          "original_form": "히가시노게이고는 너무 다작이야",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019720.1.13",
          "form": "정말 히부터 고까지 7명이 쓰는것같아",
          "original_form": "정말 히부터 고까지 7명이 쓰는것같아",
          "speaker_id": "1",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019720.1.14",
          "form": "히가시노 게이고 책은 다 재밌는 것 같아",
          "original_form": "히가시노 게이고 책은 다 재밌는 것 같아",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019720.1.15",
          "form": "최근엔 기린의 날개와 인어가 잠든집을 읽었어",
          "original_form": "최근엔 기린의 날개와 인어가 잠든집을 읽었어",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019720.1.16",
          "form": "주제가 참신해",
          "original_form": "주제가 참신해",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019720.1.17",
          "form": "인어가잠든집은 나도보았어",
          "original_form": "인어가잠든집은 나도보았어",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019720.1.18",
          "form": "주제가신기했고 가족사랑도있었어",
          "original_form": "주제가신기했고 가족사랑도있었어",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019720.1.19",
          "form": "항상 재밌는글을 쓰는작가야",
          "original_form": "항상 재밌는글을 쓰는작가야",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019720.1.20",
          "form": "그 작가가 쓴 책은 영화로도 많이 제작되었던데?",
          "original_form": "그 작가가 쓴 책은 영화로도 많이 제작되었던데?",
          "speaker_id": "2",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100019720.1.21",
          "form": "인어가잠든집도 영화화 됐다는데 궁금해",
          "original_form": "인어가잠든집도 영화화 됐다는데 궁금해",
          "speaker_id": "1",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019720.1.22",
          "form": "용의자 x의 헌신은 우리나라에서 개봉도 했잖아",
          "original_form": "용의자 x의 헌신은 우리나라에서 개봉도 했잖아",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019720.1.23",
          "form": "책을 꼭 읽어보고싶어",
          "original_form": "책을 꼭 읽어보고싶어",
          "speaker_id": "1",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019720.1.24",
          "form": "10월에 개봉해!",
          "original_form": "10월에 개봉해!",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019720.1.25",
          "form": "기대된다",
          "original_form": "기대된다",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019720.1.26",
          "form": "그래 원작 먼저보고싶다",
          "original_form": "그래 원작 먼저보고싶다",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019720.1.27",
          "form": "역시 책은 좋은거야",
          "original_form": "역시 책은 좋은거야",
          "speaker_id": "1",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019720.1.28",
          "form": "인어가 잠든집 봤어?",
          "original_form": "인어가 잠든집 봤어?",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019720.1.29",
          "form": "ㅇㅇ독서동아리에서봤어",
          "original_form": "ㅇㅇ독서동아리에서봤어",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019720.1.30",
          "form": "애기가안쓰러웠어",
          "original_form": "애기가안쓰러웠어",
          "speaker_id": "1",
          "time": "20210827 21:57"
        }
      ]
    }
  ]
