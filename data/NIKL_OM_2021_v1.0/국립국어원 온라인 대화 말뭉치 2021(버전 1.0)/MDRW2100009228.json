      "utterance": [
        {
          "id": "MDRW2100009228.1.1",
          "form": "내일 다시 월요일이네요ㅋㅋ",
          "original_form": "내일 다시 월요일이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009228.1.2",
          "form": "일하러가야재ㅋㅋ",
          "original_form": "일하러가야재ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009228.1.3",
          "form": "월요병ㅡㅡ",
          "original_form": "월요병ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009228.1.4",
          "form": "그러니깐요ㅜㅠ",
          "original_form": "그러니깐요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009228.1.5",
          "form": "내일 알바하러갑니다ㅜㅋㅋㅋㅋㅋ",
          "original_form": "내일 알바하러갑니다ㅜㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009228.1.6",
          "form": "니도 알바가는군",
          "original_form": "니도 알바가는군",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009228.1.7",
          "form": "스트레스 받지말고 일하고 오자ㅋㅋ",
          "original_form": "스트레스 받지말고 일하고 오자ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009228.1.8",
          "form": "사람대하는게 제일힘듬",
          "original_form": "사람대하는게 제일힘듬",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009228.1.9",
          "form": "그러니깐요",
          "original_form": "그러니깐요",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009228.1.10",
          "form": "언니 일도 어려워보이진않았지만 사람 상대하는일이니 어려움이 많겠다싶더라구요.",
          "original_form": "언니 일도 어려워보이진않았지만 사람 상대하는일이니 어려움이 많겠다싶더라구요.",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009228.1.11",
          "form": "진짜 사람대하는게 제일 힘든것같아요",
          "original_form": "진짜 사람대하는게 제일 힘든것같아요",
          "speaker_id": "1",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009228.1.12",
          "form": "월요일이네ㅋ",
          "original_form": "월요일이네ㅋ",
          "speaker_id": "2",
          "time": "20210823 07:55"
        },
        {
          "id": "MDRW2100009228.1.13",
          "form": "한주의 시작이구먼ㅋㅋ",
          "original_form": "한주의 시작이구먼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 07:55"
        },
        {
          "id": "MDRW2100009228.1.14",
          "form": "이번주 업무일지안써서 지금쓰고있음ㅡㅡ",
          "original_form": "이번주 업무일지안써서 지금쓰고있음ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 07:58"
        },
        {
          "id": "MDRW2100009228.1.15",
          "form": "아하ㅜ일찍부터 쓰고있네요",
          "original_form": "아하ㅜ일찍부터 쓰고있네요",
          "speaker_id": "1",
          "time": "20210823 08:37"
        },
        {
          "id": "MDRW2100009228.1.16",
          "form": "저는 name3이데려다주고 이제 name4준비시키고 일가려구요ㅋ",
          "original_form": "저는 &name3&이데려다주고 이제 &name4&준비시키고 일가려구요ㅋ",
          "speaker_id": "1",
          "time": "20210823 08:38"
        },
        {
          "id": "MDRW2100009228.1.17",
          "form": "난 도착",
          "original_form": "난 도착",
          "speaker_id": "2",
