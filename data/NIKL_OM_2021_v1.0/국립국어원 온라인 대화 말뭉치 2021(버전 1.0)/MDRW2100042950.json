      "utterance": [
        {
          "id": "MDRW2100042950.1.1",
          "form": "반려동물키우시나요?",
          "original_form": "반려동물키우시나요?",
          "speaker_id": "1",
          "time": "20210907 11:56"
        },
        {
          "id": "MDRW2100042950.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210907 11:57"
        },
        {
          "id": "MDRW2100042950.1.3",
          "form": "키우시나요",
          "original_form": "키우시나요",
          "speaker_id": "2",
          "time": "20210907 11:57"
        },
        {
          "id": "MDRW2100042950.1.4",
          "form": "네! 저는 강아지 키워요",
          "original_form": "네! 저는 강아지 키워요",
          "speaker_id": "1",
          "time": "20210907 11:58"
        },
        {
          "id": "MDRW2100042950.1.5",
          "form": "포메라니안이고",
          "original_form": "포메라니안이고",
          "speaker_id": "1",
          "time": "20210907 11:58"
        },
        {
          "id": "MDRW2100042950.1.6",
          "form": "색은 갈ㅡ색입니다",
          "original_form": "색은 갈ㅡ색입니다",
          "speaker_id": "1",
          "time": "20210907 11:58"
        },
        {
          "id": "MDRW2100042950.1.7",
          "form": "이름은 name3이에요ㅎㅎ",
          "original_form": "이름은 &name3&이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:58"
        },
        {
          "id": "MDRW2100042950.1.8",
          "form": "이름이 참 귀엽네요",
          "original_form": "이름이 참 귀엽네요",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042950.1.9",
          "form": "한마리 키우시나요",
          "original_form": "한마리 키우시나요",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042950.1.10",
          "form": "넹 저희집에서 엄마가",
          "original_form": "넹 저희집에서 엄마가",
          "speaker_id": "1",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042950.1.11",
          "form": "키우고싶으면 돈전부 스스로벌어서 키워라하셔서",
          "original_form": "키우고싶으면 돈전부 스스로벌어서 키워라하셔서",
          "speaker_id": "1",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042950.1.12",
          "form": "ㅎㅎㅠ한마리 잘키우는게 제 목표에요",
          "original_form": "ㅎㅎㅠ한마리 잘키우는게 제 목표에요",
          "speaker_id": "1",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042950.1.13",
          "form": "강아지 좋아하시나요?",
          "original_form": "강아지 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042950.1.14",
          "form": "아기 키우는것 처럼 손이 많이 가죠",
          "original_form": "아기 키우는것 처럼 손이 많이 가죠",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042950.1.15",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210907 12:07"
        },
        {
          "id": "MDRW2100042950.1.16",
          "form": "저는 아직 아기를 못키워봤지만 그말에 공감해요ㅎㅎㅠ",
          "original_form": "저는 아직 아기를 못키워봤지만 그말에 공감해요ㅎㅎㅠ",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042950.1.17",
          "form": "매일산책시키고",
          "original_form": "매일산책시키고",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042950.1.18",
          "form": "사료도 잘먹게해주고",
          "original_form": "사료도 잘먹게해주고",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042950.1.19",
          "form": "씻겨주고, .",
          "original_form": "씻겨주고, .",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042950.1.20",
          "form": "하루가 바쁘네용ㅠ",
          "original_form": "하루가 바쁘네용ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042950.1.21",
          "form": "케어 하다보면 하루가 다가겠죠",
          "original_form": "케어 하다보면 하루가 다가겠죠",
          "speaker_id": "2",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042950.1.22",
          "form": "ㅠㅠ맞아요",
          "original_form": "ㅠㅠ맞아요",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042950.1.23",
          "form": "근데또",
          "original_form": "근데또",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042950.1.24",
          "form": "name3이가 귀여우니까",
          "original_form": "&name3&이가 귀여우니까",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042950.1.25",
          "form": "다른 반려는 키우고 싶은건 없나요",
          "original_form": "다른 반려는 키우고 싶은건 없나요",
          "speaker_id": "2",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042950.1.26",
          "form": "저 강아지입양전에는",
          "original_form": "저 강아지입양전에는",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042950.1.27",
          "form": "햄스터 생각했었는데",
          "original_form": "햄스터 생각했었는데",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042950.1.28",
          "form": "케이지에 넣고 키우면 스트레스 많이 받는다고해서ㅠㅠ",
          "original_form": "케이지에 넣고 키우면 스트레스 많이 받는다고해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042950.1.29",
          "form": "name2님은 키우고싶은 반려동물 없나요?",
          "original_form": "&name2&님은 키우고싶은 반려동물 없나요?",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042950.1.30",
          "form": "그렇겠네요",
          "original_form": "그렇겠네요",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100042950.1.31",
          "form": "참 쉬운게 없네요",
          "original_form": "참 쉬운게 없네요",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100042950.1.32",
          "form": "햄스터 귀엽긴 하죠",
          "original_form": "햄스터 귀엽긴 하죠",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100042950.1.33",
          "form": "햄스터 정말 작고 귀여운거같아요",
          "original_form": "햄스터 정말 작고 귀여운거같아요",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100042950.1.34",
          "form": "햄스터표정보고 '뵤'라고 하는것도",
          "original_form": "햄스터표정보고 '뵤'라고 하는것도",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100042950.1.35",
          "form": "귀여운거같아요",
          "original_form": "귀여운거같아요",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100042950.1.36",
          "form": "작고 귀엽고 물레방아 돌리는것도 이쁘죠",
          "original_form": "작고 귀엽고 물레방아 돌리는것도 이쁘죠",
          "speaker_id": "2",
          "time": "20210908 10:10"
        }
      ]
