      "utterance": [
        {
          "id": "MDRW2100043179.1.1",
          "form": "제주도좋나요",
          "original_form": "제주도좋나요",
          "speaker_id": "1",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100043179.1.2",
          "form": "금방 육지에서 돌아왔는데",
          "original_form": "금방 육지에서 돌아왔는데",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100043179.1.3",
          "form": "좋으네요 ㅎㅎ",
          "original_form": "좋으네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043179.1.4",
          "form": "ㅎㅎㅎ좋을것같아요",
          "original_form": "ㅎㅎㅎ좋을것같아요",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043179.1.5",
          "form": "확실히 더 덥네요",
          "original_form": "확실히 더 덥네요",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043179.1.6",
          "form": "부산에서 왔는데",
          "original_form": "부산에서 왔는데",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043179.1.7",
          "form": "바로 셔츠 벗었어요",
          "original_form": "바로 셔츠 벗었어요",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043179.1.8",
          "form": "와그런정도에요?",
          "original_form": "와그런정도에요?",
          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043179.1.9",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043179.1.10",
          "form": "네 온습도가 다 높네요",
          "original_form": "네 온습도가 다 높네요",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100043179.1.11",
          "form": "이제 오름 갈라했는데",
          "original_form": "이제 오름 갈라했는데",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100043179.1.12",
          "form": "낼은 해변가야 겠어요",
          "original_form": "낼은 해변가야 겠어요",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100043179.1.13",
          "form": "서쪽에계시나요 동쪽에계시나요",
          "original_form": "서쪽에계시나요 동쪽에계시나요",
          "speaker_id": "1",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043179.1.14",
          "form": "저는 제주시고 동쪽이예요 시라봉근처",
          "original_form": "저는 제주시고 동쪽이예요 시라봉근처",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043179.1.15",
          "form": "함덕가야죠",
          "original_form": "함덕가야죠",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043179.1.16",
          "form": "ㅎㅎ함덕좋죠",
          "original_form": "ㅎㅎ함덕좋죠",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043179.1.17",
          "form": "바다이쁘잖아요",
          "original_form": "바다이쁘잖아요",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043179.1.18",
          "form": "협재나 우도도 좋지만 버스가 한번에 가야 ㅎㅎ",
          "original_form": "협재나 우도도 좋지만 버스가 한번에 가야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043179.1.19",
          "form": "ㅎㅎ협재저는제일좋아해요",
          "original_form": "ㅎㅎ협재저는제일좋아해요",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043179.1.20",
          "form": "색깔이 탑이죠",
          "original_form": "색깔이 탑이죠",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043179.1.21",
          "form": "그맑은느낌ㅎㅎ",
          "original_form": "그맑은느낌ㅎㅎ",
