      "utterance": [
        {
          "id": "MDRW2100020090.1.1",
          "form": "진심으로",
          "original_form": "진심으로",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020090.1.2",
          "form": "누구 좋아하는 게 죄도 아니고",
          "original_form": "누구 좋아하는 게 죄도 아니고",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020090.1.3",
          "form": "정말 감사합니다..",
          "original_form": "정말 감사합니다..",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020090.1.4",
          "form": "그 마음에 맞춰주려고 한 것도 죄도 아니구요",
          "original_form": "그 마음에 맞춰주려고 한 것도 죄도 아니구요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020090.1.5",
          "form": "저는 못좋아하는게 죄처럼 느껴졌었거든요",
          "original_form": "저는 못좋아하는게 죄처럼 느껴졌었거든요",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020090.1.6",
          "form": "한풀이를 하고있네욬ㅋㅋㅋㅋ",
          "original_form": "한풀이를 하고있네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020090.1.7",
          "form": "약간 그런거 있잖아유",
          "original_form": "약간 그런거 있잖아유",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020090.1.8",
          "form": "사랑이 절대적인 가치라고 미디어에서 염불 외니까",
          "original_form": "사랑이 절대적인 가치라고 미디어에서 염불 외니까",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020090.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020090.1.10",
          "form": "사랑을 거절하는/못 받아주는 사람은 못된 사람이라고",
          "original_form": "사랑을 거절하는/못 받아주는 사람은 못된 사람이라고",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020090.1.11",
          "form": "몰아가는 그런 분위기 있어요",
          "original_form": "몰아가는 그런 분위기 있어요",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020090.1.12",
          "form": "ㅜㅜ 아 그런가요 저는 스스로 그렇게 생각했는데",
          "original_form": "ㅜㅜ 아 그런가요 저는 스스로 그렇게 생각했는데",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020090.1.13",
          "form": "근데 내 마음 내 호불호는 나한테 달린 거지",
          "original_form": "근데 내 마음 내 호불호는 나한테 달린 거지",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020090.1.14",
          "form": "아이 물론 사랑이 기적같은 일을 일으킬 때가 있어요!",
          "original_form": "아이 물론 사랑이 기적같은 일을 일으킬 때가 있어요!",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020090.1.15",
          "form": "그렇더라구요 마음이 이성으로 조절되지않더라구요",
          "original_form": "그렇더라구요 마음이 이성으로 조절되지않더라구요",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020090.1.16",
          "form": "하지만 그걸 나누는 게 의무는 아니니께..",
          "original_form": "하지만 그걸 나누는 게 의무는 아니니께..",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020090.1.17",
          "form": "사랑을 주거나 받는 거 둘 다",
          "original_form": "사랑을 주거나 받는 거 둘 다",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020090.1.18",
          "form": "의무가 아니니까 너무 걱정 마세요!",
          "original_form": "의무가 아니니까 너무 걱정 마세요!",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020090.1.19",
          "form": "제가 제때 거절하지 못한게 너무 미안했는데",
          "original_form": "제가 제때 거절하지 못한게 너무 미안했는데",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020090.1.20",
          "form": "서툴었다고 생각해야겠어요 ㅎㅎ ㅜ",
          "original_form": "서툴었다고 생각해야겠어요 ㅎㅎ ㅜ",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020090.1.21",
          "form": "맞아요 살다 보면 이런저런 시행착오를 거치는거죠",
          "original_form": "맞아요 살다 보면 이런저런 시행착오를 거치는거죠",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020090.1.22",
          "form": "ㅇㅑ밤에 상담을ㅋㅋㅋㅋㅋ 감사해요",
          "original_form": "ㅇㅑ밤에 상담을ㅋㅋㅋㅋㅋ 감사해요",
