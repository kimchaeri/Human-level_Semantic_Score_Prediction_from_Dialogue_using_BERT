      "utterance": [
        {
          "id": "MDRW2100021932.1.1",
          "form": "감사합니다 꼭 가볼게요 ㅎㅎ",
          "original_form": "감사합니다 꼭 가볼게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021932.1.2",
          "form": "동호회 활동 하시나요? ",
          "original_form": "동호회 활동 하시나요? ",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100021932.1.3",
          "form": "아니욥~ 학교다닐땐 했는데 지금은 따로 활동하는건 없어요",
          "original_form": "아니욥~ 학교다닐땐 했는데 지금은 따로 활동하는건 없어요",
          "speaker_id": "2",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100021932.1.4",
          "form": "학교는 대학교 말씀이신가요?",
          "original_form": "학교는 대학교 말씀이신가요?",
          "speaker_id": "1",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100021932.1.5",
          "form": "어떤 동호회 하셨어요? ㅎㅎ",
          "original_form": "어떤 동호회 하셨어요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100021932.1.6",
          "form": "네 맞아요~ 봉사동아리랑 신문학회요",
          "original_form": "네 맞아요~ 봉사동아리랑 신문학회요",
          "speaker_id": "2",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100021932.1.7",
          "form": "와 멋진데요ㅋㅋ 따뜻한 지식인 느낌",
          "original_form": "와 멋진데요ㅋㅋ 따뜻한 지식인 느낌",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100021932.1.8",
          "form": "어떤 게 더 재미있으셨어요?",
          "original_form": "어떤 게 더 재미있으셨어요?",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100021932.1.9",
          "form": "흠 신문학회요~ 엄청 재밋게 놀러다녔거든요 방학마다",
          "original_form": "흠 신문학회요~ 엄청 재밋게 놀러다녔거든요 방학마다",
          "speaker_id": "2",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100021932.1.10",
          "form": "신문 학회에서 놀러 다닐 일이 있나요? 취재하러 가신걸까요",
          "original_form": "신문 학회에서 놀러 다닐 일이 있나요? 취재하러 가신걸까요",
          "speaker_id": "1",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100021932.1.11",
          "form": "방학때마다 다같이 갔어요~^^ 년에 두번정도네요",
          "original_form": "방학때마다 다같이 갔어요~^^ 년에 두번정도네요",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100021932.1.12",
          "form": "학교앞에서도 놀고ㅋㅋㅋ 물론 모여서 기사도 쓰고 토론도 했지만 끝나면 다같이 한잔하러가고 그게좋았네요",
          "original_form": "학교앞에서도 놀고ㅋㅋㅋ 물론 모여서 기사도 쓰고 토론도 했지만 끝나면 다같이 한잔하러가고 그게좋았네요",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100021932.1.13",
          "form": "와ㅋㅋㅋㅋ 왼잔 대학 로망의 집합체같은 추억인데요 ㅎㅎㅎ",
          "original_form": "와ㅋㅋㅋㅋ 왼잔 대학 로망의 집합체같은 추억인데요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100021932.1.14",
          "form": "봉사동아리도 나름 재밋엇어요 주말마다 가서 봉사하고 끝나고 회식?하고",
          "original_form": "봉사동아리도 나름 재밋엇어요 주말마다 가서 봉사하고 끝나고 회식?하고",
          "speaker_id": "2",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100021932.1.15",
          "form": "봉사는 어디로 가셨어요?",
          "original_form": "봉사는 어디로 가셨어요?",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100021932.1.16",
          "form": "장애인시설에가서 했어요~",
          "original_form": "장애인시설에가서 했어요~",
          "speaker_id": "2",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100021932.1.17",
          "form": "오~ 어떤 활동 하셨나요?",
          "original_form": "오~ 어떤 활동 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100021932.1.18",
          "form": "가서 식사도 돕고 활동시간에 같이 놀기도 하구 근방에 산책도 갔었어요",
          "original_form": "가서 식사도 돕고 활동시간에 같이 놀기도 하구 근방에 산책도 갔었어요",
          "speaker_id": "2",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100021932.1.19",
          "form": "오 멋져요... 의미있는 활동 하셨군요!",
          "original_form": "오 멋져요... 의미있는 활동 하셨군요!",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100021932.1.20",
          "form": "그런 동아리 하면 같이 한 사람들이랑도 더 돈독해지겠어요",
          "original_form": "그런 동아리 하면 같이 한 사람들이랑도 더 돈독해지겠어요",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100021932.1.21",
          "form": "매주주말마다갔고 끝나면 항상다같이 사진찍고~ 저녁에 한잔하러갔었네요ㅋㅋ",
          "original_form": "매주주말마다갔고 끝나면 항상다같이 사진찍고~ 저녁에 한잔하러갔었네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:55"
        }
      ]
