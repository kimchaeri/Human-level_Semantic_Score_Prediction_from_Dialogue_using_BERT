      "utterance": [
        {
          "id": "MDRW2100032552.1.1",
          "form": "식사는 하셨어요?",
          "original_form": "식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032552.1.2",
          "form": "넵넵 두부김치에 밥이용",
          "original_form": "넵넵 두부김치에 밥이용",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032552.1.3",
          "form": "두부김치 맛있죠ㅎㅎㅎ",
          "original_form": "두부김치 맛있죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032552.1.4",
          "form": "요리직접하세요?",
          "original_form": "요리직접하세요?",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032552.1.5",
          "form": "아뇽",
          "original_form": "아뇽",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032552.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032552.1.7",
          "form": "두부를 시장가서 사오긴했어요",
          "original_form": "두부를 시장가서 사오긴했어요",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032552.1.8",
          "form": "그냥 그두부 썰어서 김치꺼내서 밥에 먹었죠",
          "original_form": "그냥 그두부 썰어서 김치꺼내서 밥에 먹었죠",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032552.1.9",
          "form": "ㅋㅋㅋㅋ생두부인가요",
          "original_form": "ㅋㅋㅋㅋ생두부인가요",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032552.1.10",
          "form": "생두부 맞을걸요?ㅋㅋㅋ아닌강ㅋㅋㅋ 그냥 먹었어요",
          "original_form": "생두부 맞을걸요?ㅋㅋㅋ아닌강ㅋㅋㅋ 그냥 먹었어요",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032552.1.11",
          "form": "어렸을때부터",
          "original_form": "어렸을때부터",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032552.1.12",
          "form": "생두부먹어도상관없을꺼에요!",
          "original_form": "생두부먹어도상관없을꺼에요!",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032552.1.13",
          "form": "커피도드세요?",
          "original_form": "커피도드세요?",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032552.1.14",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100032552.1.15",
          "form": "카페인은 살려고 마셔성 ㅋㅋㅋ 평소에는 디카페인 먹으려고해요",
          "original_form": "카페인은 살려고 마셔성 ㅋㅋㅋ 평소에는 디카페인 먹으려고해요",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100032552.1.16",
          "form": "밤샐때나 피곤할때 카페인 먹어요",
          "original_form": "밤샐때나 피곤할때 카페인 먹어요",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100032552.1.17",
          "form": "앗 마시면 두근거리세요?",
          "original_form": "앗 마시면 두근거리세요?",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100032552.1.18",
          "form": "아 아뇨 근데 밤샐때 권장량 그이상 마셔섴ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 아뇨 근데 밤샐때 권장량 그이상 마셔섴ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100032552.1.19",
          "form": "나름 건강관리에요",
          "original_form": "나름 건강관리에요",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100032552.1.20",
          "form": "평소에는 안먹는걸루",
          "original_form": "평소에는 안먹는걸루",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100032552.1.21",
          "form": "밤을새는것",
          "original_form": "밤을새는것",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100032552.1.22",
          "form": "하유힘들죠",
          "original_form": "하유힘들죠",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100032552.1.23",
          "form": "이젠못하겠어요ㅠ",
          "original_form": "이젠못하겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100032552.1.24",
          "form": "디저트도좋아하세요?",
          "original_form": "디저트도좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100032552.1.25",
          "form": "넵넵 민초 좋아해여",
          "original_form": "넵넵 민초 좋아해여",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100032552.1.26",
