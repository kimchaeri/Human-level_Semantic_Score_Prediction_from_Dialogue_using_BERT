      "utterance": [
        {
          "id": "MDRW2100019516.1.1",
          "form": "여행 가고 싶으신 곳 있나요",
          "original_form": "여행 가고 싶으신 곳 있나요",
          "speaker_id": "1",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019516.1.2",
          "form": "가고 싶은 곳은 많은데",
          "original_form": "가고 싶은 곳은 많은데",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019516.1.3",
          "form": "가본 곳은 없네요",
          "original_form": "가본 곳은 없네요",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019516.1.4",
          "form": "왜 없나요",
          "original_form": "왜 없나요",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019516.1.5",
          "form": "저도 가고 싶어요",
          "original_form": "저도 가고 싶어요",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019516.1.6",
          "form": "부산에 이번에 곧있으면",
          "original_form": "부산에 이번에 곧있으면",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019516.1.7",
          "form": "롯데월드가 열려요",
          "original_form": "롯데월드가 열려요",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019516.1.8",
          "form": "알고있지만 분명 미어터질거예요",
          "original_form": "알고있지만 분명 미어터질거예요",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019516.1.9",
          "form": "그래서 당분간 갈 생각이 없어요",
          "original_form": "그래서 당분간 갈 생각이 없어요",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019516.1.10",
          "form": "슬프네요",
          "original_form": "슬프네요",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100019516.1.11",
          "form": "곧이에요",
          "original_form": "곧이에요",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100019516.1.12",
          "form": "9월에 열려요",
          "original_form": "9월에 열려요",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100019516.1.13",
          "form": "허미 사람 터져나가겠네",
          "original_form": "허미 사람 터져나가겠네",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100019516.1.14",
          "form": "거긴 지금 4단계 아녀",
          "original_form": "거긴 지금 4단계 아녀",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100019516.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100019516.1.16",
          "form": "해외가고 싶어요",
          "original_form": "해외가고 싶어요",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100019516.1.17",
          "form": "해외 좋지...먼 과거에 가본 적 있는데...",
          "original_form": "해외 좋지...먼 과거에 가본 적 있는데...",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100019516.1.18",
          "form": "즐거웠는데...",
          "original_form": "즐거웠는데...",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100019516.1.19",
          "form": "지금은 왜...",
          "original_form": "지금은 왜...",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100019516.1.20",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100019516.1.21",
          "form": "국내여행도",
          "original_form": "국내여행도",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100019516.1.22",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100019516.1.23",
          "form": "같은 시내 돌아다니는 것도 고민해야하는게",
          "original_form": "같은 시내 돌아다니는 것도 고민해야하는게",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100019516.1.24",
          "form": "너무 슬퍼",
          "original_form": "너무 슬퍼",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100019516.1.25",
          "form": "마스크 안쓰고 싶은데 일할 땐 쓰는게 좋긴해요",
          "original_form": "마스크 안쓰고 싶은데 일할 땐 쓰는게 좋긴해요",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100019516.1.26",
          "form": "얼굴 신경안써도 돼서",
          "original_form": "얼굴 신경안써도 돼서",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100019516.1.27",
          "form": "맞아ㅋㅋㅋㅋ마스크에 안경 쓰면 준비 끝남",
          "original_form": "맞아ㅋㅋㅋㅋ마스크에 안경 쓰면 준비 끝남",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100019516.1.28",
          "form": "그건 편한..게 아니야!",
          "original_form": "그건 편한..게 아니야!",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100019516.1.29",
          "form": "집에서도 마스크 써야하나 고민한다고!",
          "original_form": "집에서도 마스크 써야하나 고민한다고!",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100019516.1.30",
          "form": "집에서는 왜 쓰나요...?",
          "original_form": "집에서는 왜 쓰나요...?",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100019516.1.31",
          "form": "전 안싸요",
          "original_form": "전 안싸요",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100019516.1.32",
          "form": "남동생이 출장가잖아요",
          "original_form": "남동생이 출장가잖아요",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100019516.1.33",
          "form": "집에 들여보내질 마세요",
          "original_form": "집에 들여보내질 마세요",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100019516.1.34",
          "form": "어떤가요",
          "original_form": "어떤가요",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100019516.1.35",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100019516.1.36",
          "form": "아주 좋아요",
          "original_form": "아주 좋아요",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100019516.1.37",
          "form": "쫓아내야겠어요",
          "original_form": "쫓아내야겠어요",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100019516.1.38",
          "form": "좋은 생각이에요",
          "original_form": "좋은 생각이에요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100019516.1.39",
          "form": "전 꼭 해외를 갈거에요",
          "original_form": "전 꼭 해외를 갈거에요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100019516.1.40",
          "form": "기다리세요",
          "original_form": "기다리세요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100019516.1.41",
          "form": "매직마이크쇼",
          "original_form": "매직마이크쇼",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100019516.1.42",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100019516.1.43",
          "form": "롸뇨",
          "original_form": "롸뇨",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100019516.1.44",
          "form": "모롸요",
          "original_form": "모롸요",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100019516.1.45",
          "form": "뭐죠",
          "original_form": "뭐죠",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100019516.1.46",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100019516.1.47",
          "form": "핫한거 있는데...",
          "original_form": "핫한거 있는데...",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100019516.1.48",
          "form": "알려주셔야죠 호호 궁금하네요",
          "original_form": "알려주셔야죠 호호 궁금하네요",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100019516.1.49",
          "form": "유튜브로 함 보세요",
          "original_form": "유튜브로 함 보세요",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100019516.1.50",
          "form": "추천드립니다",
          "original_form": "추천드립니다",
          "speaker_id": "1",
          "time": "20210901 21:57"
        }
      ]
    }
  ]
}
