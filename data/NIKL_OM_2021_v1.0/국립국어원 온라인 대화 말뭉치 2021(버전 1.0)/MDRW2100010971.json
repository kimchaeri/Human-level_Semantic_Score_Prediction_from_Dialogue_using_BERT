      "utterance": [
        {
          "id": "MDRW2100010971.1.1",
          "form": "커피알못이지만 그 힐링공간이 좋아요ㅎ",
          "original_form": "커피알못이지만 그 힐링공간이 좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010971.1.2",
          "form": "점심에 간단히 카페식 먹으며 죽쳐요",
          "original_form": "점심에 간단히 카페식 먹으며 죽쳐요",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100010971.1.3",
          "form": "어느 카페 좋아하세요?",
          "original_form": "어느 카페 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100010971.1.4",
          "form": "오 저도 사실 커피를 막 잘 모르겠더라구요",
          "original_form": "오 저도 사실 커피를 막 잘 모르겠더라구요",
          "speaker_id": "1",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100010971.1.5",
          "form": "전에는 인스타감성카페 많이갔는데",
          "original_form": "전에는 인스타감성카페 많이갔는데",
          "speaker_id": "1",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100010971.1.6",
          "form": "실패한 적이 많아서....",
          "original_form": "실패한 적이 많아서....",
          "speaker_id": "1",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010971.1.7",
          "form": "기프티콘 쓸 겸 스타벅스 젤 많이 가요 ㅋㅋㅋㅋㅋ",
          "original_form": "기프티콘 쓸 겸 스타벅스 젤 많이 가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010971.1.8",
          "form": "ㅎㅎ 그런데는 갬성으로 가는 거라",
          "original_form": "ㅎㅎ 그런데는 갬성으로 가는 거라",
          "speaker_id": "2",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010971.1.9",
          "form": "저도 스벅이 제일 만만해요",
          "original_form": "저도 스벅이 제일 만만해요",
          "speaker_id": "2",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010971.1.10",
          "form": "엑스트라도 무료고 공간도 편하고",
          "original_form": "엑스트라도 무료고 공간도 편하고",
          "speaker_id": "2",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010971.1.11",
          "form": "주로 무슨 음료 드세요?",
          "original_form": "주로 무슨 음료 드세요?",
          "speaker_id": "2",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010971.1.12",
          "form": "자허블 젤 많이먹고",
          "original_form": "자허블 젤 많이먹고",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010971.1.13",
          "form": "바크콜.. 좋아해요 바닐라크림콜드브루",
          "original_form": "바크콜.. 좋아해요 바닐라크림콜드브루",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010971.1.14",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010971.1.15",
          "form": "저는 아메리카노에 휘핑 올려 먹어요",
          "original_form": "저는 아메리카노에 휘핑 올려 먹어요",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010971.1.16",
          "form": "핫이든 콜드든 할것 없이ㅎ",
          "original_form": "핫이든 콜드든 할것 없이ㅎ",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010971.1.17",
          "form": "우와 맛있나요?",
          "original_form": "우와 맛있나요?",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010971.1.18",
          "form": "그냥 휘핑?? 에쏘휩??",
          "original_form": "그냥 휘핑?? 에쏘휩??",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010971.1.19",
          "form": "에쏘휩보다 그냥 휩이요!",
          "original_form": "에쏘휩보다 그냥 휩이요!",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010971.1.20",
          "form": "스벅은 휩이 맛있어서",
          "original_form": "스벅은 휩이 맛있어서",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010971.1.21",
          "form": "핫으로 먹으면 나중엔 라떼 같아요",
          "original_form": "핫으로 먹으면 나중엔 라떼 같아요",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010971.1.22",
          "form": "쓰고 달고 고소해서 좋아해요",
          "original_form": "쓰고 달고 고소해서 좋아해요",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010971.1.23",
          "form": "바크콜은 당도가 어떤가요?",
          "original_form": "바크콜은 당도가 어떤가요?",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010971.1.24",
          "form": "단 거 잘 못먹는데 검색하니 맛나 보여요",
          "original_form": "단 거 잘 못먹는데 검색하니 맛나 보여요",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010971.1.25",
          "form": "음 약간 달아요",
          "original_form": "음 약간 달아요",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010971.1.26",
          "form": "그리고 돌체라떼처럼... 화장실에 가게되는 ..^^..",
          "original_form": "그리고 돌체라떼처럼... 화장실에 가게되는 ..^^..",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010971.1.27",
          "form": "전 바크콜에 에쏘휩 올려먹어요",
          "original_form": "전 바크콜에 에쏘휩 올려먹어요",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010971.1.28",
          "form": "아앗 어쩐지",
          "original_form": "아앗 어쩐지",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010971.1.29",
          "form": "전 예전에 리스트레토 비안코 좋아했어요",
          "original_form": "전 예전에 리스트레토 비안코 좋아했어요",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010971.1.30",
          "form": "근데 그것도 화장실 가기 좋더라고요ㅎ",
          "original_form": "근데 그것도 화장실 가기 좋더라고요ㅎ",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010971.1.31",
          "form": "고소하고 진한 음료들이 대체로 그런듯",
          "original_form": "고소하고 진한 음료들이 대체로 그런듯",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010971.1.32",
          "form": "우와 저 한번도 안 먹어봤는데",
          "original_form": "우와 저 한번도 안 먹어봤는데",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010971.1.33",
          "form": "이름이 뭔가 간지나네요",
          "original_form": "이름이 뭔가 간지나네요",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010971.1.34",
          "form": "ㅎㅎ 돌체라떼보다 진하고 고소해요",
          "original_form": "ㅎㅎ 돌체라떼보다 진하고 고소해요",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010971.1.35",
          "form": "근데 단종된 것이 함정ㅜ",
          "original_form": "근데 단종된 것이 함정ㅜ",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010971.1.36",
          "form": "내일은 한번 바크콜 도전해봐야겠어요",
          "original_form": "내일은 한번 바크콜 도전해봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010971.1.37",
          "form": "당도 살짝 조절은 되는 듯하니",
          "original_form": "당도 살짝 조절은 되는 듯하니",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010971.1.38",
          "form": "바크콜의 세계로,, 초대합니다",
          "original_form": "바크콜의 세계로,, 초대합니다",
          "speaker_id": "1",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100010971.1.39",
          "form": "커피 말고 다른 메뉴도 드세요?",
          "original_form": "커피 말고 다른 메뉴도 드세요?",
          "speaker_id": "1",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100010971.1.40",
          "form": "저는 자허블 말고 자몽피지오 파예요ㅎ",
          "original_form": "저는 자허블 말고 자몽피지오 파예요ㅎ",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100010971.1.41",
          "form": "근데 요새는 저 가는 스벅에 안보여서 단종됐을까봐 조마조마ㅜ",
          "original_form": "근데 요새는 저 가는 스벅에 안보여서 단종됐을까봐 조마조마ㅜ",
          "speaker_id": "2",
