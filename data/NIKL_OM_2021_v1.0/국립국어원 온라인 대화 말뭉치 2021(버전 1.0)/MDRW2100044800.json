          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044800.1.5",
          "form": "서레기들",
          "original_form": "서레기들",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044800.1.6",
          "form": "그딴 채널 봐주고",
          "original_form": "그딴 채널 봐주고",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044800.1.7",
          "form": "무슨 소리 해도 진리인 양 떠받들어주니까",
          "original_form": "무슨 소리 해도 진리인 양 떠받들어주니까",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044800.1.8",
          "form": "가세연은 슈퍼챗이라도",
          "original_form": "가세연은 슈퍼챗이라도",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.9",
          "form": "더 저렇게 된거겠지",
          "original_form": "더 저렇게 된거겠지",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.10",
          "form": "많이벌지",
          "original_form": "많이벌지",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.11",
          "form": "김용호는 많이벌지도못하는 모양이던데",
          "original_form": "김용호는 많이벌지도못하는 모양이던데",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.14",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.15",
          "form": "관종인듯",
          "original_form": "관종인듯",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.16",
          "form": "50억 어디서 번담",
          "original_form": "50억 어디서 번담",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.17",
          "form": "김건모는 시작이고",
          "original_form": "김건모는 시작이고",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.18",
          "form": "나머진ㄴ 줄줄히 소세지일텐디",
          "original_form": "나머진ㄴ 줄줄히 소세지일텐디",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 꼴좋다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 꼴좋다",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.20",
          "form": "저놈들도 망하네?",
          "original_form": "저놈들도 망하네?",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.21",
          "form": "그래도 존버하면 망하긴 하나보바",
          "original_form": "그래도 존버하면 망하긴 하나보바",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044800.1.22",
          "form": "안준영도 망한거보면",
          "original_form": "안준영도 망한거보면",
          "speaker_id": "2",
