        },
        {
          "id": "MDRW2100020101.1.2",
          "form": "트로트 좋아하니",
          "original_form": "트로트 좋아하니",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020101.1.3",
          "form": "나 미스터트롯 재밌게 봤어 트로트",
          "original_form": "나 미스터트롯 재밌게 봤어 트로트",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020101.1.4",
          "form": "나는 원래는 트로트 싫어했어",
          "original_form": "나는 원래는 트로트 싫어했어",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020101.1.5",
          "form": "지금은 좋아해?",
          "original_form": "지금은 좋아해?",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020101.1.6",
          "form": "모아 하는 트로트 가수가 생겼",
          "original_form": "모아 하는 트로트 가수가 생겼",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020101.1.7",
          "form": "좋아하는",
          "original_form": "좋아하는",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020101.1.8",
          "form": "오 누구야",
          "original_form": "오 누구야",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020101.1.9",
          "form": "정동원이라고 알아",
          "original_form": "정동원이라고 알아",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020101.1.10",
          "form": "동원이 알지 천재잖아",
          "original_form": "동원이 알지 천재잖아",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020101.1.11",
          "form": "노래를 정말 잘하고 또 귀엽기까지 해",
          "original_form": "노래를 정말 잘하고 또 귀엽기까지 해",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020101.1.12",
          "form": "노래가 마음을 울리더라",
          "original_form": "노래가 마음을 울리더라",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020101.1.13",
          "form": "얼굴도 꽤 잘생기지 않았니",
          "original_form": "얼굴도 꽤 잘생기지 않았니",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020101.1.14",
          "form": "엄청 잘생김",
          "original_form": "엄청 잘생김",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020101.1.15",
          "form": "동원 이중 노래 중에 추천해 줄게",
          "original_form": "동원 이중 노래 중에 추천해 줄게",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020101.1.16",
          "form": "어떤 노래 제일 좋아해",
          "original_form": "어떤 노래 제일 좋아해",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020101.1.17",
          "form": "여백이라는 노래 꼭 들어봐",
