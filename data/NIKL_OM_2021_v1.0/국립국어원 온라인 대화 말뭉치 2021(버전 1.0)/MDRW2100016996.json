      "utterance": [
        {
          "id": "MDRW2100016996.1.1",
          "form": "뭔가 음식얘기가 제일 재밌는 거 같아요",
          "original_form": "뭔가 음식얘기가 제일 재밌는 거 같아요",
          "speaker_id": "1",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016996.1.2",
          "form": "딴거 이제 할 이야기가 없어요ㅋㅋㅋㅋㅋ",
          "original_form": "딴거 이제 할 이야기가 없어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016996.1.3",
          "form": "아 누님 식당 알바 해본적 있어요?",
          "original_form": "아 누님 식당 알바 해본적 있어요?",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100016996.1.4",
          "form": "있어요ㅋㅋ 남치니 식당서",
          "original_form": "있어요ㅋㅋ 남치니 식당서",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100016996.1.5",
          "form": "앗... 거기서 사랑이 싹튼건가요",
          "original_form": "앗... 거기서 사랑이 싹튼건가요",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100016996.1.6",
          "form": "아뇨ㅋㅋ사귀고나서 도와줬어요ㅋㅋ",
          "original_form": "아뇨ㅋㅋ사귀고나서 도와줬어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100016996.1.7",
          "form": "오... 서빙했어요?",
          "original_form": "오... 서빙했어요?",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100016996.1.8",
          "form": "넹ㅋㅋㅋ",
          "original_form": "넹ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100016996.1.9",
          "form": "예전에 전 술 파는 음식집에서 설거지했었는데",
          "original_form": "예전에 전 술 파는 음식집에서 설거지했었는데",
          "speaker_id": "2",
          "time": "20210827 11:05"
        },
        {
          "id": "MDRW2100016996.1.10",
          "form": "이것저것 주워먹게되지 않아요?",
          "original_form": "이것저것 주워먹게되지 않아요?",
          "speaker_id": "2",
          "time": "20210827 11:06"
        },
        {
          "id": "MDRW2100016996.1.11",
          "form": "손님이 손도 안댄거",
          "original_form": "손님이 손도 안댄거",
          "speaker_id": "2",
          "time": "20210827 11:06"
        },
        {
          "id": "MDRW2100016996.1.12",
          "form": "...?전 아니었는데ㅋㅋㅋㅋㅋ",
          "original_form": "...?전 아니었는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100016996.1.13",
          "form": "점심장사 때 한거라...",
          "original_form": "점심장사 때 한거라...",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100016996.1.14",
          "form": "다들 벤또시켰었어요",
          "original_form": "다들 벤또시켰었어요",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100016996.1.15",
          "form": "그렇게 말하면 내가 뭐가됩니깤ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇게 말하면 내가 뭐가됩니깤ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100016996.1.16",
          "form": "ㅋㅋㅋㅋㅋ아니 뭐 먹을 수도 있죠ㅋㅋㅋ아까우니까...ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ아니 뭐 먹을 수도 있죠ㅋㅋㅋ아까우니까...ㅜ",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100016996.1.17",
          "form": "주워먹는 쏘세지가 을매나 맛있는데ㅜㅜ",
          "original_form": "주워먹는 쏘세지가 을매나 맛있는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100016996.1.18",
          "form": "ㅋㅋㅋㅋㅋ쏘세지좋아해요?ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ쏘세지좋아해요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100016996.1.19",
          "form": "맥주에 쏘세지면 그날은 파티죠",
          "original_form": "맥주에 쏘세지면 그날은 파티죠",
          "speaker_id": "2",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100016996.1.20",
          "form": "ㅋㅋㅋㅋ맥주맛있나요...",
          "original_form": "ㅋㅋㅋㅋ맥주맛있나요...",
          "speaker_id": "1",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100016996.1.21",
          "form": "술 못마시는 1인이라...",
          "original_form": "술 못마시는 1인이라...",
          "speaker_id": "1",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100016996.1.22",
          "form": "맥주는 근데 저도",
          "original_form": "맥주는 근데 저도",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100016996.1.23",
          "form": "진짜 막 일하고 덥고 피곤할때 500 한캔 딱 마시는거 아니면",
          "original_form": "진짜 막 일하고 덥고 피곤할때 500 한캔 딱 마시는거 아니면",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100016996.1.24",
          "form": "잘 안마셔요",
          "original_form": "잘 안마셔요",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100016996.1.25",
          "form": "쏘세지 있을때 빼고",
          "original_form": "쏘세지 있을때 빼고",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100016996.1.26",
          "form": "ㅋㅋㅋㅋ치킨에 맥주 한 잔 마셨다가 얼굴 벌겋게 올라와서 취한 이후로는 맥주안마셔요...",
          "original_form": "ㅋㅋㅋㅋ치킨에 맥주 한 잔 마셨다가 얼굴 벌겋게 올라와서 취한 이후로는 맥주안마셔요...",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100016996.1.27",
          "form": "name2은 치맥보다 쏘맥인가여ㅋ",
          "original_form": "&name2&은 치맥보다 쏘맥인가여ㅋ",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100016996.1.28",
          "form": "알쓰시군요",
          "original_form": "알쓰시군요",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100016996.1.29",
          "form": "전 근데 위스키파...",
          "original_form": "전 근데 위스키파...",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100016996.1.30",
          "form": "쏘맥이라니까 깡술마시는것같네여ㅋㅋㅋㅋㅋ",
          "original_form": "쏘맥이라니까 깡술마시는것같네여ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100016996.1.31",
          "form": "노렸어요ㅋ",
          "original_form": "노렸어요ㅋ",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100016996.1.32",
          "form": "소주에 맥주가 아닌 쏘세지에 맥주ㅋ",
          "original_form": "소주에 맥주가 아닌 쏘세지에 맥주ㅋ",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100016996.1.33",
          "form": "요즘에는 논알콜 맥주도 있다지만",
          "original_form": "요즘에는 논알콜 맥주도 있다지만",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100016996.1.34",
          "form": "전 맥주 맛 모르겠어요",
          "original_form": "전 맥주 맛 모르겠어요",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100016996.1.35",
          "form": "몸 엄청 써서 엄청 피곤할 때 시원하게 한번 마셔봐요",
          "original_form": "몸 엄청 써서 엄청 피곤할 때 시원하게 한번 마셔봐요",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100016996.1.36",
          "form": "아... 이래서 맥주 맥주 하는구나 할걸요",
          "original_form": "아... 이래서 맥주 맥주 하는구나 할걸요",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100016996.1.37",
          "form": "전 그걸 첨 느낀게 고딩때 친구 부모님 과수원 일 도와드리고 느껴봤음",
          "original_form": "전 그걸 첨 느낀게 고딩때 친구 부모님 과수원 일 도와드리고 느껴봤음",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100016996.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋ힘들게 일하고 마셔야 느낄 수 있는 맥주의 맛",
          "original_form": "ㅋㅋㅋㅋㅋㅋ힘들게 일하고 마셔야 느낄 수 있는 맥주의 맛",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100016996.1.39",
          "form": "그래서 식사는 했나요",
          "original_form": "그래서 식사는 했나요",
