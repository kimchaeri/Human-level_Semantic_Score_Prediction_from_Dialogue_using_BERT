      "utterance": [
        {
          "id": "MDRW2100022654.1.1",
          "form": "안녕하세요! name2은 드라마 좋아하시나요?",
          "original_form": "안녕하세요! &name2&은 드라마 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022654.1.2",
          "form": "드라마 마지막으로 본게",
          "original_form": "드라마 마지막으로 본게",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022654.1.3",
          "form": "멜로가 체질",
          "original_form": "멜로가 체질",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022654.1.4",
          "form": "입니다ㅎㅎ",
          "original_form": "입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022654.1.5",
          "form": "name1은 드라마 좋아하시나요?",
          "original_form": "&name1&은 드라마 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022654.1.6",
          "form": "네!!! 전 스릴러 드라마 좋아해요!",
          "original_form": "네!!! 전 스릴러 드라마 좋아해요!",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022654.1.7",
          "form": "보이스나 비숲 같은!",
          "original_form": "보이스나 비숲 같은!",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022654.1.8",
          "form": "헉 저는 무서운걸",
          "original_form": "헉 저는 무서운걸",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022654.1.9",
          "form": "못봐요",
          "original_form": "못봐요",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022654.1.10",
          "form": "예능을 더많이봐요",
          "original_form": "예능을 더많이봐요",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022654.1.11",
          "form": "헛 그렇구나!",
          "original_form": "헛 그렇구나!",
          "speaker_id": "1",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100022654.1.12",
          "form": "예능 어던 프로 보세요?",
          "original_form": "예능 어던 프로 보세요?",
          "speaker_id": "1",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100022654.1.13",
          "form": "저는 런닝맨.노는언니.골때리는그녀들",
          "original_form": "저는 런닝맨.노는언니.골때리는그녀들",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022654.1.14",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022654.1.15",
          "form": "헐 저도 런닝맨 좋아해요!",
          "original_form": "헐 저도 런닝맨 좋아해요!",
          "speaker_id": "1",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022654.1.16",
          "form": "지금도 보고 있어요 ㅎㅎ",
          "original_form": "지금도 보고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022654.1.17",
          "form": "오늘 런닝맨 게스트 나오나요?!",
          "original_form": "오늘 런닝맨 게스트 나오나요?!",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022654.1.18",
          "form": "아니요! 오늘 공포특집이에요!!!!",
          "original_form": "아니요! 오늘 공포특집이에요!!!!",
          "speaker_id": "1",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022654.1.19",
          "form": "헉 게스트없는편이 더 재밌더라고요",
          "original_form": "헉 게스트없는편이 더 재밌더라고요",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022654.1.20",
          "form": "존잼각이네요",
          "original_form": "존잼각이네요",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022654.1.21",
          "form": "맞아요!!! 오늘 오랜만에 추리하게 되더라고요 ㅎㅎ",
          "original_form": "맞아요!!! 오늘 오랜만에 추리하게 되더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022654.1.22",
          "form": "런닝맨 최애 편이 뭐세요?!",
          "original_form": "런닝맨 최애 편이 뭐세요?!",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022654.1.23",
          "form": "아 최근회차는 다 너무재밌어서",
          "original_form": "아 최근회차는 다 너무재밌어서",
          "speaker_id": "2",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022654.1.24",
          "form": "계란맨인가 나오는 이상한마을 그것도재밌었어요",
          "original_form": "계란맨인가 나오는 이상한마을 그것도재밌었어요",
          "speaker_id": "2",
          "time": "20210829 18:21"
        },
        {
          "id": "MDRW2100022654.1.25",
          "form": "헐 맞아요!!!!!!!!!!!",
          "original_form": "헐 맞아요!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022654.1.26",
          "form": "그거 초반애 진짜 무섭지 않았나요",
          "original_form": "그거 초반애 진짜 무섭지 않았나요",
          "speaker_id": "1",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022654.1.27",
          "form": "저 허수아비 달려올 때 소리질렀어요",
          "original_form": "저 허수아비 달려올 때 소리질렀어요",
          "speaker_id": "1",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022654.1.28",
          "form": "맞아요! 그 회차 초반에 진짜 무섭잖아요 ㅠㅠ",
          "original_form": "맞아요! 그 회차 초반에 진짜 무섭잖아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022654.1.29",
          "form": "허수아비 달려올 때 너무 무서웠어요",
          "original_form": "허수아비 달려올 때 너무 무서웠어요",
          "speaker_id": "1",
          "time": "20210829 18:24"
        }
      ]
