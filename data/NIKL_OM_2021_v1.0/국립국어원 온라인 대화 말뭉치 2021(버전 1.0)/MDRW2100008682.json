      "utterance": [
        {
          "id": "MDRW2100008682.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008682.1.2",
          "form": "아직이요 좀 이따 먹으려구요",
          "original_form": "아직이요 좀 이따 먹으려구요",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008682.1.3",
          "form": "name2님은 이따 저녁 드시나요?",
          "original_form": "&name2&님은 이따 저녁 드시나요?",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008682.1.4",
          "form": "아점먹고 안먹어서 곧 먹으려고요~",
          "original_form": "아점먹고 안먹어서 곧 먹으려고요~",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008682.1.5",
          "form": "뭐드실 건가요?",
          "original_form": "뭐드실 건가요?",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008682.1.6",
          "form": "떡볶이 먹을예정이에요 ! 근데 중간에 배가 고파서 간식 먹었더니 배가 덜 고프네요...",
          "original_form": "떡볶이 먹을예정이에요 ! 근데 중간에 배가 고파서 간식 먹었더니 배가 덜 고프네요...",
          "speaker_id": "1",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008682.1.7",
          "form": "아아 아까 떡볶이 드신댔죠",
          "original_form": "아아 아까 떡볶이 드신댔죠",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008682.1.8",
          "form": "간식은 뭐드셨나요?",
          "original_form": "간식은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008682.1.9",
          "form": "저는 방금 식사하고왔어요 ㅎㅎ",
          "original_form": "저는 방금 식사하고왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008682.1.10",
          "form": "간식 복숭아랑 그릭요거트 먹었어요!",
          "original_form": "간식 복숭아랑 그릭요거트 먹었어요!",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008682.1.11",
          "form": "요즘 유행하는 복숭아안에 그릭요거트 넣어먹는거 따라해봤는데 맛있더라구요",
          "original_form": "요즘 유행하는 복숭아안에 그릭요거트 넣어먹는거 따라해봤는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008682.1.12",
          "form": "헐 저도 그거 너무 해먹어보고 싶어요",
          "original_form": "헐 저도 그거 너무 해먹어보고 싶어요",
          "speaker_id": "2",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100008682.1.13",
          "form": "요즘 복숭아 재철이라 또 엄청 달고 맛있잖아요",
          "original_form": "요즘 복숭아 재철이라 또 엄청 달고 맛있잖아요",
          "speaker_id": "2",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100008682.1.14",
          "form": "완전 맛있어요! 꼭 드셔보세요",
          "original_form": "완전 맛있어요! 꼭 드셔보세요",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008682.1.15",
          "form": "복숭아가 달달하니 너무 맛있어요",
          "original_form": "복숭아가 달달하니 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100008682.1.16",
          "form": "하 레시피 저장해뒀네요",
          "original_form": "하 레시피 저장해뒀네요",
          "speaker_id": "2",
          "time": "20210822 18:57"
        },
        {
          "id": "MDRW2100008682.1.17",
          "form": "조만간 아침으로 해먹어야겠어요",
          "original_form": "조만간 아침으로 해먹어야겠어요",
          "speaker_id": "2",
          "time": "20210822 18:57"
        },
        {
          "id": "MDRW2100008682.1.18",
          "form": "그릭요거트도 진짜 너무 좋아하는데 ㅜㅜb",
          "original_form": "그릭요거트도 진짜 너무 좋아하는데 ㅜㅜb",
          "speaker_id": "2",
          "time": "20210822 18:58"
        },
        {
          "id": "MDRW2100008682.1.19",
          "form": "그릭요거트도 너무 맛있죠..",
          "original_form": "그릭요거트도 너무 맛있죠..",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008682.1.20",
          "form": "그래놀라랑 먹으니까 더 맛있더라구요 건강에도 좋구..!",
          "original_form": "그래놀라랑 먹으니까 더 맛있더라구요 건강에도 좋구..!",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008682.1.21",
          "form": "그래놀라도 건강에 좋은가요?!",
          "original_form": "그래놀라도 건강에 좋은가요?!",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008682.1.22",
          "form": "잘 안챙겨 먹었었는데 호오",
          "original_form": "잘 안챙겨 먹었었는데 호오",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008682.1.23",
          "form": "네네 둘이 같이 먹으면 좋다네요",
          "original_form": "네네 둘이 같이 먹으면 좋다네요",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008682.1.24",
          "form": "물론 그래놀라는 많이 먹으면 살찌지만요.. !",
          "original_form": "물론 그래놀라는 많이 먹으면 살찌지만요.. !",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008682.1.25",
          "form": "저도 살짝 사먹어보ㅓ야겠네요",
          "original_form": "저도 살짝 사먹어보ㅓ야겠네요",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008682.1.26",
          "form": "완전 맛있던대!!",
          "original_form": "완전 맛있던대!!",
          "speaker_id": "2",
          "time": "20210822 19:35"
        }
      ]
