      "utterance": [
        {
          "id": "MDRW2100043718.1.1",
          "form": "혹시 반려동물은 안키우세요!?",
          "original_form": "혹시 반려동물은 안키우세요!?",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043718.1.2",
          "form": "네 ㅜㅜㅜ 키울수없어서",
          "original_form": "네 ㅜㅜㅜ 키울수없어서",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043718.1.3",
          "form": "고양이키우고싶어요",
          "original_form": "고양이키우고싶어요",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043718.1.4",
          "form": "아ㅠㅠ 전 개 키우거든요.. 저도 고양이 키워보고 싶어요!",
          "original_form": "아ㅠㅠ 전 개 키우거든요.. 저도 고양이 키워보고 싶어요!",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043718.1.5",
          "form": "동네에 길냥이들은 많은데 뭔가 만지기 무서워요 ㅋㅋ",
          "original_form": "동네에 길냥이들은 많은데 뭔가 만지기 무서워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043718.1.6",
          "form": "길냥이들은 다 도주도망가던걸요 ㅠ",
          "original_form": "길냥이들은 다 도주도망가던걸요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043718.1.7",
          "form": "냥냥펀치 맞을까봐 ㅎㅎㅎ 저희동네엔 캣맘들이 몇분 계시더라구요",
          "original_form": "냥냥펀치 맞을까봐 ㅎㅎㅎ 저희동네엔 캣맘들이 몇분 계시더라구요",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043718.1.8",
          "form": "맨날 꼬리만 봐요",
          "original_form": "맨날 꼬리만 봐요",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043718.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043718.1.10",
          "form": "부럽가 ㅠㅠㅠ캣맘",
          "original_form": "부럽가 ㅠㅠㅠ캣맘",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043718.1.11",
          "form": "그래서 쫌 많이 보여요 몇마리는 도망안가는데 만지기는 무서워요 ㅋㅋㅋ",
          "original_form": "그래서 쫌 많이 보여요 몇마리는 도망안가는데 만지기는 무서워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043718.1.12",
          "form": "그럼 반려동물은 한 번도 안키워보셨나요?",
          "original_form": "그럼 반려동물은 한 번도 안키워보셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043718.1.13",
          "form": "만질만큼 다가오나요?",
          "original_form": "만질만큼 다가오나요?",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043718.1.14",
          "form": "아주어릴때 기억안날때 개를 키웠다고하건데",
          "original_form": "아주어릴때 기억안날때 개를 키웠다고하건데",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043718.1.15",
          "form": "기억에없어요",
          "original_form": "기억에없어요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043718.1.16",
          "form": "네~ 냐옹하면서 부비적 거리기도 해용",
          "original_form": "네~ 냐옹하면서 부비적 거리기도 해용",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043718.1.17",
          "form": "우와...손 다 타서 와 ㅠㅜㅜㅜ부러워요",
          "original_form": "우와...손 다 타서 와 ㅠㅜㅜㅜ부러워요",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043718.1.18",
          "form": "만디고싶어요",
          "original_form": "만디고싶어요",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043718.1.19",
          "form": "부르면 대답도 하더라구요ㅎㅎ",
          "original_form": "부르면 대답도 하더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043718.1.20",
          "form": "부시럭 소리나면 뭐주는 줄 알고 다가와요 ㅋㅋㅋ",
          "original_form": "부시럭 소리나면 뭐주는 줄 알고 다가와요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043718.1.21",
          "form": "근데 비오거나 넘 덥고 추울땐 불쌍하고 안타깝더라구요ㅠㅠ",
          "original_form": "근데 비오거나 넘 덥고 추울땐 불쌍하고 안타깝더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043718.1.22",
          "form": "와아아ㅏ아.....넘귀여워요 ㅠㅠㅠㅠ",
          "original_form": "와아아ㅏ아.....넘귀여워요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043718.1.23",
          "form": "다 알아서 잘 살아갈겁니다 ㅠㅠㅠ",
          "original_form": "다 알아서 잘 살아갈겁니다 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043718.1.24",
          "form": "저도 사료라던지 간식 챙겨주고 싶은데",
          "original_form": "저도 사료라던지 간식 챙겨주고 싶은데",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043718.1.25",
          "form": "집까지 따라오고 그럴까봐 그러지도 못하구",
          "original_form": "집까지 따라오고 그럴까봐 그러지도 못하구",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043718.1.26",
          "form": "저희집개가 고양이를 디게 싫어하거든요ㅋㅋㅋ",
          "original_form": "저희집개가 고양이를 디게 싫어하거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043718.1.27",
          "form": "아하..개 키우시면 그렇죠 ㅠㅠㅠㅠ",
          "original_form": "아하..개 키우시면 그렇죠 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043718.1.28",
          "form": "그래서 뭘 해주진 못하고 마음으로만 응원해여",
          "original_form": "그래서 뭘 해주진 못하고 마음으로만 응원해여",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043718.1.29",
          "form": "그것만으로 충분할겁니다 ㅎㅎ",
          "original_form": "그것만으로 충분할겁니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:11"
        }
      ]
