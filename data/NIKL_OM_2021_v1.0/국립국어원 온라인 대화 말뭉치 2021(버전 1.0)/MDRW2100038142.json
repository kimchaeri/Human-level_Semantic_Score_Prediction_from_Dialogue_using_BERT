      "utterance": [
        {
          "id": "MDRW2100038142.1.1",
          "form": "집안일 좋아하시나요",
          "original_form": "집안일 좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038142.1.2",
          "form": "전 세탁기 청소하려고 세탁조 클리너 샀아요",
          "original_form": "전 세탁기 청소하려고 세탁조 클리너 샀아요",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100038142.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100038142.1.4",
          "form": "전 살만큼만한답니다",
          "original_form": "전 살만큼만한답니다",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100038142.1.5",
          "form": "저 어제 바닥 닦아서 행복해용",
          "original_form": "저 어제 바닥 닦아서 행복해용",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038142.1.6",
          "form": "룸메이트 있으신가요",
          "original_form": "룸메이트 있으신가요",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038142.1.7",
          "form": "지굼은 안계세유",
          "original_form": "지굼은 안계세유",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038142.1.8",
          "form": "룸메이트한테 잘해주셔요",
          "original_form": "룸메이트한테 잘해주셔요",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038142.1.9",
          "form": "집에 안들어오다니",
          "original_form": "집에 안들어오다니",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038142.1.10",
          "form": "그라무요",
          "original_form": "그라무요",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038142.1.11",
          "form": "아니 그분이 안들어오는것은 그분스케쥴이라 ㅎ",
          "original_form": "아니 그분이 안들어오는것은 그분스케쥴이라 ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100038142.1.12",
          "form": "집안일줄 가장 싫은것은?",
          "original_form": "집안일줄 가장 싫은것은?",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100038142.1.13",
          "form": "다 귀찮소",
          "original_form": "다 귀찮소",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038142.1.14",
          "form": "육아는요",
          "original_form": "육아는요",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038142.1.15",
          "form": "안키워봐서  가늠할수없네요",
          "original_form": "안키워봐서  가늠할수없네요",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100038142.1.16",
          "form": "헹글라이더 어려우신가요? 묻는것과 같습니다",
          "original_form": "헹글라이더 어려우신가요? 묻는것과 같습니다",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038142.1.17",
          "form": "아이돌봄 해보신걸로 아눈데요",
          "original_form": "아이돌봄 해보신걸로 아눈데요",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038142.1.18",
          "form": "전 육아하는지 직장을 다니는 게 ...",
          "original_form": "전 육아하는지 직장을 다니는 게 ...",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100038142.1.19",
          "form": "세시간씩은 뭐 어렵지않죠",
          "original_form": "세시간씩은 뭐 어렵지않죠",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100038142.1.20",
          "form": "올 전 3시간도 어려움",
          "original_form": "올 전 3시간도 어려움",
          "speaker_id": "1",
          "time": "20210901 23:52"
        }
      ]
