        {
          "id": "MDRW2100044275.1.1",
          "form": "지금 드시고 싶으신거 있으세요?",
          "original_form": "지금 드시고 싶으신거 있으세요?",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044275.1.2",
          "form": "지금 치킨먹고싶어요",
          "original_form": "지금 치킨먹고싶어요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044275.1.3",
          "form": "뿌링클이요",
          "original_form": "뿌링클이요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044275.1.4",
          "form": "우아 치킨 ㅠㅠ",
          "original_form": "우아 치킨 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044275.1.5",
          "form": "뿌링클이 어디꺼죵?",
          "original_form": "뿌링클이 어디꺼죵?",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044275.1.6",
          "form": "bhc인데 치즈가루뿌린거요",
          "original_form": "bhc인데 치즈가루뿌린거요",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044275.1.7",
          "form": "하지만 참아야겠죠",
          "original_form": "하지만 참아야겠죠",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044275.1.8",
          "form": "우아 ㅠㅠㅠ",
          "original_form": "우아 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044275.1.9",
          "form": "다른치킨 좋아하시는거 있으세요?",
          "original_form": "다른치킨 좋아하시는거 있으세요?",
