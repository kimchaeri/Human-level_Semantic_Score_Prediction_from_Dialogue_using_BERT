      "utterance": [
        {
          "id": "MDRW2100030739.1.1",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "2",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030739.1.2",
          "form": "그냥 웃는 건 인식이 안 되는데 하하하라고 하는 건 인식이 되네요",
          "original_form": "그냥 웃는 건 인식이 안 되는데 하하하라고 하는 건 인식이 되네요",
          "speaker_id": "2",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030739.1.3",
          "form": "오늘의 집이라는 사이트 아시나요",
          "original_form": "오늘의 집이라는 사이트 아시나요",
          "speaker_id": "2",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030739.1.4",
          "form": "캬캬캬 하고 오시면 돼요",
          "original_form": "캬캬캬 하고 오시면 돼요",
          "speaker_id": "1",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030739.1.5",
          "form": "웃으시면",
          "original_form": "웃으시면",
          "speaker_id": "1",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.6",
          "form": "거기에서 물건을 한 번씩 사거든요",
          "original_form": "거기에서 물건을 한 번씩 사거든요",
          "speaker_id": "2",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.7",
          "form": "차차차",
          "original_form": "차차차",
          "speaker_id": "2",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.8",
          "form": "캬캬캬",
          "original_form": "캬캬캬",
          "speaker_id": "2",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.9",
          "form": "인테리어 쇼핑모르죠",
          "original_form": "인테리어 쇼핑모르죠",
          "speaker_id": "1",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.10",
          "form": "거기에서 전에 스팀 다리미를 샀어요",
          "original_form": "거기에서 전에 스팀 다리미를 샀어요",
          "speaker_id": "2",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.11",
          "form": "쇼핑모르는 일본식 발음이네요",
          "original_form": "쇼핑모르는 일본식 발음이네요",
          "speaker_id": "1",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.12",
          "form": "이거 뭐야",
          "original_form": "이거 뭐야",
          "speaker_id": "1",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.13",
          "form": "성능이랑 디자인이랑 고민을 하다가 디자인이 예쁜 스팀 다리미를 구매했는데",
          "original_form": "성능이랑 디자인이랑 고민을 하다가 디자인이 예쁜 스팀 다리미를 구매했는데",
          "speaker_id": "2",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.14",
          "form": "쇼핑몰 발음이 잘 안 되시나요",
          "original_form": "쇼핑몰 발음이 잘 안 되시나요",
          "speaker_id": "2",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.15",
          "form": "스탠드형인가요",
          "original_form": "스탠드형인가요",
          "speaker_id": "1",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.16",
          "form": "저는 괜찮네요",
          "original_form": "저는 괜찮네요",
          "speaker_id": "2",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030739.1.17",
          "form": "쇼핑몰",
          "original_form": "쇼핑몰",
          "speaker_id": "1",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030739.1.18",
          "form": "아니요 디자인만 보고 샀어요 엄청 예쁜 빨간 색깔이에요",
          "original_form": "아니요 디자인만 보고 샀어요 엄청 예쁜 빨간 색깔이에요",
          "speaker_id": "2",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030739.1.19",
          "form": "되네요",
          "original_form": "되네요",
          "speaker_id": "1",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030739.1.20",
          "form": "약간 드라이기처럼 생긴 그런 거요",
          "original_form": "약간 드라이기처럼 생긴 그런 거요",
          "speaker_id": "2",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030739.1.21",
          "form": "민트색상 사고 싶었는데 품절이어서 빨간색으로 샀는데 이뻐요",
          "original_form": "민트색상 사고 싶었는데 품절이어서 빨간색으로 샀는데 이뻐요",
          "speaker_id": "2",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030739.1.22",
          "form": "아 핸디형 이네요",
          "original_form": "아 핸디형 이네요",
          "speaker_id": "1",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030739.1.23",
          "form": "네 핸디형 이요",
          "original_form": "네 핸디형 이요",
          "speaker_id": "2",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030739.1.24",
          "form": "그거 잘 펴지나요",
          "original_form": "그거 잘 펴지나요",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030739.1.25",
          "form": "저도 핸드 스팀 갖고 싶긴 했는데",
          "original_form": "저도 핸드 스팀 갖고 싶긴 했는데",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030739.1.26",
          "form": "네 기능에는 아무 이상 없더라고요 저는 매우 만족하며 쓰고 있습니다",
          "original_form": "네 기능에는 아무 이상 없더라고요 저는 매우 만족하며 쓰고 있습니다",
          "speaker_id": "2",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030739.1.27",
          "form": "척추 잘 다려지셨나요",
          "original_form": "척추 잘 다려지셨나요",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030739.1.28",
          "form": "디자인이 예뻐서 쓰고 또 쓰고 또 쓰고 싶더라고요",
          "original_form": "디자인이 예뻐서 쓰고 또 쓰고 또 쓰고 싶더라고요",
          "speaker_id": "2",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030739.1.29",
          "form": "셔츠ㅜ",
          "original_form": "셔츠ㅜ",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030739.1.30",
          "form": "척추는 안 다려 봤습니다",
          "original_form": "척추는 안 다려 봤습니다",
          "speaker_id": "2",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030739.1.31",
          "form": "아 셔츠 말씀이군요",
          "original_form": "아 셔츠 말씀이군요",
          "speaker_id": "2",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030739.1.32",
          "form": "발음이 이상한가 봐요",
          "original_form": "발음이 이상한가 봐요",
          "speaker_id": "1",
          "time": "20210904 12:11"
        }
      ]
