        },
        {
          "id": "MDRW2100030446.1.2",
          "form": "엄청 싸우자나요",
          "original_form": "엄청 싸우자나요",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030446.1.3",
          "form": "ㅎㅎ 맞아요",
          "original_form": "ㅎㅎ 맞아요",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030446.1.4",
          "form": "베테랑 영화보면 유아인도 그러고",
          "original_form": "베테랑 영화보면 유아인도 그러고",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030446.1.5",
          "form": "아 부럽네",
          "original_form": "아 부럽네",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030446.1.6",
          "form": "생각해보니 싸우고싶네요 ㅋㅋ",
          "original_form": "생각해보니 싸우고싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030446.1.7",
          "form": "그 막대한 재산 크하~~",
          "original_form": "그 막대한 재산 크하~~",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030446.1.8",
          "form": "지금 선전포고하신거?",
          "original_form": "지금 선전포고하신거?",
          "speaker_id": "1",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030446.1.9",
          "form": "마음이 바뀌었어요",
          "original_form": "마음이 바뀌었어요",
          "speaker_id": "1",
          "time": "20210904 01:12"
        },
        {
          "id": "MDRW2100030446.1.10",
          "form": "전 JY 증손주로 태어날 거임",
          "original_form": "전 JY 증손주로 태어날 거임",
          "speaker_id": "1",
          "time": "20210904 01:12"
        },
        {
          "id": "MDRW2100030446.1.11",
          "form": "JY?",
          "original_form": "JY?",
          "speaker_id": "2",
          "time": "20210904 01:21"
        },
        {
          "id": "MDRW2100030446.1.12",
          "form": "머리 굴리고 있습니다",
          "original_form": "머리 굴리고 있습니다",
          "speaker_id": "2",
          "time": "20210904 01:22"
        },
        {
          "id": "MDRW2100030446.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210904 01:22"
        },
        {
          "id": "MDRW2100030446.1.14",
          "form": "재드래곤!",
          "original_form": "재드래곤!",
          "speaker_id": "2",
          "time": "20210904 01:22"
        },
        {
          "id": "MDRW2100030446.1.15",
          "form": "진짜 좋것다 ㅋㅋㅋ",
          "original_form": "진짜 좋것다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:23"
        },
        {
          "id": "MDRW2100030446.1.16",
          "form": "네네! ㅎㅎ 삼성의 난 같은 거 생겨도 계열사 하나만 떼주면 물러날 거예요!",
          "original_form": "네네! ㅎㅎ 삼성의 난 같은 거 생겨도 계열사 하나만 떼주면 물러날 거예요!",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030446.1.17",
          "form": "아 어제 아기 울어서 달래다 같이 잠들었네요",
          "original_form": "아 어제 아기 울어서 달래다 같이 잠들었네요",
          "speaker_id": "2",
          "time": "20210904 08:35"
        },
        {
          "id": "MDRW2100030446.1.18",
          "form": "아침에 티비 시청은 뉴스인가요?",
          "original_form": "아침에 티비 시청은 뉴스인가요?",
          "speaker_id": "2",
          "time": "20210904 08:37"
        },
        {
          "id": "MDRW2100030446.1.19",
          "form": "ㅜㅜ 싫어요... 뉴스 채널 다 사라졌으면",
          "original_form": "ㅜㅜ 싫어요... 뉴스 채널 다 사라졌으면",
          "speaker_id": "1",
          "time": "20210904 09:09"
        },
        {
          "id": "MDRW2100030446.1.20",
          "form": "일찍 일나셧네요 ㅎㅎ",
          "original_form": "일찍 일나셧네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 09:30"
        },
        {
          "id": "MDRW2100030446.1.21",
          "form": "산책 다녀오셧어요?",
          "original_form": "산책 다녀오셧어요?",
          "speaker_id": "2",
          "time": "20210904 09:30"
        },
        {
          "id": "MDRW2100030446.1.22",
          "form": "지금 산책이..아직 안 끝났어요ㅎ",
          "original_form": "지금 산책이..아직 안 끝났어요ㅎ",
          "speaker_id": "1",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100030446.1.23",
          "form": "두마리라 따로따로 해줘야해서",
          "original_form": "두마리라 따로따로 해줘야해서",
          "speaker_id": "1",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100030446.1.24",
          "form": "님은 쉬는날엔 늦잠...을 자기엔 아기가 있군요!",
          "original_form": "님은 쉬는날엔 늦잠...을 자기엔 아기가 있군요!",
          "speaker_id": "1",
          "time": "20210904 09:32"
        },
        {
          "id": "MDRW2100030446.1.25",
          "form": "아 두마리 오 오오",
          "original_form": "아 두마리 오 오오",
          "speaker_id": "2",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100030446.1.26",
          "form": "저도 아기 산책중입니다",
          "original_form": "저도 아기 산책중입니다",
          "speaker_id": "2",
          "time": "20210904 09:34"
        },
        {
          "id": "MDRW2100030446.1.27",
          "form": "일어나자마자 YouTube로 시작해서",
          "original_form": "일어나자마자 YouTube로 시작해서",
          "speaker_id": "2",
          "time": "20210904 09:34"
        },
        {
          "id": "MDRW2100030446.1.28",
          "form": "너무 영상만 봐서 밖에 나왔습니다",
          "original_form": "너무 영상만 봐서 밖에 나왔습니다",
          "speaker_id": "2",
          "time": "20210904 09:34"
        },
        {
          "id": "MDRW2100030446.1.29",
          "form": "놀이터 가 고 있습니다",
          "original_form": "놀이터 가 고 있습니다",
          "speaker_id": "2",
          "time": "20210904 09:35"
        },
        {
          "id": "MDRW2100030446.1.30",
          "form": "오오 착한 아빠네요!!!",
          "original_form": "오오 착한 아빠네요!!!",
          "speaker_id": "1",
          "time": "20210904 09:35"
        },
        {
          "id": "MDRW2100030446.1.31",
          "form": "유튜브로 2000년대 가수 영상 보셨나요",
          "original_form": "유튜브로 2000년대 가수 영상 보셨나요",
          "speaker_id": "1",
          "time": "20210904 09:36"
        },
        {
          "id": "MDRW2100030446.1.32",
          "form": "검색해보겠습니다",
          "original_form": "검색해보겠습니다",
          "speaker_id": "2",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030446.1.33",
          "form": "유승준 비전!",
          "original_form": "유승준 비전!",
          "speaker_id": "2",
          "time": "20210904 10:14"
        },
        {
          "id": "MDRW2100030446.1.34",
          "form": "스카이 영원 너무 좋아했는데요",
          "original_form": "스카이 영원 너무 좋아했는데요",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100030446.1.35",
          "form": "애창곡이었습니다",
          "original_form": "애창곡이었습니다",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100030446.1.36",
          "form": "유승준ㅜㅜ 옛날에 좋아했는데",
          "original_form": "유승준ㅜㅜ 옛날에 좋아했는데",
          "speaker_id": "1",
          "time": "20210904 10:42"
        },
        {
          "id": "MDRW2100030446.1.37",
          "form": "지금 들어도 명곡들이죠",
          "original_form": "지금 들어도 명곡들이죠",
