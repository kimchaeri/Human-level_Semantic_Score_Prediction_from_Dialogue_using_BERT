      "utterance": [
        {
          "id": "MDRW2100017730.1.1",
          "form": "요즘 일은 어때",
          "original_form": "요즘 일은 어때",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017730.1.2",
          "form": "흠 크게 바쁘지도 않으면서 할 일은 있어서 괜춘한듯?",
          "original_form": "흠 크게 바쁘지도 않으면서 할 일은 있어서 괜춘한듯?",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017730.1.3",
          "form": "근데 기획이 은근 손 많이 간다",
          "original_form": "근데 기획이 은근 손 많이 간다",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017730.1.4",
          "form": "계획세우기는 끝남?",
          "original_form": "계획세우기는 끝남?",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017730.1.5",
          "form": "ㅇㅇ 그거는 화욜에 끝나고 이제 그것대로 일하는중",
          "original_form": "ㅇㅇ 그거는 화욜에 끝나고 이제 그것대로 일하는중",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017730.1.6",
          "form": "조율하는 부서가 제일 힘들지",
          "original_form": "조율하는 부서가 제일 힘들지",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017730.1.7",
          "form": "괴롭히는 사람은 없니",
          "original_form": "괴롭히는 사람은 없니",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017730.1.8",
          "form": "조율이랄지..아무래도 첫 기둥을 세우는 쪽이다보니까?",
          "original_form": "조율이랄지..아무래도 첫 기둥을 세우는 쪽이다보니까?",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017730.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 당장엔?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 당장엔?",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017730.1.10",
          "form": "그냥저냥 잘 지내고 있음",
          "original_form": "그냥저냥 잘 지내고 있음",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017730.1.11",
          "form": "ㅋㅋㅋㅋ그나마다행이네",
          "original_form": "ㅋㅋㅋㅋ그나마다행이네",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017730.1.12",
          "form": "하지만 방심하지 않는닷...!",
          "original_form": "하지만 방심하지 않는닷...!",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017730.1.13",
          "form": "넌 이제 일 좀 늘어남??",
          "original_form": "넌 이제 일 좀 늘어남??",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017730.1.14",
          "form": "팀장님이 이것저것 시키니까",
          "original_form": "팀장님이 이것저것 시키니까",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017730.1.15",
          "form": "거기 하나씩 손대고있는데",
          "original_form": "거기 하나씩 손대고있는데",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017730.1.16",
          "form": "그러다보니 이런저런 일이많네",
          "original_form": "그러다보니 이런저런 일이많네",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017730.1.17",
          "form": "내일은 외국담당자들이랑 연락한다",
          "original_form": "내일은 외국담당자들이랑 연락한다",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017730.1.18",
          "form": "헐 멋있다",
          "original_form": "헐 멋있다",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017730.1.19",
          "form": "그럼 너도 외국어로 솰라솰라 하는거??",
          "original_form": "그럼 너도 외국어로 솰라솰라 하는거??",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017730.1.20",
          "form": "말로는 못하겠어서",
          "original_form": "말로는 못하겠어서",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017730.1.21",
          "form": "메일로하지ㅋㅋㅋ",
          "original_form": "메일로하지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017730.1.22",
          "form": "이욜 간지",
          "original_form": "이욜 간지",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017730.1.23",
          "form": "사실 한국에서도 메신저랑 메일이",
          "original_form": "사실 한국에서도 메신저랑 메일이",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017730.1.24",
          "form": "제일편함",
          "original_form": "제일편함",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017730.1.25",
          "form": "전화.. 시러욧",
          "original_form": "전화.. 시러욧",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017730.1.26",
          "form": "나는...낡은 함수 찾아서 새 함수로 치환하는거 했는데",
          "original_form": "나는...낡은 함수 찾아서 새 함수로 치환하는거 했는데",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017730.1.27",
          "form": "업무전화받을일은 별로없지?",
          "original_form": "업무전화받을일은 별로없지?",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017730.1.28",
          "form": "걍 노가다인데 옆에 막 코드 띄워놓고 하니까",
          "original_form": "걍 노가다인데 옆에 막 코드 띄워놓고 하니까",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017730.1.29",
          "form": "괜히 있어보이는 기분",
          "original_form": "괜히 있어보이는 기분",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017730.1.30",
          "form": "전화...나도 싫어",
          "original_form": "전화...나도 싫어",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017730.1.31",
          "form": "ㅇㅇ 난 아예 없어",
          "original_form": "ㅇㅇ 난 아예 없어",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017730.1.32",
          "form": "칼퇴했어?",
          "original_form": "칼퇴했어?",
          "speaker_id": "1",
          "time": "20210831 18:37"
        }
      ]
    }
