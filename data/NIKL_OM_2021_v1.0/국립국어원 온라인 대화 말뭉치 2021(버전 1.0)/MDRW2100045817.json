      "utterance": [
        {
          "id": "MDRW2100045817.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100045817.1.2",
          "form": "아뇨 안좋아해요",
          "original_form": "아뇨 안좋아해요",
          "speaker_id": "1",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045817.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045817.1.4",
          "form": "아니요ㅠㅠㅠ",
          "original_form": "아니요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045817.1.5",
          "form": "숨만 쉬고 삽니다ㅋㅋㅋ",
          "original_form": "숨만 쉬고 삽니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045817.1.6",
          "form": "운동 돈내고 다녀보신적은 없으신가요?",
          "original_form": "운동 돈내고 다녀보신적은 없으신가요?",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045817.1.7",
          "form": "필라테스 3달정도 다녀봤어요!",
          "original_form": "필라테스 3달정도 다녀봤어요!",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045817.1.8",
          "form": "근데 운동이 되는지는 모르겠어요ㅋㅋ",
          "original_form": "근데 운동이 되는지는 모르겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045817.1.9",
          "form": "필테도 무슨 강사분 이력봐야하고 복잡하더라고요 ㅠ 사짜(?)가 많이서",
          "original_form": "필테도 무슨 강사분 이력봐야하고 복잡하더라고요 ㅠ 사짜(?)가 많이서",
          "speaker_id": "1",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100045817.1.10",
          "form": "맞아요ㅜㅜ 저도 동네에서 그냥 했더니 전문적이진 않더라고요",
          "original_form": "맞아요ㅜㅜ 저도 동네에서 그냥 했더니 전문적이진 않더라고요",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100045817.1.11",
          "form": "제 대학 동기가 아무 상괸도 없는 인문대생에 대학때 운동도 전혀안했는데 갑자기 필테 강사돼서 놀랐던 기억이...ㅎㅎ",
          "original_form": "제 대학 동기가 아무 상괸도 없는 인문대생에 대학때 운동도 전혀안했는데 갑자기 필테 강사돼서 놀랐던 기억이...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045817.1.12",
          "form": "허걱...",
          "original_form": "허걱...",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045817.1.13",
          "form": "그건 너무 충격인데요ㅠㅠㅠ",
          "original_form": "그건 너무 충격인데요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045817.1.14",
          "form": "네진짜 충격...ㅎㅎ 동기들 다 놀랐어요",
          "original_form": "네진짜 충격...ㅎㅎ 동기들 다 놀랐어요",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045817.1.15",
          "form": "근데 자격증 따는데 1년도ㅜ안걸린다고하더라구요",
          "original_form": "근데 자격증 따는데 1년도ㅜ안걸린다고하더라구요",
          "speaker_id": "1",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100045817.1.16",
          "form": "헐, , ,",
          "original_form": "헐, , ,",
          "speaker_id": "2",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100045817.1.17",
          "form": "진짜 잘 골라서 가야겠어요ㅠㅜ",
          "original_form": "진짜 잘 골라서 가야겠어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045817.1.18",
          "form": "괜히 더 나빠지겠네요",
          "original_form": "괜히 더 나빠지겠네요",
          "speaker_id": "2",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045817.1.19",
          "form": "맞아요 이상한데 가면 몸 더 틀어진다고 하더라고요",
          "original_form": "맞아요 이상한데 가면 몸 더 틀어진다고 하더라고요",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045817.1.20",
          "form": "안하느니보다 못하군요ㅜㅜ",
          "original_form": "안하느니보다 못하군요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045817.1.21",
          "form": "근데 운동을 안하다보니 시작이 어려운 거 같아요",
          "original_form": "근데 운동을 안하다보니 시작이 어려운 거 같아요",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045817.1.22",
          "form": "맞아요.. 집에서 홈트해야지 하긴하는데 ㅋㅋ",
          "original_form": "맞아요.. 집에서 홈트해야지 하긴하는데 ㅋㅋ",
