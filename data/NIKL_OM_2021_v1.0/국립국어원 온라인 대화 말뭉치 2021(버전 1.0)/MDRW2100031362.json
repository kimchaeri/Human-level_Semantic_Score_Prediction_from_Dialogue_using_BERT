      "utterance": [
        {
          "id": "MDRW2100031362.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100031362.1.2",
          "form": "네 천주교요",
          "original_form": "네 천주교요",
          "speaker_id": "2",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100031362.1.3",
          "form": "성당 안간지는 오래지만",
          "original_form": "성당 안간지는 오래지만",
          "speaker_id": "2",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100031362.1.4",
          "form": "그렇군요 저는 불교에요",
          "original_form": "그렇군요 저는 불교에요",
          "speaker_id": "1",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100031362.1.5",
          "form": "절에는 잘 안가긴해요 ㅎㅎ",
          "original_form": "절에는 잘 안가긴해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100031362.1.6",
          "form": "아 불교시구나",
          "original_form": "아 불교시구나",
          "speaker_id": "2",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100031362.1.7",
          "form": "종교 모임도 하셔요?",
          "original_form": "종교 모임도 하셔요?",
          "speaker_id": "2",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100031362.1.8",
          "form": "아니요 그냥 어릴 때 엄마가 믿은 정도라서요",
          "original_form": "아니요 그냥 어릴 때 엄마가 믿은 정도라서요",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100031362.1.9",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031362.1.10",
          "form": "부모님 따라서 종교가 결정되는 경우가 많죠",
          "original_form": "부모님 따라서 종교가 결정되는 경우가 많죠",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031362.1.11",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "1",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031362.1.12",
          "form": "정말 그래요",
          "original_form": "정말 그래요",
          "speaker_id": "1",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031362.1.13",
          "form": "저도 부모님이 성당 다니셔서",
          "original_form": "저도 부모님이 성당 다니셔서",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031362.1.14",
          "form": "자연스레 천주교인이 된거라",
          "original_form": "자연스레 천주교인이 된거라",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031362.1.15",
          "form": "그럼 부모님은 성당에 자주 다니세요?",
          "original_form": "그럼 부모님은 성당에 자주 다니세요?",
          "speaker_id": "1",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031362.1.16",
          "form": "매주 미사 참여하셨는데",
          "original_form": "매주 미사 참여하셨는데",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031362.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031362.1.18",
          "form": "요즘은 집에서 평화방송으로 미사 참례하세요",
          "original_form": "요즘은 집에서 평화방송으로 미사 참례하세요",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031362.1.19",
          "form": "아 코로나 때문에 그런가 봐요",
          "original_form": "아 코로나 때문에 그런가 봐요",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031362.1.20",
          "form": "네 코로나 단계가 높아지면서",
          "original_form": "네 코로나 단계가 높아지면서",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031362.1.21",
          "form": "종교모임도 다 자제하라고 해서요",
          "original_form": "종교모임도 다 자제하라고 해서요",
          "speaker_id": "2",
          "time": "20210905 10:25"
        }
      ]
