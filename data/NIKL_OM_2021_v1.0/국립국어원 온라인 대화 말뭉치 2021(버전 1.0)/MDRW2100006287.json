      "utterance": [
        {
          "id": "MDRW2100006287.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006287.1.2",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006287.1.3",
          "form": "가본 여행지중에 어디가 제일 좋으셧어요?",
          "original_form": "가본 여행지중에 어디가 제일 좋으셧어요?",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006287.1.4",
          "form": "신행으로 간 몰디브요..ㅎㅎ",
          "original_form": "신행으로 간 몰디브요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100006287.1.5",
          "form": "와 몰디브!!",
          "original_form": "와 몰디브!!",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100006287.1.6",
          "form": "몰디브 안가봣어요 ㅋㅋㅋㅋㅋ",
          "original_form": "몰디브 안가봣어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006287.1.7",
          "form": "어디 가보셨나요??",
          "original_form": "어디 가보셨나요??",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006287.1.8",
          "form": "저는 가본곳 중에는 라오스가 제일 좋앗어요",
          "original_form": "저는 가본곳 중에는 라오스가 제일 좋앗어요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006287.1.9",
          "form": "어떤 점이 좋았나요",
          "original_form": "어떤 점이 좋았나요",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006287.1.10",
          "form": "음 제가 그때 생각이 많던 시기여서",
          "original_form": "음 제가 그때 생각이 많던 시기여서",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006287.1.11",
          "form": "라오스 자체가 느릿느릿한 느낌이 들어서 좋앗어요",
          "original_form": "라오스 자체가 느릿느릿한 느낌이 들어서 좋앗어요",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006287.1.12",
          "form": "그렇군요 꽃청춘에서 본 기억이..",
          "original_form": "그렇군요 꽃청춘에서 본 기억이..",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006287.1.13",
          "form": "국내여행도 자주 가세요?",
          "original_form": "국내여행도 자주 가세요?",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006287.1.14",
          "form": "네 아이들때문에 국외는 아무래도 제약이 많아서..",
          "original_form": "네 아이들때문에 국외는 아무래도 제약이 많아서..",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100006287.1.15",
          "form": "국내 여행지는 어디가 좋으셧어요?",
          "original_form": "국내 여행지는 어디가 좋으셧어요?",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100006287.1.16",
          "form": "제주도.부산.거제.경주.여수 다좋더라구요",
          "original_form": "제주도.부산.거제.경주.여수 다좋더라구요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006287.1.17",
          "form": "맞아요 국내도 좋은것같아요",
          "original_form": "맞아요 국내도 좋은것같아요",
          "speaker_id": "1",
          "time": "20210907 23:00"
        }
      ]
