      "utterance": [
        {
          "id": "MDRW2100017001.1.1",
          "form": "맛집 좋아하시나요",
          "original_form": "맛집 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017001.1.2",
          "form": "이왕에 가는 거 맛집이면 좋지요ㅋㅋ",
          "original_form": "이왕에 가는 거 맛집이면 좋지요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017001.1.3",
          "form": "어떤메뉴요?",
          "original_form": "어떤메뉴요?",
          "speaker_id": "1",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017001.1.4",
          "form": "특정 메뉴가 있다기보다는 그냥 가는 곳 근처 맛집 검색해보고 가는 편이에요",
          "original_form": "특정 메뉴가 있다기보다는 그냥 가는 곳 근처 맛집 검색해보고 가는 편이에요",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017001.1.5",
          "form": "저는 돈까스좋아해요",
          "original_form": "저는 돈까스좋아해요",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017001.1.6",
          "form": "돈까스 맛집 아는 곳 있으신가요",
          "original_form": "돈까스 맛집 아는 곳 있으신가요",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017001.1.7",
          "form": "크레이지카츠요!",
          "original_form": "크레이지카츠요!",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017001.1.8",
          "form": "체인점이에요?",
          "original_form": "체인점이에요?",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017001.1.9",
          "form": "합정 본점이 최고입니다",
          "original_form": "합정 본점이 최고입니다",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017001.1.10",
          "form": "근처 갈 일 생기면 가봐야겠네요ㅋㅋ",
          "original_form": "근처 갈 일 생기면 가봐야겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017001.1.11",
          "form": "네 특로스카츠추천!",
          "original_form": "네 특로스카츠추천!",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017001.1.12",
          "form": "돈까스 땡기네요 ㅋㅋ",
          "original_form": "돈까스 땡기네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017001.1.13",
          "form": "전 일식 돈가스가 좋더라구요",
          "original_form": "전 일식 돈가스가 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017001.1.14",
          "form": "저도 일식이더 좋아여",
          "original_form": "저도 일식이더 좋아여",
          "speaker_id": "1",
