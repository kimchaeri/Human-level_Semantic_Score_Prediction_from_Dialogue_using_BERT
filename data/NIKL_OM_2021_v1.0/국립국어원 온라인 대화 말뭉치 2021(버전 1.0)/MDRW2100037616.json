      "utterance": [
        {
          "id": "MDRW2100037616.1.1",
          "form": "롯데리아도 드시나요?",
          "original_form": "롯데리아도 드시나요?",
          "speaker_id": "2",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100037616.1.2",
          "form": "네 롯데리아 별론줄 알았는데요",
          "original_form": "네 롯데리아 별론줄 알았는데요",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100037616.1.3",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100037616.1.4",
          "form": "전에 우연히 먹어보니 맛있더라구요?",
          "original_form": "전에 우연히 먹어보니 맛있더라구요?",
          "speaker_id": "1",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100037616.1.5",
          "form": "맞죠 ㅋㅋ",
          "original_form": "맞죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100037616.1.6",
          "form": "저도 편견이 있었는데",
          "original_form": "저도 편견이 있었는데",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100037616.1.7",
          "form": "아재버거 맛있었어요",
          "original_form": "아재버거 맛있었어요",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100037616.1.8",
          "form": "온라인에서 엄청 이유없이 까는거같아요",
          "original_form": "온라인에서 엄청 이유없이 까는거같아요",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100037616.1.9",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100037616.1.10",
          "form": "버거집인데 버거말고 사이드가 맛있다면서 ㅋㅋ",
          "original_form": "버거집인데 버거말고 사이드가 맛있다면서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100037616.1.11",
          "form": "롯데가 일본 이미지도 있고 하니까 그런가싶구요",
          "original_form": "롯데가 일본 이미지도 있고 하니까 그런가싶구요",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100037616.1.12",
          "form": "양상추가 맥도날드는 잘게 부서져있는데요 롯데리아는 촘촘히 포개져서 좋던데요",
          "original_form": "양상추가 맥도날드는 잘게 부서져있는데요 롯데리아는 촘촘히 포개져서 좋던데요",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100037616.1.13",
          "form": "아 맞아요!",
          "original_form": "아 맞아요!",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100037616.1.14",
          "form": "가끔 맥날은 양상추가 거의 안들어간거같기도 ㅋㅋ 합니다",
          "original_form": "가끔 맥날은 양상추가 거의 안들어간거같기도 ㅋㅋ 합니다",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100037616.1.15",
          "form": "그런 여론을 유도하는 세력이 있는걸까요 ㅋㅋ 뭔가 밈처럼 그러더라구요",
          "original_form": "그런 여론을 유도하는 세력이 있는걸까요 ㅋㅋ 뭔가 밈처럼 그러더라구요",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037616.1.16",
          "form": "맞죠 ㅋㅋㅋ",
          "original_form": "맞죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037616.1.17",
          "form": "그런건 보통",
          "original_form": "그런건 보통",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037616.1.18",
          "form": "라이벌 그룹이 하던데",
          "original_form": "라이벌 그룹이 하던데",
          "speaker_id": "2",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100037616.1.19",
          "form": "작년에 모짜렐라인더버거 우연히 먹고",
          "original_form": "작년에 모짜렐라인더버거 우연히 먹고",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100037616.1.20",
          "form": "매장에 사람이 없어서 바로만들었긴 했지만 암튼 생각외로 맛있어서요",
          "original_form": "매장에 사람이 없어서 바로만들었긴 했지만 암튼 생각외로 맛있어서요",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100037616.1.21",
          "form": "네네 !",
          "original_form": "네네 !",
          "speaker_id": "2",
          "time": "20210901 13:53"
        }
