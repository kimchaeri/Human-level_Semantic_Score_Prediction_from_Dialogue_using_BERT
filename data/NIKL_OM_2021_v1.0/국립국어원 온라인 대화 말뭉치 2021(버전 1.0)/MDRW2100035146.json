      "utterance": [
        {
          "id": "MDRW2100035146.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210829 07:02"
        },
        {
          "id": "MDRW2100035146.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 10:55"
        },
        {
          "id": "MDRW2100035146.1.3",
          "form": "좋아하는 음식 있으세요",
          "original_form": "좋아하는 음식 있으세요",
          "speaker_id": "2",
          "time": "20210830 10:55"
        },
        {
          "id": "MDRW2100035146.1.4",
          "form": "저는 피자 좋아해요~",
          "original_form": "저는 피자 좋아해요~",
          "speaker_id": "1",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100035146.1.5",
          "form": "피자 너무 맛있죠",
          "original_form": "피자 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100035146.1.6",
          "form": "저는 불족발 매운음식 좋아한답니다",
          "original_form": "저는 불족발 매운음식 좋아한답니다",
          "speaker_id": "2",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100035146.1.7",
          "form": "오!! 매운거좋아하시는군요! 부러워요 전 지독한맵찔이라ㅠㅠ",
          "original_form": "오!! 매운거좋아하시는군요! 부러워요 전 지독한맵찔이라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035146.1.8",
          "form": "저도 처음에는 못 먹었어요",
          "original_form": "저도 처음에는 못 먹었어요",
          "speaker_id": "2",
          "time": "20210830 11:08"
        },
        {
          "id": "MDRW2100035146.1.9",
          "form": "먹다 보니까 자꾸 더매운 거를 찾더라고요~",
          "original_form": "먹다 보니까 자꾸 더매운 거를 찾더라고요~",
          "speaker_id": "2",
          "time": "20210830 11:08"
        },
        {
          "id": "MDRW2100035146.1.10",
          "form": "아 그런가요? 맞아요 잘드시는분들보니 약간 중독성이잇다더라구요",
          "original_form": "아 그런가요? 맞아요 잘드시는분들보니 약간 중독성이잇다더라구요",
          "speaker_id": "1",
          "time": "20210830 11:09"
        },
        {
          "id": "MDRW2100035146.1.11",
          "form": "나이가 드니 위가 아프더라구요ㅠ",
          "original_form": "나이가 드니 위가 아프더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210830 11:10"
        },
        {
          "id": "MDRW2100035146.1.12",
          "form": "자제하려고 하는데",
          "original_form": "자제하려고 하는데",
          "speaker_id": "2",
          "time": "20210830 11:10"
        },
        {
          "id": "MDRW2100035146.1.13",
          "form": "습관이 돼서 잘 안되네요ㅠ",
          "original_form": "습관이 돼서 잘 안되네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 11:10"
        },
        {
          "id": "MDRW2100035146.1.14",
          "form": "아하.. 맞아요 매운거 너무 많이드시면 몸에안좋아여 ㅜㅜ",
          "original_form": "아하.. 맞아요 매운거 너무 많이드시면 몸에안좋아여 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 11:11"
        },
        {
          "id": "MDRW2100035146.1.15",
          "form": "배달음식도 좀 자제하고 자극적인 음식도 좀 자제해야 하는데",
          "original_form": "배달음식도 좀 자제하고 자극적인 음식도 좀 자제해야 하는데",
          "speaker_id": "2",
          "time": "20210830 11:12"
        },
        {
          "id": "MDRW2100035146.1.16",
          "form": "로제 떡볶이 좋아하시나요",
          "original_form": "로제 떡볶이 좋아하시나요",
          "speaker_id": "2",
          "time": "20210830 11:12"
        },
        {
          "id": "MDRW2100035146.1.17",
          "form": "로제 좋아해요!! 맵찔이인 저에겐 천사같은존재",
          "original_form": "로제 좋아해요!! 맵찔이인 저에겐 천사같은존재",
          "speaker_id": "1",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100035146.1.18",
          "form": "은근 중독성 있는 맛이더라구요",
          "original_form": "은근 중독성 있는 맛이더라구요",
          "speaker_id": "2",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100035146.1.19",
          "form": "젊은 사람들이 왜 좋아하는지 알겠어요ㅋㅋ",
          "original_form": "젊은 사람들이 왜 좋아하는지 알겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100035146.1.20",
          "form": "맞죠맞죠~ 느끼한거싫어하시는분들도 잘드시더라구요!",
          "original_form": "맞죠맞죠~ 느끼한거싫어하시는분들도 잘드시더라구요!",
          "speaker_id": "1",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100035146.1.21",
          "form": "맞아요 쪼끔 매콤하면서도 부드럽고",
          "original_form": "맞아요 쪼끔 매콤하면서도 부드럽고",
          "speaker_id": "2",
          "time": "20210830 11:18"
        }
      ]
