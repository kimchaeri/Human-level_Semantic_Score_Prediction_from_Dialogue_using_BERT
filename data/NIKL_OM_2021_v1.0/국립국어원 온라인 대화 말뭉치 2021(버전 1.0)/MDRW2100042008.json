      "utterance": [
        {
          "id": "MDRW2100042008.1.1",
          "form": "제주도에도 비가 오고 있나요?",
          "original_form": "제주도에도 비가 오고 있나요?",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100042008.1.2",
          "form": "비가 5분전에 왔어요",
          "original_form": "비가 5분전에 왔어요",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100042008.1.3",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100042008.1.4",
          "form": "저흰 오늘 하루종일 비가 왔어요",
          "original_form": "저흰 오늘 하루종일 비가 왔어요",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100042008.1.5",
          "form": "지금도 오고",
          "original_form": "지금도 오고",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100042008.1.6",
          "form": "이게 금방 비오다말다비오다말다쨍",
          "original_form": "이게 금방 비오다말다비오다말다쨍",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100042008.1.7",
          "form": "이런느낌...ㅎㅎㅎ",
          "original_form": "이런느낌...ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100042008.1.8",
          "form": "아 많이 오진 않고요?",
          "original_form": "아 많이 오진 않고요?",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100042008.1.9",
          "form": "네.담주 월욜인가 태풍온대요",
          "original_form": "네.담주 월욜인가 태풍온대요",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100042008.1.10",
          "form": "아악 태풍이라니ㅠ",
          "original_form": "아악 태풍이라니ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100042008.1.11",
          "form": "입도하고 처음 격는거라",
          "original_form": "입도하고 처음 격는거라",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100042008.1.12",
          "form": "장마 지나고 태풍인가요",
          "original_form": "장마 지나고 태풍인가요",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100042008.1.13",
          "form": "네 두려우면서도 기대되는? ㅎㅎ",
          "original_form": "네 두려우면서도 기대되는? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100042008.1.14",
          "form": "아아 그러시구나",
          "original_form": "아아 그러시구나",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100042008.1.15",
          "form": "제주쪽이 태풍 영향권인가요?",
          "original_form": "제주쪽이 태풍 영향권인가요?",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100042008.1.16",
          "form": "전신주도 쓰러지고 나무도 쓰러지고..",
          "original_form": "전신주도 쓰러지고 나무도 쓰러지고..",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100042008.1.17",
          "form": "일단 그렇게들 말씀하시더라구요",
          "original_form": "일단 그렇게들 말씀하시더라구요",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100042008.1.18",
          "form": "아 대단하네요",
          "original_form": "아 대단하네요",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100042008.1.19",
          "form": "창문 잘 닫구 조심하려구요",
          "original_form": "창문 잘 닫구 조심하려구요",
          "speaker_id": "2",
          "time": "20210907 19:20"
        }
      ]
