      "utterance": [
        {
          "id": "MDRW2100029371.1.1",
          "form": "name1님! 제가 맛집 리스트를 정리하고 있는데 혹시 추천해주실 맛집 있나요?",
          "original_form": "&name1&님! 제가 맛집 리스트를 정리하고 있는데 혹시 추천해주실 맛집 있나요?",
          "speaker_id": "2",
          "time": "20210903 03:22"
        },
        {
          "id": "MDRW2100029371.1.2",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210903 03:22"
        },
        {
          "id": "MDRW2100029371.1.3",
          "form": "술좋아하세요?!",
          "original_form": "술좋아하세요?!",
          "speaker_id": "1",
          "time": "20210903 03:22"
        },
        {
          "id": "MDRW2100029371.1.4",
          "form": "헉 넵",
          "original_form": "헉 넵",
          "speaker_id": "2",
          "time": "20210903 03:22"
        },
        {
          "id": "MDRW2100029371.1.5",
          "form": "제 삶입니다",
          "original_form": "제 삶입니다",
          "speaker_id": "2",
          "time": "20210903 03:22"
        },
        {
          "id": "MDRW2100029371.1.6",
          "form": "거주지가 어느쪽이세요?!",
          "original_form": "거주지가 어느쪽이세요?!",
          "speaker_id": "1",
          "time": "20210903 03:23"
        },
        {
          "id": "MDRW2100029371.1.7",
          "form": "저 서울이요!!",
          "original_form": "저 서울이요!!",
          "speaker_id": "2",
          "time": "20210903 03:25"
        },
        {
          "id": "MDRW2100029371.1.8",
          "form": "아 그럼 혹시 문래아세요?!",
          "original_form": "아 그럼 혹시 문래아세요?!",
          "speaker_id": "1",
          "time": "20210903 03:26"
        },
        {
          "id": "MDRW2100029371.1.9",
          "form": "문래동! 영등포쪽!",
          "original_form": "문래동! 영등포쪽!",
          "speaker_id": "1",
          "time": "20210903 03:26"
        },
        {
          "id": "MDRW2100029371.1.10",
          "form": "헐!!!완전 자주 가요~~~",
          "original_form": "헐!!!완전 자주 가요~~~",
          "speaker_id": "2",
          "time": "20210903 03:28"
        },
        {
          "id": "MDRW2100029371.1.11",
          "form": "문래동 브릭스박스 가보세요,,,!",
          "original_form": "문래동 브릭스박스 가보세요,,,!",
          "speaker_id": "1",
          "time": "20210903 03:28"
        },
        {
          "id": "MDRW2100029371.1.12",
          "form": "와인하고",
          "original_form": "와인하고",
          "speaker_id": "1",
          "time": "20210903 03:28"
        },
        {
          "id": "MDRW2100029371.1.13",
          "form": "진토닉이 진짜맛있어요",
          "original_form": "진토닉이 진짜맛있어요",
          "speaker_id": "1",
          "time": "20210903 03:28"
        },
        {
          "id": "MDRW2100029371.1.14",
          "form": "진토닉,,최고!",
          "original_form": "진토닉,,최고!",
          "speaker_id": "1",
          "time": "20210903 03:28"
        },
        {
          "id": "MDRW2100029371.1.15",
          "form": "헉 감사합니다 지금 당장 메모해둘게요",
          "original_form": "헉 감사합니다 지금 당장 메모해둘게요",
          "speaker_id": "2",
          "time": "20210903 03:28"
        },
        {
          "id": "MDRW2100029371.1.16",
          "form": "오늘은친구분이랑 뭐드시러가세요?!!",
          "original_form": "오늘은친구분이랑 뭐드시러가세요?!!",
          "speaker_id": "1",
          "time": "20210903 03:29"
        },
        {
          "id": "MDRW2100029371.1.17",
          "form": "문래 몽밀이랑 철판희도 맛집입니다앙!!",
          "original_form": "문래 몽밀이랑 철판희도 맛집입니다앙!!",
          "speaker_id": "2",
          "time": "20210903 03:29"
        },
        {
          "id": "MDRW2100029371.1.18",
          "form": "저는 오늘 친구랑 서촌 메밀국수 가게에 갈 것 같아요!",
          "original_form": "저는 오늘 친구랑 서촌 메밀국수 가게에 갈 것 같아요!",
          "speaker_id": "2",
          "time": "20210903 03:29"
        },
        {
          "id": "MDRW2100029371.1.19",
          "form": "뭐파는곳인가요?!!",
          "original_form": "뭐파는곳인가요?!!",
          "speaker_id": "1",
          "time": "20210903 03:29"
        },
        {
          "id": "MDRW2100029371.1.20",
          "form": "아 맛있겠다",
          "original_form": "아 맛있겠다",
          "speaker_id": "1",
          "time": "20210903 03:29"
        },
        {
          "id": "MDRW2100029371.1.21",
          "form": "헉!! 몽밀 냉모밀이 진짜 맛있구요!!",
          "original_form": "헉!! 몽밀 냉모밀이 진짜 맛있구요!!",
          "speaker_id": "2",
          "time": "20210903 03:30"
        },
        {
          "id": "MDRW2100029371.1.22",
          "form": "철판희는 철판볶음밥 파는 가게입니다",
          "original_form": "철판희는 철판볶음밥 파는 가게입니다",
          "speaker_id": "2",
          "time": "20210903 03:30"
        },
        {
          "id": "MDRW2100029371.1.23",
          "form": "깔끔하고 짱이에요 ㅠ",
          "original_form": "깔끔하고 짱이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 03:30"
        },
        {
          "id": "MDRW2100029371.1.24",
          "form": "아 ㅠ 둘다최고네요 꼭 가볼게요",
          "original_form": "아 ㅠ 둘다최고네요 꼭 가볼게요",
          "speaker_id": "1",
          "time": "20210903 03:31"
        }
      ]
