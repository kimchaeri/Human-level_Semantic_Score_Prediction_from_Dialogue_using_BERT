        {
          "id": "MDRW2100007199.1.1",
          "form": "식기세척기 문 열어놔",
          "original_form": "식기세척기 문 열어놔",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007199.1.2",
          "form": "알았어",
          "original_form": "알았어",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007199.1.3",
          "form": "열려있는데?",
          "original_form": "열려있는데?",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007199.1.4",
          "form": "그래? 잘했어",
          "original_form": "그래? 잘했어",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007199.1.5",
          "form": "창문도 잘 보고",
          "original_form": "창문도 잘 보고",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007199.1.6",
          "form": "창문 닫으라구?",
          "original_form": "창문 닫으라구?",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007199.1.7",
          "form": "또 비올까봐",
          "original_form": "또 비올까봐",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007199.1.8",
          "form": "응 창문도 확인해볼게",
          "original_form": "응 창문도 확인해볼게",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007199.1.9",
          "form": "웅 그래",
          "original_form": "웅 그래",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007199.1.10",
          "form": "컴퓨터 본체 남는건",
          "original_form": "컴퓨터 본체 남는건",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007199.1.11",
          "form": "정리해뒀어?",
          "original_form": "정리해뒀어?",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007199.1.12",
          "form": "창문 닫았당",
          "original_form": "창문 닫았당",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007199.1.13",
          "form": "응 책상 구석에 놔뒀지",
          "original_form": "응 책상 구석에 놔뒀지",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007199.1.14",
          "form": "응 그거 들고 처가 가야지",
          "original_form": "응 그거 들고 처가 가야지",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007199.1.15",
          "form": "엄마아빠드리려면 안에있는거 다 지워야겠네",
          "original_form": "엄마아빠드리려면 안에있는거 다 지워야겠네",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007199.1.16",
          "form": "근데 무거운데 가지고가려고?",
          "original_form": "근데 무거운데 가지고가려고?",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007199.1.17",
          "form": "튼튼하니까^-^",
          "original_form": "튼튼하니까^-^",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007199.1.18",
          "form": "흠 내려갈때 우리 짐도 있을텐데",
          "original_form": "흠 내려갈때 우리 짐도 있을텐데",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007199.1.19",
          "form": "고건 고민해봅시당",
          "original_form": "고건 고민해봅시당",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007199.1.20",
          "form": "아니 괜찮아ㅎㅎ",
          "original_form": "아니 괜찮아ㅎㅎ",
