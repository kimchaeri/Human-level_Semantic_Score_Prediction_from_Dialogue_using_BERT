      "utterance": [
        {
          "id": "MDRW2100035606.1.1",
          "form": "햄버거 좋아하세요?",
          "original_form": "햄버거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035606.1.2",
          "form": "네 좋아해요 버거킹 롯데 노브랜드 다 잘먹어요 ㅋㅋ",
          "original_form": "네 좋아해요 버거킹 롯데 노브랜드 다 잘먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035606.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035606.1.4",
          "form": "저두요 ㅎ",
          "original_form": "저두요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035606.1.5",
          "form": "노브랜드도",
          "original_form": "노브랜드도",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035606.1.6",
          "form": "버거가 있나요?",
          "original_form": "버거가 있나요?",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035606.1.7",
          "form": "노브랜드도 버거를 팔아요",
          "original_form": "노브랜드도 버거를 팔아요",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035606.1.8",
          "form": "근데..버거킹이 더 나아욬",
          "original_form": "근데..버거킹이 더 나아욬",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035606.1.9",
          "form": "ㅋㅋㅋㅋㅋ 우와.. 싱기하네요",
          "original_form": "ㅋㅋㅋㅋㅋ 우와.. 싱기하네요",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035606.1.10",
          "form": "최애가 어디에요?",
          "original_form": "최애가 어디에요?",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035606.1.11",
          "form": "전 모스버거 난반치킨을 좋아하는데 지점이 거의 업서요",
          "original_form": "전 모스버거 난반치킨을 좋아하는데 지점이 거의 업서요",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100035606.1.12",
          "form": "name2님은 뭘 좋아하세요?",
          "original_form": "&name2&님은 뭘 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035606.1.13",
          "form": "헐 모스버거 아시는군요",
          "original_form": "헐 모스버거 아시는군요",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035606.1.14",
          "form": "저 예전에 처음 먹고 반했어요",
          "original_form": "저 예전에 처음 먹고 반했어요",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035606.1.15",
          "form": "한떄 일본여행자들 필수 인증템이었잖아요",
          "original_form": "한떄 일본여행자들 필수 인증템이었잖아요",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035606.1.16",
          "form": "전 모스버거 라이스버거도 좋아하구,, 대부분의 메뉴를 좋아하,,지만!",
          "original_form": "전 모스버거 라이스버거도 좋아하구,, 대부분의 메뉴를 좋아하,,지만!",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035606.1.17",
          "form": "모스버거가요???!?!??",
          "original_form": "모스버거가요???!?!??",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035606.1.18",
          "form": "먹으려면 강남이나 고속버스터미널에 가야한단점 ㅠㅠ",
          "original_form": "먹으려면 강남이나 고속버스터미널에 가야한단점 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035606.1.19",
          "form": "몰랐어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "몰랐어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035606.1.20",
          "form": "네 되게 옛날에 오타쿠쪽에선 여행가면 다 모스버거 먹었어요",
          "original_form": "네 되게 옛날에 오타쿠쪽에선 여행가면 다 모스버거 먹었어요",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035606.1.21",
          "form": "아하.....",
          "original_form": "아하.....",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035606.1.22",
          "form": "이유는 모르겠지만,, 메론소다 사진직구,,",
          "original_form": "이유는 모르겠지만,, 메론소다 사진직구,,",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035606.1.23",
          "form": "저도 서울 터미널에서맨",
          "original_form": "저도 서울 터미널에서맨",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035606.1.24",
          "form": "먹었거든요ㅋㅋㅋㅋ",
          "original_form": "먹었거든요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035606.1.25",
          "form": "한국에 들어오기전엔 오타쿠들기리 여행가면 모 스버거럴 먹고 사진을 직었는데",
          "original_form": "한국에 들어오기전엔 오타쿠들기리 여행가면 모 스버거럴 먹고 사진을 직었는데",
          "speaker_id": "1",
