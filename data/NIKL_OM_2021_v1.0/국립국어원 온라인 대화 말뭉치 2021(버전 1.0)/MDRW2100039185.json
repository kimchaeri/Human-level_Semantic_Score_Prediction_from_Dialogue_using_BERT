      "utterance": [
        {
          "id": "MDRW2100039185.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039185.1.2",
          "form": "아직이요ㅠㅠ",
          "original_form": "아직이요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039185.1.3",
          "form": "아직이요?",
          "original_form": "아직이요?",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039185.1.4",
          "form": "배고프시겠어요 ㅠㅜ",
          "original_form": "배고프시겠어요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039185.1.5",
          "form": "빵 사와서 조금 이따가 먹으려고요",
          "original_form": "빵 사와서 조금 이따가 먹으려고요",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039185.1.6",
          "form": "식습관이 엉망이네요 ㅎㅎ",
          "original_form": "식습관이 엉망이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039185.1.7",
          "form": "그래도 머 드실수 있으니 다행이네요",
          "original_form": "그래도 머 드실수 있으니 다행이네요",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039185.1.8",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039185.1.9",
          "form": "저는 김치볶음밥 해먹었어요",
          "original_form": "저는 김치볶음밥 해먹었어요",
          "speaker_id": "2",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039185.1.10",
          "form": "계란국이랑요",
          "original_form": "계란국이랑요",
          "speaker_id": "2",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039185.1.11",
          "form": "헉 너무 좋아하는 메뉴들이에요",
          "original_form": "헉 너무 좋아하는 메뉴들이에요",
          "speaker_id": "1",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039185.1.12",
          "form": "스팸에 김치볶음밥",
          "original_form": "스팸에 김치볶음밥",
          "speaker_id": "2",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039185.1.13",
          "form": "스팸은 언제나 진리죠 ㅎㅎㅎ",
          "original_form": "스팸은 언제나 진리죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039185.1.14",
          "form": "헉 스팸구우면 맛있죠 ㅎㅎ",
          "original_form": "헉 스팸구우면 맛있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039185.1.15",
          "form": "근데 어릴때 부모님이 잘 안사주셨어서 ㅋㅋㅋ 성인되고서도 잘 안사먹는 것 같아요",
          "original_form": "근데 어릴때 부모님이 잘 안사주셨어서 ㅋㅋㅋ 성인되고서도 잘 안사먹는 것 같아요",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039185.1.16",
          "form": "ㅎㅎㅎ 저는 성인되서 먹어보고 제입맛에 맞으니",
          "original_form": "ㅎㅎㅎ 저는 성인되서 먹어보고 제입맛에 맞으니",
          "speaker_id": "2",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039185.1.17",
          "form": "제가 막 구워먹고 볶아먹고 그래요 ㅎ",
          "original_form": "제가 막 구워먹고 볶아먹고 그래요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039185.1.18",
          "form": "빵은 어떤거 좋아하세요?",
          "original_form": "빵은 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039185.1.19",
          "form": "저 소세지빵 피자빵 이런거 너무 좋아해요 사랑해요ㅠ",
          "original_form": "저 소세지빵 피자빵 이런거 너무 좋아해요 사랑해요ㅠ",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039185.1.20",
          "form": "아...그러시구낭",
          "original_form": "아...그러시구낭",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039185.1.21",
          "form": "그런것도 맛나죠 ㅎㅎㅎ",
          "original_form": "그런것도 맛나죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039185.1.22",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039185.1.23",
          "form": "저도 예전에는 소세지빵 피자빵 이런거 좋아했는데요",
          "original_form": "저도 예전에는 소세지빵 피자빵 이런거 좋아했는데요",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039185.1.24",
          "form": "요즘은 소보루같은거 곡물식빵 같은거 찾게 되요",
          "original_form": "요즘은 소보루같은거 곡물식빵 같은거 찾게 되요",
          "speaker_id": "2",
          "time": "20210903 22:06"
        }
      ]
