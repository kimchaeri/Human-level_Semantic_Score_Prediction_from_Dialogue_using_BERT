      "utterance": [
        {
          "id": "MDRW2100003785.1.1",
          "form": "그럼 어떤 날씨 좋아해?",
          "original_form": "그럼 어떤 날씨 좋아해?",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003785.1.2",
          "form": "선선한 봄 날씨가 좋지",
          "original_form": "선선한 봄 날씨가 좋지",
          "speaker_id": "1",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003785.1.3",
          "form": "선선하면 좋지",
          "original_form": "선선하면 좋지",
          "speaker_id": "2",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003785.1.4",
          "form": "덥고 추운 건 싫어",
          "original_form": "덥고 추운 건 싫어",
          "speaker_id": "1",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003785.1.5",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "2",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003785.1.6",
          "form": "그럼 여름하고 겨울 중에 어떤 계절이 나아?",
          "original_form": "그럼 여름하고 겨울 중에 어떤 계절이 나아?",
          "speaker_id": "2",
          "time": "20210821 12:04"
        },
        {
          "id": "MDRW2100003785.1.7",
          "form": "나는 겨울!",
          "original_form": "나는 겨울!",
          "speaker_id": "1",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003785.1.8",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003785.1.9",
          "form": "ㅋㅋㅋㅋ 그렇지",
          "original_form": "ㅋㅋㅋㅋ 그렇지",
          "speaker_id": "2",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003785.1.10",
          "form": "여름에는 너무 덥지",
          "original_form": "여름에는 너무 덥지",
          "speaker_id": "1",
          "time": "20210821 12:05"
        },
        {
          "id": "MDRW2100003785.1.11",
          "form": "나도 겨울이 차라리 나",
          "original_form": "나도 겨울이 차라리 나",
          "speaker_id": "2",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003785.1.12",
          "form": "겨울엔 껴입으면 되니까",
          "original_form": "겨울엔 껴입으면 되니까",
          "speaker_id": "1",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003785.1.13",
          "form": "껴입으면 따뜻하지",
          "original_form": "껴입으면 따뜻하지",
          "speaker_id": "2",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003785.1.14",
          "form": "지구온난화 때문에 점점 봄가을이 줄어들어서 슬퍼",
          "original_form": "지구온난화 때문에 점점 봄가을이 줄어들어서 슬퍼",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003785.1.15",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003785.1.16",
          "form": "환경보호 해야하는데",
          "original_form": "환경보호 해야하는데",
          "speaker_id": "1",
