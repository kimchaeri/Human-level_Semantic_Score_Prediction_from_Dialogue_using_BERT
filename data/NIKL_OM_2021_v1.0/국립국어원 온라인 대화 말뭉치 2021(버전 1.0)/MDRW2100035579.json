      "utterance": [
        {
          "id": "MDRW2100035579.1.1",
          "form": "아녀 저는 킹덤만 봤어요",
          "original_form": "아녀 저는 킹덤만 봤어요",
          "speaker_id": "2",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035579.1.2",
          "form": "아신전은 못보구요ㅜ",
          "original_form": "아신전은 못보구요ㅜ",
          "speaker_id": "2",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035579.1.3",
          "form": "킹덤 시리즈 아신빼고는",
          "original_form": "킹덤 시리즈 아신빼고는",
          "speaker_id": "1",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035579.1.4",
          "form": "다 보신건가용?",
          "original_form": "다 보신건가용?",
          "speaker_id": "1",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035579.1.5",
          "form": "네네 진짜 꿀잼!",
          "original_form": "네네 진짜 꿀잼!",
          "speaker_id": "2",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035579.1.6",
          "form": "킹덤이 제 인생 드라마라ㅋㅋㅋㅋ",
          "original_form": "킹덤이 제 인생 드라마라ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035579.1.7",
          "form": "넘 재밌어요",
          "original_form": "넘 재밌어요",
          "speaker_id": "1",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035579.1.8",
          "form": "좀비 무서워하는데 그건 괜찮았어요",
          "original_form": "좀비 무서워하는데 그건 괜찮았어요",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.9",
          "form": "엇 저두요ㅋㅋㅋ",
          "original_form": "엇 저두요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.10",
          "form": "살짝 잔인하긴 하지만...",
          "original_form": "살짝 잔인하긴 하지만...",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.11",
          "form": "ㅋㅋㅋ묘하게 좀 덜무서워요",
          "original_form": "ㅋㅋㅋ묘하게 좀 덜무서워요",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.12",
          "form": "대탈출은 보세용?",
          "original_form": "대탈출은 보세용?",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.13",
          "form": "젤 무서운건 그 왕!",
          "original_form": "젤 무서운건 그 왕!",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.14",
          "form": "아아 맞아요",
          "original_form": "아아 맞아요",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.15",
          "form": "저 대탈출은 티비에 해주면 가끔 봐요",
          "original_form": "저 대탈출은 티비에 해주면 가끔 봐요",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.16",
          "form": "그냥 이성은 없고 좀비의 본능만 남아있는ㅠ",
          "original_form": "그냥 이성은 없고 좀비의 본능만 남아있는ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.17",
          "form": "왕이 젤무서워요ㅜ",
          "original_form": "왕이 젤무서워요ㅜ",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100035579.1.18",
          "form": "대탈출 저번주꺼 재밌었어요!",
          "original_form": "대탈출 저번주꺼 재밌었어요!",
          "speaker_id": "1",
          "time": "20210829 20:41"
        }
      ]
    }
