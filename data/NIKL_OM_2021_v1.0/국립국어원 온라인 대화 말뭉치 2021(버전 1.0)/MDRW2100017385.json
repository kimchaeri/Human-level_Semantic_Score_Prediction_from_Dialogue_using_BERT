      "utterance": [
        {
          "id": "MDRW2100017385.1.1",
          "form": "최근 제일 잘 산 물건 있어요?",
          "original_form": "최근 제일 잘 산 물건 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017385.1.2",
          "form": "브리타요!",
          "original_form": "브리타요!",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017385.1.3",
          "form": "오 왜요?",
          "original_form": "오 왜요?",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017385.1.4",
          "form": "잘쓰고 있어서요",
          "original_form": "잘쓰고 있어서요",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017385.1.5",
          "form": "좀 귀찮진 않나요",
          "original_form": "좀 귀찮진 않나요",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017385.1.6",
          "form": "관심있던거라 궁금",
          "original_form": "관심있던거라 궁금",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017385.1.7",
          "form": "정수기도 써보고",
          "original_form": "정수기도 써보고",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017385.1.8",
          "form": "생수도 마셔봤는데",
          "original_form": "생수도 마셔봤는데",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017385.1.9",
          "form": "제일 덜귀찮았어요",
          "original_form": "제일 덜귀찮았어요",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017385.1.10",
          "form": "아 그렇군요 의외로 안 귀찮구나..참고할게요 고마워요",
          "original_form": "아 그렇군요 의외로 안 귀찮구나..참고할게요 고마워요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017385.1.11",
          "form": "사고 싶은 물건 있으세요?",
          "original_form": "사고 싶은 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017385.1.12",
          "form": "전 요즘 물욕이 없어서 장바구니도 다 비우고 있는 중인데",
          "original_form": "전 요즘 물욕이 없어서 장바구니도 다 비우고 있는 중인데",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017385.1.13",
          "form": "가을 오기 전에 가을 옷 장만이나 할까봐요",
          "original_form": "가을 오기 전에 가을 옷 장만이나 할까봐요",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017385.1.14",
          "form": "저도 요즘 먹을것만 사고있네요",
          "original_form": "저도 요즘 먹을것만 사고있네요",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017385.1.15",
          "form": "저두 앵겔지수 높아요",
          "original_form": "저두 앵겔지수 높아요",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017385.1.16",
          "form": "이마트에서 닭꼬치 샀는데 너무 맛있어요",
          "original_form": "이마트에서 닭꼬치 샀는데 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017385.1.17",
          "form": "닭고기 좋아하세요?",
          "original_form": "닭고기 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017385.1.18",
          "form": "좋아하는 건 돼지고긴데 나혼자산다에 나오는 거 보고 시켜먹은 뒤로 간간히 먹어요",
          "original_form": "좋아하는 건 돼지고긴데 나혼자산다에 나오는 거 보고 시켜먹은 뒤로 간간히 먹어요",
          "speaker_id": "1",
          "time": "20210826 19:38"
        }
      ]
