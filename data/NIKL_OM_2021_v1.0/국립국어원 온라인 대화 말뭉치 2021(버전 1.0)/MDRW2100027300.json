      "utterance": [
        {
          "id": "MDRW2100027300.1.1",
          "form": "동생도 컵 받았대?",
          "original_form": "동생도 컵 받았대?",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027300.1.2",
          "form": "그 미야베 미유키 알아요?",
          "original_form": "그 미야베 미유키 알아요?",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027300.1.3",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027300.1.4",
          "form": "동생 도서관에도 그 이벤트를 하는지 몰겠음",
          "original_form": "동생 도서관에도 그 이벤트를 하는지 몰겠음",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027300.1.5",
          "form": "미미여사 재밌음",
          "original_form": "미미여사 재밌음",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027300.1.6",
          "form": "무려 21년 5월31일 출판된소설 빌림",
          "original_form": "무려 21년 5월31일 출판된소설 빌림",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100027300.1.7",
          "form": "머빌림?",
          "original_form": "머빌림?",
          "speaker_id": "2",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027300.1.8",
          "form": "미미여사님 책이고",
          "original_form": "미미여사님 책이고",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027300.1.9",
          "form": "제목은 기타기타 사건부",
          "original_form": "제목은 기타기타 사건부",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027300.1.10",
          "form": "새책이야",
          "original_form": "새책이야",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027300.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027300.1.12",
          "form": "그분 신간인가",
          "original_form": "그분 신간인가",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027300.1.13",
          "form": "나도보고싶다",
          "original_form": "나도보고싶다",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027300.1.14",
          "form": "미미여사 현대물은 미야베 월드 제1막이고",
          "original_form": "미미여사 현대물은 미야베 월드 제1막이고",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027300.1.15",
          "form": "시대물은 2막이래요",
          "original_form": "시대물은 2막이래요",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027300.1.16",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027300.1.17",
          "form": "이건 시대물이고",
          "original_form": "이건 시대물이고",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027300.1.18",
          "form": "판타지물은 3막인가",
          "original_form": "판타지물은 3막인가",
          "speaker_id": "2",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027300.1.19",
          "form": "새로시작하신 시리즈임",
          "original_form": "새로시작하신 시리즈임",
          "speaker_id": "1",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027300.1.20",
          "form": "브레이브스토리는 애니 극장판도 있음",
          "original_form": "브레이브스토리는 애니 극장판도 있음",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027300.1.21",
          "form": "기타기타 시리즈",
          "original_form": "기타기타 시리즈",
          "speaker_id": "1",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027300.1.22",
          "form": "기타이치가 주인공임",
          "original_form": "기타이치가 주인공임",
          "speaker_id": "1",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027300.1.23",
          "form": "에도가 배경임",
          "original_form": "에도가 배경임",
          "speaker_id": "1",
