        },
        {
          "id": "MDRW2100023013.1.2",
          "form": "치킨 먹었어요^^;",
          "original_form": "치킨 먹었어요^^;",
          "speaker_id": "2",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100023013.1.3",
          "form": "name1은 뭐드셨나요~ㅎㅎ",
          "original_form": "&name1&은 뭐드셨나요~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100023013.1.4",
          "form": "맛있겠어요 ㅎ",
          "original_form": "맛있겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100023013.1.5",
          "form": "전 집밥먹고 복슝아먹었어요 ㅎ",
          "original_form": "전 집밥먹고 복슝아먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100023013.1.6",
          "form": "너무 짰어요!ㅜㅠ",
          "original_form": "너무 짰어요!ㅜㅠ",
          "speaker_id": "2",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023013.1.7",
          "form": "저도 엄마 집밥 먹고싶네요ㅜㅜㅋㅋ",
          "original_form": "저도 엄마 집밥 먹고싶네요ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023013.1.8",
          "form": "무슨메뉴였너요",
          "original_form": "무슨메뉴였너요",
          "speaker_id": "1",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023013.1.9",
          "form": "복숭아.",
          "original_form": "복숭아{emoji:♥}.{emoji:♥}",
          "speaker_id": "2",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023013.1.10",
          "form": "맞아요ㅠㅠ 엄마집밥이최고에요",
          "original_form": "맞아요ㅠㅠ 엄마집밥이최고에요",
          "speaker_id": "1",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023013.1.11",
          "form": "전생선구이를 좋아하는데 ㅋㅋㅋ제가 할자신은없고 엄마네가면 꼭구워주세요 ㅋㅋ",
          "original_form": "전생선구이를 좋아하는데 ㅋㅋㅋ제가 할자신은없고 엄마네가면 꼭구워주세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023013.1.12",
          "form": "교촌 레드콤보1/2+허니오리웨지감자세트였어요",
          "original_form": "교촌 레드콤보1/2+허니오리웨지감자세트였어요",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023013.1.13",
          "form": "2개나받아서 2번째 먹었는데",
          "original_form": "2개나받아서 2번째 먹었는데",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023013.1.14",
          "form": "둘다 너무 짜고짭니당",
          "original_form": "둘다 너무 짜고짭니당",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023013.1.15",
          "form": "그런후기많더라구요~",
          "original_form": "그런후기많더라구요~",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023013.1.16",
          "form": "에고ㅠ",
          "original_form": "에고ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023013.1.17",
          "form": "소스가좀과햇나보ㅏ요",
          "original_form": "소스가좀과햇나보ㅏ요",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023013.1.18",
          "form": "저도 생선구이는 ....ㅎㅎㅎ",
          "original_form": "저도 생선구이는 ....ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023013.1.19",
          "form": "그런가봐요 요즘 배달음식 다 짜서.ㅠㅠ",
          "original_form": "그런가봐요 요즘 배달음식 다 짜서.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023013.1.20",
          "form": "생선은 항상  오븐이나 에어프라이어에 돌려요",
          "original_form": "생선은 항상  오븐이나 에어프라이어에 돌려요",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023013.1.21",
          "form": "요즘엔 안한지 꽤 됐네요",
          "original_form": "요즘엔 안한지 꽤 됐네요",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.22",
          "form": "전 웬만하면 사지도 않아요ㅠ",
          "original_form": "전 웬만하면 사지도 않아요ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.23",
          "form": "그래도 프라이팬에 굽는게 맛나더라구요~",
          "original_form": "그래도 프라이팬에 굽는게 맛나더라구요~",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.24",
          "form": "저도용ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도용ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.25",
          "form": "사면 후회 ㅋㅋㅋ",
          "original_form": "사면 후회 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.26",
          "form": "저도저도 ㅋㅋ  생선은..어려운것",
          "original_form": "저도저도 ㅋㅋ  생선은..어려운것",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.27",
          "form": "뱐찬가게에서 생선구워서파능거 가끔사요",
          "original_form": "뱐찬가게에서 생선구워서파능거 가끔사요",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.28",
          "form": "저도 그렇게 구워파는거 사거나",
          "original_form": "저도 그렇게 구워파는거 사거나",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.29",
          "form": "완제품으로 나와서",
          "original_form": "완제품으로 나와서",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.30",
          "form": "고등어 4~5천원인데",
          "original_form": "고등어 4~5천원인데",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.31",
          "form": "전자렌지에 돌리면 되는거 그런거 사서 돌렸어요",
          "original_form": "전자렌지에 돌리면 되는거 그런거 사서 돌렸어요",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.32",
          "form": "제가 한마리사서 집에서 귭는게 더 비싸게 먹힐꺼같아요 ㅋㅋㅋ",
          "original_form": "제가 한마리사서 집에서 귭는게 더 비싸게 먹힐꺼같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023013.1.33",
          "form": "기름이랑 다치우려면.. ㅋㅋㅋㅋㅋ",
          "original_form": "기름이랑 다치우려면.. ㅋㅋㅋㅋㅋ",
