      "utterance": [
        {
          "id": "MDRW2100033735.1.1",
          "form": "지구 나이트도 달리셨나요?",
          "original_form": "지구 나이트도 달리셨나요?",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100033735.1.2",
          "form": "아뇨 지마켓은 딱히 살게없어서ㅎ",
          "original_form": "아뇨 지마켓은 딱히 살게없어서ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100033735.1.3",
          "form": "12시 뭐할거있을까요?",
          "original_form": "12시 뭐할거있을까요?",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033735.1.4",
          "form": "정신없을시간인데",
          "original_form": "정신없을시간인데",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033735.1.5",
          "form": "위메프!!!",
          "original_form": "위메프!!!",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033735.1.6",
          "form": "옥션 렌탈해야되는데 안뜨네요",
          "original_form": "옥션 렌탈해야되는데 안뜨네요",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100033735.1.7",
          "form": "아ㅠㅠ 위메프의배신",
          "original_form": "아ㅠㅠ 위메프의배신",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100033735.1.8",
          "form": "없나요?ㅎ",
          "original_form": "없나요?ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100033735.1.9",
          "form": "피자마루 떡볶이라니",
          "original_form": "피자마루 떡볶이라니",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100033735.1.10",
          "form": "에잇 패스요",
          "original_form": "에잇 패스요",
          "speaker_id": "1",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100033735.1.11",
          "form": "그니까요ㅠㅠ",
          "original_form": "그니까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033735.1.12",
          "form": "카페베네0원요",
          "original_form": "카페베네0원요",
          "speaker_id": "1",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033735.1.13",
          "form": "고고",
          "original_form": "고고",
          "speaker_id": "1",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033735.1.14",
          "form": "꺄 샀어욤",
          "original_form": "꺄 샀어욤",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033735.1.15",
          "form": "이것도혹시 앱에넣어주려나ㅜ",
          "original_form": "이것도혹시 앱에넣어주려나ㅜ",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100033735.1.16",
          "form": "그냥 문자오는거아니예요?",
          "original_form": "그냥 문자오는거아니예요?",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100033735.1.17",
          "form": "헉 윙잇 쿠폰만료라는데 확인하고올게요ㅡㅡ",
          "original_form": "헉 윙잇 쿠폰만료라는데 확인하고올게요ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100033735.1.18",
          "form": "아니에요ㅠ카페베네 앱으로 들어와요",
          "original_form": "아니에요ㅠ카페베네 앱으로 들어와요",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100033735.1.19",
          "form": "막은건가",
          "original_form": "막은건가",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100033735.1.20",
          "form": "엇 저앱없는데",
          "original_form": "엇 저앱없는데",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100033735.1.21",
          "form": "헉ㅠ네ㅠㅠ",
          "original_form": "헉ㅠ네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033735.1.22",
          "form": "쿠폰은 아직살아있네요ㅎ 첫구매쿠폰인듯한데 적욤될지",
          "original_form": "쿠폰은 아직살아있네요ㅎ 첫구매쿠폰인듯한데 적욤될지",
          "speaker_id": "1",
          "time": "20210825 00:10"
        }
      ]
