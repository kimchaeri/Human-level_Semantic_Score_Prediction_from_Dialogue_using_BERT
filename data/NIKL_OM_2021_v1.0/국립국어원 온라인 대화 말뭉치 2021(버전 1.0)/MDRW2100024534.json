      "utterance": [
        {
          "id": "MDRW2100024534.1.1",
          "form": "아 퍼즐게임을 하시는군요!!",
          "original_form": "아 퍼즐게임을 하시는군요!!",
          "speaker_id": "2",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100024534.1.2",
          "form": "저도 예전에 많이 했었어요",
          "original_form": "저도 예전에 많이 했었어요",
          "speaker_id": "2",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100024534.1.3",
          "form": "그런데 점점 머리쓰는게 싫어지더라구요 ㅋㅋㅋㅋ",
          "original_form": "그런데 점점 머리쓰는게 싫어지더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:27"
        },
        {
          "id": "MDRW2100024534.1.4",
          "form": "닌텐도에서도 두뇌트레이닝 재밌게 했었는데...",
          "original_form": "닌텐도에서도 두뇌트레이닝 재밌게 했었는데...",
          "speaker_id": "2",
          "time": "20210831 13:27"
        },
        {
          "id": "MDRW2100024534.1.5",
          "form": "저도 두뇌트레이닝 했었는데ㅋㅋ",
          "original_form": "저도 두뇌트레이닝 했었는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:09"
        },
        {
          "id": "MDRW2100024534.1.6",
          "form": "안경쓴 아저씨 얼굴 나오는거 맞죠?",
          "original_form": "안경쓴 아저씨 얼굴 나오는거 맞죠?",
          "speaker_id": "1",
          "time": "20210831 14:09"
        },
        {
          "id": "MDRW2100024534.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 맞아요! 들으니까 생각나네요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맞아요! 들으니까 생각나네요!!",
          "speaker_id": "2",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100024534.1.8",
          "form": "조금이라도 젊게 나오려고 애를썼는데...쉽지 않았죠",
          "original_form": "조금이라도 젊게 나오려고 애를썼는데...쉽지 않았죠",
          "speaker_id": "2",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100024534.1.9",
          "form": "저는 그때 너무 어려서 못푸는 바람에 나이들게 나왔었어요ㅋㅋ",
          "original_form": "저는 그때 너무 어려서 못푸는 바람에 나이들게 나왔었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100024534.1.10",
          "form": "아 그런거였으려나요??",
          "original_form": "아 그런거였으려나요??",
          "speaker_id": "2",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100024534.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 저는 닌텐도 마리오게임도 많이 했어요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저는 닌텐도 마리오게임도 많이 했어요!!",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024534.1.12",
          "form": "마리오는 기본이죠ㅋㅋ 저는 커비 주로 했어요",
          "original_form": "마리오는 기본이죠ㅋㅋ 저는 커비 주로 했어요",
          "speaker_id": "1",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024534.1.13",
          "form": "커비도 좋아했어요!!",
          "original_form": "커비도 좋아했어요!!",
          "speaker_id": "2",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100024534.1.14",
          "form": "커비가 닌텐도겜 중에서는 젤 쉬웠죠",
          "original_form": "커비가 닌텐도겜 중에서는 젤 쉬웠죠",
          "speaker_id": "1",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100024534.1.15",
          "form": "슈퍼마리오가 워낙 어려워서ㅜ",
          "original_form": "슈퍼마리오가 워낙 어려워서ㅜ",
          "speaker_id": "2",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100024534.1.16",
          "form": "혹시 그려라터치 내가만드는 세상 아세요??",
          "original_form": "혹시 그려라터치 내가만드는 세상 아세요??",
          "speaker_id": "2",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100024534.1.17",
          "form": "헐 저 엄청 좋아했어요",
          "original_form": "헐 저 엄청 좋아했어요",
          "speaker_id": "1",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100024534.1.18",
          "form": "좋아하셨다니! ㅋㅋㅋㅋ 뭔가 마이너 게임인 줄 알았는데 반갑네요",
          "original_form": "좋아하셨다니! ㅋㅋㅋㅋ 뭔가 마이너 게임인 줄 알았는데 반갑네요",
          "speaker_id": "2",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100024534.1.19",
          "form": "저는 그 옷이 깨질때마다 캐릭터가 참 불쌍하더라고요...?",
          "original_form": "저는 그 옷이 깨질때마다 캐릭터가 참 불쌍하더라고요...?",
          "speaker_id": "2",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100024534.1.20",
          "form": "저도 그 게임 아는 분 처음봐요",
          "original_form": "저도 그 게임 아는 분 처음봐요",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024534.1.21",
          "form": "그것도 어렸을때 해서 검은 친구 엄청 무서워했었는데",
          "original_form": "그것도 어렸을때 해서 검은 친구 엄청 무서워했었는데",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024534.1.22",
          "form": "옷깨지는 것도 아깝고",
          "original_form": "옷깨지는 것도 아깝고",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024534.1.23",
          "form": "엇!! 저도 뭔가 무서웠다는 기억이 있어요",
          "original_form": "엇!! 저도 뭔가 무서웠다는 기억이 있어요",
          "speaker_id": "2",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100024534.1.24",
          "form": "좀 섬뜩한 느낌...?",
          "original_form": "좀 섬뜩한 느낌...?",
          "speaker_id": "2",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100024534.1.25",
          "form": "맞아요 분명 배경은 밝고 예쁜데 말이죠",
          "original_form": "맞아요 분명 배경은 밝고 예쁜데 말이죠",
          "speaker_id": "1",
          "time": "20210831 14:51"
        }
      ]
    }
