      "utterance": [
        {
          "id": "MDRW2100017830.1.1",
          "form": "혹시 공연 자주 보세요?",
          "original_form": "혹시 공연 자주 보세요?",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017830.1.2",
          "form": "공연은 큰공연은 본적없는것같아요 아마? 소극장공연은 몇번본적있어요",
          "original_form": "공연은 큰공연은 본적없는것같아요 아마? 소극장공연은 몇번본적있어요",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017830.1.3",
          "form": "아 저도 소극장 좋아해요",
          "original_form": "아 저도 소극장 좋아해요",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017830.1.4",
          "form": "큰 공연은 비싸서 잘 못가요 ㅠㅠ",
          "original_form": "큰 공연은 비싸서 잘 못가요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017830.1.5",
          "form": "친구들이랑 갔었는데 처음봤을때 정말 가까워서 신기하고 재미있더라구요",
          "original_form": "친구들이랑 갔었는데 처음봤을때 정말 가까워서 신기하고 재미있더라구요",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017830.1.6",
          "form": "마지막 공연이 언제세요?",
          "original_form": "마지막 공연이 언제세요?",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017830.1.7",
          "form": "맞아요 ㅎㅎ 목소리가 쩌렁쩌렁!",
          "original_form": "맞아요 ㅎㅎ 목소리가 쩌렁쩌렁!",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017830.1.8",
          "form": "엄청 신기했었어요",
          "original_form": "엄청 신기했었어요",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017830.1.9",
          "form": "저는 좀 본지 오래됐어요 몇년전인지 가물가물할정도 ㅋㅋㅋㅋ",
          "original_form": "저는 좀 본지 오래됐어요 몇년전인지 가물가물할정도 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017830.1.10",
          "form": "무슨 공연이었어요?",
          "original_form": "무슨 공연이었어요?",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017830.1.11",
          "form": "사실 연극제목이 기억이 잘 안나는데 보잉보잉이었나...이 제목밖에 생각이안나네요",
          "original_form": "사실 연극제목이 기억이 잘 안나는데 보잉보잉이었나...이 제목밖에 생각이안나네요",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017830.1.12",
          "form": "ㅎㅎ 귀여운 제목이네요",
          "original_form": "ㅎㅎ 귀여운 제목이네요",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017830.1.13",
          "form": "보신것중에서 어떤게 재미있으셨나요?",
          "original_form": "보신것중에서 어떤게 재미있으셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017830.1.14",
          "form": "ㅋㅋㅋㅋ 제목이 귀엽죠 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 제목이 귀엽죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017830.1.15",
          "form": "참여형 연극이요",
          "original_form": "참여형 연극이요",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017830.1.16",
          "form": "제목은 저도 기억 안나는데",
          "original_form": "제목은 저도 기억 안나는데",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017830.1.17",
          "form": "우와 ㅋㅋㅋ",
          "original_form": "우와 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017830.1.18",
          "form": "관객참여형이어서 계속 무대마다 줄거리가 바뀌어요",
          "original_form": "관객참여형이어서 계속 무대마다 줄거리가 바뀌어요",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017830.1.19",
          "form": "참여형이라니 신기해요 재미있었겠어요",
          "original_form": "참여형이라니 신기해요 재미있었겠어요",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017830.1.20",
          "form": "아 제목 생각났다",
          "original_form": "아 제목 생각났다",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017830.1.21",
          "form": "주인공은 바로 너",
          "original_form": "주인공은 바로 너",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017830.1.22",
          "form": "이거일거에요",
          "original_form": "이거일거에요",
          "speaker_id": "1",
          "time": "20210826 21:26"
        }
      ]
    }
