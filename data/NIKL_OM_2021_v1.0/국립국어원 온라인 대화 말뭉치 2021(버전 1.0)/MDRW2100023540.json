      "utterance": [
        {
          "id": "MDRW2100023540.1.1",
          "form": "최근에 영화 보신거 있으세요?",
          "original_form": "최근에 영화 보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210830 11:14"
        },
        {
          "id": "MDRW2100023540.1.2",
          "form": "크루엘라를 가장 최근에 봤어요!",
          "original_form": "크루엘라를 가장 최근에 봤어요!",
          "speaker_id": "2",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100023540.1.3",
          "form": "크루엘라요?",
          "original_form": "크루엘라요?",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023540.1.4",
          "form": "첨들어봐요",
          "original_form": "첨들어봐요",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023540.1.5",
          "form": "무슨영화에요?",
          "original_form": "무슨영화에요?",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023540.1.6",
          "form": "디즈니에서 만든 실사화 영화예요!",
          "original_form": "디즈니에서 만든 실사화 영화예요!",
          "speaker_id": "2",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100023540.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023540.1.8",
          "form": "잼있겠는데요?",
          "original_form": "잼있겠는데요?",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023540.1.9",
          "form": "재밌게봤어요",
          "original_form": "재밌게봤어요",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023540.1.10",
          "form": "전 마블영화 보고싶은데",
          "original_form": "전 마블영화 보고싶은데",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023540.1.11",
          "form": "극장을 못가니...",
          "original_form": "극장을 못가니...",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100023540.1.12",
          "form": "12월에 스파이더맨 개봉하는거 아시나요??",
          "original_form": "12월에 스파이더맨 개봉하는거 아시나요??",
          "speaker_id": "2",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023540.1.13",
          "form": "네 알아요",
          "original_form": "네 알아요",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023540.1.14",
          "form": "완전기대중",
          "original_form": "완전기대중",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023540.1.15",
          "form": "닥터스트레인지 나온대요",
          "original_form": "닥터스트레인지 나온대요",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023540.1.16",
          "form": "스파이더맨 자체의 서사를 원하지만 판권 때문데 어쩔 수 었다더라구요ㅠ",
          "original_form": "스파이더맨 자체의 서사를 원하지만 판권 때문데 어쩔 수 었다더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100023540.1.17",
          "form": "아 그런거에요?",
          "original_form": "아 그런거에요?",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100023540.1.18",
          "form": "이번주에 샹치?",
          "original_form": "이번주에 샹치?",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100023540.1.19",
          "form": "새로운 영웅물 개봉하던데",
          "original_form": "새로운 영웅물 개봉하던데",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100023540.1.20",
          "form": "그건 조금 기다렸다 평을 보려구요..",
          "original_form": "그건 조금 기다렸다 평을 보려구요..",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100023540.1.21",
          "form": "전 이번주에 보러가볼까 고민중입니다",
          "original_form": "전 이번주에 보러가볼까 고민중입니다",
          "speaker_id": "1",
          "time": "20210830 21:37"
        }
      ]
