      "utterance": [
        {
          "id": "MDRW2100010077.1.1",
          "form": "모임 좋아하세요?",
          "original_form": "모임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010077.1.2",
          "form": "친목은 좋아하는데 딱히 하는 활동은 없어요",
          "original_form": "친목은 좋아하는데 딱히 하는 활동은 없어요",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010077.1.3",
          "form": "모임이나 동호회해보신적 있나요?",
          "original_form": "모임이나 동호회해보신적 있나요?",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010077.1.4",
          "form": "저는 뭐 동기 모임?ㅋㅋ",
          "original_form": "저는 뭐 동기 모임?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100010077.1.5",
          "form": "어릴땐 학교에서 축구 소모임!",
          "original_form": "어릴땐 학교에서 축구 소모임!",
          "speaker_id": "2",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100010077.1.6",
          "form": "아 동기모임ㅎ 아직도하세요?",
          "original_form": "아 동기모임ㅎ 아직도하세요?",
          "speaker_id": "1",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100010077.1.7",
          "form": "졸업하니 먹고살기바빠서 흐지부지되던데",
          "original_form": "졸업하니 먹고살기바빠서 흐지부지되던데",
          "speaker_id": "1",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100010077.1.8",
          "form": "아 회사동기입니다 ㅎㅎ",
          "original_form": "아 회사동기입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100010077.1.9",
          "form": "대학은 뭐 몇명 연락 안하죠",
          "original_form": "대학은 뭐 몇명 연락 안하죠",
          "speaker_id": "2",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100010077.1.10",
          "form": "대학동기인줄 알았네요ㅎ",
          "original_form": "대학동기인줄 알았네요ㅎ",
          "speaker_id": "1",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100010077.1.11",
          "form": "취미로 하나 모임활동 함 좋을거같은데 취미가없으니 문제",
          "original_form": "취미로 하나 모임활동 함 좋을거같은데 취미가없으니 문제",
          "speaker_id": "1",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100010077.1.12",
          "form": "근데 요새 시국이 이래서ㅠ",
          "original_form": "근데 요새 시국이 이래서ㅠ",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010077.1.13",
          "form": "그러게요 친구약속도 다취소하는 상황이라",
          "original_form": "그러게요 친구약속도 다취소하는 상황이라",
          "speaker_id": "1",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010077.1.14",
          "form": "동기모임도 요즘은 못하시겠네요",
          "original_form": "동기모임도 요즘은 못하시겠네요",
          "speaker_id": "1",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010077.1.15",
          "form": "네 맞아요 진짜 가끔 겨우 얼굴만ㅠ",
          "original_form": "네 맞아요 진짜 가끔 겨우 얼굴만ㅠ",
          "speaker_id": "2",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100010077.1.16",
          "form": "동기모임이면 대략 몇명이예요?",
          "original_form": "동기모임이면 대략 몇명이예요?",
          "speaker_id": "1",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010077.1.17",
          "form": "원래는 한 넷씩해서 모임 서너개 있었는데",
          "original_form": "원래는 한 넷씩해서 모임 서너개 있었는데",
          "speaker_id": "2",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010077.1.18",
          "form": "요즘은 한두명씩 모여서ㅠㅠ",
          "original_form": "요즘은 한두명씩 모여서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010077.1.19",
          "form": "모임인데 한두명이면 그건 모임이아닌듯ㅎㅎ",
          "original_form": "모임인데 한두명이면 그건 모임이아닌듯ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010077.1.20",
          "form": "소모임에 속하려나요",
          "original_form": "소모임에 속하려나요",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010077.1.21",
          "form": "한 넷정도가",
          "original_form": "한 넷정도가",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100010077.1.22",
          "form": "소모임이지 싶습니다",
          "original_form": "소모임이지 싶습니다",
          "speaker_id": "2",
          "time": "20210823 16:39"
        }
      ]
    }
