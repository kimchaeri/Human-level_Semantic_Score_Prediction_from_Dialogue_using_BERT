      "utterance": [
        {
          "id": "MDRW2100042786.1.1",
          "form": "가족모두 제주도로가셨나요?",
          "original_form": "가족모두 제주도로가셨나요?",
          "speaker_id": "1",
          "time": "20210907 09:04"
        },
        {
          "id": "MDRW2100042786.1.2",
          "form": "저는 혼자 제주살이중인데",
          "original_form": "저는 혼자 제주살이중인데",
          "speaker_id": "2",
          "time": "20210907 09:04"
        },
        {
          "id": "MDRW2100042786.1.3",
          "form": "밥 먹기가 심심해요",
          "original_form": "밥 먹기가 심심해요",
          "speaker_id": "2",
          "time": "20210907 09:04"
        },
        {
          "id": "MDRW2100042786.1.4",
          "form": "혼자요?",
          "original_form": "혼자요?",
          "speaker_id": "1",
          "time": "20210907 09:04"
        },
        {
          "id": "MDRW2100042786.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.6",
          "form": "미혼이신가보네요",
          "original_form": "미혼이신가보네요",
          "speaker_id": "1",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.8",
          "form": "서울살다가 제주로 온지 3주",
          "original_form": "서울살다가 제주로 온지 3주",
          "speaker_id": "2",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.9",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.10",
          "form": "되었어요",
          "original_form": "되었어요",
          "speaker_id": "2",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.11",
          "form": "하하 주부신가요",
          "original_form": "하하 주부신가요",
          "speaker_id": "2",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.12",
          "form": "일때문에요?",
          "original_form": "일때문에요?",
          "speaker_id": "1",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.13",
          "form": "아뇨 프리랜서예여",
          "original_form": "아뇨 프리랜서예여",
          "speaker_id": "2",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.14",
          "form": "아님 그냥 쉬시러 가셨나요?",
          "original_form": "아님 그냥 쉬시러 가셨나요?",
          "speaker_id": "1",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042786.1.15",
          "form": "어떤일 프리랜서세요?",
          "original_form": "어떤일 프리랜서세요?",
          "speaker_id": "1",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042786.1.16",
          "form": "내년 3월까지 집은 계약했어요",
          "original_form": "내년 3월까지 집은 계약했어요",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042786.1.17",
          "form": "기획하는일이요",
          "original_form": "기획하는일이요",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042786.1.18",
          "form": "아..그렇군요",
          "original_form": "아..그렇군요",
          "speaker_id": "1",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042786.1.19",
          "form": "가족은 몇분같이 사세요",
          "original_form": "가족은 몇분같이 사세요",
          "speaker_id": "2",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100042786.1.20",
          "form": "세상에다양한직업이 많네요",
          "original_form": "세상에다양한직업이 많네요",
          "speaker_id": "1",
          "time": "20210907 09:07"
        }
      ]
