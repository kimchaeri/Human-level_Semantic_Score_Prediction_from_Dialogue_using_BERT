      "utterance": [
        {
          "id": "MDRW2100024678.1.1",
          "form": "오늘 비가오네요ㅜㅜ",
          "original_form": "오늘 비가오네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024678.1.2",
          "form": "따뜻한 북엇국 먹었는데 뜨듯하고 맑은국물이 좋아요 ㅎㅎ",
          "original_form": "따뜻한 북엇국 먹었는데 뜨듯하고 맑은국물이 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024678.1.3",
          "form": "오늘 저녁은 뭐 드세요?",
          "original_form": "오늘 저녁은 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024678.1.4",
          "form": "아직 저녁을 못먹어서 지금 고민중이예요!!",
          "original_form": "아직 저녁을 못먹어서 지금 고민중이예요!!",
          "speaker_id": "2",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100024678.1.5",
          "form": "혹시 메뉴 추천 가능하신가요",
          "original_form": "혹시 메뉴 추천 가능하신가요",
          "speaker_id": "2",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100024678.1.6",
          "form": "뭔가 간단하게 너무 배부르지않게 먹고싶네요 ㅎㅎ",
          "original_form": "뭔가 간단하게 너무 배부르지않게 먹고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100024678.1.7",
          "form": "저는 지금 로제짬뽕 먹고있어요!!!!",
          "original_form": "저는 지금 로제짬뽕 먹고있어요!!!!",
          "speaker_id": "1",
          "time": "20210831 19:51"
        },
        {
          "id": "MDRW2100024678.1.8",
          "form": "니뽕내뽕 추천합니다 ㅋㅋㅋㅋ",
          "original_form": "니뽕내뽕 추천합니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:51"
        },
        {
          "id": "MDRW2100024678.1.9",
          "form": "너무맛있어옼ㅋㅋㅋ",
          "original_form": "너무맛있어옼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100024678.1.10",
          "form": "오오오 짬뽕!!!!",
          "original_form": "오오오 짬뽕!!!!",
          "speaker_id": "2",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100024678.1.11",
          "form": "저희동네에는 근데 니뽕내뽕이 없네요ㅠㅠ 까비",
          "original_form": "저희동네에는 근데 니뽕내뽕이 없네요ㅠㅠ 까비",
          "speaker_id": "2",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100024678.1.12",
          "form": "요즘근데 로제가 유행인가봐요 로제짬뽕이라니!!!",
          "original_form": "요즘근데 로제가 유행인가봐요 로제짬뽕이라니!!!",
          "speaker_id": "2",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100024678.1.13",
          "form": "생각만해도 맛있을것같네요",
          "original_form": "생각만해도 맛있을것같네요",
          "speaker_id": "2",
          "time": "20210831 19:53"
        },
        {
          "id": "MDRW2100024678.1.14",
          "form": "예전부터 있던 메뉸데 진~~짜 오랜만에 먹어요 ㅎㅎㅎ!!!",
          "original_form": "예전부터 있던 메뉸데 진~~짜 오랜만에 먹어요 ㅎㅎㅎ!!!",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024678.1.15",
          "form": "아아ㅠㅠ 없다니 아쉽..",
          "original_form": "아아ㅠㅠ 없다니 아쉽..",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024678.1.16",
          "form": "아마 비슷한 매장은 있을것 같은데 없을까요ㅠ",
          "original_form": "아마 비슷한 매장은 있을것 같은데 없을까요ㅠ",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100024678.1.17",
          "form": "아쉬운대로",
          "original_form": "아쉬운대로",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024678.1.18",
          "form": "매운로제파스타 시켰어요 ㅎㅎ",
          "original_form": "매운로제파스타 시켰어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024678.1.19",
          "form": "나중에 니뽕내뽕가면 꼭 먹어야겠네용",
          "original_form": "나중에 니뽕내뽕가면 꼭 먹어야겠네용",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024678.1.20",
          "form": "꺅 매운로제파스타...",
          "original_form": "꺅 매운로제파스타...",
          "speaker_id": "1",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024678.1.21",
          "form": "벌써부터 속이 뜨거워집니다 ㅋㅋㅋ",
          "original_form": "벌써부터 속이 뜨거워집니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024678.1.22",
          "form": "어디서 주문하셨어요?",
          "original_form": "어디서 주문하셨어요?",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024678.1.23",
          "form": "괴짜쉐프파스타라고 체인점인거같은데",
          "original_form": "괴짜쉐프파스타라고 체인점인거같은데",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024678.1.24",
          "form": "아실랑가 모르겠네용",
          "original_form": "아실랑가 모르겠네용",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024678.1.25",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024678.1.26",
          "form": "얼른 먹고 오늘은 일찍 자야겠어요",
          "original_form": "얼른 먹고 오늘은 일찍 자야겠어요",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024678.1.27",
          "form": "앗 그맛을 음미하셔야죠!! 피곤하신가봐요ㅜㅎㅎ",
          "original_form": "앗 그맛을 음미하셔야죠!! 피곤하신가봐요ㅜㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100024678.1.28",
          "form": "넹 아직 화요일밖에 안됐는데도 피곤하네요ㅠㅠ",
          "original_form": "넹 아직 화요일밖에 안됐는데도 피곤하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:34"
        },
        {
          "id": "MDRW2100024678.1.29",
          "form": "얼른 배달이 왔음 좋겠네요 ㅎㅎ",
          "original_form": "얼른 배달이 왔음 좋겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:34"
        },
        {
          "id": "MDRW2100024678.1.30",
          "form": "아아 몇분걸린다고 떴어요? ㅜㅜ 언넝와야할텐데",
          "original_form": "아아 몇분걸린다고 떴어요? ㅜㅜ 언넝와야할텐데",
          "speaker_id": "1",
          "time": "20210831 20:35"
        },
        {
          "id": "MDRW2100024678.1.31",
          "form": "아직 30분정도 남았다는데",
          "original_form": "아직 30분정도 남았다는데",
          "speaker_id": "2",
          "time": "20210831 20:37"
        },
        {
          "id": "MDRW2100024678.1.32",
          "form": "매번 이렇게 써놓고 더 일찍오더라구요",
          "original_form": "매번 이렇게 써놓고 더 일찍오더라구요",
          "speaker_id": "2",
          "time": "20210831 20:37"
        },
        {
          "id": "MDRW2100024678.1.33",
          "form": "맞아요 ㅋㅋㅋ기대감을 덜으려고 그런가 ㅎㅎ",
          "original_form": "맞아요 ㅋㅋㅋ기대감을 덜으려고 그런가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:39"
        },
        {
          "id": "MDRW2100024678.1.34",
          "form": "근데 비가와서 좀 걸릴것 같기는 하네요ㅠㅠ",
          "original_form": "근데 비가와서 좀 걸릴것 같기는 하네요ㅠㅠ",
