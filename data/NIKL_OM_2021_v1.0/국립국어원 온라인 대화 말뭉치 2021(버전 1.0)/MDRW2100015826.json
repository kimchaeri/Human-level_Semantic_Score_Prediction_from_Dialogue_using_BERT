      "utterance": [
        {
          "id": "MDRW2100015826.1.1",
          "form": "이제 비가 안올까요?",
          "original_form": "이제 비가 안올까요?",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015826.1.2",
          "form": "지금 비오네요ㅜ",
          "original_form": "지금 비오네요ㅜ",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015826.1.3",
          "form": "비와요? 방에 있었더니 비가 오는줄도 몰랐네여 ㅠ",
          "original_form": "비와요? 방에 있었더니 비가 오는줄도 몰랐네여 ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015826.1.4",
          "form": "너무습해서 제습기 틀었어요",
          "original_form": "너무습해서 제습기 틀었어요",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015826.1.5",
          "form": "제습기 틀면 너무 신기해요ㅋㅋ 물이 가득 차는게ㅋㅋ",
          "original_form": "제습기 틀면 너무 신기해요ㅋㅋ 물이 가득 차는게ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015826.1.6",
          "form": "비좀 그만오면 좋겠어요",
          "original_form": "비좀 그만오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015826.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100015826.1.8",
          "form": "부산에 홍수 났다네요",
          "original_form": "부산에 홍수 났다네요",
          "speaker_id": "1",
          "time": "20210826 01:40"
        },
        {
          "id": "MDRW2100015826.1.9",
          "form": "매번 왜그러나 모르겠어요",
          "original_form": "매번 왜그러나 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 01:40"
        },
        {
          "id": "MDRW2100015826.1.10",
          "form": "10년 뒤면 잠긴대요",
          "original_form": "10년 뒤면 잠긴대요",
          "speaker_id": "1",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015826.1.11",
          "form": "그런말 돌던데 무섭네요ㅜㅜ",
          "original_form": "그런말 돌던데 무섭네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015826.1.12",
          "form": "집값도 비싼데ㅋㅋ",
          "original_form": "집값도 비싼데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015826.1.13",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100015826.1.14",
          "form": "전 겨울이요!",
          "original_form": "전 겨울이요!",
          "speaker_id": "2",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100015826.1.15",
          "form": "롱패딩입고 돌아당기고",
          "original_form": "롱패딩입고 돌아당기고",
          "speaker_id": "2",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100015826.1.16",
          "form": "뜨끈뜨끈 보일러켜놓고",
          "original_form": "뜨끈뜨끈 보일러켜놓고",
          "speaker_id": "2",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100015826.1.17",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "2",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015826.1.18",
          "form": "귤까먹으면 천국이죠",
          "original_form": "귤까먹으면 천국이죠",
          "speaker_id": "1",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015826.1.19",
          "form": "맞아용ㅋㅋ여름은 돌아다니기 넘 힘들어요",
          "original_form": "맞아용ㅋㅋ여름은 돌아다니기 넘 힘들어요",
          "speaker_id": "2",
          "time": "20210826 01:53"
        }
      ]
