      "utterance": [
        {
          "id": "MDRW2100031627.1.1",
          "form": "나두 조아",
          "original_form": "나두 조아",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031627.1.2",
          "form": "오늘 집에 손님와서",
          "original_form": "오늘 집에 손님와서",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031627.1.3",
          "form": "마쉿는 냄새가 가득해서",
          "original_form": "마쉿는 냄새가 가득해서",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031627.1.4",
          "form": "name3가 발광했어",
          "original_form": "&name3&가 발광했어",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031627.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ마쉿는 냄새 가득",
          "original_form": "ㅋㅋㅋㅋㅋㅋ마쉿는 냄새 가득",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031627.1.6",
          "form": "이거슨 name3님을 고문하는거시여",
          "original_form": "이거슨 &name3&님을 고문하는거시여",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031627.1.7",
          "form": "맛난 냄새를 그렇게 잔뜩 피웠음 특별간식이라도 줬어야지 ㅋㅋ",
          "original_form": "맛난 냄새를 그렇게 잔뜩 피웠음 특별간식이라도 줬어야지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031627.1.8",
          "form": "아우 줬지 당근 줬지",
          "original_form": "아우 줬지 당근 줬지",
          "speaker_id": "2",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031627.1.9",
          "form": "그리고 늦은 밤에 산책도 델꼬 다녀왔다구요",
          "original_form": "그리고 늦은 밤에 산책도 델꼬 다녀왔다구요",
          "speaker_id": "2",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031627.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 욕심많은 강아지는 그정도로 만족 못하는거에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 욕심많은 강아지는 그정도로 만족 못하는거에요",
          "speaker_id": "1",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031627.1.11",
          "form": "그치만 회나 탕슉을 줄 수는 없었던거예요",
          "original_form": "그치만 회나 탕슉을 줄 수는 없었던거예요",
          "speaker_id": "2",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031627.1.12",
          "form": "머...소시지 먹고 만족했는지",
          "original_form": "머...소시지 먹고 만족했는지",
          "speaker_id": "2",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031627.1.13",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031627.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031627.1.15",
          "form": "조용해지더라구",
          "original_form": "조용해지더라구",
          "speaker_id": "2",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031627.1.16",
          "form": "니들만 맛잇는거먹냐!",
          "original_form": "니들만 맛잇는거먹냐!",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.17",
          "form": "나도 줘라!!",
          "original_form": "나도 줘라!!",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.18",
          "form": "소시지 투입",
          "original_form": "소시지 투입",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.19",
          "form": "음~ 냐미~",
          "original_form": "음~ 냐미~",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.20",
          "form": "어쩔 수 없다!",
          "original_form": "어쩔 수 없다!",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.21",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.22",
          "form": "좀 전까지는 내방에서 자더니",
          "original_form": "좀 전까지는 내방에서 자더니",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.23",
          "form": "또 아부지방 갓니",
          "original_form": "또 아부지방 갓니",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.24",
          "form": "어느새 부모님 방에 갔나바",
          "original_form": "어느새 부모님 방에 갔나바",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.25",
          "form": "없어졌어",
          "original_form": "없어졌어",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.26",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.27",
          "form": "은신술",
          "original_form": "은신술",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.28",
          "form": "하긴 내방에 둔 쿠션보다는",
          "original_form": "하긴 내방에 둔 쿠션보다는",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.29",
          "form": "거의 카카시네 닌자견",
          "original_form": "거의 카카시네 닌자견",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031627.1.30",
          "form": "안방 매트리스가",
          "original_form": "안방 매트리스가",
          "speaker_id": "2",
          "time": "20210905 01:24"
        },
        {
          "id": "MDRW2100031627.1.31",
          "form": "좋기는 할거야",
          "original_form": "좋기는 할거야",
          "speaker_id": "2",
          "time": "20210905 01:24"
        },
        {
          "id": "MDRW2100031627.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:24"
        },
        {
          "id": "MDRW2100031627.1.33",
          "form": "카카시네 멈머들도 귀여웠음",
          "original_form": "카카시네 멈머들도 귀여웠음",
          "speaker_id": "2",
          "time": "20210905 01:24"
        },
        {
          "id": "MDRW2100031627.1.34",
          "form": "소리소문없이 사라진다 흡 핫",
          "original_form": "소리소문없이 사라진다 흡 핫",
          "speaker_id": "1",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100031627.1.35",
          "form": "개들이 귀신같이 좋은 쿠션 매트 알더라",
          "original_form": "개들이 귀신같이 좋은 쿠션 매트 알더라",
          "speaker_id": "1",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031627.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031627.1.37",
          "form": "개들이 잘 아는것",
          "original_form": "개들이 잘 아는것",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031627.1.38",
          "form": "좋은 쿠션 매트",
          "original_form": "좋은 쿠션 매트",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031627.1.39",
          "form": "보일러 파이프 지나가는 자리",
          "original_form": "보일러 파이프 지나가는 자리",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031627.1.40",
          "form": "ㄹㅇ루 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄹㅇ루 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100031627.1.41",
          "form": "후자는 고양이들이 장난아니래",
          "original_form": "후자는 고양이들이 장난아니래",
          "speaker_id": "1",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100031627.1.42",
          "form": "가장 해 잘드는자리",
          "original_form": "가장 해 잘드는자리",
          "speaker_id": "1",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100031627.1.43",
          "form": "가장 따뜻한 구들목",
          "original_form": "가장 따뜻한 구들목",
          "speaker_id": "1",
          "time": "20210905 01:48"
        },
        {
          "id": "MDRW2100031627.1.44",
          "form": "맞아맞아맞아",
          "original_form": "맞아맞아맞아",
          "speaker_id": "2",
          "time": "20210905 01:52"
        },
        {
          "id": "MDRW2100031627.1.45",
          "form": "열받은 컴터 본체 위도 차지한다더라",
          "original_form": "열받은 컴터 본체 위도 차지한다더라",
          "speaker_id": "2",
          "time": "20210905 01:52"
        },
        {
          "id": "MDRW2100031627.1.46",
          "form": "귀여웡",
          "original_form": "귀여웡",
          "speaker_id": "1",
          "time": "20210905 03:42"
        },
        {
          "id": "MDRW2100031627.1.47",
          "form": "슬림형 티비 위에 뜨뜻하게 열받는데 올라가서 그 위에 고양이 올라갈 장 짜서 맞춘 사람도 잇대잖아",
          "original_form": "슬림형 티비 위에 뜨뜻하게 열받는데 올라가서 그 위에 고양이 올라갈 장 짜서 맞춘 사람도 잇대잖아",
          "speaker_id": "1",
          "time": "20210905 03:42"
        },
        {
          "id": "MDRW2100031627.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100031627.1.49",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100031627.1.50",
          "form": "나 그거 만화에서도 봤어!",
          "original_form": "나 그거 만화에서도 봤어!",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100031627.1.51",
          "form": "현재 김쫑",
          "original_form": "현재 김쫑",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100031627.1.52",
          "form": "새우 굽는 엄마 옆에서 알짱알짱",
          "original_form": "새우 굽는 엄마 옆에서 알짱알짱",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100031627.1.53",
          "form": "새우!",
          "original_form": "새우!",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100031627.1.54",
          "form": "오늘도 맛잇는거먹는구나",
          "original_form": "오늘도 맛잇는거먹는구나",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100031627.1.55",
          "form": "name3랑 나랑 위치바꾸기",
          "original_form": "&name3&랑 나랑 위치바꾸기",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100031627.1.56",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100031627.1.57",
          "form": "어제 먹고 남은 새우 구워먹음",
          "original_form": "어제 먹고 남은 새우 구워먹음",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100031627.1.58",
          "form": "name3는 안대...",
          "original_form": "&name3&는 안대...",
          "speaker_id": "2",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100031627.1.59",
          "form": "방금 친척과의 영상통화에서 늙었네 소리 들었쫑",
          "original_form": "방금 친척과의 영상통화에서 늙었네 소리 들었쫑",
          "speaker_id": "2",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100031627.1.60",
          "form": "새우만 먹고 도로 돌아갈게",
          "original_form": "새우만 먹고 도로 돌아갈게",
          "speaker_id": "1",
          "time": "20210905 17:36"
        },
        {
