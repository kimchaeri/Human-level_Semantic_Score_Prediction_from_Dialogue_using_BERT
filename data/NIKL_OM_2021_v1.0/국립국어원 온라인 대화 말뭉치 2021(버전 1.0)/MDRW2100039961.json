      "utterance": [
        {
          "id": "MDRW2100039961.1.1",
          "form": "저 나혼자산다 틀어놓고 있는데",
          "original_form": "저 나혼자산다 틀어놓고 있는데",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039961.1.2",
          "form": "이장우라고 연기자 아세요?",
          "original_form": "이장우라고 연기자 아세요?",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100039961.1.3",
          "form": "살뺐나요??",
          "original_form": "살뺐나요??",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039961.1.4",
          "form": "네 완전",
          "original_form": "네 완전",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039961.1.5",
          "form": "다들 미심쩍어했는데!",
          "original_form": "다들 미심쩍어했는데!",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039961.1.6",
          "form": "ㅇㅏ 배신자",
          "original_form": "ㅇㅏ 배신자",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039961.1.7",
          "form": "ㅋㅋㅋ 지금 보는데",
          "original_form": "ㅋㅋㅋ 지금 보는데",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039961.1.8",
          "form": "셀프 바디프로필 찍고 있어요",
          "original_form": "셀프 바디프로필 찍고 있어요",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039961.1.9",
          "form": "마지막 보루같은 동지..그런 느낌이었는데요!!",
          "original_form": "마지막 보루같은 동지..그런 느낌이었는데요!!",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039961.1.10",
          "form": "ㅋㅋㅋㅋ 진짜 살 못뺄거 같은 연예인이었는데",
          "original_form": "ㅋㅋㅋㅋ 진짜 살 못뺄거 같은 연예인이었는데",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039961.1.11",
          "form": "역시 연예인이었어요",
          "original_form": "역시 연예인이었어요",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039961.1.12",
          "form": "쭉 빠져서 왔어요ㅋㅋㅋㅋ",
          "original_form": "쭉 빠져서 왔어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100039961.1.13",
          "form": "직업이니만큼!! 빼야죠?",
          "original_form": "직업이니만큼!! 빼야죠?",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039961.1.14",
          "form": "너무 푸짐쓰..",
          "original_form": "너무 푸짐쓰..",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039961.1.15",
          "form": "근데 볼이 패였어요ㅋㅋㅋ",
          "original_form": "근데 볼이 패였어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039961.1.16",
          "form": "25키로 뺐다고",
          "original_form": "25키로 뺐다고",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039961.1.17",
          "form": "캬...",
          "original_form": "캬...",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039961.1.18",
          "form": "초딩 하나 빠져나갔네요!",
          "original_form": "초딩 하나 빠져나갔네요!",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100039961.1.19",
          "form": "지금 트레이너랑 같이 나왔는데",
          "original_form": "지금 트레이너랑 같이 나왔는데",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100039961.1.20",
          "form": "저 트레이너 옷 올려드리고 싶다...",
          "original_form": "저 트레이너 옷 올려드리고 싶다...",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100039961.1.21",
          "form": "생각하고 있어요 참 과하네여...",
          "original_form": "생각하고 있어요 참 과하네여...",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100039961.1.22",
          "form": "ㅋㅋ진짜ㅋㅋ 싫으네요ㅋㅋ",
          "original_form": "ㅋㅋ진짜ㅋㅋ 싫으네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039961.1.23",
          "form": "캡쳐보고왔어요",
          "original_form": "캡쳐보고왔어요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039961.1.24",
          "form": "원래 백키로 육박했다코",
          "original_form": "원래 백키로 육박했다코",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039961.1.25",
          "form": "ㅋㅋㅋㅋㅋ 이사람들 진짜 과해여",
          "original_form": "ㅋㅋㅋㅋㅋ 이사람들 진짜 과해여",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039961.1.26",
          "form": "둘이 같이 다니면 안될거 같은데",
          "original_form": "둘이 같이 다니면 안될거 같은데",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039961.1.27",
          "form": "ㅋㅋ악앜 그런거 너무 싫어요 오글오글",
          "original_form": "ㅋㅋ악앜 그런거 너무 싫어요 오글오글",
          "speaker_id": "2",
          "time": "20210903 23:46"
        }
      ]
    }
