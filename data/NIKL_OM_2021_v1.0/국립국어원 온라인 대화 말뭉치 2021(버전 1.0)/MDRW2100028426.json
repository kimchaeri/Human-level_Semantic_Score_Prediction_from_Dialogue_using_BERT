        {
          "id": "MDRW2100028426.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028426.1.2",
          "form": "아뇨ㅠ",
          "original_form": "아뇨ㅠ",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028426.1.3",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028426.1.4",
          "form": "전 좋아해요",
          "original_form": "전 좋아해요",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028426.1.5",
          "form": "걸어다니는 걸 좋아해서 여행도 좋아해요",
          "original_form": "걸어다니는 걸 좋아해서 여행도 좋아해요",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028426.1.6",
          "form": "기억나는여행지 있으세요?!",
          "original_form": "기억나는여행지 있으세요?!",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028426.1.7",
          "form": "여행 왜 싫어하세요?",
          "original_form": "여행 왜 싫어하세요?",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028426.1.8",
          "form": "저는 집순이에요ㅠ",
          "original_form": "저는 집순이에요ㅠ",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028426.1.9",
          "form": "아 전 런던 갔을때 제일 좋았어요",
          "original_form": "아 전 런던 갔을때 제일 좋았어요",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028426.1.10",
          "form": "나가는거자체를 별로안좋아해요",
          "original_form": "나가는거자체를 별로안좋아해요",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028426.1.11",
          "form": "멋있어요 어떤게 가장 기억에남으세요?!",
          "original_form": "멋있어요 어떤게 가장 기억에남으세요?!",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028426.1.12",
          "form": "저도 평소엔 정말 안나가서 공감해요ㅎㅎ",
          "original_form": "저도 평소엔 정말 안나가서 공감해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028426.1.13",
          "form": "뮤지컬 좋아해서 매일 뮤지컬보러갔던거랑",
          "original_form": "뮤지컬 좋아해서 매일 뮤지컬보러갔던거랑",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028426.1.14",
          "form": "그런가요?!! ㅎㅎ",
          "original_form": "그런가요?!! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028426.1.15",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028426.1.16",
          "form": "박물관 이곳저곳 갔던게 기억에 남네요",
          "original_form": "박물관 이곳저곳 갔던게 기억에 남네요",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028426.1.17",
          "form": "영국이 세계 이곳저곳에서 물건을 정말 많이 훔쳐갔더군요..",
          "original_form": "영국이 세계 이곳저곳에서 물건을 정말 많이 훔쳐갔더군요..",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028426.1.18",
          "form": "우와.. 사진도많이 찍으셨어요?!",
          "original_form": "우와.. 사진도많이 찍으셨어요?!",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028426.1.19",
          "form": "ㅋㄱㄱㄱㅋㅋㄱㅋㄱㅋㅋ살뜰히 훔치더라고요",
          "original_form": "ㅋㄱㄱㄱㅋㅋㄱㅋㄱㅋㅋ살뜰히 훔치더라고요",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028426.1.20",
          "form": "웃긴놈들 그걸전시하다니",
          "original_form": "웃긴놈들 그걸전시하다니",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028426.1.21",
          "form": "사진 많이 찍었어요ㅎㅎ",
          "original_form": "사진 많이 찍었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028426.1.22",
          "form": "가끔씩 꺼내보고 기운얻고 그래요",
          "original_form": "가끔씩 꺼내보고 기운얻고 그래요",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028426.1.23",
          "form": "긍까요ㅋㅋㅋㅋ 진짜 웃긴 놈들",
          "original_form": "긍까요ㅋㅋㅋㅋ 진짜 웃긴 놈들",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028426.1.24",
          "form": "훔쳐온게 너무 많아서",
          "original_form": "훔쳐온게 너무 많아서",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028426.1.25",
          "form": "영국박물관은 무료인거라고 하더라고요",
          "original_form": "영국박물관은 무료인거라고 하더라고요",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100028426.1.26",
          "form": "ㅋㄱㄱㅋㄱㅋㄱㅋㄱㄱ아무료에요?!ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㄱㄱㅋㄱㅋㄱㅋㄱㄱ아무료에요?!ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100028426.1.27",
          "form": "도둑개그하네요ㅋㅋㄱㅋㅋ",
          "original_form": "도둑개그하네요ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100028426.1.28",
          "form": "네네ㅋㅋㅋ 무슨 법이 있어서 자기네 나라 물건이 몇퍼센트 이상이어야",
