      "utterance": [
        {
          "id": "MDRW2100011336.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100011336.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210824 11:58"
        },
        {
          "id": "MDRW2100011336.1.3",
          "form": "게임 좋아하시나요",
          "original_form": "게임 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 12:19"
        },
        {
          "id": "MDRW2100011336.1.4",
          "form": "하고는싶은데 막상 좋아하는 게임은 없어요 ㅠ",
          "original_form": "하고는싶은데 막상 좋아하는 게임은 없어요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100011336.1.5",
          "form": "저는 예전에 음식만들어파는 타이쿤게임 좋아했는데 모바일에서도 일개미라며 놀리더라고요 ㅋㅋ",
          "original_form": "저는 예전에 음식만들어파는 타이쿤게임 좋아했는데 모바일에서도 일개미라며 놀리더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:28"
        },
        {
          "id": "MDRW2100011336.1.6",
          "form": "ㅋㅋㅋ오저도그런게임좋아해요 ㅋㅋ저는 롤러코스터타이쿤처럼 놀이기구세우고 마을꾸미고 그런거요",
          "original_form": "ㅋㅋㅋ오저도그런게임좋아해요 ㅋㅋ저는 롤러코스터타이쿤처럼 놀이기구세우고 마을꾸미고 그런거요",
          "speaker_id": "2",
          "time": "20210825 11:29"
        },
        {
          "id": "MDRW2100011336.1.7",
          "form": "어 맞아요 롤러코스터도 좋아했어요 나중엔 제가ㅡ게임하다가 멀미했죠 ㅎ",
          "original_form": "어 맞아요 롤러코스터도 좋아했어요 나중엔 제가ㅡ게임하다가 멀미했죠 ㅎ",
          "speaker_id": "1",
          "time": "20210825 11:32"
        },
        {
          "id": "MDRW2100011336.1.8",
          "form": "왜요??",
          "original_form": "왜요??",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100011336.1.9",
          "form": "엄청 오래해서요 ㅎㅎ 몇시간씩",
          "original_form": "엄청 오래해서요 ㅎㅎ 몇시간씩",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100011336.1.10",
          "form": "으 ㅋㅋㅋ장시간게임은 건강을 해칠수있습니다",
          "original_form": "으 ㅋㅋㅋ장시간게임은 건강을 해칠수있습니다",
          "speaker_id": "2",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100011336.1.11",
          "form": "네 ㅎㅎ 프로게이머 연봉 듣고 오바아닌가 했는데 그 길도 쉬운게 아닌거같아요",
          "original_form": "네 ㅎㅎ 프로게이머 연봉 듣고 오바아닌가 했는데 그 길도 쉬운게 아닌거같아요",
          "speaker_id": "1",
          "time": "20210825 12:17"
        },
        {
          "id": "MDRW2100011336.1.12",
          "form": "프로게이머들은 좋아하는게임도 하면서 돈벌어서 부럽다생각했는데 진짜 직업으로하니까 힘들어보이긴해요 ㅠ",
          "original_form": "프로게이머들은 좋아하는게임도 하면서 돈벌어서 부럽다생각했는데 진짜 직업으로하니까 힘들어보이긴해요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 12:17"
        },
        {
          "id": "MDRW2100011336.1.13",
          "form": "네 손목도 아플거같고.. 닌텐도는 안하세요",
          "original_form": "네 손목도 아플거같고.. 닌텐도는 안하세요",
          "speaker_id": "1",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100011336.1.14",
          "form": "안그래도 닌텐도 동물의 숲 하고싶어서 샀다가 팩이 너무 비싸서 다시 중고로 팔아버렸어요 ㅋㅋ",
          "original_form": "안그래도 닌텐도 동물의 숲 하고싶어서 샀다가 팩이 너무 비싸서 다시 중고로 팔아버렸어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100011336.1.15",
          "form": "저는 플스있는데 게임 잘 못하는 사람은 닌텐도가 더 좋겠더라고요",
          "original_form": "저는 플스있는데 게임 잘 못하는 사람은 닌텐도가 더 좋겠더라고요",
          "speaker_id": "1",
          "time": "20210825 12:20"
        }
      ]
