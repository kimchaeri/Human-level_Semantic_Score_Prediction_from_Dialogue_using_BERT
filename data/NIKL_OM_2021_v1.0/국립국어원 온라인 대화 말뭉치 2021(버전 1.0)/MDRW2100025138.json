        },
        {
          "id": "MDRW2100025138.1.2",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "1",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100025138.1.3",
          "form": "아뇨ㅎㅎ 이전 연애가 길었어서 그런지 만나기 쉽지 않네요",
          "original_form": "아뇨ㅎㅎ 이전 연애가 길었어서 그런지 만나기 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025138.1.4",
          "form": "요샌 뭐 코로나라",
          "original_form": "요샌 뭐 코로나라",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025138.1.5",
          "form": "스치는 사람도 거의 없구요",
          "original_form": "스치는 사람도 거의 없구요",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025138.1.6",
          "form": "와우....연애기간은\r\n얼마나 되셨던거에요?",
          "original_form": "와우....연애기간은\r\n얼마나 되셨던거에요?",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025138.1.7",
          "form": "5년반?? 쯤이었어요ㅋㅋ",
          "original_form": "5년반?? 쯤이었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 08:53"
        },
        {
          "id": "MDRW2100025138.1.8",
          "form": "와 정말 긴 연애였네요:)",
          "original_form": "와 정말 긴 연애였네요:)",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025138.1.9",
          "form": "저는 그렇게\r\n긴 연애를 해본적이 없어여ㅠ",
          "original_form": "저는 그렇게\r\n긴 연애를 해본적이 없어여ㅠ",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025138.1.10",
          "form": "최대로 긴게 얼마였나요?",
          "original_form": "최대로 긴게 얼마였나요?",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025138.1.11",
          "form": "저는 일년반이\r\n가장 긴 연애였어요ㅋㅋ",
          "original_form": "저는 일년반이\r\n가장 긴 연애였어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025138.1.12",
          "form": "자주 만나셨나봐요 그럼 보통 짧은 경우가 많던데",
          "original_form": "자주 만나셨나봐요 그럼 보통 짧은 경우가 많던데",
          "speaker_id": "2",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025138.1.13",
          "form": "음... 아니용ㅋㅋ\r\n연애 횟수도 적어요 ㅋㅋ",
          "original_form": "음... 아니용ㅋㅋ\r\n연애 횟수도 적어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025138.1.14",
          "form": "근데 길게 만나는게 좋다고는 못하겠네요",
          "original_form": "근데 길게 만나는게 좋다고는 못하겠네요",
          "speaker_id": "2",
          "time": "20210901 12:20"
        },
        {
          "id": "MDRW2100025138.1.15",
          "form": "헤어지고 많이 힘들거 같아요ㅠ",
          "original_form": "헤어지고 많이 힘들거 같아요ㅠ",
          "speaker_id": "1",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025138.1.16",
          "form": "헤어짐은 뭐 언제나 누구에게나\r\n아프고 힘든 시간이겠지만 그 깊이가\r\n다를거 같아요",
          "original_form": "헤어짐은 뭐 언제나 누구에게나\r\n아프고 힘든 시간이겠지만 그 깊이가\r\n다를거 같아요",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025138.1.17",
          "form": "많이 붙어있던 만큼 알게모르게 생활에서 티가 나긴하지만 뭐",
          "original_form": "많이 붙어있던 만큼 알게모르게 생활에서 티가 나긴하지만 뭐",
          "speaker_id": "2",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025138.1.18",
          "form": "연애의 끝은 이별 아니면 결혼일거니까요ㅎㅎ",
          "original_form": "연애의 끝은 이별 아니면 결혼일거니까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025138.1.19",
          "form": "이별아니면 결혼.... ㅠㅠ",
          "original_form": "이별아니면 결혼.... ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025138.1.20",
          "form": "선택지가 둘 뿐인게 슬프네요",
          "original_form": "선택지가 둘 뿐인게 슬프네요",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025138.1.21",
          "form": "슬프긴 하지만",
          "original_form": "슬프긴 하지만",
          "speaker_id": "2",
          "time": "20210901 14:24"
        },
        {
          "id": "MDRW2100025138.1.22",
          "form": "인연이 아니었구나 하고 털어내야죠",
          "original_form": "인연이 아니었구나 하고 털어내야죠",
          "speaker_id": "2",
          "time": "20210901 14:24"
        },
        {
          "id": "MDRW2100025138.1.23",
          "form": "털어낸다는게\r\n말처럼 쉬운게 아니잖아요:)",
          "original_form": "털어낸다는게\r\n말처럼 쉬운게 아니잖아요:)",
          "speaker_id": "1",
          "time": "20210901 14:25"
        }
      ]
    }
