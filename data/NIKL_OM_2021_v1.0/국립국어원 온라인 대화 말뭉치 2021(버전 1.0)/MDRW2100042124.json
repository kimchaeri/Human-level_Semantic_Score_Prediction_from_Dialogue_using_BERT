      "utterance": [
        {
          "id": "MDRW2100042124.1.1",
          "form": "치킨",
          "original_form": "치킨{emoji:♡}",
          "speaker_id": "2",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042124.1.2",
          "form": "치킨은 언제나 옳죠",
          "original_form": "치킨은 언제나 옳죠",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042124.1.3",
          "form": "오늘아침 편의점 들러서 너무 배고파서 샌드위치사서먹고있어요",
          "original_form": "오늘아침 편의점 들러서 너무 배고파서 샌드위치사서먹고있어요",
          "speaker_id": "2",
          "time": "20210907 06:50"
        },
        {
          "id": "MDRW2100042124.1.4",
          "form": "어떤 샌드위치 드세요? :)",
          "original_form": "어떤 샌드위치 드세요? :)",
          "speaker_id": "1",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100042124.1.5",
          "form": "참치샐러드맛이었어요",
          "original_form": "참치샐러드맛이었어요",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042124.1.6",
          "form": "먹으니깐 좋더라구용",
          "original_form": "먹으니깐 좋더라구용",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042124.1.7",
          "form": "지난번에 편의점 삼각김밥 가격 듣고\n놀랬었는데 샌드위치 가격도\n착하지는 않겠네요 ㅋㅋ",
          "original_form": "지난번에 편의점 삼각김밥 가격 듣고\n놀랬었는데 샌드위치 가격도\n착하지는 않겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042124.1.8",
          "form": "아ㅜㅜ가격이 좀 비싸긴해요",
          "original_form": "아ㅜㅜ가격이 좀 비싸긴해요",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042124.1.9",
          "form": "요샌 참 싼게 없네요ㅋㅋ",
          "original_form": "요샌 참 싼게 없네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042124.1.10",
          "form": "점심은 뭐드실거에요?",
          "original_form": "점심은 뭐드실거에요?",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042124.1.11",
          "form": "저는 직장에서 돈내고 먹어요 오늘의 메뉴는!!",
          "original_form": "저는 직장에서 돈내고 먹어요 오늘의 메뉴는!!",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042124.1.12",
          "form": "함박스테이크가 하이라이트네요",
          "original_form": "함박스테이크가 하이라이트네요",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042124.1.13",
          "form": "그거보구힘내야져",
          "original_form": "그거보구힘내야져",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042124.1.14",
          "form": "점심어떻게먹어요?",
          "original_form": "점심어떻게먹어요?",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042124.1.15",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042124.1.16",
          "form": "구내식당 이용하시나 봐요:)",
          "original_form": "구내식당 이용하시나 봐요:)",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042124.1.17",
          "form": "저는 로제떡볶이를 시킬까\n간단히 집밥을 먹을까 생각중입니다ㅋ",
          "original_form": "저는 로제떡볶이를 시킬까\n간단히 집밥을 먹을까 생각중입니다ㅋ",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042124.1.18",
          "form": "로제맛있겠답!!!",
          "original_form": "로제맛있겠답!!!",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042124.1.19",
          "form": "점심어떻게 먹을지 먹고나서 얘기해주세옹ㅋㅋ",
          "original_form": "점심어떻게 먹을지 먹고나서 얘기해주세옹ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042124.1.20",
          "form": "ㅋㅋ함박이도 맛있을거 같아요",
          "original_form": "ㅋㅋ함박이도 맛있을거 같아요",
          "speaker_id": "1",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042124.1.21",
          "form": "저도 함박스테이크먹고 얘기해들릴께용",
          "original_form": "저도 함박스테이크먹고 얘기해들릴께용",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042124.1.22",
          "form": "구내식당 맛집인가요?ㅋㅋ",
          "original_form": "구내식당 맛집인가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042124.1.23",
          "form": "그날의메뉴가정해져있어요 그냥 평범해요",
          "original_form": "그날의메뉴가정해져있어요 그냥 평범해요",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042124.1.24",
          "form": "낼은 새우튀김이 하이라이트네요",
          "original_form": "낼은 새우튀김이 하이라이트네요",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042124.1.25",
          "form": "딴건별루ㅋㄱㅋㅋ",
          "original_form": "딴건별루ㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042124.1.26",
          "form": "새우튀김좋아해서 낼도 기대되네용",
