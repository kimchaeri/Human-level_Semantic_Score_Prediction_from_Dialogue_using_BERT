      "utterance": [
        {
          "id": "MDRW2100005889.1.1",
          "form": "이제 앞으로 위드코로나가 되면 무엇을 가장 하고 싶나요?",
          "original_form": "이제 앞으로 위드코로나가 되면 무엇을 가장 하고 싶나요?",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005889.1.2",
          "form": "국내여행이요!",
          "original_form": "국내여행이요!",
          "speaker_id": "2",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100005889.1.3",
          "form": "우선 이번주 목요일에 1차 백신 맞고",
          "original_form": "우선 이번주 목요일에 1차 백신 맞고",
          "speaker_id": "2",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100005889.1.4",
          "form": "차후에 2차 백신 맞으면",
          "original_form": "차후에 2차 백신 맞으면",
          "speaker_id": "2",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100005889.1.5",
          "form": "조금 다녀볼까해요",
          "original_form": "조금 다녀볼까해요",
          "speaker_id": "2",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100005889.1.6",
          "form": "아직 백신 안맞았군요",
          "original_form": "아직 백신 안맞았군요",
          "speaker_id": "1",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100005889.1.7",
          "form": "백신 맞으면 마음이 좀 놓이니까 다녀도 되겠지요",
          "original_form": "백신 맞으면 마음이 좀 놓이니까 다녀도 되겠지요",
          "speaker_id": "1",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100005889.1.8",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005889.1.9",
          "form": "저희 부부는 완료자 돼서 이제 좀 다녀도 될것 같습니다",
          "original_form": "저희 부부는 완료자 돼서 이제 좀 다녀도 될것 같습니다",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005889.1.10",
          "form": "추석 지나고 움직여 보려고 생각중입니다",
          "original_form": "추석 지나고 움직여 보려고 생각중입니다",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005889.1.11",
          "form": "부럽네요!",
          "original_form": "부럽네요!",
          "speaker_id": "2",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100005889.1.12",
          "form": "작년부터 차로 이동은 안하고 있어서 정말 동네에만 있었는데",
          "original_form": "작년부터 차로 이동은 안하고 있어서 정말 동네에만 있었는데",
          "speaker_id": "1",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100005889.1.13",
          "form": "이제는 드라이브도 좀 해야겠습니다",
          "original_form": "이제는 드라이브도 좀 해야겠습니다",
          "speaker_id": "1",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100005889.1.14",
          "form": "기분전환겸 드라이브도 참 좋죠",
          "original_form": "기분전환겸 드라이브도 참 좋죠",
          "speaker_id": "2",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100005889.1.15",
          "form": "동네에 공원도 있지만 좀 멀리 나가는 건 또 다르니까요",
          "original_form": "동네에 공원도 있지만 좀 멀리 나가는 건 또 다르니까요",
          "speaker_id": "1",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100005889.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100005889.1.17",
          "form": "국내중에 어디 가고픈가요?",
          "original_form": "국내중에 어디 가고픈가요?",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100005889.1.18",
          "form": "제주도 혹은 강원도 쪽으로 가보고 싶어요!",
          "original_form": "제주도 혹은 강원도 쪽으로 가보고 싶어요!",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100005889.1.19",
          "form": "좋네요 휴양지",
          "original_form": "좋네요 휴양지",
          "speaker_id": "1",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100005889.1.20",
          "form": "저도 바닷바람좀 쐬고 싶어요",
          "original_form": "저도 바닷바람좀 쐬고 싶어요",
          "speaker_id": "1",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100005889.1.21",
          "form": "동해바람이 갠적으론 젤 좋더라구요 ㅎㅎ",
          "original_form": "동해바람이 갠적으론 젤 좋더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100005889.1.22",
          "form": "좋지요",
          "original_form": "좋지요",
          "speaker_id": "1",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100005889.1.23",
          "form": "특히 동해바람 맞으면서",
          "original_form": "특히 동해바람 맞으면서",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005889.1.24",
          "form": "먹는 회가 정말..",
          "original_form": "먹는 회가 정말..",
          "speaker_id": "2",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005889.1.25",
          "form": "회 저도 좋아합니다",
          "original_form": "회 저도 좋아합니다",
          "speaker_id": "1",
          "time": "20210907 16:35"
        },
        {
          "id": "MDRW2100005889.1.26",
          "form": "해산물을 좋아해요",
          "original_form": "해산물을 좋아해요",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100005889.1.27",
          "form": "대게라던지 회라던지 좋죠 ㅠㅠ",
          "original_form": "대게라던지 회라던지 좋죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100005889.1.28",
          "form": "연포탕도 좋고요",
          "original_form": "연포탕도 좋고요",
          "speaker_id": "2",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100005889.1.29",
          "form": "듣고 있으니 먹고싶습니다",
          "original_form": "듣고 있으니 먹고싶습니다",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100005889.1.30",
          "form": "바닷가에서 사는게 꿈이었는데",
          "original_form": "바닷가에서 사는게 꿈이었는데",
          "speaker_id": "1",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100005889.1.31",
          "form": "저도 그렇네요 ㅠㅠ",
          "original_form": "저도 그렇네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100005889.1.32",
          "form": "위드 코로나 된다면",
          "original_form": "위드 코로나 된다면",
          "speaker_id": "2",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100005889.1.33",
          "form": "바닷가 한번 가야겠어요",
          "original_form": "바닷가 한번 가야겠어요",
          "speaker_id": "2",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100005889.1.34",
          "form": "늦더라도..",
          "original_form": "늦더라도..",
          "speaker_id": "2",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100005889.1.35",
          "form": "겨울전에 백신접종률 높으면",
          "original_form": "겨울전에 백신접종률 높으면",
          "speaker_id": "1",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005889.1.36",
          "form": "위드코로나 할듯 합니다",
          "original_form": "위드코로나 할듯 합니다",
          "speaker_id": "1",
          "time": "20210907 16:59"
        },
        {
          "id": "MDRW2100005889.1.37",
          "form": "그럼 바닷가로 드라이브 가야겠어요",
          "original_form": "그럼 바닷가로 드라이브 가야겠어요",
          "speaker_id": "1",
          "time": "20210907 16:59"
        },
        {
          "id": "MDRW2100005889.1.38",
          "form": "목표가 80% 후에 위드코로나 한다곤 하더라고요",
          "original_form": "목표가 80% 후에 위드코로나 한다곤 하더라고요",
          "speaker_id": "2",
          "time": "20210907 16:59"
        }
