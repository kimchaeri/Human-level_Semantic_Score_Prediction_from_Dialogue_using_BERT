      "utterance": [
        {
          "id": "MDRW2100025738.1.1",
          "form": "나는 파마!",
          "original_form": "나는 파마!",
          "speaker_id": "2",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025738.1.2",
          "form": "햇다고?",
          "original_form": "햇다고?",
          "speaker_id": "1",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025738.1.3",
          "form": "아니 언니 파마 하라구ㅋㅋㅋ",
          "original_form": "아니 언니 파마 하라구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025738.1.4",
          "form": "아 진짜? 왜? 그게 나아?",
          "original_form": "아 진짜? 왜? 그게 나아?",
          "speaker_id": "1",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025738.1.5",
          "form": "응 언니 파마했을때 되게 귀엽게 잘어울렸어",
          "original_form": "응 언니 파마했을때 되게 귀엽게 잘어울렸어",
          "speaker_id": "2",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025738.1.6",
          "form": "난 매직할려고 했는데ㅠ.ㅠ",
          "original_form": "난 매직할려고 했는데ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025738.1.7",
          "form": "앜ㅋㅋㅋㅋㅋ 파마할지 매직할지 고민이라매~!",
          "original_form": "앜ㅋㅋㅋㅋㅋ 파마할지 매직할지 고민이라매~!",
          "speaker_id": "2",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025738.1.8",
          "form": "그건그런데 거의 매직으로 기울었었거든ㅋㅋㅋㅋㅋ근데 파마가 낫다니까 또 흔들려",
          "original_form": "그건그런데 거의 매직으로 기울었었거든ㅋㅋㅋㅋㅋ근데 파마가 낫다니까 또 흔들려",
          "speaker_id": "1",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025738.1.9",
          "form": "파마가 잘어울렸어ㅋㅋ 매직한거는 제대로 못봐서 모르겠당",
          "original_form": "파마가 잘어울렸어ㅋㅋ 매직한거는 제대로 못봐서 모르겠당",
          "speaker_id": "2",
          "time": "20210901 16:38"
        },
        {
          "id": "MDRW2100025738.1.10",
          "form": "왜 매직햇을때도 괜찮다그랫는데ㅠ.ㅠ 립서비스였냐구요..",
          "original_form": "왜 매직햇을때도 괜찮다그랫는데ㅠ.ㅠ 립서비스였냐구요..",
          "speaker_id": "1",
          "time": "20210901 16:38"
        },
        {
          "id": "MDRW2100025738.1.11",
          "form": "그때도 잘어울리긴 했나봐...전 그짓말은 안해욥...",
          "original_form": "그때도 잘어울리긴 했나봐...전 그짓말은 안해욥...",
          "speaker_id": "2",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025738.1.12",
          "form": "ㅋㅋㅋㅋ그럼 파마로 알아봐야겠다",
          "original_form": "ㅋㅋㅋㅋ그럼 파마로 알아봐야겠다",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025738.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ약간 뽀글이 파마보다 볼륨감 있는 그런거 하면 잘어울릴거같애 고급스럽게!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ약간 뽀글이 파마보다 볼륨감 있는 그런거 하면 잘어울릴거같애 고급스럽게!",
          "speaker_id": "2",
