      "utterance": [
        {
          "id": "MDRW2100021851.1.1",
          "form": "저녁 뭐드셨어요?",
          "original_form": "저녁 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021851.1.2",
          "form": "밥이요. 있는 반찬이랑 같이",
          "original_form": "밥이요. 있는 반찬이랑 같이",
          "speaker_id": "2",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021851.1.3",
          "form": "전 저녁 굶었어요 배고프네요",
          "original_form": "전 저녁 굶었어요 배고프네요",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021851.1.4",
          "form": "낼 전어회 먹으려고 벼르고있어요",
          "original_form": "낼 전어회 먹으려고 벼르고있어요",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021851.1.5",
          "form": "회 좋아하세요?",
          "original_form": "회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021851.1.6",
          "form": "네 좋아해요. 비싸서 잘 못먹죠ㅋㅋ",
          "original_form": "네 좋아해요. 비싸서 잘 못먹죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021851.1.7",
          "form": "전어회는 못먹어봤네요",
          "original_form": "전어회는 못먹어봤네요",
          "speaker_id": "2",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021851.1.8",
          "form": "저도 비싸서 자주는 안먹지만 전어는 철되면 꼭 먹어요 ㅎㅎ",
          "original_form": "저도 비싸서 자주는 안먹지만 전어는 철되면 꼭 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021851.1.9",
          "form": "꼬숩하니 넘 맛있어요",
          "original_form": "꼬숩하니 넘 맛있어요",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021851.1.10",
          "form": "오... 전어구이는 먹어봤는데 신기하네요",
          "original_form": "오... 전어구이는 먹어봤는데 신기하네요",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021851.1.11",
          "form": "회는 보통 광어나 연어 먹거든요",
          "original_form": "회는 보통 광어나 연어 먹거든요",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021851.1.12",
          "form": "전어철에는 회시키면 전어구이 서비스로 주는곳도 많아요 ㅎㅎ",
          "original_form": "전어철에는 회시키면 전어구이 서비스로 주는곳도 많아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021851.1.13",
          "form": "부산이라 회가 흔해서 그런걸수도있두",
          "original_form": "부산이라 회가 흔해서 그런걸수도있두",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021851.1.14",
          "form": "저도 광어랑 연어 좋아해요",
          "original_form": "저도 광어랑 연어 좋아해요",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021851.1.15",
          "form": "먹어보고 싶네요",
          "original_form": "먹어보고 싶네요",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021851.1.16",
          "form": "가을이 철이니까 기회되면 드셔보세요 ㅎㅎ 전 뼈 없이 썰어달라고해서 먹어요~",
          "original_form": "가을이 철이니까 기회되면 드셔보세요 ㅎㅎ 전 뼈 없이 썰어달라고해서 먹어요~",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021851.1.17",
          "form": "그럼 곧이네요. 알게 된 김에 꼭 먹어야겠어요ㅋㅋ",
          "original_form": "그럼 곧이네요. 알게 된 김에 꼭 먹어야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021851.1.18",
          "form": "내일은 뭐 드실건가요~?",
          "original_form": "내일은 뭐 드실건가요~?",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021851.1.19",
          "form": "치킨 먹으려구요",
          "original_form": "치킨 먹으려구요",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021851.1.20",
          "form": "요새 광고하는 자담치킨이요",
          "original_form": "요새 광고하는 자담치킨이요",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021851.1.21",
          "form": "자담 맛있어요!! 메뉴도 정하셨나요?",
          "original_form": "자담 맛있어요!! 메뉴도 정하셨나요?",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021851.1.22",
          "form": "스리라차치킨이요",
          "original_form": "스리라차치킨이요",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021851.1.23",
          "form": "맛있다구 하더라구요",
          "original_form": "맛있다구 하더라구요",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021851.1.24",
          "form": "자담치킨드셨나요!?!",
          "original_form": "자담치킨드셨나요!?!",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100021851.1.25",
          "form": "저녁에 드시나..? 저도 스리라차 좋아해서 궁금하즈라규요",
          "original_form": "저녁에 드시나..? 저도 스리라차 좋아해서 궁금하즈라규요",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100021851.1.26",
          "form": "네 맛있더라구요",
          "original_form": "네 맛있더라구요",
          "speaker_id": "2",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100021851.1.27",
          "form": "살짝 매콤해서 좋았어요.",
          "original_form": "살짝 매콤해서 좋았어요.",
          "speaker_id": "2",
          "time": "20210829 18:43"
        }
      ]
    }
