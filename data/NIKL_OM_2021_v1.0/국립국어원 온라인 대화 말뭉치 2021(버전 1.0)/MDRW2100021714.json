      "utterance": [
        {
          "id": "MDRW2100021714.1.1",
          "form": "민트초코 좋아하세용?",
          "original_form": "민트초코 좋아하세용?",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021714.1.2",
          "form": "네!!!!",
          "original_form": "네!!!!",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021714.1.3",
          "form": "엇 저두요!ㅋㅋㅋㅋ",
          "original_form": "엇 저두요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021714.1.4",
          "form": "마아악 좋아하는편은 아니지만",
          "original_form": "마아악 좋아하는편은 아니지만",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.5",
          "form": "베라민초는 진짜좋아해요",
          "original_form": "베라민초는 진짜좋아해요",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.6",
          "form": "배스킨라빈스에서 6월인가 민초 나왔다고해서 먹었는데 맛있었어용",
          "original_form": "배스킨라빈스에서 6월인가 민초 나왔다고해서 먹었는데 맛있었어용",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.7",
          "form": "통했네요ㅋㅋㅋㅋ",
          "original_form": "통했네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.9",
          "form": "그르게요",
          "original_form": "그르게요",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.10",
          "form": "민트송이가 나왔대서",
          "original_form": "민트송이가 나왔대서",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.11",
          "form": "먹어볼라했는데 근처편의점에는 없더라구요",
          "original_form": "먹어볼라했는데 근처편의점에는 없더라구요",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.12",
          "form": "민트 입안에서 화하고 상쾌한게 중독적이에요ㅋㅋㅋㅋ",
          "original_form": "민트 입안에서 화하고 상쾌한게 중독적이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.14",
          "form": "ㅠㅠ 저희 동네도 업서요",
          "original_form": "ㅠㅠ 저희 동네도 업서요",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.15",
          "form": "깔끔한게!!",
          "original_form": "깔끔한게!!",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021714.1.16",
          "form": "이번 네고왕때",
          "original_form": "이번 네고왕때",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021714.1.17",
          "form": "배스킨라빈스 아이스크림 드시러 가셨나용?",
          "original_form": "배스킨라빈스 아이스크림 드시러 가셨나용?",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021714.1.18",
          "form": "그거 복날마다 하는 거래서",
          "original_form": "그거 복날마다 하는 거래서",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021714.1.19",
          "form": "딱히 먹어야지!!하는 마음이 없더라고요",
          "original_form": "딱히 먹어야지!!하는 마음이 없더라고요",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021714.1.20",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021714.1.21",
          "form": "그러고 얼마 안있다가 바로 요기요에서 할인하더라구요",
          "original_form": "그러고 얼마 안있다가 바로 요기요에서 할인하더라구요",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021714.1.22",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021714.1.23",
          "form": "맛있으니까요",
          "original_form": "맛있으니까요",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021714.1.24",
          "form": "네네ㅋㅋㅋㅋ 저 그날",
          "original_form": "네네ㅋㅋㅋㅋ 저 그날",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021714.1.25",
          "form": "쿼터? 한통이랑",
          "original_form": "쿼터? 한통이랑",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021714.1.26",
          "form": "블록팩 4개 샀는데",
          "original_form": "블록팩 4개 샀는데",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021714.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021714.1.28",
          "form": "일주일도 못가서 다 사라졌어요",
          "original_form": "일주일도 못가서 다 사라졌어요",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021714.1.29",
          "form": "베라픽이 뭐에요??",
          "original_form": "베라픽이 뭐에요??",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021714.1.30",
          "form": "저 엄마는 외계인이랑",
          "original_form": "저 엄마는 외계인이랑",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021714.1.31",
          "form": "그 라즈베리소르베라고",
          "original_form": "그 라즈베리소르베라고",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021714.1.32",
          "form": "빠알간고 있는데",
          "original_form": "빠알간고 있는데",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021714.1.33",
          "form": "완전 맛있어요",
          "original_form": "완전 맛있어요",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021714.1.34",
          "form": "저희동네에는 업서요..ㅠ",
          "original_form": "저희동네에는 업서요..ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021714.1.35",
          "form": "전 초코나무숲이랑 초콜릿무스!",
          "original_form": "전 초코나무숲이랑 초콜릿무스!",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021714.1.36",
          "form": "이게 한정이었다가 다시 나왔다고 하더라구요",
          "original_form": "이게 한정이었다가 다시 나왔다고 하더라구요",
          "speaker_id": "1",
          "time": "20210828 21:54"
        }
      ]
