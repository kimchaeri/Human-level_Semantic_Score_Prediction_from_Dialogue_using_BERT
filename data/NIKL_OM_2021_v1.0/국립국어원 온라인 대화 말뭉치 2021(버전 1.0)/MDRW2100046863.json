        },
        {
          "id": "MDRW2100046863.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211107 00:45"
        },
        {
          "id": "MDRW2100046863.1.3",
          "form": "단풍구경 가실 계획 있으신가요 ㅎㅎ",
          "original_form": "단풍구경 가실 계획 있으신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211107 00:45"
        },
        {
          "id": "MDRW2100046863.1.4",
          "form": "엇 죄송합니다",
          "original_form": "엇 죄송합니다",
          "speaker_id": "1",
          "time": "20211106 13:18"
        },
        {
          "id": "MDRW2100046863.1.5",
          "form": "식사하고 왔어요",
          "original_form": "식사하고 왔어요",
          "speaker_id": "1",
          "time": "20211106 13:18"
        },
        {
          "id": "MDRW2100046863.1.6",
          "form": "주말에도 바쁘신가봅니다 ㅜㅜ",
          "original_form": "주말에도 바쁘신가봅니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20211106 13:19"
        },
        {
          "id": "MDRW2100046863.1.7",
          "form": "앗 그건 아니고 그냥 늦잠 잤어요 ㅎ....",
          "original_form": "앗 그건 아니고 그냥 늦잠 잤어요 ㅎ....",
          "speaker_id": "2",
          "time": "20211106 13:19"
        },
        {
          "id": "MDRW2100046863.1.8",
          "form": "앗\r\nㅋㅋㅋㅋ 반전",
          "original_form": "앗\r\nㅋㅋㅋㅋ 반전",
          "speaker_id": "1",
          "time": "20211106 13:19"
        },
        {
          "id": "MDRW2100046863.1.9",
          "form": "주말엔 늦잠도 자 줘야죠 !!!",
          "original_form": "주말엔 늦잠도 자 줘야죠 !!!",
          "speaker_id": "1",
          "time": "20211106 13:19"
        },
        {
          "id": "MDRW2100046863.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ ㅋㅋㅋ 밀린 잠을 보충했습니다 하하",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ ㅋㅋㅋ 밀린 잠을 보충했습니다 하하",
          "speaker_id": "2",
          "time": "20211106 13:19"
        },
        {
          "id": "MDRW2100046863.1.11",
          "form": "어제 오늘",
          "original_form": "어제 오늘",
          "speaker_id": "2",
          "time": "20211106 13:19"
        },
        {
          "id": "MDRW2100046863.1.12",
          "form": "날씨 엄청 좋더라구요 ㅠㅠ",
          "original_form": "날씨 엄청 좋더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211106 13:20"
        },
        {
          "id": "MDRW2100046863.1.13",
          "form": "좀 더울 정도 ㅎㅎ",
          "original_form": "좀 더울 정도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211106 13:20"
        },
        {
          "id": "MDRW2100046863.1.14",
          "form": "날씨는 좋은데",
          "original_form": "날씨는 좋은데",
          "speaker_id": "1",
          "time": "20211106 13:20"
        },
        {
          "id": "MDRW2100046863.1.15",
          "form": "미세먼지가",
          "original_form": "미세먼지가",
          "speaker_id": "1",
          "time": "20211106 13:20"
        },
        {
          "id": "MDRW2100046863.1.16",
          "form": "앗 말씀듣고",
          "original_form": "앗 말씀듣고",
          "speaker_id": "1",
          "time": "20211106 13:21"
        },
        {
          "id": "MDRW2100046863.1.17",
          "form": "하늘을 봤더니",
          "original_form": "하늘을 봤더니",
          "speaker_id": "2",
          "time": "20211106 13:21"
        },
        {
          "id": "MDRW2100046863.1.18",
          "form": "무슨 일이지 이게",
          "original_form": "무슨 일이지 이게",
          "speaker_id": "2",
          "time": "20211106 13:21"
        },
        {
          "id": "MDRW2100046863.1.19",
          "form": "......",
          "original_form": "......",
          "speaker_id": "2",
          "time": "20211106 13:21"
        },
        {
          "id": "MDRW2100046863.1.20",
          "form": "미세먼지.....",
          "original_form": "미세먼지.....",
          "speaker_id": "2",
          "time": "20211106 13:21"
        },
        {
          "id": "MDRW2100046863.1.21",
          "form": "심하네요 진짜",
          "original_form": "심하네요 진짜",
          "speaker_id": "2",
          "time": "20211106 13:22"
        },
        {
          "id": "MDRW2100046863.1.22",
          "form": "맞아요 날씨가 너무 아깝습니다 ㅜㅜ",
          "original_form": "맞아요 날씨가 너무 아깝습니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20211106 13:22"
        },
        {
          "id": "MDRW2100046863.1.23",
          "form": "창문 닫고 계세요",
          "original_form": "창문 닫고 계세요",
          "speaker_id": "1",
          "time": "20211106 13:22"
        },
        {
          "id": "MDRW2100046863.1.24",
          "form": "하 그래야겠어요 ㅠㅠ 감사합니당",
          "original_form": "하 그래야겠어요 ㅠㅠ 감사합니당",
          "speaker_id": "2",
          "time": "20211106 13:23"
        },
        {
          "id": "MDRW2100046863.1.25",
          "form": "ㅇ늘은",
          "original_form": "ㅇ늘은",
          "speaker_id": "2",
          "time": "20211106 13:23"
        },
        {
          "id": "MDRW2100046863.1.26",
          "form": "점심식사도",
          "original_form": "점심식사도",
          "speaker_id": "1",
          "time": "20211106 13:23"
        },
        {
          "id": "MDRW2100046863.1.27",
          "form": "오늘은",
          "original_form": "오늘은",
          "speaker_id": "2",
          "time": "20211106 13:23"
        },
        {
          "id": "MDRW2100046863.1.28",
          "form": "집콕 예정이신가요 ㅎㅎ",
          "original_form": "집콕 예정이신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211106 13:23"
        },
        {
          "id": "MDRW2100046863.1.29",
          "form": "아직 안 하신거죠~",
          "original_form": "아직 안 하신거죠~",
          "speaker_id": "1",
          "time": "20211106 13:23"
        },
        {
          "id": "MDRW2100046863.1.30",
          "form": "네 ㅠㅠ 좀 이따 먹을라구요 ㅎ",
          "original_form": "네 ㅠㅠ 좀 이따 먹을라구요 ㅎ",
          "speaker_id": "2",
          "time": "20211106 13:23"
        },
        {
          "id": "MDRW2100046863.1.31",
          "form": "네네 \r\n어차피 마스크를 끼긴 해도 ㅎㅎ\r\n공기가 이러면 나가기가 싫더라구요",
          "original_form": "네네 \r\n어차피 마스크를 끼긴 해도 ㅎㅎ\r\n공기가 이러면 나가기가 싫더라구요",
          "speaker_id": "1",
          "time": "20211106 13:23"
        },
        {
          "id": "MDRW2100046863.1.32",
          "form": "이따 나가야되는데 큰일이네요",
          "original_form": "이따 나가야되는데 큰일이네요",
          "speaker_id": "2",
          "time": "20211106 13:24"
        },
        {
          "id": "MDRW2100046863.1.33",
          "form": "휴휴",
          "original_form": "휴휴",
          "speaker_id": "2",
          "time": "20211106 13:24"
        },
        {
          "id": "MDRW2100046863.1.34",
          "form": "현대사회는 쉽지않군요",
          "original_form": "현대사회는 쉽지않군요",
          "speaker_id": "2",
          "time": "20211106 13:24"
        },
        {
          "id": "MDRW2100046863.1.35",
          "form": "전염병에 미세먼지가 디폴트라니\r\n\r\n",
          "original_form": "전염병에 미세먼지가 디폴트라니\r\n\r\n",
