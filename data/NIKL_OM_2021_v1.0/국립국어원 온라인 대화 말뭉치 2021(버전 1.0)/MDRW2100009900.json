      "utterance": [
        {
          "id": "MDRW2100009900.1.1",
          "form": "근데 그건 온라인이 아닌디",
          "original_form": "근데 그건 온라인이 아닌디",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009900.1.2",
          "form": "아침체조같지않아?",
          "original_form": "아침체조같지않아?",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009900.1.3",
          "form": "올림픽 봄?",
          "original_form": "올림픽 봄?",
          "speaker_id": "1",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009900.1.4",
          "form": "나 솔직히 말하면 한경기도 안봄",
          "original_form": "나 솔직히 말하면 한경기도 안봄",
          "speaker_id": "2",
          "time": "20210823 11:35"
        },
        {
          "id": "MDRW2100009900.1.5",
          "form": "이때까지 아는척만 했어ㅜ",
          "original_form": "이때까지 아는척만 했어ㅜ",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009900.1.6",
          "form": "흑 가식적이었어ㅜ",
          "original_form": "흑 가식적이었어ㅜ",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009900.1.7",
          "form": "ㅋㅋㅋㅋ 대세를 따르려는 노력",
          "original_form": "ㅋㅋㅋㅋ 대세를 따르려는 노력",
          "speaker_id": "1",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009900.1.8",
          "form": "배구 정확히 무슨메달땄는지도",
          "original_form": "배구 정확히 무슨메달땄는지도",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009900.1.9",
          "form": "때려맞춤",
          "original_form": "때려맞춤",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009900.1.10",
          "form": "메달 못땀ㅜㅜㅜ",
          "original_form": "메달 못땀ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009900.1.11",
          "form": "눈치보고 은메달?",
          "original_form": "눈치보고 은메달?",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009900.1.12",
          "form": "인가 생각했는데",
          "original_form": "인가 생각했는데",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009900.1.13",
          "form": "절대 말로 안해서 다행이다 ㅋㅋ",
          "original_form": "절대 말로 안해서 다행이다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:37"
        },
        {
          "id": "MDRW2100009900.1.14",
          "form": "뽀록날뻔함",
          "original_form": "뽀록날뻔함",
          "speaker_id": "2",
          "time": "20210823 11:37"
        },
        {
          "id": "MDRW2100009900.1.15",
          "form": "4강 갓다",
          "original_form": "4강 갓다",
          "speaker_id": "1",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009900.1.16",
          "form": "멋졌어ㅜ",
          "original_form": "멋졌어ㅜ",
          "speaker_id": "1",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009900.1.17",
          "form": "그럼 4위했구나 배구는",
          "original_form": "그럼 4위했구나 배구는",
          "speaker_id": "2",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009900.1.18",
          "form": "야구는 어캐됨",
          "original_form": "야구는 어캐됨",
          "speaker_id": "2",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009900.1.19",
          "form": "그거도 두루뭉실한 이야기만했었음 ㅋㅋㅋ",
          "original_form": "그거도 두루뭉실한 이야기만했었음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009900.1.20",
          "form": "야구도 졌든데 그건기억안남",
          "original_form": "야구도 졌든데 그건기억안남",
          "speaker_id": "1",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009900.1.21",
          "form": "ㅋㅋ 야구 본진이자나요 님",
          "original_form": "ㅋㅋ 야구 본진이자나요 님",
          "speaker_id": "1",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009900.1.22",
          "form": "요새 좀 애정떨어짐",
          "original_form": "요새 좀 애정떨어짐",
          "speaker_id": "2",
          "time": "20210823 11:41"
        },
        {
          "id": "MDRW2100009900.1.23",
          "form": "차라리 야구애니? 가좋달까",
          "original_form": "차라리 야구애니? 가좋달까",
          "speaker_id": "2",
          "time": "20210823 11:41"
        },
        {
          "id": "MDRW2100009900.1.24",
          "form": "야구애니는 깨끗해",
          "original_form": "야구애니는 깨끗해",
          "speaker_id": "1",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009900.1.25",
          "form": "열정이 넘쳐",
          "original_form": "열정이 넘쳐",
          "speaker_id": "1",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009900.1.26",
          "form": "응 난 선수들 도박하고",
          "original_form": "응 난 선수들 도박하고",
          "speaker_id": "2",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009900.1.27",
          "form": "애정 사라짐",
          "original_form": "애정 사라짐",
          "speaker_id": "2",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009900.1.28",
          "form": "나도222",
          "original_form": "나도222",
          "speaker_id": "1",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009900.1.29",
          "form": "이 사람들을 응원한다는거 자체에 환멸생기고",
          "original_form": "이 사람들을 응원한다는거 자체에 환멸생기고",
          "speaker_id": "2",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009900.1.30",
          "form": "여기에 돈 1도 쓰기싫어짐",
          "original_form": "여기에 돈 1도 쓰기싫어짐",
          "speaker_id": "2",
          "time": "20210823 11:47"
        }
      ]
