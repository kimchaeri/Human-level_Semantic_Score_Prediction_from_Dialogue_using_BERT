      "utterance": [
        {
          "id": "MDRW2100042846.1.1",
          "form": "자다가 name3이가 팔쳐서 아파서 깨고ㅜ",
          "original_form": "자다가 &name3&이가 팔쳐서 아파서 깨고ㅜ",
          "speaker_id": "2",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042846.1.2",
          "form": "깊은잠을 못 자겠더라ㅜ",
          "original_form": "깊은잠을 못 자겠더라ㅜ",
          "speaker_id": "2",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042846.1.3",
          "form": "name4이 한글이야호틀어줘라",
          "original_form": "&name4&이 한글이야호틀어줘라",
          "speaker_id": "1",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042846.1.4",
          "form": "백신이 나는 3일지나야 나오더라",
          "original_form": "백신이 나는 3일지나야 나오더라",
          "speaker_id": "2",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042846.1.5",
          "form": "틀어주면 뭐하노ㅜ안보는데ㅜ",
          "original_form": "틀어주면 뭐하노ㅜ안보는데ㅜ",
          "speaker_id": "2",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042846.1.6",
          "form": "그냥 보고만있다ㅜ글자를 보는게아니라",
          "original_form": "그냥 보고만있다ㅜ글자를 보는게아니라",
          "speaker_id": "2",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042846.1.7",
          "form": "난 환각처럼그렇노 정신못차리겠다",
          "original_form": "난 환각처럼그렇노 정신못차리겠다",
          "speaker_id": "1",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042846.1.8",
          "form": "내가 그랬다ㅜ",
          "original_form": "내가 그랬다ㅜ",
          "speaker_id": "2",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042846.1.9",
          "form": "멍하고ㅜ조심해라",
          "original_form": "멍하고ㅜ조심해라",
          "speaker_id": "2",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042846.1.10",
          "form": "왜그렇지",
          "original_form": "왜그렇지",
          "speaker_id": "1",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042846.1.11",
          "form": "운전하다 사고 많이 난단다",
