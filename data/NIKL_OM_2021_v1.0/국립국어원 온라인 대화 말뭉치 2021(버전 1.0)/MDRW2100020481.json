      "utterance": [
        {
          "id": "MDRW2100020481.1.1",
          "form": "먼저 맥도날드로 가보죠",
          "original_form": "먼저 맥도날드로 가보죠",
          "speaker_id": "1",
          "time": "20210828 08:14"
        },
        {
          "id": "MDRW2100020481.1.2",
          "form": "맥도날드에선 뭘 드시나요",
          "original_form": "맥도날드에선 뭘 드시나요",
          "speaker_id": "1",
          "time": "20210828 08:14"
        },
        {
          "id": "MDRW2100020481.1.3",
          "form": "맥도날드는 상하이죠",
          "original_form": "맥도날드는 상하이죠",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.5",
          "form": "아시는구낭",
          "original_form": "아시는구낭",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.6",
          "form": "와 맥날은 상하이죠",
          "original_form": "와 맥날은 상하이죠",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.7",
          "form": "저 그거 먹으러 맥날가잖아요",
          "original_form": "저 그거 먹으러 맥날가잖아요",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ정답",
          "original_form": "ㅋㅋㅋㅋㅋㅋ정답",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.9",
          "form": "오로지 상하이만을 위해서",
          "original_form": "오로지 상하이만을 위해서",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.10",
          "form": "맞아요 최근에 딴거먹었는데 별로더라구요",
          "original_form": "맞아요 최근에 딴거먹었는데 별로더라구요",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.11",
          "form": "약간 안에 치킨패티가 매콤한게 매력이잖아요",
          "original_form": "약간 안에 치킨패티가 매콤한게 매력이잖아요",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.12",
          "form": "그럼 롯데리아는?",
          "original_form": "그럼 롯데리아는?",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020481.1.13",
          "form": "롯데리아역시 치킨버거 좋아합니다",
          "original_form": "롯데리아역시 치킨버거 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020481.1.14",
          "form": "안에 마요네즈와 델리소스가 핵심",
          "original_form": "안에 마요네즈와 델리소스가 핵심",
          "speaker_id": "1",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020481.1.15",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020481.1.16",
          "form": "앗저두요 ㅎㅎㅎ 갑자기 이름이 기억안나네요 치킨버거..",
          "original_form": "앗저두요 ㅎㅎㅎ 갑자기 이름이 기억안나네요 치킨버거..",
          "speaker_id": "2",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020481.1.17",
          "form": "유러피안 프런코도 좋아해요",
          "original_form": "유러피안 프런코도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020481.1.18",
          "form": "유러피안버거 좋아하실",
          "original_form": "유러피안버거 좋아하실",
          "speaker_id": "1",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020481.1.19",
          "form": "소름",
          "original_form": "소름",
          "speaker_id": "1",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020481.1.20",
          "form": "그거 치고있었는데",
          "original_form": "그거 치고있었는데",
          "speaker_id": "1",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020481.1.21",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020481.1.22",
