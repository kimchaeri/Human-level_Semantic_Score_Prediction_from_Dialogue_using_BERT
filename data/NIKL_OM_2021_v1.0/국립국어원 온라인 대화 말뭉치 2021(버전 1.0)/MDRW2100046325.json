        },
        {
          "id": "MDRW2100046325.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211020 09:10"
        },
        {
          "id": "MDRW2100046325.1.3",
          "form": "국내여행 좋아하시나요?",
          "original_form": "국내여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211020 09:10"
        },
        {
          "id": "MDRW2100046325.1.4",
          "form": "국내여행 진짜 좋아합니다.",
          "original_form": "국내여행 진짜 좋아합니다.",
          "speaker_id": "1",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046325.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20211020 14:12"
        },
        {
          "id": "MDRW2100046325.1.6",
          "form": "저는 사실 강릉이랑 부산, 군산",
          "original_form": "저는 사실 강릉이랑 부산, 군산",
          "speaker_id": "2",
          "time": "20211020 14:12"
        },
        {
          "id": "MDRW2100046325.1.7",
          "form": "이 정도만 가봐서",
          "original_form": "이 정도만 가봐서",
          "speaker_id": "2",
          "time": "20211020 14:12"
        },
        {
          "id": "MDRW2100046325.1.8",
          "form": "많이 갔다고는 못하겠는데..",
          "original_form": "많이 갔다고는 못하겠는데..",
          "speaker_id": "2",
          "time": "20211020 14:12"
        },
        {
          "id": "MDRW2100046325.1.9",
          "form": "많이가보셨나봐요!?",
          "original_form": "많이가보셨나봐요!?",
          "speaker_id": "2",
          "time": "20211020 14:12"
        },
        {
          "id": "MDRW2100046325.1.10",
          "form": "저는 서쪽 외에는 전국 많이 다녔습니다.",
          "original_form": "저는 서쪽 외에는 전국 많이 다녔습니다.",
          "speaker_id": "1",
          "time": "20211020 14:13"
        },
        {
          "id": "MDRW2100046325.1.11",
          "form": "제주도, 춘천, 부산, 대구 등 등",
          "original_form": "제주도, 춘천, 부산, 대구 등 등",
          "speaker_id": "1",
          "time": "20211020 14:13"
        },
        {
          "id": "MDRW2100046325.1.12",
          "form": "우와아",
          "original_form": "우와아",
          "speaker_id": "2",
          "time": "20211020 14:13"
        },
        {
          "id": "MDRW2100046325.1.13",
          "form": "대구도 진짜 가보고싶은 도신데",
          "original_form": "대구도 진짜 가보고싶은 도신데",
          "speaker_id": "2",
          "time": "20211020 14:13"
        },
        {
          "id": "MDRW2100046325.1.14",
          "form": "대구여행 재밌으셨나요?",
          "original_form": "대구여행 재밌으셨나요?",
          "speaker_id": "2",
          "time": "20211020 14:13"
        },
        {
          "id": "MDRW2100046325.1.15",
          "form": "네 재미있더라고요.",
          "original_form": "네 재미있더라고요.",
          "speaker_id": "1",
          "time": "20211020 14:14"
        },
        {
          "id": "MDRW2100046325.1.16",
          "form": "특히 곱창이 맛있습니다^^",
          "original_form": "특히 곱창이 맛있습니다^^",
          "speaker_id": "1",
          "time": "20211020 14:14"
        },
        {
          "id": "MDRW2100046325.1.17",
          "form": "대구 막창?!",
          "original_form": "대구 막창?!",
          "speaker_id": "2",
          "time": "20211020 14:14"
        },
        {
          "id": "MDRW2100046325.1.18",
          "form": "크아",
          "original_form": "크아",
          "speaker_id": "2",
          "time": "20211020 14:14"
        },
        {
          "id": "MDRW2100046325.1.19",
          "form": "맞습니다!",
          "original_form": "맞습니다!",
          "speaker_id": "1",
          "time": "20211020 14:14"
        },
        {
          "id": "MDRW2100046325.1.20",
          "form": "제 먹킷리스트에 항상 있는 ㅎㅎ",
          "original_form": "제 먹킷리스트에 항상 있는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 14:14"
        },
        {
          "id": "MDRW2100046325.1.21",
          "form": "저도 곧 제주도 여행을 갈까 하는데",
          "original_form": "저도 곧 제주도 여행을 갈까 하는데",
          "speaker_id": "2",
          "time": "20211020 14:14"
        },
        {
          "id": "MDRW2100046325.1.22",
          "form": "ㅎㅎ 거기에 술한잔이면 제격이죠",
          "original_form": "ㅎㅎ 거기에 술한잔이면 제격이죠",
          "speaker_id": "1",
          "time": "20211020 14:14"
        },
        {
          "id": "MDRW2100046325.1.23",
          "form": "제주도에서 가보셨던 음식점 중에",
          "original_form": "제주도에서 가보셨던 음식점 중에",
          "speaker_id": "2",
          "time": "20211020 14:14"
        },
        {
          "id": "MDRW2100046325.1.24",
          "form": "정말 맛있었다!!!! 하는 곳 한 군데만 공유해주실 수 있을까요 하핫",
          "original_form": "정말 맛있었다!!!! 하는 곳 한 군데만 공유해주실 수 있을까요 하핫",
          "speaker_id": "2",
          "time": "20211020 14:15"
        },
        {
          "id": "MDRW2100046325.1.25",
          "form": "막창에 술 ㅜㅜ 반칙입니다 정말",
          "original_form": "막창에 술 ㅜㅜ 반칙입니다 정말",
          "speaker_id": "2",
          "time": "20211020 14:15"
        },
        {
          "id": "MDRW2100046325.1.26",
          "form": "지금 갑자기 시장이름이 기억 안나는데 남쪽에 큰 시장이 하나 있거든요",
          "original_form": "지금 갑자기 시장이름이 기억 안나는데 남쪽에 큰 시장이 하나 있거든요",
          "speaker_id": "1",
          "time": "20211020 14:15"
        },
        {
          "id": "MDRW2100046325.1.27",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20211020 14:15"
        },
        {
          "id": "MDRW2100046325.1.28",
          "form": "그 곳에 맬젓에 삼겹살 하는 집이 있는데",
          "original_form": "그 곳에 맬젓에 삼겹살 하는 집이 있는데",
          "speaker_id": "1",
          "time": "20211020 14:15"
        },
        {
          "id": "MDRW2100046325.1.29",
          "form": "늘 1시간 이상 줄서야 하고",
          "original_form": "늘 1시간 이상 줄서야 하고",
          "speaker_id": "1",
          "time": "20211020 14:15"
        },
        {
          "id": "MDRW2100046325.1.30",
          "form": "돈사돈은 아니죠??",
          "original_form": "돈사돈은 아니죠??",
          "speaker_id": "2",
          "time": "20211020 14:15"
        },
        {
          "id": "MDRW2100046325.1.31",
          "form": "어!",
          "original_form": "어!",
          "speaker_id": "1",
          "time": "20211020 14:15"
        },
        {
          "id": "MDRW2100046325.1.32",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20211020 14:16"
        },
        {
          "id": "MDRW2100046325.1.33",
          "form": "돈사돈인가요?",
          "original_form": "돈사돈인가요?",
          "speaker_id": "2",
          "time": "20211020 14:16"
        },
        {
          "id": "MDRW2100046325.1.34",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20211020 14:16"
        },
        {
          "id": "MDRW2100046325.1.35",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "1",
          "time": "20211020 14:16"
        },
        {
          "id": "MDRW2100046325.1.36",
          "form": "비린내 없고",
          "original_form": "비린내 없고",
          "speaker_id": "1",
          "time": "20211020 14:16"
        },
        {
          "id": "MDRW2100046325.1.37",
          "form": "진짜 맛있는 고기와 맬젓이었습니다.",
          "original_form": "진짜 맛있는 고기와 맬젓이었습니다.",
          "speaker_id": "1",
          "time": "20211020 14:16"
        },
        {
          "id": "MDRW2100046325.1.38",
          "form": "호오.....ㅠㅠㅠㅠㅠ 맛있겠다..........",
          "original_form": "호오.....ㅠㅠㅠㅠㅠ 맛있겠다..........",
          "speaker_id": "2",
          "time": "20211020 14:16"
        },
        {
          "id": "MDRW2100046325.1.39",
          "form": "꼭 리스트에 넣고 다녀오겠습니다 ㅎㅎㅎ",
          "original_form": "꼭 리스트에 넣고 다녀오겠습니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 14:16"
        },
        {
          "id": "MDRW2100046325.1.40",
          "form": "추천 감사해요!",
