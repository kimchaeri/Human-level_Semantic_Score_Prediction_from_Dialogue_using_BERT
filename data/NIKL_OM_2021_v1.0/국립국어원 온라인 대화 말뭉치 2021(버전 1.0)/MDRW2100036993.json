      "utterance": [
        {
          "id": "MDRW2100036993.1.1",
          "form": "게임하시는거있나요!",
          "original_form": "게임하시는거있나요!",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100036993.1.2",
          "form": "게임과는 거리가 먼 삶을 살아와서요..ㅎ",
          "original_form": "게임과는 거리가 먼 삶을 살아와서요..ㅎ",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100036993.1.3",
          "form": "name2님은 하시는 게임 있나요?",
          "original_form": "&name2&님은 하시는 게임 있나요?",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100036993.1.4",
          "form": "저는 레알팜합니다! 아시나용!",
          "original_form": "저는 레알팜합니다! 아시나용!",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100036993.1.5",
          "form": "들어는 봤어요",
          "original_form": "들어는 봤어요",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100036993.1.6",
          "form": "농사 짓는거죠?",
          "original_form": "농사 짓는거죠?",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100036993.1.7",
          "form": "우리딸도 예전에 하던데.. 비슷한거",
          "original_form": "우리딸도 예전에 하던데.. 비슷한거",
          "speaker_id": "1",
          "time": "20210904 20:50"
        },
        {
          "id": "MDRW2100036993.1.8",
          "form": "네 맞아요ㅋㄱㅋ 농사지으면 진짜 농작물을 보내준다고 광고해가지구ㅋㅋㅋ",
          "original_form": "네 맞아요ㅋㄱㅋ 농사지으면 진짜 농작물을 보내준다고 광고해가지구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:52"
        },
        {
          "id": "MDRW2100036993.1.9",
          "form": "아.. 그럼 같은거네요",
          "original_form": "아.. 그럼 같은거네요",
          "speaker_id": "1",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100036993.1.10",
          "form": "제가 왜 게임을 하냐고 했더니 그렇게 말하면서 이건 그냥 게임이 아니라고 했었어요",
          "original_form": "제가 왜 게임을 하냐고 했더니 그렇게 말하면서 이건 그냥 게임이 아니라고 했었어요",
          "speaker_id": "1",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100036993.1.11",
          "form": "ㅋㅋㅋㅋㅋ집착하면서 하게됩니다ㅋㄱㅋㅋㄱ무서운게임이예용..",
          "original_form": "ㅋㅋㅋㅋㅋ집착하면서 하게됩니다ㅋㄱㅋㅋㄱ무서운게임이예용..",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100036993.1.12",
          "form": "그런가요? 제가 좀 못마땅해했더니 딸이 항변을 하더라구요",
          "original_form": "그런가요? 제가 좀 못마땅해했더니 딸이 항변을 하더라구요",
          "speaker_id": "1",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100036993.1.13",
          "form": "냅뒀어요 ㅎㅎ",
          "original_form": "냅뒀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100036993.1.14",
          "form": "ㅋㅋㅋㅋㅋ왠지 저걸 타야만 해낸거같아가지구..ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ왠지 저걸 타야만 해낸거같아가지구..ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100036993.1.15",
          "form": "이게 게임을 하는 사람과 안하는 사람이겠죠?",
          "original_form": "이게 게임을 하는 사람과 안하는 사람이겠죠?",
          "speaker_id": "1",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100036993.1.16",
          "form": "하는 사람은 이유가 있을테고 안하는 저같은 사람은 왜하나 싶고",
          "original_form": "하는 사람은 이유가 있을테고 안하는 저같은 사람은 왜하나 싶고",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100036993.1.17",
          "form": "그냥 스트레스 해소용이겠거니 생각하고 있어요",
          "original_form": "그냥 스트레스 해소용이겠거니 생각하고 있어요",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100036993.1.18",
          "form": "맞아요ㅋㅋㅋ그냥그러려니하면편한거같아요",
          "original_form": "맞아요ㅋㅋㅋ그냥그러려니하면편한거같아요",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100036993.1.19",
          "form": "그래요 우리 남편도 바둑게임을 최근에 시작했는데",
          "original_form": "그래요 우리 남편도 바둑게임을 최근에 시작했는데",
          "speaker_id": "1",
          "time": "20210905 06:56"
        },
        {
          "id": "MDRW2100036993.1.20",
          "form": "코로나때문에 스트레스 받아서 하겠거니 하고 있어요",
          "original_form": "코로나때문에 스트레스 받아서 하겠거니 하고 있어요",
          "speaker_id": "1",
          "time": "20210905 06:56"
        },
        {
          "id": "MDRW2100036993.1.21",
          "form": "ㅋㅋㅋㅋ그래도 좋게좋게 넘어가주시네요ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ그래도 좋게좋게 넘어가주시네요ㅎㅎ",
