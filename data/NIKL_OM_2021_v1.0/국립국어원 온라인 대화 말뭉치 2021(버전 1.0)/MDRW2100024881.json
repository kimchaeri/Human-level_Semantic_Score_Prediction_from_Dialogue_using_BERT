      "utterance": [
        {
          "id": "MDRW2100024881.1.1",
          "form": "가장좋아하는카페는어디야?",
          "original_form": "가장좋아하는카페는어디야?",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024881.1.2",
          "form": "나느스벅ㅋㅋ너는?",
          "original_form": "나느스벅ㅋㅋ너는?",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024881.1.3",
          "form": "나도야ㅋㅋ무난해",
          "original_form": "나도야ㅋㅋ무난해",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024881.1.4",
          "form": "나는집,회사근처에있어ㅋㅋ",
          "original_form": "나는집,회사근처에있어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100024881.1.5",
          "form": "이래저래자주가게됨",
          "original_form": "이래저래자주가게됨",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100024881.1.6",
          "form": "ㅋㅋ맞아ㅋㅋ거리중요하지",
          "original_form": "ㅋㅋ맞아ㅋㅋ거리중요하지",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100024881.1.7",
          "form": "그거랑기프티콘도많아서자주감",
          "original_form": "그거랑기프티콘도많아서자주감",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100024881.1.8",
          "form": "스벅케이크도맛있자나ㅋㅋ",
          "original_form": "스벅케이크도맛있자나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100024881.1.9",
          "form": "치즈케이크먹고싶어",
          "original_form": "치즈케이크먹고싶어",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100024881.1.10",
          "form": "꾸덕꾸덕한거",
          "original_form": "꾸덕꾸덕한거",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
