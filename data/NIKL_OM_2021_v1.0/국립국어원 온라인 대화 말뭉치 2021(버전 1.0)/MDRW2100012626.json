        },
        {
          "id": "MDRW2100012626.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100012626.1.3",
          "form": "좋아하는 스포츠 있으세요?",
          "original_form": "좋아하는 스포츠 있으세요?",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100012626.1.4",
          "form": "배구요",
          "original_form": "배구요",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100012626.1.5",
          "form": "저도 올림픽 보고 배구에 빠졌어요",
          "original_form": "저도 올림픽 보고 배구에 빠졌어요",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100012626.1.6",
          "form": "일본 터키전에서 박진감 넘쳤어요",
          "original_form": "일본 터키전에서 박진감 넘쳤어요",
          "speaker_id": "1",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100012626.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100012626.1.8",
          "form": "박정아 선수가 너무 멋있었어요",
          "original_form": "박정아 선수가 너무 멋있었어요",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100012626.1.9",
          "form": "저는 양효진 선수가 멋있어요",
          "original_form": "저는 양효진 선수가 멋있어요",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100012626.1.10",
          "form": "삼각편대가 열일했죠",
          "original_form": "삼각편대가 열일했죠",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100012626.1.11",
          "form": "리베로 오지영선수도 멋있더라고요",
          "original_form": "리베로 오지영선수도 멋있더라고요",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100012626.1.12",
          "form": "아 오지영 선수도 대단하죠",
          "original_form": "아 오지영 선수도 대단하죠",
          "speaker_id": "1",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100012626.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100012626.1.14",
          "form": "코로나만 아니었으면 직관하러 갔을 것 같아요",
          "original_form": "코로나만 아니었으면 직관하러 갔을 것 같아요",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100012626.1.15",
          "form": "어느팀 응원하세요?",
          "original_form": "어느팀 응원하세요?",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100012626.1.16",
          "form": "전 김희진 선수 좋아해서 아이비케이요",
          "original_form": "전 김희진 선수 좋아해서 아이비케이요",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100012626.1.17",
          "form": "근데 구단 바꾸면 응원하는 팀도 바꿀 거예요ㅋㅋ",
          "original_form": "근데 구단 바꾸면 응원하는 팀도 바꿀 거예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100012626.1.18",
          "form": "감독 너무 맘에 안 들어요",
          "original_form": "감독 너무 맘에 안 들어요",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100012626.1.19",
          "form": "저는 못정하겠어요ㅋㅋ",
          "original_form": "저는 못정하겠어요ㅋㅋ",
