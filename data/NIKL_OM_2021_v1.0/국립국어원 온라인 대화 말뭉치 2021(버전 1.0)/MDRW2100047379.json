      "utterance": [
        {
          "id": "MDRW2100047379.1.1",
          "form": "점심 뭐 드셨어요?",
          "original_form": "점심 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047379.1.2",
          "form": "전 된장국에 계란 후라이",
          "original_form": "전 된장국에 계란 후라이",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047379.1.3",
          "form": "한식이요",
          "original_form": "한식이요",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047379.1.4",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047379.1.5",
          "form": "저도 백반먹엇어요 ㅋㅋㅋㅋ",
          "original_form": "저도 백반먹엇어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100047379.1.6",
          "form": "한식 좋아하세요?",
          "original_form": "한식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100047379.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100047379.1.8",
          "form": "저 한식도 좋고 다 좋아해요",
          "original_form": "저 한식도 좋고 다 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100047379.1.9",
          "form": "한식은 먹으면 건강해지는 느낌들어 좋아요",
          "original_form": "한식은 먹으면 건강해지는 느낌들어 좋아요",
          "speaker_id": "2",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100047379.1.10",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100047379.1.11",
          "form": "저도 한식 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 한식 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100047379.1.12",
          "form": "양식좋아하다가도 한식으로 돌아가더라구요",
          "original_form": "양식좋아하다가도 한식으로 돌아가더라구요",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100047379.1.13",
          "form": "건강상 그렇죠 ㅎㅎ",
          "original_form": "건강상 그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100047379.1.14",
          "form": "먹고 속도 좋고",
          "original_form": "먹고 속도 좋고",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100047379.1.15",
          "form": "결국 한국사람이라 그런가봐요",
          "original_form": "결국 한국사람이라 그런가봐요",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100047379.1.16",
          "form": "치킨 햄버거도 좋고",
          "original_form": "치킨 햄버거도 좋고",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100047379.1.17",
          "form": "그런데 한식 딱 먹으면 와 우리 건강해졌다",
          "original_form": "그런데 한식 딱 먹으면 와 우리 건강해졌다",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100047379.1.18",
          "form": "딸이랑 좋아해요 ㅋㅋ",
          "original_form": "딸이랑 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100047379.1.19",
          "form": "맞아요 ㅋㅋㅋㅋㅋ그리고 속이 편해요",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ그리고 속이 편해요",
          "speaker_id": "1",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100047379.1.20",
          "form": "딸 식습관 잘들이셨네요",
          "original_form": "딸 식습관 잘들이셨네요",
          "speaker_id": "1",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100047379.1.21",
          "form": "어릴때 건강하게 먹는 습관 들이기 쉽지 않은데 대단하세요",
          "original_form": "어릴때 건강하게 먹는 습관 들이기 쉽지 않은데 대단하세요",
          "speaker_id": "1",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100047379.1.22",
          "form": "ㅋㅋ근데 햄버거 더좋아해요 다욧 한다고 한식",
          "original_form": "ㅋㅋ근데 햄버거 더좋아해요 다욧 한다고 한식",
