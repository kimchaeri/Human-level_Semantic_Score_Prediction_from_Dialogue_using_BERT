      "utterance": [
        {
          "id": "MDRW2100044644.1.1",
          "form": "쌀국수 좋아하시나요???",
          "original_form": "쌀국수 좋아하시나요???",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044644.1.2",
          "form": "완전 좋아해요 ㅠㅠ",
          "original_form": "완전 좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044644.1.3",
          "form": "누누씌는요?",
          "original_form": "누누씌는요?",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044644.1.4",
          "form": "저는 쌀국수보단 파스타가 더 잘맞아요ㅋㅋ쿠ㅜ",
          "original_form": "저는 쌀국수보단 파스타가 더 잘맞아요ㅋㅋ쿠ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044644.1.5",
          "form": "오우 저도 파스타도 좋아하는데",
          "original_form": "오우 저도 파스타도 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044644.1.6",
          "form": "날씨가 이래서 그런가 쌀국수 완전 땡겨요",
          "original_form": "날씨가 이래서 그런가 쌀국수 완전 땡겨요",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044644.1.7",
          "form": "파스탄ㄴ 어떤 파스타 좋아하세요?",
          "original_form": "파스탄ㄴ 어떤 파스타 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044644.1.8",
          "form": "매운크림파스타요!!!",
          "original_form": "매운크림파스타요!!!",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044644.1.9",
          "form": "블루님은요?",
          "original_form": "블루님은요?",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044644.1.10",
          "form": "오 매운크림파스타!!!!",
          "original_form": "오 매운크림파스타!!!!",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044644.1.11",
          "form": "전 요즘 오일이 엄청 땡겨요 ㅠㅠ",
          "original_form": "전 요즘 오일이 엄청 땡겨요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044644.1.12",
          "form": "감바스 먹고싶어요",
          "original_form": "감바스 먹고싶어요",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044644.1.13",
          "form": "아아앜",
          "original_form": "아아앜",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044644.1.14",
          "form": "감바스 존맛이에요",
          "original_form": "감바스 존맛이에요",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044644.1.15",
          "form": "저 올리브오일 진짜 좋아하거든요",
          "original_form": "저 올리브오일 진짜 좋아하거든요",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044644.1.16",
          "form": "올리브오일에 맛있는거만 넣어서 끓이면 감바스!!",
          "original_form": "올리브오일에 맛있는거만 넣어서 끓이면 감바스!!",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044644.1.17",
          "form": "오!!! 전 여태까지ㅜ집에서 해먹을때 매번 시판 파스타 소스 넣어서",
          "original_form": "오!!! 전 여태까지ㅜ집에서 해먹을때 매번 시판 파스타 소스 넣어서",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044644.1.18",
          "form": "해먹얶는데 다음엔 누누씌방법으로 한번 해볼게요",
          "original_form": "해먹얶는데 다음엔 누누씌방법으로 한번 해볼게요",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044644.1.19",
          "form": "새우 페퍼론치노 마늘이 끓여진 올리브오일에",
          "original_form": "새우 페퍼론치노 마늘이 끓여진 올리브오일에",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044644.1.20",
          "form": "빵 찍어먹으면",
          "original_form": "빵 찍어먹으면",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044644.1.21",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044644.1.22",
          "form": "역시ㅠ맛잘ㅇ알 ㅠㅠ",
          "original_form": "역시ㅠ맛잘ㅇ알 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044644.1.23",
          "form": "저 갑자기 간장게장도 넘 먹고파요",
          "original_form": "저 갑자기 간장게장도 넘 먹고파요",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044644.1.24",
          "form": "헉 맛있겠다",
          "original_form": "헉 맛있겠다",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044644.1.25",
          "form": "+ 양념게장도...",
          "original_form": "+ 양념게장도...",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044644.1.26",
          "form": "그쵸..",
          "original_form": "그쵸..",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044644.1.27",
          "form": "휴 내일은 좀 맛있는걸 많이 먹어야겠어요",
          "original_form": "휴 내일은 좀 맛있는걸 많이 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044644.1.28",
          "form": "그럼요 그럼요",
          "original_form": "그럼요 그럼요",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044644.1.29",
          "form": "맛있는거 매일드세요",
          "original_form": "맛있는거 매일드세요",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044644.1.30",
          "form": "내일은 꼭꼭 쌀국수를 먹어야게ㅛ어요",
          "original_form": "내일은 꼭꼭 쌀국수를 먹어야게ㅛ어요",
          "speaker_id": "1",
          "time": "20210907 23:41"
        }
      ]
    }
