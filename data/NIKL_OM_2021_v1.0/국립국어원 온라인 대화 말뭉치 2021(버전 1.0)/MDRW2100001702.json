      "utterance": [
        {
          "id": "MDRW2100001702.1.1",
          "form": "강아지 키워?",
          "original_form": "강아지 키워?",
          "speaker_id": "2",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001702.1.2",
          "form": "아니 강아지 예전에 한마리 키웠었는데 지금은 안키워",
          "original_form": "아니 강아지 예전에 한마리 키웠었는데 지금은 안키워",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001702.1.3",
          "form": "나는 본가에서 네마리 키우거든 ㅎㅎ",
          "original_form": "나는 본가에서 네마리 키우거든 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001702.1.4",
          "form": "우와 네마리나 키워? 힘들겠다",
          "original_form": "우와 네마리나 키워? 힘들겠다",
          "speaker_id": "1",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001702.1.5",
          "form": "청소가 힘들긴 한데 그래도 귀여우니까 ㅋㅋ",
          "original_form": "청소가 힘들긴 한데 그래도 귀여우니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001702.1.6",
          "form": "그치 귀엽긴 하지. 밖에 나갔다 오면 엄청 반겨주잖아",
          "original_form": "그치 귀엽긴 하지. 밖에 나갔다 오면 엄청 반겨주잖아",
          "speaker_id": "1",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001702.1.7",
          "form": "그맛에 키우는거지~",
          "original_form": "그맛에 키우는거지~",
          "speaker_id": "2",
          "time": "20210819 18:17"
        },
        {
          "id": "MDRW2100001702.1.8",
          "form": "근데 사료값이나 동물병원비도 많이 나오지 않어?",
          "original_form": "근데 사료값이나 동물병원비도 많이 나오지 않어?",
          "speaker_id": "1",
          "time": "20210819 18:17"
        },
        {
          "id": "MDRW2100001702.1.9",
          "form": "엄청 많이 나와 ㅜㅜ",
          "original_form": "엄청 많이 나와 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 18:17"
        },
        {
          "id": "MDRW2100001702.1.10",
          "form": "돈은 많이 들어도 강아지 많으면 행복하긴 하겠다",
          "original_form": "돈은 많이 들어도 강아지 많으면 행복하긴 하겠다",
          "speaker_id": "1",
          "time": "20210819 18:18"
        },
        {
          "id": "MDRW2100001702.1.11",
          "form": "강아지들이 애교부릴때 귀여워서 하루 피로가 싹 가시긴 하지 ㅎㅎ",
          "original_form": "강아지들이 애교부릴때 귀여워서 하루 피로가 싹 가시긴 하지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001702.1.12",
          "form": "나중에 나도 강아지 다시 키우고 싶다",
          "original_form": "나중에 나도 강아지 다시 키우고 싶다",
          "speaker_id": "1",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001702.1.13",
          "form": "나중에 애기 낳으면 분양 해줄게",
          "original_form": "나중에 애기 낳으면 분양 해줄게",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001702.1.14",
          "form": "진짜? 대박~~~",
          "original_form": "진짜? 대박~~~",
