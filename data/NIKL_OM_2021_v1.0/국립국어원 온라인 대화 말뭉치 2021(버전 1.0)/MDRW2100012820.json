      "utterance": [
        {
          "id": "MDRW2100012820.1.1",
          "form": "집안일 자주하시나요?",
          "original_form": "집안일 자주하시나요?",
          "speaker_id": "2",
          "time": "20210825 01:58"
        },
        {
          "id": "MDRW2100012820.1.2",
          "form": "퇴근하면 기진맥진이라 평일엔 잘 못해요ㅜ",
          "original_form": "퇴근하면 기진맥진이라 평일엔 잘 못해요ㅜ",
          "speaker_id": "1",
          "time": "20210825 01:58"
        },
        {
          "id": "MDRW2100012820.1.3",
          "form": "자취하시나요?",
          "original_form": "자취하시나요?",
          "speaker_id": "2",
          "time": "20210825 01:58"
        },
        {
          "id": "MDRW2100012820.1.4",
          "form": "아뇨 집서 출퇴근해요",
          "original_form": "아뇨 집서 출퇴근해요",
          "speaker_id": "1",
          "time": "20210825 01:59"
        },
        {
          "id": "MDRW2100012820.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 01:59"
        },
        {
          "id": "MDRW2100012820.1.6",
          "form": "가족들이랑 같이살아요",
          "original_form": "가족들이랑 같이살아요",
          "speaker_id": "2",
          "time": "20210825 02:00"
        },
        {
          "id": "MDRW2100012820.1.7",
          "form": "백수라서 집안일 제가 거의합니다",
          "original_form": "백수라서 집안일 제가 거의합니다",
          "speaker_id": "2",
          "time": "20210825 02:00"
        },
        {
          "id": "MDRW2100012820.1.8",
          "form": "아이고 집안일이 해도해도 끝이 없는데",
          "original_form": "아이고 집안일이 해도해도 끝이 없는데",
          "speaker_id": "1",
          "time": "20210825 02:01"
        },
        {
          "id": "MDRW2100012820.1.9",
          "form": "힘드시겠어요",
          "original_form": "힘드시겠어요",
          "speaker_id": "1",
          "time": "20210825 02:01"
        },
        {
          "id": "MDRW2100012820.1.10",
          "form": "안해도 되는데 더러운 꼴을 못봐서 제가해요ㅋㅋ",
          "original_form": "안해도 되는데 더러운 꼴을 못봐서 제가해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 02:01"
        },
        {
          "id": "MDRW2100012820.1.11",
          "form": "룸메이트와 생활해본적 있으신가요?",
          "original_form": "룸메이트와 생활해본적 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 02:02"
        },
        {
          "id": "MDRW2100012820.1.12",
          "form": "아뇨 기숙사생활을 하셨었나요?",
          "original_form": "아뇨 기숙사생활을 하셨었나요?",
          "speaker_id": "1",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012820.1.13",
          "form": "아니요 할기회가 있었는데",
          "original_form": "아니요 할기회가 있었는데",
          "speaker_id": "2",
          "time": "20210825 02:05"
        },
        {
          "id": "MDRW2100012820.1.14",
          "form": "혼자가 편해서",
          "original_form": "혼자가 편해서",
          "speaker_id": "2",
          "time": "20210825 02:05"
        },
        {
          "id": "MDRW2100012820.1.15",
          "form": "나왔어요ㅋㅋ",
          "original_form": "나왔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 02:05"
        },
        {
          "id": "MDRW2100012820.1.16",
          "form": "ㅎㅎ 안할수 있으면 안하는게 낫죠",
          "original_form": "ㅎㅎ 안할수 있으면 안하는게 낫죠",
          "speaker_id": "1",
          "time": "20210825 02:08"
        },
        {
          "id": "MDRW2100012820.1.17",
          "form": "가족끼리 부대끼는것도 가끔은 힘든데 타인과 한 방이라니",
          "original_form": "가족끼리 부대끼는것도 가끔은 힘든데 타인과 한 방이라니",
          "speaker_id": "1",
          "time": "20210825 02:08"
        },
        {
          "id": "MDRW2100012820.1.18",
          "form": "맞아요 타인과 같이 생활할 생각하니 거부감 들더라고요",
          "original_form": "맞아요 타인과 같이 생활할 생각하니 거부감 들더라고요",
          "speaker_id": "2",
          "time": "20210825 02:09"
        },
        {
          "id": "MDRW2100012820.1.19",
          "form": "사실 그래서 결혼하는 사람들 보면 신기하고 대단하기도 해요",
          "original_form": "사실 그래서 결혼하는 사람들 보면 신기하고 대단하기도 해요",
          "speaker_id": "1",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100012820.1.20",
          "form": "저도 비혼으로써 공감이 안되네요",
          "original_form": "저도 비혼으로써 공감이 안되네요",
          "speaker_id": "2",
          "time": "20210825 02:12"
        }
      ]
