      "utterance": [
        {
          "id": "MDRW2100003975.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 오늘 라면을 먹었는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 오늘 라면을 먹었는데",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003975.1.2",
          "form": "이렇게 건강에서 한 걸음 더 멀어지네요",
          "original_form": "이렇게 건강에서 한 걸음 더 멀어지네요",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003975.1.3",
          "form": "아니 미용과 건강에 너무 안좋은고 아니냐구요",
          "original_form": "아니 미용과 건강에 너무 안좋은고 아니냐구요",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003975.1.4",
          "form": "그런 것 같아요",
          "original_form": "그런 것 같아요",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003975.1.5",
          "form": "저는 우유랑 김치랑 밥먹었어요 식욕 0이에요 흑",
          "original_form": "저는 우유랑 김치랑 밥먹었어요 식욕 0이에요 흑",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003975.1.6",
          "form": "우유랑 김치랑 밥이요...?",
          "original_form": "우유랑 김치랑 밥이요...?",
          "speaker_id": "2",
          "time": "20210821 14:24"
        },
        {
          "id": "MDRW2100003975.1.7",
          "form": "무슨 조합인지는 모르겠지만 건강은 해보여요",
          "original_form": "무슨 조합인지는 모르겠지만 건강은 해보여요",
          "speaker_id": "2",
          "time": "20210821 14:24"
        },
        {
          "id": "MDRW2100003975.1.8",
          "form": "네 괴식인데.. 제가 우유랑 밥먹는거 좋아해요",
          "original_form": "네 괴식인데.. 제가 우유랑 밥먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003975.1.9",
          "form": "건강ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "건강ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003975.1.10",
          "form": "우유랑 밥이라니",
          "original_form": "우유랑 밥이라니",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003975.1.11",
          "form": "너무 괴식이고 건강해보이네요",
          "original_form": "너무 괴식이고 건강해보이네요",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003975.1.12",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003975.1.13",
          "form": "name1님도 운동 안하시죠?",
          "original_form": "&name1&님도 운동 안하시죠?",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003975.1.14",
          "form": "저 가끔해요",
          "original_form": "저 가끔해요",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
