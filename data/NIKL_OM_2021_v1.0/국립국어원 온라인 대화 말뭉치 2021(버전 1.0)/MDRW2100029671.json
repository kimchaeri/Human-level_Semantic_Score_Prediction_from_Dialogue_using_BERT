      "utterance": [
        {
          "id": "MDRW2100029671.1.1",
          "form": "날씨가 좋네",
          "original_form": "날씨가 좋네",
          "speaker_id": "1",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029671.1.2",
          "form": "비가 오고 나서 그런지 점점 시원해져서 좋다",
          "original_form": "비가 오고 나서 그런지 점점 시원해져서 좋다",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029671.1.3",
          "form": "빨리 겨울이 왔으면 좋겠어",
          "original_form": "빨리 겨울이 왔으면 좋겠어",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029671.1.4",
          "form": "비가 와서 그런가 미세먼지도 적네",
          "original_form": "비가 와서 그런가 미세먼지도 적네",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029671.1.5",
          "form": "여름보단 겨울이좋아",
          "original_form": "여름보단 겨울이좋아",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029671.1.6",
          "form": "여름엔 벌레가 너무 많아",
          "original_form": "여름엔 벌레가 너무 많아",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029671.1.7",
          "form": "여름은 왜",
          "original_form": "여름은 왜",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029671.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029671.1.9",
          "form": "겨울은 춥잖아",
          "original_form": "겨울은 춥잖아",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029671.1.10",
          "form": "일단 우리집앞 송충이가",
          "original_form": "일단 우리집앞 송충이가",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029671.1.11",
