      "utterance": [
        {
          "id": "MDRW2100022611.1.1",
          "form": "하이볼 좋아하세요?",
          "original_form": "하이볼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022611.1.2",
          "form": "ㅎㅎㅎ하이볼 좋아하죠~",
          "original_form": "ㅎㅎㅎ하이볼 좋아하죠~",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022611.1.3",
          "form": "근데 하이볼이 원래 그 컵을 지칭하는 건 알고 계세요?",
          "original_form": "근데 하이볼이 원래 그 컵을 지칭하는 건 알고 계세요?",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022611.1.4",
          "form": "ㅋㅋㅋㅋㅋ저는 맥주랑 하이볼 젤 좋아해여",
          "original_form": "ㅋㅋㅋㅋㅋ저는 맥주랑 하이볼 젤 좋아해여",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022611.1.5",
          "form": "세로로 길쭉한 컵을 하이볼이라고 불러요",
          "original_form": "세로로 길쭉한 컵을 하이볼이라고 불러요",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022611.1.6",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.7",
          "form": "ㅎㅎ하이볼 짱 좋죠",
          "original_form": "ㅎㅎ하이볼 짱 좋죠",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.8",
          "form": "신기하네요 ㅋㅋㅋㅋ",
          "original_form": "신기하네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.9",
          "form": "거기에 보드카랑 토닉워터 이런거 넣잖아요",
          "original_form": "거기에 보드카랑 토닉워터 이런거 넣잖아요",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.10",
          "form": "산토리위스키 맛잇어요 ㅠ",
          "original_form": "산토리위스키 맛잇어요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.11",
          "form": "그래서 뭔가 깔끔해서 좋아요 ㅜㅜ",
          "original_form": "그래서 뭔가 깔끔해서 좋아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.12",
          "form": "산토리 위스키는 안먹어봤네용 무슨 맛이에요?",
          "original_form": "산토리 위스키는 안먹어봤네용 무슨 맛이에요?",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.13",
          "form": "맞아요 ㅠㅠ아니면 탄산수넣어두되구",
          "original_form": "맞아요 ㅠㅠ아니면 탄산수넣어두되구",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.14",
          "form": "전 위스키 막 탄내나는 맛만 먹어봐가지고 ㅋㅋㅋ",
          "original_form": "전 위스키 막 탄내나는 맛만 먹어봐가지고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.15",
          "form": "위스키는 제 취향 아닌걸로 정리 했어요 ㅜ",
          "original_form": "위스키는 제 취향 아닌걸로 정리 했어요 ㅜ",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022611.1.16",
          "form": "앜ㅋㅋㅋㅋ그렇군요 저는위스키 많이먹진목햇어요",
          "original_form": "앜ㅋㅋㅋㅋ그렇군요 저는위스키 많이먹진목햇어요",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022611.1.17",
          "form": "산토리 위스키는 어디서 드셔보신거에요?ㅋㅋㅋ",
          "original_form": "산토리 위스키는 어디서 드셔보신거에요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022611.1.18",
          "form": "음 레몬이랑같이들어가는데 깔끔해요 ㅋㅋㅋㅋ",
          "original_form": "음 레몬이랑같이들어가는데 깔끔해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022611.1.19",
          "form": "보통 이자카야같은데서요!",
          "original_form": "보통 이자카야같은데서요!",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022611.1.20",
          "form": "아 한잔씩 나오는 거요~?",
          "original_form": "아 한잔씩 나오는 거요~?",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022611.1.21",
          "form": "이자카야 얘기하니까 꼬치 먹고 싶다..",
          "original_form": "이자카야 얘기하니까 꼬치 먹고 싶다..",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022611.1.22",
          "form": "주로 이자카야가면 산토리하이볼이 많아요",
          "original_form": "주로 이자카야가면 산토리하이볼이 많아요",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022611.1.23",
          "form": "꼬치 좀 넉넉히 줬음 좋겠어요...",
          "original_form": "꼬치 좀 넉넉히 줬음 좋겠어요...",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022611.1.24",
          "form": "ㅋㅋㅋㅋㅋ크 맛잇겟어요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ크 맛잇겟어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022611.1.25",
          "form": "그쵸...이자카야 넘비싸요",
          "original_form": "그쵸...이자카야 넘비싸요",
          "speaker_id": "2",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022611.1.26",
          "form": "하나씩 먹으면 절반이 사라져버리고...",
          "original_form": "하나씩 먹으면 절반이 사라져버리고...",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022611.1.27",
          "form": "그럼 서로 눈치게임 시작 ㅋㅋㅋㅋ",
          "original_form": "그럼 서로 눈치게임 시작 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022611.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아뇨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아뇨",
          "speaker_id": "2",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022611.1.29",
          "form": ";; 안쳐지네",
          "original_form": ";; 안쳐지네",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022611.1.30",
          "form": "혹시 그거 먹어봤어요 일본 술?",
          "original_form": "혹시 그거 먹어봤어요 일본 술?",
