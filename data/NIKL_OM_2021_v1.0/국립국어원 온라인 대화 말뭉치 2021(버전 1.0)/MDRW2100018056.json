      "utterance": [
        {
          "id": "MDRW2100018056.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018056.1.2",
          "form": "저 name2예요",
          "original_form": "저 &name2&예요",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018056.1.3",
          "form": "name2님 닉넴 name2예요",
          "original_form": "&name2&님 닉넴 &name2&예요",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100018056.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100018056.1.5",
          "form": "네 방금 설정했어요 ㅋㅋㅋㅋ",
          "original_form": "네 방금 설정했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100018056.1.6",
          "form": "넷플릭스에서 보시는 거 있나요?",
          "original_form": "넷플릭스에서 보시는 거 있나요?",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018056.1.7",
          "form": "저지금은 해지했고",
          "original_form": "저지금은 해지했고",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018056.1.8",
          "form": "잼게본게 미녀삼총사랑",
          "original_form": "잼게본게 미녀삼총사랑",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018056.1.9",
          "form": "올드가드요",
          "original_form": "올드가드요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018056.1.10",
          "form": "그리고 미니멀라이프 다큐요!",
          "original_form": "그리고 미니멀라이프 다큐요!",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018056.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018056.1.12",
          "form": "넷플이 다큐로 유명하다고 하긴 하더라고요",
          "original_form": "넷플이 다큐로 유명하다고 하긴 하더라고요",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018056.1.13",
          "form": "문어 다큐가 진짜 유명하던데",
          "original_form": "문어 다큐가 진짜 유명하던데",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018056.1.14",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018056.1.15",
          "form": "왤케노잼일것같죠..",
          "original_form": "왤케노잼일것같죠..",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018056.1.16",
          "form": "그래도 나중에 찾아봐야겠네욬ㅋ",
          "original_form": "그래도 나중에 찾아봐야겠네욬ㅋ",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018056.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저는 사실 넷플 잘 안 봐서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저는 사실 넷플 잘 안 봐서",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018056.1.18",
          "form": "그거도 안봤는데",
          "original_form": "그거도 안봤는데",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018056.1.19",
          "form": "재밌으니까 유명하지 않을까요?",
          "original_form": "재밌으니까 유명하지 않을까요?",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018056.1.20",
          "form": "저는 넷플로 하나봐요",
          "original_form": "저는 넷플로 하나봐요",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018056.1.21",
          "form": "체인지데이즈",
          "original_form": "체인지데이즈",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018056.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018056.1.23",
          "form": "장르가뭐에요?",
          "original_form": "장르가뭐에요?",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018056.1.24",
          "form": "뭐예여?!",
          "original_form": "뭐예여?!",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018056.1.25",
          "form": "하트시그널 같은건데",
          "original_form": "하트시그널 같은건데",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018056.1.26",
          "form": "예능?이라고 해야되나",
          "original_form": "예능?이라고 해야되나",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018056.1.27",
          "form": "사귀는 연인들이",
          "original_form": "사귀는 연인들이",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018056.1.28",
          "form": "권태기와서 ㅋㅋㅋ",
          "original_form": "권태기와서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018056.1.29",
          "form": "세커플이모여서",
          "original_form": "세커플이모여서",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018056.1.30",
          "form": "서로 데이트 다른 커플이랑 하고",
          "original_form": "서로 데이트 다른 커플이랑 하고",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018056.1.31",
          "form": "본인애인이랑 더 만날지 말지 결정하는...?",
          "original_form": "본인애인이랑 더 만날지 말지 결정하는...?",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018056.1.32",
          "form": "비슷하게 환승연애랑 돌싱글즈가 요즘 핫해요",
          "original_form": "비슷하게 환승연애랑 돌싱글즈가 요즘 핫해요",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018056.1.33",
          "form": "전 그중에서 환승연애가 젤 잼나요",
          "original_form": "전 그중에서 환승연애가 젤 잼나요",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018056.1.34",
          "form": "그거는 헤어진 애들이 각자 구남친 구여친 숨기고 8명 모여서",
          "original_form": "그거는 헤어진 애들이 각자 구남친 구여친 숨기고 8명 모여서",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018056.1.35",
          "form": "구애인으랑 잘되거나 새로운 인연을 사귀거나... 하능것",
          "original_form": "구애인으랑 잘되거나 새로운 인연을 사귀거나... 하능것",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018056.1.36",
          "form": "다 마라맛이에요",
          "original_form": "다 마라맛이에요",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018056.1.37",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.38",
          "form": "매운데요",
          "original_form": "매운데요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.40",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.41",
          "form": "존잼쓰..",
          "original_form": "존잼쓰..",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.42",
          "form": "유교한국에서는 못하겠네요",
          "original_form": "유교한국에서는 못하겠네요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.43",
          "form": "저도 처음에친구가 보라고할땐",
          "original_form": "저도 처음에친구가 보라고할땐",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.44",
          "form": "욕하면사 시작햇는데",
          "original_form": "욕하면사 시작햇는데",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.45",
          "form": "네 저도 볼래요..",
          "original_form": "네 저도 볼래요..",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.46",
          "form": "스며들어ㅛ어요",
          "original_form": "스며들어ㅛ어요",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018056.1.48",
          "form": "체며들다",
          "original_form": "체며들다",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018056.1.49",
          "form": "ㅋㅋㅋㅋㅋ 존잼인것,,",
          "original_form": "ㅋㅋㅋㅋㅋ 존잼인것,,",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018056.1.50",
          "form": "하트시그널 재질이에요",
          "original_form": "하트시그널 재질이에요",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018056.1.51",
          "form": "이런게잼써요 외국꺼는 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이런게잼써요 외국꺼는 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018056.1.52",
          "form": "하트시그널은 보셨나요?",
          "original_form": "하트시그널은 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018056.1.53",
          "form": "하트시그널 안봤어요",
          "original_form": "하트시그널 안봤어요",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018056.1.54",
          "form": "근데 체인즈는 엄청잼날것같아요",
          "original_form": "근데 체인즈는 엄청잼날것같아요",
          "speaker_id": "2",
          "time": "20210826 22:43"
        }
      ]
