      "utterance": [
        {
          "id": "MDRW2100001846.1.1",
          "form": "최근에 보신 영화 있으세요?",
          "original_form": "최근에 보신 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001846.1.2",
          "form": "저 정글크루즈 봤어요",
          "original_form": "저 정글크루즈 봤어요",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001846.1.3",
          "form": "저는 블랙위도우요!",
          "original_form": "저는 블랙위도우요!",
          "speaker_id": "1",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001846.1.4",
          "form": "오 저는 블랙위도우 이해가 안가요 ㅜ 내용이..",
          "original_form": "오 저는 블랙위도우 이해가 안가요 ㅜ 내용이..",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001846.1.5",
          "form": "마블을 잘 안봐서그런건가",
          "original_form": "마블을 잘 안봐서그런건가",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001846.1.6",
          "form": "아 그러면 이해못할수도요 저는 마블팬이라 다봤거든요",
          "original_form": "아 그러면 이해못할수도요 저는 마블팬이라 다봤거든요",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001846.1.7",
          "form": "크루엘라는 보셨나요?",
          "original_form": "크루엘라는 보셨나요?",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001846.1.8",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001846.1.9",
          "form": "요새는 vod가 빨리나와서 좋아요 크루엘라는 집에서봤어요",
          "original_form": "요새는 vod가 빨리나와서 좋아요 크루엘라는 집에서봤어요",
          "speaker_id": "1",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001846.1.10",
          "form": "가볍게 재밋게보기 좋았네요",
          "original_form": "가볍게 재밋게보기 좋았네요",
          "speaker_id": "1",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001846.1.11",
          "form": "저도 집에서 사서 봤어요 ㅇㅇ",
          "original_form": "저도 집에서 사서 봤어요 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001846.1.12",
          "form": "ㅎㅎ 크루엘라는 재밌더라고요",
          "original_form": "ㅎㅎ 크루엘라는 재밌더라고요",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001846.1.13",
          "form": "디즈니 좋아해서 저는",
          "original_form": "디즈니 좋아해서 저는",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001846.1.14",
          "form": "엠마 너무 이쁘죠~",
          "original_form": "엠마 너무 이쁘죠~",
          "speaker_id": "1",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001846.1.15",
          "form": "진짜 너무 이뻐요 넘나 잘어울롬",
          "original_form": "진짜 너무 이뻐요 넘나 잘어울롬",
          "speaker_id": "2",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001846.1.16",
          "form": "림",
          "original_form": "림",
          "speaker_id": "2",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001846.1.17",
          "form": "그쵸 연기도 너무 잘하고 ~ 표정변할때 와 했어요",
          "original_form": "그쵸 연기도 너무 잘하고 ~ 표정변할때 와 했어요",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001846.1.18",
          "form": "그니까요 사이다..",
          "original_form": "그니까요 사이다..",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001846.1.19",
          "form": "다른 디즈니 영화 어떤거 좋아하세료?",
          "original_form": "다른 디즈니 영화 어떤거 좋아하세료?",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001846.1.20",
          "form": "저 디즈니 최근거는 루카? 봐시어요",
          "original_form": "저 디즈니 최근거는 루카? 봐시어요",
