      "utterance": [
        {
          "id": "MDRW2100021029.1.1",
          "form": "최근에 구매하신 물건 있나요?",
          "original_form": "최근에 구매하신 물건 있나요?",
          "speaker_id": "1",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100021029.1.2",
          "form": "네 최근에 옷을 샀어요",
          "original_form": "네 최근에 옷을 샀어요",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021029.1.3",
          "form": "오 .. 가을 옷 사셨나요?",
          "original_form": "오 .. 가을 옷 사셨나요?",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021029.1.4",
          "form": "음 가을 옷? 블레이저 샀습니다",
          "original_form": "음 가을 옷? 블레이저 샀습니다",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021029.1.5",
          "form": "그쳐 이제 겉옷이 필요한 시기죠",
          "original_form": "그쳐 이제 겉옷이 필요한 시기죠",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021029.1.6",
          "form": "최근에 구입하신 물건이있나요",
          "original_form": "최근에 구입하신 물건이있나요",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021029.1.7",
          "form": "저는 최근에 핸드폰 바꿨어요!",
          "original_form": "저는 최근에 핸드폰 바꿨어요!",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100021029.1.8",
          "form": "가을옷도 구매하구... 이것저것 쇼핑했네요..",
          "original_form": "가을옷도 구매하구... 이것저것 쇼핑했네요..",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100021029.1.9",
          "form": "오 핸드폰 어떤 기종으로 바꾸셨나요?",
          "original_form": "오 핸드폰 어떤 기종으로 바꾸셨나요?",
          "speaker_id": "2",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100021029.1.10",
          "form": "최근에 나온 플립으로 바꿨습니다!",
          "original_form": "최근에 나온 플립으로 바꿨습니다!",
          "speaker_id": "1",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100021029.1.11",
          "form": "오 플립.. 접히는 스마트폰이라 엄천 신기하던데",
          "original_form": "오 플립.. 접히는 스마트폰이라 엄천 신기하던데",
          "speaker_id": "2",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100021029.1.12",
          "form": "불편하진않으신가여",
          "original_form": "불편하진않으신가여",
          "speaker_id": "2",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100021029.1.13",
          "form": "아직 못받았어요.. 주문이 밀리나봐요..ㅠ 근데 투고해서 써보니까 괜찮더라구요 !",
          "original_form": "아직 못받았어요.. 주문이 밀리나봐요..ㅠ 근데 투고해서 써보니까 괜찮더라구요 !",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021029.1.14",
          "form": "아 그렇군여",
          "original_form": "아 그렇군여",
          "speaker_id": "2",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100021029.1.15",
          "form": "저도 나중에 한번 써보고싶네요",
          "original_form": "저도 나중에 한번 써보고싶네요",
          "speaker_id": "2",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100021029.1.16",
          "form": "네네 예전에 폴더폰 썼었었는데 다시 돌아가는 기분이라 신기하네요 ..",
