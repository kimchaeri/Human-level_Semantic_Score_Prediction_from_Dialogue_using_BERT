      "utterance": [
        {
          "id": "MDRW2100018392.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018392.1.2",
          "form": "안녕하세요. 저녁 드셨어요?",
          "original_form": "안녕하세요. 저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018392.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018392.1.4",
          "form": "제가 저녁을 햄버걸먹었는데 어디껄까요?",
          "original_form": "제가 저녁을 햄버걸먹었는데 어디껄까요?",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018392.1.5",
          "form": "햄버거하면 버거킹이죠",
          "original_form": "햄버거하면 버거킹이죠",
          "speaker_id": "2",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018392.1.6",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "2",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018392.1.7",
          "form": "와우!",
          "original_form": "와우!",
          "speaker_id": "1",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018392.1.8",
          "form": "어떤버거일까요?",
          "original_form": "어떤버거일까요?",
          "speaker_id": "1",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018392.1.9",
          "form": "헉 맞췄다니...",
          "original_form": "헉 맞췄다니...",
          "speaker_id": "2",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100018392.1.10",
          "form": "몬스터버거?",
          "original_form": "몬스터버거?",
          "speaker_id": "2",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100018392.1.11",
          "form": "땡",
          "original_form": "땡",
          "speaker_id": "1",
          "time": "20210827 01:11"
        },
        {
          "id": "MDRW2100018392.1.12",
          "form": "틀렸답니다",
          "original_form": "틀렸답니다",
          "speaker_id": "1",
          "time": "20210827 01:11"
        },
        {
          "id": "MDRW2100018392.1.13",
          "form": "트리플 머쉬룸 버거...? 는 절판됐댔는데...",
          "original_form": "트리플 머쉬룸 버거...? 는 절판됐댔는데...",
          "speaker_id": "2",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100018392.1.14",
          "form": "또 뭐가 있을까요...?",
          "original_form": "또 뭐가 있을까요...?",
          "speaker_id": "2",
          "time": "20210827 01:13"
        },
        {
          "id": "MDRW2100018392.1.15",
          "form": "ㅠㅠ흑흑",
          "original_form": "ㅠㅠ흑흑",
          "speaker_id": "1",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018392.1.16",
          "form": "정답을 알려드릴까요",
          "original_form": "정답을 알려드릴까요",
          "speaker_id": "1",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018392.1.17",
          "form": "네 어서 말씀해보세요 궁금..",
          "original_form": "네 어서 말씀해보세요 궁금..",
          "speaker_id": "2",
          "time": "20210827 01:17"
        },
        {
          "id": "MDRW2100018392.1.18",
          "form": "ㅋㅋㅋㅋ상어버거랍니다",
          "original_form": "ㅋㅋㅋㅋ상어버거랍니다",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100018392.1.19",
          "form": "여기서다음 심층질문",
          "original_form": "여기서다음 심층질문",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100018392.1.20",
          "form": "세상에 상어버거라니...",
          "original_form": "세상에 상어버거라니...",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100018392.1.21",
          "form": "들어본적도 없는데요...!",
          "original_form": "들어본적도 없는데요...!",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100018392.1.22",
          "form": "1아기상어\n2엄마상어\n3아빠상어\n무슨버거를 먹었을까요?",
          "original_form": "1아기상어\n2엄마상어\n3아빠상어\n무슨버거를 먹었을까요?",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100018392.1.23",
          "form": "새우버거랍니다",
          "original_form": "새우버거랍니다",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100018392.1.24",
          "form": "아빠상어정도는 돼야 배가 부르지 않을까요?",
          "original_form": "아빠상어정도는 돼야 배가 부르지 않을까요?",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100018392.1.25",
          "form": "종류가 세가지나 되다니..",
          "original_form": "종류가 세가지나 되다니..",
          "speaker_id": "2",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100018392.1.26",
          "form": "아기상어는 솔직히 먹기엔 좀 불쌍하죠",
          "original_form": "아기상어는 솔직히 먹기엔 좀 불쌍하죠",
          "speaker_id": "2",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100018392.1.27",
          "form": "ㅋㅋㅋㅋ 오오 버거에따라 크기가다르다는걸",
          "original_form": "ㅋㅋㅋㅋ 오오 버거에따라 크기가다르다는걸",
          "speaker_id": "1",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100018392.1.28",
          "form": "눈치채셨군요?!",
          "original_form": "눈치채셨군요?!",
          "speaker_id": "1",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100018392.1.29",
          "form": "후후후 제가 좀 통찰력이 있었나요?",
          "original_form": "후후후 제가 좀 통찰력이 있었나요?",
          "speaker_id": "2",
          "time": "20210827 01:30"
        },
        {
          "id": "MDRW2100018392.1.30",
          "form": "그래서 뭐 드셨어요?!",
          "original_form": "그래서 뭐 드셨어요?!",
          "speaker_id": "2",
          "time": "20210827 01:30"
        },
        {
          "id": "MDRW2100018392.1.31",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018392.1.32",
          "form": "엄마상어버거랍니다",
          "original_form": "엄마상어버거랍니다",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018392.1.33",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100018392.1.34",
          "form": "헐... 내가 틀렸다니....ㅠㅠㅠ",
          "original_form": "헐... 내가 틀렸다니....ㅠㅠㅠ",
          "speaker_id": "2",
