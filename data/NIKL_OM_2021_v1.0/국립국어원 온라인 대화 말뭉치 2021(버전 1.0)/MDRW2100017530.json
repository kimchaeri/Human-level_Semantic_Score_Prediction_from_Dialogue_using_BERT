      "utterance": [
        {
          "id": "MDRW2100017530.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017530.1.2",
          "form": "게임 좋아해요?",
          "original_form": "게임 좋아해요?",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017530.1.3",
          "form": "모바일 게임 즐겨해요~",
          "original_form": "모바일 게임 즐겨해요~",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017530.1.4",
          "form": "프렌즈타운같은 퍼즐류ㅎㅎ",
          "original_form": "프렌즈타운같은 퍼즐류ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017530.1.5",
          "form": "오 퍼즐 잘 맞추시나봐요 ㅎㅎ",
          "original_form": "오 퍼즐 잘 맞추시나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017530.1.6",
          "form": "그 전엔 무슨 게임 하셨어요?",
          "original_form": "그 전엔 무슨 게임 하셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017530.1.7",
          "form": "작년에 한창 유행하던",
          "original_form": "작년에 한창 유행하던",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017530.1.8",
          "form": "어몽어스요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어몽어스요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017530.1.9",
          "form": "헐 저 어몽어스 진짜 재밌게 했는데 ㅎㅎ",
          "original_form": "헐 저 어몽어스 진짜 재밌게 했는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:01"
        },
        {
          "id": "MDRW2100017530.1.10",
          "form": "초반엔 엄청 재밌었는ㄷ0",
          "original_form": "초반엔 엄청 재밌었는ㄷ0",
          "speaker_id": "2",
          "time": "20210826 20:04"
        },
        {
          "id": "MDRW2100017530.1.11",
          "form": "근데 비제이들이 홍보(?)하면서 부터 초등학생도 많이 하기 시작하더니..",
          "original_form": "근데 비제이들이 홍보(?)하면서 부터 초등학생도 많이 하기 시작하더니..",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017530.1.12",
          "form": "업데이트도 하고.. 하면서 조금 흥미 떨어졌어요.",
          "original_form": "업데이트도 하고.. 하면서 조금 흥미 떨어졌어요.",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017530.1.13",
          "form": "아 맞아요 젤 기본맵에 어그로끌려서",
          "original_form": "아 맞아요 젤 기본맵에 어그로끌려서",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017530.1.14",
          "form": "다른맵갔더니 넘 어렵고....",
          "original_form": "다른맵갔더니 넘 어렵고....",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017530.1.15",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017530.1.16",
          "form": "그에 비해 아까 말씀하신 타운즈는 어떤 게임이에요?",
          "original_form": "그에 비해 아까 말씀하신 타운즈는 어떤 게임이에요?",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017530.1.17",
          "form": "같은모양 3개이상 맞춰서 소멸시키는?",
          "original_form": "같은모양 3개이상 맞춰서 소멸시키는?",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017530.1.18",
          "form": "기본적인 퍼즐게임이요ㅋㅋㅋ카카오꺼",
          "original_form": "기본적인 퍼즐게임이요ㅋㅋㅋ카카오꺼",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017530.1.19",
          "form": "그거 예전에 유행했던 애니팡이랑 비슷하네요, 맞나요?",
          "original_form": "그거 예전에 유행했던 애니팡이랑 비슷하네요, 맞나요?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017530.1.20",
          "form": "네네 맞아요!!",
          "original_form": "네네 맞아요!!",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017530.1.21",
          "form": "게임 뭐 하는거 있으세요?",
          "original_form": "게임 뭐 하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017530.1.22",
          "form": "저 모바일게임은 안하고 메이플스토리 해요~",
          "original_form": "저 모바일게임은 안하고 메이플스토리 해요~",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017530.1.23",
          "form": "아~~메이플 저한테는 추억의겜ㅜ",
          "original_form": "아~~메이플 저한테는 추억의겜ㅜ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017530.1.24",
          "form": "메이플도 모바일 있잖아요",
          "original_form": "메이플도 모바일 있잖아요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017530.1.25",
          "form": "모바일 메이플 해봤는데 좀 재미없더라구요..",
          "original_form": "모바일 메이플 해봤는데 좀 재미없더라구요..",
