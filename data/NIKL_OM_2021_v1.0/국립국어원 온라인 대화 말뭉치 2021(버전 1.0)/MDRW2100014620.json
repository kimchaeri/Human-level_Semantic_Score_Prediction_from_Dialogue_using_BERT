        },
        {
          "id": "MDRW2100014620.1.2",
          "form": "부대ㅉ개에다가",
          "original_form": "부대ㅉ개에다가",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014620.1.3",
          "form": "먹었으용 ㅋㅋ",
          "original_form": "먹었으용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014620.1.4",
          "form": "오 부찌 맛있었겠네요",
          "original_form": "오 부찌 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014620.1.5",
          "form": "님이 만드셨나염? ㅋㅋ",
          "original_form": "님이 만드셨나염? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014620.1.6",
          "form": "아녓ㅋㅋㅋㅋ",
          "original_form": "아녓ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014620.1.7",
          "form": "땅스부부 부대찌개였나??",
          "original_form": "땅스부부 부대찌개였나??",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014620.1.8",
          "form": "아 ㅋㅋㅋㅋ 포장해오셨군영ㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ 포장해오셨군영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014620.1.9",
          "form": "나도 먹어봤음ㅋㅋ",
          "original_form": "나도 먹어봤음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014620.1.10",
          "form": "괜찮드라구용 ㅋㅋㅋ",
          "original_form": "괜찮드라구용 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100014620.1.11",
          "form": "가격도 저렴했던거 같은데",
          "original_form": "가격도 저렴했던거 같은데",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100014620.1.12",
          "form": "가성비 좋았음ㅋㅋ",
          "original_form": "가성비 좋았음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100014620.1.13",
          "form": "옹 ㅋㅋ맞아 ㅋㅋㅋ",
          "original_form": "옹 ㅋㅋ맞아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100014620.1.14",
          "form": "배고프다......",
          "original_form": "배고프다......",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100014620.1.15",
          "form": "지굼??ㅋㅋ",
          "original_form": "지굼??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100014620.1.16",
          "form": "애들 저녁먹이는데\r\n진이 다 빠져서 안먹었더니",
          "original_form": "애들 저녁먹이는데\r\n진이 다 빠져서 안먹었더니",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100014620.1.17",
          "form": "배가 고프네 이제ㅠ",
          "original_form": "배가 고프네 이제ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100014620.1.18",
          "form": "헝 ㅠㅠㅠ",
          "original_form": "헝 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 07:57"
        },
        {
          "id": "MDRW2100014620.1.19",
          "form": "아침에라도 뭐드시죵...",
          "original_form": "아침에라도 뭐드시죵...",
          "speaker_id": "2",
          "time": "20210826 07:57"
        },
        {
          "id": "MDRW2100014620.1.20",
          "form": "네ㅋㅋ 당연히 먹어야죠",
          "original_form": "네ㅋㅋ 당연히 먹어야죠",
          "speaker_id": "1",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100014620.1.21",
          "form": "오늘메뉴는 멉니깡 ㅋㅋ",
          "original_form": "오늘메뉴는 멉니깡 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:46"
        },
        {
          "id": "MDRW2100014620.1.22",
          "form": "애기들은 밥주고\r\n나는 떡볶이 밀키트있길래\r\n떡볶이 해먹을까 고민중ㅋㅋㅈ",
          "original_form": "애기들은 밥주고\r\n나는 떡볶이 밀키트있길래\r\n떡볶이 해먹을까 고민중ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100014620.1.23",
          "form": "떡볶이 맛있쥐 ㅋㅋㅋㅋ",
          "original_form": "떡볶이 맛있쥐 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100014620.1.24",
          "form": "밀키트면 그래도 간단하게해먹을수있겠넹",
          "original_form": "밀키트면 그래도 간단하게해먹을수있겠넹",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100014620.1.25",
          "form": "아 근데 저게 3인분이라ㅋㅋ",
          "original_form": "아 근데 저게 3인분이라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:48"
        },
        {
          "id": "MDRW2100014620.1.26",
          "form": "맨날 먹고싶어도\r\n혼자 다 못먹으니 참게 되는ㅋ",
          "original_form": "맨날 먹고싶어도\r\n혼자 다 못먹으니 참게 되는ㅋ",
          "speaker_id": "1",
          "time": "20210826 09:48"
        }
