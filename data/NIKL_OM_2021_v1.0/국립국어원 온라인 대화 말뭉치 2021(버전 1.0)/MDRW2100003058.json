      "utterance": [
        {
          "id": "MDRW2100003058.1.1",
          "form": "가장 최근에 구매한 물건이 뭐에요?",
          "original_form": "가장 최근에 구매한 물건이 뭐에요?",
          "speaker_id": "1",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100003058.1.2",
          "form": "다지기요.ㅋㅋㅋ",
          "original_form": "다지기요.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100003058.1.3",
          "form": "다지기요? 야채다지기?",
          "original_form": "다지기요? 야채다지기?",
          "speaker_id": "1",
          "time": "20210820 18:08"
        },
        {
          "id": "MDRW2100003058.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 18:09"
        },
        {
          "id": "MDRW2100003058.1.5",
          "form": "락앤락몰에 적립금 모아서 야채다지기 샀어요.",
          "original_form": "락앤락몰에 적립금 모아서 야채다지기 샀어요.",
          "speaker_id": "2",
          "time": "20210820 18:09"
        },
        {
          "id": "MDRW2100003058.1.6",
          "form": "볶음밥 해 먹을 때 쓰려고",
          "original_form": "볶음밥 해 먹을 때 쓰려고",
          "speaker_id": "2",
          "time": "20210820 18:10"
        },
        {
          "id": "MDRW2100003058.1.7",
          "form": "오 꿀이네요:)",
          "original_form": "오 꿀이네요:)",
          "speaker_id": "1",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003058.1.8",
          "form": "얼마전에 이유식 해먹일때\r\n유용하게 쓰던 야채다지기 고장나서\r\n내돈내산 했어요 저는 ㅠ",
          "original_form": "얼마전에 이유식 해먹일때\r\n유용하게 쓰던 야채다지기 고장나서\r\n내돈내산 했어요 저는 ㅠ",
          "speaker_id": "1",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003058.1.9",
          "form": "한번 사용했어요. 양파다질 때",
          "original_form": "한번 사용했어요. 양파다질 때",
          "speaker_id": "2",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003058.1.10",
          "form": "아깝네요.ㅜㅜ",
          "original_form": "아깝네요.ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003058.1.11",
          "form": "잘 다져지던가요? ㅋㅋ\r\n저는 키친플라워꺼 샀는데\r\n너무 크고 무겁고 하더라구요ㅠ",
          "original_form": "잘 다져지던가요? ㅋㅋ\r\n저는 키친플라워꺼 샀는데\r\n너무 크고 무겁고 하더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003058.1.12",
          "form": "잘 되던데요. 전 그렇게 크진 않고 적당했어요",
          "original_form": "잘 되던데요. 전 그렇게 크진 않고 적당했어요",
          "speaker_id": "2",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003058.1.13",
          "form": "다음엔 락앤락꺼를 사봐야겠네요ㅋ",
          "original_form": "다음엔 락앤락꺼를 사봐야겠네요ㅋ",
          "speaker_id": "1",
          "time": "20210820 18:15"
        },
        {
          "id": "MDRW2100003058.1.14",
          "form": "또 고장 날까요?ㅋㅋㅋ",
          "original_form": "또 고장 날까요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003058.1.15",
          "form": "쉽게 고장날 애는 아니긴 하네여 ㅠㅋㅋ",
          "original_form": "쉽게 고장날 애는 아니긴 하네여 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100003058.1.16",
          "form": "최근에 뭐 구매하셨어요?",
          "original_form": "최근에 뭐 구매하셨어요?",
          "speaker_id": "2",
          "time": "20210820 18:18"
        },
        {
          "id": "MDRW2100003058.1.17",
          "form": "저는 꼬맹이 소근육 발달에\r\n좋은 장난감이요 ㅋㅋ",
          "original_form": "저는 꼬맹이 소근육 발달에\r\n좋은 장난감이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:19"
        },
        {
          "id": "MDRW2100003058.1.18",
          "form": "ㅋㅋㅋ애들 장난감 너무 귀엽던데요",
          "original_form": "ㅋㅋㅋ애들 장난감 너무 귀엽던데요",
          "speaker_id": "2",
          "time": "20210820 18:19"
        },
        {
          "id": "MDRW2100003058.1.19",
          "form": "가격도 좀 작고 귀여울순 없을까요ㅋㅋ",
          "original_form": "가격도 좀 작고 귀여울순 없을까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100003058.1.20",
          "form": "내아이를 위한거니 사긴 하지만\r\n늘 가격은 참 사악하단 생각이ㅋ",
          "original_form": "내아이를 위한거니 사긴 하지만\r\n늘 가격은 참 사악하단 생각이ㅋ",
          "speaker_id": "1",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100003058.1.21",
          "form": "그러게요. 장난감도 너무 비싸죠?ㅜㅜ",
          "original_form": "그러게요. 장난감도 너무 비싸죠?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100003058.1.22",
          "form": "네ㅠ 정말 너무 비싼거 같아영",
          "original_form": "네ㅠ 정말 너무 비싼거 같아영",
          "speaker_id": "1",
          "time": "20210820 18:21"
        }
      ]
    }
