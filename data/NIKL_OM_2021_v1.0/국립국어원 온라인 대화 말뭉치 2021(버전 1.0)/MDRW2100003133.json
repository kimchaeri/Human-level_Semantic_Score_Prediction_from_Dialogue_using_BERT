      "utterance": [
        {
          "id": "MDRW2100003133.1.1",
          "form": "저녁 되니까 엄청 시원하다",
          "original_form": "저녁 되니까 엄청 시원하다",
          "speaker_id": "2",
          "time": "20210820 19:09"
        },
        {
          "id": "MDRW2100003133.1.2",
          "form": "그러게~ 그런데 새들이 왜 이렇게 시끄러워",
          "original_form": "그러게~ 그런데 새들이 왜 이렇게 시끄러워",
          "speaker_id": "1",
          "time": "20210820 19:10"
        },
        {
          "id": "MDRW2100003133.1.3",
          "form": "무슨 일 있나?",
          "original_form": "무슨 일 있나?",
          "speaker_id": "2",
          "time": "20210820 19:10"
        },
        {
          "id": "MDRW2100003133.1.4",
          "form": "까치 까마귀 아주 난리네 ㅋㅋ",
          "original_form": "까치 까마귀 아주 난리네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:10"
        },
        {
          "id": "MDRW2100003133.1.5",
          "form": "너무 시원해서 좋아서 그러나 ㅎㅎ",
          "original_form": "너무 시원해서 좋아서 그러나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 19:10"
        },
        {
          "id": "MDRW2100003133.1.6",
          "form": "이제 저녁 바람이 차가울 정도야",
          "original_form": "이제 저녁 바람이 차가울 정도야",
          "speaker_id": "1",
          "time": "20210820 19:11"
        },
        {
          "id": "MDRW2100003133.1.7",
          "form": "낮에도 바람은 시원하던데",
          "original_form": "낮에도 바람은 시원하던데",
          "speaker_id": "2",
          "time": "20210820 19:11"
        },
        {
          "id": "MDRW2100003133.1.8",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210820 19:11"
        },
        {
          "id": "MDRW2100003133.1.9",
          "form": "ㅇㅇ 낮에도 바람 잘 불고 시원했어",
          "original_form": "ㅇㅇ 낮에도 바람 잘 불고 시원했어",
          "speaker_id": "2",
          "time": "20210820 19:11"
        },
        {
          "id": "MDRW2100003133.1.10",
          "form": "비와서 기온이 더 내려갔나보다",
          "original_form": "비와서 기온이 더 내려갔나보다",
          "speaker_id": "1",
          "time": "20210820 19:11"
        },
        {
          "id": "MDRW2100003133.1.11",
          "form": "이러다 확 추워지는 것 아니겠지?",
          "original_form": "이러다 확 추워지는 것 아니겠지?",
          "speaker_id": "2",
          "time": "20210820 19:11"
        },
        {
          "id": "MDRW2100003133.1.12",
          "form": "그럴지도",
          "original_form": "그럴지도",
          "speaker_id": "1",
          "time": "20210820 19:11"
        },
        {
          "id": "MDRW2100003133.1.13",
          "form": "안돼~~ 지금이 좋은데 ㅎㅎ",
          "original_form": "안돼~~ 지금이 좋은데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 19:12"
        },
        {
          "id": "MDRW2100003133.1.14",
          "form": "비가 더 안오길 바래야지",
          "original_form": "비가 더 안오길 바래야지",
          "speaker_id": "1",
          "time": "20210820 19:12"
        },
        {
          "id": "MDRW2100003133.1.15",
          "form": "훌 갑자기 또 비와",
          "original_form": "훌 갑자기 또 비와",
          "speaker_id": "2",
