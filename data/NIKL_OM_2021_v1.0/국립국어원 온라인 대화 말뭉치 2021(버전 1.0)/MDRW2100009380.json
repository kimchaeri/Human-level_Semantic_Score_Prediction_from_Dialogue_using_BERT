      "utterance": [
        {
          "id": "MDRW2100009380.1.1",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009380.1.2",
          "form": "요즘 읽으려고 노력하고 있어요!",
          "original_form": "요즘 읽으려고 노력하고 있어요!",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009380.1.3",
          "form": "재태크 책이요",
          "original_form": "재태크 책이요",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009380.1.4",
          "form": "오 저는 즐겨보진않는데",
          "original_form": "오 저는 즐겨보진않는데",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009380.1.5",
          "form": "앞에 있으면 잘읽긴해요 ㅋㅋ",
          "original_form": "앞에 있으면 잘읽긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009380.1.6",
          "form": "소설책같은것도 읽으세요?",
          "original_form": "소설책같은것도 읽으세요?",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009380.1.7",
          "form": "소설은 해리포터 다읽어봣구요",
          "original_form": "소설은 해리포터 다읽어봣구요",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009380.1.8",
          "form": "나미야 잡화점같은거 ㅋㅋ유명한건 몇번 읽었어요",
          "original_form": "나미야 잡화점같은거 ㅋㅋ유명한건 몇번 읽었어요",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009380.1.9",
          "form": "오 저희집에도 몇권 있는데 나미야 잡화점도 많이 봤어요",
          "original_form": "오 저희집에도 몇권 있는데 나미야 잡화점도 많이 봤어요",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009380.1.10",
          "form": "요즘 가을되서 뭔가 책읽으면 좋을것같아요",
          "original_form": "요즘 가을되서 뭔가 책읽으면 좋을것같아요",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009380.1.11",
          "form": "공원에 돗자리깔고 책읽고싶네요 ㅠㅠ",
          "original_form": "공원에 돗자리깔고 책읽고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009380.1.12",
          "form": "돗자리요ㅋㅋㅋㅋㅋ재밌겠네요 전 서울 안살아서 한강에 돗자리 펴고 읽어보고 싶어요",
          "original_form": "돗자리요ㅋㅋㅋㅋㅋ재밌겠네요 전 서울 안살아서 한강에 돗자리 펴고 읽어보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009380.1.13",
          "form": "저도 서울은 아니고 경기도요 ㅠ",
          "original_form": "저도 서울은 아니고 경기도요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009380.1.14",
          "form": "그 흔하다는 한강라면도 안먹어봣네요 아직",
          "original_form": "그 흔하다는 한강라면도 안먹어봣네요 아직",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009380.1.15",
          "form": "경기도라도 가까우니까 작가싸인회나 행사 가기 쉬우시겠어요",
          "original_form": "경기도라도 가까우니까 작가싸인회나 행사 가기 쉬우시겠어요",
          "speaker_id": "1",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009380.1.16",
          "form": "네네 쉬운데도 잘안가게되긴하는데",
          "original_form": "네네 쉬운데도 잘안가게되긴하는데",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009380.1.17",
          "form": "앞으로 좀 다녀보려구요 ㅎㅎ",
          "original_form": "앞으로 좀 다녀보려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009380.1.18",
          "form": "요즘 베스트셀러는 뭐가 있죠?",
          "original_form": "요즘 베스트셀러는 뭐가 있죠?",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009380.1.19",
          "form": "흠 글쎄요 ㅠㅠ",
          "original_form": "흠 글쎄요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:14"
        }
      ]
