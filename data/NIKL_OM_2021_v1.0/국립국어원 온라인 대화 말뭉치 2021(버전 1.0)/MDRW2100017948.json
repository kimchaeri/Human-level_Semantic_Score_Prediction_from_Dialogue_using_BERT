        {
          "id": "MDRW2100017948.1.1",
          "form": "오늘 뭐사셨어요?",
          "original_form": "오늘 뭐사셨어요?",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100017948.1.2",
          "form": "오늘... 아직 산게 없어요..!!",
          "original_form": "오늘... 아직 산게 없어요..!!",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100017948.1.3",
          "form": "오늘 무얼 사셨나요?",
          "original_form": "오늘 무얼 사셨나요?",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100017948.1.4",
          "form": "믹서기요",
          "original_form": "믹서기요",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100017948.1.5",
          "form": "베리 비싼",
          "original_form": "베리 비싼",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
