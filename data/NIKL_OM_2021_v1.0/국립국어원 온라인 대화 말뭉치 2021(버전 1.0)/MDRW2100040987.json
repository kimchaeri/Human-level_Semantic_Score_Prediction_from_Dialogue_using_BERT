      "utterance": [
        {
          "id": "MDRW2100040987.1.1",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ아니 이나즈마",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ아니 이나즈마",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100040987.1.2",
          "form": "다 망해서",
          "original_form": "다 망해서",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100040987.1.3",
          "form": "나루카미 섬이랑 그 저항군있던 섬 외엔",
          "original_form": "나루카미 섬이랑 그 저항군있던 섬 외엔",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100040987.1.4",
          "form": "민가도 없던데",
          "original_form": "민가도 없던데",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100040987.1.5",
          "form": "ㅇㅇ 전 이런 음침한 도시 싫은데요....",
          "original_form": "ㅇㅇ 전 이런 음침한 도시 싫은데요....",
          "speaker_id": "1",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100040987.1.6",
          "form": "누가 좋아하는거야",
          "original_form": "누가 좋아하는거야",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100040987.1.7",
          "form": "아니 그리고 야에미코는 ㄹㅇ",
          "original_form": "아니 그리고 야에미코는 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100040987.1.8",
          "form": "왜 직접 안하는거임",
          "original_form": "왜 직접 안하는거임",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100040987.1.9",
          "form": "내가보기엔ㅎ",
          "original_form": "내가보기엔ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100040987.1.10",
          "form": "직접 천수백안 신상까지 가기만했어도",
          "original_form": "직접 천수백안 신상까지 가기만했어도",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100040987.1.11",
          "form": "라이덴 쌍수들고 환영하면서 내려옴ㅎ",
          "original_form": "라이덴 쌍수들고 환영하면서 내려옴ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100040987.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ그칰ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그칰ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100040987.1.13",
          "form": "그리고 도체",
          "original_form": "그리고 도체",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100040987.1.14",
          "form": "라이덴이 주인공의 뭘보고",
          "original_form": "라이덴이 주인공의 뭘보고",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100040987.1.15",
          "form": "심상세계까지 열어서",
          "original_form": "심상세계까지 열어서",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100040987.1.16",
          "form": "팼다는건지",
          "original_form": "팼다는건지",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100040987.1.17",
          "form": "아직도 이해안감",
          "original_form": "아직도 이해안감",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100040987.1.18",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.19",
          "form": "아니 걍",
          "original_form": "아니 걍",
          "speaker_id": "1",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.20",
          "form": "그 꼭두각시가 진짜 라이덴이라",
          "original_form": "그 꼭두각시가 진짜 라이덴이라",
          "speaker_id": "1",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.21",
          "form": "원리원칙주의자인게",
          "original_form": "원리원칙주의자인게",
          "speaker_id": "1",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.22",
          "form": "ㅇㅇ걍",
          "original_form": "ㅇㅇ걍",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.23",
          "form": "백배는 더 흥미로웠을듯",
          "original_form": "백배는 더 흥미로웠을듯",
          "speaker_id": "1",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.24",
          "form": "매력적인 악역으로",
          "original_form": "매력적인 악역으로",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.25",
          "form": "내버려두는게",
          "original_form": "내버려두는게",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.26",
          "form": "아니 사실 난",
          "original_form": "아니 사실 난",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.27",
          "form": "걔랑 내면이랑",
          "original_form": "걔랑 내면이랑",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.28",
          "form": "뭔차인지",
          "original_form": "뭔차인지",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.29",
          "form": "아직도 모르겠어",
          "original_form": "아직도 모르겠어",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100040987.1.30",
          "form": "걍 그 라이덴이",
          "original_form": "걍 그 라이덴이",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100040987.1.31",
          "form": "주인공이랑 싸우면서 왜 영원을 추구하는지",
          "original_form": "주인공이랑 싸우면서 왜 영원을 추구하는지",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100040987.1.32",
          "form": "구구절절했어도",
          "original_form": "구구절절했어도",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100040987.1.33",
          "form": "차이가 있나??",
          "original_form": "차이가 있나??",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100040987.1.34",
          "form": "밥도 안먹고 밀었는데",
          "original_form": "밥도 안먹고 밀었는데",
          "speaker_id": "2",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100040987.1.35",
          "form": "너무 허무하다...",
          "original_form": "너무 허무하다...",
          "speaker_id": "2",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100040987.1.36",
          "form": "밥이나 먹을래",
          "original_form": "밥이나 먹을래",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100040987.1.37",
          "form": "ㅇㅇ....",
          "original_form": "ㅇㅇ....",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100040987.1.38",
          "form": "초기 삼봉행도",
          "original_form": "초기 삼봉행도",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100040987.1.39",
          "form": "pv보다 비중이 없다는게 충격적 ㅋㅋㅋㅋㅋ",
          "original_form": "pv보다 비중이 없다는게 충격적 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100040987.1.40",
          "form": "호재궁 파트는 아예 날라갔던데",
          "original_form": "호재궁 파트는 아예 날라갔던데",
          "speaker_id": "1",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100040987.1.41",
          "form": "ㅇ ㅏ",
          "original_form": "ㅇ ㅏ",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100040987.1.42",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100040987.1.43",
          "form": "나 와타츠미 서브퀘 하는데",
          "original_form": "나 와타츠미 서브퀘 하는데",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100040987.1.44",
          "form": "마지막 봉인 풀기에서",
          "original_form": "마지막 봉인 풀기에서",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100040987.1.45",
          "form": "다 풀엇는데 장치가 활성화가 안됨",
          "original_form": "다 풀엇는데 장치가 활성화가 안됨",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100040987.1.46",
          "form": "공략 봐도 이게 맞는거같은데;;",
          "original_form": "공략 봐도 이게 맞는거같은데;;",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100040987.1.47",
          "form": "와타츠미도 뭐 봉인 푸는게 있어?",
          "original_form": "와타츠미도 뭐 봉인 푸는게 있어?",
