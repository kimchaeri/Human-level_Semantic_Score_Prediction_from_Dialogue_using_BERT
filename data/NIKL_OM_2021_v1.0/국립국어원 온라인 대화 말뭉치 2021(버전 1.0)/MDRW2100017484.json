      "utterance": [
        {
          "id": "MDRW2100017484.1.1",
          "form": "요즘 위국일기라는 만화책 읽는데",
          "original_form": "요즘 위국일기라는 만화책 읽는데",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017484.1.2",
          "form": "감정 표현이 정말 섬세하더라구요",
          "original_form": "감정 표현이 정말 섬세하더라구요",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017484.1.3",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017484.1.4",
          "form": "한번 찾아봐야겠네요",
          "original_form": "한번 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017484.1.5",
          "form": "네 조카 데리고 살게된 비혼 여성의 이야기인데",
          "original_form": "네 조카 데리고 살게된 비혼 여성의 이야기인데",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017484.1.6",
          "form": "뭔가 상황이 많이 이해가 되더라구요",
          "original_form": "뭔가 상황이 많이 이해가 되더라구요",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017484.1.7",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017484.1.8",
          "form": "평소에도 만화책 자주 보시나요?",
          "original_form": "평소에도 만화책 자주 보시나요?",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017484.1.9",
          "form": "뭔가 어느날 너무 옛날에 보던 만화들이 땡겨서 보기 시작해서",
          "original_form": "뭔가 어느날 너무 옛날에 보던 만화들이 땡겨서 보기 시작해서",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017484.1.10",
          "form": "그냥 가끔 봐요ㅋㅋㅋ",
          "original_form": "그냥 가끔 봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017484.1.11",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017484.1.12",
          "form": "다른 분야의 도서도 즐겨 보시나요?ㅎㅎ",
          "original_form": "다른 분야의 도서도 즐겨 보시나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017484.1.13",
          "form": "넹 최근에 소크라테스 익스프레스라는 철학책을 읽기 시작했어요",
          "original_form": "넹 최근에 소크라테스 익스프레스라는 철학책을 읽기 시작했어요",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017484.1.14",
          "form": "어렵네요",
          "original_form": "어렵네요",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017484.1.15",
          "form": "아 철학책은 조금 어렵죠",
          "original_form": "아 철학책은 조금 어렵죠",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017484.1.16",
          "form": "뭔가 독서모임이라도 하며 읽어야할 듯한 도서 유형이예요",
          "original_form": "뭔가 독서모임이라도 하며 읽어야할 듯한 도서 유형이예요",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017484.1.17",
          "form": "뭔가 전공 상관없이 다양한 카테고리로 보려고 시도 중인데",
          "original_form": "뭔가 전공 상관없이 다양한 카테고리로 보려고 시도 중인데",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017484.1.18",
          "form": "철학책 수학책 이렇게 읽었는데 역시 재미없어요ㅋㅋㅋ",
          "original_form": "철학책 수학책 이렇게 읽었는데 역시 재미없어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017484.1.19",
          "form": "다양한 카테고리의 책 읽는 건 좋네요!!",
          "original_form": "다양한 카테고리의 책 읽는 건 좋네요!!",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017484.1.20",
          "form": "네 뭔가 편향된 사고를 하기 싫어서ㅎㅎ",
          "original_form": "네 뭔가 편향된 사고를 하기 싫어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017484.1.21",
          "form": "어떤 책 좋아하세요",
          "original_form": "어떤 책 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017484.1.22",
          "form": "저도 이것저것 보려고 하는 편인데",
          "original_form": "저도 이것저것 보려고 하는 편인데",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017484.1.23",
          "form": "직업관련서적이나 소설책을 주로 봐요",
          "original_form": "직업관련서적이나 소설책을 주로 봐요",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017484.1.24",
          "form": "저도 결국은 그 두개를 젤 많이 보는 것 같아요ㅋㅋ",
          "original_form": "저도 결국은 그 두개를 젤 많이 보는 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017484.1.25",
          "form": "근데 최근에 읽은 소설이 좀 허무해서",
          "original_form": "근데 최근에 읽은 소설이 좀 허무해서",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017484.1.26",
          "form": "어떤 소설이었나요?",
          "original_form": "어떤 소설이었나요?",
          "speaker_id": "2",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017484.1.27",
          "form": "버드 스트라이크라고 구병모 작가가 쓴 소설인데",
          "original_form": "버드 스트라이크라고 구병모 작가가 쓴 소설인데",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017484.1.28",
          "form": "설명엔 러브스토리였는데",
          "original_form": "설명엔 러브스토리였는데",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017484.1.29",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017484.1.30",
          "form": "남주가 도망가서 뭐 사랑이 이뤄지지 않더라구요",
          "original_form": "남주가 도망가서 뭐 사랑이 이뤄지지 않더라구요",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017484.1.31",
          "form": "왜 도망가는거지..",
          "original_form": "왜 도망가는거지..",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017484.1.32",
          "form": "에잇 허무한 전개네요",
          "original_form": "에잇 허무한 전개네요",
          "speaker_id": "2",
          "time": "20210826 19:59"
        }
      ]
