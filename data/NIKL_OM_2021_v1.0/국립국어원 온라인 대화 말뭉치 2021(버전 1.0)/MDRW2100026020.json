      "utterance": [
        {
          "id": "MDRW2100026020.1.1",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100026020.1.2",
          "form": "아뇨! 아직이에요",
          "original_form": "아뇨! 아직이에요",
          "speaker_id": "1",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026020.1.3",
          "form": "name2님은 드셨나요?",
          "original_form": "&name2&님은 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026020.1.4",
          "form": "네 저는 좀전에 먹었어요!",
          "original_form": "네 저는 좀전에 먹었어요!",
          "speaker_id": "2",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026020.1.5",
          "form": "뭐드셨나요~?",
          "original_form": "뭐드셨나요~?",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100026020.1.6",
          "form": "저는 로제 떡볶이요!\r\n뭐 드실건가요?",
          "original_form": "저는 로제 떡볶이요!\r\n뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026020.1.7",
          "form": "헉 떡볶이 맛있겠네요 저는 쌀국수 먹을거에요!",
          "original_form": "헉 떡볶이 맛있겠네요 저는 쌀국수 먹을거에요!",
          "speaker_id": "1",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100026020.1.8",
          "form": "오! 비오는날  쌀국수도 좋네요!",
          "original_form": "오! 비오는날  쌀국수도 좋네요!",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026020.1.9",
          "form": "체인점꺼 드세요?",
          "original_form": "체인점꺼 드세요?",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100026020.1.10",
          "form": "아뇨! 베트남 쌀국수 포보 봉지가 있어서 먹으려고요",
          "original_form": "아뇨! 베트남 쌀국수 포보 봉지가 있어서 먹으려고요",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026020.1.11",
          "form": "컵라면처럼 끓는 물 부어서 먹으면 되어서 간편해요",
          "original_form": "컵라면처럼 끓는 물 부어서 먹으면 되어서 간편해요",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026020.1.12",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026020.1.13",
          "form": "그것도 맛있나요?",
          "original_form": "그것도 맛있나요?",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026020.1.14",
          "form": "저도 한번 먹어봐야겠네요",
          "original_form": "저도 한번 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026020.1.15",
          "form": "체인점이랑 맛이 비슷해서 저는 구비해두고 즐겨먹어요~",
          "original_form": "체인점이랑 맛이 비슷해서 저는 구비해두고 즐겨먹어요~",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026020.1.16",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026020.1.17",
          "form": "이름이 포보예요?",
          "original_form": "이름이 포보예요?",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026020.1.18",
          "form": "네 조미유도 들어있어서 생각보다 맛있어요!",
          "original_form": "네 조미유도 들어있어서 생각보다 맛있어요!",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026020.1.19",
          "form": "포보 쌀국수라고 치시면 나올거에요!",
          "original_form": "포보 쌀국수라고 치시면 나올거에요!",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026020.1.20",
          "form": "오호! 그렇군요 \r\n마트에도 파나요?",
          "original_form": "오호! 그렇군요 \r\n마트에도 파나요?",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026020.1.21",
          "form": "봉지로 된것과 용기에 담긴것 두가지가 있는데 용기에 담긴게 비싸지만 고명이 더 많아요",
          "original_form": "봉지로 된것과 용기에 담긴것 두가지가 있는데 용기에 담긴게 비싸지만 고명이 더 많아요",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026020.1.22",
          "form": "마트에서는 안봐서 편의점에서는 용기에 담긴건 파는걸 봤어요!",
          "original_form": "마트에서는 안봐서 편의점에서는 용기에 담긴건 파는걸 봤어요!",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026020.1.23",
          "form": "오 고명도 많이 들어있군요!\r\n가끔 쌀국수 먹고싶을때  그거 먹으면 되겠네요",
          "original_form": "오 고명도 많이 들어있군요!\r\n가끔 쌀국수 먹고싶을때  그거 먹으면 되겠네요",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026020.1.24",
          "form": "추천드려요 :)",
          "original_form": "추천드려요 :)",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026020.1.25",
          "form": "식당꺼는 양도 많고해서 좀 부담스러웠는데",
          "original_form": "식당꺼는 양도 많고해서 좀 부담스러웠는데",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026020.1.26",
          "form": "감사해요!",
          "original_form": "감사해요!",
          "speaker_id": "2",
          "time": "20210901 19:39"
        }
      ]
    }
