      "utterance": [
        {
          "id": "MDRW2100041630.1.1",
          "form": "오 경주요!",
          "original_form": "오 경주요!",
          "speaker_id": "2",
          "time": "20210906 13:32"
        },
        {
          "id": "MDRW2100041630.1.2",
          "form": "네 엄청 좋았어요",
          "original_form": "네 엄청 좋았어요",
          "speaker_id": "1",
          "time": "20210906 13:32"
        },
        {
          "id": "MDRW2100041630.1.3",
          "form": "뭐타고 가녀오셨나요?",
          "original_form": "뭐타고 가녀오셨나요?",
          "speaker_id": "2",
          "time": "20210906 13:32"
        },
        {
          "id": "MDRW2100041630.1.4",
          "form": "저 기차요 ㅎㅎ",
          "original_form": "저 기차요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 13:32"
        },
        {
          "id": "MDRW2100041630.1.5",
          "form": "아 경주역 생각나네요",
          "original_form": "아 경주역 생각나네요",
          "speaker_id": "2",
          "time": "20210906 13:33"
        },
        {
          "id": "MDRW2100041630.1.6",
          "form": "네 거기서 내렸어요 ㅋㅋㅋ",
          "original_form": "네 거기서 내렸어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:33"
        },
        {
          "id": "MDRW2100041630.1.7",
          "form": "경주 참 좋죠",
          "original_form": "경주 참 좋죠",
          "speaker_id": "2",
          "time": "20210906 13:33"
        },
        {
          "id": "MDRW2100041630.1.8",
          "form": "네 건물들 지붕이 다 기와지붕형태라 예뻤어요",
          "original_form": "네 건물들 지붕이 다 기와지붕형태라 예뻤어요",
          "speaker_id": "1",
          "time": "20210906 13:33"
        },
        {
          "id": "MDRW2100041630.1.9",
          "form": "저는 불국사 다녀왔던 생각나네요",
          "original_form": "저는 불국사 다녀왔던 생각나네요",
          "speaker_id": "2",
          "time": "20210906 13:33"
        },
        {
          "id": "MDRW2100041630.1.10",
          "form": "저는 이틀간 지내면서 코스로 쭉 여행했는데 다 너무 좋았어요",
          "original_form": "저는 이틀간 지내면서 코스로 쭉 여행했는데 다 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210906 13:34"
        },
        {
          "id": "MDRW2100041630.1.11",
          "form": "전통양식이 있는게 좋아요",
          "original_form": "전통양식이 있는게 좋아요",
          "speaker_id": "2",
          "time": "20210906 13:34"
        },
        {
          "id": "MDRW2100041630.1.12",
          "form": "네 엄청 더웠지만 ㅋㅋ",
          "original_form": "네 엄청 더웠지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:34"
        },
        {
          "id": "MDRW2100041630.1.13",
          "form": "여름에 가셨어요?",
          "original_form": "여름에 가셨어요?",
          "speaker_id": "2",
          "time": "20210906 13:34"
        },
        {
          "id": "MDRW2100041630.1.14",
          "form": "네ㅠㅠ 다음에 갈땐 봄에 가야겠어요",
          "original_form": "네ㅠㅠ 다음에 갈땐 봄에 가야겠어요",
          "speaker_id": "1",
          "time": "20210906 13:34"
        }
