      "utterance": [
        {
          "id": "MDRW2100011476.1.1",
          "form": "결혼에 대해 어떻게 생각하세요?",
          "original_form": "결혼에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100011476.1.2",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100011476.1.3",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011476.1.4",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100011476.1.5",
          "form": "네.기혼입니다. 아이도 있어요. 이상형이 어떻게 되나요?",
          "original_form": "네.기혼입니다. 아이도 있어요. 이상형이 어떻게 되나요?",
          "speaker_id": "2",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100011476.1.6",
          "form": "와우 아이가있으시군요",
          "original_form": "와우 아이가있으시군요",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011476.1.7",
          "form": "저는 성격이 중요한것같아요!",
          "original_form": "저는 성격이 중요한것같아요!",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011476.1.8",
          "form": "외모는 안보시나요?",
          "original_form": "외모는 안보시나요?",
          "speaker_id": "2",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011476.1.9",
          "form": "ㅋㅋㅋ외모는",
          "original_form": "ㅋㅋㅋ외모는",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100011476.1.10",
          "form": "안경을 안꼈으면좋겠어요",
          "original_form": "안경을 안꼈으면좋겠어요",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100011476.1.11",
          "form": "전 키크면좋은데 배우자는 키가 크지 않네요 ㅋ 이상형은 이상형일뿐인가봐요",
          "original_form": "전 키크면좋은데 배우자는 키가 크지 않네요 ㅋ 이상형은 이상형일뿐인가봐요",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100011476.1.12",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100011476.1.13",
          "form": "사실 성격이잘맞으면",
          "original_form": "사실 성격이잘맞으면",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100011476.1.14",
          "form": "제일베스트죠",
          "original_form": "제일베스트죠",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100011476.1.15",
          "form": "그쵸 성격이잘맞아야죠",
          "original_form": "그쵸 성격이잘맞아야죠",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100011476.1.16",
          "form": "연예인중 이상형에 가까운사람이 있나요?",
          "original_form": "연예인중 이상형에 가까운사람이 있나요?",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100011476.1.17",
          "form": "음 연예인은 외적인것만으로니까",
          "original_form": "음 연예인은 외적인것만으로니까",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100011476.1.18",
          "form": "저는 여진구? 유승호?",
          "original_form": "저는 여진구? 유승호?",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100011476.1.19",
          "form": "외적인 이상형은  현빈 요.",
          "original_form": "외적인 이상형은  현빈 요.",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100011476.1.20",
          "form": "눈빛이 매력있어요.",
          "original_form": "눈빛이 매력있어요.",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100011476.1.21",
          "form": "현빈좋죠ㅋㅋㅋㅋ",
          "original_form": "현빈좋죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100011476.1.22",
          "form": "시크릿가든",
          "original_form": "시크릿가든",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100011476.1.23",
          "form": "잼게봤었는데",
          "original_form": "잼게봤었는데",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100011476.1.24",
          "form": "저두요. 사랑의 불시착도 재미있게 봤어요.데이트 상대로 현빈씨 좋을것 같아요",
          "original_form": "저두요. 사랑의 불시착도 재미있게 봤어요.데이트 상대로 현빈씨 좋을것 같아요",
          "speaker_id": "2",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100011476.1.25",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100011476.1.26",
          "form": "현빈 여친있지않나요? 공개여친",
          "original_form": "현빈 여친있지않나요? 공개여친",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100011476.1.27",
          "form": "여친있죠. 손예진 배우~이상형이니까요.",
          "original_form": "여친있죠. 손예진 배우~이상형이니까요.",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100011476.1.28",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100011476.1.29",
          "form": "둘이사귀나요?",
          "original_form": "둘이사귀나요?",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100011476.1.30",
          "form": "네. 모르셨군요~둘이 사귄다고 했어요",
          "original_form": "네. 모르셨군요~둘이 사귄다고 했어요",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100011476.1.31",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100011476.1.32",
          "form": "진짜 몰랐네요",
          "original_form": "진짜 몰랐네요",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100011476.1.33",
          "form": "잘어울리네요",
          "original_form": "잘어울리네요",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100011476.1.34",
          "form": "잘어울리는 커플이죠.",
          "original_form": "잘어울리는 커플이죠.",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100011476.1.35",
          "form": "그러게요ㄷㄷ",
          "original_form": "그러게요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100011476.1.36",
          "form": "결혼하겠군요",
          "original_form": "결혼하겠군요",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100011476.1.37",
          "form": "결혼해서 잘살았으면 좋겠어요.",
          "original_form": "결혼해서 잘살았으면 좋겠어요.",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100011476.1.38",
          "form": "뭔가 둘은",
          "original_form": "뭔가 둘은",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100011476.1.39",
          "form": "잘살것같긴하네요ㅋㅋㅋ",
          "original_form": "잘살것같긴하네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:54"
        }
      ]
    }
