      "utterance": [
        {
          "id": "MDRW2100009100.1.1",
          "form": "올 겨울쯤에는 먹을 수 있을까요?",
          "original_form": "올 겨울쯤에는 먹을 수 있을까요?",
          "speaker_id": "2",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009100.1.2",
          "form": "코로나전에 갔어야했는데요",
          "original_form": "코로나전에 갔어야했는데요",
          "speaker_id": "1",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009100.1.3",
          "form": "집에서도 할 수는 있지만 뭔가 그 불어터진 느낌이 안나죠",
          "original_form": "집에서도 할 수는 있지만 뭔가 그 불어터진 느낌이 안나죠",
          "speaker_id": "2",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009100.1.4",
          "form": "그쳐 그 엄청 많은 국물속에 떡이 계속 있어야해요",
          "original_form": "그쳐 그 엄청 많은 국물속에 떡이 계속 있어야해요",
          "speaker_id": "1",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009100.1.5",
          "form": "맞아요 오뎅육수에 계속 뭉근히 끓여진 그맛",
          "original_form": "맞아요 오뎅육수에 계속 뭉근히 끓여진 그맛",
          "speaker_id": "2",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009100.1.6",
          "form": "맛있는건 이렇게 많은데 왜 위장은 주먹만한가요",
          "original_form": "맛있는건 이렇게 많은데 왜 위장은 주먹만한가요",
          "speaker_id": "1",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100009100.1.7",
          "form": "그 묵? 묵 꼬지도 맛있어요 육수가 많이 베어들지는 않지만요ㅋㅋ",
          "original_form": "그 묵? 묵 꼬지도 맛있어요 육수가 많이 베어들지는 않지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100009100.1.8",
          "form": "소화능력이 확실히 떨어지긴 하죠",
          "original_form": "소화능력이 확실히 떨어지긴 하죠",
          "speaker_id": "2",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009100.1.9",
          "form": "헉 묵꼬지는 뭐죠 처음들어봐요 저 일본식 오뎅에 들어가는 곤약 좋아해요",
          "original_form": "헉 묵꼬지는 뭐죠 처음들어봐요 저 일본식 오뎅에 들어가는 곤약 좋아해요",
          "speaker_id": "2",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009100.1.10",
          "form": "아 곤약꼬지에요 말이 헛나왔네요",
          "original_form": "아 곤약꼬지에요 말이 헛나왔네요",
          "speaker_id": "1",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009100.1.11",
          "form": "어쩐지 어감이 낯설더라니 곤약이에요",
          "original_form": "어쩐지 어감이 낯설더라니 곤약이에요",
          "speaker_id": "1",
          "time": "20210822 21:23"
        },
        {
          "id": "MDRW2100009100.1.12",
          "form": "곤약 좋아요 식감도",
          "original_form": "곤약 좋아요 식감도",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009100.1.13",
          "form": "급식메뉴중에 돼지고기 장조림이 있으면",
          "original_form": "급식메뉴중에 돼지고기 장조림이 있으면",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009100.1.14",
          "form": "어 맞아여",
          "original_form": "어 맞아여",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009100.1.15",
          "form": "거기 메추리얼이랑 곤약 주사위모양으로 썬것을 같이 넣어줬어요",
          "original_form": "거기 메추리얼이랑 곤약 주사위모양으로 썬것을 같이 넣어줬어요",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009100.1.16",
          "form": "그 곤약을 엄청 좋아했어요 ㅋㅋㅋ",
          "original_form": "그 곤약을 엄청 좋아했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009100.1.17",
          "form": "메추리알만큼 곤약도 좋아요",
          "original_form": "메추리알만큼 곤약도 좋아요",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009100.1.18",
          "form": "우열을 가릴수없으요",
          "original_form": "우열을 가릴수없으요",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009100.1.19",
          "form": "맞아요 고기보다 메추리알이랑 곤약이 더 좋았어요 ㅋㅋ",
          "original_form": "맞아요 고기보다 메추리알이랑 곤약이 더 좋았어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009100.1.20",
          "form": "아시는군요 해피",
          "original_form": "아시는군요 해피",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009100.1.21",
          "form": "내일 회사밥에 장조림 나왔으면 좋겠어요ㅋㅋ",
          "original_form": "내일 회사밥에 장조림 나왔으면 좋겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009100.1.22",
          "form": "으아 내일 출근이네요 재택힘내시고ㅋㅋ 내일 심심하면 연락해요!",
          "original_form": "으아 내일 출근이네요 재택힘내시고ㅋㅋ 내일 심심하면 연락해요!",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009100.1.23",
          "form": "회사 점심 맛있으면 좋겠네요! 좋은 밤 되셔요 :)",
          "original_form": "회사 점심 맛있으면 좋겠네요! 좋은 밤 되셔요 :)",
          "speaker_id": "2",
          "time": "20210822 21:33"
        }
      ]
    }
