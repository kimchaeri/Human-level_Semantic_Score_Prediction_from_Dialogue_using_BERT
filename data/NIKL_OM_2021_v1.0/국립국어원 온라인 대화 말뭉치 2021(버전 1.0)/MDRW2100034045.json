      "utterance": [
        {
          "id": "MDRW2100034045.1.1",
          "form": "좋아하는 게임",
          "original_form": "좋아하는 게임",
          "speaker_id": "2",
          "time": "20210827 12:47"
        },
        {
          "id": "MDRW2100034045.1.2",
          "form": "있으신가용",
          "original_form": "있으신가용",
          "speaker_id": "2",
          "time": "20210827 12:47"
        },
        {
          "id": "MDRW2100034045.1.3",
          "form": "아니요 ㅋㅋ 아 컨텐츠 버튼 잘못눌러버렸어요 ㅠ",
          "original_form": "아니요 ㅋㅋ 아 컨텐츠 버튼 잘못눌러버렸어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100034045.1.4",
          "form": "혹시 있으신가요?",
          "original_form": "혹시 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100034045.1.5",
          "form": "ㅋㅋㅋㅋㅋㅎ 전 요새는 안하는데",
          "original_form": "ㅋㅋㅋㅋㅋㅎ 전 요새는 안하는데",
          "speaker_id": "2",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100034045.1.6",
          "form": "예전에는 크아 메이플 이런 거 했어요",
          "original_form": "예전에는 크아 메이플 이런 거 했어요",
          "speaker_id": "2",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100034045.1.7",
          "form": "아 저두 크아 ㅋㅋㅋ 추억의게임이네요",
          "original_form": "아 저두 크아 ㅋㅋㅋ 추억의게임이네요",
          "speaker_id": "1",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100034045.1.8",
          "form": "테런도 자주했어요",
          "original_form": "테런도 자주했어요",
          "speaker_id": "1",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100034045.1.9",
          "form": "ㅎ 어 저두요",
          "original_form": "ㅎ 어 저두요",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100034045.1.10",
          "form": "저 설원?",
          "original_form": "저 설원?",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100034045.1.11",
          "form": "그거 좋아했어요",
          "original_form": "그거 좋아했어요",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100034045.1.12",
          "form": "아 설원은 첨들어봐요",
          "original_form": "아 설원은 첨들어봐요",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100034045.1.13",
          "form": "게임을 못하니깐 자연히 관심이 없어지더라구요 ㅠ",
          "original_form": "게임을 못하니깐 자연히 관심이 없어지더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100034045.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아ㅎ 그럴 수 있죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아ㅎ 그럴 수 있죠",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100034045.1.15",
          "form": "저도 많이 졌거든요...",
          "original_form": "저도 많이 졌거든요...",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100034045.1.16",
          "form": "ㅋㅋㅋㅋㅋ 게임잘하는분들보면 신기해요",
          "original_form": "ㅋㅋㅋㅋㅋ 게임잘하는분들보면 신기해요",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100034045.1.17",
          "form": "진짜요.. 넘나 신기...",
          "original_form": "진짜요.. 넘나 신기...",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100034045.1.18",
          "form": "신들린 컨트롤보면 멋지기도해요 ㅋㅋ",
          "original_form": "신들린 컨트롤보면 멋지기도해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100034045.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ오래한 짬바들이 보여요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오래한 짬바들이 보여요",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100034045.1.20",
          "form": "그죠 ㅋㅋㅋ",
          "original_form": "그죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:31"
        },
        {
          "id": "MDRW2100034045.1.21",
          "form": "ㅋㅋㅋ네 다 티나요",
          "original_form": "ㅋㅋㅋ네 다 티나요",
          "speaker_id": "2",
          "time": "20210827 19:44"
        }
      ]
