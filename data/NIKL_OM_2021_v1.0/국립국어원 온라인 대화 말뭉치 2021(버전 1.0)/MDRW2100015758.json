        },
        {
          "id": "MDRW2100015758.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015758.1.3",
          "form": "영상 보실때 주로 어디서 보세요?",
          "original_form": "영상 보실때 주로 어디서 보세요?",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015758.1.4",
          "form": "영상은 따로 잘안보고 주로 티비봐요",
          "original_form": "영상은 따로 잘안보고 주로 티비봐요",
          "speaker_id": "2",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015758.1.5",
          "form": "name1님은?",
          "original_form": "&name1&님은?",
          "speaker_id": "2",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015758.1.6",
          "form": "저는 티비도보고 유튜브 주로 봐여!",
          "original_form": "저는 티비도보고 유튜브 주로 봐여!",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015758.1.7",
          "form": "드라마 좋아하세요?",
          "original_form": "드라마 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015758.1.8",
          "form": "드라마도 좋아하고 예능도 ㅎ",
          "original_form": "드라마도 좋아하고 예능도 ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015758.1.9",
          "form": "짐 유퀴즈보고있어요",
          "original_form": "짐 유퀴즈보고있어요",
          "speaker_id": "2",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100015758.1.10",
          "form": "오 저도 유퀴즈 좋아해요 ㅋㅋㅋ오늘꺼 보시나요?",
          "original_form": "오 저도 유퀴즈 좋아해요 ㅋㅋㅋ오늘꺼 보시나요?",
          "speaker_id": "1",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015758.1.11",
          "form": "저번주껀가요  여서정선수나와요",
          "original_form": "저번주껀가요  여서정선수나와요",
          "speaker_id": "2",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100015758.1.12",
          "form": "오늘한거에요! 재밌더라구용",
          "original_form": "오늘한거에요! 재밌더라구용",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015758.1.13",
          "form": "아 황정민나온것만봐서",
          "original_form": "아 황정민나온것만봐서",
          "speaker_id": "2",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100015758.1.14",
          "form": "ㅋㅋㅋㅋ네 황정민나온게 진짜 재밋어요",
          "original_form": "ㅋㅋㅋㅋ네 황정민나온게 진짜 재밋어요",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015758.1.15",
          "form": "보다말아서 다보고자야겠네요ㅎ",
          "original_form": "보다말아서 다보고자야겠네요ㅎ",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015758.1.16",
          "form": "김제덕선수귀엽던데",
          "original_form": "김제덕선수귀엽던데",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015758.1.17",
          "form": "그쳐ㅋㅋㅋㅋ귀여워요",
          "original_form": "그쳐ㅋㅋㅋㅋ귀여워요",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015758.1.18",
          "form": "요즘 드라마 뭐보세요?",
          "original_form": "요즘 드라마 뭐보세요?",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015758.1.19",
          "form": "슬의생도 가끔보고",
          "original_form": "슬의생도 가끔보고",
          "speaker_id": "2",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015758.1.20",
          "form": "알고있지만 은끝났고",
          "original_form": "알고있지만 은끝났고",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015758.1.21",
          "form": "펜하 시즌3는 잼없어서 나중에몰아볼려구요",
          "original_form": "펜하 시즌3는 잼없어서 나중에몰아볼려구요",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015758.1.22",
          "form": "아아 저두 슬의생 펜하봐요",
          "original_form": "아아 저두 슬의생 펜하봐요",
          "speaker_id": "1",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015758.1.23",
          "form": "펜하 초반엔 노잼인데 뒤엔 재밋더라구요",
          "original_form": "펜하 초반엔 노잼인데 뒤엔 재밋더라구요",
          "speaker_id": "1",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015758.1.24",
          "form": "펜하시즌2까진 본방사수였거든요",
          "original_form": "펜하시즌2까진 본방사수였거든요",
          "speaker_id": "2",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015758.1.25",
          "form": "딱 거기서끝냈어야하는데",
          "original_form": "딱 거기서끝냈어야하는데",
          "speaker_id": "2",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015758.1.26",
          "form": "하긴 그건그래요 ㅠ 딱 좋앗는데",
          "original_form": "하긴 그건그래요 ㅠ 딱 좋앗는데",
          "speaker_id": "1",
          "time": "20210826 01:19"
        },
        {
          "id": "MDRW2100015758.1.27",
          "form": "곧 끝날때 된거같아서 담주부턴 볼까생각중이요",
          "original_form": "곧 끝날때 된거같아서 담주부턴 볼까생각중이요",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015758.1.28",
          "form": "네넵 몰아보는게 더 몰입되고 좋을거같아요",
          "original_form": "네넵 몰아보는게 더 몰입되고 좋을거같아요",
          "speaker_id": "1",
          "time": "20210826 01:21"
        }
      ]
