      "utterance": [
        {
          "id": "MDRW2100015987.1.1",
          "form": "이번 올림픽에서 여자 배구 경기 봤어?",
          "original_form": "이번 올림픽에서 여자 배구 경기 봤어?",
          "speaker_id": "2",
          "time": "20210826 05:13"
        },
        {
          "id": "MDRW2100015987.1.2",
          "form": "봤어 정말 멋있었지!",
          "original_form": "봤어 정말 멋있었지!",
          "speaker_id": "1",
          "time": "20210826 05:14"
        },
        {
          "id": "MDRW2100015987.1.3",
          "form": "김연경 선수 너무 멋있더라고",
          "original_form": "김연경 선수 너무 멋있더라고",
          "speaker_id": "2",
          "time": "20210826 05:14"
        },
        {
          "id": "MDRW2100015987.1.4",
          "form": "4강전 상대가 누구였지?",
          "original_form": "4강전 상대가 누구였지?",
          "speaker_id": "1",
          "time": "20210826 05:14"
        },
        {
          "id": "MDRW2100015987.1.5",
          "form": "브라질 선수들이였나?",
          "original_form": "브라질 선수들이였나?",
          "speaker_id": "2",
          "time": "20210826 05:15"
        },
        {
          "id": "MDRW2100015987.1.6",
          "form": "맞아 브라질 맞네 기억났어  그 팀도 정말 잘하더라",
          "original_form": "맞아 브라질 맞네 기억났어  그 팀도 정말 잘하더라",
          "speaker_id": "1",
          "time": "20210826 05:15"
        },
        {
          "id": "MDRW2100015987.1.7",
          "form": "일본이랑 경기했을때도 되게 재미있게 봤어",
          "original_form": "일본이랑 경기했을때도 되게 재미있게 봤어",
          "speaker_id": "2",
          "time": "20210826 05:16"
        },
        {
          "id": "MDRW2100015987.1.8",
          "form": "일본이 의외로 수비가 좋더라",
          "original_form": "일본이 의외로 수비가 좋더라",
          "speaker_id": "1",
          "time": "20210826 05:16"
        },
        {
          "id": "MDRW2100015987.1.9",
          "form": "아 맞아 의외로 잘해서 긴박하고 재밌더라",
          "original_form": "아 맞아 의외로 잘해서 긴박하고 재밌더라",
          "speaker_id": "2",
          "time": "20210826 05:17"
        },
        {
          "id": "MDRW2100015987.1.10",
          "form": "5세트는 정말 아찔하더라",
          "original_form": "5세트는 정말 아찔하더라",
          "speaker_id": "1",
          "time": "20210826 05:17"
        },
        {
          "id": "MDRW2100015987.1.11",
          "form": "역시 한일전 겸기가 재밌긴 해 ㅋㅋ",
          "original_form": "역시 한일전 겸기가 재밌긴 해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 05:17"
        },
        {
          "id": "MDRW2100015987.1.12",
          "form": "김연경 선수가 진짜 잘하더라",
          "original_form": "김연경 선수가 진짜 잘하더라",
          "speaker_id": "1",
          "time": "20210826 05:19"
        },
        {
          "id": "MDRW2100015987.1.13",
          "form": "양효진 선수도 되게 잘하더라",
          "original_form": "양효진 선수도 되게 잘하더라",
          "speaker_id": "2",
          "time": "20210826 05:20"
        },
        {
          "id": "MDRW2100015987.1.14",
          "form": "그 선수가 저번 시즌 연봉 1위래",
          "original_form": "그 선수가 저번 시즌 연봉 1위래",
          "speaker_id": "1",
          "time": "20210826 05:21"
        },
        {
          "id": "MDRW2100015987.1.15",
          "form": "오 역시 연봉 1위선수는 다르다",
          "original_form": "오 역시 연봉 1위선수는 다르다",
          "speaker_id": "2",
          "time": "20210826 05:21"
        },
        {
          "id": "MDRW2100015987.1.16",
          "form": "그치? 배구 다음에 보러가자",
          "original_form": "그치? 배구 다음에 보러가자",
          "speaker_id": "1",
          "time": "20210826 05:21"
        }
      ]
