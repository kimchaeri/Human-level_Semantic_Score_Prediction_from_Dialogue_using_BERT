      "utterance": [
        {
          "id": "MDRW2100039646.1.1",
          "form": "정말 다들 우아하게 저격하네요....",
          "original_form": "정말 다들 우아하게 저격하네요....",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039646.1.2",
          "form": "셀레나 고메즈도 그렇고",
          "original_form": "셀레나 고메즈도 그렇고",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039646.1.3",
          "form": "저같으면 욕 때려박은",
          "original_form": "저같으면 욕 때려박은",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039646.1.4",
          "form": "가사썼을텐데!",
          "original_form": "가사썼을텐데!",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039646.1.5",
          "form": "아 그러니까요 전 이미 청불 가사 됐을 걸요",
          "original_form": "아 그러니까요 전 이미 청불 가사 됐을 걸요",
          "speaker_id": "1",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039646.1.6",
          "form": "셀레나 고메즈도 노래 넘넘 좋아요 ㅎ.ㅎ",
          "original_form": "셀레나 고메즈도 노래 넘넘 좋아요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039646.1.7",
          "form": "맞아요 청불 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 청불 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039646.1.8",
          "form": "외모도 나노 단위로 깔거고",
          "original_form": "외모도 나노 단위로 깔거고",
          "speaker_id": "2",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039646.1.9",
          "form": "명예훼손죄로 고소당할 각오하고",
          "original_form": "명예훼손죄로 고소당할 각오하고",
          "speaker_id": "2",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039646.1.10",
          "form": "가사 썼을거예요~~!",
          "original_form": "가사 썼을거예요~~!",
          "speaker_id": "2",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039646.1.11",
          "form": "지금은 데자부 듣는 중이에요!",
          "original_form": "지금은 데자부 듣는 중이에요!",
          "speaker_id": "2",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039646.1.12",
          "form": "이 뮤비는 뭔가 케이팝 느낌 나는,,,",
          "original_form": "이 뮤비는 뭔가 케이팝 느낌 나는,,,",
          "speaker_id": "2",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039646.1.13",
          "form": "데자부 뮤비에 나오는 현여친 금발머리가",
          "original_form": "데자부 뮤비에 나오는 현여친 금발머리가",
          "speaker_id": "1",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039646.1.14",
          "form": "실제.. 그 배우랑 완전 판박이래요",
          "original_form": "실제.. 그 배우랑 완전 판박이래요",
          "speaker_id": "1",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039646.1.15",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039646.1.16",
          "form": "진짜 저격송이네요",
          "original_form": "진짜 저격송이네요",
          "speaker_id": "2",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039646.1.17",
          "form": "아 자꾸 누가 닮았다 보니까",
          "original_form": "아 자꾸 누가 닮았다 보니까",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039646.1.18",
          "form": "트와이스 나연이랑",
          "original_form": "트와이스 나연이랑",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039646.1.19",
          "form": "그거 다 보시구 그 뮤비 해석!?!?! 이런 거 찾아보시면 깜짝 놀라싱걸요",
          "original_form": "그거 다 보시구 그 뮤비 해석!?!?! 이런 거 찾아보시면 깜짝 놀라싱걸요",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039646.1.20",
          "form": "자꾸 겹쳐보여욬ㅋㅋㅋ",
          "original_form": "자꾸 겹쳐보여욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039646.1.21",
          "form": "헐헐 그 느김 있다!!!!!!",
          "original_form": "헐헐 그 느김 있다!!!!!!",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039646.1.22",
          "form": "헐 진짜요? 당장 보러가야지",
          "original_form": "헐 진짜요? 당장 보러가야지",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039646.1.23",
          "form": "그쵸그쵸!!!",
          "original_form": "그쵸그쵸!!!",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039646.1.24",
          "form": "네!! 대박 그냥 제가 혼자 봤을 땐 몰랐는데",
          "original_form": "네!! 대박 그냥 제가 혼자 봤을 땐 몰랐는데",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039646.1.25",
          "form": "듣고보니 그런느낌!",
          "original_form": "듣고보니 그런느낌!",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039646.1.26",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋ 저 지금",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋ 저 지금",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039646.1.27",
          "form": "티나 프롬 코리아 라는 채널에서",
          "original_form": "티나 프롬 코리아 라는 채널에서",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039646.1.28",
          "form": "해석한 영상",
          "original_form": "해석한 영상",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039646.1.29",
          "form": "보고있어요!",
          "original_form": "보고있어요!",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039646.1.30",
          "form": "헛 그 채널은 처음 들어보는데",
          "original_form": "헛 그 채널은 처음 들어보는데",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039646.1.31",
          "form": "어떤가요! 설명 잘해주나요?!",
          "original_form": "어떤가요! 설명 잘해주나요?!",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039646.1.32",
          "form": "지금 보는 중인데",
          "original_form": "지금 보는 중인데",
          "speaker_id": "2",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039646.1.33",
          "form": "가사 해석이네요?",
          "original_form": "가사 해석이네요?",
          "speaker_id": "2",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039646.1.34",
          "form": "잘못 찾았나봐욬ㅋㅋㅋ",
          "original_form": "잘못 찾았나봐욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:18"
        }
