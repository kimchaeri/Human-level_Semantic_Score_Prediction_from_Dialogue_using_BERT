      "utterance": [
        {
          "id": "MDRW2100030068.1.1",
          "form": "하이욤",
          "original_form": "하이욤",
          "speaker_id": "1",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100030068.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100030068.1.3",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100030068.1.4",
          "form": "네 저는 주로 롤 많이해요",
          "original_form": "네 저는 주로 롤 많이해요",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030068.1.5",
          "form": "그러시구나 저는 그냥 퍼즐게임같은 걸 주로 해요",
          "original_form": "그러시구나 저는 그냥 퍼즐게임같은 걸 주로 해요",
          "speaker_id": "2",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030068.1.6",
          "form": "롤은 무슨 포지션 주로 하시나요?",
          "original_form": "롤은 무슨 포지션 주로 하시나요?",
          "speaker_id": "2",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030068.1.7",
          "form": "저는 정글 많이해요",
          "original_form": "저는 정글 많이해요",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030068.1.8",
          "form": "포지션은 다 아세요?",
          "original_form": "포지션은 다 아세요?",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030068.1.9",
          "form": "그냥 대충 알아요",
          "original_form": "그냥 대충 알아요",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030068.1.10",
          "form": "동생있어서",
          "original_form": "동생있어서",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030068.1.11",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030068.1.12",
          "form": "롤 해본적 있어요?",
          "original_form": "롤 해본적 있어요?",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030068.1.13",
          "form": "아뇨 동생이 너무 해서 하기 싫더라구요",
          "original_form": "아뇨 동생이 너무 해서 하기 싫더라구요",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030068.1.14",
          "form": "나름 재밌는데",
          "original_form": "나름 재밌는데",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030068.1.15",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030068.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ게임 엄청 못해서 게임하면 욕먹어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ게임 엄청 못해서 게임하면 욕먹어요",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030068.1.17",
          "form": "카트라이더도 잘 못해요...",
          "original_form": "카트라이더도 잘 못해요...",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030068.1.18",
          "form": "카트라이더 여자분들이 많이 하시더라구요",
          "original_form": "카트라이더 여자분들이 많이 하시더라구요",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030068.1.19",
          "form": "아 진짜요? 남자들도 많이 하는 줄",
          "original_form": "아 진짜요? 남자들도 많이 하는 줄",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100030068.1.20",
          "form": "졸업하기 전에 과에서 다같이 하고 그랬거든요",
          "original_form": "졸업하기 전에 과에서 다같이 하고 그랬거든요",
          "speaker_id": "2",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030068.1.21",
          "form": "남자들도 많이 하긴하죠",
          "original_form": "남자들도 많이 하긴하죠",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030068.1.22",
          "form": "근데 호불호가 심해서",
          "original_form": "근데 호불호가 심해서",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030068.1.23",
          "form": "전 못 꼈지만...ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 못 꼈지만...ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030068.1.24",
          "form": "매니아틱한 느낌이에요",
          "original_form": "매니아틱한 느낌이에요",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030068.1.25",
          "form": "아 그래요? 모바일 버전 막 나왓을 때만 인기였나",
          "original_form": "아 그래요? 모바일 버전 막 나왓을 때만 인기였나",
          "speaker_id": "2",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030068.1.26",
          "form": "요즘은 배그나 롤이 아직도 유행인가요?",
          "original_form": "요즘은 배그나 롤이 아직도 유행인가요?",
          "speaker_id": "2",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030068.1.27",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030068.1.28",
          "form": "새로운 게임이 많이 안나오니까",
          "original_form": "새로운 게임이 많이 안나오니까",
          "speaker_id": "1",
          "time": "20210903 20:43"
        }
      ]
    }
