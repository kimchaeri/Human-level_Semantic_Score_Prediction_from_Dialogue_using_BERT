      "utterance": [
        {
          "id": "MDRW2100028292.1.1",
          "form": "40퍼 고기 득템입니다!!",
          "original_form": "40퍼 고기 득템입니다!!",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028292.1.2",
          "form": "40퍼 대단하네요",
          "original_form": "40퍼 대단하네요",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028292.1.3",
          "form": "다 피와 살이되는",
          "original_form": "다 피와 살이되는",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028292.1.4",
          "form": "이미 다 뱃속으로 ㅎㅎㅎ",
          "original_form": "이미 다 뱃속으로 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028292.1.5",
          "form": "ㅋㅋ 굳 디너네요",
          "original_form": "ㅋㅋ 굳 디너네요",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028292.1.6",
          "form": "네 한끼 잘 먹었습니다 ㅋㅋ",
          "original_form": "네 한끼 잘 먹었습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028292.1.7",
          "form": "전 요거트로 디너했습니다 ㅜㅠ",
          "original_form": "전 요거트로 디너했습니다 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028292.1.8",
          "form": "왜죠ㅠ",
          "original_form": "왜죠ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028292.1.9",
          "form": "별로 배도 안고프고. 요즘 살이 부쩍 쪄서",
          "original_form": "별로 배도 안고프고. 요즘 살이 부쩍 쪄서",
          "speaker_id": "2",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100028292.1.10",
          "form": "아 가끔 한끼 그렇게 하면 좀 소화되기도 하고ㅎㅎ",
          "original_form": "아 가끔 한끼 그렇게 하면 좀 소화되기도 하고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100028292.1.11",
          "form": "근데 금새 배고파진다는 점",
          "original_form": "근데 금새 배고파진다는 점",
          "speaker_id": "2",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100028292.1.12",
          "form": "그건 감수해야...ㅎㅎ",
          "original_form": "그건 감수해야...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100028292.1.13",
          "form": "다이어트의 길은 험난 고난",
          "original_form": "다이어트의 길은 험난 고난",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100028292.1.14",
          "form": "그러나 정작 빠지지는 않더라구요",
          "original_form": "그러나 정작 빠지지는 않더라구요",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100028292.1.15",
          "form": "맞어요....",
          "original_form": "맞어요....",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100028292.1.16",
          "form": "적게 먹고 운동을 해야하는데",
          "original_form": "적게 먹고 운동을 해야하는데",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100028292.1.17",
          "form": "운도 싫음. ㅜㅠ",
          "original_form": "운도 싫음. ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100028292.1.18",
          "form": "운동요",
          "original_form": "운동요",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100028292.1.19",
          "form": "ㅋㅋ귀찮죠 ㅋㅋ",
          "original_form": "ㅋㅋ귀찮죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100028292.1.20",
          "form": "최근에 뭐 사셨나요",
          "original_form": "최근에 뭐 사셨나요",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100028292.1.21",
          "form": "운동기구라면 허벅지운동하는 버터플라이링이요",
          "original_form": "운동기구라면 허벅지운동하는 버터플라이링이요",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100028292.1.22",
          "form": "오 효과가 있나요",
          "original_form": "오 효과가 있나요",
          "speaker_id": "1",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100028292.1.23",
          "form": "누워서 할수있는거라 샀는데",
          "original_form": "누워서 할수있는거라 샀는데",
          "speaker_id": "2",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100028292.1.24",
          "form": "허벅지가 아파요",
          "original_form": "허벅지가 아파요",
          "speaker_id": "2",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100028292.1.25",
          "form": "그럼 효과가 있는거죠",
          "original_form": "그럼 효과가 있는거죠",
          "speaker_id": "1",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100028292.1.26",
          "form": "그...렇겠죠?",
          "original_form": "그...렇겠죠?",
          "speaker_id": "2",
          "time": "20210903 13:15"
        }
      ]
    }
