      "utterance": [
        {
          "id": "MDRW2100020262.1.1",
          "form": "그 미사포는 개개인마다 디자인이 다르나요? ㅋㅋㅋㅋ 궁금했어요 ㅋㅋㅋㅋ",
          "original_form": "그 미사포는 개개인마다 디자인이 다르나요? ㅋㅋㅋㅋ 궁금했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020262.1.2",
          "form": "ㅋㅋㅋ달라요달라",
          "original_form": "ㅋㅋㅋ달라요달라",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020262.1.3",
          "form": "성물방에서",
          "original_form": "성물방에서",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020262.1.4",
          "form": "골라서 사는거에요.ㅋ ㅋ",
          "original_form": "골라서 사는거에요.ㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020262.1.5",
          "form": "엄마가 저는 젤 저려미로 사주셨는데...",
          "original_form": "엄마가 저는 젤 저려미로 사주셨는데...",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.6",
          "form": "아 그런 용품을 파는 곳이 성물방인가요?",
          "original_form": "아 그런 용품을 파는 곳이 성물방인가요?",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.7",
          "form": "옆자리 애 미사포는 이뻐보여서 ㅋㅋㅋㅋ",
          "original_form": "옆자리 애 미사포는 이뻐보여서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.8",
          "form": "약간 레이스처럼 보였는데",
          "original_form": "약간 레이스처럼 보였는데",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.9",
          "form": "네네 레이스",
          "original_form": "네네 레이스",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.10",
          "form": "레이스같은거에요??",
          "original_form": "레이스같은거에요??",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.12",
          "form": "아그렇구나",
          "original_form": "아그렇구나",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.13",
          "form": "그 레이스 밑에 그림이",
          "original_form": "그 레이스 밑에 그림이",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.14",
          "form": "다 달라요",
          "original_form": "다 달라요",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.15",
          "form": "그림이있어요??",
          "original_form": "그림이있어요??",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.16",
          "form": "꽃무늬.포도무늬",
          "original_form": "꽃무늬.포도무늬",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.17",
          "form": "여러가지에요",
          "original_form": "여러가지에요",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.18",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.19",
          "form": "아아아아! 약간 종교적 상징이 들어가있군요!",
          "original_form": "아아아아! 약간 종교적 상징이 들어가있군요!",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020262.1.20",
          "form": "네네 성물방인데 거기서",
          "original_form": "네네 성물방인데 거기서",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020262.1.21",
          "form": "물건을 사고",
          "original_form": "물건을 사고",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020262.1.22",
          "form": "신부님께 축성 해달라고하면",
          "original_form": "신부님께 축성 해달라고하면",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020262.1.23",
          "form": "축성기도해주시는데",
          "original_form": "축성기도해주시는데",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020262.1.24",
          "form": "그럼 그것에 의미가 생기는것 같더라구요",
          "original_form": "그럼 그것에 의미가 생기는것 같더라구요",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020262.1.25",
          "form": "미사포는 축성이 필수인가요?",
          "original_form": "미사포는 축성이 필수인가요?",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020262.1.26",
          "form": "잘은모르지만 좋은거같아서 항상 축성은 받아요 ㅎㅎ",
          "original_form": "잘은모르지만 좋은거같아서 항상 축성은 받아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:50"
        }
      ]
    }
