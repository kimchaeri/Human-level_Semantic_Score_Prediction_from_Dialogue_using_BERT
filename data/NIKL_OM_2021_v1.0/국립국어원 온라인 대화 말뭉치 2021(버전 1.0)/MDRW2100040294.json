      "utterance": [
        {
          "id": "MDRW2100040294.1.1",
          "form": "너무 편하죠",
          "original_form": "너무 편하죠",
          "speaker_id": "2",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100040294.1.2",
          "form": "감자탕 맛있겠다",
          "original_form": "감자탕 맛있겠다",
          "speaker_id": "2",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100040294.1.3",
          "form": "시어머니께서 감자탕을 한 솥 그리셨는데 손이 크셔서 엄청 많이 했대요",
          "original_form": "시어머니께서 감자탕을 한 솥 그리셨는데 손이 크셔서 엄청 많이 했대요",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100040294.1.4",
          "form": "전예전에는 감자가 많아서 감자탕인 줄 알았어요",
          "original_form": "전예전에는 감자가 많아서 감자탕인 줄 알았어요",
          "speaker_id": "2",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100040294.1.5",
          "form": "아 감자탕이 아니라 감자국 이었나",
          "original_form": "아 감자탕이 아니라 감자국 이었나",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100040294.1.6",
          "form": "감자가 들어가는 국이요",
          "original_form": "감자가 들어가는 국이요",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040294.1.7",
          "form": "저희 동네 감자탕은 고기보다 감자가 더 많았거든요",
          "original_form": "저희 동네 감자탕은 고기보다 감자가 더 많았거든요",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040294.1.8",
          "form": "도미노 치즈 퐁듀 피자 광고 나오는데 굉장히 맛있어 보이네요",
          "original_form": "도미노 치즈 퐁듀 피자 광고 나오는데 굉장히 맛있어 보이네요",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040294.1.9",
          "form": "도미노피자 맛있는데 비싸요",
          "original_form": "도미노피자 맛있는데 비싸요",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040294.1.10",
          "form": "혹시 반올림 피자에서 시켜 드셔 보셨나요",
          "original_form": "혹시 반올림 피자에서 시켜 드셔 보셨나요",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040294.1.11",
          "form": "아니요ㅜㅜ",
          "original_form": "아니요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040294.1.12",
          "form": "가성비 피자 가게 중에서 제일 맛있는 것 같아요",
          "original_form": "가성비 피자 가게 중에서 제일 맛있는 것 같아요",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100040294.1.13",
          "form": "대신동에 있더라구요",
          "original_form": "대신동에 있더라구요",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100040294.1.14",
          "form": "매장 안에서 먹을 수 있는지는 잘 모르겠지만 어쨌든 맛있었어요",
          "original_form": "매장 안에서 먹을 수 있는지는 잘 모르겠지만 어쨌든 맛있었어요",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100040294.1.15",
          "form": "제 친구는 닭발을 시켜 먹었더라 구요",
          "original_form": "제 친구는 닭발을 시켜 먹었더라 구요",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100040294.1.16",
          "form": "혹시 국물닭발 맛있게 먹는 법 아세요",
          "original_form": "혹시 국물닭발 맛있게 먹는 법 아세요",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100040294.1.17",
          "form": "배고프면 맛있어요",
          "original_form": "배고프면 맛있어요",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100040294.1.18",
          "form": "양이 작 어도 맛있어요",
          "original_form": "양이 작 어도 맛있어요",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100040294.1.19",
          "form": "아닌가요?ㅋㅋㅋ",
          "original_form": "아닌가요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100040294.1.20",
          "form": "국물닭발 할 때 깻잎을 넣어서 끓여 보세요",
          "original_form": "국물닭발 할 때 깻잎을 넣어서 끓여 보세요",
          "speaker_id": "1",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100040294.1.21",
          "form": "완전 신세계 일 겁니다 진짜 맛있어요",
          "original_form": "완전 신세계 일 겁니다 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100040294.1.22",
          "form": "오진짜 맛있을 거 같네요",
          "original_form": "오진짜 맛있을 거 같네요",
          "speaker_id": "2",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040294.1.23",
          "form": "밥솥 눌렀어요",
          "original_form": "밥솥 눌렀어요",
          "speaker_id": "2",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040294.1.24",
          "form": "다음에 꼭 시도 해서 먹어 보세요 비법이랍니다",
          "original_form": "다음에 꼭 시도 해서 먹어 보세요 비법이랍니다",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040294.1.25",
          "form": "네 금방 눌렀어요",
          "original_form": "네 금방 눌렀어요",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040294.1.26",
          "form": "엄청 맛있을 거 같아요",
          "original_form": "엄청 맛있을 거 같아요",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040294.1.27",
          "form": "제가 밥솥 누르게 하려고 잠시 멈추신 거군요",
          "original_form": "제가 밥솥 누르게 하려고 잠시 멈추신 거군요",
          "speaker_id": "1",
          "time": "20210904 15:43"
        }
      ]
