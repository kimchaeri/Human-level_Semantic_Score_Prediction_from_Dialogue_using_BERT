      "utterance": [
        {
          "id": "MDRW2100020781.1.1",
          "form": "안녕하세요 집안일하는거 좋아하시나요 ?!",
          "original_form": "안녕하세요 집안일하는거 좋아하시나요 ?!",
          "speaker_id": "1",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020781.1.2",
          "form": "아눀ㅋㅋ좋아하진 않아요!",
          "original_form": "아눀ㅋㅋ좋아하진 않아요!",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020781.1.3",
          "form": "저는 청소가 힘들어요ㅠㅠ",
          "original_form": "저는 청소가 힘들어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020781.1.4",
          "form": "저도요 ,,",
          "original_form": "저도요 ,,",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020781.1.5",
          "form": "친구는 청소를 좋아하더라고요",
          "original_form": "친구는 청소를 좋아하더라고요",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020781.1.6",
          "form": "육아에 대해서는 생각 있으세요 ?",
          "original_form": "육아에 대해서는 생각 있으세요 ?",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020781.1.7",
          "form": "아뇨ㅋㅋㅋ미혼이고...",
          "original_form": "아뇨ㅋㅋㅋ미혼이고...",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100020781.1.8",
          "form": "육아는 진짜 힘든거같아요",
          "original_form": "육아는 진짜 힘든거같아요",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100020781.1.9",
          "form": "저도 미혼이고",
          "original_form": "저도 미혼이고",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100020781.1.10",
          "form": "육아는 생각도없는",
          "original_form": "육아는 생각도없는",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100020781.1.11",
          "form": "비혼주의자입니다ㅎ",
          "original_form": "비혼주의자입니다ㅎ",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100020781.1.12",
          "form": "저도 그런 거 같아요ㅎㅎ",
          "original_form": "저도 그런 거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100020781.1.13",
          "form": "원래 아이들 좋아하고 이랬는데 육아랑은 또 ㄷㅏ르니까요~",
          "original_form": "원래 아이들 좋아하고 이랬는데 육아랑은 또 ㄷㅏ르니까요~",
          "speaker_id": "2",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100020781.1.14",
          "form": "보는것과 다른 인생은 실전이죠",
          "original_form": "보는것과 다른 인생은 실전이죠",
          "speaker_id": "1",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100020781.1.15",
          "form": "ㅋㅋㅋ그리고 너무 힘들 거 같아요",
          "original_form": "ㅋㅋㅋ그리고 너무 힘들 거 같아요",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100020781.1.16",
          "form": "가족들이랑 같이 사세요?",
          "original_form": "가족들이랑 같이 사세요?",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100020781.1.17",
          "form": "네ㅠ",
          "original_form": "네ㅠ",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100020781.1.18",
          "form": "얼른독립해야하는데",
          "original_form": "얼른독립해야하는데",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100020781.1.19",
          "form": "ㅋㅋㅋ저도 가족들이랑 살아요",
          "original_form": "ㅋㅋㅋ저도 가족들이랑 살아요",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100020781.1.20",
          "form": "원랜 자취했었는데 다시 집으로 들어왔어요",
          "original_form": "원랜 자취했었는데 다시 집으로 들어왔어요",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100020781.1.21",
          "form": "저도 자취하다가 다시들어왔어요!",
          "original_form": "저도 자취하다가 다시들어왔어요!",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100020781.1.22",
          "form": "그래서 더 나가고싶어요 얼른",
          "original_form": "그래서 더 나가고싶어요 얼른",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100020781.1.23",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100020781.1.24",
          "form": "진짜 자취 그리워요",
          "original_form": "진짜 자취 그리워요",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100020781.1.25",
          "form": "한번해봤어서 더 그 재미를 못잊고,,",
          "original_form": "한번해봤어서 더 그 재미를 못잊고,,",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100020781.1.26",
          "form": "룸메이트있으셨어요?",
          "original_form": "룸메이트있으셨어요?",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100020781.1.27",
          "form": "전 기숙사 살았었는데",
          "original_form": "전 기숙사 살았었는데",
          "speaker_id": "2",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100020781.1.28",
          "form": "그땐 룸메 있었어요",
          "original_form": "그땐 룸메 있었어요",
          "speaker_id": "2",
          "time": "20210829 23:57"
        }
      ]
    }
  ]
