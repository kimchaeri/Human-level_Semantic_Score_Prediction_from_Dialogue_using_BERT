      "utterance": [
        {
          "id": "MDRW2100046637.1.1",
          "form": "안녕하세요\r\n",
          "original_form": "안녕하세요\r\n",
          "speaker_id": "1",
          "time": "20211103 22:03"
        },
        {
          "id": "MDRW2100046637.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211103 22:05"
        },
        {
          "id": "MDRW2100046637.1.3",
          "form": "날씨가 추워졌어요",
          "original_form": "날씨가 추워졌어요",
          "speaker_id": "2",
          "time": "20211103 22:05"
        },
        {
          "id": "MDRW2100046637.1.4",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20211103 22:05"
        },
        {
          "id": "MDRW2100046637.1.5",
          "form": "가을이 사라진 느낌",
          "original_form": "가을이 사라진 느낌",
          "speaker_id": "1",
          "time": "20211103 22:05"
        },
        {
          "id": "MDRW2100046637.1.6",
          "form": "그쵸 ㅠㅠ 가을 좋아하는데 너무 아쉬워요",
          "original_form": "그쵸 ㅠㅠ 가을 좋아하는데 너무 아쉬워요",
          "speaker_id": "2",
          "time": "20211103 22:06"
        },
        {
          "id": "MDRW2100046637.1.7",
          "form": "봄이랑 가을중에 뭘 더 좋아해요?",
          "original_form": "봄이랑 가을중에 뭘 더 좋아해요?",
          "speaker_id": "1",
          "time": "20211103 22:06"
        },
        {
          "id": "MDRW2100046637.1.8",
          "form": "전 가을이요!",
          "original_form": "전 가을이요!",
          "speaker_id": "2",
          "time": "20211103 22:06"
        },
        {
          "id": "MDRW2100046637.1.9",
          "form": "봄은 바람이 너무 많이 불어요 ㅋㅋㅋ",
          "original_form": "봄은 바람이 너무 많이 불어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 22:06"
        },
        {
          "id": "MDRW2100046637.1.10",
          "form": "봄이 더 좋던데 ㅋㅋ",
          "original_form": "봄이 더 좋던데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211103 22:07"
        },
        {
          "id": "MDRW2100046637.1.11",
          "form": "가을은 낙엽이 떨어지고..",
          "original_form": "가을은 낙엽이 떨어지고..",
          "speaker_id": "1",
          "time": "20211103 22:07"
        },
        {
          "id": "MDRW2100046637.1.12",
          "form": "봄은 새싹이 돋아나고..",
          "original_form": "봄은 새싹이 돋아나고..",
          "speaker_id": "1",
          "time": "20211103 22:07"
        },
        {
          "id": "MDRW2100046637.1.13",
          "form": "아~~~ 그런 의미로 봄도 좋긴 해요! ㅎㅎㅎ",
          "original_form": "아~~~ 그런 의미로 봄도 좋긴 해요! ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211103 22:07"
        },
        {
          "id": "MDRW2100046637.1.14",
          "form": "저는 겨울이 가장 좋아요",
          "original_form": "저는 겨울이 가장 좋아요",
          "speaker_id": "2",
          "time": "20211103 22:07"
        },
        {
          "id": "MDRW2100046637.1.15",
          "form": "으..겨울 추워서 싫음",
          "original_form": "으..겨울 추워서 싫음",
          "speaker_id": "1",
          "time": "20211103 22:08"
        },
        {
          "id": "MDRW2100046637.1.16",
          "form": "여름도 싫고",
          "original_form": "여름도 싫고",
          "speaker_id": "1",
          "time": "20211103 22:08"
        },
        {
          "id": "MDRW2100046637.1.17",
          "form": "봄을 가장 좋아하시나봐요!",
          "original_form": "봄을 가장 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20211103 22:08"
        },
        {
          "id": "MDRW2100046637.1.18",
          "form": "단풍 나들이 계획은 있으신가요?",
          "original_form": "단풍 나들이 계획은 있으신가요?",
          "speaker_id": "2",
          "time": "20211103 22:08"
        },
        {
          "id": "MDRW2100046637.1.19",
          "form": "사실 딱히 좋아하는 계절은 없어요",
          "original_form": "사실 딱히 좋아하는 계절은 없어요",
          "speaker_id": "1",
          "time": "20211103 22:09"
        },
        {
          "id": "MDRW2100046637.1.20",
          "form": "그냥 근처에 드라이브나 하면서 단풍 구경 하려구요",
          "original_form": "그냥 근처에 드라이브나 하면서 단풍 구경 하려구요",
          "speaker_id": "1",
          "time": "20211103 22:09"
        },
        {
          "id": "MDRW2100046637.1.21",
          "form": "단풍졌을때 드라이브 하면 너무 좋더라구요",
          "original_form": "단풍졌을때 드라이브 하면 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20211103 22:10"
        },
        {
          "id": "MDRW2100046637.1.22",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211103 22:11"
        }
      ]
