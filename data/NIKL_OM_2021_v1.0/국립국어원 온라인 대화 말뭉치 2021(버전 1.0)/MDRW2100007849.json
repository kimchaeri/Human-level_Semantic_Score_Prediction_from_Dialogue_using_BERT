      "utterance": [
        {
          "id": "MDRW2100007849.1.1",
          "form": "안녕하세요 드라마 좋아하세요?",
          "original_form": "안녕하세요 드라마 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007849.1.2",
          "form": "가족들 보는거 따라서 봤어요",
          "original_form": "가족들 보는거 따라서 봤어요",
          "speaker_id": "2",
          "time": "20210822 11:21"
        },
        {
          "id": "MDRW2100007849.1.3",
          "form": "요즘에 드라마 보신 것 있으세요?",
          "original_form": "요즘에 드라마 보신 것 있으세요?",
          "speaker_id": "2",
          "time": "20210822 11:21"
        },
        {
          "id": "MDRW2100007849.1.4",
          "form": "저는 드라마 좋아해서 요즘에 악마판사랑 너는나의 봄 봐요",
          "original_form": "저는 드라마 좋아해서 요즘에 악마판사랑 너는나의 봄 봐요",
          "speaker_id": "1",
          "time": "20210822 11:22"
        },
        {
          "id": "MDRW2100007849.1.5",
          "form": "와 다 처음들어보는 드라마네요",
          "original_form": "와 다 처음들어보는 드라마네요",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007849.1.6",
          "form": "저는 펜트하우스랑 오케이 광자매 봤어요",
          "original_form": "저는 펜트하우스랑 오케이 광자매 봤어요",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007849.1.7",
          "form": "아 저도 오케이 광자매는 가족들이랑 같이 봤어요",
          "original_form": "아 저도 오케이 광자매는 가족들이랑 같이 봤어요",
          "speaker_id": "1",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007849.1.8",
          "form": "저도 가족들이 봐서 봤는데 나름 재밌더라고요",
          "original_form": "저도 가족들이 봐서 봤는데 나름 재밌더라고요",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007849.1.9",
          "form": "맞아요 재밌더라구요  유튜브로는 주로 뭐 보세요?",
          "original_form": "맞아요 재밌더라구요  유튜브로는 주로 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007849.1.10",
          "form": "저 예랑가랑 보는데 혹시 아세요?",
          "original_form": "저 예랑가랑 보는데 혹시 아세요?",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007849.1.11",
          "form": "진짜 재밌어요",
          "original_form": "진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007849.1.12",
          "form": "네 알아요!  맞아요 진짜 재밌어용",
          "original_form": "네 알아요!  맞아요 진짜 재밌어용",
          "speaker_id": "1",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007849.1.13",
          "form": "음악은 혹시 뭐 들으세요?",
          "original_form": "음악은 혹시 뭐 들으세요?",
          "speaker_id": "2",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007849.1.14",
          "form": "음악은 요즘에 슬기로운 의사생활2 OST 듣고있어요!",
          "original_form": "음악은 요즘에 슬기로운 의사생활2 OST 듣고있어요!",
          "speaker_id": "1",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100007849.1.15",
          "form": "오 슬의생 ost면 진짜 좋을 것 같네요",
          "original_form": "오 슬의생 ost면 진짜 좋을 것 같네요",
          "speaker_id": "2",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007849.1.16",
          "form": "저는 보통 아이돌 음악 많이 들어요",
          "original_form": "저는 보통 아이돌 음악 많이 들어요",
          "speaker_id": "2",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007849.1.17",
          "form": "저는 가끔 아이돌노래 들어요",
          "original_form": "저는 가끔 아이돌노래 들어요",
          "speaker_id": "1",
