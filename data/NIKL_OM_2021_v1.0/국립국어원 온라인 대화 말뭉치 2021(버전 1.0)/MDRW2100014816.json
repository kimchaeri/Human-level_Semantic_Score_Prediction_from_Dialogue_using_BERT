        },
        {
          "id": "MDRW2100014816.1.2",
          "form": "집 밥 먹어라",
          "original_form": "집 밥 먹어라",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014816.1.3",
          "form": "계속 집밥만 먹었잖아",
          "original_form": "계속 집밥만 먹었잖아",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014816.1.4",
          "form": "집에 아직 먹을 거 많다",
          "original_form": "집에 아직 먹을 거 많다",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014816.1.5",
          "form": "글쿤.. ㅠ알았당",
          "original_form": "글쿤.. ㅠ알았당",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014816.1.6",
          "form": "뭐 해 줄까",
          "original_form": "뭐 해 줄까",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014816.1.7",
          "form": "음.. 오므라이스? ㅋㅋ",
          "original_form": "음.. 오므라이스? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014816.1.8",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014816.1.9",
          "form": "채소 많으니까 딱 좋을듯",
          "original_form": "채소 많으니까 딱 좋을듯",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014816.1.10",
          "form": "그래 냉장고 털어라",
          "original_form": "그래 냉장고 털어라",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014816.1.11",
          "form": "좋았으.. 냉장고 좀 비우자 ㅋㅋ",
          "original_form": "좋았으.. 냉장고 좀 비우자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014816.1.12",
          "form": "그래 다 털면 맛있는 거 사 먹어라",
          "original_form": "그래 다 털면 맛있는 거 사 먹어라",
