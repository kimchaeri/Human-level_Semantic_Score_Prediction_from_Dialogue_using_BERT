        },
        {
          "id": "MDRW2100032174.1.3",
          "form": "최근에 여행 간 곳 있어??",
          "original_form": "최근에 여행 간 곳 있어??",
          "speaker_id": "1",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100032174.1.4",
          "form": "음...꿈나라",
          "original_form": "음...꿈나라",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032174.1.5",
          "form": "아닠ㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032174.1.6",
          "form": "현실에서 간 곳은 없어??",
          "original_form": "현실에서 간 곳은 없어??",
          "speaker_id": "1",
          "time": "20210905 13:59"
        },
        {
          "id": "MDRW2100032174.1.7",
          "form": "음...아!부산 갔던거 같아~",
          "original_form": "음...아!부산 갔던거 같아~",
          "speaker_id": "2",
          "time": "20210905 13:59"
        },
        {
          "id": "MDRW2100032174.1.8",
          "form": "부산 가면 어묵 먹었어??",
          "original_form": "부산 가면 어묵 먹었어??",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032174.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032174.1.10",
          "form": "어묵은 못먹었어ㅋㅋ",
          "original_form": "어묵은 못먹었어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032174.1.11",
          "form": "아니!!!",
          "original_form": "아니!!!",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032174.1.12",
          "form": "그럼 부산을 가는 이유가 없잖아!!",
          "original_form": "그럼 부산을 가는 이유가 없잖아!!",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100032174.1.13",
          "form": "아 name2는 부산 바로 옆에 사니까...",
          "original_form": "아 &name2&는 부산 바로 옆에 사니까...",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032174.1.14",
          "form": "그럴 수도 있나",
          "original_form": "그럴 수도 있나",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032174.1.15",
          "form": "ㅋㅋㅋㄱㅋㄱㅋ사실",
          "original_form": "ㅋㅋㅋㄱㅋㄱㅋ사실",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032174.1.16",
          "form": "부산 쇼핑하러가ㅋㅋ",
          "original_form": "부산 쇼핑하러가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032174.1.17",
          "form": "고렇군...",
          "original_form": "고렇군...",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032174.1.18",
          "form": "내가 시내 가는 거랑 비슷한가봐...",
          "original_form": "내가 시내 가는 거랑 비슷한가봐...",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032174.1.19",
          "form": "나도 시내 나간다고 납작만두 먹는 건 아니니까(?)",
          "original_form": "나도 시내 나간다고 납작만두 먹는 건 아니니까(?)",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032174.1.20",
          "form": "응ㅋㅋㅋㅋㅋㅋ",
          "original_form": "응ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
