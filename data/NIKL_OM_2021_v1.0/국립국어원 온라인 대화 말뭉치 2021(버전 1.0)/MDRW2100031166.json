      "utterance": [
        {
          "id": "MDRW2100031166.1.1",
          "form": "헉 그게 뭔가요",
          "original_form": "헉 그게 뭔가요",
          "speaker_id": "2",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031166.1.2",
          "form": "먹물떡볶이가 나왔더라고요...?",
          "original_form": "먹물떡볶이가 나왔더라고요...?",
          "speaker_id": "1",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100031166.1.3",
          "form": "일단 듣기에는 딱히 맛이 좋아보이지는 않네요..",
          "original_form": "일단 듣기에는 딱히 맛이 좋아보이지는 않네요..",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031166.1.4",
          "form": "비주얼도... 제취향은 아니더라고요",
          "original_form": "비주얼도... 제취향은 아니더라고요",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031166.1.5",
          "form": "떡볶이는 떡볶이 자체로 맛있는데 말이죠ㅋㅋㅋ",
          "original_form": "떡볶이는 떡볶이 자체로 맛있는데 말이죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031166.1.6",
          "form": "맞아요 저는 밀떡파에욤",
          "original_form": "맞아요 저는 밀떡파에욤",
          "speaker_id": "2",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031166.1.7",
          "form": "오 저도 밀떡이요!!",
          "original_form": "오 저도 밀떡이요!!",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031166.1.8",
          "form": "쌀떡은 너무 쫀득쫀득하지 않아요?",
          "original_form": "쌀떡은 너무 쫀득쫀득하지 않아요?",
          "speaker_id": "1",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031166.1.9",
          "form": "쌀떡은 뭔가 식감이 답답하다고 해야하나?",
          "original_form": "쌀떡은 뭔가 식감이 답답하다고 해야하나?",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031166.1.10",
          "form": "좀 간도 잘 안베어서 별로더라구요",
          "original_form": "좀 간도 잘 안베어서 별로더라구요",
          "speaker_id": "2",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100031166.1.11",
          "form": "밀떡은 씹히는대로 씹혀서 좋은거 같아요",
          "original_form": "밀떡은 씹히는대로 씹혀서 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210904 20:06"
        },
        {
          "id": "MDRW2100031166.1.12",
          "form": "맞아요 간도잘베고 그냥 호로록 들어가져요",
          "original_form": "맞아요 간도잘베고 그냥 호로록 들어가져요",
          "speaker_id": "2",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100031166.1.13",
          "form": "저녁 드셨나요??",
          "original_form": "저녁 드셨나요??",
          "speaker_id": "2",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100031166.1.14",
          "form": "넹 먹었어요!",
          "original_form": "넹 먹었어요!",
          "speaker_id": "1",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100031166.1.15",
          "form": "소갈비 구워먹었네용",
          "original_form": "소갈비 구워먹었네용",
          "speaker_id": "1",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100031166.1.16",
          "form": "님은 뭐드셨어요?",
          "original_form": "님은 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100031166.1.17",
          "form": "저는 아까 국수먹었는데",
          "original_form": "저는 아까 국수먹었는데",
          "speaker_id": "2",
          "time": "20210904 21:42"
        },
        {
          "id": "MDRW2100031166.1.18",
          "form": "넘 맛있는거 드시고 오셨네요",
          "original_form": "넘 맛있는거 드시고 오셨네요",
          "speaker_id": "2",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031166.1.19",
          "form": "ㅋㅋㅋ오 국수도 맛있죠!",
          "original_form": "ㅋㅋㅋ오 국수도 맛있죠!",
          "speaker_id": "1",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100031166.1.20",
          "form": "무슨 국수드셨어요?",
          "original_form": "무슨 국수드셨어요?",
          "speaker_id": "1",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100031166.1.21",
          "form": "잔치국수 먹고 왔어요",
          "original_form": "잔치국수 먹고 왔어요",
          "speaker_id": "2",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100031166.1.22",
          "form": "5시쯤 먹었더니 살짝 입맛이 다시 돌긴 하네요",
          "original_form": "5시쯤 먹었더니 살짝 입맛이 다시 돌긴 하네요",
          "speaker_id": "2",
          "time": "20210904 21:47"
        },
        {
          "id": "MDRW2100031166.1.23",
          "form": "아이 그러면 배고플때 됐죠~~!!",
          "original_form": "아이 그러면 배고플때 됐죠~~!!",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031166.1.24",
          "form": "히히 비빔면 먹을까요",
          "original_form": "히히 비빔면 먹을까요",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100031166.1.25",
          "form": "비빔면 먹어요",
          "original_form": "비빔면 먹어요",
          "speaker_id": "1",
          "time": "20210904 21:51"
        },
        {
          "id": "MDRW2100031166.1.26",
          "form": "캬 맛있겠어요",
          "original_form": "캬 맛있겠어요",
          "speaker_id": "1",
          "time": "20210904 21:52"
        },
        {
          "id": "MDRW2100031166.1.27",
          "form": "고민될때는 먹어야죠ㅋㅋㅋ",
          "original_form": "고민될때는 먹어야죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:52"
        },
        {
          "id": "MDRW2100031166.1.28",
          "form": "비빔면은 항상 1개는 적고 2개는 너무 많은거같아요",
          "original_form": "비빔면은 항상 1개는 적고 2개는 너무 많은거같아요",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100031166.1.29",
          "form": "보통 비빔면 먹을때 몇개 끓여 드시나요?ㅋㅋㅋ",
          "original_form": "보통 비빔면 먹을때 몇개 끓여 드시나요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031166.1.30",
          "form": "아 맞아요 진짜",
          "original_form": "아 맞아요 진짜",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031166.1.31",
          "form": "아 진짜요",
          "original_form": "아 진짜요",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031166.1.32",
          "form": "한개는 작고 두개는 많고",
          "original_form": "한개는 작고 두개는 많고",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031166.1.33",
          "form": "그래서 전 국수면을 더넣어요ㅋㅋㅋ",
          "original_form": "그래서 전 국수면을 더넣어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:55"
        }
      ]
