      "utterance": [
        {
          "id": "MDRW2100041807.1.1",
          "form": "구미사랑상품권 사나요?",
          "original_form": "구미사랑상품권 사나요?",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041807.1.2",
          "form": "ㅋㅋㅋ내 폰도 이상하네 알림안오네 흠",
          "original_form": "ㅋㅋㅋ내 폰도 이상하네 알림안오네 흠",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041807.1.3",
          "form": "ㅋㅋ아뉴 샀나요?",
          "original_form": "ㅋㅋ아뉴 샀나요?",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041807.1.4",
          "form": "진짜 춥지않나유..",
          "original_form": "진짜 춥지않나유..",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041807.1.5",
          "form": "난 주방좀 정리하려고",
          "original_form": "난 주방좀 정리하려고",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041807.1.6",
          "form": "문닫고 에어콘틀엇다",
          "original_form": "문닫고 에어콘틀엇다",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041807.1.7",
          "form": "ㅋㅋㅋㅋㅋ아무래도 같은 지역이 아닌게 분명",
          "original_form": "ㅋㅋㅋㅋㅋ아무래도 같은 지역이 아닌게 분명",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041807.1.8",
          "form": "ㅋㅋㅋㅋ아까사모가 밖에가 시원한거같네 하면서 문열어둘까해서 네~햇는데그러고",
          "original_form": "ㅋㅋㅋㅋ아까사모가 밖에가 시원한거같네 하면서 문열어둘까해서 네~햇는데그러고",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041807.1.9",
          "form": "에어콘은안껏더라 ㅋㅋㅋ나도 나중에 알앗지만",
          "original_form": "에어콘은안껏더라 ㅋㅋㅋ나도 나중에 알앗지만",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041807.1.10",
          "form": "내가 놀면뭐하니잼잇다고",
          "original_form": "내가 놀면뭐하니잼잇다고",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041807.1.11",
          "form": "ㅋㅋㅋ저번주꺼?",
          "original_form": "ㅋㅋㅋ저번주꺼?",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041807.1.12",
          "form": "하니깐 다시보기하는데 막 엄청 웃기게 웃더라 ㅋㅋㅋ",
          "original_form": "하니깐 다시보기하는데 막 엄청 웃기게 웃더라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041807.1.13",
          "form": "나 양치하는데 막와서 정준하보라면서 ㅋㅋㅋ크ㅡㅡ",
          "original_form": "나 양치하는데 막와서 정준하보라면서 ㅋㅋㅋ크ㅡㅡ",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041807.1.14",
          "form": "ㅋㅋㅋ취향저격당했나봄",
          "original_form": "ㅋㅋㅋ취향저격당했나봄",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041807.1.15",
