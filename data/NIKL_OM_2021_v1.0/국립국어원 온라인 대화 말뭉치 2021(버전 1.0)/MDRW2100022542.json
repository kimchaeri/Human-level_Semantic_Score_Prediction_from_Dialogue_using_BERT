      "utterance": [
        {
          "id": "MDRW2100022542.1.1",
          "form": "저는 버터랑 식빵 등등 베이커리류 샀어요 ㅋㅋㅋㅋ",
          "original_form": "저는 버터랑 식빵 등등 베이커리류 샀어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100022542.1.2",
          "form": "혹시 키워드 3개다 입력하세요?",
          "original_form": "혹시 키워드 3개다 입력하세요?",
          "speaker_id": "1",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022542.1.3",
          "form": "아니요 저는 필수만요!",
          "original_form": "아니요 저는 필수만요!",
          "speaker_id": "2",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022542.1.4",
          "form": "아 ㅎㅎ 갑자기 또 대화가 초기화 돼서 ㅠㅠ",
          "original_form": "아 ㅎㅎ 갑자기 또 대화가 초기화 돼서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022542.1.5",
          "form": "버터 식빵 베이커리류 아주 알차게 샀네요",
          "original_form": "버터 식빵 베이커리류 아주 알차게 샀네요",
          "speaker_id": "1",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022542.1.6",
          "form": "마켓컬리가 밀키트 잘 되어있는 것 같아요",
          "original_form": "마켓컬리가 밀키트 잘 되어있는 것 같아요",
          "speaker_id": "1",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022542.1.7",
          "form": "네네 ㅋㅋㅋㅋ버터 이즈니버터 맛잇어요",
          "original_form": "네네 ㅋㅋㅋㅋ버터 이즈니버터 맛잇어요",
          "speaker_id": "2",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022542.1.8",
          "form": "혹시 그 버터쿠키도 사보셨나요",
          "original_form": "혹시 그 버터쿠키도 사보셨나요",
          "speaker_id": "1",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022542.1.9",
          "form": "맞아요 밀키트 인기잇더라구요",
          "original_form": "맞아요 밀키트 인기잇더라구요",
          "speaker_id": "2",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022542.1.10",
          "form": "오 아니요 어느브랜드인가요?",
          "original_form": "오 아니요 어느브랜드인가요?",
          "speaker_id": "2",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100022542.1.11",
          "form": "ㅋㅋㅋ밀키트 진짜 맛있어용 간편하구",
          "original_form": "ㅋㅋㅋ밀키트 진짜 맛있어용 간편하구",
          "speaker_id": "1",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100022542.1.12",
          "form": "버터쿠키 그거 브랜드는 잘 모르겠어요",
          "original_form": "버터쿠키 그거 브랜드는 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100022542.1.13",
          "form": "근데 3개씩 소포장 되어있어서 좋았어요!",
          "original_form": "근데 3개씩 소포장 되어있어서 좋았어요!",
          "speaker_id": "1",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100022542.1.14",
          "form": "오 한번 찾아볼게요",
          "original_form": "오 한번 찾아볼게요",
          "speaker_id": "2",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100022542.1.15",
          "form": "또 쿠폰 주더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "또 쿠폰 주더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100022542.1.16",
          "form": "네네 그래서 저 그거 갖고 다녀요",
          "original_form": "네네 그래서 저 그거 갖고 다녀요",
          "speaker_id": "1",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100022542.1.17",
          "form": "한강 같은데 가서 아아랑 먹으면 딱이에요",
          "original_form": "한강 같은데 가서 아아랑 먹으면 딱이에요",
