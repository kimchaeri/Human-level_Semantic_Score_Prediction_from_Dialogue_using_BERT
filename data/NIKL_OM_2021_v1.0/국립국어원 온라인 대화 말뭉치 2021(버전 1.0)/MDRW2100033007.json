      "utterance": [
        {
          "id": "MDRW2100033007.1.1",
          "form": "저는 교대근무라서",
          "original_form": "저는 교대근무라서",
          "speaker_id": "2",
          "time": "20210906 12:55"
        },
        {
          "id": "MDRW2100033007.1.2",
          "form": "월요일이 딱히 없습니다",
          "original_form": "월요일이 딱히 없습니다",
