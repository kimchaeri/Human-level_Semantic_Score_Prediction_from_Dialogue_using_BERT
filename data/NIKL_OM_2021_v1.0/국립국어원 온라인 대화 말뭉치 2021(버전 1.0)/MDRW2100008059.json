      "utterance": [
        {
          "id": "MDRW2100008059.1.1",
          "form": "운동 좋아하시나요?",
          "original_form": "운동 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008059.1.2",
          "form": "운동 좋아합니다!!",
          "original_form": "운동 좋아합니다!!",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008059.1.3",
          "form": "요즘에도 운동하시나요?",
          "original_form": "요즘에도 운동하시나요?",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008059.1.4",
          "form": "코로나 때문에 쉬고 있어요ㅜ",
          "original_form": "코로나 때문에 쉬고 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008059.1.5",
          "form": "name1님은 운동하세요?",
          "original_form": "&name1&님은 운동하세요?",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008059.1.6",
          "form": "요즘 유산소만 1~2시간 해요",
          "original_form": "요즘 유산소만 1~2시간 해요",
          "speaker_id": "1",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008059.1.7",
          "form": "코로나 전에는 어떤 운동 하셨어요?",
          "original_form": "코로나 전에는 어떤 운동 하셨어요?",
          "speaker_id": "1",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008059.1.8",
          "form": "필라테스요!!",
          "original_form": "필라테스요!!",
          "speaker_id": "2",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008059.1.9",
          "form": "체형교정된다고 해서 해봤는데",
          "original_form": "체형교정된다고 해서 해봤는데",
          "speaker_id": "2",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008059.1.10",
          "form": "생각외로 힘든운동이였어요ㅎㅎ",
          "original_form": "생각외로 힘든운동이였어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008059.1.11",
          "form": "오 저도 필라테스 해보고 싶어요",
          "original_form": "오 저도 필라테스 해보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008059.1.12",
          "form": "숨은 키를 찾아준다는데 몸이 좀 펴지나요?ㅎㅎ",
          "original_form": "숨은 키를 찾아준다는데 몸이 좀 펴지나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008059.1.13",
          "form": "그룹으로 하면 효과보긴 좀 어렵지만",
          "original_form": "그룹으로 하면 효과보긴 좀 어렵지만",
          "speaker_id": "2",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008059.1.14",
          "form": "1:1은 쌤만 잘만나면 좋은거같아요!!",
          "original_form": "1:1은 쌤만 잘만나면 좋은거같아요!!",
          "speaker_id": "2",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008059.1.15",
          "form": "저는 몸이 워낙 다 난리라서 ㅋㅋㅋ 쉽지않았습니다 ㅋㅋ",
          "original_form": "저는 몸이 워낙 다 난리라서 ㅋㅋㅋ 쉽지않았습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008059.1.16",
          "form": "저도 유연하지 않아서 좀 걱정되네요",
          "original_form": "저도 유연하지 않아서 좀 걱정되네요",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008059.1.17",
          "form": "필라테스 어느정도 하셨어요?",
          "original_form": "필라테스 어느정도 하셨어요?",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008059.1.18",
          "form": "전 서실 운동 시작한 건 두 달밖에 안 됐어요",
          "original_form": "전 서실 운동 시작한 건 두 달밖에 안 됐어요",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008059.1.19",
          "form": "저도 6개월 정도 하다가 쉬고있어서....",
          "original_form": "저도 6개월 정도 하다가 쉬고있어서....",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008059.1.20",
          "form": "집에서 나름 홈트하는뎀",
          "original_form": "집에서 나름 홈트하는뎀",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008059.1.21",
          "form": "만족스럽진 않네요ㅎㅎ",
          "original_form": "만족스럽진 않네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008059.1.22",
          "form": "저도 혼자하려니까 힘들어요",
          "original_form": "저도 혼자하려니까 힘들어요",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008059.1.23",
          "form": "저는 의지가 너무 약해서",
          "original_form": "저는 의지가 너무 약해서",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008059.1.24",
          "form": "요즘 걷기만 좀 해요",
          "original_form": "요즘 걷기만 좀 해요",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008059.1.25",
          "form": "걷는것도 중요한거 같요!!",
          "original_form": "걷는것도 중요한거 같요!!",
          "speaker_id": "2",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008059.1.26",
          "form": "바른 자세로 걷는거 배웠거든요!!",
          "original_form": "바른 자세로 걷는거 배웠거든요!!",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008059.1.27",
          "form": "저도 걷는 법 앉는 법 막 찾아봤어요",
          "original_form": "저도 걷는 법 앉는 법 막 찾아봤어요",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008059.1.28",
          "form": "바른 자세가 쉽지 않더라고요",
          "original_form": "바른 자세가 쉽지 않더라고요",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008059.1.29",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008059.1.30",
          "form": "그래도 누가 가르쳐줘서 한동안 고정되어야 할만해요ㅜ",
          "original_form": "그래도 누가 가르쳐줘서 한동안 고정되어야 할만해요ㅜ",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008059.1.31",
          "form": "운동은 솔직히 혼자 배우긴 쉽지않는거 같아요...ㅎㅎ",
          "original_form": "운동은 솔직히 혼자 배우긴 쉽지않는거 같아요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:15"
        }
      ]
