      "utterance": [
        {
          "id": "MDRW2100039828.1.1",
          "form": "헉 너무너무 좋으셨겠다....",
          "original_form": "헉 너무너무 좋으셨겠다....",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039828.1.2",
          "form": "5월 말이 비성수기군요!?",
          "original_form": "5월 말이 비성수기군요!?",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039828.1.3",
          "form": "좋은 정보 알아갑니다,, 제 생일이 그 때거든요!",
          "original_form": "좋은 정보 알아갑니다,, 제 생일이 그 때거든요!",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039828.1.4",
          "form": "5월 생일이시군요",
          "original_form": "5월 생일이시군요",
          "speaker_id": "1",
          "time": "20210904 22:22"
        },
        {
          "id": "MDRW2100039828.1.5",
          "form": "아 최근에 계곡 갔다와ㅏㅆ었네요",
          "original_form": "아 최근에 계곡 갔다와ㅏㅆ었네요",
          "speaker_id": "1",
          "time": "20210904 22:22"
        },
        {
          "id": "MDRW2100039828.1.6",
          "form": "오오 계곡!",
          "original_form": "오오 계곡!",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100039828.1.7",
          "form": "한참 더울 때 다녀오셨나봐요~~~",
          "original_form": "한참 더울 때 다녀오셨나봐요~~~",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100039828.1.8",
          "form": "넵 근데 사람들이 다 개울 따라 거리두기 하고 있어서 넘 웃겼어요 ㅋㅋ",
          "original_form": "넵 근데 사람들이 다 개울 따라 거리두기 하고 있어서 넘 웃겼어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:56"
        },
        {
          "id": "MDRW2100039828.1.9",
          "form": "앜ㅋㅋㅋㅋㅋ 아닠ㅋㅋ 너무 웃겨욬ㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 아닠ㅋㅋ 너무 웃겨욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100039828.1.10",
          "form": "개울 따라 거리두기라니,,",
          "original_form": "개울 따라 거리두기라니,,",
          "speaker_id": "2",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100039828.1.11",
          "form": "한국인들 말 잘들어서 귀여워요ㅠ",
          "original_form": "한국인들 말 잘들어서 귀여워요ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100039828.1.12",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100039828.1.13",
          "form": "대만 가보셨나요?",
          "original_form": "대만 가보셨나요?",
          "speaker_id": "1",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100039828.1.14",
          "form": "아니요... 가보진 못했고",
          "original_form": "아니요... 가보진 못했고",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100039828.1.15",
          "form": "대만 친구랑 인스타로 알게되어서",
          "original_form": "대만 친구랑 인스타로 알게되어서",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100039828.1.16",
          "form": "연락하고 지낸지 2년 넘었어요!",
          "original_form": "연락하고 지낸지 2년 넘었어요!",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100039828.1.17",
          "form": "오? 대만친구랑 어떻게 인스타로 알게 되셨나요??",
          "original_form": "오? 대만친구랑 어떻게 인스타로 알게 되셨나요??",
          "speaker_id": "1",
          "time": "20210906 10:37"
        },
        {
          "id": "MDRW2100039828.1.18",
          "form": "제가... 포켓몬 중에 푸린을 덕질하는데",
          "original_form": "제가... 포켓몬 중에 푸린을 덕질하는데",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100039828.1.19",
          "form": "덕질 계정 운영하다가",
          "original_form": "덕질 계정 운영하다가",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100039828.1.20",
          "form": "만난 친구예요!!!",
          "original_form": "만난 친구예요!!!",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100039828.1.21",
          "form": "앜ㅋㅋㅋㅋㅋ푸린을 덕질하다니",
          "original_form": "앜ㅋㅋㅋㅋㅋ푸린을 덕질하다니",
          "speaker_id": "1",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100039828.1.22",
          "form": "처음 들어봐요",
          "original_form": "처음 들어봐요",
          "speaker_id": "1",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100039828.1.23",
          "form": "대만 친구...그럼 집에도 재워주지ㅜ않을까여",
          "original_form": "대만 친구...그럼 집에도 재워주지ㅜ않을까여",
          "speaker_id": "1",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100039828.1.24",
          "form": "왠지 그럴지도 모르겠어요!!!!",
          "original_form": "왠지 그럴지도 모르겠어요!!!!",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100039828.1.25",
          "form": "헉 그럼 너무 좋겠네요",
          "original_form": "헉 그럼 너무 좋겠네요",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100039828.1.26",
          "form": "숙소 비용만 줄여도...!!!",
          "original_form": "숙소 비용만 줄여도...!!!",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100039828.1.27",
          "form": "그리고 좋은 곳 같이 놀러 다닐 수도 잇으니까요",
          "original_form": "그리고 좋은 곳 같이 놀러 다닐 수도 잇으니까요",
          "speaker_id": "1",
          "time": "20210906 10:39"
        },
        {
          "id": "MDRW2100039828.1.28",
          "form": "짱이다...",
          "original_form": "짱이다...",
          "speaker_id": "1",
          "time": "20210906 10:39"
        },
        {
          "id": "MDRW2100039828.1.29",
          "form": "현지인 맛집이 찐이잖아요",
          "original_form": "현지인 맛집이 찐이잖아요",
          "speaker_id": "1",
          "time": "20210906 10:39"
        },
        {
          "id": "MDRW2100039828.1.30",
          "form": "히히 그쵸!!!!!",
          "original_form": "히히 그쵸!!!!!",
          "speaker_id": "2",
          "time": "20210906 10:40"
        },
        {
          "id": "MDRW2100039828.1.31",
          "form": "하 빨리 코로나가 끝나야하는데.......",
          "original_form": "하 빨리 코로나가 끝나야하는데.......",
