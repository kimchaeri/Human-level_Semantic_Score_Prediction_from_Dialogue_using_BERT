      "utterance": [
        {
          "id": "MDRW2100026715.1.1",
          "form": "가을이 다가오는데 가을옷 샀어??",
          "original_form": "가을이 다가오는데 가을옷 샀어??",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026715.1.2",
          "form": "아직 안샀다! 너는 샀어?",
          "original_form": "아직 안샀다! 너는 샀어?",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026715.1.3",
          "form": "아니 아직 사지는 않았어!",
          "original_form": "아니 아직 사지는 않았어!",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026715.1.4",
          "form": "슬슬 사려고",
          "original_form": "슬슬 사려고",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026715.1.5",
          "form": "사고싶은 옷 있어??",
          "original_form": "사고싶은 옷 있어??",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026715.1.6",
          "form": "아노락!! 사고싶어",
          "original_form": "아노락!! 사고싶어",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026715.1.7",
          "form": "헐 나도!",
          "original_form": "헐 나도!",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026715.1.8",
          "form": "요즘 바람막이 재질로된 아노락 찾아보고 있었어!",
          "original_form": "요즘 바람막이 재질로된 아노락 찾아보고 있었어!",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026715.1.9",
          "form": "신기하다 ㅋㅋㅋ",
          "original_form": "신기하다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026715.1.10",
          "form": "역시 가을엔 바막이지",
          "original_form": "역시 가을엔 바막이지",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026715.1.11",
          "form": "맞아맞아!",
          "original_form": "맞아맞아!",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100026715.1.12",
          "form": "요즘 바막재질 아노락 예쁜거 너무 많아서",
          "original_form": "요즘 바막재질 아노락 예쁜거 너무 많아서",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100026715.1.13",
          "form": "뭘 살지 고민이야",
          "original_form": "뭘 살지 고민이야",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100026715.1.14",
          "form": "산다면 어떤 색상 살거야?",
          "original_form": "산다면 어떤 색상 살거야?",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100026715.1.15",
          "form": "옷을 잘 입는 편이 아니라ㅋㅋㅋㅋ",
          "original_form": "옷을 잘 입는 편이 아니라ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100026715.1.16",
          "form": "무채색 위주로 보고 있어!",
          "original_form": "무채색 위주로 보고 있어!",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100026715.1.17",
          "form": "옷 잘 입어???",
          "original_form": "옷 잘 입어???",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100026715.1.18",
          "form": "나도 잘입진않아 ㅋㅋㅋ",
          "original_form": "나도 잘입진않아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100026715.1.19",
          "form": "그냥 있는대로 주워입지",
          "original_form": "그냥 있는대로 주워입지",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100026715.1.20",
          "form": "ㅋㅋㅋㅋㅋ나도 그냥 패고만 아닌 수준이야...",
          "original_form": "ㅋㅋㅋㅋㅋ나도 그냥 패고만 아닌 수준이야...",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100026715.1.21",
          "form": "특히나 코로나 이후엔 편한거만 입어가지고",
          "original_form": "특히나 코로나 이후엔 편한거만 입어가지고",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100026715.1.22",
          "form": "편한 게 최고지!!!! 그게 멋",
          "original_form": "편한 게 최고지!!!! 그게 멋",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100026715.1.23",
          "form": "맞아! 그게 멋이지!",
          "original_form": "맞아! 그게 멋이지!",
          "speaker_id": "1",
          "time": "20210902 18:39"
        }
      ]
