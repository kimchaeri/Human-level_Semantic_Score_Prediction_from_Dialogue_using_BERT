      "utterance": [
        {
          "id": "MDRW2100027863.1.1",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "1",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100027863.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100027863.1.3",
          "form": "아니요 운동안해요ㅜ",
          "original_form": "아니요 운동안해요ㅜ",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100027863.1.4",
          "form": "해야하는데",
          "original_form": "해야하는데",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100027863.1.5",
          "form": "ㅋㅋㅋ저두요",
          "original_form": "ㅋㅋㅋ저두요",
          "speaker_id": "1",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100027863.1.6",
          "form": "말로는 해야지해야지",
          "original_form": "말로는 해야지해야지",
          "speaker_id": "1",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100027863.1.7",
          "form": "하는데 몸은 누워있어요...",
          "original_form": "하는데 몸은 누워있어요...",
          "speaker_id": "1",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100027863.1.8",
          "form": "그쵸 늘 낼부터 해야지 이러기만 해요",
          "original_form": "그쵸 늘 낼부터 해야지 이러기만 해요",
          "speaker_id": "2",
          "time": "20210905 09:56"
        },
        {
          "id": "MDRW2100027863.1.9",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100027863.1.10",
          "form": "영양제는 챙겨드시나요?",
          "original_form": "영양제는 챙겨드시나요?",
          "speaker_id": "1",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100027863.1.11",
          "form": "비타민 프로폴리스 유산균 기억날때 먹고 잇어요",
          "original_form": "비타민 프로폴리스 유산균 기억날때 먹고 잇어요",
          "speaker_id": "2",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100027863.1.12",
          "form": "오 ㅋㅋ저두요 ㅋㅋ 유산균은 어디거 드시나요?",
          "original_form": "오 ㅋㅋ저두요 ㅋㅋ 유산균은 어디거 드시나요?",
          "speaker_id": "1",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100027863.1.13",
          "form": "정착을 못했어요",
          "original_form": "정착을 못했어요",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100027863.1.14",
          "form": "뉴트리 지근억 비피도",
          "original_form": "뉴트리 지근억 비피도",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100027863.1.15",
          "form": "자로우",
          "original_form": "자로우",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100027863.1.16",
          "form": "저두요ㅠ 저도 아직 맘에 드는 것이 없네요 ㅠ",
          "original_form": "저두요ㅠ 저도 아직 맘에 드는 것이 없네요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100027863.1.17",
          "form": "랏토핏도 처음ㅇ먹을때 좋다고 생각했는데 먹다보니 쫌 아닌것같구ㅜㅜ",
          "original_form": "랏토핏도 처음ㅇ먹을때 좋다고 생각했는데 먹다보니 쫌 아닌것같구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100027863.1.18",
          "form": "균주바꿔주는게 좋다고 하더라구요",
          "original_form": "균주바꿔주는게 좋다고 하더라구요",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100027863.1.19",
          "form": "락토핏이 종근당이죠?",
          "original_form": "락토핏이 종근당이죠?",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100027863.1.20",
          "form": "네네 ㅋㅋ 우리가족락토핏~ 맨날 선전하는 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋ 우리가족락토핏~ 맨날 선전하는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100027863.1.21",
          "form": "핏핏핏핏",
          "original_form": "핏핏핏핏",
          "speaker_id": "2",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100027863.1.22",
          "form": "귀에 맴도네요ㅋㅋ",
          "original_form": "귀에 맴도네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100027863.1.23",
          "form": "광고의 힘",
          "original_form": "광고의 힘",
          "speaker_id": "2",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100027863.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그쵸 ㅋㅋㅋ저도 노래부르면서 썼어요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그쵸 ㅋㅋㅋ저도 노래부르면서 썼어요 ㅋㅋㅋㅋㅋ",
