      "utterance": [
        {
          "id": "MDRW2100008088.1.1",
          "form": "프렌차이즈 커피는 너무써",
          "original_form": "프렌차이즈 커피는 너무써",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008088.1.2",
          "form": "아메리카노 말하는구야?",
          "original_form": "아메리카노 말하는구야?",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008088.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008088.1.4",
          "form": "사약 먹는듯",
          "original_form": "사약 먹는듯",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008088.1.5",
          "form": "저번에 뭔 드립커피라는데 비싼데 갔는데",
          "original_form": "저번에 뭔 드립커피라는데 비싼데 갔는데",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008088.1.6",
          "form": "세상에 원두를 다 홀랑 태운거로 해서",
          "original_form": "세상에 원두를 다 홀랑 태운거로 해서",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008088.1.7",
          "form": "다 못먹고 나왔어",
          "original_form": "다 못먹고 나왔어",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008088.1.8",
          "form": "넌 어떤 커피?",
          "original_form": "넌 어떤 커피?",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008088.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛있는데만 골라서 찍어놓고 가야지 므ㅓ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛있는데만 골라서 찍어놓고 가야지 므ㅓ",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100008088.1.10",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008088.1.11",
          "form": "난 원래는 단거만 마셨는데 한 1년 2년? 전부터",
          "original_form": "난 원래는 단거만 마셨는데 한 1년 2년? 전부터",
          "speaker_id": "1",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008088.1.12",
          "form": "그래서 커피는 집에서",
          "original_form": "그래서 커피는 집에서",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008088.1.13",
          "form": "카페는 음료",
          "original_form": "카페는 음료",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008088.1.14",
          "form": "아아 뜨아도 좋아하기 시작했지ㅋㅋㅋ",
          "original_form": "아아 뜨아도 좋아하기 시작했지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008088.1.15",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008088.1.16",
          "form": "마자 아메리는 너무 가성비 떨어지잖아",
          "original_form": "마자 아메리는 너무 가성비 떨어지잖아",
          "speaker_id": "1",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100008088.1.17",
          "form": "맞아 맛난데는 진짜 맛나",
