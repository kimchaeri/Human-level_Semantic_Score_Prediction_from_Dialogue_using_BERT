      "utterance": [
        {
          "id": "MDRW2100035952.1.1",
          "form": "유튜브도 자주보시나요?",
          "original_form": "유튜브도 자주보시나요?",
          "speaker_id": "1",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100035952.1.2",
          "form": "네네! 예랑가랑 자주 봐요!",
          "original_form": "네네! 예랑가랑 자주 봐요!",
          "speaker_id": "2",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100035952.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100035952.1.4",
          "form": "아 예랑가랑 알죠 ㅋㅋㅋ",
          "original_form": "아 예랑가랑 알죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:14"
        },
        {
          "id": "MDRW2100035952.1.5",
          "form": "전 주로 먹방봅니다 ㅎㅎ",
          "original_form": "전 주로 먹방봅니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:14"
        },
        {
          "id": "MDRW2100035952.1.6",
          "form": "오 먹방 누구요!?",
          "original_form": "오 먹방 누구요!?",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100035952.1.7",
          "form": "저 햇님 좋아해요 !",
          "original_form": "저 햇님 좋아해요 !",
          "speaker_id": "1",
          "time": "20210830 18:17"
        },
        {
          "id": "MDRW2100035952.1.8",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210830 18:17"
        },
        {
          "id": "MDRW2100035952.1.9",
          "form": "헐 네네!! 그 놀토에 나오시는 분 맞죠",
          "original_form": "헐 네네!! 그 놀토에 나오시는 분 맞죠",
          "speaker_id": "2",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100035952.1.10",
          "form": "맞아요 ! 아시는구나 ㅎㅎ",
          "original_form": "맞아요 ! 아시는구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:19"
        },
        {
          "id": "MDRW2100035952.1.11",
          "form": "네네!! 엄청 많이 먹는 거 보고",
          "original_form": "네네!! 엄청 많이 먹는 거 보고",
          "speaker_id": "2",
          "time": "20210830 18:20"
        },
        {
          "id": "MDRW2100035952.1.12",
          "form": "깜짝 놀랐었거든요 ㅎ.ㅎ",
          "original_form": "깜짝 놀랐었거든요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210830 18:20"
        },
        {
          "id": "MDRW2100035952.1.13",
          "form": "맞아욬ㅋㅋ 입이 짧지않아요 ㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋ 입이 짧지않아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:20"
        },
        {
          "id": "MDRW2100035952.1.14",
          "form": "그러니까요 풀네임은 입짧은 햇님이라면서요!!",
          "original_form": "그러니까요 풀네임은 입짧은 햇님이라면서요!!",
          "speaker_id": "2",
          "time": "20210830 18:22"
        },
        {
          "id": "MDRW2100035952.1.15",
          "form": "절대.. 짧을 수가 없던데. ..",
          "original_form": "절대.. 짧을 수가 없던데. ..",
          "speaker_id": "2",
          "time": "20210830 18:22"
        },
        {
          "id": "MDRW2100035952.1.16",
          "form": "ㅋㅋㅋㄱㅋㄱ네네 !!!",
          "original_form": "ㅋㅋㅋㄱㅋㄱ네네 !!!",
          "speaker_id": "1",
          "time": "20210830 18:25"
        },
        {
          "id": "MDRW2100035952.1.17",
          "form": "놀토도 보시는군요!",
          "original_form": "놀토도 보시는군요!",
          "speaker_id": "1",
          "time": "20210830 18:25"
        },
        {
          "id": "MDRW2100035952.1.18",
          "form": "매번 챙겨보는 건 아닌데 웃긴 회!? 만 봐요",
          "original_form": "매번 챙겨보는 건 아닌데 웃긴 회!? 만 봐요",
          "speaker_id": "2",
