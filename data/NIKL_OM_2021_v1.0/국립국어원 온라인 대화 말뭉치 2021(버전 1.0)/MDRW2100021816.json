      "utterance": [
        {
          "id": "MDRW2100021816.1.1",
          "form": "최근에 구매하신 물건 뭐에요?",
          "original_form": "최근에 구매하신 물건 뭐에요?",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021816.1.2",
          "form": "저는 동물의숲 타이틀 샀어요",
          "original_form": "저는 동물의숲 타이틀 샀어요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021816.1.3",
          "form": "최근에 구매하신거 있어요?",
          "original_form": "최근에 구매하신거 있어요?",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021816.1.4",
          "form": "저는 그릭요거트 만들려고 면보자기 삿어요",
          "original_form": "저는 그릭요거트 만들려고 면보자기 삿어요",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021816.1.5",
          "form": "동숲 재밋어요!",
          "original_form": "동숲 재밋어요!",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021816.1.6",
          "form": "동숲하시나요?",
          "original_form": "동숲하시나요?",
          "speaker_id": "1",
          "time": "20210829 00:49"
        },
        {
          "id": "MDRW2100021816.1.7",
          "form": "넹넹 해욤",
          "original_form": "넹넹 해욤",
          "speaker_id": "2",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100021816.1.8",
          "form": "중고거래로 사서 틈틈히 하고있는데 소소하게 재밌어요",
          "original_form": "중고거래로 사서 틈틈히 하고있는데 소소하게 재밌어요",
          "speaker_id": "1",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021816.1.9",
          "form": "섬꾸미고 돈벌고 주민 만나고 재밌네요",
          "original_form": "섬꾸미고 돈벌고 주민 만나고 재밌네요",
          "speaker_id": "1",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100021816.1.10",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100021816.1.11",
          "form": "돈버는 재미가 잇어요",
          "original_form": "돈버는 재미가 잇어요",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100021816.1.12",
          "form": "오늘 무사셨나요?",
          "original_form": "오늘 무사셨나요?",
          "speaker_id": "1",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100021816.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100021816.1.14",
          "form": "아녀 ㅋㅋㅋ 저 늦게인나서 못삿어요 ㅌㅋㅌ",
          "original_form": "아녀 ㅋㅋㅋ 저 늦게인나서 못삿어요 ㅌㅋㅌ",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100021816.1.15",
          "form": "일요일 오전만 파는거 너무하지않나 싶어요 늦잠자는사람 배려해줘라!",
          "original_form": "일요일 오전만 파는거 너무하지않나 싶어요 늦잠자는사람 배려해줘라!",
          "speaker_id": "1",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100021816.1.16",
          "form": "맞아요ㅠㅠ ㅌㅋㅌ 근데 살짝 시간 건드리면 살수는 잇어요 ㅋㅋㅋ",
          "original_form": "맞아요ㅠㅠ ㅌㅋㅌ 근데 살짝 시간 건드리면 살수는 잇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100021816.1.17",
          "form": "앜ㅋㄲㅋㅋ그렇긴하죠 저는 리얼타임 지향이라 놓치고하는 재미로해요",
          "original_form": "앜ㅋㄲㅋㅋ그렇긴하죠 저는 리얼타임 지향이라 놓치고하는 재미로해요",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100021816.1.18",
          "form": "요거트는 만들어보셨어요?",
          "original_form": "요거트는 만들어보셨어요?",
          "speaker_id": "1",
