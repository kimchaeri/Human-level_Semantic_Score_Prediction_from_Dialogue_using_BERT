      "utterance": [
        {
          "id": "MDRW2100045155.1.1",
          "form": "빵은 어디꺼 사드세용?",
          "original_form": "빵은 어디꺼 사드세용?",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045155.1.2",
          "form": "파바여 할인도 하고 이번에는 핑크퐁 사운드북 준대서 샀어요",
          "original_form": "파바여 할인도 하고 이번에는 핑크퐁 사운드북 준대서 샀어요",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045155.1.3",
          "form": "둘째댐시,",
          "original_form": "둘째댐시,",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045155.1.4",
          "form": "엇 몰랐어요! 핑키퐁사운드북이요?",
          "original_form": "엇 몰랐어요! 핑키퐁사운드북이요?",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045155.1.5",
          "form": "핑크퐁 사운드북이요",
          "original_form": "핑크퐁 사운드북이요",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045155.1.6",
          "form": "사전예약했는데ㅠ",
          "original_form": "사전예약했는데ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045155.1.7",
          "form": "14900원에 100원 사운드북 값으로 15000원 했어여ㅠ",
          "original_form": "14900원에 100원 사운드북 값으로 15000원 했어여ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045155.1.8",
          "form": "와짱이다ㅜㅜㅜ 빵도사고 핑크퐁사운드북까지ㅠ",
          "original_form": "와짱이다ㅜㅜㅜ 빵도사고 핑크퐁사운드북까지ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045155.1.9",
          "form": "애가 아기상어를 너무 좋아해서 사봤어여 근데 책은 아니고 걍 노래만 나오더라구요 그래도 100원이니까",
          "original_form": "애가 아기상어를 너무 좋아해서 사봤어여 근데 책은 아니고 걍 노래만 나오더라구요 그래도 100원이니까",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045155.1.10",
          "form": "그쵸그쵸 내일은 오랜만에 마트나 가야겠어요ㅋㅋ",
          "original_form": "그쵸그쵸 내일은 오랜만에 마트나 가야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045155.1.11",
          "form": "오오 머사실라구요?",
          "original_form": "오오 머사실라구요?",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045155.1.12",
          "form": "시리얼도사고 과자이것저것이랑ㅋㅋ",
          "original_form": "시리얼도사고 과자이것저것이랑ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045155.1.13",
          "form": "쿠팡은 양이 많아서ㅜㅜ",
          "original_form": "쿠팡은 양이 많아서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045155.1.14",
          "form": "맞아여ㅠㅠㅠ 갯수가 많은건 부담이져ㅠ",
          "original_form": "맞아여ㅠㅠㅠ 갯수가 많은건 부담이져ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045155.1.15",
          "form": "마가렛트를 사고싶은데 쿠팡은 네박스나 파는거에요!ㅋㅋ",
          "original_form": "마가렛트를 사고싶은데 쿠팡은 네박스나 파는거에요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045155.1.16",
          "form": "으아....",
          "original_form": "으아....",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045155.1.17",
          "form": "많긴많네여ㅠ",
          "original_form": "많긴많네여ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045155.1.18",
          "form": "아 마가렛트..ㅠ맛있겠네여ㅠ",
          "original_form": "아 마가렛트..ㅠ맛있겠네여ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045155.1.19",
          "form": "살찌긴하지만..다먹을것아니구..몇개만ㅋㅋ",
          "original_form": "살찌긴하지만..다먹을것아니구..몇개만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045155.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 먹다보면 봉지가 수북..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 먹다보면 봉지가 수북..",
