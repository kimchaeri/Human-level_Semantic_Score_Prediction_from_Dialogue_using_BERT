      "utterance": [
        {
          "id": "MDRW2100021391.1.1",
          "form": "무슨 일?",
          "original_form": "무슨 일?",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021391.1.2",
          "form": "갈망굿즈라 허겁지겁 샀는데",
          "original_form": "갈망굿즈라 허겁지겁 샀는데",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021391.1.3",
          "form": "응 근데 무슨 일이 있었어",
          "original_form": "응 근데 무슨 일이 있었어",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021391.1.4",
          "form": "파는사람이 미성년자더라구...",
          "original_form": "파는사람이 미성년자더라구...",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021391.1.5",
          "form": "오오 근데 그게 문제였어?",
          "original_form": "오오 근데 그게 문제였어?",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021391.1.6",
          "form": "그냥 그래도 샀는데.. 돈은 받고 배송을 안해줘..",
          "original_form": "그냥 그래도 샀는데.. 돈은 받고 배송을 안해줘..",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021391.1.7",
          "form": "엥 왜 안해줘? 사기 아냐?",
          "original_form": "엥 왜 안해줘? 사기 아냐?",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021391.1.8",
          "form": "언제 배송해주냐고 닥달하니까 학교때문에 자꾸 변명하는거야",
          "original_form": "언제 배송해주냐고 닥달하니까 학교때문에 자꾸 변명하는거야",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021391.1.9",
          "form": "그니까..",
          "original_form": "그니까..",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021391.1.10",
          "form": "아니 지금 등교 하긴 해?",
          "original_form": "아니 지금 등교 하긴 해?",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021391.1.11",
          "form": "그래서 왠지 물건 없는거같아서 환불해달라구 했거든?",
          "original_form": "그래서 왠지 물건 없는거같아서 환불해달라구 했거든?",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021391.1.12",
          "form": "응응 어떻게 됐어",
          "original_form": "응응 어떻게 됐어",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021391.1.13",
          "form": "그러니까 돈을 다 썼대.......",
          "original_form": "그러니까 돈을 다 썼대.......",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021391.1.14",
          "form": "경찰에 신고해야겠네",
          "original_form": "경찰에 신고해야겠네",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021391.1.15",
          "form": "그래서 할부로 환불해주면 안되냐고 하더라 ㅋㅋ",
          "original_form": "그래서 할부로 환불해주면 안되냐고 하더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021391.1.16",
          "form": "ㅋㅋㅋ말도 안되는 소리하네 무슨",
          "original_form": "ㅋㅋㅋ말도 안되는 소리하네 무슨",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021391.1.17",
          "form": "그래서 안됟나고 하고 신고한다고 하니까 결국 환불받긴 했는데...이래서 사람들이 미자랑은 거래하지 말라고 하는구나 배웠어..",
          "original_form": "그래서 안됟나고 하고 신고한다고 하니까 결국 환불받긴 했는데...이래서 사람들이 미자랑은 거래하지 말라고 하는구나 배웠어..",
          "speaker_id": "1",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100021391.1.18",
          "form": "맞아 미성년자들 버르장머리가 없어 아주",
          "original_form": "맞아 미성년자들 버르장머리가 없어 아주",
          "speaker_id": "2",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100021391.1.19",
          "form": "나 최근에 멍청비용 낼뻔함",
          "original_form": "나 최근에 멍청비용 낼뻔함",
          "speaker_id": "1",
