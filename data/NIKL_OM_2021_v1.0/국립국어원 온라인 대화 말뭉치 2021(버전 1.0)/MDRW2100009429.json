      "utterance": [
        {
          "id": "MDRW2100009429.1.1",
          "form": "약간 이것도 안심각하게보는거 같아요",
          "original_form": "약간 이것도 안심각하게보는거 같아요",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009429.1.2",
          "form": "특히 일드중 코믹수사물들은 ㅋㅋ",
          "original_form": "특히 일드중 코믹수사물들은 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009429.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ 아 일드 코믹수사물은 본적이없어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아 일드 코믹수사물은 본적이없어요",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009429.1.4",
          "form": "수사랑 코믹이 어떻게 양립하나요?",
          "original_form": "수사랑 코믹이 어떻게 양립하나요?",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009429.1.5",
          "form": "심각하지않은?",
          "original_form": "심각하지않은?",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009429.1.6",
          "form": "막 우리코난보고 저녁에잠못잔다거나 하지않는것처럼 ㅋㅋ",
          "original_form": "막 우리코난보고 저녁에잠못잔다거나 하지않는것처럼 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009429.1.7",
          "form": "아하 ㅋㅋㅋㅋ 그 일드 특유의 감성이 있는거같아요",
          "original_form": "아하 ㅋㅋㅋㅋ 그 일드 특유의 감성이 있는거같아요",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009429.1.8",
          "form": "저는 못따라가겠어요 ㅋㅋㅋ",
          "original_form": "저는 못따라가겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009429.1.9",
          "form": "전 좀 그게 웃기더라고요",
          "original_form": "전 좀 그게 웃기더라고요",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009429.1.10",
          "form": "웰케 기억력좋아 ㅋㅋ",
          "original_form": "웰케 기억력좋아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009429.1.11",
          "form": "몇시에 뭐했는지 넘 잘기억하는거요ㄱ ㄲ",
          "original_form": "몇시에 뭐했는지 넘 잘기억하는거요ㄱ ㄲ",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009429.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 그게 좀 웃길수도 있겠네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 그게 좀 웃길수도 있겠네요",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009429.1.13",
          "form": "뻘하게 웃긴",
          "original_form": "뻘하게 웃긴",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009429.1.14",
          "form": "대본이 좀ㅋㅋㅋ 사실과 동떨어져잇네요",
          "original_form": "대본이 좀ㅋㅋㅋ 사실과 동떨어져잇네요",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009429.1.15",
          "form": "그리고 범인도 되게 기억잘해!",
          "original_form": "그리고 범인도 되게 기억잘해!",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009429.1.16",
          "form": "스치듯 지나갔는데 ㅋㅋ",
          "original_form": "스치듯 지나갔는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009429.1.17",
          "form": "전솔직히 그 줄세워놓고 찾으라하면 못할거같은데 ㅋㅋ",
          "original_form": "전솔직히 그 줄세워놓고 찾으라하면 못할거같은데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009429.1.18",
          "form": "ㅋㅋㅋㅋㅋ아 그거 되게신기해요",
          "original_form": "ㅋㅋㅋㅋㅋ아 그거 되게신기해요",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009429.1.19",
          "form": "며칠전 일 진술하고",
          "original_form": "며칠전 일 진술하고",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009429.1.20",
          "form": "그런 의미에서 순한맛 수사물들은 좀 재밌게 보는편이에요",
          "original_form": "그런 의미에서 순한맛 수사물들은 좀 재밌게 보는편이에요",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009429.1.21",
          "form": "스트레스  안받고 ㅋㅋ",
          "original_form": "스트레스  안받고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009429.1.22",
          "form": "코믹으로 보시는구낰ㅋㅋㅋ",
          "original_form": "코믹으로 보시는구낰ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009429.1.23",
          "form": "저는 드라마같은거 너무 길어서 잘 못보거든요",
          "original_form": "저는 드라마같은거 너무 길어서 잘 못보거든요",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009429.1.24",
          "form": "그래도 스트레스 해소용으로는 좋은거같아요 오히려 기니까 꾸준히 볼수잇고",
          "original_form": "그래도 스트레스 해소용으로는 좋은거같아요 오히려 기니까 꾸준히 볼수잇고",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009429.1.25",
          "form": "전 영화는 좀 아쉽더라고요",
          "original_form": "전 영화는 좀 아쉽더라고요",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009429.1.26",
          "form": "그 캐릭터들이랑 헤어진다는게ㅜ",
          "original_form": "그 캐릭터들이랑 헤어진다는게ㅜ",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009429.1.27",
          "form": "아 맞네요 두시간동안 정들었는데 바로 이별하는거 좀 슬퍼요",
          "original_form": "아 맞네요 두시간동안 정들었는데 바로 이별하는거 좀 슬퍼요",
          "speaker_id": "1",
          "time": "20210823 08:50"
        },
        {
          "id": "MDRW2100009429.1.28",
          "form": "혹시 좋아하는 영화 시리즈 있으세요?",
          "original_form": "혹시 좋아하는 영화 시리즈 있으세요?",
          "speaker_id": "1",
          "time": "20210823 09:29"
        },
        {
          "id": "MDRW2100009429.1.29",
          "form": "반지의제왕이요",
          "original_form": "반지의제왕이요",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009429.1.30",
          "form": "언제봐도 명작이야ㅜㅜㅜ",
          "original_form": "언제봐도 명작이야ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009429.1.31",
          "form": "해리포터도 괜찮구요",
          "original_form": "해리포터도 괜찮구요",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009429.1.32",
          "form": "넘 고전인가",
          "original_form": "넘 고전인가",
          "speaker_id": "2",
          "time": "20210823 09:48"
        }
      ]
