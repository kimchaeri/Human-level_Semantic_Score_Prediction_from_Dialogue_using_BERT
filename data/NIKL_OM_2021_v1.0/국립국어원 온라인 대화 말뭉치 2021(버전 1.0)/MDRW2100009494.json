      "utterance": [
        {
          "id": "MDRW2100009494.1.1",
          "form": "안녕하세요. 혹시 반려동물 있으신가요?",
          "original_form": "안녕하세요. 혹시 반려동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009494.1.2",
          "form": "안녕하세요~ 저는 없습니다..님은요?",
          "original_form": "안녕하세요~ 저는 없습니다..님은요?",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009494.1.3",
          "form": "저는 예전에 금붕어를 키웠는데 지금은 없어요ㅎㅎ",
          "original_form": "저는 예전에 금붕어를 키웠는데 지금은 없어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009494.1.4",
          "form": "좋아하는 동물 있으세요?",
          "original_form": "좋아하는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009494.1.5",
          "form": "저는 개 좋아해요",
          "original_form": "저는 개 좋아해요",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009494.1.6",
          "form": "금붕어 좋아하셨나봐여~",
          "original_form": "금붕어 좋아하셨나봐여~",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009494.1.7",
          "form": "네 금붕어 좋아해요~",
          "original_form": "네 금붕어 좋아해요~",
          "speaker_id": "1",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009494.1.8",
          "form": "강아지 좋아하시면 강아지 유튜브도 보시나요?",
          "original_form": "강아지 좋아하시면 강아지 유튜브도 보시나요?",
          "speaker_id": "1",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009494.1.9",
          "form": "유튜브는 잘 몰라서요.. 반려동물 영상이 많은가요?",
          "original_form": "유튜브는 잘 몰라서요.. 반려동물 영상이 많은가요?",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009494.1.10",
          "form": "저도 많이 보지는 않지만 많이 있는거 같아요!",
          "original_form": "저도 많이 보지는 않지만 많이 있는거 같아요!",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009494.1.11",
          "form": "그럼 전에 키워본 동물도 없나요?",
          "original_form": "그럼 전에 키워본 동물도 없나요?",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009494.1.12",
          "form": "저는 없어요..",
          "original_form": "저는 없어요..",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009494.1.13",
          "form": "키우고 싶은 동물 있으신가요?",
          "original_form": "키우고 싶은 동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009494.1.14",
          "form": "제 주변에도 안 키우는 친구들이 더 많아요!",
          "original_form": "제 주변에도 안 키우는 친구들이 더 많아요!",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009494.1.15",
          "form": "전 고양이 키워보고 싶어요ㅎㅎ",
          "original_form": "전 고양이 키워보고 싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009494.1.16",
          "form": "고양이 정말 귀엽죠~",
          "original_form": "고양이 정말 귀엽죠~",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009494.1.17",
          "form": "고양이 어떤 점을 제일 좋아하시나요?",
          "original_form": "고양이 어떤 점을 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009494.1.18",
          "form": "기분에 따라 꼬리 움직이는게 너무 귀여워요~",
          "original_form": "기분에 따라 꼬리 움직이는게 너무 귀여워요~",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009494.1.19",
          "form": "키우고 싶은 동물 있으신가요?",
          "original_form": "키우고 싶은 동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009494.1.20",
          "form": "개를 키우고 싶어요",
          "original_form": "개를 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009494.1.21",
          "form": "고양이도 너무 좋구요",
          "original_form": "고양이도 너무 좋구요",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009494.1.22",
          "form": "너무 좋을거 같아요!",
          "original_form": "너무 좋을거 같아요!",
          "speaker_id": "1",
          "time": "20210823 00:09"
        }
      ]
