        {
          "id": "MDRW2100021904.1.1",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021904.1.2",
          "form": "까눌레좋아하세요?",
          "original_form": "까눌레좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021904.1.3",
          "form": "없어서 못 먹죠! 어디 맛집 아세요? ㅋㅋㅋㅋㅋ",
          "original_form": "없어서 못 먹죠! 어디 맛집 아세요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021904.1.4",
          "form": "여의도에 하얀과자점 맛있어요!",
          "original_form": "여의도에 하얀과자점 맛있어요!",
          "speaker_id": "2",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021904.1.5",
          "form": "당장 네이버 지도에 기록...",
          "original_form": "당장 네이버 지도에 기록...",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021904.1.6",
          "form": "홍대에 미쁘동도 기록해주세요..",
          "original_form": "홍대에 미쁘동도 기록해주세요..",
          "speaker_id": "2",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021904.1.7",
          "form": "해산물 좋아하세요?",
          "original_form": "해산물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021904.1.8",
          "form": "넵 당장 기록...",
          "original_form": "넵 당장 기록...",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021904.1.9",
          "form": "해산물도 없어서 못 먹어요",
          "original_form": "해산물도 없어서 못 먹어요",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021904.1.10",
          "form": "침흘리는 척 했더니 나쁜말이라고 하네요ㅠ 참나ㅠ ㅋㅋㅋㅋ",
          "original_form": "침흘리는 척 했더니 나쁜말이라고 하네요ㅠ 참나ㅠ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021904.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무해",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021904.1.12",
          "form": "미쁘동 여기 비주얼이 대박인데요",
          "original_form": "미쁘동 여기 비주얼이 대박인데요",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021904.1.13",
          "form": "서울대입구에 안녕베트남도 맛있습니다",
          "original_form": "서울대입구에 안녕베트남도 맛있습니다",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021904.1.14",
          "form": "와 저 저녁 진짜 많이 먹었는데 다시 배고파 지고 있어요",
          "original_form": "와 저 저녁 진짜 많이 먹었는데 다시 배고파 지고 있어요",
          "speaker_id": "1",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021904.1.15",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021904.1.16",
          "form": "성공이네요 ^^!",
          "original_form": "성공이네요 ^^!",
          "speaker_id": "2",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021904.1.17",
          "form": "아 근데 저  웬만하면 다 잘 먹는데 가리는? 음식 하나 있어요",
          "original_form": "아 근데 저  웬만하면 다 잘 먹는데 가리는? 음식 하나 있어요",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021904.1.18",
          "form": "빨리 뭔지 물어봐 주세요",
          "original_form": "빨리 뭔지 물어봐 주세요",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021904.1.19",
          "form": "빨리 알려주세요",
          "original_form": "빨리 알려주세요",
          "speaker_id": "2",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021904.1.20",
          "form": "저 물에 빠진 면을 별로 안 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "저 물에 빠진 면을 별로 안 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021904.1.21",
          "form": "국물이 흥건한... 면...",
          "original_form": "국물이 흥건한... 면...",
          "speaker_id": "1",
          "time": "20210829 00:01"
