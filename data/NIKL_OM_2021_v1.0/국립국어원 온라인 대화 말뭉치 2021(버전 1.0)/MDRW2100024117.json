      "utterance": [
        {
          "id": "MDRW2100024117.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024117.1.2",
          "form": "강아지를 한마리 키우고있습니다",
          "original_form": "강아지를 한마리 키우고있습니다",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024117.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024117.1.4",
          "form": "제가 키우는 강아지는 나이가 많습니다. 무려 15살이에요",
          "original_form": "제가 키우는 강아지는 나이가 많습니다. 무려 15살이에요",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024117.1.5",
          "form": "별명이 늙은애기입니다",
          "original_form": "별명이 늙은애기입니다",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024117.1.6",
          "form": "신사라는 이름이 잘 어울릴것만 같군요",
          "original_form": "신사라는 이름이 잘 어울릴것만 같군요",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024117.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ오호 쪽집게시네요 이름이 신사입니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ오호 쪽집게시네요 이름이 신사입니다",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024117.1.8",
          "form": "어제 산책을 시키지 않으셨나요 왜인지 모르게 그럴것만 같아요",
          "original_form": "어제 산책을 시키지 않으셨나요 왜인지 모르게 그럴것만 같아요",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024117.1.9",
          "form": "아쉽게도 저는 강아지를 키우지 않고 있어요",
          "original_form": "아쉽게도 저는 강아지를 키우지 않고 있어요",
          "speaker_id": "1",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024117.1.10",
          "form": "오 산책을 시켰지요",
          "original_form": "오 산책을 시켰지요",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024117.1.11",
          "form": "산책은 몇분 코스입니까?",
          "original_form": "산책은 몇분 코스입니까?",
          "speaker_id": "1",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024117.1.12",
          "form": "그러시군요 매우 귀찮으니 키우지 않는게 좋습니다",
          "original_form": "그러시군요 매우 귀찮으니 키우지 않는게 좋습니다",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024117.1.13",
          "form": "평균 산책시간은 20~30분 입니다",
          "original_form": "평균 산책시간은 20~30분 입니다",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024117.1.14",
          "form": "하루에 두번 아침 저녁이고 길게가는날는 40~50분도 갑니다",
          "original_form": "하루에 두번 아침 저녁이고 길게가는날는 40~50분도 갑니다",
          "speaker_id": "2",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024117.1.15",
          "form": "와 부지런하시네요",
          "original_form": "와 부지런하시네요",
          "speaker_id": "1",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024117.1.16",
          "form": "행복한 강아지겠어요 신사",
          "original_form": "행복한 강아지겠어요 신사",
          "speaker_id": "1",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024117.1.17",
          "form": "근데 모르겠죠 상팔자라",
          "original_form": "근데 모르겠죠 상팔자라",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024117.1.18",
          "form": "강아지는 그렇게 산책 시켜줘도 만족이 안되나봅니다. 계속 나가고 싶어해요",
          "original_form": "강아지는 그렇게 산책 시켜줘도 만족이 안되나봅니다. 계속 나가고 싶어해요",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024117.1.19",
          "form": "이젠 나이가 많아서 오래걸으면 힘들어합니다",
          "original_form": "이젠 나이가 많아서 오래걸으면 힘들어합니다",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024117.1.20",
          "form": "더울때도 산책 시켰나요?",
          "original_form": "더울때도 산책 시켰나요?",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024117.1.21",
          "form": "유모차는 필수지요 그럼요",
          "original_form": "유모차는 필수지요 그럼요",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024117.1.22",
          "form": "지칠만도 하겠어요",
          "original_form": "지칠만도 하겠어요",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024117.1.23",
          "form": "역시 상팔자인거 같네요",
          "original_form": "역시 상팔자인거 같네요",
          "speaker_id": "1",
          "time": "20210830 22:23"
        }
      ]
    }
