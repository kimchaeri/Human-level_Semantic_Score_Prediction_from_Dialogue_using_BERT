      "utterance": [
        {
          "id": "MDRW2100031607.1.1",
          "form": "네 봤씀니댜",
          "original_form": "네 봤씀니댜",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100031607.1.2",
          "form": "대충 보긴 했는데 보긴 봤어오",
          "original_form": "대충 보긴 했는데 보긴 봤어오",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100031607.1.3",
          "form": "이번주 뭐였어오?",
          "original_form": "이번주 뭐였어오?",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031607.1.4",
          "form": "이번주 주제가 위험한 여자들?",
          "original_form": "이번주 주제가 위험한 여자들?",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100031607.1.5",
          "form": "그 하나는 그냥 실제 사건이구",
          "original_form": "그 하나는 그냥 실제 사건이구",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100031607.1.6",
          "form": "헐 위험한 여자들이면",
          "original_form": "헐 위험한 여자들이면",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100031607.1.7",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031607.1.8",
          "form": "또 엄여인???",
          "original_form": "또 엄여인???",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031607.1.9",
          "form": "하나는 그 산에서 안좋은 일 당하셔서 악령이 된 여자 귀신 이야기 이구",
          "original_form": "하나는 그 산에서 안좋은 일 당하셔서 악령이 된 여자 귀신 이야기 이구",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031607.1.10",
          "form": "하나는....기억 안나오",
          "original_form": "하나는....기억 안나오",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031607.1.11",
          "form": "..........",
          "original_form": "..........",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031607.1.12",
          "form": "담주 유툽 기다리세오",
          "original_form": "담주 유툽 기다리세오",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031607.1.13",
          "form": "헐 무서워오",
          "original_form": "헐 무서워오",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031607.1.14",
          "form": "시먀괴다뫼에서 본 것 중 뭐가 가장 무서웠어?",
          "original_form": "시먀괴다뫼에서 본 것 중 뭐가 가장 무서웠어?",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100031607.1.15",
          "form": "나 그 일본 민박집?",
          "original_form": "나 그 일본 민박집?",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100031607.1.16",
          "form": "막 엄청 무서웠던 에피는 기억이 안나오",
          "original_form": "막 엄청 무서웠던 에피는 기억이 안나오",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100031607.1.17",
          "form": "아 일본민박집이 혹시",
          "original_form": "아 일본민박집이 혹시",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031607.1.18",
          "form": "에어비엔비에서...?",
          "original_form": "에어비엔비에서...?",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031607.1.19",
          "form": "오카사 민박집!",
          "original_form": "오카사 민박집!",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031607.1.20",
          "form": "헐 뭐죠",
          "original_form": "헐 뭐죠",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031607.1.21",
          "form": "네넹 그거 맞아요",
          "original_form": "네넹 그거 맞아요",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031607.1.22",
          "form": "그그 경고문이 살벌한 그거죠???",
          "original_form": "그그 경고문이 살벌한 그거죠???",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031607.1.23",
          "form": "집이 이상하면 밖으로 도망치라는...",
          "original_form": "집이 이상하면 밖으로 도망치라는...",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031607.1.24",
          "form": "네넹 그거 맞은!! 내일 낮게 유툽 다시 봐야지",
          "original_form": "네넹 그거 맞은!! 내일 낮게 유툽 다시 봐야지",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031607.1.25",
          "form": "지금은 못봐...",
          "original_form": "지금은 못봐...",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031607.1.26",
          "form": "님은 뭐가 무서웠나오",
          "original_form": "님은 뭐가 무서웠나오",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031607.1.27",
          "form": "저는 그 여고생들 따라오는 남자요",
          "original_form": "저는 그 여고생들 따라오는 남자요",
          "speaker_id": "1",
          "time": "20210904 23:20"
        }
      ]
    }
