      "utterance": [
        {
          "id": "MDRW2100033777.1.1",
          "form": "보고과자보다 더 득템느낌인데?ㅋ",
          "original_form": "보고과자보다 더 득템느낌인데?ㅋ",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.2",
          "form": "나 저번에 커피 만원치샀는데ㅋㅋ",
          "original_form": "나 저번에 커피 만원치샀는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.3",
          "form": "난 뜨자마자 바로잡음 근데 너희동네 순삭이라며",
          "original_form": "난 뜨자마자 바로잡음 근데 너희동네 순삭이라며",
          "speaker_id": "1",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.4",
          "form": "좋더라ㅋㅋ",
          "original_form": "좋더라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.5",
          "form": "니가 배달했어?",
          "original_form": "니가 배달했어?",
          "speaker_id": "1",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.6",
          "form": "응 근데 내꺼뜨는건",
          "original_form": "응 근데 내꺼뜨는건",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.7",
          "form": "잡았어ㅋㅋ",
          "original_form": "잡았어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.8",
          "form": "오 배달비 얼마?",
          "original_form": "오 배달비 얼마?",
          "speaker_id": "1",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.9",
          "form": "응응 막 뜨자마자 눌렀지",
          "original_form": "응응 막 뜨자마자 눌렀지",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.10",
          "form": "3000원이었나?",
          "original_form": "3000원이었나?",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033777.1.11",
          "form": "하여튼 그랬는듯ㅋㅋㅋㅋ",
          "original_form": "하여튼 그랬는듯ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100033777.1.12",
          "form": "응 거기서 수락 누르면 뜨더라",
          "original_form": "응 거기서 수락 누르면 뜨더라",
          "speaker_id": "1",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100033777.1.13",
          "form": "응응 그러니깐ㅋ",
          "original_form": "응응 그러니깐ㅋ",
          "speaker_id": "2",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100033777.1.14",
          "form": "좋다 거리좀 있음 액수가 큰듯",
          "original_form": "좋다 거리좀 있음 액수가 큰듯",
          "speaker_id": "1",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100033777.1.15",
          "form": "응 그렇더라구ㅋㅋ",
          "original_form": "응 그렇더라구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100033777.1.16",
          "form": "지오자전거태우고갔다가ㅋㅋ오는길에 잠들었더라ㅋㅋ",
          "original_form": "지오자전거태우고갔다가ㅋㅋ오는길에 잠들었더라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100033777.1.17",
          "form": "좋다좋다 아이스크림도 있는데",
          "original_form": "좋다좋다 아이스크림도 있는데",
          "speaker_id": "1",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100033777.1.18",
          "form": "도착하니ㅋㅋ낮잠 자더라고 웃겼어ㅋㅋ",
          "original_form": "도착하니ㅋㅋ낮잠 자더라고 웃겼어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100033777.1.19",
          "form": "아이스크림두셋은 쿠폰이 큰게 안먹혀서 과자1셋 아스크림한셋은 3천까지 먹힘",
          "original_form": "아이스크림두셋은 쿠폰이 큰게 안먹혀서 과자1셋 아스크림한셋은 3천까지 먹힘",
          "speaker_id": "1",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100033777.1.20",
          "form": "아이스크림도 좋네ㅋㅋ",
          "original_form": "아이스크림도 좋네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100033777.1.21",
          "form": "가격아름답구만ㅋㅋ",
          "original_form": "가격아름답구만ㅋㅋ",
