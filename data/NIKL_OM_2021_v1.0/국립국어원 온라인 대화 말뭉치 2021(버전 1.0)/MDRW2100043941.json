      "utterance": [
        {
          "id": "MDRW2100043941.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나 일단",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나 일단",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043941.1.2",
          "form": "장당 11만원짜리 하나 찜 해두긴 했오",
          "original_form": "장당 11만원짜리 하나 찜 해두긴 했오",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043941.1.3",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043941.1.4",
          "form": "난.. 없어.. 그냥 정가치고..해야하나...",
          "original_form": "난.. 없어.. 그냥 정가치고..해야하나...",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043941.1.5",
          "form": "아냐",
          "original_form": "아냐",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043941.1.6",
          "form": "내가 두장 찜해놔서",
          "original_form": "내가 두장 찜해놔서",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043941.1.7",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043941.1.8",
          "form": "같이 가면 돼",
          "original_form": "같이 가면 돼",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043941.1.9",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043941.1.10",
          "form": "(씐남",
          "original_form": "(씐남",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043941.1.11",
          "form": "ㅋㅋㅋㅋㅋ 그치만",
          "original_form": "ㅋㅋㅋㅋㅋ 그치만",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043941.1.12",
          "form": "더 저렴한게 나오면 갈아타야하니꺼",
          "original_form": "더 저렴한게 나오면 갈아타야하니꺼",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043941.1.13",
          "form": "꾸준히 보고잇으렴",
          "original_form": "꾸준히 보고잇으렴",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043941.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 당근 알람 키워드 설정하긴 했는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 당근 알람 키워드 설정하긴 했는데",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043941.1.15",
          "form": "이동네는 기대하면 안돼..",
          "original_form": "이동네는 기대하면 안돼..",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043941.1.16",
          "form": "중고나라도 알림 해줄까",
          "original_form": "중고나라도 알림 해줄까",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043941.1.17",
          "form": "둥",
          "original_form": "둥",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043941.1.18",
