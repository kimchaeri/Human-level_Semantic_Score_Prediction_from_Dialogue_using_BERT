      "utterance": [
        {
          "id": "MDRW2100009127.1.1",
          "form": "그리구 라면죽 해먹었어요~",
          "original_form": "그리구 라면죽 해먹었어요~",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009127.1.2",
          "form": "오오 튜닝의 끝은 순정이라죠",
          "original_form": "오오 튜닝의 끝은 순정이라죠",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009127.1.3",
          "form": "샤브먹고 죽해먹잖아요~ 그걸 라면에...",
          "original_form": "샤브먹고 죽해먹잖아요~ 그걸 라면에...",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009127.1.4",
          "form": "오오 어떻게 해먹어요?",
          "original_form": "오오 어떻게 해먹어요?",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009127.1.5",
          "form": "계란 넣어서?",
          "original_form": "계란 넣어서?",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009127.1.6",
          "form": "국물 조금 남기고 네네!",
          "original_form": "국물 조금 남기고 네네!",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009127.1.7",
          "form": "계란 푸는거죠?",
          "original_form": "계란 푸는거죠?",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009127.1.8",
          "form": "밥넣고 계란넣고 휘휘 풀어서 ㅎㅎ",
          "original_form": "밥넣고 계란넣고 휘휘 풀어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009127.1.9",
          "form": "무한 휘젓기~~",
          "original_form": "무한 휘젓기~~",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009127.1.10",
          "form": "라면 물 좀더 잡을걸 그랬어요ㅜ",
          "original_form": "라면 물 좀더 잡을걸 그랬어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009127.1.11",
          "form": "그러면 서서히 죽이 되요!",
          "original_form": "그러면 서서히 죽이 되요!",
          "speaker_id": "1",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009127.1.12",
          "form": "국물이 거의 안남아가는데",
          "original_form": "국물이 거의 안남아가는데",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009127.1.13",
          "form": "아 국물 거의 안남겨도 돼요!",
          "original_form": "아 국물 거의 안남겨도 돼요!",
          "speaker_id": "1",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009127.1.14",
          "form": "아 오래 젓는게 포인트군요",
          "original_form": "아 오래 젓는게 포인트군요",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009127.1.15",
          "form": "바닥에 남을정도만 해서 ㅎㅎ",
          "original_form": "바닥에 남을정도만 해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100009127.1.16",
          "form": "저는 라면먹구 그냥 국물에 밥말아먹기만 했었는데 이런 방법으로 먹기도 하더라구요~",
          "original_form": "저는 라면먹구 그냥 국물에 밥말아먹기만 했었는데 이런 방법으로 먹기도 하더라구요~",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009127.1.17",
          "form": "그래서 해봤더니 음~~ ㅋㅋ 만족",
          "original_form": "그래서 해봤더니 음~~ ㅋㅋ 만족",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009127.1.18",
          "form": "밥 한공기에 계란 몇개가 적절한가요",
          "original_form": "밥 한공기에 계란 몇개가 적절한가요",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009127.1.19",
          "form": "음... 저는 반공기에 계란 하나 넣었어요!",
          "original_form": "음... 저는 반공기에 계란 하나 넣었어요!",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009127.1.20",
          "form": "두개?",
          "original_form": "두개?",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009127.1.21",
          "form": "레시피가 있을수도 있어요 검색하면 ㅋㅋㅋㅋ",
          "original_form": "레시피가 있을수도 있어요 검색하면 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009127.1.22",
          "form": "아마도...?",
          "original_form": "아마도...?",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009127.1.23",
          "form": "라면 다 먹어가니 이제 곧 도전해볼게요",
          "original_form": "라면 다 먹어가니 이제 곧 도전해볼게요",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009127.1.24",
          "form": "아직도 배고파서 밥 한공기 다 넣어야 할듯",
          "original_form": "아직도 배고파서 밥 한공기 다 넣어야 할듯",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009127.1.25",
          "form": "ㅋㅋㅋㅋㅋ오늘 한끼도 못드셨나요 ㅠㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ오늘 한끼도 못드셨나요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009127.1.26",
          "form": "그러면 너무 배불러서 못자는거 아니에요?",
          "original_form": "그러면 너무 배불러서 못자는거 아니에요?",
          "speaker_id": "1",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009127.1.27",
          "form": "원래 아침 안먹고 점심 저녁에 잘 먹어요ㅎ",
          "original_form": "원래 아침 안먹고 점심 저녁에 잘 먹어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009127.1.28",
          "form": "지금 요리하는 중인데 얼추 죽 모양 나요!!",
          "original_form": "지금 요리하는 중인데 얼추 죽 모양 나요!!",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009127.1.29",
          "form": "남는게 부족한 거보다 낫죠ㅎ",
          "original_form": "남는게 부족한 거보다 낫죠ㅎ",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100009127.1.30",
          "form": "신기하죠 ㅋㅋ 라면으로 죽해먹는생각을 해본적이 없어서",
          "original_form": "신기하죠 ㅋㅋ 라면으로 죽해먹는생각을 해본적이 없어서",
          "speaker_id": "1",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009127.1.31",
          "form": "죽 모양새가 나니까 신기하더라구요",
          "original_form": "죽 모양새가 나니까 신기하더라구요",
          "speaker_id": "1",
          "time": "20210822 21:42"
        }
      ]
