      "utterance": [
        {
          "id": "MDRW2100038431.1.1",
          "form": "헐 저 서핑도 해보고 싶어요!",
          "original_form": "헐 저 서핑도 해보고 싶어요!",
          "speaker_id": "1",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100038431.1.2",
          "form": "물론 계속 넘어질 것 같지만 ㅎ.ㅎ",
          "original_form": "물론 계속 넘어질 것 같지만 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100038431.1.3",
          "form": "헐 저두요!",
          "original_form": "헐 저두요!",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100038431.1.4",
          "form": "저도 균형감각이 없어서 서핑 할 용기가 안나는데",
          "original_form": "저도 균형감각이 없어서 서핑 할 용기가 안나는데",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100038431.1.5",
          "form": "파도 타면 기분 째질거같아요",
          "original_form": "파도 타면 기분 째질거같아요",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100038431.1.6",
          "form": "그니까요 약간 포세이돈 된 기분이지 않을까요?!??!?!",
          "original_form": "그니까요 약간 포세이돈 된 기분이지 않을까요?!??!?!",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100038431.1.7",
          "form": "ㅎ.ㅎ",
          "original_form": "ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100038431.1.8",
          "form": "우와 포세이돈!!!",
          "original_form": "우와 포세이돈!!!",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038431.1.9",
          "form": "초보자 강습도 잇더라구요",
          "original_form": "초보자 강습도 잇더라구요",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038431.1.10",
          "form": "헐 코로나 끝나면 당장 달려가야겠아요",
          "original_form": "헐 코로나 끝나면 당장 달려가야겠아요",
          "speaker_id": "1",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038431.1.11",
          "form": "물 속에선 마스크 못 쓰니까..",
          "original_form": "물 속에선 마스크 못 쓰니까..",
          "speaker_id": "1",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038431.1.12",
          "form": "아 그 방수마스크 있는거 아세요?",
          "original_form": "아 그 방수마스크 있는거 아세요?",
          "speaker_id": "2",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100038431.1.13",
          "form": "헐 몰라요!! 그런게 있나요?!?!?!??!??????",
          "original_form": "헐 몰라요!! 그런게 있나요?!?!?!??!??????",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100038431.1.14",
          "form": "네!! 그게 잇어서 물놀이할때 그거쓰고하면돼요!",
          "original_form": "네!! 그게 잇어서 물놀이할때 그거쓰고하면돼요!",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100038431.1.15",
          "form": "헐 대박 알았으면 당장 구매하는 건데 ㅠ.^",
          "original_form": "헐 대박 알았으면 당장 구매하는 건데 ㅠ.^",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100038431.1.16",
          "form": "저 무서워서 올해 물놀이 한 번도 난 갔거든요",
          "original_form": "저 무서워서 올해 물놀이 한 번도 난 갔거든요",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100038431.1.17",
          "form": "안가신게 나을수도잇어요 ㅎㅎㅎㅎ 저는 한번 갓는데 다른사람들이 마스크를 잘 안끼더라구요",
          "original_form": "안가신게 나을수도잇어요 ㅎㅎㅎㅎ 저는 한번 갓는데 다른사람들이 마스크를 잘 안끼더라구요",
          "speaker_id": "2",
          "time": "20210903 08:49"
        },
        {
          "id": "MDRW2100038431.1.18",
          "form": "네에...? 코로나가 이렇게 심한데도요????????",
          "original_form": "네에...? 코로나가 이렇게 심한데도요????????",
          "speaker_id": "1",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100038431.1.19",
          "form": "에반데........ 그러고는 붙어서 물놀이하나요..,",
          "original_form": "에반데........ 그러고는 붙어서 물놀이하나요..,",
          "speaker_id": "1",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100038431.1.20",
          "form": "저는 바닷가에 간거였는데 마스크 쓴사람도 있고 벗는사람도 있었어요",
          "original_form": "저는 바닷가에 간거였는데 마스크 쓴사람도 있고 벗는사람도 있었어요",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100038431.1.21",
          "form": "실내는 좀 더 철져하겟죠..?",
          "original_form": "실내는 좀 더 철져하겟죠..?",
          "speaker_id": "2",
          "time": "20210903 13:16"
        },
        {
          "id": "MDRW2100038431.1.22",
          "form": "으악!!!!!!!!!!",
          "original_form": "으악!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100038431.1.23",
          "form": "실내는 ㅠ.ㅠ 더 철저해야죠!!! 전 정말 코로나 때문에 매일 매일 건강 걱정하는데",
          "original_form": "실내는 ㅠ.ㅠ 더 철저해야죠!!! 전 정말 코로나 때문에 매일 매일 건강 걱정하는데",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100038431.1.24",
          "form": "용감한 사람들이 생각보다 많구만요 ^.^",
          "original_form": "용감한 사람들이 생각보다 많구만요 ^.^",
