      "utterance": [
        {
          "id": "MDRW2100025707.1.1",
          "form": "기구타는것도 잇지만 사진 찍으러 많이가니까ㅎㅎ",
          "original_form": "기구타는것도 잇지만 사진 찍으러 많이가니까ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025707.1.2",
          "form": "ㅋㅋㅋㅋㅋ맞아요 그거죠",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 그거죠",
          "speaker_id": "2",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025707.1.3",
          "form": "저희도 관람차 앞에서 사진찍고 관람차 타고 풍경찍는데  1시간 이상 보낸 것 같아요",
          "original_form": "저희도 관람차 앞에서 사진찍고 관람차 타고 풍경찍는데  1시간 이상 보낸 것 같아요",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025707.1.4",
          "form": "오~ 관람차까지",
          "original_form": "오~ 관람차까지",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025707.1.5",
          "form": "있을건 다 있네요ㅋㅋㅋ",
          "original_form": "있을건 다 있네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025707.1.6",
          "form": "역시 경주!",
          "original_form": "역시 경주!",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025707.1.7",
          "form": "경주 최고죠!",
          "original_form": "경주 최고죠!",
          "speaker_id": "2",
          "time": "20210901 16:21"
        },
        {
          "id": "MDRW2100025707.1.8",
          "form": "그런데 저는 상황이 많이 괜찮아지면 롯데월드 다시 한번 가고 싶어요ㅠㅠ",
          "original_form": "그런데 저는 상황이 많이 괜찮아지면 롯데월드 다시 한번 가고 싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025707.1.9",
          "form": "아ㅜㅜ 최고죠",
          "original_form": "아ㅜㅜ 최고죠",
          "speaker_id": "1",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025707.1.10",
          "form": "실내가 있다는 게 너무너무 좋더라구요ㅎㅎㅎㅎ",
          "original_form": "실내가 있다는 게 너무너무 좋더라구요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025707.1.11",
          "form": "놀이동산 가는 것도 여행이니까요ㅎㅎ",
          "original_form": "놀이동산 가는 것도 여행이니까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100025707.1.12",
          "form": "저는 지방 살아서 그런가",
          "original_form": "저는 지방 살아서 그런가",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025707.1.13",
          "form": "서울 가는 자체가 여행이에요ㅋㅋㅋㅋㅋ",
          "original_form": "서울 가는 자체가 여행이에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025707.1.14",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋ 저도 지방인인데 서울 친구집 놀러 갈 때면 항상 여행 계획 세우고 올라가요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋ 저도 지방인인데 서울 친구집 놀러 갈 때면 항상 여행 계획 세우고 올라가요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:26"
        },
        {
          "id": "MDRW2100025707.1.15",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "1",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100025707.1.16",
          "form": "랜선여행이라도",
          "original_form": "랜선여행이라도",
          "speaker_id": "1",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100025707.1.17",
          "form": "해야겠어요ㅜㅜ",
          "original_form": "해야겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100025707.1.18",
          "form": "하 정말요ㅠㅠ",
          "original_form": "하 정말요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100025707.1.19",
          "form": "다들 취업하고 바쁘게 사는데 날 잡아서 다 같이 영상통화하면서 놀고싶네요ㅜㅠ",
          "original_form": "다들 취업하고 바쁘게 사는데 날 잡아서 다 같이 영상통화하면서 놀고싶네요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100025707.1.20",
          "form": "해외여행 다녀온 곳",
          "original_form": "해외여행 다녀온 곳",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100025707.1.21",
          "form": "있으새요?!",
          "original_form": "있으새요?!",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100025707.1.22",
          "form": "아니요ㅜㅠㅠㅠㅠ 해외여행 경험 아직 한 번도 없어요",
          "original_form": "아니요ㅜㅠㅠㅠㅠ 해외여행 경험 아직 한 번도 없어요",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100025707.1.23",
          "form": "진짜 너무너무 가고싶어요",
          "original_form": "진짜 너무너무 가고싶어요",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100025707.1.24",
          "form": "어디 가보고 싶으세요?!",
          "original_form": "어디 가보고 싶으세요?!",
          "speaker_id": "1",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100025707.1.25",
          "form": "하필 이시국이라 올해까지도 힘들겠네요ㅠㅜ",
          "original_form": "하필 이시국이라 올해까지도 힘들겠네요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100025707.1.26",
          "form": "저 유렵여행 가고싶어요!",
          "original_form": "저 유렵여행 가고싶어요!",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100025707.1.27",
          "form": "제가 마지막으로 간 해외여행이",
          "original_form": "제가 마지막으로 간 해외여행이",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100025707.1.28",
          "form": "유럽여행인데ㅜㅜ",
          "original_form": "유럽여행인데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 00:41"
        }
      ]
    }
