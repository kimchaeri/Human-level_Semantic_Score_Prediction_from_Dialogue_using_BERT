      "utterance": [
        {
          "id": "MDRW2100000469.1.1",
          "form": "이번에 올림픽 보셨어요?",
          "original_form": "이번에 올림픽 보셨어요?",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000469.1.2",
          "form": "네 봤죠!",
          "original_form": "네 봤죠!",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000469.1.3",
          "form": "배구랑 양궁 너무 재밌게 봤어요",
          "original_form": "배구랑 양궁 너무 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000469.1.4",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000469.1.5",
          "form": "특히 양궁선수들 너무 멋있더라구요",
          "original_form": "특히 양궁선수들 너무 멋있더라구요",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000469.1.6",
          "form": "언제한번 꼭 직관가보고싶어요!",
          "original_form": "언제한번 꼭 직관가보고싶어요!",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000469.1.7",
          "form": "마자요 저두요ㅠ\n배구도 직관가보고 싶어요",
          "original_form": "마자요 저두요ㅠ\n배구도 직관가보고 싶어요",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000469.1.8",
          "form": "양궁 대학교때 수업에서 배웠었는데 직접해도 진짜 재밌어요",
          "original_form": "양궁 대학교때 수업에서 배웠었는데 직접해도 진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000469.1.9",
          "form": "요새는 양궁카페도 많이 생겨서 실제로 해보시면 더 재밌으실거에요",
          "original_form": "요새는 양궁카페도 많이 생겨서 실제로 해보시면 더 재밌으실거에요",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000469.1.10",
          "form": "오 저는 한번도 해본적은 없어요!",
          "original_form": "오 저는 한번도 해본적은 없어요!",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000469.1.11",
          "form": "배구 선수는 좋아하는 사람 있으신가요?",
          "original_form": "배구 선수는 좋아하는 사람 있으신가요?",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000469.1.12",
          "form": "한번 시도해보세요 진짜 재밌어요",
          "original_form": "한번 시도해보세요 진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000469.1.13",
          "form": "배구선수 다 좋죠",
          "original_form": "배구선수 다 좋죠",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000469.1.14",
          "form": "김연경선수도 좋고",
          "original_form": "김연경선수도 좋고",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000469.1.15",
          "form": "맞아요 김연경선수 너무 멋있어요",
          "original_form": "맞아요 김연경선수 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000469.1.16",
          "form": "김희진선수 양효진선수 다 좋아요!",
          "original_form": "김희진선수 양효진선수 다 좋아요!",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000469.1.17",
          "form": "여자 배구 리그가 이번 기회로 많이 알려져서 좋은 것 같아요",
          "original_form": "여자 배구 리그가 이번 기회로 많이 알려져서 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000469.1.18",
          "form": "김희진 선수 너무 멋있죠",
          "original_form": "김희진 선수 너무 멋있죠",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000469.1.19",
          "form": "마자요 더 관심 많이 받으면 좋을텐데요ㅠ",
          "original_form": "마자요 더 관심 많이 받으면 좋을텐데요ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000469.1.20",
          "form": "어떤 선수 좋아하세요?",
          "original_form": "어떤 선수 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000469.1.21",
          "form": "저도 김연견선수 좋아해요",
          "original_form": "저도 김연견선수 좋아해요",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000469.1.22",
          "form": "경기하는 모습 볼때마다 너무 멋있더라구요",
          "original_form": "경기하는 모습 볼때마다 너무 멋있더라구요",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000469.1.23",
          "form": "마자요 진짜 멋있죠ㅠ\n마인드도 너무 좋으셔서 더 좋은거같아요",
          "original_form": "마자요 진짜 멋있죠ㅠ\n마인드도 너무 좋으셔서 더 좋은거같아요",
          "speaker_id": "2",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000469.1.24",
          "form": "그러니까요!",
          "original_form": "그러니까요!",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000469.1.25",
          "form": "바쁜데도 여자 배구를 알리려고 예능에 나왔다는 이야기도 들었는데",
          "original_form": "바쁜데도 여자 배구를 알리려고 예능에 나왔다는 이야기도 들었는데",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000469.1.26",
          "form": "정말 대단한 것 같아요",
          "original_form": "정말 대단한 것 같아요",
          "speaker_id": "1",
          "time": "20210819 16:26"
        }
      ]
    }
  ]
