      "utterance": [
        {
          "id": "MDRW2100024285.1.1",
          "form": "허니스파클링",
          "original_form": "허니스파클링",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024285.1.2",
          "form": "잘 드세용?",
          "original_form": "잘 드세용?",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024285.1.3",
          "form": "아왤케 붙여서보냈지",
          "original_form": "아왤케 붙여서보냈지",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024285.1.4",
          "form": "오잉? 괜찮아요ㅎㅎ",
          "original_form": "오잉? 괜찮아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024285.1.5",
          "form": "ㅋㅋㅋㅋㅋ허니스파클링이 뭘까요...... 탄산음료인가...",
          "original_form": "ㅋㅋㅋㅋㅋ허니스파클링이 뭘까요...... 탄산음료인가...",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024285.1.6",
          "form": "그 교촌시킬때",
          "original_form": "그 교촌시킬때",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024285.1.7",
          "form": "콜라대신 주는 맛없는 음료수용!",
          "original_form": "콜라대신 주는 맛없는 음료수용!",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024285.1.8",
          "form": "헐 대박 지금 찾아보고 왔어요! 이런걸 줬나요? 전 왜 한번도 안받아본것 같죠",
          "original_form": "헐 대박 지금 찾아보고 왔어요! 이런걸 줬나요? 전 왜 한번도 안받아본것 같죠",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024285.1.9",
          "form": "신기하네욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛없어요?",
          "original_form": "신기하네욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛없어요?",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024285.1.10",
          "form": "헐 그 지점 뭘까용?ㅋㅋㅋ",
          "original_form": "헐 그 지점 뭘까용?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024285.1.11",
          "form": "네네ㅠㅠ",
          "original_form": "네네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024285.1.12",
          "form": "진짜 맛없어요....",
          "original_form": "진짜 맛없어요....",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024285.1.13",
          "form": "돈 아끼려고 이거 만든것같은데",
          "original_form": "돈 아끼려고 이거 만든것같은데",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024285.1.14",
          "form": "치킨 시킬때마다 버려요ㅠㅠ",
          "original_form": "치킨 시킬때마다 버려요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024285.1.15",
          "form": "헐... 왜만들었지....",
          "original_form": "헐... 왜만들었지....",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024285.1.16",
          "form": "고객의 니즈도 모르는",
          "original_form": "고객의 니즈도 모르는",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024285.1.17",
          "form": "바보교촌..",
          "original_form": "바보교촌..",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024285.1.18",
          "form": "이번에 하도 비판을 받아서 그런지",
          "original_form": "이번에 하도 비판을 받아서 그런지",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024285.1.19",
          "form": "다른 음료로 바꿨다고 하던데",
          "original_form": "다른 음료로 바꿨다고 하던데",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024285.1.20",
          "form": "최근에 시켜먹질 못했어요ㅠㅜ",
          "original_form": "최근에 시켜먹질 못했어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024285.1.21",
          "form": "아 바껴서 제가 못받았나봐요!!",
          "original_form": "아 바껴서 제가 못받았나봐요!!",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024285.1.22",
          "form": "어제 못받았더든요",
          "original_form": "어제 못받았더든요",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024285.1.23",
          "form": "아 밤인데",
          "original_form": "아 밤인데",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024285.1.24",
          "form": "탄산음료 얘기하니까 시원하고 달콤한 음료수 먹고싶어요",
          "original_form": "탄산음료 얘기하니까 시원하고 달콤한 음료수 먹고싶어요",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024285.1.25",
          "form": "지금 제일 땡기는건 레모네이드..",
          "original_form": "지금 제일 땡기는건 레모네이드..",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024285.1.26",
          "form": "아 그래서 못받으신거구나..! 이제야 이해가 되네요ㅋㅋㅋㅋ",
          "original_form": "아 그래서 못받으신거구나..! 이제야 이해가 되네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024285.1.27",
          "form": "신거 잘드세용??",
          "original_form": "신거 잘드세용??",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024285.1.28",
          "form": "엄청 잘 먹는 편은 아닌데 가끔 새콤달콤한게 땡겨요! 여름에 특히 그래요ㅎㅎㅎㅎ",
          "original_form": "엄청 잘 먹는 편은 아닌데 가끔 새콤달콤한게 땡겨요! 여름에 특히 그래요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100024285.1.29",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100024285.1.30",
          "form": "저는 신걸 잘 못먹는편인데",
          "original_form": "저는 신걸 잘 못먹는편인데",
          "speaker_id": "1",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100024285.1.31",
          "form": "신걸 잘 먹을수록",
          "original_form": "신걸 잘 먹을수록",
          "speaker_id": "1",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100024285.1.32",
          "form": "신체나이가 젊다고 하더라구요!",
          "original_form": "신체나이가 젊다고 하더라구요!",
          "speaker_id": "1",
          "time": "20210831 00:15"
        }
      ]
