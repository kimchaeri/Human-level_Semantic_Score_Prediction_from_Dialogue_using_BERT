      "utterance": [
        {
          "id": "MDRW2100006613.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100006613.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210908 03:24"
        },
        {
          "id": "MDRW2100006613.1.3",
          "form": "유투브나 주로보는 티비프로 있으신가요?",
          "original_form": "유투브나 주로보는 티비프로 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100006613.1.4",
          "form": "저는 요즘 식스센스봐요!",
          "original_form": "저는 요즘 식스센스봐요!",
          "speaker_id": "2",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100006613.1.5",
          "form": "아 식스센스 최근에 봤어요 저도",
          "original_form": "아 식스센스 최근에 봤어요 저도",
          "speaker_id": "1",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100006613.1.6",
          "form": "전부는 못봤는데 가장 재밌었던 회차 있으신가요",
          "original_form": "전부는 못봤는데 가장 재밌었던 회차 있으신가요",
          "speaker_id": "1",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100006613.1.7",
          "form": "저는 투피엠준호 나온거요!",
          "original_form": "저는 투피엠준호 나온거요!",
          "speaker_id": "2",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100006613.1.8",
          "form": "식시센스분들 좋은데 거기에 또 좋은 준호까지 나와서 ㅋㅋ",
          "original_form": "식시센스분들 좋은데 거기에 또 좋은 준호까지 나와서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100006613.1.9",
          "form": "투피엠 준호 오랜만이네요 ㅋㅋㅋ",
          "original_form": "투피엠 준호 오랜만이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006613.1.10",
          "form": "그 회차는 어떤 가짜들 내용이었나요?",
          "original_form": "그 회차는 어떤 가짜들 내용이었나요?",
          "speaker_id": "1",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006613.1.11",
          "form": "어.....................준호가 춤춘거밖에 기억이안나욬ㅋㅋㅋㅋㅋ",
          "original_form": "어.....................준호가 춤춘거밖에 기억이안나욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006613.1.12",
          "form": "ㅋㅋㅋㅋ 식스센스가 재밌어서 본게 아니셨군요",
          "original_form": "ㅋㅋㅋㅋ 식스센스가 재밌어서 본게 아니셨군요",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100006613.1.13",
          "form": "그건아닌데여....그편이기억이안나는거에요....ㅋㅋㅋㅋ",
          "original_form": "그건아닌데여....그편이기억이안나는거에요....ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006613.1.14",
          "form": "제가 유튭에서 우리집준호보고 너무 기뻐가지고",
          "original_form": "제가 유튭에서 우리집준호보고 너무 기뻐가지고",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006613.1.15",
          "form": "투피엠 팬이신가봐요",
          "original_form": "투피엠 팬이신가봐요",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100006613.1.16",
          "form": "우리집으로가자? 그 노래는 들어본거같아요",
          "original_form": "우리집으로가자? 그 노래는 들어본거같아요",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100006613.1.17",
          "form": "아니원래 안좋아했는데 그 유튭영상보고 좋아졌어요ㅋㅋ",
          "original_form": "아니원래 안좋아했는데 그 유튭영상보고 좋아졌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:36"
        },
        {
          "id": "MDRW2100006613.1.18",
          "form": "ㅋㅋㅋ 특이케이스네요",
          "original_form": "ㅋㅋㅋ 특이케이스네요",
          "speaker_id": "1",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100006613.1.19",
          "form": "안좋아했는데 갑자기 좋아한경우는ㅋㅋ",
          "original_form": "안좋아했는데 갑자기 좋아한경우는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:37"
        },
