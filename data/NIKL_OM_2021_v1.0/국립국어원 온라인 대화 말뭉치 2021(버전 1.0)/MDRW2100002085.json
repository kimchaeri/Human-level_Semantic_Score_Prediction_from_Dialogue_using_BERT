      "utterance": [
        {
          "id": "MDRW2100002085.1.1",
          "form": "유투브 많이보세요?",
          "original_form": "유투브 많이보세요?",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002085.1.2",
          "form": "카페브이로그 보는 편이에요",
          "original_form": "카페브이로그 보는 편이에요",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002085.1.3",
          "form": "어떤 컨텐츠 주로 보세요?",
          "original_form": "어떤 컨텐츠 주로 보세요?",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002085.1.4",
          "form": "저는 먹는거 좋아해서",
          "original_form": "저는 먹는거 좋아해서",
          "speaker_id": "1",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002085.1.5",
          "form": "먹방 술방 잘봐요 ㅎㅎ",
          "original_form": "먹방 술방 잘봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002085.1.6",
          "form": "name2님은 ?",
          "original_form": "&name2&님은 ?",
          "speaker_id": "1",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002085.1.7",
          "form": "그냥 얼음에 물 넣고 시럽넣고 이런게 마음에 안정이 와서 브이로그 멍하게 보는 편이에요. 가끔 예능클립보구요",
          "original_form": "그냥 얼음에 물 넣고 시럽넣고 이런게 마음에 안정이 와서 브이로그 멍하게 보는 편이에요. 가끔 예능클립보구요",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002085.1.8",
          "form": "먹방은 누구보세요?",
          "original_form": "먹방은 누구보세요?",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002085.1.9",
          "form": "그냥 일반인들요 섬마을훈태도 잼나요 ㅎㅎ",
          "original_form": "그냥 일반인들요 섬마을훈태도 잼나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002085.1.10",
          "form": "가성비 좋은곳 찾아가고",
          "original_form": "가성비 좋은곳 찾아가고",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002085.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002085.1.12",
          "form": "오 섬마을이라니 재밌을거같아요",
          "original_form": "오 섬마을이라니 재밌을거같아요",
          "speaker_id": "2",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002085.1.13",
          "form": "한 번 봐봐야겠어요 뭔가 힐링재질",
          "original_form": "한 번 봐봐야겠어요 뭔가 힐링재질",
          "speaker_id": "2",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002085.1.14",
          "form": "아녜요 ㅠㅋㅋㅋㅋ 거의 술방이라",
          "original_form": "아녜요 ㅠㅋㅋㅋㅋ 거의 술방이라",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002085.1.15",
          "form": "힐링느낌은 아니구요 ㅋㅋㅋㅋ",
          "original_form": "힐링느낌은 아니구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002085.1.16",
          "form": "채널명만 보고 힐링템일줄ㅎㅎ",
          "original_form": "채널명만 보고 힐링템일줄ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002085.1.17",
          "form": "술방의 매력은 뭔가요",
          "original_form": "술방의 매력은 뭔가요",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002085.1.18",
          "form": "술을 좋아하는데 매번 마실수없어서",
          "original_form": "술을 좋아하는데 매번 마실수없어서",
          "speaker_id": "1",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002085.1.19",
          "form": "대리만족이에여 전 ㅠㅋㅋ",
          "original_form": "대리만족이에여 전 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002085.1.20",
          "form": "좋아하는 연옌 있어여?",
          "original_form": "좋아하는 연옌 있어여?",
          "speaker_id": "1",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002085.1.21",
          "form": "가수요 배우요?",
          "original_form": "가수요 배우요?",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002085.1.22",
          "form": "가수요 !",
          "original_form": "가수요 !",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002085.1.23",
          "form": "가수는 태연 좋아해요",
          "original_form": "가수는 태연 좋아해요",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002085.1.24",
          "form": "심심해님은요?",
          "original_form": "심심해님은요?",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002085.1.25",
          "form": "오 저도 태연좋아해요 ~ 요즘엔 헤이즈도 좋더라구요",
          "original_form": "오 저도 태연좋아해요 ~ 요즘엔 헤이즈도 좋더라구요",
          "speaker_id": "1",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002085.1.26",
          "form": "저두요 특색있어서 좋아요",
          "original_form": "저두요 특색있어서 좋아요",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002085.1.27",
          "form": "헤이즈는 헤이즈가 장르니까요",
          "original_form": "헤이즈는 헤이즈가 장르니까요",
          "speaker_id": "2",
          "time": "20210819 23:37"
        }
      ]
