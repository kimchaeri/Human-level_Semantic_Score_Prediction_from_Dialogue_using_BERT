      "utterance": [
        {
          "id": "MDRW2100015692.1.1",
          "form": "자주 시켜먹는 배달 음식 있을까요?",
          "original_form": "자주 시켜먹는 배달 음식 있을까요?",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015692.1.2",
          "form": "저는 떡볶이요 name1님은요?",
          "original_form": "저는 떡볶이요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015692.1.3",
          "form": "저도 떡볶이요 찌찌뽕이네염",
          "original_form": "저도 떡볶이요 찌찌뽕이네염",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015692.1.4",
          "form": "로제 떡볶이 드셔보셨나요?",
          "original_form": "로제 떡볶이 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015692.1.5",
          "form": "네 저 로제떡볶이 완전 좋아해요",
          "original_form": "네 저 로제떡볶이 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015692.1.6",
          "form": "당면 추가하시나여",
          "original_form": "당면 추가하시나여",
          "speaker_id": "1",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015692.1.7",
          "form": "돈있으면 추가하는데 못할 때도 많아요",
          "original_form": "돈있으면 추가하는데 못할 때도 많아요",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015692.1.8",
          "form": "어떤 브랜드꺼 제일 좋아하시나요?",
          "original_form": "어떤 브랜드꺼 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015692.1.9",
          "form": "저는 일떡이었나? 사실 여러 곳을 도전해보는 타입이라",
          "original_form": "저는 일떡이었나? 사실 여러 곳을 도전해보는 타입이라",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015692.1.10",
          "form": "딱! 정해져 있진 않은 것같아요",
          "original_form": "딱! 정해져 있진 않은 것같아요",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015692.1.11",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015692.1.12",
          "form": "저는 아직까진 로제는 걸작이요",
          "original_form": "저는 아직까진 로제는 걸작이요",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015692.1.13",
          "form": "브랜드 이름이 일떡인가요?",
          "original_form": "브랜드 이름이 일떡인가요?",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015692.1.14",
          "form": "네넹! 방금 기억이 잘 안나서 찾아봤는데",
          "original_form": "네넹! 방금 기억이 잘 안나서 찾아봤는데",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015692.1.15",
          "form": "일떡 1일 1떡이었어요",
          "original_form": "일떡 1일 1떡이었어요",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015692.1.16",
          "form": "먹어보고 싶은데 제 주변엔 없네요ㅠㅠ 가기 로제가 맛있는거죠?",
          "original_form": "먹어보고 싶은데 제 주변엔 없네요ㅠㅠ 가기 로제가 맛있는거죠?",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015692.1.17",
          "form": "헉 로제는 모르겠어요 근데 너무 달지도 않고 물리지 않아서 좋아요",
          "original_form": "헉 로제는 모르겠어요 근데 너무 달지도 않고 물리지 않아서 좋아요",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015692.1.18",
          "form": "로제도 맛있지 않을까요? 있는지는 확인해봐야 알것같아요ㅋㅋ",
          "original_form": "로제도 맛있지 않을까요? 있는지는 확인해봐야 알것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015692.1.19",
          "form": "아 그냥 떡볶이 추천하신건가요?",
          "original_form": "아 그냥 떡볶이 추천하신건가요?",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015692.1.20",
          "form": "넷ㅠ 근데 방금 보니까 거기 로제도 파네요! 로제 맛있다는 리뷰 많아요",
          "original_form": "넷ㅠ 근데 방금 보니까 거기 로제도 파네요! 로제 맛있다는 리뷰 많아요",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015692.1.21",
          "form": "오 그렇군요 먹을 일 생기면 꼭 먹어볼께요",
          "original_form": "오 그렇군요 먹을 일 생기면 꼭 먹어볼께요",
          "speaker_id": "2",
