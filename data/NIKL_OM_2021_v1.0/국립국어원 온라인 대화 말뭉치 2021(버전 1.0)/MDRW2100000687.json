      "utterance": [
        {
          "id": "MDRW2100000687.1.1",
          "form": "애기 병원갔다가 엄청 울었어요ㅠㅠ",
          "original_form": "애기 병원갔다가 엄청 울었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000687.1.2",
          "form": "설사는 어때요?",
          "original_form": "설사는 어때요?",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100000687.1.3",
          "form": "약 바꾸고 지금 처음먹은거라 아침에 봐야되요",
          "original_form": "약 바꾸고 지금 처음먹은거라 아침에 봐야되요",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100000687.1.4",
          "form": "애기 빨리 낫기를 바랍니다",
          "original_form": "애기 빨리 낫기를 바랍니다",
          "speaker_id": "1",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100000687.1.5",
          "form": "name3이는 요새 잘 지내고 있어요?",
          "original_form": "&name3&이는 요새 잘 지내고 있어요?",
          "speaker_id": "1",
          "time": "20210820 07:02"
        },
        {
          "id": "MDRW2100000687.1.6",
          "form": "힘들어해요ㅠㅠ 센터장님도 줏대없고 끌려다는 타입이고",
          "original_form": "힘들어해요ㅠㅠ 센터장님도 줏대없고 끌려다는 타입이고",
          "speaker_id": "2",
          "time": "20210820 08:02"
        },
        {
          "id": "MDRW2100000687.1.7",
          "form": "내년에 가을에 신축생기면 옮긴데요",
          "original_form": "내년에 가을에 신축생기면 옮긴데요",
          "speaker_id": "2",
          "time": "20210820 08:02"
        },
        {
          "id": "MDRW2100000687.1.8",
          "form": "얼굴에 살이 쏘옥 빠졌어요 퀭하고",
          "original_form": "얼굴에 살이 쏘옥 빠졌어요 퀭하고",
          "speaker_id": "2",
          "time": "20210820 08:04"
        },
        {
          "id": "MDRW2100000687.1.9",
          "form": "지금도 신축건물로 옮긴거잖아요",
          "original_form": "지금도 신축건물로 옮긴거잖아요",
          "speaker_id": "1",
          "time": "20210820 08:44"
        },
        {
          "id": "MDRW2100000687.1.10",
          "form": "건물을 또 지어요?",
          "original_form": "건물을 또 지어요?",
          "speaker_id": "1",
          "time": "20210820 08:44"
        },
        {
          "id": "MDRW2100000687.1.11",
          "form": "이건 지어져있던 개인건물을 군에서 매입한거예요",
          "original_form": "이건 지어져있던 개인건물을 군에서 매입한거예요",
          "speaker_id": "2",
          "time": "20210820 08:49"
        },
        {
          "id": "MDRW2100000687.1.12",
          "form": "치매관련해서 건물큰거 또 지어요",
          "original_form": "치매관련해서 건물큰거 또 지어요",
          "speaker_id": "2",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100000687.1.13",
          "form": "승진해요 그러면?",
          "original_form": "승진해요 그러면?",
          "speaker_id": "1",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100000687.1.14",
          "form": "지금 건강가정다문화 국장이잖아요?\r\n다른분야(치매) 이직하려고 하는건데 직급은 같아요 국장!",
          "original_form": "지금 건강가정다문화 국장이잖아요?\r\n다른분야(치매) 이직하려고 하는건데 직급은 같아요 국장!",
          "speaker_id": "2",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100000687.1.15",
          "form": "근데 거기가 훨씬 커요",
          "original_form": "근데 거기가 훨씬 커요",
          "speaker_id": "2",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100000687.1.16",
          "form": "아 아무래도 규모가 큰쪽이 좋아요",
          "original_form": "아 아무래도 규모가 큰쪽이 좋아요",
          "speaker_id": "1",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100000687.1.17",
          "form": "밥은 먹었어요?",
          "original_form": "밥은 먹었어요?",
          "speaker_id": "1",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100000687.1.18",
          "form": "이제 먹어요 저희는 11시50분부터 점심시간이예요",
          "original_form": "이제 먹어요 저희는 11시50분부터 점심시간이예요",
          "speaker_id": "2",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100000687.1.19",
          "form": "오늘 제육쌈밥! 사먹어요",
          "original_form": "오늘 제육쌈밥! 사먹어요",
          "speaker_id": "2",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100000687.1.20",
          "form": "오 좋겠다",
          "original_form": "오 좋겠다",
          "speaker_id": "1",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100000687.1.21",
          "form": "우리는 요즘 나가서 못먹어요",
          "original_form": "우리는 요즘 나가서 못먹어요",
          "speaker_id": "1",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100000687.1.22",
          "form": "회사 식당가든가",
          "original_form": "회사 식당가든가",
          "speaker_id": "1",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100000687.1.23",
          "form": "도시락시켜 먹어요",
          "original_form": "도시락시켜 먹어요",
          "speaker_id": "1",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100000687.1.24",
          "form": "회사식당 없어서 사먹어요ㅋㅋ",
          "original_form": "회사식당 없어서 사먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100000687.1.25",
          "form": "우리는 확진자가 거의 없어서",
          "original_form": "우리는 확진자가 거의 없어서",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100000687.1.26",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
