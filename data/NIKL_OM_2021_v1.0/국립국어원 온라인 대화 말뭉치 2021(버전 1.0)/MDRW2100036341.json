      "utterance": [
        {
          "id": "MDRW2100036341.1.1",
          "form": "전 맥주파에요 ㅋㅋ",
          "original_form": "전 맥주파에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036341.1.2",
          "form": "맥주 어디꺼 드세용?",
          "original_form": "맥주 어디꺼 드세용?",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036341.1.3",
          "form": "전 클라우드 드래프트 나오곤 ㅋㅋㅋ",
          "original_form": "전 클라우드 드래프트 나오곤 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036341.1.4",
          "form": "그것만 한참마셧네요 ㅋㅋ원래 테라먹다가",
          "original_form": "그것만 한참마셧네요 ㅋㅋ원래 테라먹다가",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036341.1.5",
          "form": "헐 저랑 똑같으시네요...",
          "original_form": "헐 저랑 똑같으시네요...",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036341.1.6",
          "form": "그냥 클라우드 말고 꼭 드래프트!",
          "original_form": "그냥 클라우드 말고 꼭 드래프트!",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036341.1.7",
          "form": "ㅋㅋㅋ맞아요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036341.1.8",
          "form": "파랑파랑 ㅋㅋ",
          "original_form": "파랑파랑 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036341.1.9",
          "form": "이거 안파는 편의점이 의외로 좀 있어서",
          "original_form": "이거 안파는 편의점이 의외로 좀 있어서",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036341.1.10",
          "form": "사려면 대형마트 가야해서 귀찮아요ㅋㅋㅋ",
          "original_form": "사려면 대형마트 가야해서 귀찮아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036341.1.11",
          "form": "잘가던 맥주집 사장님한테 ㅋㅋㅋ",
          "original_form": "잘가던 맥주집 사장님한테 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036341.1.12",
          "form": "병맥출시됐을때 말해서 ㅋㅋ사장님이 발주해주셨는데 ㅋㅋㅋ",
          "original_form": "병맥출시됐을때 말해서 ㅋㅋ사장님이 발주해주셨는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036341.1.13",
          "form": "아무도 안찾는다고 ㅋㅋ 제가 가서 열심히 먹었죠",
          "original_form": "아무도 안찾는다고 ㅋㅋ 제가 가서 열심히 먹었죠",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036341.1.14",
          "form": "와 사람들이 맛을 뭘 모르네요",
          "original_form": "와 사람들이 맛을 뭘 모르네요",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036341.1.15",
          "form": "코로나심해지고 못간지도 오래네요ㅠㅠ",
          "original_form": "코로나심해지고 못간지도 오래네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036341.1.16",
          "form": "이 맛있는걸",
          "original_form": "이 맛있는걸",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036341.1.17",
          "form": "아 저두요ㅠㅠ 집에서 주로 혼술합니당",
          "original_form": "아 저두요ㅠㅠ 집에서 주로 혼술합니당",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036341.1.18",
          "form": "저도 혼맥 많이햇죠ㅠㅠ 특히 한여룸엔",
          "original_form": "저도 혼맥 많이햇죠ㅠㅠ 특히 한여룸엔",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036341.1.19",
          "form": "진짜 매일 맥주캔딴거같아요ㅠㅠ",
          "original_form": "진짜 매일 맥주캔딴거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036341.1.20",
          "form": "샤워하기전에 맥주 냉동실에 옮겨놓고",
          "original_form": "샤워하기전에 맥주 냉동실에 옮겨놓고",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036341.1.21",
          "form": "천국이에용ㅋㅋㅋ",
          "original_form": "천국이에용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036341.1.22",
          "form": "샤워하고나서 바로",
          "original_form": "샤워하고나서 바로",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036341.1.23",
          "form": "먹으면ㅋㅋㅋ",
          "original_form": "먹으면ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036341.1.24",
          "form": "와 ㅋㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036341.1.25",
          "form": "여기 사진을 못올려서 ㅋㅋㅋ아쉽네요",
          "original_form": "여기 사진을 못올려서 ㅋㅋㅋ아쉽네요",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036341.1.26",
          "form": "잔까지 같이넣어서 진짜",
          "original_form": "잔까지 같이넣어서 진짜",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036341.1.27",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036341.1.28",
          "form": "와 박수 짝짝짝",
          "original_form": "와 박수 짝짝짝",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036341.1.29",
          "form": "할맥같이 얼음잔",
          "original_form": "할맥같이 얼음잔",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036341.1.30",
          "form": "같이 따라마셧는디",
          "original_form": "같이 따라마셧는디",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036341.1.31",
          "form": "크으... 아 치맥 엄청 땡기네요ㅋㅋㅋㅋㅋ",
          "original_form": "크으... 아 치맥 엄청 땡기네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036341.1.32",
          "form": "맥주가 살이 너무 쪄서 자제해야하지만...ㅠㅠ",
          "original_form": "맥주가 살이 너무 쪄서 자제해야하지만...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:44"
        }
      ]
