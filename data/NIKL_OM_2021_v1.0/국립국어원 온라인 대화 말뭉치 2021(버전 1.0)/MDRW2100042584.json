      "utterance": [
        {
          "id": "MDRW2100042584.1.1",
          "form": "나 내  이사",
          "original_form": "나 내  이사",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042584.1.2",
          "form": "엄청 힘들겠다",
          "original_form": "엄청 힘들겠다",
          "speaker_id": "1",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042584.1.3",
          "form": "그래서 친구 두명이 도와준다더라",
          "original_form": "그래서 친구 두명이 도와준다더라",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042584.1.4",
          "form": "일단 이사 처음에 가면 청소하는 데 시간이 오래 걸릴 거야",
          "original_form": "일단 이사 처음에 가면 청소하는 데 시간이 오래 걸릴 거야",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042584.1.5",
          "form": "나도 그게 걱정이야",
          "original_form": "나도 그게 걱정이야",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042584.1.6",
          "form": "오래된 집이라 해지",
          "original_form": "오래된 집이라 해지",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042584.1.7",
          "form": "맞아 그래서 청소 가진 집안일의 시간이 무척 오래 걸릴 것 같아",
          "original_form": "맞아 그래서 청소 가진 집안일의 시간이 무척 오래 걸릴 것 같아",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042584.1.8",
          "form": "걱정이 많이 되네",
          "original_form": "걱정이 많이 되네",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042584.1.9",
          "form": "그래서 청소 팁 같은 거 있으면 드러 놀려고",
          "original_form": "그래서 청소 팁 같은 거 있으면 드러 놀려고",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100042584.1.10",
          "form": "내가 알려줄 수 있는 건 화장실 청소 방법이야",
          "original_form": "내가 알려줄 수 있는 건 화장실 청소 방법이야",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042584.1.11",
          "form": "꼭 알려줘봐",
          "original_form": "꼭 알려줘봐",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042584.1.12",
          "form": "일단 전체에 락스를 뿌려",
          "original_form": "일단 전체에 락스를 뿌려",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042584.1.13",
          "form": "그런 다음에",
          "original_form": "그런 다음에",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042584.1.14",
          "form": "솔로 전체를 문질러",
          "original_form": "솔로 전체를 문질러",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100042584.1.15",
          "form": "변기 안에도 깨끗하게 해야겠다",
          "original_form": "변기 안에도 깨끗하게 해야겠다",
          "speaker_id": "2",
          "time": "20210906 23:12"
        }
      ]
