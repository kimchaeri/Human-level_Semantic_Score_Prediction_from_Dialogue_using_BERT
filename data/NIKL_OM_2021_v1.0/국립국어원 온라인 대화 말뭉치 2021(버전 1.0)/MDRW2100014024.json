      "utterance": [
        {
          "id": "MDRW2100014024.1.1",
          "form": "혹시 다이어트 해보셨나요 ... ?",
          "original_form": "혹시 다이어트 해보셨나요 ... ?",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014024.1.2",
          "form": "운동하는거 있으세요?",
          "original_form": "운동하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014024.1.3",
          "form": "저는 헬스해요 !",
          "original_form": "저는 헬스해요 !",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100014024.1.4",
          "form": "예전에 피티를 했어서 계속 다니고 있어요 ㅎㅎ",
          "original_form": "예전에 피티를 했어서 계속 다니고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100014024.1.5",
          "form": "오 저도 헬스다녔는데ㅠㅜ올해 다니기 너무 어렵지않나요ㅠ",
          "original_form": "오 저도 헬스다녔는데ㅠㅜ올해 다니기 너무 어렵지않나요ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014024.1.6",
          "form": "아 맞아요 ㅠㅠ 진짜 마스크 끼고 하는거 너무 숨막히고 ...",
          "original_form": "아 맞아요 ㅠㅠ 진짜 마스크 끼고 하는거 너무 숨막히고 ...",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014024.1.7",
          "form": "런닝머신 속도도 제한되서 더 싫어요 ㅠㅠ",
          "original_form": "런닝머신 속도도 제한되서 더 싫어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014024.1.8",
          "form": "맞아요ㅠㅜ여기는 샤워도 못해서ㅋㅋ운동하고 집가는거 너무 ...",
          "original_form": "맞아요ㅠㅜ여기는 샤워도 못해서ㅋㅋ운동하고 집가는거 너무 ...",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014024.1.9",
          "form": "아 맞아욬ㅋㅋ 와 샤워 못하는거 진짜 끔찍해요",
          "original_form": "아 맞아욬ㅋㅋ 와 샤워 못하는거 진짜 끔찍해요",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014024.1.10",
          "form": "심지어 여름이니까 와 ..",
          "original_form": "심지어 여름이니까 와 ..",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014024.1.11",
          "form": "땀쩔어서 집까지가면 막...아휴ㅠㅠㅠ그니까요",
          "original_form": "땀쩔어서 집까지가면 막...아휴ㅠㅠㅠ그니까요",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014024.1.12",
          "form": "근데 또 안할수는 없고 ㅠㅠ",
          "original_form": "근데 또 안할수는 없고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014024.1.13",
          "form": "저는 약간 운동도 하다가 한번 안해버리면",
          "original_form": "저는 약간 운동도 하다가 한번 안해버리면",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014024.1.14",
          "form": "그뒤로 쭉~ 안해욬ㅋㅋ",
          "original_form": "그뒤로 쭉~ 안해욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014024.1.15",
          "form": "저도 약간그래요ㅎㅎ 한번 빠지면 다망한 기분ㅜㅜㅋㅋ그래서 매일 가려고 하는데ㅎㅎ",
          "original_form": "저도 약간그래요ㅎㅎ 한번 빠지면 다망한 기분ㅜㅜㅋㅋ그래서 매일 가려고 하는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014024.1.16",
          "form": "맞아요 진짜 가기 싫은 날에도 저는 일단 그냥 가기는 가요",
          "original_form": "맞아요 진짜 가기 싫은 날에도 저는 일단 그냥 가기는 가요",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014024.1.17",
          "form": "오 맞아요 그래야 습관든다 하더라구요",
          "original_form": "오 맞아요 그래야 습관든다 하더라구요",
