      "utterance": [
        {
          "id": "MDRW2100021489.1.1",
          "form": "기존 신발들이 뒤꿈치가 헐떡일 정도면",
          "original_form": "기존 신발들이 뒤꿈치가 헐떡일 정도면",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021489.1.2",
          "form": "반 사이즈 다운하는 게 나을 것 같긴 한데",
          "original_form": "반 사이즈 다운하는 게 나을 것 같긴 한데",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021489.1.3",
          "form": "그 네가 말한 모델이",
          "original_form": "그 네가 말한 모델이",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021489.1.4",
          "form": "작게 나온건지도 확인해봐봐",
          "original_form": "작게 나온건지도 확인해봐봐",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021489.1.5",
          "form": "신발 모델마다 또 제멋대로 나오더라고 ㅋㅋㅋ",
          "original_form": "신발 모델마다 또 제멋대로 나오더라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.6",
          "form": "그래볼까 찾아봤는데 발볼이 좀 있는 사람은",
          "original_form": "그래볼까 찾아봤는데 발볼이 좀 있는 사람은",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.7",
          "form": "반치수 업하라는데",
          "original_form": "반치수 업하라는데",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.8",
          "form": "그럼 원래사이즈거든",
          "original_form": "그럼 원래사이즈거든",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.9",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.11",
          "form": "청기백기냐고",
          "original_form": "청기백기냐고",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.12",
          "form": "반사이즈 올려 아니 올리지말고 내려",
          "original_form": "반사이즈 올려 아니 올리지말고 내려",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.13",
          "form": "흐으으으으음",
          "original_form": "흐으으으으음",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.14",
          "form": "그래서 열라 고민되는데",
          "original_form": "그래서 열라 고민되는데",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100021489.1.15",
          "form": "내가 들어도 고민되네",
          "original_form": "내가 들어도 고민되네",
          "speaker_id": "2",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.16",
          "form": "중고로 사는거라",
          "original_form": "중고로 사는거라",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.17",
          "form": "더그럼",
          "original_form": "더그럼",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.18",
          "form": "그리고 캔버스화노",
          "original_form": "그리고 캔버스화노",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.19",
          "form": "화도 보고있는데",
          "original_form": "화도 보고있는데",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.20",
          "form": "맞아 매장에서 사면 반품되는데 중고는 리스크가 있지..",
          "original_form": "맞아 매장에서 사면 반품되는데 중고는 리스크가 있지..",
          "speaker_id": "2",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.21",
          "form": "스캐치볼이라는 브랜드가",
          "original_form": "스캐치볼이라는 브랜드가",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.22",
          "form": "무료 교환에 무료환불 해준대서",
          "original_form": "무료 교환에 무료환불 해준대서",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.23",
          "form": "사이즈 고민은 없거튼",
          "original_form": "사이즈 고민은 없거튼",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.24",
          "form": "그리고 캔버스화인데도",
          "original_form": "그리고 캔버스화인데도",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021489.1.25",
          "form": "밑깔창이 기능성이더라고",
          "original_form": "밑깔창이 기능성이더라고",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021489.1.26",
          "form": "근데 단점은 가격이 비싸",
          "original_form": "근데 단점은 가격이 비싸",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021489.1.27",
          "form": "스캐치볼? 처음 들어보는데",
          "original_form": "스캐치볼? 처음 들어보는데",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021489.1.28",
          "form": "신규 브랜드인가",
          "original_form": "신규 브랜드인가",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021489.1.29",
          "form": "응 외국 브랜드라 모를만해",
          "original_form": "응 외국 브랜드라 모를만해",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100021489.1.30",
          "form": "사람들 평도 괜찮ㄱᆢ",
          "original_form": "사람들 평도 괜찮ㄱᆢ",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021489.1.31",
          "form": "디자인도 심플하고 색도 이쁘더라",
          "original_form": "디자인도 심플하고 색도 이쁘더라",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021489.1.32",
          "form": "캐치볼 여기니?",
          "original_form": "캐치볼 여기니?",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021489.1.33",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021489.1.34",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021489.1.35",
          "form": "나자꾸",
          "original_form": "나자꾸",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021489.1.36",
