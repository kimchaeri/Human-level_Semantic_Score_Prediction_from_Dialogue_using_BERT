        {
          "id": "MDRW2100041935.1.2",
          "form": "네..챙겨먹어요",
          "original_form": "네..챙겨먹어요",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100041935.1.3",
          "form": "아 어떤거 드세요?",
          "original_form": "아 어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100041935.1.4",
          "form": "비타민.오메가.칼슘요",
          "original_form": "비타민.오메가.칼슘요",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041935.1.5",
          "form": "드시는거있으세요?",
          "original_form": "드시는거있으세요?",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041935.1.6",
          "form": "저는 유산균이랑 비타민씨요",
          "original_form": "저는 유산균이랑 비타민씨요",
          "speaker_id": "2",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100041935.1.7",
          "form": "유산균은 확실히 좋은거같아요",
          "original_form": "유산균은 확실히 좋은거같아요",
          "speaker_id": "2",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100041935.1.8",
          "form": "저도참 유산균도",
          "original_form": "저도참 유산균도",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100041935.1.9",
          "form": "유산균 뭐드시나요",
          "original_form": "유산균 뭐드시나요",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100041935.1.10",
          "form": "종류에따라 효과도 좀 다른거같아요",
          "original_form": "종류에따라 효과도 좀 다른거같아요",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100041935.1.11",
          "form": "락토핏",
          "original_form": "락토핏",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100041935.1.12",
          "form": "코코에서 파는거  거의사먹어요",
          "original_form": "코코에서 파는거  거의사먹어요",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
