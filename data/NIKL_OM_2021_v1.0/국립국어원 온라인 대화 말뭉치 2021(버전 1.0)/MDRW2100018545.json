      "utterance": [
        {
          "id": "MDRW2100018545.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018545.1.2",
          "form": "좋아하는 스포츠 있으세요?",
          "original_form": "좋아하는 스포츠 있으세요?",
          "speaker_id": "1",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018545.1.3",
          "form": "스포츠알못이라ㅜ",
          "original_form": "스포츠알못이라ㅜ",
          "speaker_id": "2",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018545.1.4",
          "form": "뭐 좋아하시나요?",
          "original_form": "뭐 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018545.1.5",
          "form": "월드컵이나 올림픽 한정으로 좋아해요ㅋㅋㅋ",
          "original_form": "월드컵이나 올림픽 한정으로 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018545.1.6",
          "form": "아! 빙상종목은 좋아해요!! 찾아보는 편",
          "original_form": "아! 빙상종목은 좋아해요!! 찾아보는 편",
          "speaker_id": "1",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018545.1.7",
          "form": "빙상 어느 종목이요?",
          "original_form": "빙상 어느 종목이요?",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018545.1.8",
          "form": "좋아하는 선수가 있나요",
          "original_form": "좋아하는 선수가 있나요",
          "speaker_id": "2",
          "time": "20210827 08:09"
        },
        {
          "id": "MDRW2100018545.1.9",
          "form": "쇼트트랙 스피드스케이팅 다 좋아해요!",
          "original_form": "쇼트트랙 스피드스케이팅 다 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 08:10"
        },
        {
          "id": "MDRW2100018545.1.10",
          "form": "선수보다는 종목 자체를 좋아해서~",
          "original_form": "선수보다는 종목 자체를 좋아해서~",
          "speaker_id": "1",
          "time": "20210827 08:11"
        },
        {
          "id": "MDRW2100018545.1.11",
          "form": "그래도 꼽자면 심석희선수나 이승훈선수?!",
          "original_form": "그래도 꼽자면 심석희선수나 이승훈선수?!",
          "speaker_id": "1",
          "time": "20210827 08:11"
        },
        {
          "id": "MDRW2100018545.1.12",
          "form": "오오 박진감 넘치죠",
          "original_form": "오오 박진감 넘치죠",
          "speaker_id": "2",
          "time": "20210827 08:11"
        },
        {
          "id": "MDRW2100018545.1.13",
          "form": "전 피겨 좋아했어요",
          "original_form": "전 피겨 좋아했어요",
          "speaker_id": "2",
          "time": "20210827 08:11"
        },
        {
          "id": "MDRW2100018545.1.14",
          "form": "아ㅎㅎㅎㅎ연느ㅜㅜㅜㅜ",
          "original_form": "아ㅎㅎㅎㅎ연느ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100018545.1.15",
          "form": "가끔 영상 찾아보는데 괜히 혼자 막 벅차고",
          "original_form": "가끔 영상 찾아보는데 괜히 혼자 막 벅차고",
          "speaker_id": "1",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100018545.1.16",
          "form": "연느님 때문에 피겨점프를 다 외웠었죠",
          "original_form": "연느님 때문에 피겨점프를 다 외웠었죠",
          "speaker_id": "2",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100018545.1.17",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100018545.1.18",
          "form": "지금도 종종 봐요",
          "original_form": "지금도 종종 봐요",
          "speaker_id": "2",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100018545.1.19",
          "form": "혼자 애국심 차오르고 ㅋㅋㅋㅋㅋ",
          "original_form": "혼자 애국심 차오르고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100018545.1.20",
          "form": "각 나라 해설 다 찾아보고ㅎㅎㅎ",
          "original_form": "각 나라 해설 다 찾아보고ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:14"
        },
        {
          "id": "MDRW2100018545.1.21",
          "form": "연느님은 없지만 동계올림픽 기다려지네요",
          "original_form": "연느님은 없지만 동계올림픽 기다려지네요",
          "speaker_id": "2",
          "time": "20210827 08:15"
        },
        {
          "id": "MDRW2100018545.1.22",
          "form": "저는 아무리 느리게봐도 피겨 회전하는거",
          "original_form": "저는 아무리 느리게봐도 피겨 회전하는거",
          "speaker_id": "1",
          "time": "20210827 08:16"
        },
        {
          "id": "MDRW2100018545.1.23",
          "form": "모르겠던데... 2회전인지 3회전인지",
          "original_form": "모르겠던데... 2회전인지 3회전인지",
          "speaker_id": "1",
          "time": "20210827 08:16"
        },
        {
          "id": "MDRW2100018545.1.24",
          "form": "너무 휙휙돌아서ㅋㅋㅋ어떻게 다들 아시는지..",
          "original_form": "너무 휙휙돌아서ㅋㅋㅋ어떻게 다들 아시는지..",
          "speaker_id": "1",
          "time": "20210827 08:16"
        },
        {
          "id": "MDRW2100018545.1.25",
          "form": "저도 회전은 좀 헷갈려요ㅎㅎ",
          "original_form": "저도 회전은 좀 헷갈려요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:16"
        },
        {
          "id": "MDRW2100018545.1.26",
          "form": "발 엣지 보고 마지막 착지 발보고 짐작만 하죠",
          "original_form": "발 엣지 보고 마지막 착지 발보고 짐작만 하죠",
          "speaker_id": "2",
          "time": "20210827 08:17"
        },
        {
          "id": "MDRW2100018545.1.27",
          "form": "실제 직관하면 더 안보일 것 같아요",
          "original_form": "실제 직관하면 더 안보일 것 같아요",
          "speaker_id": "2",
          "time": "20210827 08:17"
        },
        {
          "id": "MDRW2100018545.1.28",
          "form": "집에서 슬로우모션 리플레이봐야",
          "original_form": "집에서 슬로우모션 리플레이봐야",
          "speaker_id": "2",
          "time": "20210827 08:17"
        },
        {
          "id": "MDRW2100018545.1.29",
          "form": "해설할때 무슨 토플 엣지 나오는데 어려워서",
          "original_form": "해설할때 무슨 토플 엣지 나오는데 어려워서",
          "speaker_id": "1",
          "time": "20210827 08:20"
        },
        {
          "id": "MDRW2100018545.1.30",
          "form": "찾아보고 그랬던 기억이..ㅎㅎ",
          "original_form": "찾아보고 그랬던 기억이..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:20"
        }
      ]
