        {
          "id": "MDRW2100031236.1.1",
          "form": "결혼하셨나요",
          "original_form": "결혼하셨나요",
          "speaker_id": "1",
          "time": "20210904 18:47"
        },
        {
          "id": "MDRW2100031236.1.2",
          "form": "네 name1님은 결혼하셨어요?",
          "original_form": "네 &name1&님은 결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210904 19:11"
        },
        {
          "id": "MDRW2100031236.1.3",
          "form": "네 결혼 몇년차이신가요",
          "original_form": "네 결혼 몇년차이신가요",
          "speaker_id": "1",
          "time": "20210905 06:28"
        },
        {
          "id": "MDRW2100031236.1.4",
          "form": "저는 작년 12월에 결혼했어요",
          "original_form": "저는 작년 12월에 결혼했어요",
          "speaker_id": "2",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100031236.1.5",
          "form": "이제 결혼 9개월 되었어요",
          "original_form": "이제 결혼 9개월 되었어요",
          "speaker_id": "2",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100031236.1.6",
          "form": "name1님은 얼마닌 되셨어요?",
          "original_form": "&name1&님은 얼마닌 되셨어요?",
          "speaker_id": "2",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031236.1.7",
          "form": "저는 8년차 입니다",
          "original_form": "저는 8년차 입니다",
          "speaker_id": "1",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100031236.1.8",
          "form": "자녀도 있으신가요?",
          "original_form": "자녀도 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100031236.1.9",
          "form": "네 아들 둘이요",
          "original_form": "네 아들 둘이요",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031236.1.10",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031236.1.11",
          "form": "저는 어제 날씨 좋아서 아내랑 경주 나들이 다녀왔는데",
          "original_form": "저는 어제 날씨 좋아서 아내랑 경주 나들이 다녀왔는데",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031236.1.12",
          "form": "name1님은 오를 외출 계획 있으세요?",
          "original_form": "&name1&님은 오를 외출 계획 있으세요?",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031236.1.13",
          "form": "없습니다 \r\n결혼 전 연애기간이 기셨나요?",
          "original_form": "없습니다 \r\n결혼 전 연애기간이 기셨나요?",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100031236.1.14",
          "form": "연애 일년정도 했어요",
          "original_form": "연애 일년정도 했어요",
          "speaker_id": "2",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100031236.1.15",
          "form": "저도 비슷해요 일년반정도요",
          "original_form": "저도 비슷해요 일년반정도요",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100031236.1.16",
          "form": "그러시구나 연애랑결혼이 크게 다른것같나요?",
          "original_form": "그러시구나 연애랑결혼이 크게 다른것같나요?",
          "speaker_id": "2",
          "time": "20210905 16:13"
        },
        {
          "id": "MDRW2100031236.1.17",
          "form": "그렇죠 다르죠~ name2님은 아이 있으신가요",
          "original_form": "그렇죠 다르죠~ &name2&님은 아이 있으신가요",
