      "utterance": [
        {
          "id": "MDRW2100012683.1.1",
          "form": "시험있어요?",
          "original_form": "시험있어요?",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012683.1.2",
          "form": "아니요 ㅜㅜ 직장인인걸요",
          "original_form": "아니요 ㅜㅜ 직장인인걸요",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012683.1.3",
          "form": "그리고 초중고는 개학 이제 막 했고 대학생은 아직 개강도 안했잖아요..",
          "original_form": "그리고 초중고는 개학 이제 막 했고 대학생은 아직 개강도 안했잖아요..",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012683.1.4",
          "form": "자격증이라두",
          "original_form": "자격증이라두",
          "speaker_id": "1",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100012683.1.5",
          "form": "ㅎㅎ 딱히 준비하고 있는 건 없어요.",
          "original_form": "ㅎㅎ 딱히 준비하고 있는 건 없어요.",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012683.1.6",
          "form": "학교 다니세요?",
          "original_form": "학교 다니세요?",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012683.1.7",
          "form": "아뇨 졸업했어요",
          "original_form": "아뇨 졸업했어요",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012683.1.8",
          "form": "학교 다닐 적에 공부 잘했어요?",
          "original_form": "학교 다닐 적에 공부 잘했어요?",
          "speaker_id": "2",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012683.1.9",
          "form": "초등 때요",
          "original_form": "초등 때요",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012683.1.10",
          "form": "아 초등학교 때만... ㅋㅋㅋ",
          "original_form": "아 초등학교 때만... ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012683.1.11",
          "form": "그럼 학업스트레스는 많으셨겠네요",
          "original_form": "그럼 학업스트레스는 많으셨겠네요",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012683.1.12",
          "form": "네 힘들없죠",
          "original_form": "네 힘들없죠",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012683.1.13",
          "form": "ㅠㅠ 학교생활은 어떠셨어요?",
          "original_form": "ㅠㅠ 학교생활은 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100012683.1.14",
          "form": "잼났죠",
          "original_form": "잼났죠",
          "speaker_id": "1",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100012683.1.15",
          "form": "친구들이 주변이 다 있어서",
          "original_form": "친구들이 주변이 다 있어서",
          "speaker_id": "1",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100012683.1.16",
          "form": "ㅋㅋㅋ다행이네요 동아리나 학생회 같은 것도 해봤어요?",
          "original_form": "ㅋㅋㅋ다행이네요 동아리나 학생회 같은 것도 해봤어요?",
          "speaker_id": "2",
          "time": "20210825 00:12"
