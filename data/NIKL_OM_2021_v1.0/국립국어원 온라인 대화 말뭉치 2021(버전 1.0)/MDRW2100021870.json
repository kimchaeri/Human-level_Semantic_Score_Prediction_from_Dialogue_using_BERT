      "utterance": [
        {
          "id": "MDRW2100021870.1.1",
          "form": "싸이월드하셨음까",
          "original_form": "싸이월드하셨음까",
          "speaker_id": "2",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021870.1.2",
          "form": "쪼꼼",
          "original_form": "쪼꼼",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021870.1.3",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021870.1.4",
          "form": "나는 싸이월드에 중딩사진 있을듯",
          "original_form": "나는 싸이월드에 중딩사진 있을듯",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021870.1.5",
          "form": "두렵소이다 ㄷㄷ",
          "original_form": "두렵소이다 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021870.1.6",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100021870.1.7",
          "form": "name1님도 싸이월드에 흑역사 많이 쌓아뒀쇼?",
          "original_form": "&name1&님도 싸이월드에 흑역사 많이 쌓아뒀쇼?",
          "speaker_id": "2",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021870.1.8",
          "form": "싸이월드 할 이야기.없당",
          "original_form": "싸이월드 할 이야기.없당",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021870.1.9",
          "form": "ㅋㅋㅋ  이런 나는 쪼곰있는데",
          "original_form": "ㅋㅋㅋ  이런 나는 쪼곰있는데",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021870.1.10",
          "form": "흑역사가 열리면 내사진 보여드리리다",
          "original_form": "흑역사가 열리면 내사진 보여드리리다",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021870.1.11",
          "form": "오 무슨 흑역사인데",
          "original_form": "오 무슨 흑역사인데",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021870.1.12",
          "form": "2학년1반 15번 드니 사진",
          "original_form": "2학년1반 15번 드니 사진",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021870.1.13",
          "form": "있지유 ㅋㅋㅋㅋ",
          "original_form": "있지유 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021870.1.14",
          "form": "어린데 흑역사일게 머있노",
          "original_form": "어린데 흑역사일게 머있노",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021870.1.15",
          "form": "인자 끄내보면 다 추억이디",
          "original_form": "인자 끄내보면 다 추억이디",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021870.1.16",
          "form": "그래도!",
          "original_form": "그래도!",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021870.1.17",
          "form": "누구누구 내숙제 훔쳐갔음 이런거",
          "original_form": "누구누구 내숙제 훔쳐갔음 이런거",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021870.1.18",
          "form": "난 그때 방 꾸미는거 있자나",
          "original_form": "난 그때 방 꾸미는거 있자나",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021870.1.19",
          "form": "마자 아바타꾸미기",
          "original_form": "마자 아바타꾸미기",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021870.1.20",
          "form": "그게 머 돈주고 사고 그랬어",
          "original_form": "그게 머 돈주고 사고 그랬어",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021870.1.21",
          "form": "나 색동한복입혀놨음",
          "original_form": "나 색동한복입혀놨음",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021870.1.22",
          "form": "그거 막 고민하고 그랬던듯",
          "original_form": "그거 막 고민하고 그랬던듯",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021870.1.23",
          "form": "이상한놈",
          "original_form": "이상한놈",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021870.1.24",
          "form": "name1님 흑역사 궁금하군",
          "original_form": "&name1&님 흑역사 궁금하군",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021870.1.25",
          "form": "이상하다니 무료중에 가장이쁜거 고름",
          "original_form": "이상하다니 무료중에 가장이쁜거 고름",
