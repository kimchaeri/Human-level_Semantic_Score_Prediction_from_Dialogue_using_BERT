      "utterance": [
        {
          "id": "MDRW2100017669.1.1",
          "form": "유명한 카페 가본적 있어요?",
          "original_form": "유명한 카페 가본적 있어요?",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017669.1.2",
          "form": "별로 못가봤어요",
          "original_form": "별로 못가봤어요",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017669.1.3",
          "form": "유명 카페 가본 적 없으시구나",
          "original_form": "유명 카페 가본 적 없으시구나",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017669.1.4",
          "form": "그럼 맛집은요?",
          "original_form": "그럼 맛집은요?",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017669.1.5",
          "form": "맛집도 가까운데는 잘 가보는 편이에요",
          "original_form": "맛집도 가까운데는 잘 가보는 편이에요",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017669.1.6",
          "form": "맛집 하나만 소개해줘요~",
          "original_form": "맛집 하나만 소개해줘요~",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017669.1.7",
          "form": "제가 지방 살아서ㅋㅋ 코끼리만두라고 만둣국 맛있는데 있거든요",
          "original_form": "제가 지방 살아서ㅋㅋ 코끼리만두라고 만둣국 맛있는데 있거든요",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017669.1.8",
          "form": "우와 만둣국 좋네요",
          "original_form": "우와 만둣국 좋네요",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017669.1.9",
          "form": "그 가게만의 조리법이 있는거에요?",
          "original_form": "그 가게만의 조리법이 있는거에요?",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017669.1.10",
          "form": "그것까진 모르겠네요.",
          "original_form": "그것까진 모르겠네요.",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017669.1.11",
          "form": "매콤하게 맛있어요",
          "original_form": "매콤하게 맛있어요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017669.1.12",
          "form": "양념장이 들어가요?",
          "original_form": "양념장이 들어가요?",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017669.1.13",
          "form": "직접 넣는건 아니고 만두소가 조금 다른거 같아요",
          "original_form": "직접 넣는건 아니고 만두소가 조금 다른거 같아요",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017669.1.14",
          "form": "주인이 직접 만드는 수제만두인가봐요?",
          "original_form": "주인이 직접 만드는 수제만두인가봐요?",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017669.1.15",
          "form": "맞아요. 만두도 따로 팔구요",
          "original_form": "맞아요. 만두도 따로 팔구요",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017669.1.16",
          "form": "수제만두 진짜 맛있어요 ㅜㅜ",
          "original_form": "수제만두 진짜 맛있어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017669.1.17",
          "form": "저는 사골곰탕에 비비고만두를 넣어서 만드는데 흑흑",
          "original_form": "저는 사골곰탕에 비비고만두를 넣어서 만드는데 흑흑",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017669.1.18",
          "form": "비비고 맛있죠ㅋㅋㅋ",
          "original_form": "비비고 맛있죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:14"
        }
