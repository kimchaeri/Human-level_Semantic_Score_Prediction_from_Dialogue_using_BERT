      "utterance": [
        {
          "id": "MDRW2100029635.1.1",
          "form": "네 피쉬돔재밌어요",
          "original_form": "네 피쉬돔재밌어요",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029635.1.2",
          "form": "제친구",
          "original_form": "제친구",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029635.1.3",
          "form": "열심히 하는 중입니다",
          "original_form": "열심히 하는 중입니다",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029635.1.4",
          "form": "엄청깼던데요",
          "original_form": "엄청깼던데요",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029635.1.5",
          "form": "어느단계까지",
          "original_form": "어느단계까지",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029635.1.6",
          "form": "인기많은 게임이어요",
          "original_form": "인기많은 게임이어요",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029635.1.7",
          "form": "가셨나요",
          "original_form": "가셨나요",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029635.1.8",
          "form": "계속 광고나와서",
          "original_form": "계속 광고나와서",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029635.1.9",
          "form": "게임에서 광고 보면",
          "original_form": "게임에서 광고 보면",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.10",
          "form": "한번하니 멈출수없쥬",
          "original_form": "한번하니 멈출수없쥬",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.11",
          "form": "주는거많은게",
          "original_form": "주는거많은게",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.12",
          "form": "있는겜이 있죠",
          "original_form": "있는겜이 있죠",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.13",
          "form": "피쉬돔은 광고 없어요",
          "original_form": "피쉬돔은 광고 없어요",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.14",
          "form": "저도 그런거 자주 시청해요",
          "original_form": "저도 그런거 자주 시청해요",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.15",
          "form": "홍보광고보고 시작했쥬",
          "original_form": "홍보광고보고 시작했쥬",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.16",
          "form": "광고의 여왕입니다",
          "original_form": "광고의 여왕입니다",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.17",
          "form": "광고로 이득많이봐요",
          "original_form": "광고로 이득많이봐요",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.18",
          "form": "저는 3천판인가 초보에용",
          "original_form": "저는 3천판인가 초보에용",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.19",
          "form": "튀김키우기 광고 많이보세요",
          "original_form": "튀김키우기 광고 많이보세요",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029635.1.20",
          "form": "훨 이득입니다",
          "original_form": "훨 이득입니다",
          "speaker_id": "1",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029635.1.21",
          "form": "광고로 먹고살아야쥬",
          "original_form": "광고로 먹고살아야쥬",
          "speaker_id": "2",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029635.1.22",
          "form": "피쉬돔강추요",
          "original_form": "피쉬돔강추요",
          "speaker_id": "2",
          "time": "20210903 11:09"
        }
