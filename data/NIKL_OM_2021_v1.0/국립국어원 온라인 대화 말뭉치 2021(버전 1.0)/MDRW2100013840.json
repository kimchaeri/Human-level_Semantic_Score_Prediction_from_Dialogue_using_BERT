        {
          "id": "MDRW2100013840.1.1",
          "form": "오늘 비오더라",
          "original_form": "오늘 비오더라",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013840.1.2",
          "form": "요즘 비 많이와서 싫어",
          "original_form": "요즘 비 많이와서 싫어",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013840.1.3",
          "form": "비오는날씨 싫어해?",
          "original_form": "비오는날씨 싫어해?",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013840.1.4",
          "form": "비에 옷이 젖잖아ㅜ",
          "original_form": "비에 옷이 젖잖아ㅜ",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013840.1.5",
          "form": "맞아 난 우산 드는게 귀찮아서 싫어",
          "original_form": "맞아 난 우산 드는게 귀찮아서 싫어",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013840.1.6",
          "form": "이번 장마 지나면 가을오겠다",
          "original_form": "이번 장마 지나면 가을오겠다",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013840.1.7",
          "form": "그럼 좀 시원해지려나?",
          "original_form": "그럼 좀 시원해지려나?",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013840.1.8",
          "form": "응 그렇겠지",
          "original_form": "응 그렇겠지",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013840.1.9",
          "form": "난 여전히 더워서 에어컨을 틀어",
          "original_form": "난 여전히 더워서 에어컨을 틀어",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013840.1.10",
          "form": "ㅋㅋ더위타는구나 요즘 밤엔 쌀쌀해졌던데",
          "original_form": "ㅋㅋ더위타는구나 요즘 밤엔 쌀쌀해졌던데",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013840.1.11",
          "form": "그래도 난 아직 덥더라 ㅠㅠ 여름싫어",
          "original_form": "그래도 난 아직 덥더라 ㅠㅠ 여름싫어",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013840.1.12",
          "form": "그래도 이번 여름에 모기는 없네",
          "original_form": "그래도 이번 여름에 모기는 없네",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013840.1.13",
          "form": "아직 더웠어서 그런가봐",
          "original_form": "아직 더웠어서 그런가봐",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013840.1.14",
          "form": "이번 여름이 많이 더웠지",
          "original_form": "이번 여름이 많이 더웠지",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013840.1.15",
          "form": "여름이 끝났음좋겠어",
          "original_form": "여름이 끝났음좋겠어",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013840.1.16",
          "form": "안녕?",
          "original_form": "안녕?",
