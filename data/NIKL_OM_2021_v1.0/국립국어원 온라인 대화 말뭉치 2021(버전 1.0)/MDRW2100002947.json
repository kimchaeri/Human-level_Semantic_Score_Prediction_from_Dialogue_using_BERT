      "utterance": [
        {
          "id": "MDRW2100002947.1.1",
          "form": "나는 아마 기숙사 밥을 먹을 것 같아",
          "original_form": "나는 아마 기숙사 밥을 먹을 것 같아",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002947.1.2",
          "form": "뭔지 지금 보고올께",
          "original_form": "뭔지 지금 보고올께",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002947.1.3",
          "form": "오 불금인데 돈을 아끼는구나.",
          "original_form": "오 불금인데 돈을 아끼는구나.",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002947.1.4",
          "form": "너는 뭐먹니?",
          "original_form": "너는 뭐먹니?",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002947.1.5",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002947.1.6",
          "form": "너도 메뉴가 궁금했두나",
          "original_form": "너도 메뉴가 궁금했두나",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002947.1.7",
          "form": "우리집은 막창을 먹을 고 같아",
          "original_form": "우리집은 막창을 먹을 고 같아",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.8",
          "form": "사실 자랑을 하기 위해 연락했어.",
          "original_form": "사실 자랑을 하기 위해 연락했어.",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.9",
          "form": "우와 맛있는 메뉴를 먹네",
          "original_form": "우와 맛있는 메뉴를 먹네",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.10",
          "form": "대창전골도 참 맛있더라.",
          "original_form": "대창전골도 참 맛있더라.",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.11",
          "form": "막창은 고소하고 맛있지",
          "original_form": "막창은 고소하고 맛있지",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.12",
          "form": "우야지 배달도 좋아",
          "original_form": "우야지 배달도 좋아",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.13",
          "form": "다음에 나랑도 먹으러가자",
          "original_form": "다음에 나랑도 먹으러가자",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.14",
          "form": "너도 나중에 우야지에서 배닳해서 먹어봐",
          "original_form": "너도 나중에 우야지에서 배닳해서 먹어봐",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.15",
          "form": "배달도하는구나 그 막창집",
          "original_form": "배달도하는구나 그 막창집",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.16",
          "form": "좋아.",
          "original_form": "좋아.",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.17",
          "form": "나 너랑 그 뇨끼도 먹고싶어",
          "original_form": "나 너랑 그 뇨끼도 먹고싶어",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.18",
          "form": "나는 미트소스스파게티가 나온대",
          "original_form": "나는 미트소스스파게티가 나온대",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.19",
          "form": "뇨끼 가끔 생각나지",
          "original_form": "뇨끼 가끔 생각나지",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.20",
          "form": "저번에 막창집에서 봤는데 막창을 옆애서 직접 열심히 구우시더라",
          "original_form": "저번에 막창집에서 봤는데 막창을 옆애서 직접 열심히 구우시더라",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002947.1.21",
          "form": "미트소스스파게티 좀 나누어줄래?",
          "original_form": "미트소스스파게티 좀 나누어줄래?",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002947.1.22",
          "form": "음 상상만해도 침나온다",
          "original_form": "음 상상만해도 침나온다",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002947.1.23",
          "form": "물론 막창은 줄 수 없어",
          "original_form": "물론 막창은 줄 수 없어",
          "speaker_id": "1",
          "time": "20210820 16:57"
        }
      ]
