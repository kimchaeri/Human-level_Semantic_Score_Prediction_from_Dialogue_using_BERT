      "utterance": [
        {
          "id": "MDRW2100003169.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210820 20:01"
        },
        {
          "id": "MDRW2100003169.1.2",
          "form": "햄스터도 반려동물이라면 키우고있어요 ㅎㅎ",
          "original_form": "햄스터도 반려동물이라면 키우고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100003169.1.3",
          "form": "반려동물 뭐 키우세요?",
          "original_form": "반려동물 뭐 키우세요?",
          "speaker_id": "2",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100003169.1.4",
          "form": "오 저는 반려식물이요!  방토 엄마에요",
          "original_form": "오 저는 반려식물이요!  방토 엄마에요",
          "speaker_id": "1",
          "time": "20210820 20:22"
        },
        {
          "id": "MDRW2100003169.1.5",
          "form": "오 방울토마토키우시는군요 키우기 쉽나요?",
          "original_form": "오 방울토마토키우시는군요 키우기 쉽나요?",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003169.1.6",
          "form": "작년에는 쉽게 키워서 수확도 했는데 올해는 텄어요ㅜㅠ",
          "original_form": "작년에는 쉽게 키워서 수확도 했는데 올해는 텄어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003169.1.7",
          "form": "올해는 왜 잘 안됐나요 ㅎㅎ",
          "original_form": "올해는 왜 잘 안됐나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003169.1.8",
          "form": "애기가 있는데 뿌리채 쑤욱 뽑아서ㅋㅋㅋ 다시 심었거든요ㅋㅋ;;",
          "original_form": "애기가 있는데 뿌리채 쑤욱 뽑아서ㅋㅋㅋ 다시 심었거든요ㅋㅋ;;",
          "speaker_id": "1",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003169.1.9",
          "form": "아....ㅎㅎㅎ마음이 아프네요 주륵..",
          "original_form": "아....ㅎㅎㅎ마음이 아프네요 주륵..",
          "speaker_id": "2",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003169.1.10",
          "form": "저도 햄스터 키우다가 애 태어나고 관리가 소홀해졌네요ㅠㅠ",
          "original_form": "저도 햄스터 키우다가 애 태어나고 관리가 소홀해졌네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003169.1.11",
          "form": "그래서 그런지 꽃봉우리가 안맺혀요ㅎㅎ;",
          "original_form": "그래서 그런지 꽃봉우리가 안맺혀요ㅎㅎ;",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003169.1.12",
          "form": "또 다른거 키워보셨어요?",
          "original_form": "또 다른거 키워보셨어요?",
          "speaker_id": "2",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003169.1.13",
          "form": "어릴때 강아지 두마리 키워봤어요",
          "original_form": "어릴때 강아지 두마리 키워봤어요",
          "speaker_id": "1",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003169.1.14",
          "form": "내년엔 수확까지 성공할겁니다 ㅎㅎ",
          "original_form": "내년엔 수확까지 성공할겁니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003169.1.15",
          "form": "우와 무슨종 키우셨어요?",
          "original_form": "우와 무슨종 키우셨어요?",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003169.1.16",
          "form": "푸들이요",
          "original_form": "푸들이요",
          "speaker_id": "1",
          "time": "20210820 21:26"
        },
        {
          "id": "MDRW2100003169.1.17",
          "form": "귀여웠겠어요",
          "original_form": "귀여웠겠어요",
          "speaker_id": "2",
          "time": "20210820 21:27"
        },
        {
          "id": "MDRW2100003169.1.18",
          "form": "저도 어렸을때 삽살개키워봤어요",
          "original_form": "저도 어렸을때 삽살개키워봤어요",
