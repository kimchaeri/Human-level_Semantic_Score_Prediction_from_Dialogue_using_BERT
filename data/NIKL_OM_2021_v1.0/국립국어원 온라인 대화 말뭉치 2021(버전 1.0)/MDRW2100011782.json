      "utterance": [
        {
          "id": "MDRW2100011782.1.1",
          "form": "가족관계가 어떻게 되시나요?",
          "original_form": "가족관계가 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011782.1.2",
          "form": "남편과 아이두명 이렇게 4명이요~",
          "original_form": "남편과 아이두명 이렇게 4명이요~",
          "speaker_id": "2",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011782.1.3",
          "form": "결혼하셨군요ㅎㅎ육아고충있으신가요?",
          "original_form": "결혼하셨군요ㅎㅎ육아고충있으신가요?",
          "speaker_id": "1",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011782.1.4",
          "form": "아무래도 아이들이 연년생이다보니 육아가 힘들더라고요",
          "original_form": "아무래도 아이들이 연년생이다보니 육아가 힘들더라고요",
          "speaker_id": "2",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011782.1.5",
          "form": "몇살이에요 지금은?",
          "original_form": "몇살이에요 지금은?",
          "speaker_id": "1",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011782.1.6",
          "form": "12살,11살이요",
          "original_form": "12살,11살이요",
          "speaker_id": "2",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011782.1.7",
          "form": "한창 자랄때네요ㅎㅎ",
          "original_form": "한창 자랄때네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011782.1.8",
          "form": "네 맞아요 아이 있으신가요?",
          "original_form": "네 맞아요 아이 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011782.1.9",
          "form": "저는 아이는 없지만 아동센터에서 근무했었어서",
          "original_form": "저는 아이는 없지만 아동센터에서 근무했었어서",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011782.1.10",
          "form": "좋은일하셨네요~아이들 몇명있어요?",
          "original_form": "좋은일하셨네요~아이들 몇명있어요?",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011782.1.11",
          "form": "18명이요!",
          "original_form": "18명이요!",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011782.1.12",
          "form": "와 혼자 아이들을 혼자케어하신건가요?",
          "original_form": "와 혼자 아이들을 혼자케어하신건가요?",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011782.1.13",
          "form": "아뇨 다른 봉사자선생님들이랑 같이요ㅠㅎㅎ",
          "original_form": "아뇨 다른 봉사자선생님들이랑 같이요ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011782.1.14",
          "form": "힘들죠 아이들이 여러명이면",
          "original_form": "힘들죠 아이들이 여러명이면",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011782.1.15",
          "form": "그래도 아이들이 예뻐서 힐링많이 받았어요",
          "original_form": "그래도 아이들이 예뻐서 힐링많이 받았어요",
          "speaker_id": "1",
          "time": "20210824 16:06"
        }
