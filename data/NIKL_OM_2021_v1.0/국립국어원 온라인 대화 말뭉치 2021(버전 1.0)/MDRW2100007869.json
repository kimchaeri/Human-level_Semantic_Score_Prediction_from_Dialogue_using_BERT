      "utterance": [
        {
          "id": "MDRW2100007869.1.1",
          "form": "책 많이 읽으세요?",
          "original_form": "책 많이 읽으세요?",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007869.1.2",
          "form": "책도 폭식하는 습관이 있어서",
          "original_form": "책도 폭식하는 습관이 있어서",
          "speaker_id": "2",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007869.1.3",
          "form": "볼때는 몰아보고 아닐때는 거리가 멀고 하네요",
          "original_form": "볼때는 몰아보고 아닐때는 거리가 멀고 하네요",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007869.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007869.1.5",
          "form": "저는 이북으로 많이봐요",
          "original_form": "저는 이북으로 많이봐요",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007869.1.6",
          "form": "최근엔 동네 도서관 카드 발급 받았어요",
          "original_form": "최근엔 동네 도서관 카드 발급 받았어요",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007869.1.7",
          "form": "저는 e북으로는 아직 본적이 없네요:)",
          "original_form": "저는 e북으로는 아직 본적이 없네요:)",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007869.1.8",
          "form": "이북이 편하더라고요 한번 보기시작하니까요",
          "original_form": "이북이 편하더라고요 한번 보기시작하니까요",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007869.1.9",
          "form": "요새는 어떤 책 보고 있으세요?",
          "original_form": "요새는 어떤 책 보고 있으세요?",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007869.1.10",
          "form": "다이어트 책 보고있어요",
          "original_form": "다이어트 책 보고있어요",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007869.1.11",
          "form": "물론 실천이 힘들죠ㅜ",
          "original_form": "물론 실천이 힘들죠ㅜ",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007869.1.12",
          "form": "다이어트 책은\r\n식단 관련 내용인가요?",
          "original_form": "다이어트 책은\r\n식단 관련 내용인가요?",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007869.1.13",
          "form": "평균 700칼로리에 맞춘 요리책 느낌이었어요",
          "original_form": "평균 700칼로리에 맞춘 요리책 느낌이었어요",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007869.1.14",
          "form": "와... 700칼로리면\r\n뭘 먹을 수 있는거죠? ㅠ",
          "original_form": "와... 700칼로리면\r\n뭘 먹을 수 있는거죠? ㅠ",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007869.1.15",
          "form": "양이 진짜진짜작더라고요",
          "original_form": "양이 진짜진짜작더라고요",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007869.1.16",
          "form": "전부 다해서 100그램? 200그램?",
          "original_form": "전부 다해서 100그램? 200그램?",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007869.1.17",
          "form": "가혹한 책이네요ㅠ ㅋㅋ",
          "original_form": "가혹한 책이네요ㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007869.1.18",
          "form": "그거말고 토익책도 이북으로 빌렸었어요 ㅎ요새 도서관 좋아요 ㅎㅎ",
          "original_form": "그거말고 토익책도 이북으로 빌렸었어요 ㅎ요새 도서관 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:34"
        }
