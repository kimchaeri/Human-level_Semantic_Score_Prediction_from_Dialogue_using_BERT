      "utterance": [
        {
          "id": "MDRW2100007857.1.1",
          "form": "최근 읽은 책 있으세요",
          "original_form": "최근 읽은 책 있으세요",
          "speaker_id": "1",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007857.1.2",
          "form": "네 있어요 ㅎ",
          "original_form": "네 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 11:59"
        },
        {
          "id": "MDRW2100007857.1.3",
          "form": "제목이 뭐였나요?",
          "original_form": "제목이 뭐였나요?",
          "speaker_id": "1",
          "time": "20210822 12:00"
        },
        {
          "id": "MDRW2100007857.1.4",
          "form": "엄마에게도 땡땡이가 필요해 였어요 ㅎ",
          "original_form": "엄마에게도 땡땡이가 필요해 였어요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007857.1.5",
          "form": "책은 힐링같아요",
          "original_form": "책은 힐링같아요",
          "speaker_id": "2",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007857.1.6",
          "form": "요즘 읽으시는책있으세요?",
          "original_form": "요즘 읽으시는책있으세요?",
          "speaker_id": "2",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007857.1.7",
          "form": "저는 수면관리책 이요",
          "original_form": "저는 수면관리책 이요",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007857.1.8",
          "form": "제목이 길었는데 다섯시간만자도 충분하다였나...",
          "original_form": "제목이 길었는데 다섯시간만자도 충분하다였나...",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007857.1.9",
          "form": "오~ 그래요? 저도 4~5시간만 자려고하는데",
          "original_form": "오~ 그래요? 저도 4~5시간만 자려고하는데",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007857.1.10",
          "form": "일본 사람이쓴 책이었어요 ㅋㅋ",
          "original_form": "일본 사람이쓴 책이었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007857.1.11",
          "form": "뭔가 그런저런 실용서는 일본 사람 책이 많은듯",
          "original_form": "뭔가 그런저런 실용서는 일본 사람 책이 많은듯",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007857.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007857.1.13",
          "form": "미니멀라이프 관련 책도 일본작가분이 많아요",
          "original_form": "미니멀라이프 관련 책도 일본작가분이 많아요",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007857.1.14",
          "form": "근데 땡땡이가 뮌가요",
          "original_form": "근데 땡땡이가 뮌가요",
          "speaker_id": "1",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100007857.1.15",
          "form": "엄마에게 땡땡이가 필요하다는데 ㅋㅋ",
          "original_form": "엄마에게 땡땡이가 필요하다는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100007857.1.16",
          "form": "땡땡이 모르세요?",
          "original_form": "땡땡이 모르세요?",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100007857.1.17",
          "form": "수업빼먹고 노는거 같은거죠 ㅎ",
          "original_form": "수업빼먹고 노는거 같은거죠 ㅎ",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100007857.1.18",
          "form": "육아안하고 자유시간 가지는거예요",
          "original_form": "육아안하고 자유시간 가지는거예요",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100007857.1.19",
          "form": "아하 그 땡땡이 ㅋㅋ",
          "original_form": "아하 그 땡땡이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100007857.1.20",
          "form": "혼자서 땡땡이 무늬? 이런거 생각했어요",
          "original_form": "혼자서 땡땡이 무늬? 이런거 생각했어요",
          "speaker_id": "1",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100007857.1.21",
          "form": "ㅎㅎ 단어로 전달하다보니 그렇게 생각하실수도 있겠네요 ㅎ",
          "original_form": "ㅎㅎ 단어로 전달하다보니 그렇게 생각하실수도 있겠네요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100007857.1.22",
          "form": "좋아하는 작가 있으세요?",
          "original_form": "좋아하는 작가 있으세요?",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100007857.1.23",
          "form": "한때 히가시노 게이고 추리소설이나 아가사 크리스티 소설을 진짜 열심히 읽었었어요 ㅎㅎ",
          "original_form": "한때 히가시노 게이고 추리소설이나 아가사 크리스티 소설을 진짜 열심히 읽었었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:55"
        }
      ]
