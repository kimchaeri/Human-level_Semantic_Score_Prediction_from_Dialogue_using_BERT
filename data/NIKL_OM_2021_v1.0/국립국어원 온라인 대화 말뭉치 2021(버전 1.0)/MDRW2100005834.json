      "utterance": [
        {
          "id": "MDRW2100005834.1.1",
          "form": "그건뭐죠",
          "original_form": "그건뭐죠",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100005834.1.2",
          "form": "바퀴달린 장바구니에요",
          "original_form": "바퀴달린 장바구니에요",
          "speaker_id": "1",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100005834.1.3",
          "form": "유명한가요",
          "original_form": "유명한가요",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100005834.1.4",
          "form": "장보러갈때 많이들 써요",
          "original_form": "장보러갈때 많이들 써요",
          "speaker_id": "1",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100005834.1.5",
          "form": "번거로워보여요",
          "original_form": "번거로워보여요",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100005834.1.6",
          "form": "시작은 스벅에서 굿즈로 내놨는데 대박이 났죠",
          "original_form": "시작은 스벅에서 굿즈로 내놨는데 대박이 났죠",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100005834.1.7",
          "form": "스벅은 마케팅잘해요",
          "original_form": "스벅은 마케팅잘해요",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100005834.1.8",
          "form": "사람들이 그걸 보고 너도나도 폴딩카트를 갖고싶어하게 되었어요",
          "original_form": "사람들이 그걸 보고 너도나도 폴딩카트를 갖고싶어하게 되었어요",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100005834.1.9",
          "form": "구입하실건가요",
          "original_form": "구입하실건가요",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100005834.1.10",
          "form": "아니요 바퀴달린건 실용적이긴한데",
          "original_form": "아니요 바퀴달린건 실용적이긴한데",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100005834.1.11",
          "form": "공간차지하네요",
          "original_form": "공간차지하네요",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100005834.1.12",
          "form": "제가보기엔 어르신들께 좋아보여요",
          "original_form": "제가보기엔 어르신들께 좋아보여요",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100005834.1.13",
          "form": "카트 가지고다님안전하죠",
          "original_form": "카트 가지고다님안전하죠",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100005834.1.14",
          "form": "모양도 꽤나 실용적이고 접을 수있어서 일반 카트보단 좋아요",
          "original_form": "모양도 꽤나 실용적이고 접을 수있어서 일반 카트보단 좋아요",
          "speaker_id": "1",
          "time": "20210907 13:48"
