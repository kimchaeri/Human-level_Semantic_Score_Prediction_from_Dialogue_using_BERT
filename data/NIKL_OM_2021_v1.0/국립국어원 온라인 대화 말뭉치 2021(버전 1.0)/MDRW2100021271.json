      "utterance": [
        {
          "id": "MDRW2100021271.1.1",
          "form": "유투브 자주 보세요?",
          "original_form": "유투브 자주 보세요?",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100021271.1.2",
          "form": "네 매일 자기전에 봐요",
          "original_form": "네 매일 자기전에 봐요",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100021271.1.3",
          "form": "자주보세요?",
          "original_form": "자주보세요?",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100021271.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021271.1.5",
          "form": "어떤거보시나요?",
          "original_form": "어떤거보시나요?",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021271.1.6",
          "form": "저는 예능이나 드라마 클립 자주보고",
          "original_form": "저는 예능이나 드라마 클립 자주보고",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021271.1.7",
          "form": "알고리즘 추천영상도 ㅎㅎ 많이 보고",
          "original_form": "알고리즘 추천영상도 ㅎㅎ 많이 보고",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021271.1.8",
          "form": "먹방은 자주 안봐요! name1님은요?",
          "original_form": "먹방은 자주 안봐요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021271.1.9",
          "form": "저는 요리 영상하구요 게임, 드라마클립 봐요",
          "original_form": "저는 요리 영상하구요 게임, 드라마클립 봐요",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021271.1.10",
          "form": "승우아빠 보세여? ㅋㅋ",
          "original_form": "승우아빠 보세여? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021271.1.11",
          "form": "먹방은 식이할때 봐요..",
          "original_form": "먹방은 식이할때 봐요..",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021271.1.12",
          "form": "네 봐요!",
          "original_form": "네 봐요!",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021271.1.13",
          "form": "저희 가족들이 승우아빠 엄청봐요 저는 그냥 티비에 틀려있으면 보는 정도",
          "original_form": "저희 가족들이 승우아빠 엄청봐요 저는 그냥 티비에 틀려있으면 보는 정도",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021271.1.14",
          "form": "요즘은 덜보는데 라면직접만드는 컨텐츠 같은거 할때 자주봤어요ㅋㅋㅋ",
          "original_form": "요즘은 덜보는데 라면직접만드는 컨텐츠 같은거 할때 자주봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021271.1.15",
          "form": "요리영상 얘기하시자마자 승우아빠부터 생각났어요 ㅋㅋ",
          "original_form": "요리영상 얘기하시자마자 승우아빠부터 생각났어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021271.1.16",
          "form": "재밌으시더라구요",
          "original_form": "재밌으시더라구요",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021271.1.17",
          "form": "그 이상형월드컵 음식버전 이런것도 하더라구요",
          "original_form": "그 이상형월드컵 음식버전 이런것도 하더라구요",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021271.1.18",
          "form": "아맞아요! 그거 종류들을 요리사출신이시라 꽤나 전문적이라 유용하기도했어요",
          "original_form": "아맞아요! 그거 종류들을 요리사출신이시라 꽤나 전문적이라 유용하기도했어요",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100021271.1.19",
          "form": "종류들을 설명하시는게",
          "original_form": "종류들을 설명하시는게",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100021271.1.20",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210828 17:19"
        }
      ]
