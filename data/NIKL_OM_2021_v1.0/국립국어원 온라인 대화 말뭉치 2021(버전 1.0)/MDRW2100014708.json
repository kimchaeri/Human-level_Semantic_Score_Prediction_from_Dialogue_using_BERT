      "utterance": [
        {
          "id": "MDRW2100014708.1.1",
          "form": "꽃게탕도 좋아해요 전 ㅋ",
          "original_form": "꽃게탕도 좋아해요 전 ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014708.1.2",
          "form": "저 탕은 잘 안먹어요",
          "original_form": "저 탕은 잘 안먹어요",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014708.1.3",
          "form": "학교에서 꽃게 들어간 탕 나오면 거의 친구한테 넘겼어요",
          "original_form": "학교에서 꽃게 들어간 탕 나오면 거의 친구한테 넘겼어요",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014708.1.4",
          "form": "아 그럼 찜은요?",
          "original_form": "아 그럼 찜은요?",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014708.1.5",
          "form": "찜은 먹어요",
          "original_form": "찜은 먹어요",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014708.1.6",
          "form": "전 지금 갑자기",
          "original_form": "전 지금 갑자기",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014708.1.7",
          "form": "족발땡겨요",
          "original_form": "족발땡겨요",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014708.1.8",
          "form": "으아 저도 족발 땡겨요",
          "original_form": "으아 저도 족발 땡겨요",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014708.1.9",
          "form": "가게 닫았을텐데...ㅠㅜ",
          "original_form": "가게 닫았을텐데...ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014708.1.10",
          "form": "왜 갑자기 족발이 땡기는지....",
          "original_form": "왜 갑자기 족발이 땡기는지....",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014708.1.11",
          "form": "name2 님 최애 음식은 콩국수랑 또 뭐있나요?",
          "original_form": "&name2& 님 최애 음식은 콩국수랑 또 뭐있나요?",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014708.1.12",
          "form": "저는 면요리는 다 좋아해요",
          "original_form": "저는 면요리는 다 좋아해요",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014708.1.13",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014708.1.14",
          "form": "저녁먹었는데",
          "original_form": "저녁먹었는데",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014708.1.15",
          "form": "배고프다는",
          "original_form": "배고프다는",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014708.1.16",
          "form": "저도요... 분명 저녁 많이 먹었는데",
          "original_form": "저도요... 분명 저녁 많이 먹었는데",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014708.1.17",
          "form": "위가 밥 달라고 조르네요",
          "original_form": "위가 밥 달라고 조르네요",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014708.1.18",
          "form": "ㅋㅋㅋㅋ 한참 먹을나이자나요",
          "original_form": "ㅋㅋㅋㅋ 한참 먹을나이자나요",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014708.1.19",
          "form": "성장기는 끝났어요ㅋㅋ 이제는 먹으면 살찌더라구요",
          "original_form": "성장기는 끝났어요ㅋㅋ 이제는 먹으면 살찌더라구요",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014708.1.20",
          "form": "그럼 저는.....",
          "original_form": "그럼 저는.....",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014708.1.21",
          "form": "그래도 먹고싶은건 먹어야죠",
          "original_form": "그래도 먹고싶은건 먹어야죠",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014708.1.22",
          "form": "살 생각은 스트레스입니다",
          "original_form": "살 생각은 스트레스입니다",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014708.1.23",
          "form": "그럼 지금?ㅋㅋㅋ",
          "original_form": "그럼 지금?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100014708.1.24",
          "form": "안되겠어요 식빵 버터에 구워먹겠습니다",
          "original_form": "안되겠어요 식빵 버터에 구워먹겠습니다",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100014708.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100014708.1.26",
          "form": "토스트",
          "original_form": "토스트",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100014708.1.27",
          "form": "맛나겠다",
          "original_form": "맛나겠다",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100014708.1.28",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100014708.1.29",
          "form": "잼 안바르고 딱 식빵 한장만 먹었어요",
          "original_form": "잼 안바르고 딱 식빵 한장만 먹었어요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100014708.1.30",
          "form": "배고플땐 먹어야죠 ㅋㅋ",
          "original_form": "배고플땐 먹어야죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100014708.1.31",
          "form": "전 안되고 님은 되고",
          "original_form": "전 안되고 님은 되고",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100014708.1.32",
          "form": "name1님도 드세요",
          "original_form": "&name1&님도 드세요",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100014708.1.33",
          "form": "물로 배를 채우겠습니다",
          "original_form": "물로 배를 채우겠습니다",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100014708.1.34",
          "form": "그건 진짜 아니에요...",
          "original_form": "그건 진짜 아니에요...",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100014708.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100014708.1.36",
          "form": "그럼 전 건빵 먹겠습니다",
          "original_form": "그럼 전 건빵 먹겠습니다",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100014708.1.37",
          "form": "집에 먹을간식이 이것밖에ㅜㅜ",
          "original_form": "집에 먹을간식이 이것밖에ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100014708.1.38",
          "form": "ㅠ... 건빵 오랜만이네요 요즘에도 별사탕 들어있나요",
          "original_form": "ㅠ... 건빵 오랜만이네요 요즘에도 별사탕 들어있나요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100014708.1.39",
          "form": "이건 보리건빵",
          "original_form": "이건 보리건빵",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100014708.1.40",
          "form": "별사탕 없네요",
          "original_form": "별사탕 없네요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100014708.1.41",
          "form": "우유랑 같이 먹어야 목이 덜메이는데 우유도 없네요 ㅋㅋㅋㅋㅋㅋ",
