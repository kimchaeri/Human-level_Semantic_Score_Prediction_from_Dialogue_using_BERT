      "utterance": [
        {
          "id": "MDRW2100035693.1.1",
          "form": "그렇게 피자에대한 미련을 채우고왔어요 ㅋㅋ",
          "original_form": "그렇게 피자에대한 미련을 채우고왔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100035693.1.2",
          "form": "ㅎㅎ 얘기 들으니 제가 낚이고 있네요",
          "original_form": "ㅎㅎ 얘기 들으니 제가 낚이고 있네요",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100035693.1.3",
          "form": "피자 이 시간에 배달하는 곳 있나 뒤지는 중입니다",
          "original_form": "피자 이 시간에 배달하는 곳 있나 뒤지는 중입니다",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100035693.1.4",
          "form": "피자헛은 문닫았지만ㅜ 제가 좋아하는 몇집은 문 열었네요ㅎ",
          "original_form": "피자헛은 문닫았지만ㅜ 제가 좋아하는 몇집은 문 열었네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100035693.1.5",
          "form": "악 ㅋㅋ다이어트 안하셔도되요? 부럽다ㅜㅜ",
          "original_form": "악 ㅋㅋ다이어트 안하셔도되요? 부럽다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035693.1.6",
          "form": "여름이 갔으니...괜찮지 않을까요?",
          "original_form": "여름이 갔으니...괜찮지 않을까요?",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035693.1.7",
          "form": "코로나라 수영도 해외여행도 막혔으니",
          "original_form": "코로나라 수영도 해외여행도 막혔으니",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035693.1.8",
          "form": "글구 님도 조금전까지 드셨잖아욧!",
          "original_form": "글구 님도 조금전까지 드셨잖아욧!",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035693.1.9",
          "form": "절 악의 구렁텅이로 밀어넣으신 분이ㅜ",
          "original_form": "절 악의 구렁텅이로 밀어넣으신 분이ㅜ",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035693.1.10",
          "form": "아뇨아뇨 부러워서요 ㅋㅋㅋ",
          "original_form": "아뇨아뇨 부러워서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035693.1.11",
          "form": "전이제 두달은 빠이에요ㅜ",
          "original_form": "전이제 두달은 빠이에요ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035693.1.12",
          "form": "안녕 내 소울메이트",
          "original_form": "안녕 내 소울메이트",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035693.1.13",
          "form": "365일 피자만 먹어도 살찌지않는 사람이고싶었어ㅜ",
          "original_form": "365일 피자만 먹어도 살찌지않는 사람이고싶었어ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100035693.1.14",
          "form": "아ㅜ 다이어트하면 그 기간엔 아예 치킨피자는 안 먹나요?",
          "original_form": "아ㅜ 다이어트하면 그 기간엔 아예 치킨피자는 안 먹나요?",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100035693.1.15",
          "form": "일단 그 야식은 금지ㅜ",
          "original_form": "일단 그 야식은 금지ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100035693.1.16",
          "form": "저녁엔 다이어트쉐이크를 허허",
          "original_form": "저녁엔 다이어트쉐이크를 허허",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100035693.1.17",
          "form": "헉 다이어트쉐이크는 뭐뭐를 넣길래 한끼인가요ㄷㄷㄷ",
          "original_form": "헉 다이어트쉐이크는 뭐뭐를 넣길래 한끼인가요ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100035693.1.18",
          "form": "그냥 단백질?과 ㅋㅋ",
          "original_form": "그냥 단백질?과 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100035693.1.19",
          "form": "입이 덜심심하게 해주겠죠?",
          "original_form": "입이 덜심심하게 해주겠죠?",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100035693.1.20",
          "form": "단백질 분말형태인가요? 미숫가루처럼?",
          "original_form": "단백질 분말형태인가요? 미숫가루처럼?",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100035693.1.21",
          "form": "만화책에서 운동선수들이 마시던 쉐이크 느낌이려나요.. 거기선 워낙 괴식으로 묘사돼서ㅜ",
          "original_form": "만화책에서 운동선수들이 마시던 쉐이크 느낌이려나요.. 거기선 워낙 괴식으로 묘사돼서ㅜ",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100035693.1.22",
          "form": "넵넵 큰통에들었어요 2키로짜리 ㅋㅋ",
          "original_form": "넵넵 큰통에들었어요 2키로짜리 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100035693.1.23",
          "form": "두달먹는데여 ㅋㄱ",
          "original_form": "두달먹는데여 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100035693.1.24",
          "form": "아 그래서 두 달이군요ㅜㅜㅜ",
          "original_form": "아 그래서 두 달이군요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100035693.1.25",
          "form": "그럼 아침이라도 잘 챙겨드셔야겠어요",
          "original_form": "그럼 아침이라도 잘 챙겨드셔야겠어요",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100035693.1.26",
          "form": "아침엔 바나나랑 견과류 먹을라고용",
          "original_form": "아침엔 바나나랑 견과류 먹을라고용",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100035693.1.27",
          "form": "원래 아침 잘 안먹 ㅋㅋ",
          "original_form": "원래 아침 잘 안먹 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100035693.1.28",
          "form": "아니 아침이 바나나 저녁이 쉐이크면;;",
          "original_form": "아니 아침이 바나나 저녁이 쉐이크면;;",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100035693.1.29",
