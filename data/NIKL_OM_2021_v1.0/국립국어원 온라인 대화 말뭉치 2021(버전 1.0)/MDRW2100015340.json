      "utterance": [
        {
          "id": "MDRW2100015340.1.1",
          "form": "친구들이랑 가기로 계획한 곳 있으세요?",
          "original_form": "친구들이랑 가기로 계획한 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015340.1.2",
          "form": "저 스페인 순례길 가려했는데",
          "original_form": "저 스페인 순례길 가려했는데",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015340.1.3",
          "form": "코로나가..",
          "original_form": "코로나가..",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015340.1.4",
          "form": "베트남 뭐가 좋았어요",
          "original_form": "베트남 뭐가 좋았어요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015340.1.5",
          "form": "헐 완전 기독교신가요",
          "original_form": "헐 완전 기독교신가요",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015340.1.6",
          "form": "먹는거요 분짜랑 쌀국수 진짜",
          "original_form": "먹는거요 분짜랑 쌀국수 진짜",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.7",
          "form": "미쳐요",
          "original_form": "미쳐요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.8",
          "form": "아니요 ㅅ스페인하숙보고 가고싶더라구요",
          "original_form": "아니요 ㅅ스페인하숙보고 가고싶더라구요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.9",
          "form": "거기 살고싶었어요",
          "original_form": "거기 살고싶었어요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.10",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.11",
          "form": "쌀국수어때요",
          "original_form": "쌀국수어때요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.12",
          "form": "순례길은 근데 약간 고통이잖아요..?",
          "original_form": "순례길은 근데 약간 고통이잖아요..?",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.13",
          "form": "한국에서 먹는것과 다른가요",
          "original_form": "한국에서 먹는것과 다른가요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.14",
          "form": "아 다르더라고요",
          "original_form": "아 다르더라고요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.15",
          "form": "육수 자체가",
          "original_form": "육수 자체가",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.16",
          "form": "면도요",
          "original_form": "면도요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.17",
          "form": "생면인데 한국 생면이랑 다르고요",
          "original_form": "생면인데 한국 생면이랑 다르고요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.18",
          "form": "대만족",
          "original_form": "대만족",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015340.1.19",
          "form": "아 너무 좋은데",
          "original_form": "아 너무 좋은데",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.20",
          "form": "그냥 어딜 들어가도 맛있더라고요",
          "original_form": "그냥 어딜 들어가도 맛있더라고요",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.21",
          "form": "꼭 가보겠습니다",
          "original_form": "꼭 가보겠습니다",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.22",
          "form": "순례길 저건",
          "original_form": "순례길 저건",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.23",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.24",
          "form": "약간 생각정리나 터닝포인트 된다해서",
          "original_form": "약간 생각정리나 터닝포인트 된다해서",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.25",
          "form": "한번 가보면 어떨까",
          "original_form": "한번 가보면 어떨까",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.26",
          "form": "아아 그때 약간 심란했나보죠?",
          "original_form": "아아 그때 약간 심란했나보죠?",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.27",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.28",
          "form": "그런곳에서 산책하며 생각정리하기",
          "original_form": "그런곳에서 산책하며 생각정리하기",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015340.1.29",
          "form": "약간 방황하던시기였죠",
          "original_form": "약간 방황하던시기였죠",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015340.1.30",
          "form": "스키장 어릴 때 많이가봤어요",
          "original_form": "스키장 어릴 때 많이가봤어요",
          "speaker_id": "1",
          "time": "20210826 12:24"
        }
      ]
