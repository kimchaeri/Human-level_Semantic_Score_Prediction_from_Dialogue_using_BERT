      "utterance": [
        {
          "id": "MDRW2100028055.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028055.1.2",
          "form": "안녕하세요! 오늘 좀 더워요",
          "original_form": "안녕하세요! 오늘 좀 더워요",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028055.1.3",
          "form": "진짜요! 비도 와서 좀 습하네요",
          "original_form": "진짜요! 비도 와서 좀 습하네요",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028055.1.4",
          "form": "반팔 입었는데도 더워서 놀랐어요",
          "original_form": "반팔 입었는데도 더워서 놀랐어요",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028055.1.5",
          "form": "가을 올 줄 알았는데",
          "original_form": "가을 올 줄 알았는데",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028055.1.6",
          "form": "진짜요!",
          "original_form": "진짜요!",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028055.1.7",
          "form": "어느날은 추웠다가",
          "original_form": "어느날은 추웠다가",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028055.1.8",
          "form": "또 더웠다가",
          "original_form": "또 더웠다가",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028055.1.9",
          "form": "습했다가.. 난리났어요",
          "original_form": "습했다가.. 난리났어요",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028055.1.10",
          "form": "만두처럼 쪄지고 있는 것 같네요",
          "original_form": "만두처럼 쪄지고 있는 것 같네요",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028055.1.11",
          "form": "그래도 미세먼지는 좋아져서 다행이에요",
          "original_form": "그래도 미세먼지는 좋아져서 다행이에요",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028055.1.12",
          "form": "진짜요! 미세먼지 요즘 계속 좋은 것 같아요 뭔가 코로나 시작되고 나서 좀 좋아진 것 같아요ㅋㅋ",
          "original_form": "진짜요! 미세먼지 요즘 계속 좋은 것 같아요 뭔가 코로나 시작되고 나서 좀 좋아진 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028055.1.13",
          "form": "그리고 미세먼지 아니어도 계속 코로나 쓰게 되니까 크게 신경쓰이지도 않더라고요 ㅋㅋㅋ웃픈...현실...",
          "original_form": "그리고 미세먼지 아니어도 계속 코로나 쓰게 되니까 크게 신경쓰이지도 않더라고요 ㅋㅋㅋ웃픈...현실...",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028055.1.14",
          "form": "겨울되면 뭔가 또 늘어날 것 같구 그렇네요ㅠㅠ",
          "original_form": "겨울되면 뭔가 또 늘어날 것 같구 그렇네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028055.1.15",
          "form": "저녁 되니까 정말 선선하네요!",
          "original_form": "저녁 되니까 정말 선선하네요!",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028055.1.16",
          "form": "밤에는 춥기까지 하더라고요",
          "original_form": "밤에는 춥기까지 하더라고요",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028055.1.17",
          "form": "밤에는 반팔 입고 못 나가겠더라구요 완전 쌀쌀...",
          "original_form": "밤에는 반팔 입고 못 나가겠더라구요 완전 쌀쌀...",
          "speaker_id": "1",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028055.1.18",
          "form": "잘 때도 추워서 중간에 깼어요 어제는....",
          "original_form": "잘 때도 추워서 중간에 깼어요 어제는....",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028055.1.19",
          "form": "겨울이 오고 있군요",
          "original_form": "겨울이 오고 있군요",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028055.1.20",
          "form": "헉 진짜요?!?!!? 전 다한증 때문에 아직 더워욕..",
          "original_form": "헉 진짜요?!?!!? 전 다한증 때문에 아직 더워욕..",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028055.1.21",
          "form": "허걱...ㅋㅋㅋㅋㅋㅋㅋㅋ금방 날 시원해지겠죠 뭐",
          "original_form": "허걱...ㅋㅋㅋㅋㅋㅋㅋㅋ금방 날 시원해지겠죠 뭐",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100028055.1.22",
          "form": "겨울 좋아하세요?",
          "original_form": "겨울 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100028055.1.23",
          "form": "음... 겨울... 옷은 좋아해요!ㅋㅋㅋㅋㅋㅋ 겨울에 이불 꽁꽁 싸매는거 너무 좋아욕",
          "original_form": "음... 겨울... 옷은 좋아해요!ㅋㅋㅋㅋㅋㅋ 겨울에 이불 꽁꽁 싸매는거 너무 좋아욕",
          "speaker_id": "1",
