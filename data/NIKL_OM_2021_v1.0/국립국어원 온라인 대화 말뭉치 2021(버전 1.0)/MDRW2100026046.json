      "utterance": [
        {
          "id": "MDRW2100026046.1.1",
          "form": "등이 몸매관리 비법 알려주십셔",
          "original_form": "등이 몸매관리 비법 알려주십셔",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026046.1.2",
          "form": "하체비만입니다만 댁은 먹고 눕지 마십시오",
          "original_form": "하체비만입니다만 댁은 먹고 눕지 마십시오",
          "speaker_id": "2",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026046.1.3",
          "form": "어 제 비밀인데 팔뚝살은 어떻게하나요",
          "original_form": "어 제 비밀인데 팔뚝살은 어떻게하나요",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100026046.1.4",
          "form": "저를향해 매일 안녕해주세요",
          "original_form": "저를향해 매일 안녕해주세요",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100026046.1.5",
          "form": "흔들흔들",
          "original_form": "흔들흔들",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100026046.1.6",
          "form": "맙소사 온라인으로 홈트 나는 못하겠던데 유튜브보고 하면 할만해?",
          "original_form": "맙소사 온라인으로 홈트 나는 못하겠던데 유튜브보고 하면 할만해?",
          "speaker_id": "1",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100026046.1.7",
          "form": "지금 카톡에 온라인 피트니스서비스 하던데",
          "original_form": "지금 카톡에 온라인 피트니스서비스 하던데",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026046.1.8",
          "form": "근데 정확하게 온라인 피트니스가 뭐냐",
          "original_form": "근데 정확하게 온라인 피트니스가 뭐냐",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026046.1.9",
          "form": "홈트랑 비슷한거가",
          "original_form": "홈트랑 비슷한거가",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026046.1.10",
          "form": "런데이라는 서비스임",
          "original_form": "런데이라는 서비스임",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026046.1.11",
          "form": "카톡에서 하는 온라인서비스이름은 런데이 앱인데 관리해준댕",
          "original_form": "카톡에서 하는 온라인서비스이름은 런데이 앱인데 관리해준댕",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026046.1.12",
          "form": "런데이. ㄷㄷㄷ",
          "original_form": "런데이. ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026046.1.13",
          "form": "빡셀갓같으다",
          "original_form": "빡셀갓같으다",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026046.1.14",
          "form": "지금하믄 카카오임티",
          "original_form": "지금하믄 카카오임티",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026046.1.15",
          "form": "무료로한달쓰는이벤트중",
          "original_form": "무료로한달쓰는이벤트중",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026046.1.16",
          "form": "오 피트니스 서비스에서 이모티콘도 만들었어?",
          "original_form": "오 피트니스 서비스에서 이모티콘도 만들었어?",
          "speaker_id": "1",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100026046.1.17",
          "form": "그럼 그런 헬스녀 캐릭터같은건가",
          "original_form": "그럼 그런 헬스녀 캐릭터같은건가",
          "speaker_id": "1",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100026046.1.18",
          "form": "아니 카톡에서 해",
          "original_form": "아니 카톡에서 해",
          "speaker_id": "2",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026046.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026046.1.20",
          "form": "이모티콘플러스 무료체험 한달 이벤트",
          "original_form": "이모티콘플러스 무료체험 한달 이벤트",
          "speaker_id": "2",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026046.1.21",
          "form": "나는 피트니스 서비스가 PT받으면 주로 식단도 보내고",
          "original_form": "나는 피트니스 서비스가 PT받으면 주로 식단도 보내고",
          "speaker_id": "1",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026046.1.22",
          "form": "오우 본격적이네",
          "original_form": "오우 본격적이네",
          "speaker_id": "2",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026046.1.23",
          "form": "관리를 해주는게 중요한데",
          "original_form": "관리를 해주는게 중요한데",
          "speaker_id": "1",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026046.1.24",
          "form": "나두 안써봐서 구체적으로 몰룽",
          "original_form": "나두 안써봐서 구체적으로 몰룽",
          "speaker_id": "2",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026046.1.25",
          "form": "랜선으로는 부담감을 어케 주냔 말이다",
          "original_form": "랜선으로는 부담감을 어케 주냔 말이다",
          "speaker_id": "1",
          "time": "20210901 20:09"
        }
      ]
