      "utterance": [
        {
          "id": "MDRW2100027902.1.1",
          "form": "안녕하세요. 어떤 봉사활동을 하시나요?",
          "original_form": "안녕하세요. 어떤 봉사활동을 하시나요?",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027902.1.2",
          "form": "안타깝게도 생각은 있는데 못하고 있네요",
          "original_form": "안타깝게도 생각은 있는데 못하고 있네요",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027902.1.3",
          "form": "그나마 매번 헌혈하면서",
          "original_form": "그나마 매번 헌혈하면서",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027902.1.4",
          "form": "대신한다는 생각을 가지고 있긴 한데",
          "original_form": "대신한다는 생각을 가지고 있긴 한데",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027902.1.5",
          "form": "그렇군요 그건 알고 있어요",
          "original_form": "그렇군요 그건 알고 있어요",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027902.1.6",
          "form": "퇴근 언제 하냐고",
          "original_form": "퇴근 언제 하냐고",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027902.1.7",
          "form": "퇴근이요?",
          "original_form": "퇴근이요?",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027902.1.8",
          "form": "네 봉사",
          "original_form": "네 봉사",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027902.1.9",
          "form": "뭐 봉사 하시는거 있나요",
          "original_form": "뭐 봉사 하시는거 있나요",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027902.1.10",
          "form": "동호회라던가",
          "original_form": "동호회라던가",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027902.1.11",
          "form": "저는 저 자신에게 봉사 중이에요",
          "original_form": "저는 저 자신에게 봉사 중이에요",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027902.1.12",
          "form": "아 훌륭한 봉사죠",
          "original_form": "아 훌륭한 봉사죠",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027902.1.13",
          "form": "본래 자기부터 챙겨야 남도 챙길 수 있는게 아니겠습니까",
          "original_form": "본래 자기부터 챙겨야 남도 챙길 수 있는게 아니겠습니까",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027902.1.14",
          "form": "사실 어릴 때는",
          "original_form": "사실 어릴 때는",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027902.1.15",
          "form": "크면 봉사해야지 생각했는데",
          "original_form": "크면 봉사해야지 생각했는데",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027902.1.16",
          "form": "안타깝게도",
          "original_form": "안타깝게도",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027902.1.17",
          "form": "저도 아직까지는 제게 봉사하는 수준을",
          "original_form": "저도 아직까지는 제게 봉사하는 수준을",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027902.1.18",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027902.1.19",
          "form": "아까우니까 나눠서 얘기해 주세요",
          "original_form": "아까우니까 나눠서 얘기해 주세요",
          "speaker_id": "1",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027902.1.20",
          "form": "아 예...",
          "original_form": "아 예...",
          "speaker_id": "2",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100027902.1.21",
          "form": "그러면 동호회는 뭐 생각 있으신거 있나요",
          "original_form": "그러면 동호회는 뭐 생각 있으신거 있나요",
          "speaker_id": "2",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027902.1.22",
          "form": "저는 단체 생활이 싫어요",
          "original_form": "저는 단체 생활이 싫어요",
