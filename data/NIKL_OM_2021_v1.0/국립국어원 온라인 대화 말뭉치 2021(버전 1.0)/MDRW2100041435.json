      "utterance": [
        {
          "id": "MDRW2100041435.1.1",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "1",
          "time": "20210905 23:38"
        },
        {
          "id": "MDRW2100041435.1.2",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210905 23:38"
        },
        {
          "id": "MDRW2100041435.1.3",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "2",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100041435.1.4",
          "form": "네네 직장인이요 ㅠ 이제출근합니당",
          "original_form": "네네 직장인이요 ㅠ 이제출근합니당",
          "speaker_id": "1",
          "time": "20210906 08:49"
        },
        {
          "id": "MDRW2100041435.1.5",
          "form": "저도 출근했어요ㅠㅜ",
          "original_form": "저도 출근했어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210906 09:11"
        },
        {
          "id": "MDRW2100041435.1.6",
          "form": "월요일 너무 힘드네요@@",
          "original_form": "월요일 너무 힘드네요@@",
          "speaker_id": "2",
          "time": "20210906 09:11"
        },
        {
          "id": "MDRW2100041435.1.7",
          "form": "전 오늘 그나마 널널하네용 ..",
          "original_form": "전 오늘 그나마 널널하네용 ..",
          "speaker_id": "1",
          "time": "20210906 10:37"
        },
        {
          "id": "MDRW2100041435.1.8",
          "form": "직장이 어디세요?",
          "original_form": "직장이 어디세요?",
          "speaker_id": "1",
          "time": "20210906 10:37"
        },
        {
          "id": "MDRW2100041435.1.9",
          "form": "저는 공기업 다녀요!",
          "original_form": "저는 공기업 다녀요!",
          "speaker_id": "2",
          "time": "20210906 10:55"
        },
        {
          "id": "MDRW2100041435.1.10",
          "form": "사기업 다니시나여?",
          "original_form": "사기업 다니시나여?",
          "speaker_id": "2",
          "time": "20210906 10:55"
        },
        {
          "id": "MDRW2100041435.1.11",
          "form": "오 저는 치과다녀요",
          "original_form": "오 저는 치과다녀요",
          "speaker_id": "1",
          "time": "20210906 10:55"
        },
        {
          "id": "MDRW2100041435.1.12",
          "form": "공기업 멋지네요",
          "original_form": "공기업 멋지네요",
          "speaker_id": "1",
          "time": "20210906 10:55"
        },
        {
          "id": "MDRW2100041435.1.13",
          "form": "헉 치과도 멋진데요",
          "original_form": "헉 치과도 멋진데요",
          "speaker_id": "2",
          "time": "20210906 10:56"
        },
        {
          "id": "MDRW2100041435.1.14",
          "form": "공기업중에서도 어디다니세용? 궁금해서요 ㅋㅋ",
          "original_form": "공기업중에서도 어디다니세용? 궁금해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 10:56"
        },
        {
          "id": "MDRW2100041435.1.15",
          "form": "ㅋㅋㅋaffiliation이라고...affiliation 계열이예요",
          "original_form": "ㅋㅋㅋ&affiliation&이라고...&affiliation& 계열이예요",
          "speaker_id": "2",
          "time": "20210906 10:56"
        },
        {
          "id": "MDRW2100041435.1.16",
          "form": "근데 전 이직하고 싶어요...",
          "original_form": "근데 전 이직하고 싶어요...",
          "speaker_id": "2",
          "time": "20210906 10:57"
        },
        {
          "id": "MDRW2100041435.1.17",
          "form": "오 affiliation ㅎㅎ이직은 왜용?",
          "original_form": "오 &affiliation& ㅎㅎ이직은 왜용?",
          "speaker_id": "1",
          "time": "20210906 10:58"
        },
        {
          "id": "MDRW2100041435.1.18",
          "form": "좀더 좋은 곳으로 가고싶은...ㅎㅎㅎ",
          "original_form": "좀더 좋은 곳으로 가고싶은...ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:11"
        },
        {
          "id": "MDRW2100041435.1.19",
          "form": "치과 같은 경우도 스케쥴근무인가요?",
          "original_form": "치과 같은 경우도 스케쥴근무인가요?",
          "speaker_id": "2",
          "time": "20210906 11:12"
        },
        {
          "id": "MDRW2100041435.1.20",
          "form": "그것도 아닌게 그냥 무자비로 오는사람들이 워낙많아서요 ..",
          "original_form": "그것도 아닌게 그냥 무자비로 오는사람들이 워낙많아서요 ..",
          "speaker_id": "1",
          "time": "20210906 11:23"
        },
        {
          "id": "MDRW2100041435.1.21",
          "form": "아 그냥 무작정 들어오나여? ㄷㄷ",
          "original_form": "아 그냥 무작정 들어오나여? ㄷㄷ",
          "speaker_id": "2",
          "time": "20210906 11:25"
        },
        {
          "id": "MDRW2100041435.1.22",
          "form": "치과는 예약 안하면 대기도 오래걸리던데...",
          "original_form": "치과는 예약 안하면 대기도 오래걸리던데...",
