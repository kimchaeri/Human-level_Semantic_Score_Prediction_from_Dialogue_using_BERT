      "utterance": [
        {
          "id": "MDRW2100030622.1.1",
          "form": "흐리져?",
          "original_form": "흐리져?",
          "speaker_id": "1",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100030622.1.2",
          "form": "후리죠?",
          "original_form": "후리죠?",
          "speaker_id": "1",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030622.1.3",
          "form": "날씨 좋네요",
          "original_form": "날씨 좋네요",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030622.1.4",
          "form": "그런가여?",
          "original_form": "그런가여?",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030622.1.5",
          "form": "여긴 좋아요",
          "original_form": "여긴 좋아요",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030622.1.6",
          "form": "전형적인가을날씨",
          "original_form": "전형적인가을날씨",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030622.1.7",
          "form": "화창하고 하늘 높고",
          "original_form": "화창하고 하늘 높고",
          "speaker_id": "2",
          "time": "20210904 10:52"
        },
        {
          "id": "MDRW2100030622.1.8",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210904 10:52"
        },
        {
          "id": "MDRW2100030622.1.9",
          "form": "거긴안그래요?",
          "original_form": "거긴안그래요?",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100030622.1.10",
          "form": "전국적으로좋다했는데",
          "original_form": "전국적으로좋다했는데",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100030622.1.11",
          "form": "여긴 아니내여",
          "original_form": "여긴 아니내여",
          "speaker_id": "1",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030622.1.12",
          "form": "아..ㅇ그래요",
          "original_form": "아..ㅇ그래요",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030622.1.13",
          "form": "계속비오다 몇일날씨좋아요",
          "original_form": "계속비오다 몇일날씨좋아요",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030622.1.14",
          "form": "캠핑가고 싶은 날씨요",
          "original_form": "캠핑가고 싶은 날씨요",
          "speaker_id": "2",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030622.1.15",
          "form": "월요일까지는 아주좋대요",
          "original_form": "월요일까지는 아주좋대요",
          "speaker_id": "2",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030622.1.16",
          "form": "그른가요",
          "original_form": "그른가요",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030622.1.17",
          "form": "네..일기예보보니",
          "original_form": "네..일기예보보니",
          "speaker_id": "2",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030622.1.18",
          "form": "그랫으면",
          "original_form": "그랫으면",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030622.1.19",
          "form": "저녁엔추워요",
          "original_form": "저녁엔추워요",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030622.1.20",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100030622.1.21",
          "form": "자켓이필요해요",
          "original_form": "자켓이필요해요",
