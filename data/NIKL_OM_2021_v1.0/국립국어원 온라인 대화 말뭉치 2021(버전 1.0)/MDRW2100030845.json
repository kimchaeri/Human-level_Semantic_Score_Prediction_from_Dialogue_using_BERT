      "utterance": [
        {
          "id": "MDRW2100030845.1.1",
          "form": "그 안에선 퉁퉁이..?",
          "original_form": "그 안에선 퉁퉁이..?",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030845.1.2",
          "form": "아스트리드가 그래서 상당히 독보적인 캐릭터였던거 같아",
          "original_form": "아스트리드가 그래서 상당히 독보적인 캐릭터였던거 같아",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030845.1.3",
          "form": "아 설마 스노트아웃 이야기하는건가",
          "original_form": "아 설마 스노트아웃 이야기하는건가",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030845.1.4",
          "form": "키가 작고",
          "original_form": "키가 작고",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030845.1.5",
          "form": "히컵 사촌인데",
          "original_form": "히컵 사촌인데",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030845.1.6",
          "form": "제일 무대뽀에",
          "original_form": "제일 무대뽀에",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030845.1.7",
          "form": "불타는 용 타고다니는 친구?",
          "original_form": "불타는 용 타고다니는 친구?",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030845.1.8",
          "form": "엥 아니",
          "original_form": "엥 아니",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030845.1.9",
          "form": "피쉬레그",
          "original_form": "피쉬레그",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030845.1.10",
          "form": "영화에선 꽤나 퉁퉁이로 나온 편이지 않아?",
          "original_form": "영화에선 꽤나 퉁퉁이로 나온 편이지 않아?",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030845.1.11",
          "form": "미트러그 타고다니는 애",
          "original_form": "미트러그 타고다니는 애",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030845.1.12",
          "form": "오 아는구만",
          "original_form": "오 아는구만",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030845.1.13",
          "form": "맞아 미트러그 타고 다니는애",
          "original_form": "맞아 미트러그 타고 다니는애",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030845.1.14",
          "form": "ㅇㅇ 그친구 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 그친구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030845.1.15",
          "form": "퉁퉁이라니...",
          "original_form": "퉁퉁이라니...",
          "speaker_id": "1",
          "time": "20210904 14:47"
        },
        {
          "id": "MDRW2100030845.1.16",
          "form": "이친구는 성격 좋다구..",
          "original_form": "이친구는 성격 좋다구..",
          "speaker_id": "1",
          "time": "20210904 14:47"
        },
        {
          "id": "MDRW2100030845.1.17",
          "form": "아니 성격이 아니라 덩치갘ㅋㅋㅋㅋ",
          "original_form": "아니 성격이 아니라 덩치갘ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030845.1.18",
          "form": "음.. 이 동네 친구들이 바이킹 치고 꽤 마른편이라고 생각해",
          "original_form": "음.. 이 동네 친구들이 바이킹 치고 꽤 마른편이라고 생각해",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100030845.1.19",
          "form": "책에서 피시레그는 히컵보다 더 말랐데 ㅋㅋㅋ",
          "original_form": "책에서 피시레그는 히컵보다 더 말랐데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100030845.1.20",
          "form": "영화화되면서 뮤슨 일이 잇엇던거람ㅋㅋㅋㅋ",
          "original_form": "영화화되면서 뮤슨 일이 잇엇던거람ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100030845.1.21",
          "form": "진짜 격변의 드래곤 길들이기야 ㅋㅋㅋ",
          "original_form": "진짜 격변의 드래곤 길들이기야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030845.1.22",
          "form": "책에서는 해적이 되고",
          "original_form": "책에서는 해적이 되고",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030845.1.23",
          "form": "드래곤과 말을 하는 방법을 익히고",
          "original_form": "드래곤과 말을 하는 방법을 익히고",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030845.1.24",
          "form": "어떻게 보면 영화로 나오면 루즈했을 수도 있는 내용일 것 같은데",
          "original_form": "어떻게 보면 영화로 나오면 루즈했을 수도 있는 내용일 것 같은데",
          "speaker_id": "1",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030845.1.25",
          "form": "거기서 크게 세계관을 부수지 않고",
          "original_form": "거기서 크게 세계관을 부수지 않고",
          "speaker_id": "1",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030845.1.26",
          "form": "영화가 잘 만들어진 편인 것 같아",
          "original_form": "영화가 잘 만들어진 편인 것 같아",
          "speaker_id": "1",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030845.1.27",
          "form": "필요한것만 잘 따와서 재구성 잘한듯ㅋㅋㅋㅋ",
          "original_form": "필요한것만 잘 따와서 재구성 잘한듯ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100030845.1.28",
          "form": "ㅇㅇㅇㅇ 진짜로",
          "original_form": "ㅇㅇㅇㅇ 진짜로",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100030845.1.29",
          "form": "원래 터프넛도 쌍둥이가 아니거든",
          "original_form": "원래 터프넛도 쌍둥이가 아니거든",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100030845.1.30",
          "form": "험한말도 못 하는 앤데",
          "original_form": "험한말도 못 하는 앤데",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100030845.1.31",
          "form": "말잇못...",
          "original_form": "말잇못...",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100030845.1.32",
          "form": "그쪽도 완전 반전인뎈ㅋㅋㅋㅋ",
          "original_form": "그쪽도 완전 반전인뎈ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100030845.1.33",
          "form": "그칰ㅋㅋㅋㅋ",
          "original_form": "그칰ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100030845.1.34",
          "form": "캐릭터 이름이랑 드래곤 세계관말고 안변한게 잇나 싶을 정도 ㅋㅋㅋㅋ",
          "original_form": "캐릭터 이름이랑 드래곤 세계관말고 안변한게 잇나 싶을 정도 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100030845.1.35",
          "form": "사실 설정 멀쩡히 그대로 온 친구는 되게 적어",
          "original_form": "사실 설정 멀쩡히 그대로 온 친구는 되게 적어",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100030845.1.36",
          "form": "거의 드래곤 길들이기 스핀오프 같은 느낌이 된",
          "original_form": "거의 드래곤 길들이기 스핀오프 같은 느낌이 된",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100030845.1.37",
          "form": "원작 어디갓냐고 ㅋㅋㅋ",
          "original_form": "원작 어디갓냐고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100030845.1.38",
          "form": "제목? ㅋㅋㅋㅋ",
          "original_form": "제목? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100030845.1.39",
          "form": "그치만 재미있다구요",
          "original_form": "그치만 재미있다구요",
          "speaker_id": "1",
          "time": "20210904 16:31"
        }
      ]
