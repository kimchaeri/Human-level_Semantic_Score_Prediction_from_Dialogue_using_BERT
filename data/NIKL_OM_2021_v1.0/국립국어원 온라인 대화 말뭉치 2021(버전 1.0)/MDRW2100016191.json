      "utterance": [
        {
          "id": "MDRW2100016191.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016191.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016191.1.3",
          "form": "쇼핑 좋아하시나요",
          "original_form": "쇼핑 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100016191.1.4",
          "form": "안좋아해도 살려면 해야죠. 물이나 쌀등은 사야하니까요.ㅋㅋ 님은 좋아하시나요?",
          "original_form": "안좋아해도 살려면 해야죠. 물이나 쌀등은 사야하니까요.ㅋㅋ 님은 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100016191.1.5",
          "form": "네 옷 쇼핑 좋아합니다",
          "original_form": "네 옷 쇼핑 좋아합니다",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016191.1.6",
          "form": "와 그럼 쇼핑이 즐거우시겠어요 전 쇼핑시간이 좀 지겨워요",
          "original_form": "와 그럼 쇼핑이 즐거우시겠어요 전 쇼핑시간이 좀 지겨워요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016191.1.7",
          "form": "저도 좀 짧게 말해야겠어요 ㅋㅋ",
          "original_form": "저도 좀 짧게 말해야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016191.1.8",
          "form": "요즘엔 뭘 사셨나요?",
          "original_form": "요즘엔 뭘 사셨나요?",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016191.1.9",
          "form": "신발이요ㅎㅎㅎ",
          "original_form": "신발이요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100016191.1.10",
          "form": "운동화요? 구두요? 슬리퍼? 종류가 뭐에요?",
          "original_form": "운동화요? 구두요? 슬리퍼? 종류가 뭐에요?",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016191.1.11",
          "form": "운동화 좋아합니다",
          "original_form": "운동화 좋아합니다",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016191.1.12",
          "form": "운동화를 사셨나요?",
          "original_form": "운동화를 사셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016191.1.13",
          "form": "네네 운동화 샀습니다",
          "original_form": "네네 운동화 샀습니다",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016191.1.14",
          "form": "우와 어느 브랜드건가요 궁금해요",
          "original_form": "우와 어느 브랜드건가요 궁금해요",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016191.1.15",
          "form": "마르지엘라 샀어요!",
          "original_form": "마르지엘라 샀어요!",
          "speaker_id": "1",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016191.1.16",
          "form": "우와! 비싼브랜드네요. 전 나이키 정도가 제 최대치 운동화인데.",
          "original_form": "우와! 비싼브랜드네요. 전 나이키 정도가 제 최대치 운동화인데.",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016191.1.17",
          "form": "얼마쯤 하나요?",
          "original_form": "얼마쯤 하나요?",
          "speaker_id": "2",
          "time": "20210826 15:09"
        }
      ]
