        },
        {
          "id": "MDRW2100034384.1.2",
          "form": "화장품 고르는거 도와줘",
          "original_form": "화장품 고르는거 도와줘",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100034384.1.3",
          "form": "기초 야 메이크업이야?",
          "original_form": "기초 야 메이크업이야?",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100034384.1.4",
          "form": "메이크업 종류야",
          "original_form": "메이크업 종류야",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100034384.1.5",
          "form": "어떤 건데",
          "original_form": "어떤 건데",
          "speaker_id": "2",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100034384.1.6",
          "form": "아이 쉐도우",
          "original_form": "아이 쉐도우",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100034384.1.7",
          "form": "만들어진팔레트를 사는 게 낫지 않아?",
          "original_form": "만들어진팔레트를 사는 게 낫지 않아?",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100034384.1.8",
          "form": "그런 것들도 종류가 다양하더라고",
          "original_form": "그런 것들도 종류가 다양하더라고",
          "speaker_id": "1",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100034384.1.9",
          "form": "색깔 너 얼굴 색깔에 어울리는 걸 사야 하겠네",
          "original_form": "색깔 너 얼굴 색깔에 어울리는 걸 사야 하겠네",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100034384.1.10",
          "form": "그럼 분홍색이나 을라나",
          "original_form": "그럼 분홍색이나 을라나",
          "speaker_id": "1",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100034384.1.11",
          "form": "나는 핑크는 싫어해서",
          "original_form": "나는 핑크는 싫어해서",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100034384.1.12",
          "form": "뻘 있는 건 어때",
          "original_form": "뻘 있는 건 어때",
          "speaker_id": "1",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100034384.1.13",
          "form": "언더에 펄을 깔면 예쁘지",
          "original_form": "언더에 펄을 깔면 예쁘지",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100034384.1.14",
          "form": "화사해 보일 것 같아",
          "original_form": "화사해 보일 것 같아",
          "speaker_id": "1",
