      "utterance": [
        {
          "id": "MDRW2100025756.1.1",
          "form": "여행 좋아하나요 ㅎㅎ",
          "original_form": "여행 좋아하나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:45"
        },
        {
          "id": "MDRW2100025756.1.2",
          "form": "최근에는 잘 못 다녔지만 좋아해요",
          "original_form": "최근에는 잘 못 다녔지만 좋아해요",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025756.1.3",
          "form": "기억에 남는 여행지가 있으신가요?",
          "original_form": "기억에 남는 여행지가 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025756.1.4",
          "form": "아무래도 해외가 ㅋㅋㅋ",
          "original_form": "아무래도 해외가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025756.1.5",
          "form": "재작년에 많이 다녀왔는데요ㅠㅠ",
          "original_form": "재작년에 많이 다녀왔는데요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025756.1.6",
          "form": "가장 최근에는 어디 가셨어요?",
          "original_form": "가장 최근에는 어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025756.1.7",
          "form": "저 마지막은 대만 가오슝이요!ㅋㅋ",
          "original_form": "저 마지막은 대만 가오슝이요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025756.1.8",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025756.1.9",
          "form": "대만! 가보고 싶었는데 여태 못 가봤네요",
          "original_form": "대만! 가보고 싶었는데 여태 못 가봤네요",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025756.1.10",
          "form": "진짜 추천해요",
          "original_form": "진짜 추천해요",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100025756.1.11",
          "form": "중국 순한맛 느낌?",
          "original_form": "중국 순한맛 느낌?",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100025756.1.12",
          "form": "순한맛ㅋㅋ 예전에 중국도 가보긴 했는데 앞으로는 가기 좀 그렇더라구요",
          "original_form": "순한맛ㅋㅋ 예전에 중국도 가보긴 했는데 앞으로는 가기 좀 그렇더라구요",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100025756.1.13",
          "form": "네 그래서 대만 추천드립니다ㅋㅋ",
          "original_form": "네 그래서 대만 추천드립니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100025756.1.14",
          "form": "사람들도 착하고 영어도 좀 하고",
          "original_form": "사람들도 착하고 영어도 좀 하고",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100025756.1.15",
          "form": "물가도 싸고 ㅋㅋ",
          "original_form": "물가도 싸고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100025756.1.16",
          "form": "코로나 끝나고 해외 갈 수 있게 되면 가깝기도 하고 가보고 싶네요",
          "original_form": "코로나 끝나고 해외 갈 수 있게 되면 가깝기도 하고 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100025756.1.17",
          "form": "네 교통도 편리하구",
          "original_form": "네 교통도 편리하구",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100025756.1.18",
          "form": "홍콩이 좋았는데요 진짜",
          "original_form": "홍콩이 좋았는데요 진짜",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100025756.1.19",
          "form": "예전같지 않겠죠ㅠ",
          "original_form": "예전같지 않겠죠ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100025756.1.20",
          "form": "네 그래서 문제입니다 흑흑",
          "original_form": "네 그래서 문제입니다 흑흑",
          "speaker_id": "1",
          "time": "20210901 21:26"
