      "utterance": [
        {
          "id": "MDRW2100009597.1.1",
          "form": "실습 학교로 갔는데",
          "original_form": "실습 학교로 갔는데",
          "speaker_id": "2",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009597.1.2",
          "form": "제가 배정된 반 담임이",
          "original_form": "제가 배정된 반 담임이",
          "speaker_id": "2",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009597.1.3",
          "form": "진짜 이상했어요",
          "original_form": "진짜 이상했어요",
          "speaker_id": "2",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009597.1.4",
          "form": "어땠는데요",
          "original_form": "어땠는데요",
          "speaker_id": "1",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009597.1.5",
          "form": "학교폭력을 방관해요",
          "original_form": "학교폭력을 방관해요",
          "speaker_id": "2",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009597.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009597.1.7",
          "form": "피해자가 잘못이라고 생각하고요",
          "original_form": "피해자가 잘못이라고 생각하고요",
          "speaker_id": "2",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009597.1.8",
          "form": "그럼 안되잖아요",
          "original_form": "그럼 안되잖아요",
          "speaker_id": "1",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009597.1.9",
          "form": "그래서 막지도 않고",
          "original_form": "그래서 막지도 않고",
          "speaker_id": "2",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009597.1.10",
          "form": "와 진짜 너무하네",
          "original_form": "와 진짜 너무하네",
          "speaker_id": "1",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.11",
          "form": "당연히 절대 안돼요",
          "original_form": "당연히 절대 안돼요",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.12",
          "form": "10살이 잘못 되어 봤자",
          "original_form": "10살이 잘못 되어 봤자",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.13",
          "form": "뭔 잘못이 있겠어요",
          "original_form": "뭔 잘못이 있겠어요",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.14",
          "form": "2주간 저랑",
          "original_form": "2주간 저랑",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.15",
          "form": "실습 같이 간 메이트들이",
          "original_form": "실습 같이 간 메이트들이",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.16",
          "form": "진짜 힘들었어요",
          "original_form": "진짜 힘들었어요",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.17",
          "form": "저희가 할 수 있는 게 없으니까요",
          "original_form": "저희가 할 수 있는 게 없으니까요",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.18",
          "form": "그럴거같아요",
          "original_form": "그럴거같아요",
          "speaker_id": "1",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.19",
          "form": "고작 실습생이라서",
          "original_form": "고작 실습생이라서",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009597.1.20",
          "form": "담임이",
          "original_form": "담임이",
          "speaker_id": "2",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.21",
          "form": "군인 출신인데",
          "original_form": "군인 출신인데",
          "speaker_id": "2",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.22",
          "form": "남자였구나",
          "original_form": "남자였구나",
          "speaker_id": "1",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.23",
          "form": "지금까지 섬에 있는 분교에만 있었대요",
          "original_form": "지금까지 섬에 있는 분교에만 있었대요",
          "speaker_id": "2",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.24",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.25",
          "form": "전교생 2명이니까",
          "original_form": "전교생 2명이니까",
          "speaker_id": "2",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.26",
          "form": "교사도 자기 뿐이었나봐요",
          "original_form": "교사도 자기 뿐이었나봐요",
          "speaker_id": "2",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.27",
          "form": "알것같은 전개",
          "original_form": "알것같은 전개",
          "speaker_id": "1",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.28",
          "form": "그래서 제지가 없었던 거죠",
          "original_form": "그래서 제지가 없었던 거죠",
          "speaker_id": "2",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.29",
          "form": "그 꼴이 될 때까지",
          "original_form": "그 꼴이 될 때까지",
          "speaker_id": "2",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.30",
          "form": "저는 경기였는데",
          "original_form": "저는 경기였는데",
          "speaker_id": "2",
          "time": "20210823 01:21"
        },
        {
          "id": "MDRW2100009597.1.31",
          "form": "진짜 선생이란 단어에 무게를 크게두고싶지않은데",
          "original_form": "진짜 선생이란 단어에 무게를 크게두고싶지않은데",
          "speaker_id": "1",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.32",
          "form": "저 초등학교 때도 체벌 없었거든요",
          "original_form": "저 초등학교 때도 체벌 없었거든요",
          "speaker_id": "2",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.33",
          "form": "본인이 할 일은 해야지",
          "original_form": "본인이 할 일은 해야지",
          "speaker_id": "1",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.34",
          "form": "근데 아직도 애들 잘못하면 벌 세우고",
          "original_form": "근데 아직도 애들 잘못하면 벌 세우고",
          "speaker_id": "2",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.35",
          "form": "체벌하던가요",
          "original_form": "체벌하던가요",
          "speaker_id": "1",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.36",
          "form": "와 믿을 수 가 없다",
          "original_form": "와 믿을 수 가 없다",
          "speaker_id": "1",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.37",
          "form": "때리지는 않는데",
          "original_form": "때리지는 않는데",
          "speaker_id": "2",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.38",
          "form": "오리걸음 이런 거 시켜요",
          "original_form": "오리걸음 이런 거 시켜요",
          "speaker_id": "2",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.39",
          "form": "애들이 싸우면",
          "original_form": "애들이 싸우면",
          "speaker_id": "2",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.40",
          "form": "말리는 게 아니라",
          "original_form": "말리는 게 아니라",
          "speaker_id": "2",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.41",
          "form": "요즘세상에",
          "original_form": "요즘세상에",
          "speaker_id": "1",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.42",
          "form": "와 진짜 그런 사람도 교사라고",
          "original_form": "와 진짜 그런 사람도 교사라고",
          "speaker_id": "1",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009597.1.43",
          "form": "소란스럽다는 것에 중점 맞춰서 혼내고",
          "original_form": "소란스럽다는 것에 중점 맞춰서 혼내고",
          "speaker_id": "2",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009597.1.44",
          "form": "애들 갈등 해결 안 하고",
          "original_form": "애들 갈등 해결 안 하고",
          "speaker_id": "2",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009597.1.45",
          "form": "소란 만들었다는 이유로",
          "original_form": "소란 만들었다는 이유로",
          "speaker_id": "2",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009597.1.46",
          "form": "교실 앞에서",
          "original_form": "교실 앞에서",
          "speaker_id": "2",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009597.1.47",
          "form": "손 들고 서있게 하고",
          "original_form": "손 들고 서있게 하고",
          "speaker_id": "2",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009597.1.48",
          "form": "실습 3번째였거든요",
          "original_form": "실습 3번째였거든요",
          "speaker_id": "2",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009597.1.49",
          "form": "와 진짜 민원 안들어가나",
          "original_form": "와 진짜 민원 안들어가나",
          "speaker_id": "1",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009597.1.50",
          "form": "그런 교사는 처음이었어요",
          "original_form": "그런 교사는 처음이었어요",
          "speaker_id": "2",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009597.1.51",
          "form": "힘드셨겠어요ㅠ",
          "original_form": "힘드셨겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 01:23"
        }
      ]
    }
  ]
