      "utterance": [
        {
          "id": "MDRW2100041690.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041690.1.2",
          "form": "제주도에 사시는 건가요?",
          "original_form": "제주도에 사시는 건가요?",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041690.1.3",
          "form": "네에 지금은 육지옝6",
          "original_form": "네에 지금은 육지옝6",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041690.1.4",
          "form": "예요",
          "original_form": "예요",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041690.1.5",
          "form": "내일 돌아가요",
          "original_form": "내일 돌아가요",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041690.1.6",
          "form": "오오!! 단기로 살다가 돌아가시는 거예요?",
          "original_form": "오오!! 단기로 살다가 돌아가시는 거예요?",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041690.1.7",
          "form": "내년 3월까지 여행하면서 지내려구요",
          "original_form": "내년 3월까지 여행하면서 지내려구요",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041690.1.8",
          "form": "우와 여행도 그럼 많이 다니시나요 아님 현지인처럼 계시나요 ㅎㄹ",
          "original_form": "우와 여행도 그럼 많이 다니시나요 아님 현지인처럼 계시나요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041690.1.9",
          "form": "지금은 온지 얼마 안되서",
          "original_form": "지금은 온지 얼마 안되서",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041690.1.10",
          "form": "배낭여행객처럼 살아요",
          "original_form": "배낭여행객처럼 살아요",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041690.1.11",
          "form": "오 멋있어요",
          "original_form": "오 멋있어요",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041690.1.12",
          "form": "제주 살면서 생각못했던 부분 그런 게 있을까요?",
          "original_form": "제주 살면서 생각못했던 부분 그런 게 있을까요?",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041690.1.13",
          "form": "습도가 심하고 생각보다 날씨가 좋은 날이 별로 없어요",
          "original_form": "습도가 심하고 생각보다 날씨가 좋은 날이 별로 없어요",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041690.1.14",
          "form": "아 맞아요 비가 갑자기 엄청 내리거나 그렇죠?",
          "original_form": "아 맞아요 비가 갑자기 엄청 내리거나 그렇죠?",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041690.1.15",
          "form": "네 섬나라",
          "original_form": "네 섬나라",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041690.1.16",
          "form": "습도가 힘들어요",
          "original_form": "습도가 힘들어요",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041690.1.17",
          "form": "그리고 물가비싸요",
          "original_form": "그리고 물가비싸요",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041690.1.18",
          "form": "채소 과일 육지물품",
          "original_form": "채소 과일 육지물품",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041690.1.19",
          "form": "아 역시.. 아무래도 섬이라 그런게 있겠네요",
          "original_form": "아 역시.. 아무래도 섬이라 그런게 있겠네요",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041690.1.20",
          "form": "택배도 비싸죠?",
          "original_form": "택배도 비싸죠?",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041690.1.21",
          "form": "네 무조건 3000원 더",
          "original_form": "네 무조건 3000원 더",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041690.1.22",
          "form": "퓨",
          "original_form": "퓨",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041690.1.23",
          "form": "생활에 들어가는 비용이 꽤 나가겠네요 생각보다",
          "original_form": "생활에 들어가는 비용이 꽤 나가겠네요 생각보다",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041690.1.24",
          "form": "네에 그래서 당근마켓이 잘 되는 듯 해요",
          "original_form": "네에 그래서 당근마켓이 잘 되는 듯 해요",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041690.1.25",
          "form": "아 대박 그런 장점이 있네요 ㅋㅋㅋ",
          "original_form": "아 대박 그런 장점이 있네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041690.1.26",
          "form": "그래도 날씨봐서 오름 해변 정해서 버스타고 갈수있는게 어디예요",
          "original_form": "그래도 날씨봐서 오름 해변 정해서 버스타고 갈수있는게 어디예요",
          "speaker_id": "2",
          "time": "20210906 14:40"
        }
      ]
