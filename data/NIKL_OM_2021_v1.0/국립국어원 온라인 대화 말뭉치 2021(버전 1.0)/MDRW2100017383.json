      "utterance": [
        {
          "id": "MDRW2100017383.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017383.1.2",
          "form": "운동 좋아하지는 않는데",
          "original_form": "운동 좋아하지는 않는데",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017383.1.3",
          "form": "요즘은 꾸준히 하려고 노력중이예요",
          "original_form": "요즘은 꾸준히 하려고 노력중이예요",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017383.1.4",
          "form": "저도 안 좋아하는데 운동해야 살겠더라구요ㅋㅋ",
          "original_form": "저도 안 좋아하는데 운동해야 살겠더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017383.1.5",
          "form": "맞아요 건강을 위해서",
          "original_form": "맞아요 건강을 위해서",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017383.1.6",
          "form": "무슨 운동하세요?",
          "original_form": "무슨 운동하세요?",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017383.1.7",
          "form": "저는 필라테스 배우는데",
          "original_form": "저는 필라테스 배우는데",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017383.1.8",
          "form": "이제 한 1년 됐어요!",
          "original_form": "이제 한 1년 됐어요!",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017383.1.9",
          "form": "오 저도 필라테스!",
          "original_form": "오 저도 필라테스!",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017383.1.10",
          "form": "저는 8개월차입니당",
          "original_form": "저는 8개월차입니당",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017383.1.11",
          "form": "우와 필라테스 동지!",
          "original_form": "우와 필라테스 동지!",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017383.1.12",
          "form": "왜이렇게 막살았나 늘 후회하고 오거든요",
          "original_form": "왜이렇게 막살았나 늘 후회하고 오거든요",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017383.1.13",
          "form": "네 저도 필라테스 할 때마다",
          "original_form": "네 저도 필라테스 할 때마다",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017383.1.14",
          "form": "지난 저의 삶과 망가진 몸에 대한 반성의 시간을ㅠ",
          "original_form": "지난 저의 삶과 망가진 몸에 대한 반성의 시간을ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017383.1.15",
          "form": "그래도 필라테스 하고나면 몸이 좀 가벼워진 느낌이라 좋더라고요",
          "original_form": "그래도 필라테스 하고나면 몸이 좀 가벼워진 느낌이라 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017383.1.16",
          "form": "마자요 여태 배운 것 중에 젤 좋은 운동 같아요",
          "original_form": "마자요 여태 배운 것 중에 젤 좋은 운동 같아요",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017383.1.17",
          "form": "실제로 주변에서도 효과를 많이 알아채주시더라구요",
          "original_form": "실제로 주변에서도 효과를 많이 알아채주시더라구요",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017383.1.18",
          "form": "오호 진짜 효과가 있으신가보다",
          "original_form": "오호 진짜 효과가 있으신가보다",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017383.1.19",
          "form": "저는 사실 그렇게 드라마틱한 변화는 없어서ㅠ",
          "original_form": "저는 사실 그렇게 드라마틱한 변화는 없어서ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017383.1.20",
          "form": "원래 날씬하셨나봐요",
          "original_form": "원래 날씬하셨나봐요",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017383.1.21",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017383.1.22",
          "form": "꾸준히 통통해요ㅜ",
          "original_form": "꾸준히 통통해요ㅜ",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017383.1.23",
          "form": "앜 저도 최고치 몸무게 찍다가 한 4킬로 정도 빠진것 같아요",
          "original_form": "앜 저도 최고치 몸무게 찍다가 한 4킬로 정도 빠진것 같아요",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017383.1.24",
          "form": "오호 좋네요",
          "original_form": "오호 좋네요",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017383.1.25",
          "form": "저는 몸무게는 줄지 않고",
          "original_form": "저는 몸무게는 줄지 않고",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017383.1.26",
          "form": "라인만 조금씩 정리되는?",
          "original_form": "라인만 조금씩 정리되는?",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017383.1.27",
          "form": "오 맞아요 라인 정리 되는게 신기해요 진짜",
          "original_form": "오 맞아요 라인 정리 되는게 신기해요 진짜",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017383.1.28",
          "form": "근데 저는 유연성이 부족해서 8개월 차인데 아직 못 따라가는 동작들이 있어요ㅎㅎ",
          "original_form": "근데 저는 유연성이 부족해서 8개월 차인데 아직 못 따라가는 동작들이 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:16"
        }
      ]
