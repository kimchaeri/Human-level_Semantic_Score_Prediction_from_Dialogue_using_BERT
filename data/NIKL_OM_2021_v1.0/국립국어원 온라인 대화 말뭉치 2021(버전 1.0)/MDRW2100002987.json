      "utterance": [
        {
          "id": "MDRW2100002987.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002987.1.2",
          "form": "혼자 사시나요",
          "original_form": "혼자 사시나요",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002987.1.3",
          "form": "아니요 하숙하고 있어요",
          "original_form": "아니요 하숙하고 있어요",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002987.1.4",
          "form": "저는 혼자 투룸에 살아요",
          "original_form": "저는 혼자 투룸에 살아요",
          "speaker_id": "1",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002987.1.5",
          "form": "집안일은 누가하나요?",
          "original_form": "집안일은 누가하나요?",
          "speaker_id": "1",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002987.1.6",
          "form": "집주인이 해요",
          "original_form": "집주인이 해요",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002987.1.7",
          "form": "하숙은 그런점이 좋군요",
          "original_form": "하숙은 그런점이 좋군요",
          "speaker_id": "1",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002987.1.8",
          "form": "확실히 좋아요",
          "original_form": "확실히 좋아요",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002987.1.9",
          "form": "예전에 투룸에 살았는데",
          "original_form": "예전에 투룸에 살았는데",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002987.1.10",
          "form": "청소하기 귀찮아서 깨끗하게 살았어요",
          "original_form": "청소하기 귀찮아서 깨끗하게 살았어요",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002987.1.11",
          "form": "저는 혼자 살다보니 집이 엉망이예요",
          "original_form": "저는 혼자 살다보니 집이 엉망이예요",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002987.1.12",
          "form": "원룸에 살때는",
          "original_form": "원룸에 살때는",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002987.1.13",
          "form": "옷정리가 가장 귀찮아요",
          "original_form": "옷정리가 가장 귀찮아요",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002987.1.14",
          "form": "진짜 엉망이었어요",
          "original_form": "진짜 엉망이었어요",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002987.1.15",
          "form": "옷도 안사고 막",
          "original_form": "옷도 안사고 막",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002987.1.16",
          "form": "저만 그런게 아니군요",
          "original_form": "저만 그런게 아니군요",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002987.1.17",
          "form": "집안일 귀찮아요",
          "original_form": "집안일 귀찮아요",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002987.1.18",
          "form": "요즘엔 가사도우미 불러주는 어플도 있더라구요",
          "original_form": "요즘엔 가사도우미 불러주는 어플도 있더라구요",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002987.1.19",
          "form": "저 원룸살때 진짜 부르려고 했어요",
          "original_form": "저 원룸살때 진짜 부르려고 했어요",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002987.1.20",
          "form": "친구가 방크기에 비해 오바라고 해서 안했지만",
          "original_form": "친구가 방크기에 비해 오바라고 해서 안했지만",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002987.1.21",
          "form": "화장실 청소도 너무 힘들어요",
          "original_form": "화장실 청소도 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002987.1.22",
          "form": "남자친구를 불러서 시키곤 한답니다",
          "original_form": "남자친구를 불러서 시키곤 한답니다",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002987.1.23",
          "form": "그거 편하겠네요",
          "original_form": "그거 편하겠네요",
          "speaker_id": "2",
          "time": "20210820 17:23"
        }
