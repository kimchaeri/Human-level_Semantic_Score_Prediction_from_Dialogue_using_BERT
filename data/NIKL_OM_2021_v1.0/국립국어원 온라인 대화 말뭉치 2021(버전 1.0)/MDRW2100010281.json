          "speaker_id": "1",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010281.1.8",
          "form": "곧가을인가봐요",
          "original_form": "곧가을인가봐요",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010281.1.9",
          "form": "그런가봐요 기온이",
