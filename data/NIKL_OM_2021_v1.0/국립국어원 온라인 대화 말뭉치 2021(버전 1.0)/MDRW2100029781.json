      "utterance": [
        {
          "id": "MDRW2100029781.1.1",
          "form": "학생이신가요??",
          "original_form": "학생이신가요??",
          "speaker_id": "1",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100029781.1.2",
          "form": "네네 저 아직 학생이에요",
          "original_form": "네네 저 아직 학생이에요",
          "speaker_id": "2",
          "time": "20210903 13:30"
        },
        {
          "id": "MDRW2100029781.1.3",
          "form": "개강하셨어요??",
          "original_form": "개강하셨어요??",
          "speaker_id": "1",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100029781.1.4",
          "form": "어저께 했어요",
          "original_form": "어저께 했어요",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100029781.1.5",
          "form": "요즘은 코로나때문에 비대면도 하고 실강도 하더라구요",
          "original_form": "요즘은 코로나때문에 비대면도 하고 실강도 하더라구요",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029781.1.6",
          "form": "저도 이번주에 했어요....",
          "original_form": "저도 이번주에 했어요....",
          "speaker_id": "1",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029781.1.7",
          "form": "저는 중간고사까지는 다 비대면이에요!",
          "original_form": "저는 중간고사까지는 다 비대면이에요!",
          "speaker_id": "1",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029781.1.8",
          "form": "오늘은 수업 없으신가요??",
          "original_form": "오늘은 수업 없으신가요??",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029781.1.9",
          "form": "아침에 하나했어요",
          "original_form": "아침에 하나했어요",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029781.1.10",
          "form": "저희 교수님...너무 수업이 아침에 몰려있어요 ㅠ",
          "original_form": "저희 교수님...너무 수업이 아침에 몰려있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029781.1.11",
          "form": "어우.... 너무 싫으시겠어요",
          "original_form": "어우.... 너무 싫으시겠어요",
          "speaker_id": "1",
          "time": "20210903 13:38"
        },
        {
          "id": "MDRW2100029781.1.12",
          "form": "대면으로 하셨나요??",
          "original_form": "대면으로 하셨나요??",
          "speaker_id": "1",
          "time": "20210903 13:38"
        },
        {
          "id": "MDRW2100029781.1.13",
          "form": "비대면인데 화상강의예요 ㅠㅠ",
          "original_form": "비대면인데 화상강의예요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100029781.1.14",
          "form": "앗 다행인듯 별로네요..ㅎㅎ",
          "original_form": "앗 다행인듯 별로네요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029781.1.15",
          "form": "화상강의 얼굴 키라하는게 너무 스트레스에요....",
          "original_form": "화상강의 얼굴 키라하는게 너무 스트레스에요....",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029781.1.16",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "2",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029781.1.17",
          "form": "저 그래서 노트북 카메라 부분에 투명테이프 조금 붙여놔요",
          "original_form": "저 그래서 노트북 카메라 부분에 투명테이프 조금 붙여놔요",
          "speaker_id": "2",
          "time": "20210903 13:42"
        },
        {
          "id": "MDRW2100029781.1.18",
          "form": "그럼 화질도 흐릿하게 나오고 해서... 교수님이 지적하스면 카메라가 좀 오래됐다고 둘러대구요",
          "original_form": "그럼 화질도 흐릿하게 나오고 해서... 교수님이 지적하스면 카메라가 좀 오래됐다고 둘러대구요",
          "speaker_id": "2",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100029781.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 꿀팁이네요! 저도 투명 테이프좀 붙여놔야겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 꿀팁이네요! 저도 투명 테이프좀 붙여놔야겠어요",
          "speaker_id": "1",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029781.1.20",
          "form": "수강신청은 올클하셨어요??",
          "original_form": "수강신청은 올클하셨어요??",
          "speaker_id": "1",
          "time": "20210903 13:45"
        },
        {
          "id": "MDRW2100029781.1.21",
          "form": "저.......11학점밖에 신청못했어요..........",
          "original_form": "저.......11학점밖에 신청못했어요..........",
          "speaker_id": "2",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100029781.1.22",
          "form": "더 해야했는데ㅠㅠ",
          "original_form": "더 해야했는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100029781.1.23",
          "form": "앗... 정정때 꼭 주우실수있을거에요!",
          "original_form": "앗... 정정때 꼭 주우실수있을거에요!",
          "speaker_id": "1",
          "time": "20210903 13:48"
        },
        {
          "id": "MDRW2100029781.1.24",
          "form": "열심히 주워야죠...",
          "original_form": "열심히 주워야죠...",
          "speaker_id": "2",
          "time": "20210903 13:49"
        },
        {
          "id": "MDRW2100029781.1.25",
          "form": "수강신청...너무 열받는게... 전공인데 인원이 다 차서 신청할 수 없다고 하는 거예요",
          "original_form": "수강신청...너무 열받는게... 전공인데 인원이 다 차서 신청할 수 없다고 하는 거예요",
          "speaker_id": "2",
          "time": "20210903 13:49"
        }
      ]
