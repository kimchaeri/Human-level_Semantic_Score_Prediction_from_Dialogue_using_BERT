        },
        {
          "id": "MDRW2100035078.1.2",
          "form": "내일 스타벅스 갈까",
          "original_form": "내일 스타벅스 갈까",
          "speaker_id": "1",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100035078.1.3",
          "form": "스타벅스 음료 뭐 좋아하는데",
          "original_form": "스타벅스 음료 뭐 좋아하는데",
          "speaker_id": "2",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100035078.1.4",
          "form": "스타벅스에 신메뉴가 나왔던데",
          "original_form": "스타벅스에 신메뉴가 나왔던데",
          "speaker_id": "1",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100035078.1.5",
          "form": "스타벅스 음료는 신메뉴가 좋아",
          "original_form": "스타벅스 음료는 신메뉴가 좋아",
          "speaker_id": "1",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100035078.1.6",
          "form": "이번 신메뉴는 뭐야",
          "original_form": "이번 신메뉴는 뭐야",
          "speaker_id": "2",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100035078.1.7",
          "form": "흑임자 맛 음료야",
          "original_form": "흑임자 맛 음료야",
          "speaker_id": "1",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100035078.1.8",
          "form": "흑임자 만 음료가 요즘 유행이네",
          "original_form": "흑임자 만 음료가 요즘 유행이네",
          "speaker_id": "2",
          "time": "20210829 02:10"
        },
        {
          "id": "MDRW2100035078.1.9",
          "form": "맞아 강릉이 원조지",
          "original_form": "맞아 강릉이 원조지",
          "speaker_id": "1",
          "time": "20210829 02:10"
        },
        {
          "id": "MDRW2100035078.1.10",
          "form": "강릉에서 마셔본 적도 있어",
          "original_form": "강릉에서 마셔본 적도 있어",
          "speaker_id": "2",
          "time": "20210829 02:10"
        },
        {
          "id": "MDRW2100035078.1.11",
          "form": "강릉은 순두부도 유명하던데",
          "original_form": "강릉은 순두부도 유명하던데",
          "speaker_id": "1",
          "time": "20210829 02:10"
        },
        {
          "id": "MDRW2100035078.1.12",
          "form": "맞아 강원도 가면 두부 많이 먹지",
          "original_form": "맞아 강원도 가면 두부 많이 먹지",
          "speaker_id": "2",
          "time": "20210829 02:10"
        },
        {
          "id": "MDRW2100035078.1.13",
          "form": "두부 아이스크림도 있다고 들었어",
          "original_form": "두부 아이스크림도 있다고 들었어",
          "speaker_id": "1",
          "time": "20210829 02:10"
        },
        {
          "id": "MDRW2100035078.1.14",
          "form": "초당 아이스크림 맛있어 보이긴 하더라",
          "original_form": "초당 아이스크림 맛있어 보이긴 하더라",
          "speaker_id": "2",
          "time": "20210829 02:11"
        },
        {
          "id": "MDRW2100035078.1.15",
          "form": "난 별로 안궁금해",
          "original_form": "난 별로 안궁금해",
