      "utterance": [
        {
          "id": "MDRW2100027812.1.1",
          "form": "너 휴학하니",
          "original_form": "너 휴학하니",
          "speaker_id": "2",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027812.1.2",
          "form": "잘못하면 하게 될지도",
          "original_form": "잘못하면 하게 될지도",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027812.1.3",
          "form": "수강신청 망해서",
          "original_form": "수강신청 망해서",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027812.1.4",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027812.1.5",
          "form": "근데 수강신청 지난 거 아냐?",
          "original_form": "근데 수강신청 지난 거 아냐?",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027812.1.6",
          "form": "정정기간인가",
          "original_form": "정정기간인가",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027812.1.7",
          "form": "아직 정정기간임",
          "original_form": "아직 정정기간임",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027812.1.8",
          "form": "근데 못 잡았구나",
          "original_form": "근데 못 잡았구나",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027812.1.9",
          "form": "하나라도 더 잡아야 하는데...",
          "original_form": "하나라도 더 잡아야 하는데...",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027812.1.10",
          "form": "자리가 없어..........",
          "original_form": "자리가 없어..........",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027812.1.11",
          "form": "아니 대체 전공 티오가 왜 그모양인",
          "original_form": "아니 대체 전공 티오가 왜 그모양인",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027812.1.12",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027812.1.13",
          "form": "원투데이도 아닌데",
          "original_form": "원투데이도 아닌데",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027812.1.14",
          "form": "왜 이러는 걸까",
          "original_form": "왜 이러는 걸까",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027812.1.15",
          "form": "수업이라도 늘려달라고",
          "original_form": "수업이라도 늘려달라고",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027812.1.16",
          "form": "분반 좀 늘려보라고",
          "original_form": "분반 좀 늘려보라고",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027812.1.17",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027812.1.18",
          "form": "으아악 전공 수업 내놔",
          "original_form": "으아악 전공 수업 내놔",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027812.1.19",
          "form": "역시 휴학이 답인가...",
          "original_form": "역시 휴학이 답인가...",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027812.1.20",
          "form": "졸업필수과목티오가 극악인 수업이 싫다",
          "original_form": "졸업필수과목티오가 극악인 수업이 싫다",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027812.1.21",
          "form": "정정서라도 받아줘",
          "original_form": "정정서라도 받아줘",
