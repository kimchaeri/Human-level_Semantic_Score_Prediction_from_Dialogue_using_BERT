      "utterance": [
        {
          "id": "MDRW2100037273.1.1",
          "form": "남자가 사준 선물을 전시하면서",
          "original_form": "남자가 사준 선물을 전시하면서",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100037273.1.2",
          "form": "그걸로 자존감 채우는 애들이요...",
          "original_form": "그걸로 자존감 채우는 애들이요...",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100037273.1.3",
          "form": "나 이렇게 사랑받아 이거요? ㅋㅋ",
          "original_form": "나 이렇게 사랑받아 이거요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.4",
          "form": "저도싫어요 ㅋㅋ",
          "original_form": "저도싫어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.5",
          "form": "어 맞아요!!!!!!!",
          "original_form": "어 맞아요!!!!!!!",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.6",
          "form": "근데 그런 경우는 진짜",
          "original_form": "근데 그런 경우는 진짜",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.7",
          "form": "저도 딱 한 번 봤는데",
          "original_form": "저도 딱 한 번 봤는데",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.8",
          "form": "친구가 연대생이고",
          "original_form": "친구가 연대생이고",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.9",
          "form": "서울 살고",
          "original_form": "서울 살고",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.10",
          "form": "자꾸 남한테 해달라고 하구요",
          "original_form": "자꾸 남한테 해달라고 하구요",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.11",
          "form": "남자는 자기가 여자보다 더 떨어진다고 생각했는지",
          "original_form": "남자는 자기가 여자보다 더 떨어진다고 생각했는지",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.12",
          "form": "그런애들은 여자도 시켜먹어요",
          "original_form": "그런애들은 여자도 시켜먹어요",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.13",
          "form": "아니면 취장을 가려는건지",
          "original_form": "아니면 취장을 가려는건지",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.14",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037273.1.15",
          "form": "진짜 이런 말은 쓰기 싫은데 정말 공주병이에요ㅠ",
          "original_form": "진짜 이런 말은 쓰기 싫은데 정말 공주병이에요ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037273.1.16",
          "form": "그리고 얘도 친구한테 돈을 안썼어욬 ㅋㅋㅋㅋㅋ",
          "original_form": "그리고 얘도 친구한테 돈을 안썼어욬 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037273.1.17",
          "form": "그러면서 하는 말이",
          "original_form": "그러면서 하는 말이",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037273.1.18",
          "form": "자기는 남친이 다 사주고 해주니까",
          "original_form": "자기는 남친이 다 사주고 해주니까",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037273.1.19",
          "form": "다른 사람한테 베푸는 습관을 까먹었다고 하더라구요",
          "original_form": "다른 사람한테 베푸는 습관을 까먹었다고 하더라구요",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037273.1.20",
          "form": "말도안돼요 넘 재수없네요",
          "original_form": "말도안돼요 넘 재수없네요",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037273.1.21",
          "form": "근데 그렇게 자꾸 누가 해버릇하는 사람은",
          "original_form": "근데 그렇게 자꾸 누가 해버릇하는 사람은",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037273.1.22",
          "form": "티나긴 했어요ㅠ정말로요",
          "original_form": "티나긴 했어요ㅠ정말로요",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037273.1.23",
          "form": "아무한테나 그래요",
          "original_form": "아무한테나 그래요",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037273.1.24",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037273.1.25",
          "form": "맞아요 원래도 그랬을거예요",
          "original_form": "맞아요 원래도 그랬을거예요",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037273.1.26",
          "form": "그리고 정말 친해지기 어려운 또 다른 부류는",
          "original_form": "그리고 정말 친해지기 어려운 또 다른 부류는",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.27",
          "form": "그런게 사랑스러운거다 약간 그런식으로;;;된듯요",
          "original_form": "그런게 사랑스러운거다 약간 그런식으로;;;된듯요",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.28",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.29",
          "form": "전문직 남자랑결혼한걸 너무 자랑스러워하는 부류...",
          "original_form": "전문직 남자랑결혼한걸 너무 자랑스러워하는 부류...",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.30",
          "form": "네네 ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "네네 ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.31",
          "form": "아아 자랑스럽긴 하겠어요 ㅋㅋ",
          "original_form": "아아 자랑스럽긴 하겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.32",
          "form": "저 진짜 대박인거 봤는데",
          "original_form": "저 진짜 대박인거 봤는데",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.33",
          "form": "저희사촌언니도 그래요",
          "original_form": "저희사촌언니도 그래요",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.34",
          "form": "고모가 맨날 자랑해요",
          "original_form": "고모가 맨날 자랑해요",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.35",
          "form": "결혼사진 중하나를",
          "original_form": "결혼사진 중하나를",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.36",
          "form": "여자는 헐벗은 웨딩드레스",
          "original_form": "여자는 헐벗은 웨딩드레스",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037273.1.37",
          "form": "남자는 의사가운에 청진기",
          "original_form": "남자는 의사가운에 청진기",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037273.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 이게 뭐냐구요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 이게 뭐냐구요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
