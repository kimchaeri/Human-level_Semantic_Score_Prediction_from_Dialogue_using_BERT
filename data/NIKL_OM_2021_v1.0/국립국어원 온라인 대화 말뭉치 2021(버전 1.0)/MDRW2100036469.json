      "utterance": [
        {
          "id": "MDRW2100036469.1.1",
          "form": "즐겨보시는 유튜브 채널 있으세요?",
          "original_form": "즐겨보시는 유튜브 채널 있으세요?",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036469.1.2",
          "form": "저는 게임 좋아해서 김실장채널자주보구요",
          "original_form": "저는 게임 좋아해서 김실장채널자주보구요",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036469.1.3",
          "form": "요리채널 백종원씨 엄청봐요",
          "original_form": "요리채널 백종원씨 엄청봐요",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036469.1.4",
          "form": "먹방 햇님도 봅니다",
          "original_form": "먹방 햇님도 봅니다",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036469.1.5",
          "form": "즐겨보시는거있으세요?",
          "original_form": "즐겨보시는거있으세요?",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036469.1.6",
          "form": "오호 저랑 겹치는 건 없네요!",
          "original_form": "오호 저랑 겹치는 건 없네요!",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036469.1.7",
          "form": "저는 공포랑 미스테리 좋아해서",
          "original_form": "저는 공포랑 미스테리 좋아해서",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036469.1.8",
          "form": "디바제시카 채널 보고",
          "original_form": "디바제시카 채널 보고",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036469.1.9",
          "form": "돌비공포라디오도 듣고있어요!",
          "original_form": "돌비공포라디오도 듣고있어요!",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036469.1.10",
          "form": "공포 좋아하시는구나",
          "original_form": "공포 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036469.1.11",
          "form": "공포영화도 잘보세요?",
          "original_form": "공포영화도 잘보세요?",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036469.1.12",
          "form": "네 그럼요!",
          "original_form": "네 그럼요!",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036469.1.13",
          "form": "미드소마가 제 인생영화 중 하나예요",
          "original_form": "미드소마가 제 인생영화 중 하나예요",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036469.1.14",
          "form": "영상미가 아주 좋더라구요",
          "original_form": "영상미가 아주 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036469.1.15",
          "form": "저는 공포를 안좋아해서 듣기만했어요!",
          "original_form": "저는 공포를 안좋아해서 듣기만했어요!",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036469.1.16",
          "form": "조금 기괴한영화라고 들었어요",
          "original_form": "조금 기괴한영화라고 들었어요",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036469.1.17",
          "form": "영상미가 좋군요...",
          "original_form": "영상미가 좋군요...",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036469.1.18",
          "form": "공포영화의 매력이 뭐라고 생각하세요?",
          "original_form": "공포영화의 매력이 뭐라고 생각하세요?",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036469.1.19",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210831 00:22"
        },
        {
          "id": "MDRW2100036469.1.20",
          "form": "매력이 뭐냐면... 음",
          "original_form": "매력이 뭐냐면... 음",
          "speaker_id": "2",
          "time": "20210831 00:22"
        },
        {
          "id": "MDRW2100036469.1.21",
          "form": "힘든 상황에서",
          "original_form": "힘든 상황에서",
          "speaker_id": "2",
          "time": "20210831 00:22"
        },
        {
          "id": "MDRW2100036469.1.22",
          "form": "더 극단적인 상황을 보면",
          "original_form": "더 극단적인 상황을 보면",
          "speaker_id": "2",
          "time": "20210831 00:22"
        },
        {
          "id": "MDRW2100036469.1.23",
          "form": "지금 내 상황에 안도할 수 있다는 점?",
          "original_form": "지금 내 상황에 안도할 수 있다는 점?",
          "speaker_id": "2",
          "time": "20210831 00:22"
        },
        {
          "id": "MDRW2100036469.1.24",
          "form": "와... 그런 매력이 있군요...",
          "original_form": "와... 그런 매력이 있군요...",
          "speaker_id": "1",
          "time": "20210831 09:13"
        },
        {
          "id": "MDRW2100036469.1.25",
          "form": "넼ㅋㅋㅋㅋ 실제로 그런 효과가 있다고 하더라구요?!",
          "original_form": "넼ㅋㅋㅋㅋ 실제로 그런 효과가 있다고 하더라구요?!",
          "speaker_id": "2",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036469.1.26",
          "form": "디바제시카분은 괴담 들으시는거예요?",
          "original_form": "디바제시카분은 괴담 들으시는거예요?",
          "speaker_id": "1",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100036469.1.27",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100036469.1.28",
          "form": "괴담도 소개해주고",
          "original_form": "괴담도 소개해주고",
          "speaker_id": "2",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100036469.1.29",
          "form": "무서운 영화를 요약해주기도 하고",
          "original_form": "무서운 영화를 요약해주기도 하고",
          "speaker_id": "2",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100036469.1.30",
          "form": "실제 있었던 사건을 주로 다뤄줘요!",
          "original_form": "실제 있었던 사건을 주로 다뤄줘요!",
          "speaker_id": "2",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100036469.1.31",
          "form": "name1님은 요리채널 주로 보신다고 하셨는데",
          "original_form": "&name1&님은 요리채널 주로 보신다고 하셨는데",
          "speaker_id": "2",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100036469.1.32",
          "form": "그럼 실제로 따라하기도 하시나요?",
          "original_form": "그럼 실제로 따라하기도 하시나요?",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100036469.1.33",
          "form": "네 자주 따라해요",
          "original_form": "네 자주 따라해요",
          "speaker_id": "1",
          "time": "20210831 10:15"
        },
        {
          "id": "MDRW2100036469.1.34",
          "form": "따라한다기보다 하고싶은걸 보고배우는편이에요",
          "original_form": "따라한다기보다 하고싶은걸 보고배우는편이에요",
          "speaker_id": "1",
          "time": "20210831 10:15"
        },
        {
          "id": "MDRW2100036469.1.35",
          "form": "아 그러시구나!",
          "original_form": "아 그러시구나!",
          "speaker_id": "2",
          "time": "20210831 10:19"
        }
      ]
