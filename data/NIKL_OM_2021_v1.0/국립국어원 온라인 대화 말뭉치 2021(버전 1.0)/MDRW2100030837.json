      "utterance": [
        {
          "id": "MDRW2100030837.1.1",
          "form": "제트플립이든 폴드든 사고싶다",
          "original_form": "제트플립이든 폴드든 사고싶다",
          "speaker_id": "1",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100030837.1.2",
          "form": "3개월만 늦게살걸",
          "original_form": "3개월만 늦게살걸",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030837.1.3",
          "form": "그랫다면...",
          "original_form": "그랫다면...",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030837.1.4",
          "form": "어느쪽 샀을 거 같아?",
          "original_form": "어느쪽 샀을 거 같아?",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030837.1.5",
          "form": "어디가 더 끌려?",
          "original_form": "어디가 더 끌려?",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030837.1.6",
          "form": "근데 휩쓸리는 게 맛는듯 3개웧 후에 노트20울트라",
          "original_form": "근데 휩쓸리는 게 맛는듯 3개웧 후에 노트20울트라",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030837.1.7",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030837.1.8",
          "form": "^^;",
          "original_form": "^^;",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030837.1.9",
          "form": "안만져봤지만 플립은 작을 거 같고",
          "original_form": "안만져봤지만 플립은 작을 거 같고",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030837.1.10",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030837.1.11",
          "form": "폴드는 한손에 안잡혀서",
          "original_form": "폴드는 한손에 안잡혀서",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030837.1.12",
          "form": "접으면 잡혀",
          "original_form": "접으면 잡혀",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030837.1.13",
          "form": "하지만 펼치고 싶을 거야",
          "original_form": "하지만 펼치고 싶을 거야",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030837.1.14",
          "form": "나니 이거 언팩행사 했을 때",
          "original_form": "나니 이거 언팩행사 했을 때",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030837.1.15",
          "form": "아니 ㅇㅇ",
          "original_form": "아니 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030837.1.16",
          "form": "나닛!?",
          "original_form": "나닛!?",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030837.1.17",
          "form": "첨엔 반응 글케 안좋았는데",
          "original_form": "첨엔 반응 글케 안좋았는데",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030837.1.18",
          "form": "긍까 공개 직후에 ㅇㅇ",
          "original_form": "긍까 공개 직후에 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030837.1.19",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030837.1.20",
          "form": "gif 돌아간다니까 인기 확 늘고",
          "original_form": "gif 돌아간다니까 인기 확 늘고",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030837.1.21",
          "form": "그 소모품을 되게 잘 뽑앗드라",
          "original_form": "그 소모품을 되게 잘 뽑앗드라",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030837.1.22",
          "form": "이번에",
          "original_form": "이번에",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030837.1.23",
          "form": "충전기나 케이스나",
          "original_form": "충전기나 케이스나",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030837.1.24",
