          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000841.1.15",
          "form": "힘내 금방 지나가더라고",
          "original_form": "힘내 금방 지나가더라고",
          "speaker_id": "1",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000841.1.16",
          "form": "나도 아직 하나 남았어 ㅎ",
          "original_form": "나도 아직 하나 남았어 ㅎ",
          "speaker_id": "1",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000841.1.17",
          "form": "둘인데 한번에 갈거야",
          "original_form": "둘인데 한번에 갈거야",
          "speaker_id": "2",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000841.1.18",
