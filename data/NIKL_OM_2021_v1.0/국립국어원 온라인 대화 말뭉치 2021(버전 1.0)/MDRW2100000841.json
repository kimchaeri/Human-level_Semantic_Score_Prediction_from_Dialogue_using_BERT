      "utterance": [
        {
          "id": "MDRW2100000841.1.1",
          "form": "안녕 \r\n오늘만은 수다친구하자",
          "original_form": "안녕 \r\n오늘만은 수다친구하자",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000841.1.2",
          "form": "저녁 먹었어?",
          "original_form": "저녁 먹었어?",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000841.1.3",
          "form": "응 안녕 난먹었어 넌먹었어?",
          "original_form": "응 안녕 난먹었어 넌먹었어?",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000841.1.4",
          "form": "응 먹고 치우고 애들만 자면 자유야",
          "original_form": "응 먹고 치우고 애들만 자면 자유야",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000841.1.5",
          "form": "드라마 기다리고 있어",
          "original_form": "드라마 기다리고 있어",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000841.1.6",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000841.1.7",
          "form": "나도 다먹고 치웠는데 애들이 잘생각이없어",
          "original_form": "나도 다먹고 치웠는데 애들이 잘생각이없어",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000841.1.8",
          "form": "빨리재우고싶은데 ㅠㅠ 맨날 늦게자",
          "original_form": "빨리재우고싶은데 ㅠㅠ 맨날 늦게자",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000841.1.9",
          "form": "어제밤엔 11시에자서 너무힘들었어",
          "original_form": "어제밤엔 11시에자서 너무힘들었어",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000841.1.10",
          "form": "혹시 애들 어려?",
          "original_form": "혹시 애들 어려?",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000841.1.11",
          "form": "예전에 날 보는것 같아 ㅎ",
          "original_form": "예전에 날 보는것 같아 ㅎ",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000841.1.12",
          "form": "미취학아동이지 ㅎㅎㅎ",
          "original_form": "미취학아동이지 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000841.1.13",
          "form": "학교가면 나아지려나ㅜ",
          "original_form": "학교가면 나아지려나ㅜ",
          "speaker_id": "2",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000841.1.14",
          "form": "고생한다 ㅠ 둘이야?",
          "original_form": "고생한다 ㅠ 둘이야?",
