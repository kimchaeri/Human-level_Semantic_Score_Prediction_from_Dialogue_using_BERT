      "utterance": [
        {
          "id": "MDRW2100031318.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031318.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031318.1.3",
          "form": "전시회나 공연 좋아하세요?",
          "original_form": "전시회나 공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031318.1.4",
          "form": "관심은있는데 가거나하지는 않는거같아요",
          "original_form": "관심은있는데 가거나하지는 않는거같아요",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031318.1.5",
          "form": "뮤지컬도 티켓이 비싸고 그래서",
          "original_form": "뮤지컬도 티켓이 비싸고 그래서",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031318.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100031318.1.7",
          "form": "미술전시회는 공짜인데도 꽤 있는데",
          "original_form": "미술전시회는 공짜인데도 꽤 있는데",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100031318.1.8",
          "form": "한번 가보세요!",
          "original_form": "한번 가보세요!",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100031318.1.9",
          "form": "그렇군요ㅎㅎ 그래야겠네요",
          "original_form": "그렇군요ㅎㅎ 그래야겠네요",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031318.1.10",
          "form": "전시회 좋아하시나요?",
          "original_form": "전시회 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031318.1.11",
          "form": "네! 저는 옛날부터 좋아했어요",
          "original_form": "네! 저는 옛날부터 좋아했어요",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031318.1.12",
          "form": "다녀오면 마음이 차분해지기도 하고 좋더라구요",
          "original_form": "다녀오면 마음이 차분해지기도 하고 좋더라구요",
          "speaker_id": "1",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100031318.1.13",
          "form": "와 멋지네요",
          "original_form": "와 멋지네요",
          "speaker_id": "2",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031318.1.14",
          "form": "저는 미술전시회는 정말 문외한이라ㅋㅋ",
          "original_form": "저는 미술전시회는 정말 문외한이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031318.1.15",
          "form": "낯선기분이에요",
          "original_form": "낯선기분이에요",
          "speaker_id": "2",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031318.1.16",
          "form": "혹시 거주지가 서울이에요??",
          "original_form": "혹시 거주지가 서울이에요??",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031318.1.17",
          "form": "서울쪽이면 무료 개방하는 전시회가 정말 많아요",
          "original_form": "서울쪽이면 무료 개방하는 전시회가 정말 많아요",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031318.1.18",
          "form": "가끔 시간 날때 한번 가봐요ㅎㅎ",
          "original_form": "가끔 시간 날때 한번 가봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031318.1.19",
          "form": "네 그래야겠네요ㅎㅎ",
          "original_form": "네 그래야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:26"
        },
        {
          "id": "MDRW2100031318.1.20",
          "form": "최근엔 어떤 전시회에 다녀오셨나요?",
          "original_form": "최근엔 어떤 전시회에 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031318.1.21",
          "form": "코로나땜시 좀 된거긴한데 피카소전이요!",
          "original_form": "코로나땜시 좀 된거긴한데 피카소전이요!",
          "speaker_id": "1",
          "time": "20210904 21:31"
        },
        {
          "id": "MDRW2100031318.1.22",
          "form": "이건 유료에용",
          "original_form": "이건 유료에용",
          "speaker_id": "1",
          "time": "20210904 21:31"
        },
        {
          "id": "MDRW2100031318.1.23",
          "form": "와 그건 좀 궁금하네요",
          "original_form": "와 그건 좀 궁금하네요",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100031318.1.24",
          "form": "기억에남는 작품이 있나요?",
          "original_form": "기억에남는 작품이 있나요?",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100031318.1.25",
          "form": "한국에서의 학살이라고",
          "original_form": "한국에서의 학살이라고",
          "speaker_id": "1",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100031318.1.26",
          "form": "유명한거있는데",
          "original_form": "유명한거있는데",
          "speaker_id": "1",
          "time": "20210904 21:34"
        },
        {
          "id": "MDRW2100031318.1.27",
          "form": "아시나요?? 교과서에도 나오는데",
          "original_form": "아시나요?? 교과서에도 나오는데",
          "speaker_id": "1",
          "time": "20210904 21:34"
        },
        {
          "id": "MDRW2100031318.1.28",
          "form": "임팩트있더라구요~",
          "original_form": "임팩트있더라구요~",
          "speaker_id": "1",
          "time": "20210904 21:34"
        }
      ]
    }
  ]
