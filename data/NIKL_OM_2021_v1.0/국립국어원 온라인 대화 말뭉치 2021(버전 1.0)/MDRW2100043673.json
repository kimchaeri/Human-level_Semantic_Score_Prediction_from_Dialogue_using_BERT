      "utterance": [
        {
          "id": "MDRW2100043673.1.1",
          "form": "넼ㅋㅋ 내일 저희집 아침이에요",
          "original_form": "넼ㅋㅋ 내일 저희집 아침이에요",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043673.1.2",
          "form": "ㅋㅋㅋㅋ 내일 아침이시구나!!",
          "original_form": "ㅋㅋㅋㅋ 내일 아침이시구나!!",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043673.1.3",
          "form": "낼 먹꼬나가야해요 ㅠㅠ 낼오전에 아로마 향초 수업이있어용!ㅎㅎ",
          "original_form": "낼 먹꼬나가야해요 ㅠㅠ 낼오전에 아로마 향초 수업이있어용!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043673.1.4",
          "form": "내일 저녁도 벌써 고민중이에요",
          "original_form": "내일 저녁도 벌써 고민중이에요",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043673.1.5",
          "form": "어떤 맛있는걸 도전해봐야할까요 ㅋㅋ",
          "original_form": "어떤 맛있는걸 도전해봐야할까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043673.1.6",
          "form": "아로마 향초 수업이라니 와!!",
          "original_form": "아로마 향초 수업이라니 와!!",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043673.1.7",
          "form": "내일 저녁에는 수업갔다오다가 대충 사오시는걸로 하세여",
          "original_form": "내일 저녁에는 수업갔다오다가 대충 사오시는걸로 하세여",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043673.1.8",
          "form": "ㅋㅋㅋㅋ 귀찮으니까욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 귀찮으니까욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043673.1.9",
          "form": "ㅋㅋㅋㄱㅋ 근데 반찬가게도  다 거기서 거기더라구요!ㅜㅜ",
          "original_form": "ㅋㅋㅋㄱㅋ 근데 반찬가게도  다 거기서 거기더라구요!ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043673.1.10",
          "form": "맞아요 결국 해먹을수 밖에 없더라고요",
          "original_form": "맞아요 결국 해먹을수 밖에 없더라고요",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043673.1.11",
          "form": "그래도 도전해볼수밖에 없어요 ㅋㅋㅋ 궁금해서",
          "original_form": "그래도 도전해볼수밖에 없어요 ㅋㅋㅋ 궁금해서",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043673.1.12",
          "form": "ㅎㅎㅎ 이렇게도 먹고 저렇게도 먹고~~~~ 그쵸?ㅎㅎ",
          "original_form": "ㅎㅎㅎ 이렇게도 먹고 저렇게도 먹고~~~~ 그쵸?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043673.1.13",
          "form": "내일은  냉장고속 가지와 양배추를",
          "original_form": "내일은  냉장고속 가지와 양배추를",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043673.1.14",
          "form": "탈탈털어야해요",
          "original_form": "탈탈털어야해요",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043673.1.15",
          "form": "가지밥이나",
          "original_form": "가지밥이나",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043673.1.16",
          "form": "양배추도 있으니까 양배추 쌈",
          "original_form": "양배추도 있으니까 양배추 쌈",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043673.1.17",
          "form": "약간 가지볶음에 양배추쌈 맛있을거 같은데여",
          "original_form": "약간 가지볶음에 양배추쌈 맛있을거 같은데여",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043673.1.18",
          "form": "ㅋㅋ name1님 제맘속에 왔다가셨나봐용ㅎㅎ",
          "original_form": "ㅋㅋ &name1&님 제맘속에 왔다가셨나봐용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043673.1.19",
          "form": "양배추는 무조건 쌈할거고",
          "original_form": "양배추는 무조건 쌈할거고",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043673.1.20",
          "form": "가지는 가지밥할까 가지볶음할까 고민했거든요",
          "original_form": "가지는 가지밥할까 가지볶음할까 고민했거든요",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043673.1.21",
          "form": "사면서 남편한테 가지밥해줄까? 볶음해먹을까? 라고했었는데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사면서 남편한테 가지밥해줄까? 볶음해먹을까? 라고했었는데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043673.1.22",
          "form": "ㅋㅋㅋ 볶음이 더 잘어울려용!!!!",
          "original_form": "ㅋㅋㅋ 볶음이 더 잘어울려용!!!!",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043673.1.23",
          "form": "딱 맛있을거 같아요",
          "original_form": "딱 맛있을거 같아요",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043673.1.24",
