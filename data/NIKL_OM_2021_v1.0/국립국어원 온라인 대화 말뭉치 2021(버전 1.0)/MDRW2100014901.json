      "utterance": [
        {
          "id": "MDRW2100014901.1.1",
          "form": "동물 키우는거 있어요?",
          "original_form": "동물 키우는거 있어요?",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014901.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014901.1.3",
          "form": "현재는 없어요",
          "original_form": "현재는 없어요",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014901.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014901.1.5",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014901.1.6",
          "form": "저는 동물 거북이 밖에 못키워봤어요",
          "original_form": "저는 동물 거북이 밖에 못키워봤어요",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014901.1.7",
          "form": "예전에 키워보셨어요 그러면?",
          "original_form": "예전에 키워보셨어요 그러면?",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014901.1.8",
          "form": "저도 어릴때 거북이 강아지 햄스터 \r\n키워봤었어요 ㅋ",
          "original_form": "저도 어릴때 거북이 강아지 햄스터 \r\n키워봤었어요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014901.1.9",
          "form": "아ㅋㅋ 그래도 다양하게 키워보셧네요",
          "original_form": "아ㅋㅋ 그래도 다양하게 키워보셧네요",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014901.1.10",
          "form": "전 고양이 키워보고싶어요",
          "original_form": "전 고양이 키워보고싶어요",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014901.1.11",
          "form": "네:) 키우보고 나니\r\n더는 안키우고 싶어요ㅠ",
          "original_form": "네:) 키우보고 나니\r\n더는 안키우고 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100014901.1.12",
          "form": "고양이는 키우고 싶은\r\n이유가 있나요?",
          "original_form": "고양이는 키우고 싶은\r\n이유가 있나요?",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100014901.1.13",
          "form": "고양이 뭐랄까 까칠한데 어쩔때 애교스러운 것 같아서 너무 귀엽더라구요",
          "original_form": "고양이 뭐랄까 까칠한데 어쩔때 애교스러운 것 같아서 너무 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100014901.1.14",
          "form": "털도 좀 덜날리지 않을까하기도해서",
          "original_form": "털도 좀 덜날리지 않을까하기도해서",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100014901.1.15",
          "form": "고양이도 털 어마무시하던데용ㅜㅜ",
          "original_form": "고양이도 털 어마무시하던데용ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100014901.1.16",
          "form": "알러지는 없으세용?",
          "original_form": "알러지는 없으세용?",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100014901.1.17",
          "form": "헐 털 그정도로 날려요??",
          "original_form": "헐 털 그정도로 날려요??",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100014901.1.18",
          "form": "전 알러지는 있는지 몰겠어요",
          "original_form": "전 알러지는 있는지 몰겠어요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100014901.1.19",
          "form": "아 알러지 생각을 안해봤네요 심한사람도 있다던데ㅠ",
          "original_form": "아 알러지 생각을 안해봤네요 심한사람도 있다던데ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100014901.1.20",
          "form": "주변에 키우시는분들 보면\r\n옷에 고양이털들ㅋㅋㅋ",
          "original_form": "주변에 키우시는분들 보면\r\n옷에 고양이털들ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100014901.1.21",
          "form": "앜ㅋㅋㅋㅋㅋ 털이문제네..ㅠㅠ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 털이문제네..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100014901.1.22",
          "form": "강아지 키우실때도 털 많이 빠졌죠??",
          "original_form": "강아지 키우실때도 털 많이 빠졌죠??",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100014901.1.23",
          "form": "넼ㅋㅋ 강아지도 많이 빠졌어요",
          "original_form": "넼ㅋㅋ 강아지도 많이 빠졌어요",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100014901.1.24",
          "form": "어떤 종 키우셨어요?",
          "original_form": "어떤 종 키우셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100014901.1.25",
          "form": "전 대형견 좋아하는데",
          "original_form": "전 대형견 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100014901.1.26",
          "form": "유튭에 갈퀴로 막 긁어서 털갈이해주는거보고",
          "original_form": "유튭에 갈퀴로 막 긁어서 털갈이해주는거보고",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100014901.1.27",
          "form": "저는 미니핀이요ㅋㅋㅋㅋ",
          "original_form": "저는 미니핀이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100014901.1.28",
          "form": "멀리서만 바라보기로..ㅋㅋㅋ",
          "original_form": "멀리서만 바라보기로..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100014901.1.29",
          "form": "헐 귀여운 강아지 키우셨네요",
          "original_form": "헐 귀여운 강아지 키우셨네요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100014901.1.30",
          "form": "다른 한 마리는 믹스견ㅋㅋ",
          "original_form": "다른 한 마리는 믹스견ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100014901.1.31",
          "form": "아하 믹스견이 은근 귀여운애들 많아요",
          "original_form": "아하 믹스견이 은근 귀여운애들 많아요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100014901.1.32",
          "form": "미니핀 도베르만? 닮은 조그만애 맞죠?",
          "original_form": "미니핀 도베르만? 닮은 조그만애 맞죠?",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100014901.1.33",
          "form": "맞아용ㅋㅋ",
          "original_form": "맞아용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        }
      ]
