      "utterance": [
        {
          "id": "MDRW2100011988.1.1",
          "form": "최근에 구매한거 있나요",
          "original_form": "최근에 구매한거 있나요",
          "speaker_id": "1",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100011988.1.2",
          "form": "치킨말고는",
          "original_form": "치킨말고는",
          "speaker_id": "2",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100011988.1.3",
          "form": "노트북잉요!",
          "original_form": "노트북잉요!",
          "speaker_id": "2",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100011988.1.4",
          "form": "노트북  사셨구나",
          "original_form": "노트북  사셨구나",
          "speaker_id": "1",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.5",
          "form": "고물쓰다가 드디어 샀어요ㅎ",
          "original_form": "고물쓰다가 드디어 샀어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.6",
          "form": "어때요 만족하나요",
          "original_form": "어때요 만족하나요",
          "speaker_id": "1",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.7",
          "form": "보내주셨군요",
          "original_form": "보내주셨군요",
          "speaker_id": "1",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.8",
          "form": "오늘 주문해서 아직 모르겠어요ㅎㅎㅎ",
          "original_form": "오늘 주문해서 아직 모르겠어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.9",
          "form": "네..수명이 다했어요",
          "original_form": "네..수명이 다했어요",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.10",
          "form": "아...그것은 좋은 노트북이었습니다",
          "original_form": "아...그것은 좋은 노트북이었습니다",
          "speaker_id": "1",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.11",
          "form": "기다리는 시간이 설레겠어요",
          "original_form": "기다리는 시간이 설레겠어요",
          "speaker_id": "1",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.12",
          "form": "보내줄때가됐어요..",
          "original_form": "보내줄때가됐어요..",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.13",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.14",
          "form": "물건 사면 오기 까지 엄청 설레잖아요",
          "original_form": "물건 사면 오기 까지 엄청 설레잖아요",
          "speaker_id": "1",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.15",
          "form": "얼른 받고싶네요",
          "original_form": "얼른 받고싶네요",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011988.1.16",
          "form": "그쵸! 최근에 사신거 있나요?",
          "original_form": "그쵸! 최근에 사신거 있나요?",
          "speaker_id": "2",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100011988.1.17",
          "form": "부디 곤지암에 갇히지 않길..",
          "original_form": "부디 곤지암에 갇히지 않길..",
          "speaker_id": "1",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100011988.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ으악!감사합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ으악!감사합니다",
          "speaker_id": "2",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100011988.1.19",
          "form": "저 최근에 산거 바디워시샀어요",
          "original_form": "저 최근에 산거 바디워시샀어요",
          "speaker_id": "1",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100011988.1.20",
          "form": "자몽향 바디워시를 샀는데 만족해요",
          "original_form": "자몽향 바디워시를 샀는데 만족해요",
          "speaker_id": "1",
          "time": "20210824 18:13"
        }
      ]
