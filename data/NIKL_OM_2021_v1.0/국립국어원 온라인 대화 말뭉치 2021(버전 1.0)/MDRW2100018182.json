      "utterance": [
        {
          "id": "MDRW2100018182.1.1",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018182.1.2",
          "form": "근데 귀여워?",
          "original_form": "근데 귀여워?",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018182.1.3",
          "form": "응 약간 힘만 쎈 아이여서",
          "original_form": "응 약간 힘만 쎈 아이여서",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018182.1.4",
          "form": "아 그래? 완전 의외네",
          "original_form": "아 그래? 완전 의외네",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018182.1.5",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018182.1.6",
          "form": "난 엄청 무서운 캐릭인줄 알았어",
          "original_form": "난 엄청 무서운 캐릭인줄 알았어",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018182.1.7",
          "form": "걔가 젤 안징그러워",
          "original_form": "걔가 젤 안징그러워",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018182.1.8",
          "form": "ㅋㅋㅋ더 징그러운 애들이 많아?",
          "original_form": "ㅋㅋㅋ더 징그러운 애들이 많아?",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018182.1.9",
          "form": "응 조금 있어",
          "original_form": "응 조금 있어",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018182.1.10",
          "form": "작은 괴물도 있어?",
          "original_form": "작은 괴물도 있어?",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018182.1.11",
          "form": "연근이는 조그매 프로틴보다",
          "original_form": "연근이는 조그매 프로틴보다",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018182.1.12",
          "form": "연근이가 괴물이름이야?ㅋㅋㅋㅋ",
          "original_form": "연근이가 괴물이름이야?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018182.1.13",
          "form": "그냥 별명이야",
          "original_form": "그냥 별명이야",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018182.1.14",
          "form": "아 근데 왜 연근이야?",
          "original_form": "아 근데 왜 연근이야?",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018182.1.15",
          "form": "머리가 연근같아서",
          "original_form": "머리가 연근같아서",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018182.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ어떻게 생겼는지 궁금하다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ어떻게 생겼는지 궁금하다",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018182.1.17",
          "form": "한번 검색해봐",
          "original_form": "한번 검색해봐",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018182.1.18",
          "form": "그래야겠다 그럼 대부분 괴물들은 커?",
          "original_form": "그래야겠다 그럼 대부분 괴물들은 커?",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018182.1.19",
          "form": "펜트하우스 너무 잔인하더라",
          "original_form": "펜트하우스 너무 잔인하더라",
          "speaker_id": "1",
          "time": "20210827 23:47"
        }
