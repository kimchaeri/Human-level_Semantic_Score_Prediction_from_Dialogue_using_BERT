      "utterance": [
        {
          "id": "MDRW2100003019.1.1",
          "form": "물건 주로 어디서 구매하세요?",
          "original_form": "물건 주로 어디서 구매하세요?",
          "speaker_id": "1",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100003019.1.2",
          "form": "저는 온라인 쇼핑이 좋아요 눈치안봐도 되잖아요",
          "original_form": "저는 온라인 쇼핑이 좋아요 눈치안봐도 되잖아요",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100003019.1.3",
          "form": "약간 오프는 교환 환불이런것도 눈치보이고",
          "original_form": "약간 오프는 교환 환불이런것도 눈치보이고",
          "speaker_id": "2",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100003019.1.4",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100003019.1.5",
          "form": "괜히 잘못한 기분들고",
          "original_form": "괜히 잘못한 기분들고",
          "speaker_id": "2",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100003019.1.6",
          "form": "매장직원이 막설명해주는것도 약간부담..",
          "original_form": "매장직원이 막설명해주는것도 약간부담..",
          "speaker_id": "1",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100003019.1.7",
          "form": "맞아요 안따라왔으면..",
          "original_form": "맞아요 안따라왔으면..",
          "speaker_id": "2",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100003019.1.8",
          "form": "쳐다보지마세요..",
          "original_form": "쳐다보지마세요..",
          "speaker_id": "2",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100003019.1.9",
          "form": "그래서 온라인 이용하게되는거같아요",
          "original_form": "그래서 온라인 이용하게되는거같아요",
          "speaker_id": "2",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100003019.1.10",
          "form": "name1님도 온라인을 자주 이용하는 편이신가요?",
          "original_form": "&name1&님도 온라인을 자주 이용하는 편이신가요?",
          "speaker_id": "2",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100003019.1.11",
          "form": "네네 저도",
          "original_form": "네네 저도",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100003019.1.12",
          "form": "요즘 워낙 온라인 할인율이",
          "original_form": "요즘 워낙 온라인 할인율이",
          "speaker_id": "1",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003019.1.13",
          "form": "좋아서요",
          "original_form": "좋아서요",
          "speaker_id": "1",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003019.1.14",
          "form": "주로 구매하는 제품은 무엇인가요?",
          "original_form": "주로 구매하는 제품은 무엇인가요?",
          "speaker_id": "2",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003019.1.15",
          "form": "저는 주로 식료품 사는거같아요",
          "original_form": "저는 주로 식료품 사는거같아요",
          "speaker_id": "2",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003019.1.16",
          "form": "저두요! 보통은 그렇고",
          "original_form": "저두요! 보통은 그렇고",
          "speaker_id": "1",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003019.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003019.1.18",
          "form": "필요한 생활용품들요",
          "original_form": "필요한 생활용품들요",
          "speaker_id": "1",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003019.1.19",
          "form": "생필품도 중요하죠",
          "original_form": "생필품도 중요하죠",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003019.1.20",
          "form": "확실히 온라인이 저렴하니까",
          "original_form": "확실히 온라인이 저렴하니까",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003019.1.21",
          "form": "맞아요 중고거래 해보셨어요?",
          "original_form": "맞아요 중고거래 해보셨어요?",
          "speaker_id": "1",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003019.1.22",
          "form": "음 주로 판매입장이라 구매는 잘..ㅎ",
