      "utterance": [
        {
          "id": "MDRW2100039507.1.1",
          "form": "김치볶음밥은 백종원레시피가 맛나더라고요",
          "original_form": "김치볶음밥은 백종원레시피가 맛나더라고요",
          "speaker_id": "1",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100039507.1.2",
          "form": "그렇다고 하더라구요",
          "original_form": "그렇다고 하더라구요",
          "speaker_id": "2",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100039507.1.3",
          "form": "근데 따라하다가 간장 태워서 ㅋ",
          "original_form": "근데 따라하다가 간장 태워서 ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100039507.1.4",
          "form": "그게어려워요",
          "original_form": "그게어려워요",
          "speaker_id": "1",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100039507.1.5",
          "form": "왜케 쉽게하는거죠",
          "original_form": "왜케 쉽게하는거죠",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039507.1.6",
          "form": "탄맛난적이 있어요 ㅋㅋ",
          "original_form": "탄맛난적이 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039507.1.7",
          "form": "요리는 넘나 어렵네요",
          "original_form": "요리는 넘나 어렵네요",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039507.1.8",
          "form": "글쵸 백선생은 툭툭 쉽게하는거 같던데",
          "original_form": "글쵸 백선생은 툭툭 쉽게하는거 같던데",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039507.1.9",
          "form": "저는 제맘대로 안되는손 ㅋㅋ",
          "original_form": "저는 제맘대로 안되는손 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039507.1.10",
          "form": "손이 따로놀져 진짜",
          "original_form": "손이 따로놀져 진짜",
          "speaker_id": "1",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039507.1.11",
          "form": "칼질도 못해요",
          "original_form": "칼질도 못해요",
          "speaker_id": "1",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039507.1.12",
          "form": "그래서 그냥 김치 설탕 스팸넣고",
          "original_form": "그래서 그냥 김치 설탕 스팸넣고",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039507.1.13",
          "form": "간은 김치국물로 맞추고 말아요 ㅋ",
          "original_form": "간은 김치국물로 맞추고 말아요 ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039507.1.14",
          "form": "아 국물넣는군요 그것도 좋을거같아여",
          "original_form": "아 국물넣는군요 그것도 좋을거같아여",
          "speaker_id": "1",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100039507.1.15",
          "form": "설탕도 생각나면 넣고 아님 말고 ㅋ",
          "original_form": "설탕도 생각나면 넣고 아님 말고 ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100039507.1.16",
          "form": "저도 레시피를 보면서 결국 제맘대로ㅋ",
          "original_form": "저도 레시피를 보면서 결국 제맘대로ㅋ",
          "speaker_id": "1",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100039507.1.17",
          "form": "그래도 쭈꾸미도 하시고",
          "original_form": "그래도 쭈꾸미도 하시고",
          "speaker_id": "2",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100039507.1.18",
          "form": "실력파이실듯요",
          "original_form": "실력파이실듯요",
          "speaker_id": "2",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100039507.1.19",
          "form": "아니에요 ㅋㅋ그거 양념까지 다된 냉동ㅋㅋㅋ",
          "original_form": "아니에요 ㅋㅋ그거 양념까지 다된 냉동ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100039507.1.20",
          "form": "그래요?ㅎㅎ",
          "original_form": "그래요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100039507.1.21",
          "form": "뜯어서 볶기만 하면 되욬ㅋㅋ",
          "original_form": "뜯어서 볶기만 하면 되욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:34"
        }
