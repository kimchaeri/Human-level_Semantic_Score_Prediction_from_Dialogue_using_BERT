      "utterance": [
        {
          "id": "MDRW2100018122.1.1",
          "form": "유튜브는 뭐뭐보세요?",
          "original_form": "유튜브는 뭐뭐보세요?",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018122.1.2",
          "form": "저 브이로그 딤디 트위티",
          "original_form": "저 브이로그 딤디 트위티",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018122.1.3",
          "form": "재테크는 부읽남 월부티비 신사임당",
          "original_form": "재테크는 부읽남 월부티비 신사임당",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018122.1.4",
          "form": "name1은용~?",
          "original_form": "&name1&은용~?",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018122.1.5",
          "form": "최근엔 자빱티비만 엄청보고요",
          "original_form": "최근엔 자빱티비만 엄청보고요",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018122.1.6",
          "form": "신사임당이나 정보성 유튜버들은 구독하고",
          "original_form": "신사임당이나 정보성 유튜버들은 구독하고",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018122.1.7",
          "form": "댓글만봐요",
          "original_form": "댓글만봐요",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018122.1.8",
          "form": "어떤분들이 정리해주신거 냠냠 해요",
          "original_form": "어떤분들이 정리해주신거 냠냠 해요",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018122.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018122.1.10",
          "form": "댓글로 요약해주죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "댓글로 요약해주죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018122.1.11",
          "form": "마자여 ㅋㅋ 개웃겨",
          "original_form": "마자여 ㅋㅋ 개웃겨",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018122.1.12",
          "form": "저도가끔 먹버해요",
          "original_form": "저도가끔 먹버해요",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018122.1.13",
          "form": "네 근데",
          "original_form": "네 근데",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018122.1.14",
          "form": "댓글막아둔것도 가끔있지않나요",
          "original_form": "댓글막아둔것도 가끔있지않나요",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018122.1.15",
          "form": "신사임당만그런가요",
          "original_form": "신사임당만그런가요",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018122.1.16",
          "form": "짱나요",
          "original_form": "짱나요",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018122.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018122.1.18",
          "form": "아 그래요?ㅋㅋㅋㅋ 신사임당은 안 막잖아요",
          "original_form": "아 그래요?ㅋㅋㅋㅋ 신사임당은 안 막잖아요",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018122.1.19",
          "form": "최근꺼 막아놔서 짱났었어요",
          "original_form": "최근꺼 막아놔서 짱났었어요",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018122.1.20",
          "form": "그것만그랬나봐요",
          "original_form": "그것만그랬나봐요",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018122.1.21",
          "form": "아.... 헉 왜막았지",
          "original_form": "아.... 헉 왜막았지",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018122.1.22",
          "form": "월부티비 좋아여..굿굿 추천",
          "original_form": "월부티비 좋아여..굿굿 추천",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018122.1.23",
          "form": "방금 구독했슴다 감사합니다",
          "original_form": "방금 구독했슴다 감사합니다",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018122.1.24",
          "form": "ㅋㅋㅋㅋㅋ내집마련 화이팅,,",
          "original_form": "ㅋㅋㅋㅋㅋ내집마련 화이팅,,",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018122.1.25",
          "form": "...ㅜㅜ",
          "original_form": "...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018122.1.26",
          "form": "올해는안될것같지만 미리 공부를..!",
          "original_form": "올해는안될것같지만 미리 공부를..!",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018122.1.27",
          "form": "저두 공뷰만해요....전세살이",
          "original_form": "저두 공뷰만해요....전세살이",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018122.1.28",
          "form": "할수이따!!",
          "original_form": "할수이따!!",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018122.1.29",
          "form": "전 일상은 신아로미봅니다",
          "original_form": "전 일상은 신아로미봅니다",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018122.1.30",
          "form": "아 방구석돈벌기라는 채널도있는데",
          "original_form": "아 방구석돈벌기라는 채널도있는데",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018122.1.31",
          "form": "크리에이터 광고쪽이런 일 하시는분들이 구독하면좋을것같아요",
          "original_form": "크리에이터 광고쪽이런 일 하시는분들이 구독하면좋을것같아요",
          "speaker_id": "1",
          "time": "20210826 23:14"
        }
      ]
    }
