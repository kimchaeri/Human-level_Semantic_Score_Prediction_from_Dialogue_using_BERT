      "utterance": [
        {
          "id": "MDRW2100002996.1.1",
          "form": "왜 혈압 높으시대?",
          "original_form": "왜 혈압 높으시대?",
          "speaker_id": "2",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002996.1.2",
          "form": "저번에 병원에서 쟀더니 수치가 140 정도래",
          "original_form": "저번에 병원에서 쟀더니 수치가 140 정도래",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002996.1.3",
          "form": "관리해야겠네",
          "original_form": "관리해야겠네",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002996.1.4",
          "form": "ㅇㅇ 그래서 혈압계 알아보고 보내달라고 하심",
          "original_form": "ㅇㅇ 그래서 혈압계 알아보고 보내달라고 하심",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002996.1.5",
          "form": "혈압계는 그렇게 안비쌀거야",
          "original_form": "혈압계는 그렇게 안비쌀거야",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002996.1.6",
          "form": "ㅎㅎ 우리가 사드리는 건 아냐",
          "original_form": "ㅎㅎ 우리가 사드리는 건 아냐",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002996.1.7",
          "form": "그냥 알아만 봐드리는 거?",
          "original_form": "그냥 알아만 봐드리는 거?",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002996.1.8",
          "form": "웅 ㅎㅎ 편한 걸로 알아봐서 보내드려야겠어",
          "original_form": "웅 ㅎㅎ 편한 걸로 알아봐서 보내드려야겠어",
          "speaker_id": "1",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002996.1.9",
          "form": "ㅇㅇ 가정 혈압계가 병원보다는 수치가 좀 낮게 나오는 것 같던데",
          "original_form": "ㅇㅇ 가정 혈압계가 병원보다는 수치가 좀 낮게 나오는 것 같던데",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002996.1.10",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002996.1.11",
          "form": "ㅇㅇ 감안하고 재야할 듯",
          "original_form": "ㅇㅇ 감안하고 재야할 듯",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002996.1.12",
          "form": "미리 말씀드려야겠네",
          "original_form": "미리 말씀드려야겠네",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002996.1.13",
          "form": "혈압은 매일 재면서 관리해야하니 버튼이랑 편한거 알아봐",
          "original_form": "혈압은 매일 재면서 관리해야하니 버튼이랑 편한거 알아봐",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
