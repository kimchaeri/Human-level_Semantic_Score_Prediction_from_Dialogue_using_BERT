      "utterance": [
        {
          "id": "MDRW2100045678.1.1",
          "form": "푸딩., , 재출시 해달라ㅠㅠㅠ",
          "original_form": "푸딩., , 재출시 해달라ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045678.1.2",
          "form": "아 그러네요ㅠㅠ",
          "original_form": "아 그러네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045678.1.3",
          "form": "흑설탕 버블티도 그렇구",
          "original_form": "흑설탕 버블티도 그렇구",
          "speaker_id": "2",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045678.1.4",
          "form": "아 흑설탕 버블티도",
          "original_form": "아 흑설탕 버블티도",
          "speaker_id": "1",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045678.1.5",
          "form": "요즘은 많이 안나오나요? 카페메뉴에서 다 사라졌으려냐ㅠㅠㅠ",
          "original_form": "요즘은 많이 안나오나요? 카페메뉴에서 다 사라졌으려냐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045678.1.6",
          "form": "요즘 좀 시들한 것 같더라구요ㅠㅠ",
          "original_form": "요즘 좀 시들한 것 같더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:17"
        },
        {
          "id": "MDRW2100045678.1.7",
          "form": "그래도 요즘은 민초가 많아져서 좋아요!ㅋㅋㅋ",
          "original_form": "그래도 요즘은 민초가 많아져서 좋아요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:17"
        },
        {
          "id": "MDRW2100045678.1.8",
          "form": "맞아요!! 저도 민초단이라",
          "original_form": "맞아요!! 저도 민초단이라",
          "speaker_id": "1",
          "time": "20210908 05:17"
        },
        {
          "id": "MDRW2100045678.1.9",
          "form": "이 기회를 즐기고 있어요!!!",
          "original_form": "이 기회를 즐기고 있어요!!!",
          "speaker_id": "1",
          "time": "20210908 05:17"
        },
        {
          "id": "MDRW2100045678.1.10",
          "form": "name2님도 민초 좋아하세요?",
          "original_form": "&name2&님도 민초 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 05:17"
        },
        {
          "id": "MDRW2100045678.1.11",
          "form": "완전 좋아하죠!!!!",
          "original_form": "완전 좋아하죠!!!!",
          "speaker_id": "2",
          "time": "20210908 05:18"
        },
        {
          "id": "MDRW2100045678.1.12",
          "form": "민초는 사랑이죠ㅠㅠㅠㅠㅠ",
          "original_form": "민초는 사랑이죠ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:18"
        },
        {
          "id": "MDRW2100045678.1.13",
          "form": "과자들도 민초맛으로 많이 나와서 너무 좋아요ㅠㅠ",
          "original_form": "과자들도 민초맛으로 많이 나와서 너무 좋아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:19"
        },
        {
          "id": "MDRW2100045678.1.14",
          "form": "뭘 아는 회사들ㅎㅎㅎ",
          "original_form": "뭘 아는 회사들ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 05:19"
        },
        {
          "id": "MDRW2100045678.1.15",
          "form": "민초 한정 과자들 중에",
          "original_form": "민초 한정 과자들 중에",
          "speaker_id": "1",
          "time": "20210908 05:19"
        },
        {
          "id": "MDRW2100045678.1.16",
          "form": "뭐가 제일 맛있나요!",
          "original_form": "뭐가 제일 맛있나요!",
          "speaker_id": "1",
          "time": "20210908 05:19"
        },
        {
          "id": "MDRW2100045678.1.17",
          "form": "음...초코파이가 괜찮아요",
          "original_form": "음...초코파이가 괜찮아요",
          "speaker_id": "2",
          "time": "20210908 05:20"
        },
        {
          "id": "MDRW2100045678.1.18",
          "form": "얼려먹으면 바로 극락!",
          "original_form": "얼려먹으면 바로 극락!",
          "speaker_id": "2",
          "time": "20210908 05:20"
        },
        {
          "id": "MDRW2100045678.1.19",
          "form": "오 초코파이 메모메모",
          "original_form": "오 초코파이 메모메모",
          "speaker_id": "1",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100045678.1.20",
          "form": "그게 제일 눈에 띄긴 하더라구요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그게 제일 눈에 띄긴 하더라구요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100045678.1.21",
          "form": "안에도 마시멜로 말고 딴게 들었나요?",
          "original_form": "안에도 마시멜로 말고 딴게 들었나요?",
          "speaker_id": "1",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100045678.1.22",
          "form": "네! 안에 중앙에 민초크림같은 게 들어있어요",
          "original_form": "네! 안에 중앙에 민초크림같은 게 들어있어요",
          "speaker_id": "2",
          "time": "20210908 05:23"
        },
        {
          "id": "MDRW2100045678.1.23",
          "form": "오와우",
          "original_form": "오와우",
          "speaker_id": "1",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045678.1.24",
          "form": "세상ㅇ에",
          "original_form": "세상ㅇ에",
          "speaker_id": "1",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045678.1.25",
          "form": "만들줄 아시네...",
          "original_form": "만들줄 아시네...",
          "speaker_id": "1",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045678.1.26",
          "form": "해뜨면 마트 달려갑니다",
          "original_form": "해뜨면 마트 달려갑니다",
          "speaker_id": "1",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045678.1.27",
          "form": "name2님 말씀대로 얼려먹으면 천국갈 것 같아요...",
          "original_form": "&name2&님 말씀대로 얼려먹으면 천국갈 것 같아요...",
          "speaker_id": "1",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045678.1.28",
          "form": "민초단이 많아서 든든합니다!ㅋㅋㅋㅋ",
          "original_form": "민초단이 많아서 든든합니다!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045678.1.29",
          "form": "민초템들이 오래오래 있어주면 좋겠어요ㅠㅠ",
          "original_form": "민초템들이 오래오래 있어주면 좋겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100045678.1.30",
          "form": "맞아요ㅠㅠㅠㅠ 단종되지 말고 시장에 붙박이로 자리잡았으면...",
