      "utterance": [
        {
          "id": "MDRW2100014510.1.1",
          "form": "카페베네 100원",
          "original_form": "카페베네 100원",
          "speaker_id": "2",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014510.1.2",
          "form": "쥬스 샀어요",
          "original_form": "쥬스 샀어요",
          "speaker_id": "2",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014510.1.3",
          "form": "헉 100원이요?",
          "original_form": "헉 100원이요?",
          "speaker_id": "1",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014510.1.4",
          "form": "근데 저희집근처엔 카페베네가 없지싶어요",
          "original_form": "근데 저희집근처엔 카페베네가 없지싶어요",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014510.1.5",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014510.1.6",
          "form": "왜샀나 이러고 있어요 ㅋㅋ",
          "original_form": "왜샀나 이러고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014510.1.7",
          "form": "가격에 혹한거죠",
          "original_form": "가격에 혹한거죠",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014510.1.8",
          "form": "요즘 카페베네있나요?",
          "original_form": "요즘 카페베네있나요?",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014510.1.9",
          "form": "100원",
          "original_form": "100원",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014510.1.10",
          "form": "없어요 ㅋㅋ",
          "original_form": "없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014510.1.11",
          "form": "100원이면 진짜싸긴해요",
          "original_form": "100원이면 진짜싸긴해요",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014510.1.12",
          "form": "먹으러 차타고 가야할판입니다 ㅋㅌ",
          "original_form": "먹으러 차타고 가야할판입니다 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014510.1.13",
          "form": "차비가 더 ㅋㅋ",
          "original_form": "차비가 더 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014510.1.14",
          "form": "저라도 100원글자보고 구매버튼 눌렀을듯해요 ㅎ",
          "original_form": "저라도 100원글자보고 구매버튼 눌렀을듯해요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014510.1.15",
          "form": "네 아",
          "original_form": "네 아",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014510.1.16",
          "form": "손이 잘못했네요",
          "original_form": "손이 잘못했네요",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014510.1.17",
          "form": "또 그런게 많아요",
          "original_form": "또 그런게 많아요",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014510.1.18",
          "form": "손가락단속 ㅋ",
          "original_form": "손가락단속 ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014510.1.19",
          "form": "아 고쳐야 하는데",
          "original_form": "아 고쳐야 하는데",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014510.1.20",
          "form": "벨롭이 가끔",
          "original_form": "벨롭이 가끔",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014510.1.21",
          "form": "신발 3000원 ㅋㅋ",
          "original_form": "신발 3000원 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014510.1.22",
          "form": "막샀네요",
          "original_form": "막샀네요",
          "speaker_id": "2",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014510.1.23",
          "form": "엥?",
          "original_form": "엥?",
          "speaker_id": "1",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014510.1.24",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014510.1.25",
          "form": "3000원짜리가 있다구요?",
          "original_form": "3000원짜리가 있다구요?",
          "speaker_id": "1",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014510.1.26",
          "form": "택배비만받는건가요?",
          "original_form": "택배비만받는건가요?",
          "speaker_id": "1",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014510.1.27",
          "form": "네 택배비 따로죠 ㅋㅋ",
          "original_form": "네 택배비 따로죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.28",
          "form": "근데 쿠폰이 있어서 ㅋㅋ",
          "original_form": "근데 쿠폰이 있어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.29",
          "form": "6만원짜리가",
          "original_form": "6만원짜리가",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.30",
          "form": "사이트이름이 벨롭이예요?ㅎ",
          "original_form": "사이트이름이 벨롭이예요?ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.31",
          "form": "사이즈 한정요",
          "original_form": "사이즈 한정요",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.32",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.33",
          "form": "신을수있는거죠?ㅋ",
          "original_form": "신을수있는거죠?ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.34",
          "form": "처음엔 슬히퍼 그냥주던데요",
          "original_form": "처음엔 슬히퍼 그냥주던데요",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.35",
          "form": "예뻐요",
          "original_form": "예뻐요",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.36",
          "form": "평상시 10 에서 6만원요",
          "original_form": "평상시 10 에서 6만원요",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014510.1.37",
          "form": "예쁘기까지하다니",
          "original_form": "예쁘기까지하다니",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014510.1.38",
          "form": "와~ 진짜 거저네요",
          "original_form": "와~ 진짜 거저네요",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014510.1.39",
          "form": "네 ㅎㅎ",
