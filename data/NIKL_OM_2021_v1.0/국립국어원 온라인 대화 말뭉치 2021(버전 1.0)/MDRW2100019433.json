      "utterance": [
        {
          "id": "MDRW2100019433.1.1",
          "form": "헛 마따 좀전에 헤드윅 예기했었죠 ㅎ",
          "original_form": "헛 마따 좀전에 헤드윅 예기했었죠 ㅎ",
          "speaker_id": "2",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019433.1.2",
          "form": "특히 조승우회차는 맨 뒷자리도 구하기 힘들어요ㅠ",
          "original_form": "특히 조승우회차는 맨 뒷자리도 구하기 힘들어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:18"
        },
        {
          "id": "MDRW2100019433.1.3",
          "form": "저도 티켓팅 했었는데 자리가 너무 안좋아서 포기했어요 ㅜㅡ",
          "original_form": "저도 티켓팅 했었는데 자리가 너무 안좋아서 포기했어요 ㅜㅡ",
          "speaker_id": "2",
          "time": "20210827 19:18"
        },
        {
          "id": "MDRW2100019433.1.4",
          "form": "자리가 안좋아도 제 자리 하나 있었으면 좋겠네요ㅠ",
          "original_form": "자리가 안좋아도 제 자리 하나 있었으면 좋겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:18"
        },
        {
          "id": "MDRW2100019433.1.5",
          "form": "용케도 티켓킹 성공하셨네요",
          "original_form": "용케도 티켓킹 성공하셨네요",
          "speaker_id": "2",
          "time": "20210827 19:18"
        },
        {
          "id": "MDRW2100019433.1.6",
          "form": "조승우. 오만석. 이규형. 고은성. 렌이 헤드윅이예요.",
          "original_form": "조승우. 오만석. 이규형. 고은성. 렌이 헤드윅이예요.",
          "speaker_id": "1",
          "time": "20210827 19:19"
        },
        {
          "id": "MDRW2100019433.1.7",
          "form": "ㅋㅋㅋ 조승우랑 오만석까진 알았는데 이규형도 했나보네요",
          "original_form": "ㅋㅋㅋ 조승우랑 오만석까진 알았는데 이규형도 했나보네요",
          "speaker_id": "2",
          "time": "20210827 19:19"
        },
        {
          "id": "MDRW2100019433.1.8",
          "form": "코로나 때문에 뮤지컬 보는 것도 너무 걱정되더라구요.ㅠ",
          "original_form": "코로나 때문에 뮤지컬 보는 것도 너무 걱정되더라구요.ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:20"
        },
        {
          "id": "MDRW2100019433.1.9",
          "form": "고은성은 누군가요?",
          "original_form": "고은성은 누군가요?",
          "speaker_id": "2",
          "time": "20210827 19:20"
        },
        {
          "id": "MDRW2100019433.1.10",
          "form": "고은성은 팬텀싱어에 나왔었구. 뮤지컬배우예요.",
          "original_form": "고은성은 팬텀싱어에 나왔었구. 뮤지컬배우예요.",
          "speaker_id": "1",
          "time": "20210827 19:20"
        },
        {
          "id": "MDRW2100019433.1.11",
          "form": "ㅎ 뮤지컬 진성팬이 아니라선가 그사람은 잘모르겠네요 ㅎㅎㅎ",
          "original_form": "ㅎ 뮤지컬 진성팬이 아니라선가 그사람은 잘모르겠네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:21"
        },
        {
          "id": "MDRW2100019433.1.12",
          "form": "이규형은 아세요?",
          "original_form": "이규형은 아세요?",
          "speaker_id": "1",
          "time": "20210827 19:21"
        },
        {
          "id": "MDRW2100019433.1.13",
          "form": "넵 슬빵에 헤롱이 맞죠?",
          "original_form": "넵 슬빵에 헤롱이 맞죠?",
          "speaker_id": "2",
          "time": "20210827 19:21"
        },
        {
          "id": "MDRW2100019433.1.14",
          "form": "네 슬기로운 감빵생활에 나왔어요.ㅎㅎ",
          "original_form": "네 슬기로운 감빵생활에 나왔어요.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:22"
        },
        {
          "id": "MDRW2100019433.1.15",
          "form": "ㅋ넵 ㅎ 뮤지컬배운지 몰랐는데",
          "original_form": "ㅋ넵 ㅎ 뮤지컬배운지 몰랐는데",
          "speaker_id": "2",
          "time": "20210827 19:22"
        },
        {
          "id": "MDRW2100019433.1.16",
          "form": "노래 진짜 잘하던데 ㅋ",
          "original_form": "노래 진짜 잘하던데 ㅋ",
          "speaker_id": "1",
          "time": "20210827 19:23"
        }
      ]
