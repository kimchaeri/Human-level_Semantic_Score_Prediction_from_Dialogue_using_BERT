      "utterance": [
        {
          "id": "MDRW2100045844.1.1",
          "form": "과일 말고는 최근에 사신 거 있나요?",
          "original_form": "과일 말고는 최근에 사신 거 있나요?",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045844.1.2",
          "form": "당근마켓 이용하세요?",
          "original_form": "당근마켓 이용하세요?",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045844.1.3",
          "form": "전 가끔 이용해요!",
          "original_form": "전 가끔 이용해요!",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045844.1.4",
          "form": "이용하시나요?",
          "original_form": "이용하시나요?",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045844.1.5",
          "form": "저는 생활용품 정도요",
          "original_form": "저는 생활용품 정도요",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045844.1.6",
          "form": "얼마전에 냄비 보고 직거래 갔다 왔어요",
          "original_form": "얼마전에 냄비 보고 직거래 갔다 왔어요",
          "speaker_id": "2",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100045844.1.7",
          "form": "오 어땠나요?",
          "original_form": "오 어땠나요?",
          "speaker_id": "1",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100045844.1.8",
          "form": "전 거래 좀 어려운 느낌이라서 구경 주로 해요 ㅋㅋ",
          "original_form": "전 거래 좀 어려운 느낌이라서 구경 주로 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100045844.1.9",
          "form": "평수 냄비인데 사용감 있는거 치고는 깨끗하고 그을린 자국이 없더라고요",
          "original_form": "평수 냄비인데 사용감 있는거 치고는 깨끗하고 그을린 자국이 없더라고요",
          "speaker_id": "2",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100045844.1.10",
          "form": "아하 얼마에 사신 건가요?",
          "original_form": "아하 얼마에 사신 건가요?",
          "speaker_id": "1",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100045844.1.11",
          "form": "8천원이요 ㅋㅋ 스탠인데 저렴하죠?",
          "original_form": "8천원이요 ㅋㅋ 스탠인데 저렴하죠?",
          "speaker_id": "2",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100045844.1.12",
          "form": "옼ㅋㅋ진짜 싸네요",
          "original_form": "옼ㅋㅋ진짜 싸네요",
          "speaker_id": "1",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100045844.1.13",
          "form": "전 예전에 커블체어 팔아봤어요",
          "original_form": "전 예전에 커블체어 팔아봤어요",
          "speaker_id": "1",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100045844.1.14",
          "form": "아 커블 2개 사면 1개 더 오는거요?",
          "original_form": "아 커블 2개 사면 1개 더 오는거요?",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045844.1.15",
          "form": "네 그래서 1개는 팔았어요",
          "original_form": "네 그래서 1개는 팔았어요",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045844.1.16",
          "form": "저도 그거 사볼까 했는데 너무 비싸게 팔아서 안 샀어요",
          "original_form": "저도 그거 사볼까 했는데 너무 비싸게 팔아서 안 샀어요",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045844.1.17",
          "form": "얼마였엇나요?",
          "original_form": "얼마였엇나요?",
          "speaker_id": "1",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045844.1.18",
          "form": "제가 체구가 작아서 와이드랑 컴피 둘다 하나씩 써보고 추가 구매할 지 결정하려고 했는데 온라인가에 6천원 정도 붙이는게 보통이더라고요.",
          "original_form": "제가 체구가 작아서 와이드랑 컴피 둘다 하나씩 써보고 추가 구매할 지 결정하려고 했는데 온라인가에 6천원 정도 붙이는게 보통이더라고요.",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045844.1.19",
          "form": "와이더는 1만3천원 더 붙여서 팔고 에누리가 절대 안 통했어요 ㅠㅠ",
          "original_form": "와이더는 1만3천원 더 붙여서 팔고 에누리가 절대 안 통했어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045844.1.20",
          "form": "헉 엄청 붙여서 파네요...",
          "original_form": "헉 엄청 붙여서 파네요...",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045844.1.21",
          "form": "저는 저한테 필요없어서 파는 거라서 와이더 35천원에 팔앗는데...",
          "original_form": "저는 저한테 필요없어서 파는 거라서 와이더 35천원에 팔앗는데...",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045844.1.22",
          "form": "사는 분이 다들 35천에 판다구 그러더라구요 ㅋㅋ",
          "original_form": "사는 분이 다들 35천에 판다구 그러더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045844.1.23",
          "form": "아니 거기 동네가 어디인가요? 저는 그게 컴피 가격이었어요. 키즈는 기본 4만원대 후반인거 있죠. 우리 동네가 야박한거네요",
          "original_form": "아니 거기 동네가 어디인가요? 저는 그게 컴피 가격이었어요. 키즈는 기본 4만원대 후반인거 있죠. 우리 동네가 야박한거네요",
          "speaker_id": "2",
          "time": "20210908 08:07"
        }
      ]
    }
