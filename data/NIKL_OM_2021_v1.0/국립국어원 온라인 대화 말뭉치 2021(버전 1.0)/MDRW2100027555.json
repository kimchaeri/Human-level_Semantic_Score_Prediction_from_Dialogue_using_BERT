      "utterance": [
        {
          "id": "MDRW2100027555.1.1",
          "form": "도를 믿으십니까",
          "original_form": "도를 믿으십니까",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027555.1.2",
          "form": "그럴 수도 있고 아닐 수도 있습니다",
          "original_form": "그럴 수도 있고 아닐 수도 있습니다",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027555.1.3",
          "form": "헉걱",
          "original_form": "헉걱",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027555.1.4",
          "form": "저는 불교인데요",
          "original_form": "저는 불교인데요",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027555.1.5",
          "form": "극락 가시나요?",
          "original_form": "극락 가시나요?",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027555.1.6",
          "form": "그건 좀 어려울 듯",
          "original_form": "그건 좀 어려울 듯",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027555.1.7",
          "form": "나일론 불교신자라서요",
          "original_form": "나일론 불교신자라서요",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027555.1.8",
          "form": "부처님은 모든 신자를 사랑하시는데",
          "original_form": "부처님은 모든 신자를 사랑하시는데",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027555.1.9",
          "form": "갈 수도 있죠",
          "original_form": "갈 수도 있죠",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027555.1.10",
          "form": "저도 갈 예정입니다",
          "original_form": "저도 갈 예정입니다",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027555.1.11",
          "form": "극락 한번 가보자고!!!!",
          "original_form": "극락 한번 가보자고!!!!",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027555.1.12",
          "form": "절에 있는거 뭐라해야하지",
          "original_form": "절에 있는거 뭐라해야하지",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027555.1.13",
          "form": "단청 문양 너무 예쁘지않나요",
          "original_form": "단청 문양 너무 예쁘지않나요",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027555.1.14",
          "form": "맞아요 저 그래서 경복궁이라도 가고 싶어요",
          "original_form": "맞아요 저 그래서 경복궁이라도 가고 싶어요",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027555.1.15",
          "form": "한복도 입고 싶어요",
          "original_form": "한복도 입고 싶어요",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027555.1.16",
          "form": "불교 짱~ 한복 짱~",
          "original_form": "불교 짱~ 한복 짱~",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027555.1.17",
          "form": "경복궁 안가본지 9000년 됐네요",
          "original_form": "경복궁 안가본지 9000년 됐네요",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027555.1.18",
          "form": "헐 저는 1만년 됐는데",
          "original_form": "헐 저는 1만년 됐는데",
          "speaker_id": "2",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027555.1.19",
          "form": "제가 더 선배네요 ㅋㅋ",
          "original_form": "제가 더 선배네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027555.1.20",
          "form": "하... 졌다",
          "original_form": "하... 졌다",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027555.1.21",
          "form": "절 밥도 안먹은지 오래된듯요",
          "original_form": "절 밥도 안먹은지 오래된듯요",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027555.1.22",
          "form": "저 절밥 완전 좋아해요",
          "original_form": "저 절밥 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027555.1.23",
          "form": "고기가 없는데도 맛있는 비빔밥은 처음이었어요",
          "original_form": "고기가 없는데도 맛있는 비빔밥은 처음이었어요",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027555.1.24",
          "form": "절편도 좋아요",
          "original_form": "절편도 좋아요",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027555.1.25",
          "form": "마자요~~! 근데 저 버섯을 안 먹어서",
          "original_form": "마자요~~! 근데 저 버섯을 안 먹어서",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027555.1.26",
          "form": "버섯 안 받기 너무 어려움...",
          "original_form": "버섯 안 받기 너무 어려움...",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027555.1.27",
          "form": "아항..그럼 역시 고기를 먹을 수 있는 기독교로 가죠",
          "original_form": "아항..그럼 역시 고기를 먹을 수 있는 기독교로 가죠",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027555.1.28",
          "form": "기독교는 금지음식 없잖아요",
          "original_form": "기독교는 금지음식 없잖아요",
          "speaker_id": "2",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027555.1.29",
          "form": "짱종교",
          "original_form": "짱종교",
          "speaker_id": "2",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027555.1.30",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027555.1.31",
          "form": "또 종교됨 미안합니다",
