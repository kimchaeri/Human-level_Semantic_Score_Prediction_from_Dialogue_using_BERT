      "utterance": [
        {
          "id": "MDRW2100014398.1.1",
          "form": "유튜브 프리미엄 나도 가입할까",
          "original_form": "유튜브 프리미엄 나도 가입할까",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.2",
          "form": "영상볼 때 끊기는 거 화나는데",
          "original_form": "영상볼 때 끊기는 거 화나는데",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.3",
          "form": "좋긴조아",
          "original_form": "좋긴조아",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.4",
          "form": "노래들을때도좋더라",
          "original_form": "노래들을때도좋더라",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.5",
          "form": "체험쓴게다지만",
          "original_form": "체험쓴게다지만",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.6",
          "form": "2900원인가?",
          "original_form": "2900원인가?",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.7",
          "form": "그거는...인도인일때",
          "original_form": "그거는...인도인일때",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.8",
          "form": "이미 ott결제한 게 많은데",
          "original_form": "이미 ott결제한 게 많은데",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.9",
          "form": "참아?",
          "original_form": "참아?",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.10",
          "form": "참아 ㅇㅇ",
          "original_form": "참아 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014398.1.11",
          "form": "유튜브로 노래도듣는데",
          "original_form": "유튜브로 노래도듣는데",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014398.1.12",
          "form": "왓챠도 끝나면 좀 멈추는것ㄷ 조은듯",
          "original_form": "왓챠도 끝나면 좀 멈추는것ㄷ 조은듯",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014398.1.13",
          "form": "아 좋은생각이야",
          "original_form": "아 좋은생각이야",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014398.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014398.1.15",
          "form": "넷플은 있어야해",
          "original_form": "넷플은 있어야해",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014398.1.16",
          "form": "유튜브 노래들으면 진짜 좋대",
          "original_form": "유튜브 노래들으면 진짜 좋대",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014398.1.17",
          "form": "프리미엄",
          "original_form": "프리미엄",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014398.1.18",
          "form": "넷플은 넷플시리즈봐야돼",
          "original_form": "넷플은 넷플시리즈봐야돼",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014398.1.19",
          "form": "근데 그게 플레이리스트 따로 만들어야한데 일일히",
          "original_form": "근데 그게 플레이리스트 따로 만들어야한데 일일히",
