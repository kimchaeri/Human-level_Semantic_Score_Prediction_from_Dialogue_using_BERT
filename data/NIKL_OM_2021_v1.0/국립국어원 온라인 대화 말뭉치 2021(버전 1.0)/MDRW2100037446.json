      "utterance": [
        {
          "id": "MDRW2100037446.1.1",
          "form": "지금 비오나요??!",
          "original_form": "지금 비오나요??!",
          "speaker_id": "1",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100037446.1.2",
          "form": "아니요! 거긴 어떤가요?",
          "original_form": "아니요! 거긴 어떤가요?",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100037446.1.3",
          "form": "여기도 안와요 !!",
          "original_form": "여기도 안와요 !!",
          "speaker_id": "1",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100037446.1.4",
          "form": "우아ㅠ 눈뜨니 비 폭탄이네요ㅠ",
          "original_form": "우아ㅠ 눈뜨니 비 폭탄이네요ㅠ",
          "speaker_id": "2",
          "time": "20210901 07:28"
        },
        {
          "id": "MDRW2100037446.1.5",
          "form": "저희는 아직 안와요 ㅜㅜ",
          "original_form": "저희는 아직 안와요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100037446.1.6",
          "form": "출근 잘하셨나요 !!",
          "original_form": "출근 잘하셨나요 !!",
          "speaker_id": "1",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100037446.1.7",
          "form": "으아ㅜㅜ 다행입니닷ㅠ",
          "original_form": "으아ㅜㅜ 다행입니닷ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100037446.1.8",
          "form": "여기도 맑아졌어요!",
          "original_form": "여기도 맑아졌어요!",
          "speaker_id": "2",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100037446.1.9",
          "form": "ㅜㅜ 저희 지역은 저녁에 비온다더라구요",
          "original_form": "ㅜㅜ 저희 지역은 저녁에 비온다더라구요",
          "speaker_id": "1",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100037446.1.10",
          "form": "세상에ㅠ 퇴근하실는 피해가야할텐데요..",
          "original_form": "세상에ㅠ 퇴근하실는 피해가야할텐데요..",
          "speaker_id": "2",
          "time": "20210901 12:10"
        },
        {
          "id": "MDRW2100037446.1.11",
          "form": "뭔가 올거같은 예감입니다... !",
          "original_form": "뭔가 올거같은 예감입니다... !",
          "speaker_id": "1",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100037446.1.12",
          "form": "우산은 챙겨서 다행이에요 ㅜㅜ",
          "original_form": "우산은 챙겨서 다행이에요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100037446.1.13",
          "form": "으윽ㅠ 그나마 다행이네요!ㅠ",
          "original_form": "으윽ㅠ 그나마 다행이네요!ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037446.1.14",
          "form": "언제쯤 장마가 끝나련지ㅜㅜ",
          "original_form": "언제쯤 장마가 끝나련지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037446.1.15",
          "form": "그러게요ㅜㅠ",
          "original_form": "그러게요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100037446.1.16",
          "form": "여긴 내일까지 온다 그러더라구요",
          "original_form": "여긴 내일까지 온다 그러더라구요",
          "speaker_id": "2",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100037446.1.17",
          "form": "저희두요 !!",
          "original_form": "저희두요 !!",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100037446.1.18",
          "form": "좀 확 추워졌음 좋겠네요 ㅋㅋㅋ",
          "original_form": "좀 확 추워졌음 좋겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100037446.1.19",
          "form": "ㅋㅋㅋ차라리 그게 나을수도 있겠어욥!",
