      "utterance": [
        {
          "id": "MDRW2100032116.1.1",
          "form": "사고싶은건 많은데",
          "original_form": "사고싶은건 많은데",
          "speaker_id": "2",
          "time": "20210905 12:40"
        },
        {
          "id": "MDRW2100032116.1.2",
          "form": "미니멀라이프를하고싶어서",
          "original_form": "미니멀라이프를하고싶어서",
          "speaker_id": "2",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.3",
          "form": "오ㅋㅋ평소미니멀하게사시나봐요",
          "original_form": "오ㅋㅋ평소미니멀하게사시나봐요",
          "speaker_id": "1",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.4",
          "form": "첫애 학원비 유치원비",
          "original_form": "첫애 학원비 유치원비",
          "speaker_id": "2",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.5",
          "form": "학원비ㅠㅠ",
          "original_form": "학원비ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.6",
          "form": "둘찌 사진촬영비쓰고나면",
          "original_form": "둘찌 사진촬영비쓰고나면",
          "speaker_id": "2",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.7",
          "form": "끝날듯요",
          "original_form": "끝날듯요",
          "speaker_id": "2",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.8",
          "form": "맞아요..학원비크죠",
          "original_form": "맞아요..학원비크죠",
          "speaker_id": "1",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.9",
          "form": "제가요새사고싶은건 많은데",
          "original_form": "제가요새사고싶은건 많은데",
          "speaker_id": "2",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.10",
          "form": "알치고유용하게사용하시겠네요",
          "original_form": "알치고유용하게사용하시겠네요",
          "speaker_id": "1",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.11",
          "form": "집이작아서 슬퍼요",
          "original_form": "집이작아서 슬퍼요",
          "speaker_id": "2",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100032116.1.12",
          "form": "위로해주세요ㅠ",
          "original_form": "위로해주세요ㅠ",
          "speaker_id": "2",
          "time": "20210905 12:41"
        },
        {
