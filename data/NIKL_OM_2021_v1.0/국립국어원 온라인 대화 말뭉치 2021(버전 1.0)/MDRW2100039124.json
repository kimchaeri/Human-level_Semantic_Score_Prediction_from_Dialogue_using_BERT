      "utterance": [
        {
          "id": "MDRW2100039124.1.1",
          "form": "좋아하는 음료있으세요?",
          "original_form": "좋아하는 음료있으세요?",
          "speaker_id": "1",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100039124.1.2",
          "form": "커피요 ㅎㅎ",
          "original_form": "커피요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100039124.1.3",
          "form": "아메늑 못마시고 단거요 ㅎㅎ 크림라떼 즐겨마셔요",
          "original_form": "아메늑 못마시고 단거요 ㅎㅎ 크림라떼 즐겨마셔요",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100039124.1.4",
          "form": "크림라떼는 만들어드시나요?",
          "original_form": "크림라떼는 만들어드시나요?",
          "speaker_id": "1",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100039124.1.5",
          "form": "거의 만들어마시고 사서 마실때도 있어요",
          "original_form": "거의 만들어마시고 사서 마실때도 있어요",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100039124.1.6",
          "form": "휘핑이나 생크림사서",
          "original_form": "휘핑이나 생크림사서",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100039124.1.7",
          "form": "제가 만든커피가 제일 맛있다는 ㅎㅎ",
          "original_form": "제가 만든커피가 제일 맛있다는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100039124.1.8",
          "form": "와!",
          "original_form": "와!",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100039124.1.9",
          "form": "레시피좀 알 수 있을까요?",
          "original_form": "레시피좀 알 수 있을까요?",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100039124.1.10",
          "form": "그냥 우유 커피2샷 휘핑 섞어요 ㅎㅎ",
          "original_form": "그냥 우유 커피2샷 휘핑 섞어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039124.1.11",
          "form": "다른 레시피는 없고",
          "original_form": "다른 레시피는 없고",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039124.1.12",
          "form": "거품기로 휘핑 섞는게 다예요 ㅎㅎ",
          "original_form": "거품기로 휘핑 섞는게 다예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039124.1.13",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039124.1.14",
          "form": "전 커피는 다좋아요ㅎㅎ",
          "original_form": "전 커피는 다좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039124.1.15",
          "form": "헤이즐넛도 좋고ㅎ",
          "original_form": "헤이즐넛도 좋고ㅎ",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100039124.1.16",
          "form": "ㅎㅎ 커피는 다 좋죠",
          "original_form": "ㅎㅎ 커피는 다 좋죠",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100039124.1.17",
          "form": "점심뭐드세요?",
          "original_form": "점심뭐드세요?",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100039124.1.18",
          "form": "미역국먹을까 싶어요ㅎㅎ",
          "original_form": "미역국먹을까 싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100039124.1.19",
          "form": "뜨끈하게",
          "original_form": "뜨끈하게",
          "speaker_id": "1",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100039124.1.20",
          "form": "이런날은 뜨끈한게\r\n좋죠",
          "original_form": "이런날은 뜨끈한게\r\n좋죠",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100039124.1.21",
          "form": "저도 수제비 먹을까 제육볶음 먹을까 고민하고 있어요",
          "original_form": "저도 수제비 먹을까 제육볶음 먹을까 고민하고 있어요",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100039124.1.22",
          "form": "수제비!ㅋㅋㅋ좋네요",
          "original_form": "수제비!ㅋㅋㅋ좋네요",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100039124.1.23",
          "form": "메뉴가!",
          "original_form": "메뉴가!",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100039124.1.24",
          "form": "네 비가 오니 계속 수제비 칼국수가 땡기네요",
          "original_form": "네 비가 오니 계속 수제비 칼국수가 땡기네요",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100039124.1.25",
          "form": "보통은 면은 즐겨먹지 않는데 ㅎㅎ",
          "original_form": "보통은 면은 즐겨먹지 않는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100039124.1.26",
          "form": "비오고 쌀쌀하면 땡기죠ㅋㅋㅋ",
          "original_form": "비오고 쌀쌀하면 땡기죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100039124.1.27",
          "form": "뜨끈한 수제비 좋네요",
          "original_form": "뜨끈한 수제비 좋네요",
          "speaker_id": "1",
          "time": "20210903 11:31"
        }
      ]
    }
