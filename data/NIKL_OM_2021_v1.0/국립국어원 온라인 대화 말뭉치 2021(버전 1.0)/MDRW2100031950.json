        {
          "id": "MDRW2100031950.1.1",
          "form": "앞에꺼가",
          "original_form": "앞에꺼가",
          "speaker_id": "2",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031950.1.2",
          "form": "짤려서 안보여요ㅠ",
          "original_form": "짤려서 안보여요ㅠ",
          "speaker_id": "2",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031950.1.3",
          "form": "아아ㅠㅠ 당직비 이야기였어요 ㅋㅋ",
          "original_form": "아아ㅠㅠ 당직비 이야기였어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:11"
        },
        {
          "id": "MDRW2100031950.1.4",
          "form": "아 당직비",
          "original_form": "아 당직비",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031950.1.5",
          "form": "얼마 주나요 ㅋㅋ",
          "original_form": "얼마 주나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100031950.1.6",
          "form": "아 5만원이에요 거기는요?",
          "original_form": "아 5만원이에요 거기는요?",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031950.1.7",
          "form": "너무짜죠",
          "original_form": "너무짜죠",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031950.1.8",
          "form": "초과근무비도 2시간에 6천원 ....ㅠㅠ",
          "original_form": "초과근무비도 2시간에 6천원 ....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031950.1.9",
          "form": "저희 야간에 당직 만원인데요",
          "original_form": "저희 야간에 당직 만원인데요",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031950.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031950.1.11",
          "form": "근데 뭐 1시부터 5시 수면시간인데",
          "original_form": "근데 뭐 1시부터 5시 수면시간인데",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031950.1.12",
          "form": "그때 당직서고 5시부터 9시까지 자니까",
          "original_form": "그때 당직서고 5시부터 9시까지 자니까",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031950.1.13",
          "form": "똔똔인거 같기도",
          "original_form": "똔똔인거 같기도",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031950.1.14",
          "form": "만원이요?????ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "만원이요?????ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031950.1.15",
          "form": "너무하다....",
          "original_form": "너무하다....",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031950.1.16",
          "form": "근데 뭐 그 뒤에 쉬니까요",
          "original_form": "근데 뭐 그 뒤에 쉬니까요",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031950.1.17",
          "form": "그리고 당직때 의자 눕히고",
          "original_form": "그리고 당직때 의자 눕히고",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031950.1.18",
          "form": "자면 돼요",
          "original_form": "자면 돼요",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031950.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031950.1.20",
          "form": "그런가요 그럼 다행이에요 ㅋㅋ",
          "original_form": "그런가요 그럼 다행이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100031950.1.21",
          "form": "ㅋㅋ박봉입니다",
          "original_form": "ㅋㅋ박봉입니다",
          "speaker_id": "2",
          "time": "20210905 10:55"
        },
        {
          "id": "MDRW2100031950.1.22",
          "form": "직원 고혈을 쥐어짜는",
          "original_form": "직원 고혈을 쥐어짜는",
          "speaker_id": "2",
          "time": "20210905 10:55"
        },
        {
          "id": "MDRW2100031950.1.23",
          "form": "ㅋㅋㅋㅋㅋ공사 좋지만 이런건 정말 좀 그래요",
          "original_form": "ㅋㅋㅋㅋㅋ공사 좋지만 이런건 정말 좀 그래요",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100031950.1.24",
          "form": "뭐만 하면 혈세 세금잔치",
