      "utterance": [
        {
          "id": "MDRW2100011610.1.1",
          "form": "라면 먹었어요",
          "original_form": "라면 먹었어요",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011610.1.2",
          "form": "왜 라면 드셨어용",
          "original_form": "왜 라면 드셨어용",
          "speaker_id": "1",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100011610.1.3",
          "form": "밥 하기 귀찮아서요.ㅋㅋ",
          "original_form": "밥 하기 귀찮아서요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011610.1.4",
          "form": "ㅋㅋ아드님들이랑 같이 드신거에요?",
          "original_form": "ㅋㅋ아드님들이랑 같이 드신거에요?",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011610.1.5",
          "form": "아뇨. 혼자",
          "original_form": "아뇨. 혼자",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011610.1.6",
          "form": "작은아들도 이번주는 학교 가요",
          "original_form": "작은아들도 이번주는 학교 가요",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011610.1.7",
          "form": "아 이제 등교 시작했나요?",
          "original_form": "아 이제 등교 시작했나요?",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011610.1.8",
          "form": "그럼 학교에서 점심도 먹고 오죠?",
          "original_form": "그럼 학교에서 점심도 먹고 오죠?",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011610.1.9",
          "form": "격주로 가요",
          "original_form": "격주로 가요",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011610.1.10",
          "form": "큰아들은 매일 가고",
          "original_form": "큰아들은 매일 가고",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011610.1.11",
          "form": "저녁까지 먹고 와요.ㅋㅋㅋ",
          "original_form": "저녁까지 먹고 와요.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011610.1.12",
          "form": "우왕 학교에서 저녁까지요?",
          "original_form": "우왕 학교에서 저녁까지요?",
          "speaker_id": "1",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011610.1.13",
          "form": "너무 좋네요 ㅋㅋㅋ\r\n저도 얼른른 키워야겠어요",
          "original_form": "너무 좋네요 ㅋㅋㅋ\r\n저도 얼른른 키워야겠어요",
          "speaker_id": "1",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011610.1.14",
          "form": "저녁먹고 야자하고 와요",
          "original_form": "저녁먹고 야자하고 와요",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011610.1.15",
          "form": "공부하느라 힘들겠네요ㅠ",
          "original_form": "공부하느라 힘들겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011610.1.16",
          "form": "주말에는 특식 해주시나용,",
          "original_form": "주말에는 특식 해주시나용,",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011610.1.17",
          "form": "점심엔 국수",
          "original_form": "점심엔 국수",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011610.1.18",
          "form": "특식..안 해 줘요.",
          "original_form": "특식..안 해 줘요.",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011610.1.19",
          "form": "저녁엔 고기",
          "original_form": "저녁엔 고기",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011610.1.20",
          "form": "보통 이렇게 먹어요",
          "original_form": "보통 이렇게 먹어요",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011610.1.21",
          "form": "그게 특식인거 같은데요",
          "original_form": "그게 특식인거 같은데요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011610.1.22",
          "form": "국수는 멸치로 국물내서 해주세용?\r\n아님 비빔국수?",
          "original_form": "국수는 멸치로 국물내서 해주세용?\r\n아님 비빔국수?",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011610.1.23",
          "form": "학교에서 먹는게 더 골고루 먹을텐데",
          "original_form": "학교에서 먹는게 더 골고루 먹을텐데",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011610.1.24",
          "form": "둘 다 돌아가면서요",
          "original_form": "둘 다 돌아가면서요",
          "speaker_id": "2",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100011610.1.25",
          "form": "학교에서야 영양소에 맞춰서\r\n나오겠지만 뭐니뭐니해도\r\n엄마가 해주는게 제일 맛있죵",
          "original_form": "학교에서야 영양소에 맞춰서\r\n나오겠지만 뭐니뭐니해도\r\n엄마가 해주는게 제일 맛있죵",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011610.1.26",
          "form": "물국수하고 있으면 비빔 먹고 싶다그러고 비빔이라하면 물국수 먹고 싶다 그러고. 콱 마..ㅋㅋㅋ",
          "original_form": "물국수하고 있으면 비빔 먹고 싶다그러고 비빔이라하면 물국수 먹고 싶다 그러고. 콱 마..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011610.1.27",
          "form": "아이코ㅋㅋ 아예 둘다 준비해야겠네요ㄱ",
          "original_form": "아이코ㅋㅋ 아예 둘다 준비해야겠네요ㄱ",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100011610.1.28",
          "form": "둘다 준비하면 귀찮아서 주는대로 먹으라 그래요.ㅋㅋ",
          "original_form": "둘다 준비하면 귀찮아서 주는대로 먹으라 그래요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011610.1.29",
          "form": "저희는 아빠가 국물파고\r\n저랑 오빠는 비빔파라서\r\n엄마가 항상 두가지 다 준비하셨어요ㅠㅋ",
          "original_form": "저희는 아빠가 국물파고\r\n저랑 오빠는 비빔파라서\r\n엄마가 항상 두가지 다 준비하셨어요ㅠㅋ",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011610.1.30",
          "form": "ㅋㅋ 맞아요\r\n지금 생각해보면\r\n걍 통일해서 먹을걸\r\n엄마 번거로웠겠다 싶어요ㅠ",
          "original_form": "ㅋㅋ 맞아요\r\n지금 생각해보면\r\n걍 통일해서 먹을걸\r\n엄마 번거로웠겠다 싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100011610.1.31",
          "form": "저도 2가지 하는 때도 있는데 귀찮아서",
          "original_form": "저도 2가지 하는 때도 있는데 귀찮아서",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100011610.1.32",
          "form": "오늘 저녁은 뭐 먹을거에요?",
          "original_form": "오늘 저녁은 뭐 먹을거에요?",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011610.1.33",
          "form": "오늘 저녁은 된장찌개 하려구용ㅋㅋ",
          "original_form": "오늘 저녁은 된장찌개 하려구용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011610.1.34",
          "form": "뭐 드실거에요?",
          "original_form": "뭐 드실거에요?",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011610.1.35",
          "form": "콩나물국밥 해 먹을래요",
          "original_form": "콩나물국밥 해 먹을래요",
          "speaker_id": "2",
