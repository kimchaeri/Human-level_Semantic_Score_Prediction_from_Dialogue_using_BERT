      "utterance": [
        {
          "id": "MDRW2100044742.1.1",
          "form": "뭐 구매하신거 있으세요?",
          "original_form": "뭐 구매하신거 있으세요?",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044742.1.2",
          "form": "최근엔 딱히 없는데",
          "original_form": "최근엔 딱히 없는데",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044742.1.3",
          "form": "요즘 아이패드 사고싶어요",
          "original_form": "요즘 아이패드 사고싶어요",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044742.1.4",
          "form": "아이패드로 어떤거 하시게요?",
          "original_form": "아이패드로 어떤거 하시게요?",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044742.1.5",
          "form": "필기할 일이 많은데",
          "original_form": "필기할 일이 많은데",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044742.1.6",
          "form": "아이패드 있음 서류나 책을 많이 안들고 다녀도 될 것 같아서요",
          "original_form": "아이패드 있음 서류나 책을 많이 안들고 다녀도 될 것 같아서요",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044742.1.7",
          "form": "그럼 유용하실거에요",
          "original_form": "그럼 유용하실거에요",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044742.1.8",
          "form": "저도 가끔 필기할때 쓰는데 괜찮아요",
          "original_form": "저도 가끔 필기할때 쓰는데 괜찮아요",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044742.1.9",
          "form": "아 아이패드 사용하시나요?",
          "original_form": "아 아이패드 사용하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044742.1.10",
          "form": "오 먼저 사용해본 분의 추천이라니",
          "original_form": "오 먼저 사용해본 분의 추천이라니",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044742.1.11",
          "form": "더욱 구매욕이 생기네요",
          "original_form": "더욱 구매욕이 생기네요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044742.1.12",
          "form": "네 근데 글씨는 잘쓰지못해요ㅋㅋ",
          "original_form": "네 근데 글씨는 잘쓰지못해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044742.1.13",
          "form": "예쁘게 쓰긴 조금 힘들어요",
          "original_form": "예쁘게 쓰긴 조금 힘들어요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044742.1.14",
          "form": "아아 그래도",
          "original_form": "아아 그래도",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044742.1.15",
          "form": "연습하면 나아지지 않나요?",
          "original_form": "연습하면 나아지지 않나요?",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044742.1.16",
          "form": "맞아요 첨에 종이랑 느낌이 좀 달라서 적응이 필요해요",
          "original_form": "맞아요 첨에 종이랑 느낌이 좀 달라서 적응이 필요해요",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044742.1.17",
          "form": "네네 사면 적응할 때까지 열심히 연습해야죠",
          "original_form": "네네 사면 적응할 때까지 열심히 연습해야죠",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044742.1.18",
          "form": "그래서 종이필터를 쓰기도 해요",
          "original_form": "그래서 종이필터를 쓰기도 해요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044742.1.19",
          "form": "한번 취향따라 좋은거 골라보시길 바래요ㅎㅎ",
          "original_form": "한번 취향따라 좋은거 골라보시길 바래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044742.1.20",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044742.1.21",
          "form": "네네 잘 알아볼게요",
          "original_form": "네네 잘 알아볼게요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044742.1.22",
          "form": "한번 사용하면 진짜편하긴해요 터치되는 노트북 같아서요ㅎㅎ",
          "original_form": "한번 사용하면 진짜편하긴해요 터치되는 노트북 같아서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044742.1.23",
          "form": "네네 프로 쓰시나요?",
          "original_form": "네네 프로 쓰시나요?",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044742.1.24",
