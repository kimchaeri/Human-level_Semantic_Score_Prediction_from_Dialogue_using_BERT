      "utterance": [
        {
          "id": "MDRW2100037529.1.1",
          "form": "네 전완전완전좋아해요!!",
          "original_form": "네 전완전완전좋아해요!!",
          "speaker_id": "2",
          "time": "20210901 14:30"
        },
        {
          "id": "MDRW2100037529.1.2",
          "form": "저도 좋아하는데 지금은 자제해요",
          "original_form": "저도 좋아하는데 지금은 자제해요",
          "speaker_id": "1",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100037529.1.3",
          "form": "카페인조절이필요하긴해요ㅠ~",
          "original_form": "카페인조절이필요하긴해요ㅠ~",
          "speaker_id": "2",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100037529.1.4",
          "form": "어떤문제잇으세요ㅠㅜ?",
          "original_form": "어떤문제잇으세요ㅠㅜ?",
          "speaker_id": "2",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100037529.1.5",
          "form": "저.. 너무 많이 마셔서 위가 망가졌어요",
          "original_form": "저.. 너무 많이 마셔서 위가 망가졌어요",
          "speaker_id": "1",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100037529.1.6",
          "form": "아ㅠㅠㅠ 저도 빈속에 마셔서 한동안",
          "original_form": "아ㅠㅠㅠ 저도 빈속에 마셔서 한동안",
          "speaker_id": "2",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100037529.1.7",
          "form": "위땜에고생했었는데ㅠ 진짜 독하게 끊고 괜찮아졋다싶으니",
          "original_form": "위땜에고생했었는데ㅠ 진짜 독하게 끊고 괜찮아졋다싶으니",
          "speaker_id": "2",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100037529.1.8",
          "form": "또 많이마시긴해요ㅠㅠ",
          "original_form": "또 많이마시긴해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100037529.1.9",
          "form": "ㅎㅎ 저도 지금 슬금슬금 조금씩 마시기 시작했어요",
          "original_form": "ㅎㅎ 저도 지금 슬금슬금 조금씩 마시기 시작했어요",
          "speaker_id": "1",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100037529.1.10",
          "form": "조금씩만 드세요 ~^^",
          "original_form": "조금씩만 드세요 ~^^",
          "speaker_id": "2",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100037529.1.11",
          "form": "아메드시나요?",
          "original_form": "아메드시나요?",
          "speaker_id": "2",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100037529.1.12",
          "form": "원두를 하루이 1리터씩 먹다가 위가 아파져서",
          "original_form": "원두를 하루이 1리터씩 먹다가 위가 아파져서",
          "speaker_id": "1",
          "time": "20210901 15:09"
        },
        {
          "id": "MDRW2100037529.1.13",
          "form": "약먹고 나은후 안마셨다가",
          "original_form": "약먹고 나은후 안마셨다가",
          "speaker_id": "1",
          "time": "20210901 15:09"
        },
        {
          "id": "MDRW2100037529.1.14",
          "form": "요새 한두잔씩 다시 먹고 있네요..ㅎㅎ",
          "original_form": "요새 한두잔씩 다시 먹고 있네요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:09"
        },
        {
          "id": "MDRW2100037529.1.15",
          "form": "그쵸그쳐",
          "original_form": "그쵸그쳐",
          "speaker_id": "2",
          "time": "20210901 15:09"
        },
        {
          "id": "MDRW2100037529.1.16",
          "form": "전 안마시면 ㅋㅋ 힘이없어요ㅠㅠㅋㅋ",
          "original_form": "전 안마시면 ㅋㅋ 힘이없어요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:11"
        },
        {
          "id": "MDRW2100037529.1.17",
          "form": "카페인의존이 높은거같아요",
          "original_form": "카페인의존이 높은거같아요",
          "speaker_id": "2",
          "time": "20210901 15:11"
        },
        {
          "id": "MDRW2100037529.1.18",
          "form": "ㅎㅎㅎ 카페인이 중요하죠",
          "original_form": "ㅎㅎㅎ 카페인이 중요하죠",
          "speaker_id": "1",
          "time": "20210901 15:11"
        },
        {
          "id": "MDRW2100037529.1.19",
          "form": "ㅠㅠㅋㅋ 올해는 진짜 아아 많이마셨네요",
          "original_form": "ㅠㅠㅋㅋ 올해는 진짜 아아 많이마셨네요",
          "speaker_id": "2",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100037529.1.20",
          "form": "저는 커피 대신 차를 마시기로 했어요..",
          "original_form": "저는 커피 대신 차를 마시기로 했어요..",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100037529.1.21",
          "form": "원래 더울때도 뜨아파엿는데",
          "original_form": "원래 더울때도 뜨아파엿는데",
          "speaker_id": "2",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100037529.1.22",
