      "utterance": [
        {
          "id": "MDRW2100030685.1.1",
          "form": "저랑 같이 간 사람도 맛이 없다고 했는데 매번 손님이 많아서 신기해요",
          "original_form": "저랑 같이 간 사람도 맛이 없다고 했는데 매번 손님이 많아서 신기해요",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030685.1.2",
          "form": "다들 한 번 먹고 실망 하겠네요",
          "original_form": "다들 한 번 먹고 실망 하겠네요",
          "speaker_id": "1",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030685.1.3",
          "form": "김볶을 못 하는 여자 줘",
          "original_form": "김볶을 못 하는 여자 줘",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030685.1.4",
          "form": "알바 들인 가요",
          "original_form": "알바 들인 가요",
          "speaker_id": "1",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030685.1.5",
          "form": "신기하게 매번 손님이 많아요 다른 곳이랑 가격은 비슷해요",
          "original_form": "신기하게 매번 손님이 많아요 다른 곳이랑 가격은 비슷해요",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030685.1.6",
          "form": "뭘 잘하죠",
          "original_form": "뭘 잘하죠",
          "speaker_id": "1",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030685.1.7",
          "form": "다른 지역에서 와서 유명해서 찾아 가는 것 같아요",
          "original_form": "다른 지역에서 와서 유명해서 찾아 가는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030685.1.8",
          "form": "매번 사람들이 낚이나 봐요",
          "original_form": "매번 사람들이 낚이나 봐요",
          "speaker_id": "1",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030685.1.9",
          "form": "위치는 좋으니까요",
          "original_form": "위치는 좋으니까요",
          "speaker_id": "1",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030685.1.10",
          "form": "접근성이 좋군요",
          "original_form": "접근성이 좋군요",
          "speaker_id": "1",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030685.1.11",
          "form": "그 가게 바로 건너편에 있는 비빔 밥과 칼국수 파는 곳이 훨씬 맛있어요",
          "original_form": "그 가게 바로 건너편에 있는 비빔 밥과 칼국수 파는 곳이 훨씬 맛있어요",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030685.1.12",
          "form": "보리밥 집도 있지요",
          "original_form": "보리밥 집도 있지요",
          "speaker_id": "1",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030685.1.13",
          "form": "홍성 방도 있고요",
          "original_form": "홍성 방도 있고요",
          "speaker_id": "1",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030685.1.14",
          "form": "자갈치에 있는 보리밥 집에서 비빔 보리밥 먹었던 것 기억나네요",
          "original_form": "자갈치에 있는 보리밥 집에서 비빔 보리밥 먹었던 것 기억나네요",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030685.1.15",
          "form": "보리밥 안 먹은지 오래 되었어요",
          "original_form": "보리밥 안 먹은지 오래 되었어요",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030685.1.16",
          "form": "보리 맛있나요",
          "original_form": "보리 맛있나요",
          "speaker_id": "1",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030685.1.17",
          "form": "근처에 생선구이집 또 있었는데 말이죠",
          "original_form": "근처에 생선구이집 또 있었는데 말이죠",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030685.1.18",
          "form": "방귀 많이 나올 거 같아요",
          "original_form": "방귀 많이 나올 거 같아요",
          "speaker_id": "1",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030685.1.19",
          "form": "그냥 그랬어요 제 입맛엔 별로예요",
          "original_form": "그냥 그랬어요 제 입맛엔 별로예요",
          "speaker_id": "2",
          "time": "20210904 11:26"
        }
