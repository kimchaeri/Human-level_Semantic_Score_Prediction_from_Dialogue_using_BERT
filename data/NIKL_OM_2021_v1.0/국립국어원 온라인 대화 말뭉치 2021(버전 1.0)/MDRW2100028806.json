      "utterance": [
        {
          "id": "MDRW2100028806.1.1",
          "form": "엄마 요즘 아모르파티봄?",
          "original_form": "엄마 요즘 아모르파티봄?",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028806.1.2",
          "form": "꼬박꼬박 보고있다",
          "original_form": "꼬박꼬박 보고있다",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028806.1.3",
          "form": "그거 엄청 고구마던데 볼만함?",
          "original_form": "그거 엄청 고구마던데 볼만함?",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028806.1.4",
          "form": "요즘 사이다 될락말락한다",
          "original_form": "요즘 사이다 될락말락한다",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028806.1.5",
          "form": "오 그럴 때가 젤 재밌지",
          "original_form": "오 그럴 때가 젤 재밌지",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028806.1.6",
          "form": "거기 나쁜애 요즘 보기 싫다고 난리다",
          "original_form": "거기 나쁜애 요즘 보기 싫다고 난리다",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028806.1.7",
          "form": "연기를 잘해서 그렇지ㅋㅋ",
          "original_form": "연기를 잘해서 그렇지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028806.1.8",
          "form": "몰라 꼴보기 싫음",
          "original_form": "몰라 꼴보기 싫음",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028806.1.9",
          "form": "엄마가 자주 틀어놔서",
          "original_form": "엄마가 자주 틀어놔서",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028806.1.10",
          "form": "나는 안 봤는데도 내용 안닼ㅋㄱ",
          "original_form": "나는 안 봤는데도 내용 안닼ㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028806.1.11",
          "form": "왜 보고 싶냐?",
          "original_form": "왜 보고 싶냐?",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028806.1.12",
          "form": "아니 쫌 궁금해서",
          "original_form": "아니 쫌 궁금해서",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028806.1.13",
          "form": "거기도 나쁜짓 대박이던데",
          "original_form": "거기도 나쁜짓 대박이던데",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028806.1.14",
          "form": "완전 막장이지",
          "original_form": "완전 막장이지",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028806.1.15",
          "form": "막장이 재밌나봐",
          "original_form": "막장이 재밌나봐",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028806.1.16",
          "form": "펜트하우스도 시청률 대박이었잖아",
          "original_form": "펜트하우스도 시청률 대박이었잖아",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028806.1.17",
          "form": "그건 나중에 재미없더라",
          "original_form": "그건 나중에 재미없더라",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028806.1.18",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028806.1.19",
          "form": "하도 꼬아놓고",
          "original_form": "하도 꼬아놓고",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028806.1.20",
          "form": "질질",
          "original_form": "질질",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028806.1.21",
          "form": "끌어서",
          "original_form": "끌어서",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028806.1.22",
          "form": "아하",
          "original_form": "아하",
