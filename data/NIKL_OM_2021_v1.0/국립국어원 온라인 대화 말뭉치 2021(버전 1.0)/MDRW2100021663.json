      "utterance": [
        {
          "id": "MDRW2100021663.1.1",
          "form": "진짜 소름돋더라니까여..",
          "original_form": "진짜 소름돋더라니까여..",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021663.1.2",
          "form": "만날때 우산 잠깐 빌려줬던거 이제와서 돌려주겠다고",
          "original_form": "만날때 우산 잠깐 빌려줬던거 이제와서 돌려주겠다고",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021663.1.3",
          "form": "비도 안오는 쨍쨍한날에 찾아오더라니까요",
          "original_form": "비도 안오는 쨍쨍한날에 찾아오더라니까요",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021663.1.4",
          "form": "저 몰래 클럽가서 헤어진건데 뻔뻔한 놈이었어여..",
          "original_form": "저 몰래 클럽가서 헤어진건데 뻔뻔한 놈이었어여..",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021663.1.5",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021663.1.6",
          "form": "어디서 뻔뻔하게",
          "original_form": "어디서 뻔뻔하게",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021663.1.7",
          "form": "그렇게 하고는 안나타났어요?",
          "original_form": "그렇게 하고는 안나타났어요?",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021663.1.8",
          "form": "네.. 차단하고 아예 반응도 안했어요",
          "original_form": "네.. 차단하고 아예 반응도 안했어요",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021663.1.9",
          "form": "name1님은 사귀는 중에 클럽 용납 가능해요..?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "&name1&님은 사귀는 중에 클럽 용납 가능해요..?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021663.1.10",
          "form": "그래도 더이상 문제 없어서 다행이에요",
          "original_form": "그래도 더이상 문제 없어서 다행이에요",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021663.1.11",
          "form": "사유는 친구 생일파티",
          "original_form": "사유는 친구 생일파티",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021663.1.12",
          "form": "아뇨 전혀요ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 전혀요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021663.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021663.1.14",
          "form": "또 치트키쓰네",
          "original_form": "또 치트키쓰네",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021663.1.15",
          "form": "남자요 ㅋㅋ",
          "original_form": "남자요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021663.1.16",
          "form": "절대 여자랑 놀러간거아니라고 얘기한 상황이라면..?",
          "original_form": "절대 여자랑 놀러간거아니라고 얘기한 상황이라면..?",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021663.1.17",
          "form": "후",
          "original_form": "후",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021663.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021663.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ상상만으로 너무 화나요 ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ상상만으로 너무 화나요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021663.1.20",
          "form": "맞아요.. 솔직히 저 말도 못믿겠곸ㅋㅋㅋ",
          "original_form": "맞아요.. 솔직히 저 말도 못믿겠곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021663.1.21",
          "form": "저도 간다고 맞불 놓죠",
          "original_form": "저도 간다고 맞불 놓죠",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021663.1.22",
          "form": "안갈거지만",
          "original_form": "안갈거지만",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021663.1.23",
          "form": "평소에 얼마나 신뢰를 줬는지가 중요한 대목입니다,,",
          "original_form": "평소에 얼마나 신뢰를 줬는지가 중요한 대목입니다,,",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021663.1.24",
          "form": "그렇죠....",
          "original_form": "그렇죠....",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.25",
          "form": "ㅋㅋㅋㅋㅋ저두,,, 전 클럽도 한번도 안가봤어요",
          "original_form": "ㅋㅋㅋㅋㅋ저두,,, 전 클럽도 한번도 안가봤어요",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.26",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.27",
          "form": "헌팅포차이런곳도",
          "original_form": "헌팅포차이런곳도",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.28",
          "form": "시끄러워서.....",
          "original_form": "시끄러워서.....",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.29",
          "form": "저 사람많은거 싫어해요",
          "original_form": "저 사람많은거 싫어해요",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.31",
          "form": "저도욬ㅋㅋㅋㅋㅋ",
          "original_form": "저도욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.32",
          "form": "저 진짜 용산아이파크몰도 힘들어요",
          "original_form": "저 진짜 용산아이파크몰도 힘들어요",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 예상외로 비슷한 부분이 있네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 예상외로 비슷한 부분이 있네요",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.34",
          "form": "저두요... 놀이공원 질색",
          "original_form": "저두요... 놀이공원 질색",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021663.1.35",
          "form": "사람많은데가면 기 빨려요",
          "original_form": "사람많은데가면 기 빨려요",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021663.1.36",
          "form": "그래서 좀 안유명한 곳 위주로 데이트코스를 찾아보게 되더라구요,,,",
          "original_form": "그래서 좀 안유명한 곳 위주로 데이트코스를 찾아보게 되더라구요,,,",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021663.1.37",
          "form": "근데 안유명하면 검색조차 안될텐데",
          "original_form": "근데 안유명하면 검색조차 안될텐데",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021663.1.38",
          "form": "검색이된다는것부터가 이미 사람많은곳이라서",
          "original_form": "검색이된다는것부터가 이미 사람많은곳이라서",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021663.1.39",
          "form": "평일에 만나는수밖에 없어옄ㅋㅋㅋㅋㅋ",
          "original_form": "평일에 만나는수밖에 없어옄ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021663.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 주말에 일찍 만나고 헤어지거나 아니면 집에서 놀고 평일에 주로 만났어요",
