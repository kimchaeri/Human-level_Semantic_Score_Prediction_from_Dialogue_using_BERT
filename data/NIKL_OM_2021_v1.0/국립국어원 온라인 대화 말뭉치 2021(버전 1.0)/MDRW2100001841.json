      "utterance": [
        {
          "id": "MDRW2100001841.1.1",
          "form": "결혼한지 몇년째야?",
          "original_form": "결혼한지 몇년째야?",
          "speaker_id": "1",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001841.1.2",
          "form": "9년차예요",
          "original_form": "9년차예요",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001841.1.3",
          "form": "저는 3년입니다",
          "original_form": "저는 3년입니다",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001841.1.4",
          "form": "신혼이군요ㅎㄹ",
          "original_form": "신혼이군요ㅎㄹ",
          "speaker_id": "2",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001841.1.5",
          "form": "연애때가 좋았지",
          "original_form": "연애때가 좋았지",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001841.1.6",
          "form": "그렇긴하지..연애감정",
          "original_form": "그렇긴하지..연애감정",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001841.1.7",
          "form": "신혼같은소리하고있네",
          "original_form": "신혼같은소리하고있네",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001841.1.8",
          "form": "나는이제그런알콩달콩없어",
          "original_form": "나는이제그런알콩달콩없어",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001841.1.9",
          "form": "데이트한지가 언제인가",
          "original_form": "데이트한지가 언제인가",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001841.1.10",
          "form": "그런거 어떻게하는건지..",
          "original_form": "그런거 어떻게하는건지..",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001841.1.11",
          "form": "밖에 데이트가 뭔지 집에만 있어요",
          "original_form": "밖에 데이트가 뭔지 집에만 있어요",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001841.1.12",
          "form": "데이트로 영화본지10년이네요",
          "original_form": "데이트로 영화본지10년이네요",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001841.1.13",
