      "utterance": [
        {
          "id": "MDRW2100016565.1.1",
          "form": "좋아하는 작가있으신가요?",
          "original_form": "좋아하는 작가있으신가요?",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016565.1.2",
          "form": "전 킹덤 쓴 김은희 작가 좋아합니다!!",
          "original_form": "전 킹덤 쓴 김은희 작가 좋아합니다!!",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016565.1.3",
          "form": "아.. 김은희 작가는 대단하죠.. \n국내 거의 원탑이죠?",
          "original_form": "아.. 김은희 작가는 대단하죠.. \n국내 거의 원탑이죠?",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016565.1.4",
          "form": "장르물에선 원탑인거 같아요ㅎㅎㅎ",
          "original_form": "장르물에선 원탑인거 같아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016565.1.5",
          "form": "싸인때부터 좋아했거든요ㅎㅎ",
          "original_form": "싸인때부터 좋아했거든요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016565.1.6",
          "form": "국내 레벨 올리신분같아요.. \n저 한드 안보다가 싸인 유명하다해서 얼마전에 봤어요",
          "original_form": "국내 레벨 올리신분같아요.. \n저 한드 안보다가 싸인 유명하다해서 얼마전에 봤어요",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016565.1.7",
          "form": "싸인 뭔가 한국작가가 쓴거같지 않아요..",
          "original_form": "싸인 뭔가 한국작가가 쓴거같지 않아요..",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016565.1.8",
          "form": "진짜ㅜ 너무 재밌지 않나요ㅜ",
          "original_form": "진짜ㅜ 너무 재밌지 않나요ㅜ",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016565.1.9",
          "form": "저도 시간만 되면 재탕하고 싶네요ㅜ",
          "original_form": "저도 시간만 되면 재탕하고 싶네요ㅜ",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016565.1.10",
          "form": "김희은 작가 책도 내시나요?",
          "original_form": "김희은 작가 책도 내시나요?",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016565.1.11",
          "form": "잘 모르겠어요?",
          "original_form": "잘 모르겠어요?",
          "speaker_id": "2",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016565.1.12",
          "form": "아마 대본 쓰신다고 책은 안내는거 같은데ㅎㅎㅎㅎ",
          "original_form": "아마 대본 쓰신다고 책은 안내는거 같은데ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016565.1.13",
          "form": "아.. 글쿠낭.. \n드라마 대본같은거도 출판나오기도하던데..",
          "original_form": "아.. 글쿠낭.. \n드라마 대본같은거도 출판나오기도하던데..",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016565.1.14",
          "form": "독서는 많이 하시나요?",
          "original_form": "독서는 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016565.1.15",
          "form": "독서.. 저링 거리가 멀죠ㅎㅎ",
          "original_form": "독서.. 저링 거리가 멀죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016565.1.16",
          "form": "솔직히 도서관이 조그만 집이랑 가까웠으면 좋겠어요ㅎㅎ",
          "original_form": "솔직히 도서관이 조그만 집이랑 가까웠으면 좋겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016565.1.17",
          "form": "이것도 핑계겠지만ㅎㅎ",
          "original_form": "이것도 핑계겠지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016565.1.18",
          "form": "저희지역은 집근처까지 배달도 해주는데..",
          "original_form": "저희지역은 집근처까지 배달도 해주는데..",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016565.1.19",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016565.1.20",
          "form": "좋은곳이네요ㅋㅋㅋ",
          "original_form": "좋은곳이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016565.1.21",
          "form": "전 작은도서관이 있긴한데",
          "original_form": "전 작은도서관이 있긴한데",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016565.1.22",
          "form": "너무 작아서 책도 많이는 없더라구요ㅜ",
          "original_form": "너무 작아서 책도 많이는 없더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016565.1.23",
          "form": "추리소설? 보고 싶어서 찾아봤는데",
          "original_form": "추리소설? 보고 싶어서 찾아봤는데",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016565.1.24",
          "form": "좀 멀리 가야 있어서 ㅠㅜ",
          "original_form": "좀 멀리 가야 있어서 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016565.1.25",
          "form": "아.. 저흰 경기도인데.. \n동네 동마다 지역 미니도서관이 몇개씩 있고.. \n사이트에서 신청하면 배달해줘요..",
          "original_form": "아.. 저흰 경기도인데.. \n동네 동마다 지역 미니도서관이 몇개씩 있고.. \n사이트에서 신청하면 배달해줘요..",
          "speaker_id": "1",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016565.1.26",
          "form": "그래서 책읽기 엄청 조아요",
          "original_form": "그래서 책읽기 엄청 조아요",
          "speaker_id": "1",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016565.1.27",
          "form": "저는 부산인데",
          "original_form": "저는 부산인데",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016565.1.28",
          "form": "한번 알아봐야겠어요ㅜ",
          "original_form": "한번 알아봐야겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016565.1.29",
          "form": "아마 없는거 같긴한데",
          "original_form": "아마 없는거 같긴한데",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016565.1.30",
          "form": "있으면 좋을거같아요ㅜ",
          "original_form": "있으면 좋을거같아요ㅜ",
