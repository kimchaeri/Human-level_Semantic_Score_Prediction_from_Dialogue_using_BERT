      "utterance": [
        {
          "id": "MDRW2100014674.1.1",
          "form": "방금 막 먹었어요",
          "original_form": "방금 막 먹었어요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014674.1.2",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014674.1.3",
          "form": "name2님도 집에서 식사하셨나요?",
          "original_form": "&name2&님도 집에서 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014674.1.4",
          "form": "네 집에서 먹었어요",
          "original_form": "네 집에서 먹었어요",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014674.1.5",
          "form": "요즘 저녁 밖에서먹기 쉽지 않아요",
          "original_form": "요즘 저녁 밖에서먹기 쉽지 않아요",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014674.1.6",
          "form": "9시까지만 영업이고 2인까지만 되서ㅜㅜ",
          "original_form": "9시까지만 영업이고 2인까지만 되서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014674.1.7",
          "form": "그래도 5.5 퇴근이면은\r\n퇴근 후 여유롭게 드실수\r\n있지 않아요?",
          "original_form": "그래도 5.5 퇴근이면은\r\n퇴근 후 여유롭게 드실수\r\n있지 않아요?",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014674.1.8",
          "form": "네 칼퇴를 했다면요 ㅋㅋ",
          "original_form": "네 칼퇴를 했다면요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014674.1.9",
          "form": "요즘은 칼퇴하고 있긴해요",
          "original_form": "요즘은 칼퇴하고 있긴해요",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014674.1.10",
          "form": "회사근처에 맛집 있어요? ㅋㅋ",
          "original_form": "회사근처에 맛집 있어요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014674.1.11",
          "form": "네 족발맛집 ㅋㅋ",
          "original_form": "네 족발맛집 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014674.1.12",
          "form": "진짜 맛있어요",
          "original_form": "진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014674.1.13",
          "form": "홀ㅋㅋ 저 족발 완전 좋아하는데",
          "original_form": "홀ㅋㅋ 저 족발 완전 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014674.1.14",
          "form": "좌표 좀 주심 안될까요? ㅋㅋ",
          "original_form": "좌표 좀 주심 안될까요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014674.1.15",
          "form": "성수동 족발맛집 검색해보셔요 ㅋㅋㅋ",
          "original_form": "성수동 족발맛집 검색해보셔요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014674.1.16",
          "form": "족발 좋아하시면 만족하실듯요?",
          "original_form": "족발 좋아하시면 만족하실듯요?",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014674.1.17",
          "form": "너무 많이 나오는데요?ㅋㅋ",
          "original_form": "너무 많이 나오는데요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014674.1.18",
          "form": "위풍당당?",
          "original_form": "위풍당당?",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014674.1.19",
          "form": "성수족발?",
          "original_form": "성수족발?",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014674.1.20",
          "form": "성수족발요",
          "original_form": "성수족발요",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014674.1.21",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014674.1.22",
          "form": "아항ㅋㅋ 감사합니당!",
          "original_form": "아항ㅋㅋ 감사합니당!",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014674.1.23",
          "form": "남편분이 사오시는건가요?ㅋㅋ",
          "original_form": "남편분이 사오시는건가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014674.1.24",
          "form": "ㅋㅋ아직 성수까지 가서\r\n절 위해 족발을 사 올\r\n사랑과 열정이 남아있는지\r\n함 테스트 해볼까요? ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ아직 성수까지 가서\r\n절 위해 족발을 사 올\r\n사랑과 열정이 남아있는지\r\n함 테스트 해볼까요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014674.1.25",
          "form": "집에서 먼가요?",
          "original_form": "집에서 먼가요?",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014674.1.26",
          "form": "얘기해보세요",
          "original_form": "얘기해보세요",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014674.1.27",
          "form": "사랑테스트 ㅋㅋㅋ",
          "original_form": "사랑테스트 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014674.1.28",
          "form": "저희회사 상무님이 성수족발이 서울 3대족발집이라고 하는데",
          "original_form": "저희회사 상무님이 성수족발이 서울 3대족발집이라고 하는데",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014674.1.29",
          "form": "검색해보니 차로 45분정도\r\n걸리네요 ㅋㅋㅋ",
          "original_form": "검색해보니 차로 45분정도\r\n걸리네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014674.1.30",
          "form": "와 그 정도인가요?",
          "original_form": "와 그 정도인가요?",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014674.1.31",
          "form": "그정도면 사올이유 충분 ㅋㅋ",
          "original_form": "그정도면 사올이유 충분 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014674.1.32",
          "form": "다른 두군데는 어딨어요? ㅋㅋ",
          "original_form": "다른 두군데는 어딨어요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014674.1.33",
          "form": "화곡? ㅋㅋ",
          "original_form": "화곡? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014674.1.34",
          "form": "그 두군데는 저도 몰라요 ㅋㅋㅋㅋㅋㅋ",
