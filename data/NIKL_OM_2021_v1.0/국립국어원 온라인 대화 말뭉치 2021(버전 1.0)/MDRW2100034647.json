      "utterance": [
        {
          "id": "MDRW2100034647.1.1",
          "form": "여전히 이시간은 뉴스 보신다ㅋ",
          "original_form": "여전히 이시간은 뉴스 보신다ㅋ",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100034647.1.2",
          "form": "오늘 살림남에 김제덕 나오는가본데",
          "original_form": "오늘 살림남에 김제덕 나오는가본데",
          "speaker_id": "2",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100034647.1.3",
          "form": "양궁 선수 말하는거지?",
          "original_form": "양궁 선수 말하는거지?",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100034647.1.4",
          "form": "엉 나도 인터넷 기사 얼핏봤다",
          "original_form": "엉 나도 인터넷 기사 얼핏봤다",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100034647.1.5",
          "form": "살림남은 별 관계가 없는거 같은데ㅋ",
          "original_form": "살림남은 별 관계가 없는거 같은데ㅋ",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100034647.1.6",
          "form": "아형에 전준하랑 씨엘 나온다",
          "original_form": "아형에 전준하랑 씨엘 나온다",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100034647.1.7",
          "form": "둘이 뭐지?  같이 나오게",
          "original_form": "둘이 뭐지?  같이 나오게",
          "speaker_id": "1",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100034647.1.8",
          "form": "엉 같이 나오네 친분이 잇는갑다",
          "original_form": "엉 같이 나오네 친분이 잇는갑다",
          "speaker_id": "2",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100034647.1.9",
          "form": "의외의 친분이네",
          "original_form": "의외의 친분이네",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034647.1.10",
          "form": "그러게 신민아 드라마 하네",
          "original_form": "그러게 신민아 드라마 하네",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034647.1.11",
          "form": "아 1박 2 일 나오는 남자 김선호 나오드라",
          "original_form": "아 1박 2 일 나오는 남자 김선호 나오드라",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
