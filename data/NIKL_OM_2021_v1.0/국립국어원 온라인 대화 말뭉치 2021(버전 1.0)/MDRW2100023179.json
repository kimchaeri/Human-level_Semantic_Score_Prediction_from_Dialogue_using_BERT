      "utterance": [
        {
          "id": "MDRW2100023179.1.1",
          "form": "아 저도 그런 면이 잇긴 하겟네여",
          "original_form": "아 저도 그런 면이 잇긴 하겟네여",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023179.1.2",
          "form": "어렸을 때부터 딱지만 보면",
          "original_form": "어렸을 때부터 딱지만 보면",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023179.1.3",
          "form": "긁고 싶어져가지고....",
          "original_form": "긁고 싶어져가지고....",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023179.1.4",
          "form": "나를 원망해야디",
          "original_form": "나를 원망해야디",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023179.1.5",
          "form": "그걸 참는 게 인간이냐구요!!!!!",
          "original_form": "그걸 참는 게 인간이냐구요!!!!!",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.6",
          "form": "걔네들이 이상한거 아니에요?ㅠ",
          "original_form": "걔네들이 이상한거 아니에요?ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.7",
          "form": "저도 딱지 긁으면 흉터가 생긴다눈",
          "original_form": "저도 딱지 긁으면 흉터가 생긴다눈",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.8",
          "form": "생각울 안해봣져",
          "original_form": "생각울 안해봣져",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.9",
          "form": "머리는 아는데 그 쾌감을 못참아요",
          "original_form": "머리는 아는데 그 쾌감을 못참아요",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.10",
          "form": "아니 간지러운데 어케 참지",
          "original_form": "아니 간지러운데 어케 참지",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.11",
          "form": "맞아.. ㅠㅠㅠ",
          "original_form": "맞아.. ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.12",
          "form": "떼고싶은데 인지상정아닌가",
          "original_form": "떼고싶은데 인지상정아닌가",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.13",
          "form": "과거의 나에게 돌아가서 딱지 떼면 후회한다고 알려주고 싶네요",
          "original_form": "과거의 나에게 돌아가서 딱지 떼면 후회한다고 알려주고 싶네요",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.14",
          "form": "휴 그니까여 저도",
          "original_form": "휴 그니까여 저도",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023179.1.15",
          "form": "그래도 이목구비라도 건져서 다행이에요",
          "original_form": "그래도 이목구비라도 건져서 다행이에요",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.16",
          "form": "크 자신잇나보다",
          "original_form": "크 자신잇나보다",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.18",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.19",
          "form": "남자들도 좋아하는걸요",
          "original_form": "남자들도 좋아하는걸요",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.20",
          "form": "저는 그냥 뭐 대충 생긴",
          "original_form": "저는 그냥 뭐 대충 생긴",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.21",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.22",
          "form": "생기다 만 것보다 나아요 누나...",
          "original_form": "생기다 만 것보다 나아요 누나...",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.24",
          "form": "한국은 확실히 외모지상주의에요",
          "original_form": "한국은 확실히 외모지상주의에요",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.25",
          "form": "그냥 비호감은 아니다 정도인데",
          "original_form": "그냥 비호감은 아니다 정도인데",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023179.1.26",
          "form": "잘생기면 자다가도 떡이 생긴다 라는 속담도 만들어야 함",
          "original_form": "잘생기면 자다가도 떡이 생긴다 라는 속담도 만들어야 함",
