      "utterance": [
        {
          "id": "MDRW2100028504.1.1",
          "form": "자주보는 유튜브 뭐에요",
          "original_form": "자주보는 유튜브 뭐에요",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028504.1.2",
          "form": "저는 유튜브로 요즘 노래 많이 들어요",
          "original_form": "저는 유튜브로 요즘 노래 많이 들어요",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028504.1.3",
          "form": "노래 안 들으면 보통 동물 영상 보고",
          "original_form": "노래 안 들으면 보통 동물 영상 보고",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028504.1.4",
          "form": "아니면 게임 영상 보네요",
          "original_form": "아니면 게임 영상 보네요",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028504.1.5",
          "form": "저는 아스트로 영상 찾아봐요 ㅋㅋ",
          "original_form": "저는 아스트로 영상 찾아봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028504.1.6",
          "form": "아이돌 무대 같은거보고 따라해요",
          "original_form": "아이돌 무대 같은거보고 따라해요",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028504.1.7",
          "form": "헉 아스트로 좋아하시는구나",
          "original_form": "헉 아스트로 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028504.1.8",
          "form": "네네 누구좋아하세여",
          "original_form": "네네 누구좋아하세여",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028504.1.9",
          "form": "저는 요즘 막 좋아하는 아이돌은 이제 없고",
          "original_form": "저는 요즘 막 좋아하는 아이돌은 이제 없고",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028504.1.10",
          "form": "그냥 친구들 좋아할때마다 박수쳐줍니다",
          "original_form": "그냥 친구들 좋아할때마다 박수쳐줍니다",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028504.1.11",
          "form": "전에는 투피엠 좋아했어요",
          "original_form": "전에는 투피엠 좋아했어요",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028504.1.12",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028504.1.13",
          "form": "박수",
          "original_form": "박수",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028504.1.14",
          "form": "헐 투피엠",
          "original_form": "헐 투피엠",
