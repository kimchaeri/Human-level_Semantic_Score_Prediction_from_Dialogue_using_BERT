      "utterance": [
        {
          "id": "MDRW2100028238.1.1",
          "form": "뽑기 나오면...저도 정털리는 것 같아요",
          "original_form": "뽑기 나오면...저도 정털리는 것 같아요",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028238.1.2",
          "form": "도박장과...",
          "original_form": "도박장과...",
          "speaker_id": "1",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028238.1.3",
          "form": "네 ㅋㅋㅋㅋㅋ정떨어져요",
          "original_form": "네 ㅋㅋㅋㅋㅋ정떨어져요",
          "speaker_id": "1",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028238.1.4",
          "form": "오버워치같은 게임들은 실력이라 좀 다행이었지..",
          "original_form": "오버워치같은 게임들은 실력이라 좀 다행이었지..",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028238.1.5",
          "form": "전 또 레벨올리거나 랭크올리느게임 관심없거든여",
          "original_form": "전 또 레벨올리거나 랭크올리느게임 관심없거든여",
          "speaker_id": "1",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028238.1.6",
          "form": "와 그럼 진짜 할만한 게임이",
          "original_form": "와 그럼 진짜 할만한 게임이",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028238.1.7",
          "form": "게임상에 돌아다니는걸좋아해서 항상저렙으로 ..",
          "original_form": "게임상에 돌아다니는걸좋아해서 항상저렙으로 ..",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.8",
          "form": "없으셨겠는데요??",
          "original_form": "없으셨겠는데요??",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.9",
          "form": "그래서 요즘 할게없어요",
          "original_form": "그래서 요즘 할게없어요",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.10",
          "form": "아 그럼 마인크래프트 같은 게임 좋아하시겠다",
          "original_form": "아 그럼 마인크래프트 같은 게임 좋아하시겠다",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.11",
          "form": "아 마인크래프트도 한때했었난데",
          "original_form": "아 마인크래프트도 한때했었난데",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.12",
          "form": "네네 ㅋㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.13",
          "form": "역시 ㅋㅋㅋ 그것도 고인물이 많아서",
          "original_form": "역시 ㅋㅋㅋ 그것도 고인물이 많아서",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.14",
          "form": "전 완전 초짜라..",
          "original_form": "전 완전 초짜라..",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.15",
          "form": "근데 그건 넘 금방재미없어지대요",
          "original_form": "근데 그건 넘 금방재미없어지대요",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.16",
          "form": "제가보기엔 마크 하는 게임러들도",
          "original_form": "제가보기엔 마크 하는 게임러들도",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.17",
          "form": "약간 광기가 있는 것 같아요",
          "original_form": "약간 광기가 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028238.1.18",
          "form": "초대받아서링크들어가거나해야됴ㅣ니까",
          "original_form": "초대받아서링크들어가거나해야됴ㅣ니까",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028238.1.19",
          "form": "귀찮고..혼자하면 재미없고",
          "original_form": "귀찮고..혼자하면 재미없고",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028238.1.20",
          "form": "아 맞네맞네",
          "original_form": "아 맞네맞네",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028238.1.21",
          "form": "어서 가상현실게임같은거 생겼으면좋겠어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어서 가상현실게임같은거 생겼으면좋겠어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028238.1.22",
          "form": "시간 다들 맞아야 재밌는 게임...",
          "original_form": "시간 다들 맞아야 재밌는 게임...",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028238.1.23",
