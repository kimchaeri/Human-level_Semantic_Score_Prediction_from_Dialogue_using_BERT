      "utterance": [
        {
          "id": "MDRW2100035145.1.1",
          "form": "투움바파스타랑",
          "original_form": "투움바파스타랑",
          "speaker_id": "2",
          "time": "20210829 07:02"
        },
        {
          "id": "MDRW2100035145.1.2",
          "form": "메뉴 하나 시키는데",
          "original_form": "메뉴 하나 시키는데",
          "speaker_id": "2",
          "time": "20210829 07:03"
        },
        {
          "id": "MDRW2100035145.1.3",
          "form": "배터지게 먹고싶으면",
          "original_form": "배터지게 먹고싶으면",
          "speaker_id": "2",
          "time": "20210829 07:03"
        },
        {
          "id": "MDRW2100035145.1.4",
          "form": "베이비백립도 시켜요!",
          "original_form": "베이비백립도 시켜요!",
          "speaker_id": "2",
          "time": "20210829 07:03"
        },
        {
          "id": "MDRW2100035145.1.5",
          "form": "남으면 포장하면 되니까ㅋㅋ",
          "original_form": "남으면 포장하면 되니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 07:03"
        },
        {
          "id": "MDRW2100035145.1.6",
          "form": "오 ㅋㅋㅋㅋ 말씀하시니 폭립먹고싶어지네요 큐ㅠㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋ 말씀하시니 폭립먹고싶어지네요 큐ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 07:04"
        },
        {
          "id": "MDRW2100035145.1.7",
          "form": "계절밥상같은 곳 자주가세요??",
          "original_form": "계절밥상같은 곳 자주가세요??",
          "speaker_id": "2",
          "time": "20210829 07:07"
        },
        {
          "id": "MDRW2100035145.1.8",
          "form": "애슐리같은 뷔페식당",
          "original_form": "애슐리같은 뷔페식당",
          "speaker_id": "2",
          "time": "20210829 07:07"
        },
        {
          "id": "MDRW2100035145.1.9",
          "form": "아 어릴땐 자주갔었는데 커서는 뷔페 잘 안가게되는거같아요 ㅠ",
          "original_form": "아 어릴땐 자주갔었는데 커서는 뷔페 잘 안가게되는거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100035145.1.10",
          "form": "맞아요.. 가기 전에는",
          "original_form": "맞아요.. 가기 전에는",
          "speaker_id": "2",
          "time": "20210830 11:13"
        },
        {
          "id": "MDRW2100035145.1.11",
          "form": "진짜 많이 먹을거라고 다짐해도 막상가면",
          "original_form": "진짜 많이 먹을거라고 다짐해도 막상가면",
          "speaker_id": "2",
          "time": "20210830 11:14"
        },
        {
          "id": "MDRW2100035145.1.12",
          "form": "먹는 것만 먹고 금방 질리더라고요",
          "original_form": "먹는 것만 먹고 금방 질리더라고요",
          "speaker_id": "2",
          "time": "20210830 11:14"
        },
        {
          "id": "MDRW2100035145.1.13",
          "form": "맞아요 ㅜ 생각할수록 가성비가 떨어져서 안가게되더라구요",
          "original_form": "맞아요 ㅜ 생각할수록 가성비가 떨어져서 안가게되더라구요",
          "speaker_id": "1",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100035145.1.14",
          "form": "누가 사주면 가고 싶은데",
          "original_form": "누가 사주면 가고 싶은데",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100035145.1.15",
          "form": "사람들도 많이 안 가는지 매장찾기도 힘들더라고요",
          "original_form": "사람들도 많이 안 가는지 매장찾기도 힘들더라고요",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100035145.1.16",
          "form": "인정이요! 요새는 특히나 시국이시국인지라 더그래요",
          "original_form": "인정이요! 요새는 특히나 시국이시국인지라 더그래요",
          "speaker_id": "1",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100035145.1.17",
          "form": "점심 드셨어요~?",
          "original_form": "점심 드셨어요~?",
          "speaker_id": "2",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100035145.1.18",
          "form": "넹ㅋㅋㅋㅋ 어젠 불고기덮밥먹었어요",
          "original_form": "넹ㅋㅋㅋㅋ 어젠 불고기덮밥먹었어요",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100035145.1.19",
          "form": "오늘드셨나요?",
          "original_form": "오늘드셨나요?",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100035145.1.20",
          "form": "밤에 야식으로",
          "original_form": "밤에 야식으로",
          "speaker_id": "2",
          "time": "20210901 06:14"
        },
        {
          "id": "MDRW2100035145.1.21",
          "form": "치즈찜닭 먹었어요ㅋㅋㅋㅋ",
          "original_form": "치즈찜닭 먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 06:14"
        },
        {
          "id": "MDRW2100035145.1.22",
          "form": "치즈폭탄으로 시켜서",
          "original_form": "치즈폭탄으로 시켜서",
          "speaker_id": "2",
          "time": "20210901 06:15"
        },
        {
          "id": "MDRW2100035145.1.23",
          "form": "오 너무맛나겠네요 ㅠㅠ",
          "original_form": "오 너무맛나겠네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100035145.1.24",
          "form": "저도오늘 야식먹었어요~ㅋㅋㅋㅋ",
          "original_form": "저도오늘 야식먹었어요~ㅋㅋㅋㅋ",
