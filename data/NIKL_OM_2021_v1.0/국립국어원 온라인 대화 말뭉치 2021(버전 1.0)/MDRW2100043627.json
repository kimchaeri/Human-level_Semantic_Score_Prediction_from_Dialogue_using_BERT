        {
          "id": "MDRW2100043627.1.1",
          "form": "베라좋아하세요",
          "original_form": "베라좋아하세요",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043627.1.2",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043627.1.3",
          "form": "무슨 맛 좋아하세요?",
          "original_form": "무슨 맛 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043627.1.4",
          "form": "저는 민초요ㅎㅎ",
          "original_form": "저는 민초요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043627.1.5",
          "form": "헉 민초 드시는구나",
          "original_form": "헉 민초 드시는구나",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043627.1.6",
          "form": "저는 먹긴 먹는데 막 찾아먹진 않아요 ㅋㅋ",
          "original_form": "저는 먹긴 먹는데 막 찾아먹진 않아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043627.1.7",
          "form": "민트초코 우유는 좋아해요!",
          "original_form": "민트초코 우유는 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043627.1.8",
          "form": "ㅎㅎㅎ뉴욕치케도좋아해요",
          "original_form": "ㅎㅎㅎ뉴욕치케도좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043627.1.9",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043627.1.10",
          "form": "저는 엄외 뉴치케 아몬드봉봉 쿠앤크 이런 거 좋아해요 ㅋㅋ",
          "original_form": "저는 엄외 뉴치케 아몬드봉봉 쿠앤크 이런 거 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043627.1.11",
          "form": "레인보우샤베트두요",
          "original_form": "레인보우샤베트두요",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043627.1.12",
          "form": "쿠앤크 맛있죠",
          "original_form": "쿠앤크 맛있죠",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043627.1.13",
          "form": "네 ㅋㅋㅋ 어제두 그냥 쿠앤크 한 통 사다 퍼먹었어요",
          "original_form": "네 ㅋㅋㅋ 어제두 그냥 쿠앤크 한 통 사다 퍼먹었어요",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043627.1.14",
          "form": "ㅎㅎㅎㅎ행복하셨군요",
          "original_form": "ㅎㅎㅎㅎ행복하셨군요",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043627.1.15",
          "form": "ㅋㅋㅋㅋ네 평화를 챙겼습니다",
          "original_form": "ㅋㅋㅋㅋ네 평화를 챙겼습니다",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043627.1.16",
          "form": "아이스크림 자주 드시는 편인가요?",
          "original_form": "아이스크림 자주 드시는 편인가요?",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043627.1.17",
          "form": "전 종종 땡기더라구요",
          "original_form": "전 종종 땡기더라구요",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043627.1.18",
          "form": "베라좋아해요",
          "original_form": "베라좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043627.1.19",
          "form": "베라만 드시나요?",
          "original_form": "베라만 드시나요?",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043627.1.20",
          "form": "마트꺼도먹어요",
          "original_form": "마트꺼도먹어요",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043627.1.21",
          "form": "저는 집 주변에 그 아이스크림 할인점이 있어서 자주 가요",
          "original_form": "저는 집 주변에 그 아이스크림 할인점이 있어서 자주 가요",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043627.1.22",
          "form": "빠삐코 좋아해요 ㅋㅋ",
          "original_form": "빠삐코 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043627.1.23",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043627.1.24",
