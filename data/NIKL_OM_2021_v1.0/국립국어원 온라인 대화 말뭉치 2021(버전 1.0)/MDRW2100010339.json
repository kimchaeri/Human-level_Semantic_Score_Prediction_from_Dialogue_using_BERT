      "utterance": [
        {
          "id": "MDRW2100010339.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 17:00"
        },
        {
          "id": "MDRW2100010339.1.2",
          "form": "열받는 주제 나왔네요",
          "original_form": "열받는 주제 나왔네요",
          "speaker_id": "2",
          "time": "20210823 17:01"
        },
        {
          "id": "MDRW2100010339.1.3",
          "form": "저 호날두 경기 직관한 사람입니다",
          "original_form": "저 호날두 경기 직관한 사람입니다",
          "speaker_id": "2",
          "time": "20210823 17:01"
        },
        {
          "id": "MDRW2100010339.1.4",
          "form": "날강두..",
          "original_form": "날강두..",
          "speaker_id": "2",
          "time": "20210823 17:02"
        },
        {
          "id": "MDRW2100010339.1.5",
          "form": "와우 왜 날강두인가요ㅋㅋㅋ 어떤경기봤나요",
          "original_form": "와우 왜 날강두인가요ㅋㅋㅋ 어떤경기봤나요",
          "speaker_id": "1",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010339.1.6",
          "form": "저 해축좋아해요",
          "original_form": "저 해축좋아해요",
          "speaker_id": "1",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010339.1.7",
          "form": "한국에 유벤투스 이벤트경기 직관했어요 부들부들",
          "original_form": "한국에 유벤투스 이벤트경기 직관했어요 부들부들",
          "speaker_id": "2",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100010339.1.8",
          "form": "뛴다고 했는데 안 뛰었던",
          "original_form": "뛴다고 했는데 안 뛰었던",
          "speaker_id": "2",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010339.1.9",
          "form": "오 해축 좋아하는 클럽 있으세요?",
          "original_form": "오 해축 좋아하는 클럽 있으세요?",
          "speaker_id": "2",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010339.1.10",
          "form": "선수나",
          "original_form": "선수나",
          "speaker_id": "2",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010339.1.11",
          "form": "아 기억나네요ㅋㅋㅋ 그경기",
          "original_form": "아 기억나네요ㅋㅋㅋ 그경기",
          "speaker_id": "1",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010339.1.12",
          "form": "토트넘이요ㅋㅋㅋ",
          "original_form": "토트넘이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010339.1.13",
          "form": "쏘니",
          "original_form": "쏘니",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010339.1.14",
          "form": "어제 경기도 봤답니다",
          "original_form": "어제 경기도 봤답니다",
          "speaker_id": "1",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010339.1.15",
          "form": "아ㅋㅋㅋ 우리흥!",
          "original_form": "아ㅋㅋㅋ 우리흥!",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010339.1.16",
          "form": "예전엔 밤새 경기 챙겨보고 나가고 그랬었는데",
          "original_form": "예전엔 밤새 경기 챙겨보고 나가고 그랬었는데",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010339.1.17",
          "form": "요새는 시들해졌어요 왜인지는 모르겠지만ㅋㅋ",
          "original_form": "요새는 시들해졌어요 왜인지는 모르겠지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010339.1.18",
          "form": "어제 흥민이 잘했나요?",
          "original_form": "어제 흥민이 잘했나요?",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010339.1.19",
          "form": "ㅋㅋㅋ저는 박지성때 한창봤었어요 라리가 epl돌려가면서",
          "original_form": "ㅋㅋㅋ저는 박지성때 한창봤었어요 라리가 epl돌려가면서",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010339.1.20",
          "form": "어제 교체됐어요ㅠ 어제는 음...",
          "original_form": "어제 교체됐어요ㅠ 어제는 음...",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010339.1.21",
          "form": "안보는게나을거예요ㅋㅋㅋ",
          "original_form": "안보는게나을거예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010339.1.22",
          "form": "크~ 지성이형때 진짜 재밌었죠",
          "original_form": "크~ 지성이형때 진짜 재밌었죠",
          "speaker_id": "2",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100010339.1.23",
          "form": "경기 많이 나오질 않아서 아쉬웠지만",
          "original_form": "경기 많이 나오질 않아서 아쉬웠지만",
          "speaker_id": "2",
          "time": "20210823 17:27"
        },
        {
          "id": "MDRW2100010339.1.24",
          "form": "근데 손흥민",
          "original_form": "근데 손흥민",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010339.1.25",
          "form": "맨시티전때 장난아니었어요ㅋㅋㅋ",
          "original_form": "맨시티전때 장난아니었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010339.1.26",
          "form": "앞으로도 잘하길ㅠ 응원함",
          "original_form": "앞으로도 잘하길ㅠ 응원함",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010339.1.27",
          "form": "맞아요ㅋㅋ 개막전에서 날라다녔죠",
          "original_form": "맞아요ㅋㅋ 개막전에서 날라다녔죠",
          "speaker_id": "2",
          "time": "20210823 18:01"
        },
        {
          "id": "MDRW2100010339.1.28",
          "form": "하 흥민이 국대에서 날라다니게 누가 도와줘야하는데 아쉽네요",
          "original_form": "하 흥민이 국대에서 날라다니게 누가 도와줘야하는데 아쉽네요",
          "speaker_id": "2",
          "time": "20210823 18:02"
        },
        {
          "id": "MDRW2100010339.1.29",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:40"
        },
        {
          "id": "MDRW2100010339.1.30",
          "form": "이강인 안나오나요?",
          "original_form": "이강인 안나오나요?",
          "speaker_id": "1",
          "time": "20210824 10:40"
        },
        {
          "id": "MDRW2100010339.1.31",
          "form": "강인이 혼자서는... 버거워보이더라구여",
          "original_form": "강인이 혼자서는... 버거워보이더라구여",
          "speaker_id": "2",
          "time": "20210824 10:50"
        }
      ]
    }
