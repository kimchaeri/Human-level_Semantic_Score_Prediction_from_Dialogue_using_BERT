      "utterance": [
        {
          "id": "MDRW2100021004.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100021004.1.2",
          "form": "키워본적이 없어요 ㅠㅋㅋㅋ",
          "original_form": "키워본적이 없어요 ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100021004.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100021004.1.4",
          "form": "저도 생각만요",
          "original_form": "저도 생각만요",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100021004.1.5",
          "form": "고양이파세요 강아지파세요?",
          "original_form": "고양이파세요 강아지파세요?",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100021004.1.6",
          "form": "뭐 키우고 싶으세요?",
          "original_form": "뭐 키우고 싶으세요?",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100021004.1.7",
          "form": "전 고양이요 ㅋㅋㅋ",
          "original_form": "전 고양이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100021004.1.8",
          "form": "저도 고양이요",
          "original_form": "저도 고양이요",
          "speaker_id": "1",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100021004.1.9",
          "form": "통했네요ㅋㅋㅋ",
          "original_form": "통했네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100021004.1.10",
          "form": "오오 같네요 ㅋㅋㅋ",
          "original_form": "오오 같네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100021004.1.11",
          "form": "무슨종 좋아하세요?",
          "original_form": "무슨종 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100021004.1.12",
          "form": "보통 강아지를 더 선호하시더라구요",
          "original_form": "보통 강아지를 더 선호하시더라구요",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100021004.1.13",
          "form": "아 저는 종까지는 잘 몰라서 ㅋㅋㅋ 그냥 고양이면 다 이쁘더라구요 ㅋㅋㅋㅋ",
          "original_form": "아 저는 종까지는 잘 몰라서 ㅋㅋㅋ 그냥 고양이면 다 이쁘더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100021004.1.14",
          "form": "아 저도 다예쁘긴한데",
          "original_form": "아 저도 다예쁘긴한데",
          "speaker_id": "1",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100021004.1.15",
          "form": "그래도 최애는있어요",
          "original_form": "그래도 최애는있어요",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100021004.1.16",
          "form": "오뭔가요?",
          "original_form": "오뭔가요?",
          "speaker_id": "2",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100021004.1.17",
          "form": "스코티시폴드요",
          "original_form": "스코티시폴드요",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100021004.1.18",
          "form": "오 쳐봐야겠어요 나중에 ㅋㅋㅋ",
          "original_form": "오 쳐봐야겠어요 나중에 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100021004.1.19",
          "form": "너무 귀여워요",
          "original_form": "너무 귀여워요",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100021004.1.20",
          "form": "귀가 접힌게",
          "original_form": "귀가 접힌게",
