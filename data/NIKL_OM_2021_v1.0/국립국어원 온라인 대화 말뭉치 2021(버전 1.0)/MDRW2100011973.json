      "utterance": [
        {
          "id": "MDRW2100011973.1.1",
          "form": "구피 키우나??",
          "original_form": "구피 키우나??",
          "speaker_id": "1",
          "time": "20210824 18:00"
        },
        {
          "id": "MDRW2100011973.1.2",
          "form": "응 구피 아직 키우지ㅋ",
          "original_form": "응 구피 아직 키우지ㅋ",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100011973.1.3",
          "form": "언니는?ㅋㅋ",
          "original_form": "언니는?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100011973.1.4",
          "form": "우린 애벌레 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "우린 애벌레 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100011973.1.5",
          "form": "지금은 번데기",
          "original_form": "지금은 번데기",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100011973.1.6",
          "form": "곧 나비가 되겠지",
          "original_form": "곧 나비가 되겠지",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100011973.1.7",
          "form": "아 맞다 애벌레기",
          "original_form": "아 맞다 애벌레기",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100011973.1.8",
          "form": "애벌레ㅋ",
          "original_form": "애벌레ㅋ",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100011973.1.9",
          "form": "울집 구피 5마리였는데",
          "original_form": "울집 구피 5마리였는데",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100011973.1.10",
          "form": "한마리가 없어졌다",
          "original_form": "한마리가 없어졌다",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100011973.1.11",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100011973.1.12",
          "form": "잡아먹었는가....",
          "original_form": "잡아먹었는가....",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100011973.1.13",
          "form": "우리 name3는 아직도 고양이 키우자고",
          "original_form": "우리 &name3&는 아직도 고양이 키우자고",
          "speaker_id": "1",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100011973.1.14",
          "form": "잡아 먹었나 보다",
          "original_form": "잡아 먹었나 보다",
          "speaker_id": "2",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100011973.1.15",
          "form": "우리 첫째도 강아지 키우자고 한다",
          "original_form": "우리 첫째도 강아지 키우자고 한다",
          "speaker_id": "2",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100011973.1.16",
          "form": "하 강아지",
          "original_form": "하 강아지",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100011973.1.17",
          "form": "강아지 키울 생각 있나",
          "original_form": "강아지 키울 생각 있나",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100011973.1.18",
          "form": "귀엽긴 하더라",
          "original_form": "귀엽긴 하더라",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100011973.1.19",
          "form": "키울생각 지금은 없지",
          "original_form": "키울생각 지금은 없지",
          "speaker_id": "2",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100011973.1.20",
          "form": "나중에는 있고??",
          "original_form": "나중에는 있고??",
          "speaker_id": "1",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100011973.1.21",
          "form": "나도 정말 생각 없었는데",
          "original_form": "나도 정말 생각 없었는데",
          "speaker_id": "1",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100011973.1.22",
          "form": "우리 name3가 너무 고양이를 좋아하니까",
          "original_form": "우리 &name3&가 너무 고양이를 좋아하니까",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100011973.1.23",
          "form": "name3 초등학생 되면 키울까 생각중",
          "original_form": "&name3& 초등학생 되면 키울까 생각중",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100011973.1.24",
          "form": "응 나중에는 키울수도 있을것같아ㅋ",
          "original_form": "응 나중에는 키울수도 있을것같아ㅋ",
          "speaker_id": "2",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100011973.1.25",
          "form": "언니 고양이키우면 구경가야지ㅋ",
          "original_form": "언니 고양이키우면 구경가야지ㅋ",
          "speaker_id": "2",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100011973.1.26",
          "form": "그래 105도 고양이 입양 매일 고민함\r\nㅎㅎ",
          "original_form": "그래 105도 고양이 입양 매일 고민함\r\nㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 07:51"
        },
        {
          "id": "MDRW2100011973.1.27",
          "form": "더자고 싶군",
          "original_form": "더자고 싶군",
          "speaker_id": "1",
          "time": "20210825 07:51"
        },
        {
          "id": "MDRW2100011973.1.28",
          "form": "ㅋㅋㅋㅋ좋다좋다ㅋㅋ 고양이 귀엽더라. 내친구 키우는거보니ㅋ",
          "original_form": "ㅋㅋㅋㅋ좋다좋다ㅋㅋ 고양이 귀엽더라. 내친구 키우는거보니ㅋ",
          "speaker_id": "2",
          "time": "20210825 08:15"
        },
        {
          "id": "MDRW2100011973.1.29",
          "form": "우리 구피1마리는 잡아먹혔나봐ㅜ흔적이없다ㅜ슬프네ㅜ",
          "original_form": "우리 구피1마리는 잡아먹혔나봐ㅜ흔적이없다ㅜ슬프네ㅜ",
          "speaker_id": "2",
          "time": "20210825 08:25"
        },
        {
          "id": "MDRW2100011973.1.30",
          "form": "웁스 ㅠㅠ",
          "original_form": "웁스 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 09:03"
        },
        {
          "id": "MDRW2100011973.1.31",
          "form": "우리도 전에\r\n구피 키울때",
          "original_form": "우리도 전에\r\n구피 키울때",
          "speaker_id": "1",
          "time": "20210825 09:03"
        },
        {
          "id": "MDRW2100011973.1.32",
          "form": "잡아먹고 하니까 못키우겠....",
          "original_form": "잡아먹고 하니까 못키우겠....",
