      "utterance": [
        {
          "id": "MDRW2100017017.1.1",
          "form": "머 키우세요?",
          "original_form": "머 키우세요?",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017017.1.2",
          "form": "키우고싶은데 부모님이 허락안해주시네요 ㅠ",
          "original_form": "키우고싶은데 부모님이 허락안해주시네요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017017.1.3",
          "form": "저키우기도 바쁘대요",
          "original_form": "저키우기도 바쁘대요",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017017.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017017.1.5",
          "form": "역시 자식농사가..",
          "original_form": "역시 자식농사가..",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017017.1.6",
          "form": "동물은 뭐 좋아하세요?",
          "original_form": "동물은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017017.1.7",
          "form": "귀엽게생긴건 다좋아해요 ㅋㅋㅋㅋ",
          "original_form": "귀엽게생긴건 다좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017017.1.8",
          "form": "제일 만만한게 강아지죠",
          "original_form": "제일 만만한게 강아지죠",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017017.1.9",
          "form": "만만하대ㅋㅋㅋㅋㅋㅋ",
          "original_form": "만만하대ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017017.1.10",
          "form": "저 포메 키웠었어요",
          "original_form": "저 포메 키웠었어요",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017017.1.11",
          "form": "근데 생각보다 힘들더라구요 키우는게",
          "original_form": "근데 생각보다 힘들더라구요 키우는게",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017017.1.12",
          "form": "강아지가 나만 바라보니까 이게 좋으면서도 버거운",
          "original_form": "강아지가 나만 바라보니까 이게 좋으면서도 버거운",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017017.1.13",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017017.1.14",
          "form": "포메가 4가지가 진짜없다는데",
          "original_form": "포메가 4가지가 진짜없다는데",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017017.1.15",
          "form": "머야",
          "original_form": "머야",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017017.1.16",
          "form": "4가지가 나쁜말이군요?",
          "original_form": "4가지가 나쁜말이군요?",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017017.1.17",
          "form": "처음알았네",
          "original_form": "처음알았네",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017017.1.18",
          "form": "4가지라기보다는",
          "original_form": "4가지라기보다는",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017017.1.19",
          "form": "지가 쎈줄알아요",
          "original_form": "지가 쎈줄알아요",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017017.1.20",
          "form": "약아빠짐",
          "original_form": "약아빠짐",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017017.1.21",
          "form": "똑똑한데 멍청하고",
          "original_form": "똑똑한데 멍청하고",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017017.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017017.1.23",
          "form": "근데 ㅠㅠ파양하셨어요?",
          "original_form": "근데 ㅠㅠ파양하셨어요?",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017017.1.24",
          "form": "어캐됐어요",
          "original_form": "어캐됐어요",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017017.1.25",
          "form": "댕댕이 출가했어요ㅠ",
          "original_form": "댕댕이 출가했어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017017.1.26",
          "form": "외할머니가 옛날에 저희 집에 오셔서 좀 사셨는데",
          "original_form": "외할머니가 옛날에 저희 집에 오셔서 좀 사셨는데",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017017.1.27",
          "form": "이놈이 할머니 약한거알고",
          "original_form": "이놈이 할머니 약한거알고",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017017.1.28",
          "form": "아주 혼구녕나야겠네요",
          "original_form": "아주 혼구녕나야겠네요",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017017.1.29",
          "form": "할머니 외출하실때 몰래 따라나감",
          "original_form": "할머니 외출하실때 몰래 따라나감",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017017.1.30",
          "form": "헐ㄹ",
          "original_form": "헐ㄹ",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017017.1.31",
          "form": "그뒤로 돌아오지않았군요,,,",
          "original_form": "그뒤로 돌아오지않았군요,,,",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017017.1.32",
          "form": "얼마나키웠는데요?",
          "original_form": "얼마나키웠는데요?",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.33",
          "form": "한번은 찾았어요 간신히",
          "original_form": "한번은 찾았어요 간신히",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.34",
          "form": "4년?",
          "original_form": "4년?",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.35",
          "form": "쯤 키웟어요",
          "original_form": "쯤 키웟어요",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.36",
          "form": "헥 근데 집을못찾아와요?",
          "original_form": "헥 근데 집을못찾아와요?",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.37",
          "form": "보호소에 있는거 간신히 찾았는데",
          "original_form": "보호소에 있는거 간신히 찾았는데",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.38",
          "form": "두번째는 어디로 갔는지...",
          "original_form": "두번째는 어디로 갔는지...",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.39",
          "form": "아파트라 그런가봐요",
          "original_form": "아파트라 그런가봐요",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.40",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.41",
          "form": "누가데리고갔겠네요 포메라 ㅜ",
          "original_form": "누가데리고갔겠네요 포메라 ㅜ",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017017.1.42",
          "form": "처음 찾았을땐",
          "original_form": "처음 찾았을땐",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017017.1.43",
          "form": "옆동가서 다른집 문에서",
          "original_form": "옆동가서 다른집 문에서",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017017.1.44",
          "form": "짖어대가지구 소방서에 잡혀가서 찾았고",
          "original_form": "짖어대가지구 소방서에 잡혀가서 찾았고",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017017.1.45",
          "form": "두번째는 아예 행방불명..",
          "original_form": "두번째는 아예 행방불명..",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017017.1.46",
          "form": "빌라였으면 잘 찾아왔으려나 싶기도하네요",
          "original_form": "빌라였으면 잘 찾아왔으려나 싶기도하네요",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017017.1.47",
          "form": "ㅠ 어디 잘 살아있길 빌며,,",
          "original_form": "ㅠ 어디 잘 살아있길 빌며,,",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017017.1.48",
          "form": "외갓집이 진돗개 키웠었는데",
          "original_form": "외갓집이 진돗개 키웠었는데",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017017.1.49",
          "form": "주택에 살때 마당에서 키우다가 재개발되면서 잠시 아파트로 이사가가지고",
          "original_form": "주택에 살때 마당에서 키우다가 재개발되면서 잠시 아파트로 이사가가지고",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017017.1.50",
          "form": "아는사람 집에 잠깐 맡겻엇거든요",
          "original_form": "아는사람 집에 잠깐 맡겻엇거든요",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017017.1.51",
          "form": "차로 1시간거리되는 시골이였는데 첨에 데리고 갔다가",
          "original_form": "차로 1시간거리되는 시골이였는데 첨에 데리고 갔다가",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017017.1.52",
          "form": "뛰쳐나가가지고 없어졌다고 연락받자마자 혹시나 해서 옛날집가보니 거기있었다고,,",
          "original_form": "뛰쳐나가가지고 없어졌다고 연락받자마자 혹시나 해서 옛날집가보니 거기있었다고,,",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017017.1.53",
          "form": "와.. 똑똑이네",
          "original_form": "와.. 똑똑이네",
          "speaker_id": "2",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017017.1.54",
          "form": "그래서 또 똑같은곳에다가 데려다놧는데 거기서 밥줘도 먹지도않고 그러더니 목줄이 헐렁해져서 또 없어지고 ㅠ",
          "original_form": "그래서 또 똑같은곳에다가 데려다놧는데 거기서 밥줘도 먹지도않고 그러더니 목줄이 헐렁해져서 또 없어지고 ㅠ",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017017.1.55",
          "form": "그러고나서는 못찾았어요",
          "original_form": "그러고나서는 못찾았어요",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017017.1.56",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017017.1.57",
          "form": "사촌언니가 진짜 애지중지키웠는데",
          "original_form": "사촌언니가 진짜 애지중지키웠는데",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017017.1.58",
          "form": "언니 차소리만나면 고개빼꼼내밀고",
          "original_form": "언니 차소리만나면 고개빼꼼내밀고",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017017.1.59",
          "form": "한번 키우고나니까 다시 키우는게 쉽지 않아요",
          "original_form": "한번 키우고나니까 다시 키우는게 쉽지 않아요",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017017.1.60",
          "form": "키우고 싶긴하고 잘 키울 자신도 있는데",
          "original_form": "키우고 싶긴하고 잘 키울 자신도 있는데",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017017.1.61",
          "form": "힘듬ㅎㅎ",
          "original_form": "힘듬ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017017.1.62",
          "form": "비용도 많이 든다고,,,",
          "original_form": "비용도 많이 든다고,,,",
          "speaker_id": "1",
          "time": "20210826 17:12"
