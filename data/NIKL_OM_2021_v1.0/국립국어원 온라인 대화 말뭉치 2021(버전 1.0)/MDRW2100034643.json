      "utterance": [
        {
          "id": "MDRW2100034643.1.1",
          "form": "건강위해 챙기는거 있나요",
          "original_form": "건강위해 챙기는거 있나요",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100034643.1.2",
          "form": "설탕 든거 안먹으려고 노력해요",
          "original_form": "설탕 든거 안먹으려고 노력해요",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034643.1.3",
          "form": "와 대단하시네요 식단관리 하시는건가요",
          "original_form": "와 대단하시네요 식단관리 하시는건가요",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100034643.1.4",
          "form": "그정돈 아니고요 음료수 잘 안먹어요 과자랑요",
          "original_form": "그정돈 아니고요 음료수 잘 안먹어요 과자랑요",
          "speaker_id": "1",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100034643.1.5",
          "form": "저는 과자를 좋아해서 늘 어렵네요 ㅠ",
          "original_form": "저는 과자를 좋아해서 늘 어렵네요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100034643.1.6",
          "form": "운동하시는거 있으세요?",
          "original_form": "운동하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100034643.1.7",
          "form": "네 헬스나 필테 돌아가면서 해요",
          "original_form": "네 헬스나 필테 돌아가면서 해요",
          "speaker_id": "2",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100034643.1.8",
          "form": "필라테스 안어렵나요?",
          "original_form": "필라테스 안어렵나요?",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034643.1.9",
          "form": "음..여전히 타이트하다고 많이 해야한다고 하시네요",
          "original_form": "음..여전히 타이트하다고 많이 해야한다고 하시네요",
          "speaker_id": "2",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100034643.1.10",
          "form": "저는 수영을 이삼년 했었어요",
          "original_form": "저는 수영을 이삼년 했었어요",
          "speaker_id": "1",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100034643.1.11",
          "form": "와 그럼 모든 영법을 잘 하시겠네요",
          "original_form": "와 그럼 모든 영법을 잘 하시겠네요",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034643.1.12",
          "form": "아뇨 접영은 잘 못해요 ㅜㅜ",
          "original_form": "아뇨 접영은 잘 못해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100034643.1.13",
          "form": "25m를 접영으로 못가요",
          "original_form": "25m를 접영으로 못가요",
          "speaker_id": "1",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100034643.1.14",
          "form": "2년이면 강습은 얼마나 받으셨나요?",
          "original_form": "2년이면 강습은 얼마나 받으셨나요?",
          "speaker_id": "2",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100034643.1.15",
          "form": "잘 기억이 안나요 ㅜㅜ 강습도 2년은 받은거같아요",
          "original_form": "잘 기억이 안나요 ㅜㅜ 강습도 2년은 받은거같아요",
