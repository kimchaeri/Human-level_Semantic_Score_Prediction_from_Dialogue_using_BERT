      "utterance": [
        {
          "id": "MDRW2100011760.1.1",
          "form": "최근 구매한 물건있으신가요?",
          "original_form": "최근 구매한 물건있으신가요?",
          "speaker_id": "1",
          "time": "20210824 15:51"
        },
        {
          "id": "MDRW2100011760.1.2",
          "form": "홈쇼핑으로 오징어주문했어요",
          "original_form": "홈쇼핑으로 오징어주문했어요",
          "speaker_id": "2",
          "time": "20210824 15:51"
        },
        {
          "id": "MDRW2100011760.1.3",
          "form": "어 저도 홈쇼핑 한번씩 이용하는데",
          "original_form": "어 저도 홈쇼핑 한번씩 이용하는데",
          "speaker_id": "1",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011760.1.4",
          "form": "정말 개미지옥아닌가요ㅎ",
          "original_form": "정말 개미지옥아닌가요ㅎ",
          "speaker_id": "2",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011760.1.5",
          "form": "맞아요ㅠ충동구매하게되더라고요",
          "original_form": "맞아요ㅠ충동구매하게되더라고요",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011760.1.6",
          "form": "저도 그래요ㅋㅋ뭐 구매하셨어요?",
          "original_form": "저도 그래요ㅋㅋ뭐 구매하셨어요?",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011760.1.7",
          "form": "최근에는 레이저제모기샀어요",
          "original_form": "최근에는 레이저제모기샀어요",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011760.1.8",
          "form": "제시가 선전하는건가요?",
          "original_form": "제시가 선전하는건가요?",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011760.1.9",
          "form": "네 맞아요!유라이크",
          "original_form": "네 맞아요!유라이크",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011760.1.10",
          "form": "구매후기 좀 남겨주세요",
          "original_form": "구매후기 좀 남겨주세요",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011760.1.11",
          "form": "귀찮아서 꾸준한 사용은 못하고있는데 그래도 몇번써보니 덜나더라고요ㅎㅎ",
          "original_form": "귀찮아서 꾸준한 사용은 못하고있는데 그래도 몇번써보니 덜나더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011760.1.12",
          "form": "신기하네요 저도 고민했는데",
          "original_form": "신기하네요 저도 고민했는데",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011760.1.13",
          "form": "홈쇼핑으로 구매하신거에요?",
          "original_form": "홈쇼핑으로 구매하신거에요?",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011760.1.14",
          "form": "네네 방송보다가 질렀어요",
          "original_form": "네네 방송보다가 질렀어요",
          "speaker_id": "1",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011760.1.15",
          "form": "가격이 괜찮길래",
          "original_form": "가격이 괜찮길래",
          "speaker_id": "1",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011760.1.16",
          "form": "얼마에 구매하셨어요?",
          "original_form": "얼마에 구매하셨어요?",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011760.1.17",
          "form": "30만원대인데 적립금을 반틈주더라고요",
          "original_form": "30만원대인데 적립금을 반틈주더라고요",
          "speaker_id": "1",
          "time": "20210824 15:56"
