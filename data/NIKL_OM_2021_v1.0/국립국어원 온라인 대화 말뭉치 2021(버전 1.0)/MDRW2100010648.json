      "utterance": [
        {
          "id": "MDRW2100010648.1.1",
          "form": "쇼핑 자주 하시나요?",
          "original_form": "쇼핑 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010648.1.2",
          "form": "애기용품으로 요",
          "original_form": "애기용품으로 요",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010648.1.3",
          "form": "어디서 주로 쇼핑하세요?",
          "original_form": "어디서 주로 쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010648.1.4",
          "form": "네이버라방이요",
          "original_form": "네이버라방이요",
          "speaker_id": "2",
          "time": "20210823 21:02"
        },
        {
          "id": "MDRW2100010648.1.5",
          "form": "당신은 자주하시나요?",
          "original_form": "당신은 자주하시나요?",
          "speaker_id": "2",
          "time": "20210823 21:02"
        },
        {
          "id": "MDRW2100010648.1.6",
          "form": "저도 주로 온라인쇼핑을 자주해요 최근에 산건 뭔가요?",
          "original_form": "저도 주로 온라인쇼핑을 자주해요 최근에 산건 뭔가요?",
          "speaker_id": "1",
          "time": "20210823 21:08"
        },
        {
          "id": "MDRW2100010648.1.7",
          "form": "물티슈요",
          "original_form": "물티슈요",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100010648.1.8",
          "form": "매트도 사고",
          "original_form": "매트도 사고",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100010648.1.9",
          "form": "끝이없네요",
          "original_form": "끝이없네요",
          "speaker_id": "2",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010648.1.10",
          "form": "무슨 매트인가요?",
          "original_form": "무슨 매트인가요?",
          "speaker_id": "1",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010648.1.11",
          "form": "놀이방매트요",
          "original_form": "놀이방매트요",
          "speaker_id": "2",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010648.1.12",
          "form": "아기 키우시나 보네요 아기용품도 라방으로 많이 사시나요?",
          "original_form": "아기 키우시나 보네요 아기용품도 라방으로 많이 사시나요?",
          "speaker_id": "1",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010648.1.13",
          "form": "가격이 확실히싸더라고요",
          "original_form": "가격이 확실히싸더라고요",
          "speaker_id": "2",
          "time": "20210823 21:21"
        },
        {
          "id": "MDRW2100010648.1.14",
          "form": "뭐 사고싶은신거 있나요",
          "original_form": "뭐 사고싶은신거 있나요",
          "speaker_id": "1",
          "time": "20210823 21:21"
        },
        {
          "id": "MDRW2100010648.1.15",
          "form": "혜택도 크고",
          "original_form": "혜택도 크고",
          "speaker_id": "2",
          "time": "20210823 21:21"
        },
        {
          "id": "MDRW2100010648.1.16",
          "form": "머리 염색이나 하고싶어요",
          "original_form": "머리 염색이나 하고싶어요",
          "speaker_id": "2",
          "time": "20210823 21:21"
        },
        {
          "id": "MDRW2100010648.1.17",
          "form": "염색약도 직접 사시나요?",
          "original_form": "염색약도 직접 사시나요?",
