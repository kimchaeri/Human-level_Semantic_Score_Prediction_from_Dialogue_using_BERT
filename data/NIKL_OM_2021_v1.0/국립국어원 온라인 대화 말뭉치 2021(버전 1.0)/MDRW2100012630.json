      "utterance": [
        {
          "id": "MDRW2100012630.1.1",
          "form": "그 배민에는 메뉴가 뭐뭐있어요?",
          "original_form": "그 배민에는 메뉴가 뭐뭐있어요?",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012630.1.2",
          "form": "오므라이스?",
          "original_form": "오므라이스?",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012630.1.3",
          "form": "인도커리와 소고기미역국을 같이 파는 가게요",
          "original_form": "인도커리와 소고기미역국을 같이 파는 가게요",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012630.1.4",
          "form": "인도전문점은 대부분 커리네요 아무래도",
          "original_form": "인도전문점은 대부분 커리네요 아무래도",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012630.1.5",
          "form": "인도오므라이스 파는곳은 이탈리아오므라이스도 함께 팝니다ㅎ",
          "original_form": "인도오므라이스 파는곳은 이탈리아오므라이스도 함께 팝니다ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012630.1.6",
          "form": "으..잉? ㅋㅋㅋㅋㅋ 소고기미역국과 이탈리아오므라이스의 콜라보 ㅋㅋㅋㅋ",
          "original_form": "으..잉? ㅋㅋㅋㅋㅋ 소고기미역국과 이탈리아오므라이스의 콜라보 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100012630.1.7",
          "form": "커리 맛있죠~~ 먹고싶네요ㅜㅜㅜ",
          "original_form": "커리 맛있죠~~ 먹고싶네요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100012630.1.8",
          "form": "난 이랑 같이ㅠ",
          "original_form": "난 이랑 같이ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100012630.1.9",
          "form": "플레이난에 커리에 라씨",
          "original_form": "플레이난에 커리에 라씨",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012630.1.10",
          "form": "환상의 궁합이죠",
          "original_form": "환상의 궁합이죠",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012630.1.11",
          "form": "ㅜㅜㅜㅜ짜이도 먹고싶어요... 짜이..",
          "original_form": "ㅜㅜㅜㅜ짜이도 먹고싶어요... 짜이..",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012630.1.12",
          "form": "버터갈릭난도 드셔쥬세여...",
          "original_form": "버터갈릭난도 드셔쥬세여...",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012630.1.13",
          "form": "방금 나온 뜨끈한 난 진짜 맛있겠다ㅠㅠ",
          "original_form": "방금 나온 뜨끈한 난 진짜 맛있겠다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012630.1.14",
          "form": "ㅎㅎ전에 플레인난 사다두고 프라이팬에 살짝 구워먹었는데",
          "original_form": "ㅎㅎ전에 플레인난 사다두고 프라이팬에 살짝 구워먹었는데",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012630.1.15",
          "form": "괜찮더라고요",
          "original_form": "괜찮더라고요",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012630.1.16",
          "form": "집에 커리만 구비해두면 되니",
          "original_form": "집에 커리만 구비해두면 되니",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012630.1.17",
          "form": "오~ 그렇게도 먹을수있군여!!",
          "original_form": "오~ 그렇게도 먹을수있군여!!",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012630.1.18",
          "form": "버터갈릭난도 잘 나온 제품이 있겠죠?",
          "original_form": "버터갈릭난도 잘 나온 제품이 있겠죠?",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012630.1.19",
          "form": "네네 거진 십오년전의 일이니 지금은 그보다도 더 잘 나와있을 거예요",
          "original_form": "네네 거진 십오년전의 일이니 지금은 그보다도 더 잘 나와있을 거예요",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012630.1.20",
          "form": "그...렇겠죠? ㅋㅋㅋㅋ",
          "original_form": "그...렇겠죠? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012630.1.21",
          "form": "name2이 드신 현지의 맛은 아닐지라도ㅎ",
          "original_form": "&name2&이 드신 현지의 맛은 아닐지라도ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012630.1.22",
          "form": "하.. 진짜 맛있었어요..",
          "original_form": "하.. 진짜 맛있었어요..",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012630.1.23",
          "form": "근데 맛은 한국도 비슷해요!!",
          "original_form": "근데 맛은 한국도 비슷해요!!",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012630.1.24",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012630.1.25",
          "form": "단지 현지는 좀 더 딥한 느낌이 있어요 ㅋㅋㅋㅋ",
          "original_form": "단지 현지는 좀 더 딥한 느낌이 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012630.1.26",
          "form": "미세먼지랑 같이 먹어서 그릉가...",
          "original_form": "미세먼지랑 같이 먹어서 그릉가...",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012630.1.27",
          "form": "향신료가 많이 들어가서",
          "original_form": "향신료가 많이 들어가서",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012630.1.28",
          "form": "가 아니라 미세먼지요?ㅋㅋㅋ",
          "original_form": "가 아니라 미세먼지요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012630.1.29",
          "form": "그래도 딥한맛이 궁금하네요ㅎㅎ",
          "original_form": "그래도 딥한맛이 궁금하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012630.1.30",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100012630.1.31",
          "form": "진짜 그.. 티오피의 느낌이 있어요. 그냥 커리가 아니야... 이건 티오피야..하는 깊이가 ㅋㅋㅋ",
          "original_form": "진짜 그.. 티오피의 느낌이 있어요. 그냥 커리가 아니야... 이건 티오피야..하는 깊이가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012630.1.32",
          "form": "아니 일반과 티오피 차이면 안 비슷하잖아욧!",
          "original_form": "아니 일반과 티오피 차이면 안 비슷하잖아욧!",
          "speaker_id": "1",
