      "utterance": [
        {
          "id": "MDRW2100006538.1.1",
          "form": "왜 ㅋㅋㅋㅋ자랑 엄청 했었잖아",
          "original_form": "왜 ㅋㅋㅋㅋ자랑 엄청 했었잖아",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100006538.1.2",
          "form": "핑크머리",
          "original_form": "핑크머리",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100006538.1.3",
          "form": "나 핑크 머리 했었잖아 그래",
          "original_form": "나 핑크 머리 했었잖아 그래",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100006538.1.4",
          "form": "근데 그거 너 봤거든? 진짜 어이없다",
          "original_form": "근데 그거 너 봤거든? 진짜 어이없다",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100006538.1.5",
          "form": "싸우자는거냐",
          "original_form": "싸우자는거냐",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100006538.1.6",
          "form": "거의 물 빠지고나서 봤어",
          "original_form": "거의 물 빠지고나서 봤어",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100006538.1.7",
          "form": "눈 앞에서 본거!",
          "original_form": "눈 앞에서 본거!",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100006538.1.8",
          "form": "아 뭐야 그거면 본거잖아",
          "original_form": "아 뭐야 그거면 본거잖아",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100006538.1.9",
          "form": "ㅎㅎㅎ?",
          "original_form": "ㅎㅎㅎ?",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100006538.1.10",
          "form": "알겠어 ㅋㅋㅋ그냥 완전 핑크 보고싶었었다고",
          "original_form": "알겠어 ㅋㅋㅋ그냥 완전 핑크 보고싶었었다고",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100006538.1.11",
          "form": "너가 해 핑크",
          "original_form": "너가 해 핑크",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100006538.1.12",
          "form": "미용사님한테 가서",
          "original_form": "미용사님한테 가서",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100006538.1.13",
          "form": "내가 핑크 염색한 미용실 알려줄깧?",
          "original_form": "내가 핑크 염색한 미용실 알려줄깧?",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100006538.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100006538.1.15",
          "form": "끌리지만 부끄럽다",
          "original_form": "끌리지만 부끄럽다",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100006538.1.16",
          "form": "난 소심한 관종이라",
          "original_form": "난 소심한 관종이라",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100006538.1.17",
          "form": "ㅋㅋㅋㅋㅋ얼마였더라 한 이십인가",
          "original_form": "ㅋㅋㅋㅋㅋ얼마였더라 한 이십인가",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100006538.1.18",
          "form": "십은 넘었던거 같은데",
          "original_form": "십은 넘었던거 같은데",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100006538.1.19",
          "form": "그래도 거기가 좀 비싼 편은 아니였어 ㅋㅋㅋ",
          "original_form": "그래도 거기가 좀 비싼 편은 아니였어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100006538.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ잘했어 좋은 추억이지 뭐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ잘했어 좋은 추억이지 뭐",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100006538.1.21",
          "form": "멋있었어 그때 한다고 해서",
          "original_form": "멋있었어 그때 한다고 해서",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100006538.1.22",
          "form": "인생에 한번쯤 해볼만해ㅎ",
          "original_form": "인생에 한번쯤 해볼만해ㅎ",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100006538.1.23",
          "form": "맞아맞아 인정해",
          "original_form": "맞아맞아 인정해",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100006538.1.24",
          "form": "ㅋㅋㅋㅋㅋ펌 같은건 관심없어?",
          "original_form": "ㅋㅋㅋㅋㅋ펌 같은건 관심없어?",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100006538.1.25",
          "form": "강추할게^^",
          "original_form": "강추할게^^",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100006538.1.26",
          "form": "음 좀 !",
          "original_form": "음 좀 !",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100006538.1.27",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100006538.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100006538.1.29",
          "form": "관심없어",
          "original_form": "관심없어",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100006538.1.30",
          "form": "그렇군 의외네",
          "original_form": "그렇군 의외네",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100006538.1.31",
          "form": "머리 펌은 너가더 관심있어보여ㅋㅋㅋ",
