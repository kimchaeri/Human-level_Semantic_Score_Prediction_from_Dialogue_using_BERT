      "utterance": [
        {
          "id": "MDRW2100023599.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100023599.1.2",
          "form": "넵 안녕하세용!",
          "original_form": "넵 안녕하세용!",
          "speaker_id": "2",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100023599.1.3",
          "form": "직장에출근하셨나요?",
          "original_form": "직장에출근하셨나요?",
          "speaker_id": "1",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100023599.1.4",
          "form": "저는 점심시간이라쉬고있는데 오늘 출근해서스트레스를받았나 두통약먹고쉬고있어요",
          "original_form": "저는 점심시간이라쉬고있는데 오늘 출근해서스트레스를받았나 두통약먹고쉬고있어요",
          "speaker_id": "1",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100023599.1.5",
          "form": "아이고ㅜㅠ",
          "original_form": "아이고ㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023599.1.6",
          "form": "안좋은일이 있으셨나요?ㅠㅠㅠ",
          "original_form": "안좋은일이 있으셨나요?ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023599.1.7",
          "form": "아님 너무 바빠서....?",
          "original_form": "아님 너무 바빠서....?",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023599.1.8",
          "form": "출근자체가 스트레스인가봐요",
          "original_form": "출근자체가 스트레스인가봐요",
          "speaker_id": "1",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023599.1.9",
          "form": "취업하셨나요?",
          "original_form": "취업하셨나요?",
          "speaker_id": "1",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023599.1.10",
          "form": "했다가 퇴사했어요ㅎㅎ",
          "original_form": "했다가 퇴사했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023599.1.11",
          "form": "저도 스트레스가 심해서ㅠ",
          "original_form": "저도 스트레스가 심해서ㅠ",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023599.1.12",
          "form": "아 어떤직장에다니셨나요",
          "original_form": "아 어떤직장에다니셨나요",
          "speaker_id": "1",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023599.1.13",
          "form": "수출회사였어요ㅎㅎ",
          "original_form": "수출회사였어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023599.1.14",
          "form": "그래서 밤낮이 없더라구요",
          "original_form": "그래서 밤낮이 없더라구요",
          "speaker_id": "2",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023599.1.15",
          "form": "업무하는데힘드셨겠어요",
          "original_form": "업무하는데힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023599.1.16",
          "form": "네 그래서 퇴사했지요ㅋㅋㅋ",
          "original_form": "네 그래서 퇴사했지요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023599.1.17",
          "form": "급여는그래도많았죵?",
          "original_form": "급여는그래도많았죵?",
          "speaker_id": "1",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023599.1.18",
          "form": "저는쥐꼬리급여ㅠㅠ",
          "original_form": "저는쥐꼬리급여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023599.1.19",
          "form": "부족하진 않았던것같아요~",
          "original_form": "부족하진 않았던것같아요~",
          "speaker_id": "2",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023599.1.20",
          "form": "저는 그나마 워라벨",
          "original_form": "저는 그나마 워라벨",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023599.1.21",
          "form": "근데 겪어보니 돈 주는만큼 더 굴리더라구요",
          "original_form": "근데 겪어보니 돈 주는만큼 더 굴리더라구요",
          "speaker_id": "2",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023599.1.22",
          "form": "그대신 급여가적어요ㅠㅠ",
          "original_form": "그대신 급여가적어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023599.1.23",
          "form": "한만큼 받는거같긴해요",
          "original_form": "한만큼 받는거같긴해요",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023599.1.24",
          "form": "ㅎㅎ 그래도 일상이 있는 삶을 사실 수 있으니까요!",
          "original_form": "ㅎㅎ 그래도 일상이 있는 삶을 사실 수 있으니까요!",
          "speaker_id": "2",
          "time": "20210830 12:35"
        }
      ]
    }
  ]
