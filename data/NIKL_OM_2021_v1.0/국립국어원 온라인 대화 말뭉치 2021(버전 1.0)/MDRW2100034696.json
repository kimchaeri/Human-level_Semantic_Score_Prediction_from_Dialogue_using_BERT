      "utterance": [
        {
          "id": "MDRW2100034696.1.1",
          "form": "직장에 옮기면 어카노 만나기 불편하제",
          "original_form": "직장에 옮기면 어카노 만나기 불편하제",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100034696.1.2",
          "form": "사귀고는 자주 만나고 통화도 자주하고",
          "original_form": "사귀고는 자주 만나고 통화도 자주하고",
          "speaker_id": "1",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100034696.1.3",
          "form": "아이고 ㅋㅋ카톡이 아주그냥 끈임없이 진동이 ㅋㅋ",
          "original_form": "아이고 ㅋㅋ카톡이 아주그냥 끈임없이 진동이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100034696.1.4",
          "form": "ㅋㅋㅋㄲ",
          "original_form": "ㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100034696.1.5",
          "form": "아 좋을때다",
          "original_form": "아 좋을때다",
          "speaker_id": "2",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100034696.1.6",
          "form": "한전이면 공기업아님? 준공무원인데 직장옮기나여? 나 사실한전에 대해 아는게 그다지없어서 ㅋㅋ",
          "original_form": "한전이면 공기업아님? 준공무원인데 직장옮기나여? 나 사실한전에 대해 아는게 그다지없어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100034696.1.7",
          "form": "공기업으로알고있음",
          "original_form": "공기업으로알고있음",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034696.1.8",
          "form": "탄탄하네",
          "original_form": "탄탄하네",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034696.1.9",
          "form": "내동생은 음 이혼가정아니라서 좋은것같다고",
          "original_form": "내동생은 음 이혼가정아니라서 좋은것같다고",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034696.1.10",
          "form": "나두 잘몰랑ㅋㅋ 응원하고싶다 동생분ㅋㅋ",
          "original_form": "나두 잘몰랑ㅋㅋ 응원하고싶다 동생분ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034696.1.11",
          "form": "에 그런걸보는구나",
          "original_form": "에 그런걸보는구나",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034696.1.12",
          "form": "사실 이런건 편견이지만ㅜ",
          "original_form": "사실 이런건 편견이지만ㅜ",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034696.1.13",
          "form": "부모님의 이혼은 자식들과 관계없지만 흠",
          "original_form": "부모님의 이혼은 자식들과 관계없지만 흠",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034696.1.14",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034696.1.15",
          "form": "문제가잇남",
          "original_form": "문제가잇남",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034696.1.16",
          "form": "사실 내동생이 불안해한건",
          "original_form": "사실 내동생이 불안해한건",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100034696.1.17",
          "form": "부모님들 중 아버지 쪽이 바람펴서 이혼했다는 사실이었음",
          "original_form": "부모님들 중 아버지 쪽이 바람펴서 이혼했다는 사실이었음",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100034696.1.18",
          "form": "그분 부모님이?",
          "original_form": "그분 부모님이?",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100034696.1.19",
          "form": "근데 아빠일이고 본인은 아니지않아?",
          "original_form": "근데 아빠일이고 본인은 아니지않아?",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100034696.1.20",
          "form": "ㅇㅇ지금 일주일사귄  한전 말고",
          "original_form": "ㅇㅇ지금 일주일사귄  한전 말고",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034696.1.21",
          "form": "그전남친들이 ㅋㅋ",
          "original_form": "그전남친들이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034696.1.22",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034696.1.23",
          "form": "그냥 막연하게 아들은 아버지 닮는다는데 싶은?",
          "original_form": "그냥 막연하게 아들은 아버지 닮는다는데 싶은?",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034696.1.24",
          "form": "전남친이구나",
          "original_form": "전남친이구나",
          "speaker_id": "2",
