      "utterance": [
        {
          "id": "MDRW2100040278.1.1",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040278.1.2",
          "form": "아점으로 집밥 먹었어요",
          "original_form": "아점으로 집밥 먹었어요",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040278.1.3",
          "form": "반찬은요?",
          "original_form": "반찬은요?",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040278.1.4",
          "form": "어제 만든 오리주물럭에 비벼먹었어요! 꿀맛!",
          "original_form": "어제 만든 오리주물럭에 비벼먹었어요! 꿀맛!",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040278.1.5",
          "form": "오리주물럭을 만드셨어요????",
          "original_form": "오리주물럭을 만드셨어요????",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040278.1.6",
          "form": "오리 요즘 손질 다 해주더라고요! 삼겹살처럼요",
          "original_form": "오리 요즘 손질 다 해주더라고요! 삼겹살처럼요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040278.1.7",
          "form": "그래도 양념을 해야하잖아요",
          "original_form": "그래도 양념을 해야하잖아요",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040278.1.8",
          "form": "어젠 닭갈비양념에 고추가루랑 매실액 간장 마늘 추가했어요ㅋㅋ 치트키쓴거죠ㅋㅋ",
          "original_form": "어젠 닭갈비양념에 고추가루랑 매실액 간장 마늘 추가했어요ㅋㅋ 치트키쓴거죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100040278.1.9",
          "form": "닭갈비 양념을 사면 되는거군요. 저도 그정도는 할 수 있을것 같아요",
          "original_form": "닭갈비 양념을 사면 되는거군요. 저도 그정도는 할 수 있을것 같아요",
          "speaker_id": "1",
          "time": "20210904 18:36"
        },
        {
          "id": "MDRW2100040278.1.10",
          "form": "ㅋㅋ 네 역시 시판소스넣으니 보장된 맛이ㅋㅋ",
          "original_form": "ㅋㅋ 네 역시 시판소스넣으니 보장된 맛이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:33"
        },
        {
          "id": "MDRW2100040278.1.11",
          "form": "시판소스 베이스에 살짝 양념은 부담스럽지 않은데 그게 하면서 간을 못보잖아요",
          "original_form": "시판소스 베이스에 살짝 양념은 부담스럽지 않은데 그게 하면서 간을 못보잖아요",
          "speaker_id": "1",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100040278.1.12",
          "form": "사실 그래요ㅋㅋ 어제도 그렇게 매울지 몰랏어요ㅋㅋ",
          "original_form": "사실 그래요ㅋㅋ 어제도 그렇게 매울지 몰랏어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:07"
        },
        {
          "id": "MDRW2100040278.1.13",
          "form": "다 완성하고보니 너무 매웠어요",
          "original_form": "다 완성하고보니 너무 매웠어요",
          "speaker_id": "2",
          "time": "20210904 20:07"
        },
        {
          "id": "MDRW2100040278.1.14",
          "form": "고추가루를 너무많이 넣으셨나봐요",
          "original_form": "고추가루를 너무많이 넣으셨나봐요",
          "speaker_id": "1",
          "time": "20210904 21:41"
        },
        {
          "id": "MDRW2100040278.1.15",
          "form": "닭갈비소스가 붉달갈비라...",
          "original_form": "닭갈비소스가 붉달갈비라...",
          "speaker_id": "2",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100040278.1.16",
          "form": "앗!! 소스 선택이 잘못이었군요",
          "original_form": "앗!! 소스 선택이 잘못이었군요",
          "speaker_id": "1",
          "time": "20210904 22:11"
        }
      ]
