      "utterance": [
        {
          "id": "MDRW2100026847.1.1",
          "form": "제가 해봣는데",
          "original_form": "제가 해봣는데",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026847.1.2",
          "form": "왜이렇게 짠건지..ㅠ",
          "original_form": "왜이렇게 짠건지..ㅠ",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.3",
          "form": "아니ㅋㅋ 왜죠ㅋㅋ",
          "original_form": "아니ㅋㅋ 왜죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.4",
          "form": "500그람 사서 (해감된것) 해밧는데",
          "original_form": "500그람 사서 (해감된것) 해밧는데",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.5",
          "form": "뭘 더 넣으셨나요?",
          "original_form": "뭘 더 넣으셨나요?",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.6",
          "form": "물에 한시간 담궛는데도",
          "original_form": "물에 한시간 담궛는데도",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.7",
          "form": "이것들이",
          "original_form": "이것들이",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.8",
          "form": "입을 안벌리더라구요",
          "original_form": "입을 안벌리더라구요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.9",
          "form": "소금을 물고있나",
          "original_form": "소금을 물고있나",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.10",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.11",
          "form": "아무것도 안넣어도",
          "original_form": "아무것도 안넣어도",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026847.1.12",
          "form": "짜서",
          "original_form": "짜서",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026847.1.13",
          "form": "악마가 욕하더라구요",
          "original_form": "악마가 욕하더라구요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026847.1.14",
          "form": "그정도면 바지락과 전생에 원수 아닌가요ㅋㅋ",
          "original_form": "그정도면 바지락과 전생에 원수 아닌가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026847.1.15",
          "form": "찰지게 맞았슴니다",
          "original_form": "찰지게 맞았슴니다",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026847.1.16",
          "form": "갑자기 악마ㅋㅋㅋ",
          "original_form": "갑자기 악마ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026847.1.17",
          "form": "네 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026847.1.18",
          "form": "바지락은 저랑 안맞아요",
          "original_form": "바지락은 저랑 안맞아요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026847.1.19",
          "form": "그래도 애기랑 나중에 캐러 가볼려고요",
          "original_form": "그래도 애기랑 나중에 캐러 가볼려고요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026847.1.20",
          "form": "좋은 경험일것같습니다",
          "original_form": "좋은 경험일것같습니다",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026847.1.21",
          "form": "가슴아픈 일이군요ㅎㅎ",
          "original_form": "가슴아픈 일이군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026847.1.22",
          "form": "애기는 몇살이에욪?",
          "original_form": "애기는 몇살이에욪?",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026847.1.23",
          "form": "2살요 ㅋㅋ",
          "original_form": "2살요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026847.1.24",
          "form": "애기들이 그런거 되게 좋아한대요",
          "original_form": "애기들이 그런거 되게 좋아한대요",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026847.1.25",
          "form": "15개월입니당",
          "original_form": "15개월입니당",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026847.1.26",
          "form": "귀여울 때군요ㅎ",
          "original_form": "귀여울 때군요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026847.1.27",
          "form": "매일 같이 목욕하는데",
          "original_form": "매일 같이 목욕하는데",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026847.1.28",
          "form": "나쁜손 시전합니다",
          "original_form": "나쁜손 시전합니다",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026847.1.29",
          "form": "치욕스러워요",
          "original_form": "치욕스러워요",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026847.1.30",
          "form": "아 식음료",
          "original_form": "아 식음료",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026847.1.31",
          "form": "아닠 왜요ㅋㅋ",
          "original_form": "아닠 왜요ㅋㅋ",
