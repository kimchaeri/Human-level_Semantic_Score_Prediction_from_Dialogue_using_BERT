      "utterance": [
        {
          "id": "MDRW2100022060.1.1",
          "form": "좋아하는 연예인 잇으세요?",
          "original_form": "좋아하는 연예인 잇으세요?",
          "speaker_id": "1",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022060.1.2",
          "form": "네 있죠ㅎㅎ",
          "original_form": "네 있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:48"
        },
        {
          "id": "MDRW2100022060.1.3",
          "form": "누구인가욤",
          "original_form": "누구인가욤",
          "speaker_id": "1",
          "time": "20210829 01:48"
        },
        {
          "id": "MDRW2100022060.1.4",
          "form": "배우는 하석진 송중기 좋아하고 가수는 수호 좋아해요",
          "original_form": "배우는 하석진 송중기 좋아하고 가수는 수호 좋아해요",
          "speaker_id": "2",
          "time": "20210829 01:52"
        },
        {
          "id": "MDRW2100022060.1.5",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210829 01:52"
        },
        {
          "id": "MDRW2100022060.1.6",
          "form": "전 래퍼 래원 좋아해요",
          "original_form": "전 래퍼 래원 좋아해요",
          "speaker_id": "1",
          "time": "20210829 01:52"
        },
        {
          "id": "MDRW2100022060.1.7",
          "form": "수호가 그 엑소 인가요?",
          "original_form": "수호가 그 엑소 인가요?",
          "speaker_id": "1",
          "time": "20210829 01:52"
        },
        {
          "id": "MDRW2100022060.1.8",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210829 01:58"
        },
        {
          "id": "MDRW2100022060.1.9",
          "form": "하석진씨가 한 드라마나 영화 머잇어요?",
          "original_form": "하석진씨가 한 드라마나 영화 머잇어요?",
          "speaker_id": "1",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022060.1.10",
          "form": "1%의 어떤것/자체발광오피스/디데이/내가가장예뻣을때 이런 작품들이요",
          "original_form": "1%의 어떤것/자체발광오피스/디데이/내가가장예뻣을때 이런 작품들이요",
          "speaker_id": "2",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022060.1.11",
          "form": "오 첨들어보는데 나중에 봐볼게여",
          "original_form": "오 첨들어보는데 나중에 봐볼게여",
          "speaker_id": "1",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022060.1.12",
          "form": "네ㅎㅎ 평소에 어떤 프로그램 보네요?",
          "original_form": "네ㅎㅎ 평소에 어떤 프로그램 보네요?",
          "speaker_id": "2",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022060.1.13",
          "form": "보네요가 아니라 보시나요?",
          "original_form": "보네요가 아니라 보시나요?",
          "speaker_id": "2",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022060.1.14",
          "form": "런닝맨 봐요",
          "original_form": "런닝맨 봐요",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022060.1.15",
          "form": "티비는 자주 안봐서 잘몰라요 ㅌㅋㅋ",
          "original_form": "티비는 자주 안봐서 잘몰라요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022060.1.16",
          "form": "오 저도 런닝맨봐요ㅋㅋㅋ",
          "original_form": "오 저도 런닝맨봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022060.1.17",
          "form": "오오 요즘 티비 볼게없어요",
          "original_form": "오오 요즘 티비 볼게없어요",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100022060.1.18",
