      "utterance": [
        {
          "id": "MDRW2100035281.1.1",
          "form": "안녕하세요? 대화시간 괜찮으신가요?",
          "original_form": "안녕하세요? 대화시간 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100035281.1.2",
          "form": "네 괜찮아요!",
          "original_form": "네 괜찮아요!",
          "speaker_id": "2",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100035281.1.3",
          "form": "네 그럼 음식얘기해요 ㅋㅋ점심은 드셨나요?",
          "original_form": "네 그럼 음식얘기해요 ㅋㅋ점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100035281.1.4",
          "form": "전 김밥을 먹었어요",
          "original_form": "전 김밥을 먹었어요",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100035281.1.5",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.6",
          "form": "저 이제 먹으랴구요",
          "original_form": "저 이제 먹으랴구요",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.7",
          "form": "김밥 무슨 김밥 드셨나요?",
          "original_form": "김밥 무슨 김밥 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.8",
          "form": "그럼 먹구 얘기할까요?",
          "original_form": "그럼 먹구 얘기할까요?",
          "speaker_id": "1",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.9",
          "form": "아 먹으면서 해도돼요",
          "original_form": "아 먹으면서 해도돼요",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.10",
          "form": "전 집에서 만든걸루 먹었어요",
          "original_form": "전 집에서 만든걸루 먹었어요",
          "speaker_id": "1",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.12",
          "form": "요리잘하시나봐요!",
          "original_form": "요리잘하시나봐요!",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.13",
          "form": "전철에서 한입에 먹게 작게 싼 김말이 있잖아요",
          "original_form": "전철에서 한입에 먹게 작게 싼 김말이 있잖아요",
          "speaker_id": "1",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.14",
          "form": "김밥 만들기 힘들다 하던데",
          "original_form": "김밥 만들기 힘들다 하던데",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035281.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.17",
          "form": "꼬마김밥인가",
          "original_form": "꼬마김밥인가",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.18",
          "form": "ㅋㅋㅋㅋ이건 쉬워요 그래도 김밥이라구요",
          "original_form": "ㅋㅋㅋㅋ이건 쉬워요 그래도 김밥이라구요",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.19",
          "form": "그가 말하시는거군요",
          "original_form": "그가 말하시는거군요",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.20",
          "form": "네 꼬마김밥으로 단무지 맛살  단무지 햄",
          "original_form": "네 꼬마김밥으로 단무지 맛살  단무지 햄",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.21",
          "form": "그쳐 그 김밥 유명한곳 있던데",
          "original_form": "그쳐 그 김밥 유명한곳 있던데",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.22",
          "form": "이렇게 말아서 한접시에 쌓아서 냠냠",
          "original_form": "이렇게 말아서 한접시에 쌓아서 냠냠",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.23",
          "form": "떡볶이랑 먹으면 엄청 맛있을것같아요",
          "original_form": "떡볶이랑 먹으면 엄청 맛있을것같아요",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.24",
          "form": "찰떡궁합",
          "original_form": "찰떡궁합",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.25",
          "form": "혹시 떡볶이 좋아하시나요?",
          "original_form": "혹시 떡볶이 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.26",
          "form": "떡볶이랑 먹어도 맛있는데 오늘은 집에서 만든거니깐 간단히 먹었어요",
          "original_form": "떡볶이랑 먹어도 맛있는데 오늘은 집에서 만든거니깐 간단히 먹었어요",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100035281.1.27",
          "form": "좋아하는데 매운 요즘유행은 못따라가요",
          "original_form": "좋아하는데 매운 요즘유행은 못따라가요",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100035281.1.28",
          "form": "아 저도 너무 매운건 못먹어요",
          "original_form": "아 저도 너무 매운건 못먹어요",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100035281.1.29",
          "form": "자극적이고 배가 아프더라구요",
          "original_form": "자극적이고 배가 아프더라구요",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100035281.1.30",
          "form": "혹시 쌀떡파세요 밀떡파세요?",
          "original_form": "혹시 쌀떡파세요 밀떡파세요?",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100035281.1.31",
          "form": "밀떡요!",
          "original_form": "밀떡요!",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100035281.1.32",
          "form": "밀떡 달달 국물파!",
          "original_form": "밀떡 달달 국물파!",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100035281.1.33",
          "form": "튀김은 오징어 김말이요",
          "original_form": "튀김은 오징어 김말이요",
          "speaker_id": "1",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100035281.1.34",
          "form": "달달 국물 튀김 잘어울리죠",
          "original_form": "달달 국물 튀김 잘어울리죠",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100035281.1.35",
          "form": "전 쌀떡파인데",
          "original_form": "전 쌀떡파인데",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100035281.1.36",
          "form": "쌀떡볶이가 점점 안보여서 슬퍼요",
          "original_form": "쌀떡볶이가 점점 안보여서 슬퍼요",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100035281.1.37",
          "form": "아 쌀이면 그럴거에요.. 쌀 밀 고를 수 있는 가게면 몰라도..",
          "original_form": "아 쌀이면 그럴거에요.. 쌀 밀 고를 수 있는 가게면 몰라도..",
          "speaker_id": "1",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100035281.1.38",
          "form": "그래도 쌀떡볶이도 많지 않아요? 죠스떡볶이는 쌀떡이었던 걱 같은데",
          "original_form": "그래도 쌀떡볶이도 많지 않아요? 죠스떡볶이는 쌀떡이었던 걱 같은데",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100035281.1.39",
          "form": "죠스는 쌀떡일거구",
          "original_form": "죠스는 쌀떡일거구",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100035281.1.40",
          "form": "국대가 밀떡인가 ..검색해봐야겠네요",
          "original_form": "국대가 밀떡인가 ..검색해봐야겠네요",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100035281.1.41",
          "form": "달떡볶이가 쌀떡인것 같더라구요",
          "original_form": "달떡볶이가 쌀떡인것 같더라구요",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100035281.1.42",
          "form": "근데 확실히 요즘 나오는것들은",
          "original_form": "근데 확실히 요즘 나오는것들은",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100035281.1.43",
          "form": "밀떡이 잘 어울리는 것 같아요",
          "original_form": "밀떡이 잘 어울리는 것 같아요",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100035281.1.44",
          "form": "로제떡볶이 드셔보셨나요?",
          "original_form": "로제떡볶이 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100035281.1.45",
          "form": "진짜 맛있더라구요",
          "original_form": "진짜 맛있더라구요",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100035281.1.46",
          "form": "어떻게 이런 생각을 했는지",
          "original_form": "어떻게 이런 생각을 했는지",
          "speaker_id": "2",
          "time": "20210829 13:48"
        }
      ]
