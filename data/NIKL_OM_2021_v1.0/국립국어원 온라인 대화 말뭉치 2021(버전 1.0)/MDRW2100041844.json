      "utterance": [
        {
          "id": "MDRW2100041844.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041844.1.2",
          "form": "안녕하세요!! 혹시 직장인이신가요?",
          "original_form": "안녕하세요!! 혹시 직장인이신가요?",
          "speaker_id": "2",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041844.1.3",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041844.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041844.1.5",
          "form": "그렇구나 전 학생이에요..",
          "original_form": "그렇구나 전 학생이에요..",
          "speaker_id": "2",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041844.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041844.1.7",
          "form": "아르바이트 같은 거",
          "original_form": "아르바이트 같은 거",
          "speaker_id": "1",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041844.1.8",
          "form": "하고계시나요",
          "original_form": "하고계시나요",
          "speaker_id": "1",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041844.1.9",
          "form": "아뇨... 아직 해본 적 없어요! 고등학생이라",
          "original_form": "아뇨... 아직 해본 적 없어요! 고등학생이라",
          "speaker_id": "2",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041844.1.10",
          "form": "사실 미래에 무슨 직업 할지도 못 정한..",
          "original_form": "사실 미래에 무슨 직업 할지도 못 정한..",
          "speaker_id": "2",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041844.1.11",
          "form": "아 헉 되게 어리시군요",
          "original_form": "아 헉 되게 어리시군요",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041844.1.12",
          "form": "하고싶은 아르바이트 같은 건 있으세요?",
          "original_form": "하고싶은 아르바이트 같은 건 있으세요?",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041844.1.13",
          "form": "음.. 사실 종류는 상관없이 그냥 다양하게 해보고 싶어요!",
          "original_form": "음.. 사실 종류는 상관없이 그냥 다양하게 해보고 싶어요!",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041844.1.14",
          "form": "그래도 처음엔 제일 무난한 아르바이트로 시작할 거 같은 ㅎㅎ",
          "original_form": "그래도 처음엔 제일 무난한 아르바이트로 시작할 거 같은 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041844.1.15",
          "form": "무난한게 뭐 있죠?",
          "original_form": "무난한게 뭐 있죠?",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041844.1.16",
          "form": "편의점 같은거요?",
          "original_form": "편의점 같은거요?",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041844.1.17",
          "form": "네ㅋㄱㅋㅋ",
          "original_form": "네ㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041844.1.18",
          "form": "부모님 추천!",
          "original_form": "부모님 추천!",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041844.1.19",
          "form": "편의점 편해요",
          "original_form": "편의점 편해요",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041844.1.20",
          "form": "저 해봤거든요",
          "original_form": "저 해봤거든요",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041844.1.21",
          "form": "오 해보셨어요???? 오오",
          "original_form": "오 해보셨어요???? 오오",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041844.1.22",
          "form": "네ㅋㅋㅋㅋㅋㅋㅋ 추천해요",
          "original_form": "네ㅋㅋㅋㅋㅋㅋㅋ 추천해요",
