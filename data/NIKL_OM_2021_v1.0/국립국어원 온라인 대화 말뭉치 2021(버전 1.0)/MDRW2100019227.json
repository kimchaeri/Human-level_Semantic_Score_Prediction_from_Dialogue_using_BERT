      "utterance": [
        {
          "id": "MDRW2100019227.1.1",
          "form": "소설 좋아하세요?",
          "original_form": "소설 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019227.1.2",
          "form": "네 요즘은 잘 못읽어요",
          "original_form": "네 요즘은 잘 못읽어요",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019227.1.3",
          "form": "웹소설 잘 봐요",
          "original_form": "웹소설 잘 봐요",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019227.1.4",
          "form": "어디서 주로 읽으셨나요?",
          "original_form": "어디서 주로 읽으셨나요?",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019227.1.5",
          "form": "요샌 시리즈요. 리디에서도 읽구요",
          "original_form": "요샌 시리즈요. 리디에서도 읽구요",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019227.1.6",
          "form": "시리즈랑 리디는 뷰어가 좋죠ㅎㅎ",
          "original_form": "시리즈랑 리디는 뷰어가 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019227.1.7",
          "form": "맞아요ㅋㅋ 카카오거 추천을 받기는 하는데 뷰어가 안좋다고 해서 고민이에요",
          "original_form": "맞아요ㅋㅋ 카카오거 추천을 받기는 하는데 뷰어가 안좋다고 해서 고민이에요",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019227.1.8",
          "form": "카카오페이지 기존은 뷰어가 별론데 신규는 괜찮더라구요ㅋㅋ",
          "original_form": "카카오페이지 기존은 뷰어가 별론데 신규는 괜찮더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:20"
        },
        {
          "id": "MDRW2100019227.1.9",
          "form": "보시는 거 있나요?",
          "original_form": "보시는 거 있나요?",
          "speaker_id": "2",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100019227.1.10",
          "form": "카카페에서는 데못죽 보고 시리즈에서는 화산귀환, 전독시 등등 봐요",
          "original_form": "카카페에서는 데못죽 보고 시리즈에서는 화산귀환, 전독시 등등 봐요",
          "speaker_id": "1",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019227.1.11",
          "form": "저두 전독시 봤어요.",
          "original_form": "저두 전독시 봤어요.",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019227.1.12",
          "form": "요새 웹툰도 하잖아요",
          "original_form": "요새 웹툰도 하잖아요",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019227.1.13",
          "form": "소설 외전도 빨리 나왓으면해요ㅠ",
          "original_form": "소설 외전도 빨리 나왓으면해요ㅠ",
          "speaker_id": "1",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019227.1.14",
          "form": "그러게요ㅠㅠ 소식이 너무 없어요",
          "original_form": "그러게요ㅠㅠ 소식이 너무 없어요",
          "speaker_id": "2",
          "time": "20210827 18:05"
        },
        {
          "id": "MDRW2100019227.1.15",
          "form": "올해 안에는 나오겠지요ㅠㅠ",
          "original_form": "올해 안에는 나오겠지요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 18:05"
        },
        {
          "id": "MDRW2100019227.1.16",
          "form": "제발이요ㅠㅠ",
          "original_form": "제발이요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:07"
        }
      ]
