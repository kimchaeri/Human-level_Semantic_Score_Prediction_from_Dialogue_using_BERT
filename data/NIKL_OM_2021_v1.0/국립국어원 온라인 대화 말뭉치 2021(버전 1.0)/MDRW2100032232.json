      "utterance": [
        {
          "id": "MDRW2100032232.1.1",
          "form": "제가 너무 늦게 돌아왔네요~~ㅠㅠ",
          "original_form": "제가 너무 늦게 돌아왔네요~~ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 16:32"
        },
        {
          "id": "MDRW2100032232.1.2",
          "form": "결혼하기엔 역시 착한남자가 짱이에요!",
          "original_form": "결혼하기엔 역시 착한남자가 짱이에요!",
          "speaker_id": "2",
          "time": "20210906 16:32"
        },
        {
          "id": "MDRW2100032232.1.3",
          "form": "저희남편은 연애때도 엄청 착착했어요^^;",
          "original_form": "저희남편은 연애때도 엄청 착착했어요^^;",
          "speaker_id": "2",
          "time": "20210906 16:32"
        },
        {
          "id": "MDRW2100032232.1.4",
          "form": "아 결혼정말 잘하셨네요^^",
          "original_form": "아 결혼정말 잘하셨네요^^",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100032232.1.5",
          "form": "부럽습니다ㅋㅋ",
          "original_form": "부럽습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100032232.1.6",
          "form": "돈은좀없지만요....허허..",
          "original_form": "돈은좀없지만요....허허..",
          "speaker_id": "2",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100032232.1.7",
          "form": "돈이아주많을거아니면 착한사람이 최고더라구요 주변 애기엄마들 봐두요~ㅠㅠ",
          "original_form": "돈이아주많을거아니면 착한사람이 최고더라구요 주변 애기엄마들 봐두요~ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100032232.1.8",
          "form": "맞아요 애기한테잘하는아빠",
          "original_form": "맞아요 애기한테잘하는아빠",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100032232.1.9",
          "form": "만나기쉽지않드라구요^^",
          "original_form": "만나기쉽지않드라구요^^",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100032232.1.10",
          "form": "연애오래하셨어요^?",
          "original_form": "연애오래하셨어요^?",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100032232.1.11",
          "form": "미팅 소개팅ㅋㅋ",
          "original_form": "미팅 소개팅ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100032232.1.12",
          "form": "전 동아리에서 만났어요 ㅍㅎㅎㅎ",
          "original_form": "전 동아리에서 만났어요 ㅍㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100032232.1.13",
          "form": "다른과오빠",
          "original_form": "다른과오빠",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100032232.1.14",
          "form": "7년 만났어용ㅋ",
          "original_form": "7년 만났어용ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100032232.1.15",
          "form": "근데 7년동안 거의 안싸웠고",
          "original_form": "근데 7년동안 거의 안싸웠고",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100032232.1.16",
          "form": "결혼해서도 거의안싸워요",
          "original_form": "결혼해서도 거의안싸워요",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100032232.1.17",
          "form": "와~연애오래하셨군요",
          "original_form": "와~연애오래하셨군요",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100032232.1.18",
          "form": "이런저희도 시어머니엮이면 싸우네요 ㅍㅎㅎㅎ",
          "original_form": "이런저희도 시어머니엮이면 싸우네요 ㅍㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100032232.1.19",
          "form": "아 어쩔수없죠ㅋㅋㅋ",
          "original_form": "아 어쩔수없죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100032232.1.20",
          "form": "저희도신혼때 주요 싸움주제",
          "original_form": "저희도신혼때 주요 싸움주제",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100032232.1.21",
          "form": "오래살다보니 그마저도 그냥 참고살아요^^",
          "original_form": "오래살다보니 그마저도 그냥 참고살아요^^",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100032232.1.22",
          "form": "정말 어쩔수없는 부분이겠죠?",
          "original_form": "정말 어쩔수없는 부분이겠죠?",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100032232.1.23",
          "form": "어떤누굴 만나도?ㅜㅜ",
          "original_form": "어떤누굴 만나도?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100032232.1.24",
          "form": "정말 결혼은 현실이에요ㅠㅠㅋ",
          "original_form": "정말 결혼은 현실이에요ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100032232.1.25",
          "form": "맞아요ㅠ살아온환경이다르니어쩔수없는거같아요",
          "original_form": "맞아요ㅠ살아온환경이다르니어쩔수없는거같아요",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100032232.1.26",
          "form": "그쵸 그래도 이정도면 정말 결혼잘했다 생각하고 사네요 ㅋㅋ",
          "original_form": "그쵸 그래도 이정도면 정말 결혼잘했다 생각하고 사네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100032232.1.27",
          "form": "가끔 연애때 생각나고 그립기도하구요 ~~ㅋㅋ",
          "original_form": "가끔 연애때 생각나고 그립기도하구요 ~~ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100032232.1.28",
          "form": "그정도면정말잘하신결혼인듯요",
          "original_form": "그정도면정말잘하신결혼인듯요",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100032232.1.29",
          "form": "요즘 젊은 아빠들은 다 애들한테 잘한다는데 보면 다 그렇지도 않더라구요~~",
          "original_form": "요즘 젊은 아빠들은 다 애들한테 잘한다는데 보면 다 그렇지도 않더라구요~~",
