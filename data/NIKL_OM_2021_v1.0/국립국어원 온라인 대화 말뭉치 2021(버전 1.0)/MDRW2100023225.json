      "utterance": [
        {
          "id": "MDRW2100023225.1.1",
          "form": "안녀하세요~",
          "original_form": "안녀하세요~",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100023225.1.2",
          "form": "좋아하는 스포츠가 있으신가요?",
          "original_form": "좋아하는 스포츠가 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100023225.1.3",
          "form": "보는거요??",
          "original_form": "보는거요??",
          "speaker_id": "2",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100023225.1.4",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100023225.1.5",
          "form": "저 올림픽에 배구의 매력을 알았어요~",
          "original_form": "저 올림픽에 배구의 매력을 알았어요~",
          "speaker_id": "2",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100023225.1.6",
          "form": "그래서 하이큐 까지 봤어요 ㅋ",
          "original_form": "그래서 하이큐 까지 봤어요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100023225.1.7",
          "form": "앗 저두 올림픽으로 배구의 재미를 알았는뎈ㅋㅋㄱ",
          "original_form": "앗 저두 올림픽으로 배구의 재미를 알았는뎈ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100023225.1.8",
          "form": "하이큐면 배구만화인가요?",
          "original_form": "하이큐면 배구만화인가요?",
          "speaker_id": "1",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023225.1.9",
          "form": "네 배구만화인데 ㅋㅋㅋ 재미나요",
          "original_form": "네 배구만화인데 ㅋㅋㅋ 재미나요",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023225.1.10",
          "form": "넷플 티빙에 있더라구요 ㅋㅋ",
          "original_form": "넷플 티빙에 있더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023225.1.11",
          "form": "아? 만화영화군요? 다음에 봐봐야겠네요 ㅋㅋ",
          "original_form": "아? 만화영화군요? 다음에 봐봐야겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023225.1.12",
          "form": "그럼 직접하는 운동도 있으세요?",
          "original_form": "그럼 직접하는 운동도 있으세요?",
          "speaker_id": "1",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023225.1.13",
          "form": "집과 일터가 멀어서 핑계가 사실 엄청 큰데요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "집과 일터가 멀어서 핑계가 사실 엄청 큰데요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023225.1.14",
          "form": "멀어서 지쳐서 집오면 운동을 못하겠어요 ㅋㅋ",
          "original_form": "멀어서 지쳐서 집오면 운동을 못하겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023225.1.15",
          "form": "출퇴근 일터 걸어가는데 아침30분 저녁30분 걸으니 ㅋㅋㅋㅋㅋ 지쳐욬ㅋ",
          "original_form": "출퇴근 일터 걸어가는데 아침30분 저녁30분 걸으니 ㅋㅋㅋㅋㅋ 지쳐욬ㅋ",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023225.1.16",
          "form": "그러고 전철도 타니 체...체력이",
          "original_form": "그러고 전철도 타니 체...체력이",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023225.1.17",
          "form": "헐 진짜 먼거리네요 ㅠㅜ",
          "original_form": "헐 진짜 먼거리네요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023225.1.18",
          "form": "그럼 중계를 보는걸로 대리만족을 ㅎㅎ",
          "original_form": "그럼 중계를 보는걸로 대리만족을 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100023225.1.19",
          "form": "저는 올림픽으로 배구도 좋아하게 되었는데 클라이밍도 생소하지만 재밌개 봤어요",
          "original_form": "저는 올림픽으로 배구도 좋아하게 되었는데 클라이밍도 생소하지만 재밌개 봤어요",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023225.1.20",
          "form": "클라이밍 멋지죠 ㅋㅋㅋ 예전 친구가 그렇게 클라이밍 하던데 잔근육이 대단했던 기억이 있어요 ㅋㅋ",
          "original_form": "클라이밍 멋지죠 ㅋㅋㅋ 예전 친구가 그렇게 클라이밍 하던데 잔근육이 대단했던 기억이 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:13"
        },
        {
          "id": "MDRW2100023225.1.21",
          "form": "우오",
          "original_form": "우오",
          "speaker_id": "1",
          "time": "20210829 23:14"
        },
        {
          "id": "MDRW2100023225.1.22",
          "form": "제 친구도 클라이밍을 배우다가 생각보다 어렵다고 했는데",
          "original_form": "제 친구도 클라이밍을 배우다가 생각보다 어렵다고 했는데",
          "speaker_id": "1",
          "time": "20210829 23:15"
        },
        {
          "id": "MDRW2100023225.1.23",
          "form": "이번에 왜 어렵다고 했는지 이번에 경기를 보니 알겠다라구요",
          "original_form": "이번에 왜 어렵다고 했는지 이번에 경기를 보니 알겠다라구요",
          "speaker_id": "1",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023225.1.24",
          "form": "매달리는거니 진짜 왠만한 운동신경 아니고서야 ㅋㅋ",
          "original_form": "매달리는거니 진짜 왠만한 운동신경 아니고서야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023225.1.25",
          "form": "마자용!",
          "original_form": "마자용!",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023225.1.26",
          "form": "전 철봉도 2초만에 떨어지던 학생때 ㅋㅋㅋ",
          "original_form": "전 철봉도 2초만에 떨어지던 학생때 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:17"
        },
        {
          "id": "MDRW2100023225.1.27",
          "form": "체력장...",
          "original_form": "체력장...",
          "speaker_id": "2",
          "time": "20210829 23:17"
        },
        {
          "id": "MDRW2100023225.1.28",
          "form": "저도 턱걸이는 3초만에...ㅋㅋㅋ 철봉매달리기는 좀 버텼는데 클라이밈은 다르겠지만",
          "original_form": "저도 턱걸이는 3초만에...ㅋㅋㅋ 철봉매달리기는 좀 버텼는데 클라이밈은 다르겠지만",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023225.1.29",
          "form": "해보고 싶어요 ㅋㅋㅋㅋ",
          "original_form": "해보고 싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023225.1.30",
          "form": "혹시 펜싱도 보셨나요?",
          "original_form": "혹시 펜싱도 보셨나요?",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023225.1.31",
          "form": "아니여 ㅋㅋㅋㅋ",
          "original_form": "아니여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023225.1.32",
          "form": "전 한다면 필라테스 하고싶어요",
          "original_form": "전 한다면 필라테스 하고싶어요",
          "speaker_id": "2",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023225.1.33",
          "form": "허리랑 목이 안좋아서 ㅋㅋㅋ",
          "original_form": "허리랑 목이 안좋아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023225.1.34",
          "form": "허리랑 목이 안좋으시면 필라테스나 요가를 추천드려요!!!!",
