      "utterance": [
        {
          "id": "MDRW2100002239.1.1",
          "form": "커뮤니티 하면서",
          "original_form": "커뮤니티 하면서",
          "speaker_id": "1",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002239.1.2",
          "form": "정모 해본적 있어?",
          "original_form": "정모 해본적 있어?",
          "speaker_id": "1",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002239.1.3",
          "form": "나는 없어",
          "original_form": "나는 없어",
          "speaker_id": "2",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002239.1.4",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "2",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002239.1.5",
          "form": "나도 없어",
          "original_form": "나도 없어",
          "speaker_id": "1",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002239.1.6",
          "form": "좀 무섭다고 해야 할까",
          "original_form": "좀 무섭다고 해야 할까",
          "speaker_id": "1",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002239.1.7",
          "form": "맞아 전혀 모르는 사람이라서",
          "original_form": "맞아 전혀 모르는 사람이라서",
          "speaker_id": "2",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002239.1.8",
          "form": "무슨 일이 있을지 모르잖아",
          "original_form": "무슨 일이 있을지 모르잖아",
          "speaker_id": "1",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002239.1.9",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002239.1.10",
          "form": "확실히 정모는 위험해",
          "original_form": "확실히 정모는 위험해",
          "speaker_id": "1",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002239.1.11",
          "form": "하지만 하는 사람 보니까",
          "original_form": "하지만 하는 사람 보니까",
          "speaker_id": "1",
          "time": "20210820 01:06"
        },
        {
          "id": "MDRW2100002239.1.12",
          "form": "재미있게 하는 경우도 있더라",
          "original_form": "재미있게 하는 경우도 있더라",
          "speaker_id": "1",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002239.1.13",
          "form": "맞아 신기해",
          "original_form": "맞아 신기해",
          "speaker_id": "2",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002239.1.14",
          "form": "서로 만나서 얘기하고",
          "original_form": "서로 만나서 얘기하고",
          "speaker_id": "2",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002239.1.15",
          "form": "부럽긴 해",
          "original_form": "부럽긴 해",
          "speaker_id": "1",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002239.1.16",
          "form": "낯가림 심한 나한테는",
          "original_form": "낯가림 심한 나한테는",
          "speaker_id": "2",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002239.1.17",
          "form": "너무 먼 얘기다",
          "original_form": "너무 먼 얘기다",
          "speaker_id": "2",
          "time": "20210820 01:07"
        },
        {
          "id": "MDRW2100002239.1.18",
          "form": "나는 위험하지만 않으면",
          "original_form": "나는 위험하지만 않으면",
          "speaker_id": "1",
          "time": "20210820 01:08"
        },
        {
          "id": "MDRW2100002239.1.19",
          "form": "해보고싶지만",
          "original_form": "해보고싶지만",
          "speaker_id": "1",
          "time": "20210820 01:08"
        },
        {
          "id": "MDRW2100002239.1.20",
          "form": "아마 안할것같아",
          "original_form": "아마 안할것같아",
          "speaker_id": "1",
          "time": "20210820 01:08"
        },
        {
          "id": "MDRW2100002239.1.21",
          "form": "나도 안할듯",
          "original_form": "나도 안할듯",
          "speaker_id": "2",
          "time": "20210820 01:08"
        },
        {
          "id": "MDRW2100002239.1.22",
          "form": "ㅋㅋ 둘 다 안하네",
          "original_form": "ㅋㅋ 둘 다 안하네",
          "speaker_id": "1",
          "time": "20210820 01:09"
        }
      ]
