      "utterance": [
        {
          "id": "MDRW2100002253.1.1",
          "form": "뮤지컬 본 적 있어?",
          "original_form": "뮤지컬 본 적 있어?",
          "speaker_id": "2",
          "time": "20210820 01:29"
        },
        {
          "id": "MDRW2100002253.1.2",
          "form": "나 한두번 본적있어",
          "original_form": "나 한두번 본적있어",
          "speaker_id": "1",
          "time": "20210820 01:30"
        },
        {
          "id": "MDRW2100002253.1.3",
          "form": "한번은 미국여행가서 브로드웨이가서 봤는데",
          "original_form": "한번은 미국여행가서 브로드웨이가서 봤는데",
          "speaker_id": "1",
          "time": "20210820 01:30"
        },
        {
          "id": "MDRW2100002253.1.4",
          "form": "뭐 봤었어??",
          "original_form": "뭐 봤었어??",
          "speaker_id": "2",
          "time": "20210820 01:30"
        },
        {
          "id": "MDRW2100002253.1.5",
          "form": "잘 몰라도 좋더라 ㅠㅠㅋ",
          "original_form": "잘 몰라도 좋더라 ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210820 01:30"
        },
        {
          "id": "MDRW2100002253.1.6",
          "form": "올ㅋㅋ",
          "original_form": "올ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:30"
        },
        {
          "id": "MDRW2100002253.1.7",
          "form": "위키드라고 유명한거던데",
          "original_form": "위키드라고 유명한거던데",
          "speaker_id": "1",
          "time": "20210820 01:30"
        },
        {
          "id": "MDRW2100002253.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:30"
        },
        {
          "id": "MDRW2100002253.1.9",
          "form": "위키드 진짜 유명하지ㅋㅋ",
          "original_form": "위키드 진짜 유명하지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:31"
        },
        {
          "id": "MDRW2100002253.1.10",
          "form": "옥주현 정선아꺼 보고 싶었는데 티켓팅 빡쎄서 못 봤어ㅠㅜ",
          "original_form": "옥주현 정선아꺼 보고 싶었는데 티켓팅 빡쎄서 못 봤어ㅠㅜ",
          "speaker_id": "2",
          "time": "20210820 01:31"
        },
        {
          "id": "MDRW2100002253.1.11",
          "form": "아 뮤지컬 좋아하는구나 ㅠㅠㅋㅋㅋ",
          "original_form": "아 뮤지컬 좋아하는구나 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:31"
        },
        {
          "id": "MDRW2100002253.1.12",
          "form": "나는 지방살아서 우리동네까지는 잘 안와 ㅠㅠ",
          "original_form": "나는 지방살아서 우리동네까지는 잘 안와 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 01:32"
        },
        {
          "id": "MDRW2100002253.1.13",
          "form": "입덕한지 얼마 안 되서ㅋㅋㅋㅋ본 것 몇개 없어ㅋㅋ",
          "original_form": "입덕한지 얼마 안 되서ㅋㅋㅋㅋ본 것 몇개 없어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:32"
        },
        {
          "id": "MDRW2100002253.1.14",
          "form": "나도 그래서 서울까지 가서 봐ㅠㅜ",
          "original_form": "나도 그래서 서울까지 가서 봐ㅠㅜ",
          "speaker_id": "2",
          "time": "20210820 01:32"
        },
        {
          "id": "MDRW2100002253.1.15",
          "form": "교통비 장난 아냐ㅜ",
          "original_form": "교통비 장난 아냐ㅜ",
          "speaker_id": "2",
          "time": "20210820 01:32"
        },
        {
          "id": "MDRW2100002253.1.16",
          "form": "아 그렇구나 ㅠㅠㅋ 가장 재밌게본건 뭐야?",
          "original_form": "아 그렇구나 ㅠㅠㅋ 가장 재밌게본건 뭐야?",
          "speaker_id": "1",
          "time": "20210820 01:33"
        },
        {
          "id": "MDRW2100002253.1.17",
          "form": "드라큘라! 무대가 완전 휙휙 바껴서 진짜 재밌음ㅋㅋㅋ",
          "original_form": "드라큘라! 무대가 완전 휙휙 바껴서 진짜 재밌음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:34"
        },
        {
          "id": "MDRW2100002253.1.18",
          "form": "오 그렇구나 ㅋㅋㅋㅋ 궁금하다",
          "original_form": "오 그렇구나 ㅋㅋㅋㅋ 궁금하다",
          "speaker_id": "1",
          "time": "20210820 01:34"
        },
        {
          "id": "MDRW2100002253.1.19",
          "form": "나는 혐오스런 마츠코의 일생? 그거 무슨 이벤트 당첨되서 봤었는데",
          "original_form": "나는 혐오스런 마츠코의 일생? 그거 무슨 이벤트 당첨되서 봤었는데",
          "speaker_id": "1",
          "time": "20210820 01:34"
        },
        {
          "id": "MDRW2100002253.1.20",
          "form": "너무 좋더라구 다들 노래도 잘하고",
          "original_form": "너무 좋더라구 다들 노래도 잘하고",
          "speaker_id": "1",
          "time": "20210820 01:34"
        },
        {
          "id": "MDRW2100002253.1.21",
          "form": "오 그거 제목만 들어봄ㅋㅋㅋ",
          "original_form": "오 그거 제목만 들어봄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:35"
        },
        {
          "id": "MDRW2100002253.1.22",
          "form": "진짜 뮤배들 다 대단한 것 같아ㅋㄱㄲ",
          "original_form": "진짜 뮤배들 다 대단한 것 같아ㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210820 01:35"
        },
        {
          "id": "MDRW2100002253.1.23",
          "form": "맞아 그때 주연이 아이비?였는데",
          "original_form": "맞아 그때 주연이 아이비?였는데",
          "speaker_id": "1",
          "time": "20210820 01:35"
        },
        {
          "id": "MDRW2100002253.1.24",
          "form": "가수로만 알고있다가 그렇게 보니까 다르더라구 ㅋㅋㅋ",
          "original_form": "가수로만 알고있다가 그렇게 보니까 다르더라구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:35"
        },
        {
          "id": "MDRW2100002253.1.25",
          "form": "아이비 너무 좋아ㅠㅜㅜ",
          "original_form": "아이비 너무 좋아ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 01:35"
        },
        {
          "id": "MDRW2100002253.1.26",
          "form": "아이비 보고싶어서 친구랑 레드북 보러가기로 했어ㅋㅋ",
          "original_form": "아이비 보고싶어서 친구랑 레드북 보러가기로 했어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:36"
        },
        {
          "id": "MDRW2100002253.1.27",
          "form": "아 좋아하는구나 ㅋㅋㅋ 나는 잘 모르다가 그렇게 보니까 싱기했어",
          "original_form": "아 좋아하는구나 ㅋㅋㅋ 나는 잘 모르다가 그렇게 보니까 싱기했어",
          "speaker_id": "1",
          "time": "20210820 01:36"
        },
        {
          "id": "MDRW2100002253.1.28",
          "form": "노래도 잘하고 연기도 잘하지 않음?ㅋㅋㅋㅋ넘 멋져ㅋㅋ",
          "original_form": "노래도 잘하고 연기도 잘하지 않음?ㅋㅋㅋㅋ넘 멋져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:37"
        }
      ]
    }
