        },
        {
          "id": "MDRW2100017310.1.2",
          "form": "응 훌륭한 저녁식사 메뉴",
          "original_form": "응 훌륭한 저녁식사 메뉴",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017310.1.3",
          "form": "그거 먹고 매실도 좋았어",
          "original_form": "그거 먹고 매실도 좋았어",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017310.1.4",
          "form": "응 매실청과 탄산수",
          "original_form": "응 매실청과 탄산수",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017310.1.5",
          "form": "탄산수 이제 사놓은 것 끝이야",
          "original_form": "탄산수 이제 사놓은 것 끝이야",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017310.1.6",
          "form": "매실청 탄산수랑 하니까 맛있었지",
          "original_form": "매실청 탄산수랑 하니까 맛있었지",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017310.1.7",
          "form": "음 좀 두고 보다가 살까",
          "original_form": "음 좀 두고 보다가 살까",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017310.1.8",
          "form": "이번 주말 쯤에 탄산수 구비하자",
          "original_form": "이번 주말 쯤에 탄산수 구비하자",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017310.1.9",
          "form": "나 이번 주말에 파운드 케이크 만들러 갈거야",
          "original_form": "나 이번 주말에 파운드 케이크 만들러 갈거야",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017310.1.10",
          "form": "베이킹 잘하고 맛있게 만들어올게",
          "original_form": "베이킹 잘하고 맛있게 만들어올게",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017310.1.11",
          "form": "아맞다 그거 이번주!!!!",
          "original_form": "아맞다 그거 이번주!!!!",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017310.1.12",
          "form": "완전맛있겠다 기대된다",
          "original_form": "완전맛있겠다 기대된다",
          "speaker_id": "2",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017310.1.13",
          "form": "ㅎㅎㅎ베이킹 오랜만이야",
          "original_form": "ㅎㅎㅎ베이킹 오랜만이야",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
