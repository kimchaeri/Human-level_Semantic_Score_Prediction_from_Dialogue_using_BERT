      "utterance": [
        {
          "id": "MDRW2100004131.1.1",
          "form": "거기는 날씨가 어때요?",
          "original_form": "거기는 날씨가 어때요?",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004131.1.2",
          "form": "비 그치고 해가 쨍쨍하네요",
          "original_form": "비 그치고 해가 쨍쨍하네요",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004131.1.3",
          "form": "언제 비왔냐듯이",
          "original_form": "언제 비왔냐듯이",
          "speaker_id": "1",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004131.1.4",
          "form": "여기도 방금까지 비 엄청오다가",
          "original_form": "여기도 방금까지 비 엄청오다가",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004131.1.5",
          "form": "급 멈췄어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "급 멈췄어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004131.1.6",
          "form": "옹...",
          "original_form": "옹...",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004131.1.7",
          "form": "요새 날씨가 진짜",
          "original_form": "요새 날씨가 진짜",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004131.1.8",
          "form": "이상한것같아요",
          "original_form": "이상한것같아요",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004131.1.9",
          "form": "비도 뭔가",
          "original_form": "비도 뭔가",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004131.1.10",
          "form": "동남아처럼 스콜 형식으로 내려오구",
          "original_form": "동남아처럼 스콜 형식으로 내려오구",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004131.1.11",
          "form": "진짜 이상기후..?",
          "original_form": "진짜 이상기후..?",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004131.1.12",
          "form": "체감하고있네요...ㅠㅠ",
          "original_form": "체감하고있네요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004131.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004131.1.14",
          "form": "안온다고 했다가 비 오고",
          "original_form": "안온다고 했다가 비 오고",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004131.1.15",
          "form": "온다했다가 안오고",
          "original_form": "온다했다가 안오고",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004131.1.16",
          "form": "지구온난화 때문에 참..",
          "original_form": "지구온난화 때문에 참..",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004131.1.17",
          "form": "그러니까요...ㅠㅠ",
          "original_form": "그러니까요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004131.1.18",
          "form": "이제 정말 사계절이 없어지는건가 싶기도 하구...",
          "original_form": "이제 정말 사계절이 없어지는건가 싶기도 하구...",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004131.1.19",
          "form": "그리고 습기 때문에 힘들어요 ㅠ",
          "original_form": "그리고 습기 때문에 힘들어요 ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004131.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004131.1.21",
          "form": "진짜 이번 여름은",
          "original_form": "진짜 이번 여름은",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004131.1.22",
          "form": "찜통안의 만두가 된 기분...",
          "original_form": "찜통안의 만두가 된 기분...",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004131.1.23",
          "form": "매일매일 사우나의 연속...",
          "original_form": "매일매일 사우나의 연속...",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004131.1.24",
          "form": "햇빛이 그냥 엄청",
          "original_form": "햇빛이 그냥 엄청",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004131.1.25",
          "form": "살을 뚫을 거 같았어요",
          "original_form": "살을 뚫을 거 같았어요",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004131.1.26",
          "form": "그늘 없으면 못살아요....",
          "original_form": "그늘 없으면 못살아요....",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004131.1.27",
          "form": "원래 점심에 산책을 자주했는데",
          "original_form": "원래 점심에 산책을 자주했는데",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004131.1.28",
          "form": "포기했어요",
          "original_form": "포기했어요",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004131.1.29",
          "form": "얼마 걷지도 않았는데 살도 많이 타구",
          "original_form": "얼마 걷지도 않았는데 살도 많이 타구",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004131.1.30",
          "form": "맞아요 전 그래서",
          "original_form": "맞아요 전 그래서",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004131.1.31",
          "form": "양산을 자주 써요",
          "original_form": "양산을 자주 써요",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004131.1.32",
          "form": "양산 진짜 필수인것같아요",
          "original_form": "양산 진짜 필수인것같아요",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004131.1.33",
          "form": "진짜 이제 여름에 양산 꼭이에요",
          "original_form": "진짜 이제 여름에 양산 꼭이에요",
          "speaker_id": "2",
          "time": "20210821 15:17"
        }
      ]
    }
  ]
