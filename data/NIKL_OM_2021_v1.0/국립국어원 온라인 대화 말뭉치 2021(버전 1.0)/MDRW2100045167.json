      "utterance": [
        {
          "id": "MDRW2100045167.1.1",
          "form": "여행 좋아하나요?",
          "original_form": "여행 좋아하나요?",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045167.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045167.1.3",
          "form": "최근 가신 어디 갔나요",
          "original_form": "최근 가신 어디 갔나요",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045167.1.4",
          "form": "여행이라기엔 그렇지만 제주도 다녀왔어요",
          "original_form": "여행이라기엔 그렇지만 제주도 다녀왔어요",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045167.1.5",
          "form": "제주도 이쁘죠 전 수학여행 이후 가보지 못했네요 어땠나요?",
          "original_form": "제주도 이쁘죠 전 수학여행 이후 가보지 못했네요 어땠나요?",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045167.1.6",
          "form": "새로 생긴 루지? 같은 기구 탈 수 있는데 갔는데 재밌었어요! 추천이애요",
          "original_form": "새로 생긴 루지? 같은 기구 탈 수 있는데 갔는데 재밌었어요! 추천이애요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045167.1.7",
          "form": "오오 다양한게 생겼네요",
          "original_form": "오오 다양한게 생겼네요",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045167.1.8",
          "form": "생긴지 얼마 안 된 것 같았어요",
          "original_form": "생긴지 얼마 안 된 것 같았어요",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045167.1.9",
          "form": "어디 더 가셨나요?",
          "original_form": "어디 더 가셨나요?",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045167.1.10",
          "form": "바다 구경 갔어요",
          "original_form": "바다 구경 갔어요",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045167.1.11",
          "form": "해수욕장이름은 기억 안 나네요...",
          "original_form": "해수욕장이름은 기억 안 나네요...",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045167.1.12",
          "form": "바다 물색깔이 이뻤겠네요",
          "original_form": "바다 물색깔이 이뻤겠네요",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045167.1.13",
          "form": "날이 흐려서 기대보다는 별로였어요ㅠㅠ",
          "original_form": "날이 흐려서 기대보다는 별로였어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045167.1.14",
          "form": "저런.... \r\n제주도 말고 다른 곳 가보셨나요?",
          "original_form": "저런.... \r\n제주도 말고 다른 곳 가보셨나요?",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045167.1.15",
          "form": "최근은 아닌데 안동 갔던게 기억에 남아요",
          "original_form": "최근은 아닌데 안동 갔던게 기억에 남아요",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045167.1.16",
          "form": "안동에서 뭐하셨나요? 한옥 이뻤겠네요",
          "original_form": "안동에서 뭐하셨나요? 한옥 이뻤겠네요",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045167.1.17",
          "form": "한옥마을이랑 서원 구경했어요",
          "original_form": "한옥마을이랑 서원 구경했어요",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045167.1.18",
          "form": "안동 관광객 엄청 많다고 하던데...",
          "original_form": "안동 관광객 엄청 많다고 하던데...",
          "speaker_id": "1",
          "time": "20210908 01:27"
        }
      ]
