      "utterance": [
        {
          "id": "MDRW2100002566.1.1",
          "form": "안녕하세요~ 연애를 하고 계신가요?",
          "original_form": "안녕하세요~ 연애를 하고 계신가요?",
          "speaker_id": "1",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002566.1.2",
          "form": "안녕하세요~ 아뇨 전 솔로입니다",
          "original_form": "안녕하세요~ 아뇨 전 솔로입니다",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002566.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002566.1.4",
          "form": "아 저는 연애중입니다 ㅋㅋ",
          "original_form": "아 저는 연애중입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002566.1.5",
          "form": "연애관해서 여쭤보려고 했는데.. 안하고계시면 이상형이 어떻게 되세요?",
          "original_form": "연애관해서 여쭤보려고 했는데.. 안하고계시면 이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002566.1.6",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002566.1.7",
          "form": "저는 그냥 연애할 생각이",
          "original_form": "저는 그냥 연애할 생각이",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002566.1.8",
          "form": "없어서",
          "original_form": "없어서",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002566.1.9",
          "form": "이상형도 딱히..생각나는게 없네요",
          "original_form": "이상형도 딱히..생각나는게 없네요",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002566.1.10",
          "form": "그냥 저랑 잘 맞는 사람?",
          "original_form": "그냥 저랑 잘 맞는 사람?",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002566.1.11",
          "form": "하긴 나랑 잘 맞는게 젤 중요하죠",
          "original_form": "하긴 나랑 잘 맞는게 젤 중요하죠",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002566.1.12",
          "form": "대화 잘통하거나 그런것들",
          "original_form": "대화 잘통하거나 그런것들",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002566.1.13",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002566.1.14",
          "form": "오잇님은 연애하신 기간이 어느정도 되셨나요",
          "original_form": "오잇님은 연애하신 기간이 어느정도 되셨나요",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002566.1.15",
          "form": "저는.. 한 7년정도 된거 같아요",
          "original_form": "저는.. 한 7년정도 된거 같아요",
          "speaker_id": "1",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002566.1.16",
          "form": "결혼은 아직 생각없구요 ㅋㅋ 초코님은 결혼생각도 없으신가요?",
          "original_form": "결혼은 아직 생각없구요 ㅋㅋ 초코님은 결혼생각도 없으신가요?",
          "speaker_id": "1",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002566.1.17",
          "form": "오 오래 사귀셨네요",
          "original_form": "오 오래 사귀셨네요",
          "speaker_id": "2",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002566.1.18",
          "form": "오래사귀는커플 보면 좋아보이더라구요",
          "original_form": "오래사귀는커플 보면 좋아보이더라구요",
          "speaker_id": "2",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002566.1.19",
          "form": "전 결혼도 생각없습니다",
          "original_form": "전 결혼도 생각없습니다",
          "speaker_id": "2",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002566.1.20",
          "form": "만나다 보니 그렇게 됐네요",
          "original_form": "만나다 보니 그렇게 됐네요",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002566.1.21",
          "form": "요즘은 거의 결혼생각 많이들 없는거 같아요 제친구들도 그렇고",
          "original_form": "요즘은 거의 결혼생각 많이들 없는거 같아요 제친구들도 그렇고",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002566.1.22",
          "form": "그쵸 예전하고",
          "original_form": "그쵸 예전하고",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002566.1.23",
          "form": "많이 다른거같아요",
          "original_form": "많이 다른거같아요",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002566.1.24",
          "form": "분위기가",
          "original_form": "분위기가",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002566.1.25",
          "form": "꼭 결혼을 해야한다고 생각하지 않아요",
          "original_form": "꼭 결혼을 해야한다고 생각하지 않아요",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002566.1.26",
          "form": "1인 가구가 점점 늘어나는게 당연한 수순이라고 생각합니다",
          "original_form": "1인 가구가 점점 늘어나는게 당연한 수순이라고 생각합니다",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002566.1.27",
          "form": "맞아요 결혼이 필수인 시대는 지났죠",
          "original_form": "맞아요 결혼이 필수인 시대는 지났죠",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002566.1.28",
          "form": "누군가에게 희생 강요하는 것도 안될 일이구요",
          "original_form": "누군가에게 희생 강요하는 것도 안될 일이구요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002566.1.29",
          "form": "자기할일 하면서 재밌게 살수 있으면 그게 제일 좋은거같아요~",
          "original_form": "자기할일 하면서 재밌게 살수 있으면 그게 제일 좋은거같아요~",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002566.1.30",
          "form": "공감합니다ㅎㅎ",
          "original_form": "공감합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002566.1.31",
          "form": "억지로 다 결혼하는거보단",
          "original_form": "억지로 다 결혼하는거보단",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002566.1.32",
          "form": "본인 가치관대로",
          "original_form": "본인 가치관대로",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002566.1.33",
          "form": "자유롭게 선택 가능한 분위기가 좋은거같아요",
          "original_form": "자유롭게 선택 가능한 분위기가 좋은거같아요",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002566.1.34",
          "form": "네 ㅋㅋ 하고 싶은 사람은 하고, 안하고 싶은 사람에게는 강요안하는 사회적 분위기가 형성되었으면 좋겠네요",
          "original_form": "네 ㅋㅋ 하고 싶은 사람은 하고, 안하고 싶은 사람에게는 강요안하는 사회적 분위기가 형성되었으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210820 14:00"
        }
      ]
