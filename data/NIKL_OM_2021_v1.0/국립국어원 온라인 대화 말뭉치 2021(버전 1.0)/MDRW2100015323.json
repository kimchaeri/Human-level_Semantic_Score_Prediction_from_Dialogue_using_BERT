      "utterance": [
        {
          "id": "MDRW2100015323.1.1",
          "form": "오늘은 집안일 하나도 안해서 기뻐요 ㅋㅋㅋ",
          "original_form": "오늘은 집안일 하나도 안해서 기뻐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015323.1.2",
          "form": "ㅋㅋㅋㅋㄱ도비는자유예요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱ도비는자유예요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015323.1.3",
          "form": "저도시켜먹어서설거지없어서좋아요",
          "original_form": "저도시켜먹어서설거지없어서좋아요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015323.1.4",
          "form": "잘했어요 도비이즈프리",
          "original_form": "잘했어요 도비이즈프리",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015323.1.5",
          "form": "그냥 내가 맘을 내려놓으면 되는거였어요",
          "original_form": "그냥 내가 맘을 내려놓으면 되는거였어요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015323.1.6",
          "form": "엄마가 다 하시지만 .... 엄마도 뭐 엄마가 하실만하니 하시겠죠",
          "original_form": "엄마가 다 하시지만 .... 엄마도 뭐 엄마가 하실만하니 하시겠죠",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015323.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래놓고 또 며칠뒤에 집안일할거잖아요..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래놓고 또 며칠뒤에 집안일할거잖아요..",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015323.1.8",
          "form": "착해서...흑흑",
          "original_form": "착해서...흑흑",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015323.1.9",
          "form": "자기가 먹은 김통도 안치웠어요 동생이ㅠㅠ",
          "original_form": "자기가 먹은 김통도 안치웠어요 동생이ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015323.1.10",
          "form": "나쁘죠ㅠㅠ",
          "original_form": "나쁘죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015323.1.11",
          "form": "이제 다음주 간대요 그래서 그냥 하죠 뭐ㅠㅠ",
          "original_form": "이제 다음주 간대요 그래서 그냥 하죠 뭐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015323.1.12",
          "form": "이제 동생 가면 엄마도ㅠ집안일 안하실거같아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이제 동생 가면 엄마도ㅠ집안일 안하실거같아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015323.1.13",
          "form": "김통도안치우는건 너무한거아닌가요",
          "original_form": "김통도안치우는건 너무한거아닌가요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015323.1.14",
          "form": "이놈의자식",
          "original_form": "이놈의자식",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015323.1.15",
          "form": "이런말해도될까요",
          "original_form": "이런말해도될까요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015323.1.16",
          "form": "구쵸 ㅋㅋㅋㅋ",
          "original_form": "구쵸 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015323.1.17",
          "form": "그냥 쏙 들어가버려요 방에",
          "original_form": "그냥 쏙 들어가버려요 방에",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015323.1.18",
          "form": "가족이란 뭘까요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "가족이란 뭘까요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015323.1.19",
          "form": "애증의관계",
          "original_form": "애증의관계",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015323.1.20",
          "form": "ㅋㅋㅋㅋㄲㄱ",
          "original_form": "ㅋㅋㅋㅋㄲㄱ",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015323.1.21",
          "form": "애증의관계완전공감..",
          "original_form": "애증의관계완전공감..",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015323.1.22",
          "form": "돌아서면 또 못챙겨줘서 슬프겠죠?",
          "original_form": "돌아서면 또 못챙겨줘서 슬프겠죠?",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015323.1.23",
          "form": "맞아요..ㅜㅜ",
          "original_form": "맞아요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015323.1.24",
          "form": "앞으로 남은 나날은 잘 해줘야겠어요 ...",
          "original_form": "앞으로 남은 나날은 잘 해줘야겠어요 ...",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015323.1.25",
          "form": "흑흑이러지도저러지도못해에..",
          "original_form": "흑흑이러지도저러지도못해에..",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015323.1.26",
          "form": "그래도 너무한건 너무해 그냥 철이 없나봐요",
          "original_form": "그래도 너무한건 너무해 그냥 철이 없나봐요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015323.1.27",
          "form": "맞어요 아닌건 뭐라해야해요",
          "original_form": "맞어요 아닌건 뭐라해야해요",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015323.1.28",
          "form": "근데 그러다 싸움이나죠",
          "original_form": "근데 그러다 싸움이나죠",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015323.1.29",
          "form": "오늘 설거지했니?",
          "original_form": "오늘 설거지했니?",
          "speaker_id": "1",
          "time": "20210906 21:19"
        }
      ]
    }
