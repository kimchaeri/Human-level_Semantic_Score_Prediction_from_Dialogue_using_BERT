      "utterance": [
        {
          "id": "MDRW2100029759.1.1",
          "form": "점심 드셨나용~?",
          "original_form": "점심 드셨나용~?",
          "speaker_id": "2",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100029759.1.2",
          "form": "카페가면 자주 시키는 음로 있어요?",
          "original_form": "카페가면 자주 시키는 음로 있어요?",
          "speaker_id": "1",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100029759.1.3",
          "form": "아 저는 커피를 못 마셔요!",
          "original_form": "아 저는 커피를 못 마셔요!",
          "speaker_id": "2",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100029759.1.4",
          "form": "지금 친구랑 밥먹으러 가요!!",
          "original_form": "지금 친구랑 밥먹으러 가요!!",
          "speaker_id": "1",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100029759.1.5",
          "form": "우왕 뭐 드세요?",
          "original_form": "우왕 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100029759.1.6",
          "form": "고기 먹으러 갑니다ㅎ0ㅎ",
          "original_form": "고기 먹으러 갑니다ㅎ0ㅎ",
          "speaker_id": "1",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100029759.1.7",
          "form": "우왕~ 좋으시겠당ㅠㅁㅠ",
          "original_form": "우왕~ 좋으시겠당ㅠㅁㅠ",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029759.1.8",
          "form": "저두 고기 먹어야겠어요",
          "original_form": "저두 고기 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029759.1.9",
          "form": "학 글쿠나 커피아닌 다른 좋아하는 음료있으세요??",
          "original_form": "학 글쿠나 커피아닌 다른 좋아하는 음료있으세요??",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029759.1.10",
          "form": "급 뽐뿌...",
          "original_form": "급 뽐뿌...",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029759.1.11",
          "form": "저는 커피만 아니면 다 마시는 거 같아요 에이드도 좋구 홍차류도 좋아하구용!!",
          "original_form": "저는 커피만 아니면 다 마시는 거 같아요 에이드도 좋구 홍차류도 좋아하구용!!",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029759.1.12",
          "form": "요즘 아마스빈 사하라만 겁나 먹는 거 같아요 ㅣㅋㅋㅋㅋㅋ",
          "original_form": "요즘 아마스빈 사하라만 겁나 먹는 거 같아요 ㅣㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029759.1.13",
          "form": "스테이크 먹고 왔습니다ㅎㅎ",
          "original_form": "스테이크 먹고 왔습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100029759.1.14",
          "form": "에이드랑 홍차도 좋죠!! 루이보스 좋아해요ㅠㅠ",
          "original_form": "에이드랑 홍차도 좋죠!! 루이보스 좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:22"
        },
        {
          "id": "MDRW2100029759.1.15",
          "form": "오..아마스빈 사하라 마셔본적 없어요",
          "original_form": "오..아마스빈 사하라 마셔본적 없어요",
          "speaker_id": "1",
          "time": "20210903 14:22"
        },
        {
          "id": "MDRW2100029759.1.16",
          "form": "우왕 짱이네요~! 행복하시겠당!!",
          "original_form": "우왕 짱이네요~! 행복하시겠당!!",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029759.1.17",
          "form": "오 맛있어용 코코펄? 넣어서 먹으면 좋아료",
          "original_form": "오 맛있어용 코코펄? 넣어서 먹으면 좋아료",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029759.1.18",
          "form": "name1은 최애 카페음료 있으세요?",
          "original_form": "&name1&은 최애 카페음료 있으세요?",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029759.1.19",
          "form": "코코펄...메모...근처에 아마스빈이 없어서 슬퍼요 흑흑",
          "original_form": "코코펄...메모...근처에 아마스빈이 없어서 슬퍼요 흑흑",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029759.1.20",
          "form": "저 민초파라서 카페가면 먼저 민초있는지 부터 봐요ㅎㅎ",
          "original_form": "저 민초파라서 카페가면 먼저 민초있는지 부터 봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029759.1.21",
          "form": "앗....슬프네요ㅠㅠ",
          "original_form": "앗....슬프네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029759.1.22",
          "form": "헐 저두 민초완전좋아해용!!!!!!!!!!!!!",
          "original_form": "헐 저두 민초완전좋아해용!!!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029759.1.23",
          "form": "대박...... 요즘 카페에 민초 많이 늘어나서 너무 좋아용",
          "original_form": "대박...... 요즘 카페에 민초 많이 늘어나서 너무 좋아용",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029759.1.24",
          "form": "헐 대박 여기서 민초단을 만나다니,,,,??!",
          "original_form": "헐 대박 여기서 민초단을 만나다니,,,,??!",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029759.1.25",
          "form": "(감격)",
          "original_form": "(감격)",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029759.1.26",
          "form": "그쵸ㅠㅠㅠ 프랜차이즈카페에서도 민초 나오고해서 행복합니다..",
          "original_form": "그쵸ㅠㅠㅠ 프랜차이즈카페에서도 민초 나오고해서 행복합니다..",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029759.1.27",
          "form": "마자유마자유ㅠㅠㅠㅠ 민초 진짜 엄청 천대받았었는데",
          "original_form": "마자유마자유ㅠㅠㅠㅠ 민초 진짜 엄청 천대받았었는데",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100029759.1.28",
          "form": "요즘 완전 신분상승이에요",
          "original_form": "요즘 완전 신분상승이에요",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100029759.1.29",
          "form": "아이고ㅠㅠㅠ 흑흑 우리 민초",
          "original_form": "아이고ㅠㅠㅠ 흑흑 우리 민초",
          "speaker_id": "1",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100029759.1.30",
          "form": "올 여름은 민초라고 말할정도로 민초 상품들 쏟아져나와서",
          "original_form": "올 여름은 민초라고 말할정도로 민초 상품들 쏟아져나와서",
          "speaker_id": "1",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100029759.1.31",
          "form": "행복했어요",
          "original_form": "행복했어요",
          "speaker_id": "1",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100029759.1.32",
          "form": "마자요마자요",
          "original_form": "마자요마자요",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100029759.1.33",
          "form": "민트초코의 시대가 드디어 왔다!!",
          "original_form": "민트초코의 시대가 드디어 왔다!!",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100029759.1.34",
          "form": "저 베라가서도 항상 친구들 눈치봤었는데ㅠㅠ흑흑",
          "original_form": "저 베라가서도 항상 친구들 눈치봤었는데ㅠㅠ흑흑",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100029759.1.35",
          "form": "너무 기뻐용",
          "original_form": "너무 기뻐용",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100029759.1.36",
          "form": "민초 이제 음지의 음식이 아닙니다ㅠㅠ",
          "original_form": "민초 이제 음지의 음식이 아닙니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100029759.1.37",
          "form": "반민초단 친구들이랑은 어쩔수가 없는 흑흑...",
          "original_form": "반민초단 친구들이랑은 어쩔수가 없는 흑흑...",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100029759.1.38",
          "form": "배라 여름시즌에만 나오는 폴라베어? 그거 먹어보셨나유",
          "original_form": "배라 여름시즌에만 나오는 폴라베어? 그거 먹어보셨나유",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100029759.1.39",
          "form": "헉 아니용? 근데 그것도 민트라고 했던 거 같은데 맞나요??",
          "original_form": "헉 아니용? 근데 그것도 민트라고 했던 거 같은데 맞나요??",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100029759.1.40",
          "form": "맞아용 민트맛!",
          "original_form": "맞아용 민트맛!",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100029759.1.41",
          "form": "민트초콜릿칩보다는 민트맛 약했던거 같은데 맛있어요ㅠㅠ",
          "original_form": "민트초콜릿칩보다는 민트맛 약했던거 같은데 맛있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100029759.1.42",
          "form": "헐 먹어볼걸!!!",
          "original_form": "헐 먹어볼걸!!!",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100029759.1.43",
          "form": "못 먹어본 거 너무 아쉬워용",
          "original_form": "못 먹어본 거 너무 아쉬워용",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100029759.1.44",
          "form": "지금은 시즌 끝났죠ㅠㅠ",
          "original_form": "지금은 시즌 끝났죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100029759.1.45",
          "form": "그럴거같아요ㅠㅠㅠ 한정맛이 매달 나오니깐 흑흑...",
          "original_form": "그럴거같아요ㅠㅠㅠ 한정맛이 매달 나오니깐 흑흑...",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100029759.1.46",
          "form": "내년 여름에도 나오면 한 번 먹어보세요~~!",
          "original_form": "내년 여름에도 나오면 한 번 먹어보세요~~!",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100029759.1.47",
          "form": "넵넵!! 도전해볼 게 생겨서 좋아요",
          "original_form": "넵넵!! 도전해볼 게 생겨서 좋아요",
