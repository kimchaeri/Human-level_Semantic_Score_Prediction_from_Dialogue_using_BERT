      "utterance": [
        {
          "id": "MDRW2100040861.1.1",
          "form": "유튜브에 좋은 거 많더라고요",
          "original_form": "유튜브에 좋은 거 많더라고요",
          "speaker_id": "1",
          "time": "20210905 10:12"
        },
        {
          "id": "MDRW2100040861.1.2",
          "form": "그죠 :)",
          "original_form": "그죠 :)",
          "speaker_id": "2",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040861.1.3",
          "form": "영화관 간지도 2년이 다됐네요",
          "original_form": "영화관 간지도 2년이 다됐네요",
          "speaker_id": "2",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040861.1.4",
          "form": "그냥 보러갈까봐요",
          "original_form": "그냥 보러갈까봐요",
          "speaker_id": "2",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040861.1.5",
          "form": "어차피 직장출근이나 영화관이나",
          "original_form": "어차피 직장출근이나 영화관이나",
          "speaker_id": "2",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040861.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040861.1.7",
          "form": "네 한번 다녀오세요",
          "original_form": "네 한번 다녀오세요",
          "speaker_id": "1",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040861.1.8",
          "form": "작년인가 한번 갔는데 코나 때문에 아무도 없어서 좋았어요",
          "original_form": "작년인가 한번 갔는데 코나 때문에 아무도 없어서 좋았어요",
          "speaker_id": "1",
          "time": "20210905 10:13"
        },
        {
          "id": "MDRW2100040861.1.9",
          "form": "ㅋㅋㅋㅋ그런 장점이!",
          "original_form": "ㅋㅋㅋㅋ그런 장점이!",
          "speaker_id": "2",
          "time": "20210905 10:14"
        },
        {
          "id": "MDRW2100040861.1.10",
          "form": "영화보면서 음식물 못먹는건 좀 아쉽네요",
          "original_form": "영화보면서 음식물 못먹는건 좀 아쉽네요",
          "speaker_id": "2",
          "time": "20210905 10:15"
        },
        {
          "id": "MDRW2100040861.1.11",
          "form": "그렇긴 한데 사람이 없으니까",
          "original_form": "그렇긴 한데 사람이 없으니까",
          "speaker_id": "1",
          "time": "20210905 10:16"
        },
        {
          "id": "MDRW2100040861.1.12",
          "form": "먹어도 될 거 같았어요",
          "original_form": "먹어도 될 거 같았어요",
          "speaker_id": "1",
          "time": "20210905 10:16"
        },
        {
          "id": "MDRW2100040861.1.13",
          "form": "하긴 그렇네요!!",
          "original_form": "하긴 그렇네요!!",
          "speaker_id": "2",
          "time": "20210905 10:17"
        },
        {
          "id": "MDRW2100040861.1.14",
          "form": "작년에 뭐보셨어요~?",
          "original_form": "작년에 뭐보셨어요~?",
          "speaker_id": "2",
          "time": "20210905 10:17"
        },
        {
          "id": "MDRW2100040861.1.15",
          "form": "삼진그룹영어토익반이요",
          "original_form": "삼진그룹영어토익반이요",
          "speaker_id": "1",
          "time": "20210905 10:17"
        },
        {
          "id": "MDRW2100040861.1.16",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210905 10:17"
        },
        {
          "id": "MDRW2100040861.1.17",
          "form": "재밌게보셨나요?",
          "original_form": "재밌게보셨나요?",
          "speaker_id": "2",
          "time": "20210905 10:17"
        },
        {
          "id": "MDRW2100040861.1.18",
          "form": "네 나름 재밌엇어요",
          "original_form": "네 나름 재밌엇어요",
          "speaker_id": "1",
          "time": "20210905 10:18"
        },
        {
          "id": "MDRW2100040861.1.19",
          "form": "뮤지컬도 좋아하세요?",
          "original_form": "뮤지컬도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100040861.1.20",
          "form": "직접 본 적은 없어요",
          "original_form": "직접 본 적은 없어요",
          "speaker_id": "1",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100040861.1.21",
          "form": "클래식 좋아하시면 뮤지컬도 재밌게 보실것 같아요",
          "original_form": "클래식 좋아하시면 뮤지컬도 재밌게 보실것 같아요",
          "speaker_id": "2",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100040861.1.22",
          "form": "그런데 지금까진 기회가 없었어요",
          "original_form": "그런데 지금까진 기회가 없었어요",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100040861.1.23",
          "form": "전 그 연주 직접듣는게 매력적이더라구요 좋은 좌석에서 보면 연주하시는것도 보이고..!",
          "original_form": "전 그 연주 직접듣는게 매력적이더라구요 좋은 좌석에서 보면 연주하시는것도 보이고..!",
