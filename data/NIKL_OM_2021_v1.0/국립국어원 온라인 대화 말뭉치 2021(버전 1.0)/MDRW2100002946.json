      "utterance": [
        {
          "id": "MDRW2100002946.1.1",
          "form": "주말내내 날씨가 우중충할거같네요",
          "original_form": "주말내내 날씨가 우중충할거같네요",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002946.1.2",
          "form": "오늘은 소나기 소식도 있어요 여긴",
          "original_form": "오늘은 소나기 소식도 있어요 여긴",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002946.1.3",
          "form": "그쳐 날씨가 꾸리꾸리한게",
          "original_form": "그쳐 날씨가 꾸리꾸리한게",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002946.1.4",
          "form": "어제비오고 오늘은 여긴괜찮은데",
          "original_form": "어제비오고 오늘은 여긴괜찮은데",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002946.1.5",
          "form": "왠지 비또올거같아요",
          "original_form": "왠지 비또올거같아요",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002946.1.6",
          "form": "여기도 약간 전조증상이....",
          "original_form": "여기도 약간 전조증상이....",
          "speaker_id": "2",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100002946.1.7",
          "form": "주말에 내내 비일거같아요 ㅜㅜ",
          "original_form": "주말에 내내 비일거같아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100002946.1.8",
          "form": "일어나니 비가 완전 쏟아지네요ㅜㅜ",
          "original_form": "일어나니 비가 완전 쏟아지네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 09:49"
        },
        {
          "id": "MDRW2100002946.1.9",
          "form": "장마 시작인가봐요 ㅠㅠ 휴.....",
          "original_form": "장마 시작인가봐요 ㅠㅠ 휴.....",
          "speaker_id": "2",
          "time": "20210821 10:17"
        },
        {
          "id": "MDRW2100002946.1.10",
          "form": "좀 덜왔으면 좋겠는데 ㅠㅠ",
          "original_form": "좀 덜왔으면 좋겠는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 10:17"
        },
        {
          "id": "MDRW2100002946.1.11",
          "form": "그쳐 그냥그칠비가아니고 진짜 폭풍쏟아져요ㅜ",
          "original_form": "그쳐 그냥그칠비가아니고 진짜 폭풍쏟아져요ㅜ",
          "speaker_id": "1",
          "time": "20210821 10:22"
        },
        {
          "id": "MDRW2100002946.1.12",
          "form": "하..... 당분간 빨래 찝찝하게 생겼네요",
          "original_form": "하..... 당분간 빨래 찝찝하게 생겼네요",
          "speaker_id": "2",
          "time": "20210821 10:24"
        },
        {
          "id": "MDRW2100002946.1.13",
          "form": "내내 오는 비는 정말 감당 불가예요 ㅠㅠ",
          "original_form": "내내 오는 비는 정말 감당 불가예요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 10:25"
        },
        {
          "id": "MDRW2100002946.1.14",
          "form": "그쵸 빨래가 넘 찝찝해요",
          "original_form": "그쵸 빨래가 넘 찝찝해요",
          "speaker_id": "1",
          "time": "20210821 10:43"
        },
        {
          "id": "MDRW2100002946.1.15",
          "form": "며칠널어도 냄새나고ㅜ",
          "original_form": "며칠널어도 냄새나고ㅜ",
          "speaker_id": "1",
          "time": "20210821 10:43"
        },
        {
          "id": "MDRW2100002946.1.16",
          "form": "비올때마다 걱정인 ㅜㅜ",
          "original_form": "비올때마다 걱정인 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100002946.1.17",
          "form": "장마가 빨리 지나갔으면 좋겠어요",
          "original_form": "장마가 빨리 지나갔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100002946.1.18",
          "form": "네ㅜㅜ이번엔 장마피해없이 잘지나갔으면해요",
          "original_form": "네ㅜㅜ이번엔 장마피해없이 잘지나갔으면해요",
          "speaker_id": "1",
          "time": "20210821 13:01"
        },
        {
          "id": "MDRW2100002946.1.19",
          "form": "지반 낮은 곳 특히 걱정이네요 ㅜㅜ 뉴스로만 소식을 알수있으니 ㅜㅜ",
          "original_form": "지반 낮은 곳 특히 걱정이네요 ㅜㅜ 뉴스로만 소식을 알수있으니 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100002946.1.20",
          "form": "네ㅜ저희지역에 지반낮은곳들 작년에 피해가컸거든여ㅜㅜ",
          "original_form": "네ㅜ저희지역에 지반낮은곳들 작년에 피해가컸거든여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100002946.1.21",
          "form": "에고 올해는 비가 너무많이오지않았으면",
          "original_form": "에고 올해는 비가 너무많이오지않았으면",
          "speaker_id": "1",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100002946.1.22",
          "form": "언제그랬냐는듯 여기는 해가 쨍쨍하네요 ㅋㅋㅋ",
          "original_form": "언제그랬냐는듯 여기는 해가 쨍쨍하네요 ㅋㅋㅋ",
          "speaker_id": "2",
