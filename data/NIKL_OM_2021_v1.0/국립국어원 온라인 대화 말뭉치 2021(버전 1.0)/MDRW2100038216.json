      "utterance": [
        {
          "id": "MDRW2100038216.1.1",
          "form": "음료 좋아하세요?",
          "original_form": "음료 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100038216.1.2",
          "form": "바프 흑임자 드셔보셨어요?",
          "original_form": "바프 흑임자 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100038216.1.3",
          "form": "아니요 맛있어요??",
          "original_form": "아니요 맛있어요??",
          "speaker_id": "2",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100038216.1.4",
          "form": "진짜 맛나요",
          "original_form": "진짜 맛나요",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100038216.1.5",
          "form": "말차 바프도 맛나구요",
          "original_form": "말차 바프도 맛나구요",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100038216.1.6",
          "form": "말차는 먹어봤는데 흑임자도 있나보네요",
          "original_form": "말차는 먹어봤는데 흑임자도 있나보네요",
          "speaker_id": "2",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100038216.1.7",
          "form": "며칠전에 말차 먹어보고 맛나서",
          "original_form": "며칠전에 말차 먹어보고 맛나서",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100038216.1.8",
          "form": "어제 또 말차인줄알고",
          "original_form": "어제 또 말차인줄알고",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100038216.1.9",
          "form": "담아왔는데 한입먹고",
          "original_form": "담아왔는데 한입먹고",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100038216.1.10",
          "form": "놀랐네요",
          "original_form": "놀랐네요",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100038216.1.11",
          "form": "와사비 맛이더라구요 ㅋ",
          "original_form": "와사비 맛이더라구요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100038216.1.12",
          "form": "맛있어서요??",
          "original_form": "맛있어서요??",
          "speaker_id": "2",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100038216.1.13",
          "form": "아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100038216.1.14",
          "form": "와사비도 초록 말차도 초록이라 ㅋ",
          "original_form": "와사비도 초록 말차도 초록이라 ㅋ",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038216.1.15",
          "form": "와사비도 맛있긴 한데 예상치 못할때 ㅋㅋㅋㅋㅋ",
          "original_form": "와사비도 맛있긴 한데 예상치 못할때 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038216.1.16",
          "form": "자세히 안보고 담아 온거죠 ㅎ",
          "original_form": "자세히 안보고 담아 온거죠 ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038216.1.17",
          "form": "와사비도 초록색인가보네욬ㅋㅋㅋ",
          "original_form": "와사비도 초록색인가보네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038216.1.18",
          "form": "저는 와사비 싫어해서 초밥도 회도 안먹거든요 ㅎ",
          "original_form": "저는 와사비 싫어해서 초밥도 회도 안먹거든요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038216.1.19",
          "form": "시즈닝만 조금 되어있나보다 했는뎈ㅋㅋ",
          "original_form": "시즈닝만 조금 되어있나보다 했는뎈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038216.1.20",
          "form": "그러시면 놀라셨겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그러시면 놀라셨겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100038216.1.21",
          "form": "네 놀랐어요 ㅎㅎ",
          "original_form": "네 놀랐어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:02"
        }
      ]
