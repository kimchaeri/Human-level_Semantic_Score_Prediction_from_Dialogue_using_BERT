      "utterance": [
        {
          "id": "MDRW2100006282.1.1",
          "form": "나 내일 책 빌리러 갈 건데",
          "original_form": "나 내일 책 빌리러 갈 건데",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006282.1.2",
          "form": "엄마 뭐 보고 싶은 책 없어요",
          "original_form": "엄마 뭐 보고 싶은 책 없어요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006282.1.3",
          "form": "엄마 그러면은 인문학 하나만 빌려줘",
          "original_form": "엄마 그러면은 인문학 하나만 빌려줘",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006282.1.4",
          "form": "인문학 아무거나",
          "original_form": "인문학 아무거나",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006282.1.5",
          "form": "응 별로 두껍지 않은 걸로",
          "original_form": "응 별로 두껍지 않은 걸로",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006282.1.6",
          "form": "알았어요 인문학 중에 어떤 게 좋아요",
          "original_form": "알았어요 인문학 중에 어떤 게 좋아요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006282.1.7",
          "form": "심리나 상담",
          "original_form": "심리나 상담",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006282.1.8",
          "form": "오키 그쪽으로 찾아볼게요",
          "original_form": "오키 그쪽으로 찾아볼게요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.9",
          "form": "너는 책 뭐 빌릴려고",
          "original_form": "너는 책 뭐 빌릴려고",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.10",
          "form": "난 주식투자하는 거",
          "original_form": "난 주식투자하는 거",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.11",
          "form": "책으로도 공부하게",
          "original_form": "책으로도 공부하게",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.12",
          "form": "아무래도 책 한 건 쯤은 읽어야 될 것 같아요",
          "original_form": "아무래도 책 한 건 쯤은 읽어야 될 것 같아요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.13",
          "form": "책으로 쭉 보는 것도 좋은 방법이지",
          "original_form": "책으로 쭉 보는 것도 좋은 방법이지",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.14",
          "form": "최고로 한번 봐야 입력이 될 듯한 느낌이에요",
          "original_form": "최고로 한번 봐야 입력이 될 듯한 느낌이에요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.15",
          "form": "어렵지 않은 걸로 골라야겠네",
          "original_form": "어렵지 않은 걸로 골라야겠네",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.16",
          "form": "응 미리 찾아온 책 있어요",
          "original_form": "응 미리 찾아온 책 있어요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.17",
          "form": "몇 권 빌릴 건데",
          "original_form": "몇 권 빌릴 건데",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006282.1.18",
          "form": "일단 무리하지 않고 한 권",
          "original_form": "일단 무리하지 않고 한 권",
          "speaker_id": "1",
          "time": "20210907 22:28"
        }
