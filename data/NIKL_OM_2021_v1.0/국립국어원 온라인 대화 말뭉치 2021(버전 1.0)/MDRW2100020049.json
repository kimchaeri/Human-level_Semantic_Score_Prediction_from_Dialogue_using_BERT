          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020049.1.6",
          "form": "지역마다 다른가봐요",
          "original_form": "지역마다 다른가봐요",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020049.1.7",
          "form": "비오는날 좋아하세요?",
          "original_form": "비오는날 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020049.1.8",
          "form": "싫어요",
          "original_form": "싫어요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020049.1.9",
          "form": "ㅠㅠ얼른 비가 그쳐야할텐데",
          "original_form": "ㅠㅠ얼른 비가 그쳐야할텐데",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020049.1.10",
          "form": "습하지거 않거",
          "original_form": "습하지거 않거",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020049.1.11",
          "form": "맞아요 습한거 싫죠ㅠㅠ",
