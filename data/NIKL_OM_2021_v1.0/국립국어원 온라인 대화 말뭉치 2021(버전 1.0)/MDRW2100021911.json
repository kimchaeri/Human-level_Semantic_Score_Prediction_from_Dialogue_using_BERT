      "utterance": [
        {
          "id": "MDRW2100021911.1.1",
          "form": "믿고보는 서현진..",
          "original_form": "믿고보는 서현진..",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021911.1.2",
          "form": "그 황보통? 캐릭터도 짠하더라구요",
          "original_form": "그 황보통? 캐릭터도 짠하더라구요",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021911.1.3",
          "form": "제일 기억에 남아요",
          "original_form": "제일 기억에 남아요",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100021911.1.4",
          "form": "클립 말고 처음부터 제대로 봐보고싶은데",
          "original_form": "클립 말고 처음부터 제대로 봐보고싶은데",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100021911.1.5",
          "form": "티비엔이라...",
          "original_form": "티비엔이라...",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100021911.1.6",
          "form": "요즘 클립들도 워낙 잘 나와서..",
          "original_form": "요즘 클립들도 워낙 잘 나와서..",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021911.1.7",
          "form": "맞아요 !",
          "original_form": "맞아요 !",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021911.1.8",
          "form": "알짜배기 잘올리더라고요 ㅋㅋ",
          "original_form": "알짜배기 잘올리더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021911.1.9",
          "form": "클립 올라온 부분이 제일 재밌는 부분이잖아요ㅋㅋ",
          "original_form": "클립 올라온 부분이 제일 재밌는 부분이잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021911.1.10",
          "form": "그냥 킬링타임용으로 연애의참견도 종종 보는데",
          "original_form": "그냥 킬링타임용으로 연애의참견도 종종 보는데",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021911.1.11",
          "form": "아주 자극적으로.. 재밌게",
          "original_form": "아주 자극적으로.. 재밌게",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021911.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100021911.1.13",
          "form": "아저도 밥먹으면서 보는데",
          "original_form": "아저도 밥먹으면서 보는데",
          "speaker_id": "2",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021911.1.14",
          "form": "나쁜 말 하면서 보기 좋아요ㅋㅋㅋ",
          "original_form": "나쁜 말 하면서 보기 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021911.1.15",
          "form": "맞아요 세상 저런것들이 아직 살아있네..  하며",
          "original_form": "맞아요 세상 저런것들이 아직 살아있네..  하며",
          "speaker_id": "1",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021911.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021911.1.17",
          "form": "가끔 넘 주작스러운 것도 보이는데",
          "original_form": "가끔 넘 주작스러운 것도 보이는데",
          "speaker_id": "1",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021911.1.18",
          "form": "그래도 재밌어요",
          "original_form": "그래도 재밌어요",
          "speaker_id": "1",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021911.1.19",
          "form": "주작인 거 알아도 그런 맛에 보는거죠ㅋㅋㅋ",
          "original_form": "주작인 거 알아도 그런 맛에 보는거죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100021911.1.20",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021911.1.21",
          "form": "깊게 빠지지 않아도 되는 가벼운 맛..",
          "original_form": "깊게 빠지지 않아도 되는 가벼운 맛..",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021911.1.22",
          "form": "전에 우연히 아빠랑 보게된 적이 있는데",
          "original_form": "전에 우연히 아빠랑 보게된 적이 있는데",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021911.1.23",
          "form": "아빠가 왜 이상한 남자애들만 나오냐해서 넘 웃겼어요",
          "original_form": "아빠가 왜 이상한 남자애들만 나오냐해서 넘 웃겼어요",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021911.1.24",
          "form": "제가 이상한 남자애들이 많아서 그러겠지~~했습니당ㅋㅋㅋㅋ",
          "original_form": "제가 이상한 남자애들이 많아서 그러겠지~~했습니당ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021911.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021911.1.26",
          "form": "그러게여... 왜 이상한 남자애들만 나올까료.....",
          "original_form": "그러게여... 왜 이상한 남자애들만 나올까료.....",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021911.1.27",
          "form": "주로 친구분들이랑 취향이 잘 맞으신가요?",
          "original_form": "주로 친구분들이랑 취향이 잘 맞으신가요?",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021911.1.28",
          "form": "음 어떤부분에서요?",
          "original_form": "음 어떤부분에서요?",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021911.1.29",
          "form": "작품취향?",
          "original_form": "작품취향?",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021911.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021911.1.31",
          "form": "작품 취향요",
          "original_form": "작품 취향요",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021911.1.32",
          "form": "음 맞는 친구들도 있는데",
          "original_form": "음 맞는 친구들도 있는데",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021911.1.33",
          "form": "많진 않은거같아요",
          "original_form": "많진 않은거같아요",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021911.1.34",
          "form": "혹시 검블유라는 드라마 보셨나요?",
          "original_form": "혹시 검블유라는 드라마 보셨나요?",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021911.1.35",
          "form": "그거 보고싶었는데......",
          "original_form": "그거 보고싶었는데......",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021911.1.36",
          "form": "드라마 챙겨보는걸 잘 못해서",
          "original_form": "드라마 챙겨보는걸 잘 못해서",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021911.1.37",
          "form": "ㅡㄱ것도 클립으로만 조금 봤어요..",
          "original_form": "ㅡㄱ것도 클립으로만 조금 봤어요..",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021911.1.38",
          "form": "ㅎㅎ 그 드라마 주인공커플 중에 여자가 비혼주의자고 남자는 결혼하고 싶어하는데",
          "original_form": "ㅎㅎ 그 드라마 주인공커플 중에 여자가 비혼주의자고 남자는 결혼하고 싶어하는데",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021911.1.39",
          "form": "비혼주의인거 알고 사귀고도 남자가 계속 불편한 상황 만들어서 자주 싸우는 장면이 나오거든요",
          "original_form": "비혼주의인거 알고 사귀고도 남자가 계속 불편한 상황 만들어서 자주 싸우는 장면이 나오거든요",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021911.1.40",
          "form": "음 임수정이랑 장기용인가요?",
          "original_form": "음 임수정이랑 장기용인가요?",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021911.1.41",
          "form": "그 드라마보고 회사 친한 또래 여자 직원 넷이서 이야기 했는데",
          "original_form": "그 드라마보고 회사 친한 또래 여자 직원 넷이서 이야기 했는데",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021911.1.42",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021911.1.43",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021911.1.44",
          "form": "둘은 남자가 잘못이다 둘은 남자 이해된다로 나뉘어서 놀랐어요",
          "original_form": "둘은 남자가 잘못이다 둘은 남자 이해된다로 나뉘어서 놀랐어요",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021911.1.45",
          "form": "또래끼리도 이렇게 생각이 다르더라구요",
          "original_form": "또래끼리도 이렇게 생각이 다르더라구요",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021911.1.46",
          "form": "흠...ㅠ",
          "original_form": "흠...ㅠ",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021911.1.47",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021911.1.48",
          "form": "친구들이랑 대화하다보면 불편한 구석이 나오기도 하는데 하나하나 이야기하기도 힘들구요",
          "original_form": "친구들이랑 대화하다보면 불편한 구석이 나오기도 하는데 하나하나 이야기하기도 힘들구요",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
