      "utterance": [
        {
          "id": "MDRW2100045538.1.1",
          "form": "근데 11시니까",
          "original_form": "근데 11시니까",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045538.1.2",
          "form": "버스에 사람 별로 없을걸",
          "original_form": "버스에 사람 별로 없을걸",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045538.1.3",
          "form": "글킨 하겠네",
          "original_form": "글킨 하겠네",
          "speaker_id": "1",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045538.1.4",
          "form": "앉아갈수잇을거야",
          "original_form": "앉아갈수잇을거야",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045538.1.5",
          "form": "딱 애매한 시간이라....",
          "original_form": "딱 애매한 시간이라....",
          "speaker_id": "1",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045538.1.6",
          "form": "ㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045538.1.7",
          "form": "컨디션을 위해....",
          "original_form": "컨디션을 위해....",
          "speaker_id": "1",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045538.1.8",
          "form": "ㅇㅇ금강 막기는 안되고",
          "original_form": "ㅇㅇ금강 막기는 안되고",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045538.1.9",
          "form": "넥스트레벨은 된다더라",
          "original_form": "넥스트레벨은 된다더라",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045538.1.10",
          "form": "그거 되고 안된다고",
          "original_form": "그거 되고 안된다고",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.11",
          "form": "무슨의미가 있는지는",
          "original_form": "무슨의미가 있는지는",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.12",
          "form": "모르겠지만",
          "original_form": "모르겠지만",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.13",
          "form": "이제 슬슬 눈이 감긴다",
          "original_form": "이제 슬슬 눈이 감긴다",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.14",
          "form": "혹시ㄴㅐ가 사라지면",
          "original_form": "혹시ㄴㅐ가 사라지면",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ22",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ22",
          "speaker_id": "1",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.16",
          "form": "기절잠이라고",
          "original_form": "기절잠이라고",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.17",
          "form": "생각해줘...",
          "original_form": "생각해줘...",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.18",
          "form": "안대",
          "original_form": "안대",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.19",
          "form": "내가 이만큼 이었는데",
          "original_form": "내가 이만큼 이었는데",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045538.1.20",
          "form": "다 채우고자야대",
          "original_form": "다 채우고자야대",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045538.1.21",
          "form": "언니는",
          "original_form": "언니는",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045538.1.22",
          "form": "ㅋㅋㅋㅋ 알앗어....",
          "original_form": "ㅋㅋㅋㅋ 알앗어....",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045538.1.23",
          "form": "나는 아직도 택도 업다구",
          "original_form": "나는 아직도 택도 업다구",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045538.1.24",
          "form": "언니가 해야지",
          "original_form": "언니가 해야지",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045538.1.25",
          "form": "여튼",
          "original_form": "여튼",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045538.1.26",
          "form": "대충 한시간은 지나지 않았을까",
          "original_form": "대충 한시간은 지나지 않았을까",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045538.1.27",
          "form": "슬슬 누워도",
          "original_form": "슬슬 누워도",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045538.1.28",
          "form": "역류 안하지 않을까",
          "original_form": "역류 안하지 않을까",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045538.1.29",
          "form": "ㅇㅇ 한시간은 지났을듯...",
          "original_form": "ㅇㅇ 한시간은 지났을듯...",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045538.1.30",
          "form": "나 누웠다",
          "original_form": "나 누웠다",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045538.1.31",
          "form": "이제 진짜 기절잠 잘수도 있어",
          "original_form": "이제 진짜 기절잠 잘수도 있어",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045538.1.32",
          "form": "ㅇㅇ 잘자....",
          "original_form": "ㅇㅇ 잘자....",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045538.1.33",
          "form": "숙면은 건강에 도움이 돼....",
          "original_form": "숙면은 건강에 도움이 돼....",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045538.1.34",
          "form": "아냐 아직 안잘거야",
          "original_form": "아냐 아직 안잘거야",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045538.1.35",
          "form": "이 대화까진 끝내고 자야지",
          "original_form": "이 대화까진 끝내고 자야지",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045538.1.36",
          "form": "내가 일어나면 이제",
          "original_form": "내가 일어나면 이제",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045538.1.37",
          "form": "끝나있을텐데",
          "original_form": "끝나있을텐데",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045538.1.38",
          "form": "아깝잔아",
          "original_form": "아깝잔아",
          "speaker_id": "2",
          "time": "20210908 03:22"
        }
      ]
    }
