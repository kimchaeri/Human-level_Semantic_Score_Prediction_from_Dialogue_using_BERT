          "speaker_id": "2",
          "time": "20210829 10:45"
        },
        {
          "id": "MDRW2100022189.1.3",
          "form": "지금 연애중이신가요?",
          "original_form": "지금 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210829 10:45"
        },
        {
          "id": "MDRW2100022189.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.5",
          "form": "연애중이신가요",
          "original_form": "연애중이신가요",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.6",
          "form": "저도 아니요~",
          "original_form": "저도 아니요~",
          "speaker_id": "1",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.7",
          "form": "어..",
          "original_form": "어..",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.8",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.9",
          "form": "그럼..",
          "original_form": "그럼..",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.10",
          "form": "결혼하신건 아니겠죠?!",
          "original_form": "결혼하신건 아니겠죠?!",
          "speaker_id": "1",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.11",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "1",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.12",
          "form": "아닙니다",
          "original_form": "아닙니다",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.13",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022189.1.14",
          "form": "이상형이라..",
          "original_form": "이상형이라..",
          "speaker_id": "2",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022189.1.15",
          "form": "딱히 정해두진 않았고 그냥 서로 대화가 잘 되고.. 어.. 어렵네요ㅎ",
          "original_form": "딱히 정해두진 않았고 그냥 서로 대화가 잘 되고.. 어.. 어렵네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022189.1.16",
          "form": "name1님은 어떠신가요",
          "original_form": "&name1&님은 어떠신가요",
          "speaker_id": "2",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022189.1.17",
          "form": "그런거 있잖아요",
          "original_form": "그런거 있잖아요",
          "speaker_id": "1",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022189.1.18",
          "form": "이거하나는 꼭 본다 싶은거",
          "original_form": "이거하나는 꼭 본다 싶은거",
          "speaker_id": "1",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022189.1.19",
          "form": "name1님은 있으신가요",
          "original_form": "&name1&님은 있으신가요",
          "speaker_id": "2",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022189.1.20",
          "form": "음.. 외관은 잘 안봐서..",
          "original_form": "음.. 외관은 잘 안봐서..",
          "speaker_id": "2",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100022189.1.21",
          "form": "저는 잘 모르겠는데",
          "original_form": "저는 잘 모르겠는데",
          "speaker_id": "1",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022189.1.22",
          "form": "주변에서 하는 말이",
          "original_form": "주변에서 하는 말이",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022189.1.23",
          "form": "제가 날라리상을 좋아한대요ㅋㅋㅋ",
          "original_form": "제가 날라리상을 좋아한대요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022189.1.24",
          "form": "인정할 순 없지만",
          "original_form": "인정할 순 없지만",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022189.1.25",
          "form": "앗 그러시군요",
