      "utterance": [
        {
          "id": "MDRW2100037110.1.1",
          "form": "감사함닷",
          "original_form": "감사함닷",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037110.1.2",
          "form": "식사 하셨을까요?!",
          "original_form": "식사 하셨을까요?!",
          "speaker_id": "1",
          "time": "20210831 19:53"
        },
        {
          "id": "MDRW2100037110.1.3",
          "form": "아 네 저녁 먹었어요 name1님은 저녁 드셨나요?",
          "original_form": "아 네 저녁 먹었어요 &name1&님은 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 20:01"
        },
        {
          "id": "MDRW2100037110.1.4",
          "form": "넵! 너무 먹어서 소화가안되네욥ㅜㅠ",
          "original_form": "넵! 너무 먹어서 소화가안되네욥ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100037110.1.5",
          "form": "맛있는 저녁을 드셨나봐요?",
          "original_form": "맛있는 저녁을 드셨나봐요?",
          "speaker_id": "2",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100037110.1.6",
          "form": "넵! 김치전에 된장국 먹었습니당!",
          "original_form": "넵! 김치전에 된장국 먹었습니당!",
          "speaker_id": "1",
          "time": "20210831 20:08"
        },
        {
          "id": "MDRW2100037110.1.7",
          "form": "김치전 지금 비가와서 넘 먹고 싶네요",
          "original_form": "김치전 지금 비가와서 넘 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100037110.1.8",
          "form": "비오는날에 딱이긴 하죠!",
          "original_form": "비오는날에 딱이긴 하죠!",
          "speaker_id": "1",
          "time": "20210831 20:12"
        },
        {
          "id": "MDRW2100037110.1.9",
          "form": "김치전 좋아하세요?",
          "original_form": "김치전 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 20:13"
        },
        {
          "id": "MDRW2100037110.1.10",
          "form": "네 김치 좋아해서 김치전도 김치볶음밥도 좋아해요",
          "original_form": "네 김치 좋아해서 김치전도 김치볶음밥도 좋아해요",
          "speaker_id": "2",
          "time": "20210831 20:13"
        },
        {
          "id": "MDRW2100037110.1.11",
          "form": "김치와 밥만 있음 드시는 스탈이신가욥?",
          "original_form": "김치와 밥만 있음 드시는 스탈이신가욥?",
          "speaker_id": "1",
          "time": "20210831 20:18"
        },
        {
          "id": "MDRW2100037110.1.12",
          "form": "아직 그 경지에 이르지 못하였습니다ㅋㅋ 아 겉절이는 제외구요",
          "original_form": "아직 그 경지에 이르지 못하였습니다ㅋㅋ 아 겉절이는 제외구요",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100037110.1.13",
          "form": "오늘 반찬가게 갔는데 겉절이가 완판 됐더라고요",
          "original_form": "오늘 반찬가게 갔는데 겉절이가 완판 됐더라고요",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100037110.1.14",
          "form": "대박 그 집 겉절이가 엄청 맛있는가봐요",
          "original_form": "대박 그 집 겉절이가 엄청 맛있는가봐요",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037110.1.15",
          "form": "겉절이 맛보고 싶더라고요  새로 생겼는데 인기 있어요 맛도 좋고 친절하세요",
          "original_form": "겉절이 맛보고 싶더라고요  새로 생겼는데 인기 있어요 맛도 좋고 친절하세요",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037110.1.16",
          "form": "새로 생긴곳이 벌써 입소문 생겼나보네용",
          "original_form": "새로 생긴곳이 벌써 입소문 생겼나보네용",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037110.1.17",
          "form": "근처에 좋은곳 생겨서 부럽네요",
          "original_form": "근처에 좋은곳 생겨서 부럽네요",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037110.1.18",
          "form": "근처는 아니라서 발품을 조금 팔아야 되요ㅠ",
          "original_form": "근처는 아니라서 발품을 조금 팔아야 되요ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037110.1.19",
          "form": "앗..ㅜㅠ 근처가 아니셨군요ㅠ",
          "original_form": "앗..ㅜㅠ 근처가 아니셨군요ㅠ",
