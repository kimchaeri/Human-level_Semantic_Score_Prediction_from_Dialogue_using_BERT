      "utterance": [
        {
          "id": "MDRW2100010451.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010451.1.2",
          "form": "추리소설 읽다보시면 트릭도 간파하고 그러시나여?",
          "original_form": "추리소설 읽다보시면 트릭도 간파하고 그러시나여?",
          "speaker_id": "1",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010451.1.3",
          "form": "코난 팬들은 관상으로 범인 맞추고 그러던데",
          "original_form": "코난 팬들은 관상으로 범인 맞추고 그러던데",
          "speaker_id": "1",
          "time": "20210823 18:28"
        },
        {
          "id": "MDRW2100010451.1.4",
          "form": "코난은 가능한데 종이책은 확률이 그렇게 좋지 않았어요ㅋㅋㅋ",
          "original_form": "코난은 가능한데 종이책은 확률이 그렇게 좋지 않았어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:28"
        },
        {
          "id": "MDRW2100010451.1.5",
          "form": "특히 히가시노 게이고는 단순한 수수께끼 트릭보다는 전문 지식이 필요한 트릭들도 많았거든요",
          "original_form": "특히 히가시노 게이고는 단순한 수수께끼 트릭보다는 전문 지식이 필요한 트릭들도 많았거든요",
          "speaker_id": "2",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100010451.1.6",
          "form": "전문 지식이 담긴 책들이 많아요 그래서 흥미롭긴 한데 가끔 지루할 때도 있어요ㅋㅋㅋ",
          "original_form": "전문 지식이 담긴 책들이 많아요 그래서 흥미롭긴 한데 가끔 지루할 때도 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100010451.1.7",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010451.1.8",
          "form": "근데 보다보면 진짜",
          "original_form": "근데 보다보면 진짜",
          "speaker_id": "1",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010451.1.9",
          "form": "지식잌ㅋㅋㅋㅋㅋ",
          "original_form": "지식잌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010451.1.10",
          "form": "뭐랄까 지식 바운더리가 진짜 넓어지겠어요",
          "original_form": "뭐랄까 지식 바운더리가 진짜 넓어지겠어요",
          "speaker_id": "1",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010451.1.11",
          "form": "어 이거 누구 살해 트릭으로 나온건데 함서",
          "original_form": "어 이거 누구 살해 트릭으로 나온건데 함서",
          "speaker_id": "1",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010451.1.12",
          "form": "비행기 구조 같은 일상적이지 않은 지식이긴 해요...",
          "original_form": "비행기 구조 같은 일상적이지 않은 지식이긴 해요...",
          "speaker_id": "2",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100010451.1.13",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100010451.1.14",
          "form": "사실 볼 때도 너무 지루하면 좀 넘겨서 보느라 거의 안 남아요ㅋㅋㅋㅋ",
          "original_form": "사실 볼 때도 너무 지루하면 좀 넘겨서 보느라 거의 안 남아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100010451.1.15",
          "form": "알아두면 좋지만",
          "original_form": "알아두면 좋지만",
          "speaker_id": "1",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100010451.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100010451.1.17",
          "form": "자랑할 곳은 없는",
          "original_form": "자랑할 곳은 없는",
          "speaker_id": "1",
          "time": "20210823 18:32"
        },
        {
          "id": "MDRW2100010451.1.18",
          "form": "웹소설은 어떤 장르 많이 보세요?",
          "original_form": "웹소설은 어떤 장르 많이 보세요?",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010451.1.19",
          "form": "아까 웹소는 로판이랑 판타지 본다고 하셨는데",
          "original_form": "아까 웹소는 로판이랑 판타지 본다고 하셨는데",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010451.1.20",
          "form": "아 네네",
          "original_form": "아 네네",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010451.1.21",
          "form": "그것도 혹시 추리 많이보시나요",
          "original_form": "그것도 혹시 추리 많이보시나요",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010451.1.22",
          "form": "저도 로판이랑 판타지 봐요!!",
          "original_form": "저도 로판이랑 판타지 봐요!!",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010451.1.23",
          "form": "추리요소가 들어가여",
          "original_form": "추리요소가 들어가여",
          "speaker_id": "1",
          "time": "20210823 18:33"
        }
      ]
    }
