      "utterance": [
        {
          "id": "MDRW2100008062.1.1",
          "form": "집에 반찬을 안해서",
          "original_form": "집에 반찬을 안해서",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008062.1.2",
          "form": "나는 먹을거 안떠오르면 면 중에 골라",
          "original_form": "나는 먹을거 안떠오르면 면 중에 골라",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008062.1.3",
          "form": "먹을게 없네",
          "original_form": "먹을게 없네",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008062.1.4",
          "form": "오 라면 먹을까",
          "original_form": "오 라면 먹을까",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.5",
          "form": "좀불량하지 ㅋㅌ",
          "original_form": "좀불량하지 ㅋㅌ",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.6",
          "form": "여름엔 비빔면",
          "original_form": "여름엔 비빔면",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.7",
          "form": "라면",
          "original_form": "라면",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.8",
          "form": "짜파구리",
          "original_form": "짜파구리",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.9",
          "form": "국수나 파스타 해야겠다",
          "original_form": "국수나 파스타 해야겠다",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.10",
          "form": "맞아 비빔면이지",
          "original_form": "맞아 비빔면이지",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.11",
          "form": "열무 넣어야 하는데 없네 ㅜㅜ",
          "original_form": "열무 넣어야 하는데 없네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.12",
          "form": "뭐 국수 완전 좋구ㅋㅋㅋ파스타도ㅋㅋ면만 삶으면 되구",
          "original_form": "뭐 국수 완전 좋구ㅋㅋㅋ파스타도ㅋㅋ면만 삶으면 되구",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.13",
          "form": "열무는 아쉽게 먹어도 대지뭐ㅋㅋㅋㅋ",
          "original_form": "열무는 아쉽게 먹어도 대지뭐ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008062.1.14",
          "form": "올해는 냉육수 사와서 열무 국스도 해먹고",
          "original_form": "올해는 냉육수 사와서 열무 국스도 해먹고",
          "speaker_id": "2",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008062.1.15",
          "form": "묵국수 뭐 국수 종류 돌려서 다 해먹어봤어ㅋㅋ",
          "original_form": "묵국수 뭐 국수 종류 돌려서 다 해먹어봤어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008062.1.16",
          "form": "냉육수 좋다",
          "original_form": "냉육수 좋다",
          "speaker_id": "1",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008062.1.17",
          "form": "그 모밀도!!!",
          "original_form": "그 모밀도!!!",
          "speaker_id": "2",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008062.1.18",
          "form": "묵국수는 안먹어봤어",
          "original_form": "묵국수는 안먹어봤어",
          "speaker_id": "1",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008062.1.19",
          "form": "완전 맛있게 나오더라ㅋㅋㅋㅋㅋ패키지로 나오는거",
          "original_form": "완전 맛있게 나오더라ㅋㅋㅋㅋㅋ패키지로 나오는거",
          "speaker_id": "2",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008062.1.20",
          "form": "모밀이 짱이지",
          "original_form": "모밀이 짱이지",
          "speaker_id": "1",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008062.1.21",
          "form": "아 모밀 할까",
          "original_form": "아 모밀 할까",
          "speaker_id": "1",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008062.1.22",
          "form": "오 패키지좋다",
          "original_form": "오 패키지좋다",
          "speaker_id": "1",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008062.1.23",
          "form": "좋는 세상이다",
          "original_form": "좋는 세상이다",
          "speaker_id": "1",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008062.1.24",
          "form": "거기에 집에 있는 만두 소비해주면ㅋㅋㅋㅋ고기만두랑 찰떡이야",
          "original_form": "거기에 집에 있는 만두 소비해주면ㅋㅋㅋㅋ고기만두랑 찰떡이야",
          "speaker_id": "2",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008062.1.25",
          "form": "아 만두 딱이다",
          "original_form": "아 만두 딱이다",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008062.1.26",
          "form": "뭘아네",
          "original_form": "뭘아네",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008062.1.27",
          "form": "울딸 잔치국수 좋아해",
          "original_form": "울딸 잔치국수 좋아해",
          "speaker_id": "1",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008062.1.28",
          "form": "요새 재택하면서 메뉴 선정이 큰 고민거리거든",
          "original_form": "요새 재택하면서 메뉴 선정이 큰 고민거리거든",
          "speaker_id": "2",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008062.1.29",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008062.1.30",
          "form": "매번 끼니 챙기기 넘 힘들어",
          "original_form": "매번 끼니 챙기기 넘 힘들어",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008062.1.31",
          "form": "누가 좀 정해줌 좋겠다",
          "original_form": "누가 좀 정해줌 좋겠다",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008062.1.32",
          "form": "약간 가족구성원이나 내가 먹고 있는 음식 반영해서",
          "original_form": "약간 가족구성원이나 내가 먹고 있는 음식 반영해서",
          "speaker_id": "2",
          "time": "20210822 13:08"
        }
      ]
    }
