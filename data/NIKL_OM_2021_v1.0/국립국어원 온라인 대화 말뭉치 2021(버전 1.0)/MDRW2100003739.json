        {
          "id": "MDRW2100003739.1.2",
          "form": "그래 맛있겠네",
          "original_form": "그래 맛있겠네",
          "speaker_id": "1",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003739.1.3",
          "form": "또 뭐 해 줄까",
          "original_form": "또 뭐 해 줄까",
          "speaker_id": "2",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003739.1.4",
          "form": "음.. 머 먹지 ㅎㅎ",
          "original_form": "음.. 머 먹지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003739.1.5",
          "form": "계란 후라이 해서 같이 먹자",
          "original_form": "계란 후라이 해서 같이 먹자",
          "speaker_id": "2",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003739.1.6",
          "form": "그래용 넘 맛있겠네~",
          "original_form": "그래용 넘 맛있겠네~",
          "speaker_id": "1",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003739.1.7",
          "form": "집에 반찬도 많다",
          "original_form": "집에 반찬도 많다",
          "speaker_id": "2",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003739.1.8",
          "form": "구래 ㅋㅋ 집밥이 최고다",
          "original_form": "구래 ㅋㅋ 집밥이 최고다",
          "speaker_id": "1",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003739.1.9",
          "form": "당연하지 사 먹는 건 안 좋다",
          "original_form": "당연하지 사 먹는 건 안 좋다",
          "speaker_id": "2",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003739.1.10",
          "form": "맞다 엄마가 해주는게 맛도 젤좋고",
          "original_form": "맞다 엄마가 해주는게 맛도 젤좋고",
          "speaker_id": "1",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003739.1.11",
          "form": "그래 잘 먹어주니 좋군",
          "original_form": "그래 잘 먹어주니 좋군",
          "speaker_id": "2",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003739.1.12",
          "form": "ㅋㅋ 맛있으니까 잘 먹지요!",
          "original_form": "ㅋㅋ 맛있으니까 잘 먹지요!",
          "speaker_id": "1",
