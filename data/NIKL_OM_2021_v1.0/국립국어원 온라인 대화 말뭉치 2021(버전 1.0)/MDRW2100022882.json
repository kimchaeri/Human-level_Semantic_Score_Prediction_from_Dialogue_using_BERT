      "utterance": [
        {
          "id": "MDRW2100022882.1.1",
          "form": "동기가없어",
          "original_form": "동기가없어",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022882.1.2",
          "form": "동기가 왜 없어?",
          "original_form": "동기가 왜 없어?",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022882.1.3",
          "form": "섬이라가지고 그런가봐 나혼자야 중대에",
          "original_form": "섬이라가지고 그런가봐 나혼자야 중대에",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022882.1.4",
          "form": "넌 군필이야?",
          "original_form": "넌 군필이야?",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022882.1.5",
          "form": "정말로?",
          "original_form": "정말로?",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022882.1.6",
          "form": "나 여자라고",
          "original_form": "나 여자라고",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022882.1.7",
          "form": "정말이지",
          "original_form": "정말이지",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022882.1.8",
          "form": "그럼 선임이나 후임은?",
          "original_form": "그럼 선임이나 후임은?",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022882.1.9",
          "form": "있지요",
          "original_form": "있지요",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022882.1.10",
          "form": "넌 군인 어떻게 생각해",
          "original_form": "넌 군인 어떻게 생각해",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022882.1.11",
          "form": "친해?",
          "original_form": "친해?",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022882.1.12",
          "form": "친해친해",
          "original_form": "친해친해",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022882.1.13",
          "form": "쉴때 선후임들이랑 뭐해?",
          "original_form": "쉴때 선후임들이랑 뭐해?",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022882.1.14",
          "form": "랜덤게임할때도 있고 노래들을때 있고 이것저것?",
          "original_form": "랜덤게임할때도 있고 노래들을때 있고 이것저것?",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022882.1.15",
          "form": "랜덤게임이 뭐야?",
          "original_form": "랜덤게임이 뭐야?",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022882.1.16",
          "form": "술게임? 가튼거",
          "original_form": "술게임? 가튼거",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022882.1.17",
          "form": "아하 노래방이나 피씨방은 있어?",
          "original_form": "아하 노래방이나 피씨방은 있어?",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022882.1.18",
          "form": "노래방은 있어",
          "original_form": "노래방은 있어",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022882.1.19",
          "form": "1개",
          "original_form": "1개",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022882.1.20",
          "form": "부스가 1개야?",
          "original_form": "부스가 1개야?",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022882.1.21",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022882.1.22",
          "form": "그러면 노래 많이 못하지 않아?",
          "original_form": "그러면 노래 많이 못하지 않아?",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022882.1.23",
          "form": "중대는 파견까지 2개급인데",
          "original_form": "중대는 파견까지 2개급인데",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022882.1.24",
          "form": "파견애들이 많이써",
          "original_form": "파견애들이 많이써",
          "speaker_id": "2",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022882.1.25",
          "form": "자기네들은 없었나",
          "original_form": "자기네들은 없었나",
          "speaker_id": "2",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022882.1.26",
          "form": "제한 시간이 있어?",
          "original_form": "제한 시간이 있어?",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022882.1.27",
          "form": "제한시간은 없고",
          "original_form": "제한시간은 없고",
          "speaker_id": "2",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022882.1.28",
          "form": "천원에 5곡",
          "original_form": "천원에 5곡",
          "speaker_id": "2",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022882.1.29",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022882.1.30",
          "form": "너도 한번와바!",
          "original_form": "너도 한번와바!",
          "speaker_id": "2",
          "time": "20210829 18:54"
        }
      ]
    }
