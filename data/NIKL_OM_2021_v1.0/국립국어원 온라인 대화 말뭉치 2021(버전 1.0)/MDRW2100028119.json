      "utterance": [
        {
          "id": "MDRW2100028119.1.1",
          "form": "요기요에서 도미노피자할인하네요",
          "original_form": "요기요에서 도미노피자할인하네요",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028119.1.2",
          "form": "헐 얼마해주나요",
          "original_form": "헐 얼마해주나요",
          "speaker_id": "1",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028119.1.3",
          "form": "만원할인이요",
          "original_form": "만원할인이요",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028119.1.4",
          "form": "감자 피자 맛잇던데 요??",
          "original_form": "감자 피자 맛잇던데 요??",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028119.1.5",
          "form": "맞아요 먹고싶은데ㅜ 치킨은 뭐드시고싶으세요?",
          "original_form": "맞아요 먹고싶은데ㅜ 치킨은 뭐드시고싶으세요?",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028119.1.6",
          "form": "땅땅치킨 아시나요",
          "original_form": "땅땅치킨 아시나요",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028119.1.7",
          "form": "네 알아요",
          "original_form": "네 알아요",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028119.1.8",
          "form": "거기 3번세트",
          "original_form": "거기 3번세트",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028119.1.9",
          "form": "짱이에요",
          "original_form": "짱이에요",
          "speaker_id": "1",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100028119.1.10",
          "form": "그래요? 3번세트 구성이 어떻게되는데요?",
          "original_form": "그래요? 3번세트 구성이 어떻게되는데요?",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028119.1.11",
          "form": "오븐치킨 간장치킨이에요",
          "original_form": "오븐치킨 간장치킨이에요",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028119.1.12",
          "form": "맛있겠네요 배고파요",
          "original_form": "맛있겠네요 배고파요",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028119.1.13",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028119.1.14",
          "form": "시킬까요?",
          "original_form": "시킬까요?",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028119.1.15",
          "form": "네 그러세요 맛있을거같아요 전 피자시키고싶은데 안되겠네요ㅜ",
          "original_form": "네 그러세요 맛있을거같아요 전 피자시키고싶은데 안되겠네요ㅜ",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028119.1.16",
          "form": "왜요 뭐먹을려구요",
          "original_form": "왜요 뭐먹을려구요",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028119.1.17",
          "form": "그냥 밥먹어야겠네요 피자가게가 주문을못받네요",
          "original_form": "그냥 밥먹어야겠네요 피자가게가 주문을못받네요",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028119.1.18",
          "form": "아 주문밀렷군요",
          "original_form": "아 주문밀렷군요",
          "speaker_id": "1",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100028119.1.19",
          "form": "여기는 싼피자 없앴어요 ㅡㅡ",
          "original_form": "여기는 싼피자 없앴어요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100028119.1.20",
          "form": "감자피자 없어요",
          "original_form": "감자피자 없어요",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028119.1.21",
          "form": "그래요?너무하네요ㅜ",
          "original_form": "그래요?너무하네요ㅜ",
