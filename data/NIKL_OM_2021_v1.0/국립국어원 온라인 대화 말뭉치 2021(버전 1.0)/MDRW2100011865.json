      "utterance": [
        {
          "id": "MDRW2100011865.1.1",
          "form": "이제 곧 저녁인데 뭐드실거애요?",
          "original_form": "이제 곧 저녁인데 뭐드실거애요?",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011865.1.2",
          "form": "스파게티 해먹을까 생각중이에요",
          "original_form": "스파게티 해먹을까 생각중이에요",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011865.1.3",
          "form": "생각해놓은거 있으신가요??",
          "original_form": "생각해놓은거 있으신가요??",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011865.1.4",
          "form": "저는 간단하게 김밥을 먹을까 고민이에요",
          "original_form": "저는 간단하게 김밥을 먹을까 고민이에요",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011865.1.5",
          "form": "비도 와서 국물이 땡기네요",
          "original_form": "비도 와서 국물이 땡기네요",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100011865.1.6",
          "form": "김밥 맛있겠네요",
          "original_form": "김밥 맛있겠네요",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100011865.1.7",
          "form": "라면이랑 드시나요?",
          "original_form": "라면이랑 드시나요?",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100011865.1.8",
          "form": "라면은 안좋아해서 뭘 먹어야 할지 모르겠어요",
          "original_form": "라면은 안좋아해서 뭘 먹어야 할지 모르겠어요",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100011865.1.9",
          "form": "스파게트는 직접 하시는건가요?",
          "original_form": "스파게트는 직접 하시는건가요?",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100011865.1.10",
          "form": "우동은 어떠신가요",
          "original_form": "우동은 어떠신가요",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011865.1.11",
          "form": "네 그냥 시판 소스 써서",
          "original_form": "네 그냥 시판 소스 써서",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011865.1.12",
          "form": "간단하게 하려구요",
          "original_form": "간단하게 하려구요",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011865.1.13",
          "form": "뜨끈하면서도 면종류 나쁘지 않죠",
          "original_form": "뜨끈하면서도 면종류 나쁘지 않죠",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011865.1.14",
          "form": "점심은 뭐 드셨나요??",
          "original_form": "점심은 뭐 드셨나요??",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011865.1.15",
          "form": "점심은 자장면 먹었어요 ㅎㅎ 요즘 입맛이 없어서 큰일이네요",
          "original_form": "점심은 자장면 먹었어요 ㅎㅎ 요즘 입맛이 없어서 큰일이네요",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011865.1.16",
          "form": "해먹기도 귀찮고 배달음식만 먹는거같고",
          "original_form": "해먹기도 귀찮고 배달음식만 먹는거같고",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011865.1.17",
          "form": "입맛없을땐 뭐 먹어야할까요? ㅠ",
          "original_form": "입맛없을땐 뭐 먹어야할까요? ㅠ",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011865.1.18",
          "form": "저는.. 좀 시원한걸 먹는 편이에요",
          "original_form": "저는.. 좀 시원한걸 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011865.1.19",
          "form": "냉면이나 소바같은거..",
          "original_form": "냉면이나 소바같은거..",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011865.1.20",
          "form": "요즘엔 좀 시원해서 괜찮은데 더우면 입맛이 없더라구요",
          "original_form": "요즘엔 좀 시원해서 괜찮은데 더우면 입맛이 없더라구요",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011865.1.21",
          "form": "냉면도 먹고싶은데 안땡기는거같아요. 빵을 먹어야 하나",
          "original_form": "냉면도 먹고싶은데 안땡기는거같아요. 빵을 먹어야 하나",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011865.1.22",
          "form": "입맛 없으면 서브웨이도 자주 먹는데",
          "original_form": "입맛 없으면 서브웨이도 자주 먹는데",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100011865.1.23",
          "form": "서브웨이도 별로이신가요?",
          "original_form": "서브웨이도 별로이신가요?",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100011865.1.24",
          "form": "요즘 먹고싶은게 없네요  고기나 먹을까봐요 ㅋㅋ",
          "original_form": "요즘 먹고싶은게 없네요  고기나 먹을까봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100011865.1.25",
          "form": "아 고기 좋죠",
          "original_form": "아 고기 좋죠",
          "speaker_id": "2",
          "time": "20210824 17:09"
        }
      ]
    }
  ]
