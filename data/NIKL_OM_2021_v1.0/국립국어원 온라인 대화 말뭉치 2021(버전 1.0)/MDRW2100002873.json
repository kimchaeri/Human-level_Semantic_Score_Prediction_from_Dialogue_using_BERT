      "utterance": [
        {
          "id": "MDRW2100002873.1.1",
          "form": "혹시 종교있으세여?",
          "original_form": "혹시 종교있으세여?",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002873.1.2",
          "form": "저는 무교입니다",
          "original_form": "저는 무교입니다",
          "speaker_id": "2",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002873.1.3",
          "form": "종교가 있으신가요?",
          "original_form": "종교가 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002873.1.4",
          "form": "저도 무교요ㅋㅋㅋㅋㅋ",
          "original_form": "저도 무교요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002873.1.5",
          "form": "주변엔 불교가 많네요",
          "original_form": "주변엔 불교가 많네요",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002873.1.6",
          "form": "지원님은 ?",
          "original_form": "지원님은 ?",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002873.1.7",
          "form": "전 기독교가 많았어요. 할머니가 교회다니셨거든요..",
          "original_form": "전 기독교가 많았어요. 할머니가 교회다니셨거든요..",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002873.1.8",
          "form": "사실 신이라는 존재를 믿지는 않아서..",
          "original_form": "사실 신이라는 존재를 믿지는 않아서..",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002873.1.9",
          "form": "저두여 ㅋㅋ",
          "original_form": "저두여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002873.1.10",
          "form": "어릴땐 국수먹으러 불교",
          "original_form": "어릴땐 국수먹으러 불교",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002873.1.11",
          "form": "사탕받으러 교회",
          "original_form": "사탕받으러 교회",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002873.1.12",
          "form": "갔었는데 ㅋㅋ",
          "original_form": "갔었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002873.1.13",
          "form": "ㅋㅋㅋㅋㅋ 다들 비슷한거같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 다들 비슷한거같아요",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002873.1.14",
          "form": "선물받으러 먹을거 먹으러",
          "original_form": "선물받으러 먹을거 먹으러",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002873.1.15",
          "form": "근데 그것도 한 두번 가고 안갔던거같아요",
          "original_form": "근데 그것도 한 두번 가고 안갔던거같아요",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002873.1.16",
          "form": "이상하게 기독교는 한번 붙잡히면.. 끈질기게 달라붙더라구요",
          "original_form": "이상하게 기독교는 한번 붙잡히면.. 끈질기게 달라붙더라구요",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002873.1.17",
          "form": "네 그래서 더 거부감이 생기더라구용",
          "original_form": "네 그래서 더 거부감이 생기더라구용",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002873.1.18",
          "form": "약간 그게 있는거 같아요",
          "original_form": "약간 그게 있는거 같아요",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002873.1.19",
          "form": "불교는 거부감이 딱히 없는데 기독교는..",
          "original_form": "불교는 거부감이 딱히 없는데 기독교는..",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002873.1.20",
          "form": "약간 꺼리게되는..?",
          "original_form": "약간 꺼리게되는..?",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002873.1.21",
          "form": "뭔가 좀 음.. 아시죠...\r\n싫다는데 무작정 붙잡거나..\r\n초면에 지옥간다 그러거나..",
          "original_form": "뭔가 좀 음.. 아시죠...\r\n싫다는데 무작정 붙잡거나..\r\n초면에 지옥간다 그러거나..",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002873.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아여",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002873.1.23",
          "form": "그리고 비슷한 사이비종교들도 많구영 ㅜ",
          "original_form": "그리고 비슷한 사이비종교들도 많구영 ㅜ",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002873.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002873.1.25",
          "form": "그런거 너무 위험하고 그래서 인식이 안좋나봐요",
          "original_form": "그런거 너무 위험하고 그래서 인식이 안좋나봐요",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002873.1.26",
          "form": "코로나시국에도 모임좀 안했으면 !!",
          "original_form": "코로나시국에도 모임좀 안했으면 !!",
          "speaker_id": "1",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002873.1.27",
          "form": "그것도 맞아요ㅡㅡ 일부 교회가 뭔 놈의 모임과 집회를 그렇게 해대는지",
          "original_form": "그것도 맞아요ㅡㅡ 일부 교회가 뭔 놈의 모임과 집회를 그렇게 해대는지",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002873.1.28",
          "form": "진짜 그사람들만 아니었어도 이지경까지 안왔을거에요",
          "original_form": "진짜 그사람들만 아니었어도 이지경까지 안왔을거에요",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002873.1.29",
          "form": "그러니깐요 정말 ㅠ",
          "original_form": "그러니깐요 정말 ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:29"
        }
      ]
