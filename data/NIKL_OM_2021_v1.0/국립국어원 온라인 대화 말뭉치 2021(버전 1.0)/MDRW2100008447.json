      "utterance": [
        {
          "id": "MDRW2100008447.1.1",
          "form": "게임콘텐츠..?",
          "original_form": "게임콘텐츠..?",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008447.1.2",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008447.1.3",
          "form": "이거 첨하는데 어케하는거지",
          "original_form": "이거 첨하는데 어케하는거지",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008447.1.4",
          "form": "그냥 게임 얘기 하면 되는것 같아요",
          "original_form": "그냥 게임 얘기 하면 되는것 같아요",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008447.1.5",
          "form": "보는건 좋아해요 ㅋㅋ 근데 잘못해요",
          "original_form": "보는건 좋아해요 ㅋㅋ 근데 잘못해요",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008447.1.6",
          "form": "그리고 대화 제출 하면 되나봐요 저도 오늘 첨해바서 ㅋㅋ",
          "original_form": "그리고 대화 제출 하면 되나봐요 저도 오늘 첨해바서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008447.1.7",
          "form": "아하 어떤종류의 게임 좋아하시나요??",
          "original_form": "아하 어떤종류의 게임 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008447.1.8",
          "form": "아하 ㅋㅋㅋㅋ 신기하네염",
          "original_form": "아하 ㅋㅋㅋㅋ 신기하네염",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008447.1.9",
          "form": "하는건 스타듀 밸리 같은거고 보는건 총게임 같은거요",
          "original_form": "하는건 스타듀 밸리 같은거고 보는건 총게임 같은거요",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008447.1.10",
          "form": "님은요??",
          "original_form": "님은요??",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008447.1.11",
          "form": "아 저는 사이퍼즈라는 게임 요즘 하고있어요",
          "original_form": "아 저는 사이퍼즈라는 게임 요즘 하고있어요",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008447.1.12",
          "form": "오 뭐하는게임이에요?",
          "original_form": "오 뭐하는게임이에요?",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008447.1.13",
          "form": "약간 롤같은 게임이에요! 근데",
          "original_form": "약간 롤같은 게임이에요! 근데",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008447.1.14",
          "form": "타격감을 더한 롤?? 이런느낌 ㅎㅎ",
          "original_form": "타격감을 더한 롤?? 이런느낌 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008447.1.15",
          "form": "오 저 롤 해봤는데 너무 어려워서 포기했어요 ㅋㅋㅋ",
          "original_form": "오 저 롤 해봤는데 너무 어려워서 포기했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008447.1.16",
          "form": "저도 롤 한번도 안해봤어요 ㅋㅋㅋㅋㅋㅋ 왜냐면 너무 어렵다해서ㅜㅠ",
          "original_form": "저도 롤 한번도 안해봤어요 ㅋㅋㅋㅋㅋㅋ 왜냐면 너무 어렵다해서ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008447.1.17",
          "form": "롤 룰을 잘 모르겟더라고요 ㅋㅋㅋㅋ",
          "original_form": "롤 룰을 잘 모르겟더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008447.1.18",
          "form": "몇판해도 이해안되서 포기햇어요 ㅋㅋㅋㅋ",
          "original_form": "몇판해도 이해안되서 포기햇어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008447.1.19",
          "form": "롤 너무 어렵고 약간 뭐하는지 잘 이해가?? 안되더라구욬ㅋㅋㅋ",
          "original_form": "롤 너무 어렵고 약간 뭐하는지 잘 이해가?? 안되더라구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008447.1.20",
          "form": "페이커 명경기 이런거 봣엇는데",
          "original_form": "페이커 명경기 이런거 봣엇는데",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008447.1.21",
          "form": "이해 안가서 그냥 아...그렇구나 하고 말앗어요",
          "original_form": "이해 안가서 그냥 아...그렇구나 하고 말앗어요",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008447.1.22",
          "form": "저도 그냥 자낳대 같은거 봤는데 먼가 재밌게 하는거 같긴하는데",
          "original_form": "저도 그냥 자낳대 같은거 봤는데 먼가 재밌게 하는거 같긴하는데",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008447.1.23",
          "form": "이해가 잘 안되서 어렵네여 ㅋㅋㅋ",
          "original_form": "이해가 잘 안되서 어렵네여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008447.1.24",
          "form": "ㅋㅋㅋㅋ많이 어렵죠ㅠㅠ 다른 게임도 오버워치 같은거 해보고싶었는데 정신없어서 어렵더라구요...",
          "original_form": "ㅋㅋㅋㅋ많이 어렵죠ㅠㅠ 다른 게임도 오버워치 같은거 해보고싶었는데 정신없어서 어렵더라구요...",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008447.1.25",
          "form": "그래서 스팀게임 해볼까 생각중이에요..ㅎㅎ",
          "original_form": "그래서 스팀게임 해볼까 생각중이에요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008447.1.26",
          "form": "오버워치도 해봤는데.. ㅋㅋㅋ 전 실력이없어서",
          "original_form": "오버워치도 해봤는데.. ㅋㅋㅋ 전 실력이없어서",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008447.1.27",
          "form": "맨날 죽기만해요 ㅋㅋㅋ",
          "original_form": "맨날 죽기만해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008447.1.28",
          "form": "저두요 ㅋㅋㅋ왜 맞지?? 이러면서 ㅋㅋㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ왜 맞지?? 이러면서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008447.1.29",
          "form": "그래서 접엇답니다...ㅎㅎ",
          "original_form": "그래서 접엇답니다...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008447.1.30",
          "form": "ㅋㅋㅋ 저도 샀는데 안해서 돈아까워요",
          "original_form": "ㅋㅋㅋ 저도 샀는데 안해서 돈아까워요",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008447.1.31",
          "form": "스타듀밸리 사셨다 했었나요??",
          "original_form": "스타듀밸리 사셨다 했었나요??",
          "speaker_id": "2",
          "time": "20210822 15:59"
        },
