      "utterance": [
        {
          "id": "MDRW2100040256.1.1",
          "form": "최고였어요",
          "original_form": "최고였어요",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.2",
          "form": "용 형님은 전어회 드셨네요",
          "original_form": "용 형님은 전어회 드셨네요",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.3",
          "form": "저도 전화 먹고 싶어요",
          "original_form": "저도 전화 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.4",
          "form": "오늘 저녁은 전어회  드세요",
          "original_form": "오늘 저녁은 전어회  드세요",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.5",
          "form": "전어",
          "original_form": "전어",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.6",
          "form": "좋은 생각인 것 같아요",
          "original_form": "좋은 생각인 것 같아요",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.7",
          "form": "전어  에 고였어요어요",
          "original_form": "전어  에 고였어요어요",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.8",
          "form": "그 그 귀찮웃기다",
          "original_form": "그 그 귀찮웃기다",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.9",
          "form": "제가 원래 가던 횟집이 지금 공사 중이어서 다음 주까지 문을 안녕 돼요",
          "original_form": "제가 원래 가던 횟집이 지금 공사 중이어서 다음 주까지 문을 안녕 돼요",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.10",
          "form": "글자 웃기다",
          "original_form": "글자 웃기다",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.11",
          "form": "문을 안녕 하군요",
          "original_form": "문을 안녕 하군요",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040256.1.12",
          "form": "인식이 잘 안 되시나요",
          "original_form": "인식이 잘 안 되시나요",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 웃기다 아녀",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 웃기다 아녀",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.14",
          "form": "C 타입이 저한테 계속 말걸어요 오늘 갑자기",
          "original_form": "C 타입이 저한테 계속 말걸어요 오늘 갑자기",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.15",
          "form": "따라한거에요ㅋㄱㄴㅋ",
          "original_form": "따라한거에요ㅋㄱㄴㅋ",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.16",
          "form": "걔랑 그냥 안 할래요",
          "original_form": "걔랑 그냥 안 할래요",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.17",
          "form": "전안한지 꽤 됐어요",
          "original_form": "전안한지 꽤 됐어요",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.18",
          "form": "대화가 뚝뚝 끊겨요",
          "original_form": "대화가 뚝뚝 끊겨요",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.19",
          "form": "저는 김밥 먹었는데 김밥이 생각보다 빨리 꺼지는 것 같나요",
          "original_form": "저는 김밥 먹었는데 김밥이 생각보다 빨리 꺼지는 것 같나요",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.20",
          "form": "대화도 끊기고 계속 물어봐서 많이 쳐야 돼요",
          "original_form": "대화도 끊기고 계속 물어봐서 많이 쳐야 돼요",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.21",
          "form": "하긴 11시에 아침을 먹었으니 지금이면 배가 고플 때가 되었죠",
          "original_form": "하긴 11시에 아침을 먹었으니 지금이면 배가 고플 때가 되었죠",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040256.1.22",
          "form": "김밥은 간식이죠",
          "original_form": "김밥은 간식이죠",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040256.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040256.1.24",
          "form": "밥을 드세요 밥",
          "original_form": "밥을 드세요 밥",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040256.1.25",
          "form": "두 번 드세요 만난 걸로",
          "original_form": "두 번 드세요 만난 걸로",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040256.1.26",
          "form": "혹시 편의점에서 파는 녹차 중에서 자두맛 녹차 먹어 본 적 있으세요",
          "original_form": "혹시 편의점에서 파는 녹차 중에서 자두맛 녹차 먹어 본 적 있으세요",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040256.1.27",
          "form": "대화가 넘어가서 못 봤어요",
          "original_form": "대화가 넘어가서 못 봤어요",
          "speaker_id": "2",
          "time": "20210904 15:10"
        }
      ]
    }
