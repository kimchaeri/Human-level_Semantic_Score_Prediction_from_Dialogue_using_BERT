      "utterance": [
        {
          "id": "MDRW2100038510.1.1",
          "form": "서울 여행 오세요~~ 아 맞다 저 예전에",
          "original_form": "서울 여행 오세요~~ 아 맞다 저 예전에",
          "speaker_id": "2",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100038510.1.2",
          "form": "서울 여행 추천해주세요",
          "original_form": "서울 여행 추천해주세요",
          "speaker_id": "1",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100038510.1.3",
          "form": "저는 고즈넉한 분위기를 좋아해서",
          "original_form": "저는 고즈넉한 분위기를 좋아해서",
          "speaker_id": "2",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100038510.1.4",
          "form": "서촌이나 북촌 궁 있는데 좋아하고요",
          "original_form": "서촌이나 북촌 궁 있는데 좋아하고요",
          "speaker_id": "2",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100038510.1.5",
          "form": "요즘은 서울숲도 좋아요",
          "original_form": "요즘은 서울숲도 좋아요",
          "speaker_id": "2",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100038510.1.6",
          "form": "오... 고즈넉분위기",
          "original_form": "오... 고즈넉분위기",
          "speaker_id": "1",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100038510.1.7",
          "form": "좋죵",
          "original_form": "좋죵",
          "speaker_id": "1",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100038510.1.8",
          "form": "주말에 서울여행이나 갈까해요",
          "original_form": "주말에 서울여행이나 갈까해요",
          "speaker_id": "1",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100038510.1.9",
          "form": "오호라~~ 주말에 날씨도 화창한가보더라구요~",
          "original_form": "오호라~~ 주말에 날씨도 화창한가보더라구요~",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100038510.1.10",
          "form": "어디가좋을까요",
          "original_form": "어디가좋을까요",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100038510.1.11",
          "form": "name1님의 마음 가는대로 정해봐요~~",
          "original_form": "&name1&님의 마음 가는대로 정해봐요~~",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100038510.1.12",
          "form": "어렵네요",
          "original_form": "어렵네요",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100038510.1.13",
          "form": "어디든 가고싶어요",
          "original_form": "어디든 가고싶어요",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100038510.1.14",
          "form": "그라운드시소도 추천받앗눈데",
          "original_form": "그라운드시소도 추천받앗눈데",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100038510.1.15",
          "form": "아 그거 지금 요시고 사진전 하는..??",
          "original_form": "아 그거 지금 요시고 사진전 하는..??",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038510.1.16",
          "form": "앗 마자요 수영장 ㅋㅋㅋ",
          "original_form": "앗 마자요 수영장 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100038510.1.17",
          "form": "아 날조은데 산책도 가고싶고~",
          "original_form": "아 날조은데 산책도 가고싶고~",
          "speaker_id": "1",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100038510.1.18",
          "form": "맞아요 겨울오기전에 산책 많이많이하고싶네요",
          "original_form": "맞아요 겨울오기전에 산책 많이많이하고싶네요",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100038510.1.19",
          "form": "서울 여행가서 먹고싶은거 뭐에요",
          "original_form": "서울 여행가서 먹고싶은거 뭐에요",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100038510.1.20",
          "form": "파스타? ㅎㅎ 아당장생각나는게없네 걍 맛잇으면 다 좋죠",
          "original_form": "파스타? ㅎㅎ 아당장생각나는게없네 걍 맛잇으면 다 좋죠",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100038510.1.21",
          "form": "맞아요 맛있으면 다좋고 여행이 좋으면 괜히 더 맛있고",
          "original_form": "맞아요 맛있으면 다좋고 여행이 좋으면 괜히 더 맛있고",
