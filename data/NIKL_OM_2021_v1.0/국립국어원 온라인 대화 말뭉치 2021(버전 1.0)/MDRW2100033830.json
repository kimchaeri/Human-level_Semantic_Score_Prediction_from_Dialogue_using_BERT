      "utterance": [
        {
          "id": "MDRW2100033830.1.1",
          "form": "name2 님은 최근에 사신 것 있나요?",
          "original_form": "&name2& 님은 최근에 사신 것 있나요?",
          "speaker_id": "1",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100033830.1.2",
          "form": "저는 어제 마우스패드 샀어요",
          "original_form": "저는 어제 마우스패드 샀어요",
          "speaker_id": "2",
          "time": "20210825 11:18"
        },
        {
          "id": "MDRW2100033830.1.3",
          "form": "전 운동화요",
          "original_form": "전 운동화요",
          "speaker_id": "1",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100033830.1.4",
          "form": "또 사고 싶은 물건 있으세요?",
          "original_form": "또 사고 싶은 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100033830.1.5",
          "form": "노트북을 고민하고 있어요",
          "original_form": "노트북을 고민하고 있어요",
          "speaker_id": "2",
          "time": "20210825 11:20"
        },
        {
          "id": "MDRW2100033830.1.6",
          "form": "어떤 브랜드요?",
          "original_form": "어떤 브랜드요?",
          "speaker_id": "1",
          "time": "20210825 11:20"
        },
        {
          "id": "MDRW2100033830.1.7",
          "form": "lg 그램이요!",
          "original_form": "lg 그램이요!",
          "speaker_id": "2",
          "time": "20210825 11:20"
        },
        {
          "id": "MDRW2100033830.1.8",
          "form": "그램 가벼워서 좋죠",
          "original_form": "그램 가벼워서 좋죠",
          "speaker_id": "1",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100033830.1.9",
          "form": "전 삼성 거 쓰는데 너무 무거워요",
          "original_form": "전 삼성 거 쓰는데 너무 무거워요",
          "speaker_id": "1",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100033830.1.10",
          "form": "삼성이랑 고민 많이 했는데 휴대성이 필요해서요",
          "original_form": "삼성이랑 고민 많이 했는데 휴대성이 필요해서요",
          "speaker_id": "2",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100033830.1.11",
          "form": "휴대성은 그램이 훨씬 좋은 것 같아요",
          "original_form": "휴대성은 그램이 훨씬 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100033830.1.12",
          "form": "전 요즘에 갤럭시 워치가 갖고 싶어요",
          "original_form": "전 요즘에 갤럭시 워치가 갖고 싶어요",
          "speaker_id": "1",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100033830.1.13",
          "form": "이번 워치 너무 이쁘죠ㅠ 색상까지도요ㅠㅠ",
          "original_form": "이번 워치 너무 이쁘죠ㅠ 색상까지도요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 12:44"
        },
        {
          "id": "MDRW2100033830.1.14",
          "form": "맞아요 워치는 원래 관심없었는데 워치 사면 운동도 괜히 더 하게 되지 않을까 싶네요ㅋㅋ",
          "original_form": "맞아요 워치는 원래 관심없었는데 워치 사면 운동도 괜히 더 하게 되지 않을까 싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100033830.1.15",
          "form": "이번 디자인 때문에 엄청 고민하고있어요ㅋㅋㅋ 특히나 이번 워치는 인바디도 되게 빨리 체크하더라구요",
          "original_form": "이번 디자인 때문에 엄청 고민하고있어요ㅋㅋㅋ 특히나 이번 워치는 인바디도 되게 빨리 체크하더라구요",
          "speaker_id": "2",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100033830.1.16",
          "form": "그러니까요 그거 보고 갑자기 사고 싶어졌어요ㅋㅋㅋ",
          "original_form": "그러니까요 그거 보고 갑자기 사고 싶어졌어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033830.1.17",
          "form": "이번 플립도 너무 이쁘고요ㅠ",
          "original_form": "이번 플립도 너무 이쁘고요ㅠ",
          "speaker_id": "2",
