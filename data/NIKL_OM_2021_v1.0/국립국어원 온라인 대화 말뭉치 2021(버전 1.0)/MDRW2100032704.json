      "utterance": [
        {
          "id": "MDRW2100032704.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032704.1.2",
          "form": "안녕하세요! 좋아하시는 스포츠 있으신가요?",
          "original_form": "안녕하세요! 좋아하시는 스포츠 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032704.1.3",
          "form": "앗 저는 이번에 올림픽에서 양궁 멋있었어요",
          "original_form": "앗 저는 이번에 올림픽에서 양궁 멋있었어요",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032704.1.4",
          "form": "저도 올림픽에서 봤어요!",
          "original_form": "저도 올림픽에서 봤어요!",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032704.1.5",
          "form": "가즈아아 하는 우렁찬 목소리가 아직도 기억에 남아요 ㅎㅎ",
          "original_form": "가즈아아 하는 우렁찬 목소리가 아직도 기억에 남아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032704.1.6",
          "form": "ㅎㅈㅅㅎㅈㅎㅎ",
          "original_form": "ㅎㅈㅅㅎㅈㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032704.1.7",
          "form": "좋아하는스포츠있으세요?",
          "original_form": "좋아하는스포츠있으세요?",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032704.1.8",
          "form": "저는 이번에 올림픽을 보고",
          "original_form": "저는 이번에 올림픽을 보고",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032704.1.9",
          "form": "배구를 좋아하게 되었어요!",
          "original_form": "배구를 좋아하게 되었어요!",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032704.1.10",
          "form": "멋졌죠",
          "original_form": "멋졌죠",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032704.1.11",
          "form": "직관해보신적있으세요?",
          "original_form": "직관해보신적있으세요?",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032704.1.12",
          "form": "아니요 ㅠㅠ",
          "original_form": "아니요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032704.1.13",
          "form": "코로나 상황이라 직관은 올해 못할 것 같아서 슬프네요",
          "original_form": "코로나 상황이라 직관은 올해 못할 것 같아서 슬프네요",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032704.1.14",
          "form": "다른 스포츠 경기 직관해보신적 있으세요?",
          "original_form": "다른 스포츠 경기 직관해보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032704.1.15",
          "form": "음 야구장몇번가본적있어요",
          "original_form": "음 야구장몇번가본적있어요",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032704.1.16",
          "form": "맞아요ㅠ코로나",
          "original_form": "맞아요ㅠ코로나",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032704.1.17",
          "form": "응원하시는 야구팀이 있으신가요?",
          "original_form": "응원하시는 야구팀이 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032704.1.18",
          "form": "음 친구가 기아를 응원했어요!",
          "original_form": "음 친구가 기아를 응원했어요!",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100032704.1.19",
          "form": "저는따라따라ㅎㅎ",
          "original_form": "저는따라따라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100032704.1.20",
          "form": "그렇군요! 응원하는 팀 없이 보러가기만 해도 재미있을것 같네요",
          "original_form": "그렇군요! 응원하는 팀 없이 보러가기만 해도 재미있을것 같네요",
          "speaker_id": "2",
          "time": "20210906 18:51"
        },
        {
          "id": "MDRW2100032704.1.21",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100032704.1.22",
          "form": "가서응원가따라불렀어요",
          "original_form": "가서응원가따라불렀어요",
