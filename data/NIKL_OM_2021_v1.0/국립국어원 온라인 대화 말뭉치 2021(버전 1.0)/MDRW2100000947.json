      "utterance": [
        {
          "id": "MDRW2100000947.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000947.1.2",
          "form": "아뇨 졸업한지 오래됐는데 ㅎㅎ 학교다닐때 어떠셨어요?",
          "original_form": "아뇨 졸업한지 오래됐는데 ㅎㅎ 학교다닐때 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000947.1.3",
          "form": "전 완전 아싸",
          "original_form": "전 완전 아싸",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000947.1.4",
          "form": "저도 아싸",
          "original_form": "저도 아싸",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000947.1.5",
          "form": "좁은 인맥ㅋㅋㅋㅋㅋㅋ",
          "original_form": "좁은 인맥ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000947.1.6",
          "form": "저도요 그래서 막 과생활굴러가는거 뒤늦게 알게되고 ㅋㅋㅋㅋㅋ그랬었나 싶은적이많았어요",
          "original_form": "저도요 그래서 막 과생활굴러가는거 뒤늦게 알게되고 ㅋㅋㅋㅋㅋ그랬었나 싶은적이많았어요",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000947.1.7",
          "form": "동아리같은거 그러면 안해보셨나요?",
          "original_form": "동아리같은거 그러면 안해보셨나요?",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000947.1.8",
          "form": "아ㅋㅋㅋ저는 동아리만 열심히 했어요",
          "original_form": "아ㅋㅋㅋ저는 동아리만 열심히 했어요",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000947.1.9",
          "form": "과활동은 안 하고",
          "original_form": "과활동은 안 하고",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000947.1.10",
          "form": "그래서 과친구들은 진짜 거의 없었어요",
          "original_form": "그래서 과친구들은 진짜 거의 없었어요",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000947.1.11",
          "form": "전 친구한명이 인싸인애가있어서 걔만 따라다니다보니",
          "original_form": "전 친구한명이 인싸인애가있어서 걔만 따라다니다보니",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000947.1.12",
          "form": "동아리같은것도 하긴했는데 ㅋㅋㅋㅋㅋ진짜 안맞더라구여",
          "original_form": "동아리같은것도 하긴했는데 ㅋㅋㅋㅋㅋ진짜 안맞더라구여",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000947.1.13",
          "form": "독서토론동아리...ㅎㅎ........",
          "original_form": "독서토론동아리...ㅎㅎ........",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000947.1.14",
          "form": "아이 말만 들어도 재미가 없네요",
          "original_form": "아이 말만 들어도 재미가 없네요",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000947.1.15",
          "form": "독서토론이라니...흡",
          "original_form": "독서토론이라니...흡",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000947.1.16",
          "form": "저는 운동부였어요 검도!",
          "original_form": "저는 운동부였어요 검도!",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000947.1.17",
          "form": "와 차라리 진짜 운동부가좋은것같아요 운동하면되니까ㅜ",
          "original_form": "와 차라리 진짜 운동부가좋은것같아요 운동하면되니까ㅜ",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000947.1.18",
          "form": "저 말주변도없는데 강제러 토론해야돼서 거의죽상으로다녔어요 ㅋㅋㅋㅋ",
          "original_form": "저 말주변도없는데 강제러 토론해야돼서 거의죽상으로다녔어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000947.1.19",
          "form": "검도 넘 멋있는데요 도복입고 탓탓",
          "original_form": "검도 넘 멋있는데요 도복입고 탓탓",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000947.1.20",
          "form": "네 맞아요! 말 많이 안 해도 되고",
          "original_form": "네 맞아요! 말 많이 안 해도 되고",
          "speaker_id": "2",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000947.1.21",
          "form": "근데 훈련이 진짜 너무 고되고 어유",
          "original_form": "근데 훈련이 진짜 너무 고되고 어유",
          "speaker_id": "2",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000947.1.22",
          "form": "아침운동이 매일 있었거든요 7시에",
          "original_form": "아침운동이 매일 있었거든요 7시에",
          "speaker_id": "2",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000947.1.23",
          "form": "와우 매일이요? 동아리가 엄청본격적이었네요",
          "original_form": "와우 매일이요? 동아리가 엄청본격적이었네요",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000947.1.24",
          "form": "그래도 뭔가 학창시절에 열심히한게있으니까 그때그 더 선명하게 기억날것같아요",
          "original_form": "그래도 뭔가 학창시절에 열심히한게있으니까 그때그 더 선명하게 기억날것같아요",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000947.1.25",
          "form": "맞아요 그건 그래요!",
          "original_form": "맞아요 그건 그래요!",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000947.1.26",
          "form": "생각나고 그립고 아 옛날이여",
          "original_form": "생각나고 그립고 아 옛날이여",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000947.1.27",
          "form": "ㅋㅋㅋㅋㅋ전 좀더 열심히 다닐걸그랬어요 휴학도 2년이나 꽉꽉하고 그랬어서",
          "original_form": "ㅋㅋㅋㅋㅋ전 좀더 열심히 다닐걸그랬어요 휴학도 2년이나 꽉꽉하고 그랬어서",
          "speaker_id": "1",
          "time": "20210819 21:13"
        }
      ]
    }
