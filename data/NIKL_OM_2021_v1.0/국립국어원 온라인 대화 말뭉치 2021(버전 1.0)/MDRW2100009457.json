      "utterance": [
        {
          "id": "MDRW2100009457.1.1",
          "form": "날씨요?? 여긴 지금 우중충합니다",
          "original_form": "날씨요?? 여긴 지금 우중충합니다",
          "speaker_id": "1",
          "time": "20210905 15:58"
        },
        {
          "id": "MDRW2100009457.1.2",
          "form": "여긴 구름은 많고",
          "original_form": "여긴 구름은 많고",
          "speaker_id": "2",
          "time": "20210905 15:58"
        },
        {
          "id": "MDRW2100009457.1.3",
          "form": "습도는 48프로",
          "original_form": "습도는 48프로",
          "speaker_id": "2",
          "time": "20210905 15:58"
        },
        {
          "id": "MDRW2100009457.1.4",
          "form": "기온은 26.9도씨",
          "original_form": "기온은 26.9도씨",
          "speaker_id": "2",
          "time": "20210905 15:58"
        },
        {
          "id": "MDRW2100009457.1.5",
          "form": "커튼 빨기 좋은 날씨죠",
          "original_form": "커튼 빨기 좋은 날씨죠",
          "speaker_id": "2",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100009457.1.6",
          "form": "아 슬슬 가습기 가동해야겠네",
          "original_form": "아 슬슬 가습기 가동해야겠네",
          "speaker_id": "1",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100009457.1.7",
          "form": "제습기 끄자마자 가습기 준비 ㅋㅋㅋ",
          "original_form": "제습기 끄자마자 가습기 준비 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100009457.1.8",
          "form": "내가 가습기 여러개 갈아치웠거든",
          "original_form": "내가 가습기 여러개 갈아치웠거든",
          "speaker_id": "1",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100009457.1.9",
          "form": "혹시 아직 안샀으면 벤타 에어워셔 추천",
          "original_form": "혹시 아직 안샀으면 벤타 에어워셔 추천",
          "speaker_id": "1",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100009457.1.10",
          "form": "품질 불만족?",
          "original_form": "품질 불만족?",
          "speaker_id": "2",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100009457.1.11",
          "form": "급해서 작년에 크고 둥근거 하나 샀어",
          "original_form": "급해서 작년에 크고 둥근거 하나 샀어",
          "speaker_id": "2",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100009457.1.12",
          "form": "초음파식 가습기는 하도 이슈가 많아서 못쓰겠더라구",
          "original_form": "초음파식 가습기는 하도 이슈가 많아서 못쓰겠더라구",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100009457.1.13",
          "form": "벤타도 비싼거 아녀?",
          "original_form": "벤타도 비싼거 아녀?",
          "speaker_id": "2",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100009457.1.14",
          "form": "크고 둥근 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "크고 둥근 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100009457.1.15",
          "form": "저렴하진않지",
          "original_form": "저렴하진않지",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100009457.1.16",
          "form": "혹시 식기세척기있어?",
          "original_form": "혹시 식기세척기있어?",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100009457.1.17",
          "form": "오아 에어워셔샀었네",
          "original_form": "오아 에어워셔샀었네",
          "speaker_id": "2",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100009457.1.18",
          "form": "식세기 있어. 그 .....디스크를 식세기로 빤대매?",
          "original_form": "식세기 있어. 그 .....디스크를 식세기로 빤대매?",
          "speaker_id": "2",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100009457.1.19",
          "form": "빤대 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "빤대 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100009457.1.20",
          "form": "아아 자연기회식 샀구나 ㅋㅋ",
          "original_form": "아아 자연기회식 샀구나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100009457.1.21",
          "form": "하여튼 자연기화식 + 식기세척기 조합이 젤 만족스러웠음",
          "original_form": "하여튼 자연기화식 + 식기세척기 조합이 젤 만족스러웠음",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100009457.1.22",
          "form": "가열식이 젤 좋겠지만 사고위험때문에 ㅠ 무서워서 못쓰겠어",
          "original_form": "가열식이 젤 좋겠지만 사고위험때문에 ㅠ 무서워서 못쓰겠어",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100009457.1.23",
          "form": "맞아 애가 기어다니니까",
          "original_form": "맞아 애가 기어다니니까",
          "speaker_id": "2",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100009457.1.24",
          "form": "작년에 산 가습기도",
          "original_form": "작년에 산 가습기도",
          "speaker_id": "2",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100009457.1.25",
          "form": "내려놓고 못 쓰겠구나 싶어",
          "original_form": "내려놓고 못 쓰겠구나 싶어",
          "speaker_id": "2",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100009457.1.26",
          "form": "오늘 미세먼지도 14다",
          "original_form": "오늘 미세먼지도 14다",
          "speaker_id": "2",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100009457.1.27",
          "form": "오늘 날씨 나는 너무 좋다",
          "original_form": "오늘 날씨 나는 너무 좋다",
          "speaker_id": "2",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100009457.1.28",
          "form": "우리가 멀리살긴 멀리사나봐",
          "original_form": "우리가 멀리살긴 멀리사나봐",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100009457.1.29",
          "form": "나 세종에서 대전까지 머리하러왔거든",
          "original_form": "나 세종에서 대전까지 머리하러왔거든",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100009457.1.30",
          "form": "오는길에 비가 주룩주룩",
          "original_form": "오는길에 비가 주룩주룩",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100009457.1.31",
          "form": "대한민국 크구만",
          "original_form": "대한민국 크구만",
          "speaker_id": "2",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100009457.1.32",
          "form": "대전에 단골 미용실이 있어?",
          "original_form": "대전에 단골 미용실이 있어?",
          "speaker_id": "2",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100009457.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100009457.1.34",
          "form": "어어 예전에 살던동네에 30만원 충전해놨어",
          "original_form": "어어 예전에 살던동네에 30만원 충전해놨어",
