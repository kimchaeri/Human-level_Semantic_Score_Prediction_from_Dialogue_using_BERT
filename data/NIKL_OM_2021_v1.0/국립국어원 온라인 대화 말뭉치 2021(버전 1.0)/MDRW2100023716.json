      "utterance": [
        {
          "id": "MDRW2100023716.1.1",
          "form": "비가 오나요?",
          "original_form": "비가 오나요?",
          "speaker_id": "2",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100023716.1.2",
          "form": "아뇨 흐리기만 하네요",
          "original_form": "아뇨 흐리기만 하네요",
          "speaker_id": "1",
          "time": "20210830 15:52"
        },
        {
          "id": "MDRW2100023716.1.3",
          "form": "선선해져서좋네요",
          "original_form": "선선해져서좋네요",
          "speaker_id": "1",
          "time": "20210830 15:53"
        },
        {
          "id": "MDRW2100023716.1.4",
          "form": "오늘은 비가ㅡ많이와요",
          "original_form": "오늘은 비가ㅡ많이와요",
          "speaker_id": "2",
          "time": "20210831 14:06"
        },
        {
          "id": "MDRW2100023716.1.5",
          "form": "그러게요 앞이 안보일정도로",
          "original_form": "그러게요 앞이 안보일정도로",
          "speaker_id": "1",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100023716.1.6",
          "form": "날이 제법 쌀쌀하죠",
          "original_form": "날이 제법 쌀쌀하죠",
          "speaker_id": "2",
          "time": "20210831 15:18"
        },
        {
          "id": "MDRW2100023716.1.7",
          "form": "넹 가디건필수더라고오",
          "original_form": "넹 가디건필수더라고오",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100023716.1.8",
          "form": "맞아요 환절기엔 걸칠 옷이 있어야죠",
          "original_form": "맞아요 환절기엔 걸칠 옷이 있어야죠",
          "speaker_id": "2",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100023716.1.9",
          "form": "습한것도 싫은데",
          "original_form": "습한것도 싫은데",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100023716.1.10",
          "form": "신발젖는게 ㅎ",
          "original_form": "신발젖는게 ㅎ",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100023716.1.11",
          "form": "아ㅡ그쵸 저도 운전안할땐 쪼리나 장화신고다녔어요",
          "original_form": "아ㅡ그쵸 저도 운전안할땐 쪼리나 장화신고다녔어요",
          "speaker_id": "2",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100023716.1.12",
          "form": "비가 아직도 오네요",
          "original_form": "비가 아직도 오네요",
          "speaker_id": "1",
          "time": "20210831 20:16"
        },
        {
          "id": "MDRW2100023716.1.13",
          "form": "오늘은 운전하다가 트럭이 튀긴 물에 차가 흙탕물을 뒤집어썼어요",
          "original_form": "오늘은 운전하다가 트럭이 튀긴 물에 차가 흙탕물을 뒤집어썼어요",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100023716.1.14",
          "form": "완전 운수가 안좋네요 ㅜ",
          "original_form": "완전 운수가 안좋네요 ㅜ",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100023716.1.15",
          "form": "네 비가ㅡ오니 이런 일도 있네요..",
          "original_form": "네 비가ㅡ오니 이런 일도 있네요..",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100023716.1.16",
          "form": "힘내요",
          "original_form": "힘내요",
          "speaker_id": "1",
          "time": "20210901 20:54"
        }
      ]
