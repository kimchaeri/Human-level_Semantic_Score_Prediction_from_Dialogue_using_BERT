      "utterance": [
        {
          "id": "MDRW2100016456.1.1",
          "form": "운동할 때 무슨노래들어요",
          "original_form": "운동할 때 무슨노래들어요",
          "speaker_id": "1",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016456.1.2",
          "form": "오늘은 아이유 1시간 반복 들었습니다",
          "original_form": "오늘은 아이유 1시간 반복 들었습니다",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100016456.1.3",
          "form": "name1은 음악 요즘 뭐듣고 계시나요 sg워너비 말고",
          "original_form": "&name1&은 음악 요즘 뭐듣고 계시나요 sg워너비 말고",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100016456.1.4",
          "form": "운동 할 때도 아이유음악을 들으시다니",
          "original_form": "운동 할 때도 아이유음악을 들으시다니",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100016456.1.5",
          "form": "진짜 지조있으시네요",
          "original_form": "진짜 지조있으시네요",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100016456.1.6",
          "form": "전 운동할 때 신나는거 들어요",
          "original_form": "전 운동할 때 신나는거 들어요",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100016456.1.7",
          "form": "골든차일드의 담다디나..\r\n트와이스의 터치다운이나..\r\n세븐틴의 쁘악수나\r\n샤이니의 링딩동같은..",
          "original_form": "골든차일드의 담다디나..\r\n트와이스의 터치다운이나..\r\n세븐틴의 쁘악수나\r\n샤이니의 링딩동같은..",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100016456.1.8",
          "form": "링딩동 연륜 나온다",
          "original_form": "링딩동 연륜 나온다",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100016456.1.9",
          "form": "골든차일드로 속이려 해봤지만",
          "original_form": "골든차일드로 속이려 해봤지만",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100016456.1.10",
          "form": "아 그거 아시나요 틱톡에",
          "original_form": "아 그거 아시나요 틱톡에",
          "speaker_id": "2",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100016456.1.11",
          "form": "이루 -까만안경이 유행한다는 소리를 듣고 9n년생들이 놀라한대요",
          "original_form": "이루 -까만안경이 유행한다는 소리를 듣고 9n년생들이 놀라한대요",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100016456.1.12",
          "form": "까만안경?",
          "original_form": "까만안경?",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100016456.1.13",
          "form": "그게 무슨노래지",
          "original_form": "그게 무슨노래지",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100016456.1.14",
          "form": "흠.. 저 노래가 유행한다고요?",
          "original_form": "흠.. 저 노래가 유행한다고요?",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100016456.1.15",
          "form": "나만몰랐던 우행",
          "original_form": "나만몰랐던 우행",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100016456.1.16",
          "form": "들으면 좋더라고요",
          "original_form": "들으면 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100016456.1.17",
          "form": "name1 틱톡 안하시죠",
          "original_form": "&name1& 틱톡 안하시죠",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100016456.1.18",
          "form": "저도 틱톡 안해서 몰랐어요",
          "original_form": "저도 틱톡 안해서 몰랐어요",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100016456.1.19",
          "form": "엇.. 틱톡을 안하는 자의 비애",
          "original_form": "엇.. 틱톡을 안하는 자의 비애",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100016456.1.20",
          "form": "근데 어디서들었어요",
          "original_form": "근데 어디서들었어요",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100016456.1.21",
          "form": "틱톡하는 지인이 있으신가요",
          "original_form": "틱톡하는 지인이 있으신가요",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100016456.1.22",
          "form": "네니요",
          "original_form": "네니요",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100016456.1.23",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100016456.1.24",
          "form": "그 같이 알바하는 친구에게 들었어욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 같이 알바하는 친구에게 들었어욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100016456.1.25",
          "form": "뭐야 그게ㅋㅋㅋㅋㅋ",
          "original_form": "뭐야 그게ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100016456.1.26",
          "form": "아 친구분은 틱톡을 하시는구나",
          "original_form": "아 친구분은 틱톡을 하시는구나",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100016456.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016456.1.28",
          "form": "ㄱ..그런거같아요",
          "original_form": "ㄱ..그런거같아요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016456.1.29",
          "form": "또 무슨 노래 유행하는지는 안들었나요",
          "original_form": "또 무슨 노래 유행하는지는 안들었나요",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016456.1.30",
          "form": "틱톡 가입하라고만 들었네요.",
          "original_form": "틱톡 가입하라고만 들었네요.",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016456.1.31",
          "form": "나도 이제 유행에 뒤쳐지는건가",
          "original_form": "나도 이제 유행에 뒤쳐지는건가",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016456.1.32",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016456.1.33",
          "form": "우리 같이 늙탱이 되나봐요",
          "original_form": "우리 같이 늙탱이 되나봐요",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016456.1.34",
          "form": "손의 손잡고나 부릅시다",
          "original_form": "손의 손잡고나 부릅시다",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016456.1.35",
          "form": "엮지마세여(?)",
          "original_form": "엮지마세여(?)",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
