      "utterance": [
        {
          "id": "MDRW2100020117.1.1",
          "form": "재택근무해보셨어요?",
          "original_form": "재택근무해보셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020117.1.2",
          "form": "아니요..저는 재택근무를 할수없는 직종이라ㅠ",
          "original_form": "아니요..저는 재택근무를 할수없는 직종이라ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020117.1.3",
          "form": "아 ㅜㅜ 저도 못해봤어요",
          "original_form": "아 ㅜㅜ 저도 못해봤어요",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020117.1.4",
          "form": "재택근무 하고싶네요 저도ㅠ",
          "original_form": "재택근무 하고싶네요 저도ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020117.1.5",
          "form": "1년넘게 하는 친구 있는데",
          "original_form": "1년넘게 하는 친구 있는데",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020117.1.6",
          "form": "너무 부럽더라고요 ㅋㅋㅋ",
          "original_form": "너무 부럽더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020117.1.7",
          "form": "와..진짜 부럽네요",
          "original_form": "와..진짜 부럽네요",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020117.1.8",
          "form": "재택근무는 단점없대요?ㅋㅋ",
          "original_form": "재택근무는 단점없대요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020117.1.9",
          "form": "음 아무래도 신입이라",
          "original_form": "음 아무래도 신입이라",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020117.1.10",
          "form": "질문할땐 좀 불편하다더라구요",
          "original_form": "질문할땐 좀 불편하다더라구요",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020117.1.11",
          "form": "그거빼곤 좋대요 ㅋㅋㅋㅋ",
          "original_form": "그거빼곤 좋대요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020117.1.12",
          "form": "재택해도 회사 잘돌아가나보네요",
          "original_form": "재택해도 회사 잘돌아가나보네요",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020117.1.13",
          "form": "그런가봐요 ㅋㅋㅋ",
          "original_form": "그런가봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020117.1.14",
          "form": "아~신입이면 진짜 업무의 절반이 질문인데",
          "original_form": "아~신입이면 진짜 업무의 절반이 질문인데",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020117.1.15",
          "form": "예전엔 재택이 꿈이었는데",
          "original_form": "예전엔 재택이 꿈이었는데",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020117.1.16",
          "form": "질문 못하면  답답하죠",
          "original_form": "질문 못하면  답답하죠",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020117.1.17",
          "form": "그러니깐요 ㅜㅜ",
          "original_form": "그러니깐요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020117.1.18",
          "form": "코로나로 재택 강제로 할줄은 몰랐어요 ㅋㅋㅋ저는 안하지만..",
          "original_form": "코로나로 재택 강제로 할줄은 몰랐어요 ㅋㅋㅋ저는 안하지만..",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020117.1.19",
          "form": "저도 재택 한번해보고싶네요",
          "original_form": "저도 재택 한번해보고싶네요",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020117.1.20",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
