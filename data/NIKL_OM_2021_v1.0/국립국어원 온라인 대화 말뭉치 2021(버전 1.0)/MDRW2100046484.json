      "utterance": [
        {
          "id": "MDRW2100046484.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211025 17:49"
        },
        {
          "id": "MDRW2100046484.1.2",
          "form": "네 안녕하세요~",
          "original_form": "네 안녕하세요~",
          "speaker_id": "2",
          "time": "20211025 17:56"
        },
        {
          "id": "MDRW2100046484.1.3",
          "form": "혹시 글램핑 좋아하세요?",
          "original_form": "혹시 글램핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20211025 17:56"
        },
        {
          "id": "MDRW2100046484.1.4",
          "form": "글램핌 좋아하는데 몇번 못가봤어요",
          "original_form": "글램핌 좋아하는데 몇번 못가봤어요",
          "speaker_id": "1",
          "time": "20211025 17:56"
        },
        {
          "id": "MDRW2100046484.1.5",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20211025 17:57"
        },
        {
          "id": "MDRW2100046484.1.6",
          "form": "엄청 좋아하긴하는데",
          "original_form": "엄청 좋아하긴하는데",
          "speaker_id": "2",
          "time": "20211025 17:57"
        },
        {
          "id": "MDRW2100046484.1.7",
          "form": "시간이 잘안나서..ㅎㅎ",
          "original_form": "시간이 잘안나서..ㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 17:57"
        },
        {
          "id": "MDRW2100046484.1.8",
          "form": "그리고 좀 유명한곳은",
          "original_form": "그리고 좀 유명한곳은",
          "speaker_id": "2",
          "time": "20211025 17:57"
        },
        {
          "id": "MDRW2100046484.1.9",
          "form": "예약이 정말 어렵더라구요 ㅋㅋ",
          "original_form": "예약이 정말 어렵더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211025 17:57"
        },
        {
          "id": "MDRW2100046484.1.10",
          "form": "맞아요 예약하기가 힘들어요",
          "original_form": "맞아요 예약하기가 힘들어요",
          "speaker_id": "1",
          "time": "20211025 17:57"
        },
        {
          "id": "MDRW2100046484.1.11",
          "form": "미리 안하면..",
          "original_form": "미리 안하면..",
          "speaker_id": "1",
          "time": "20211025 17:57"
        },
        {
          "id": "MDRW2100046484.1.12",
          "form": "주말에가려면 한달전에는 해야될거같더라구요;;",
          "original_form": "주말에가려면 한달전에는 해야될거같더라구요;;",
          "speaker_id": "2",
          "time": "20211025 17:57"
        },
        {
          "id": "MDRW2100046484.1.13",
          "form": "요즘같은 성수기라서 잡기가 정말 힘들더라구요",
          "original_form": "요즘같은 성수기라서 잡기가 정말 힘들더라구요",
          "speaker_id": "1",
          "time": "20211025 17:58"
        },
        {
          "id": "MDRW2100046484.1.14",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20211025 18:00"
        },
        {
          "id": "MDRW2100046484.1.15",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20211025 18:00"
        },
        {
          "id": "MDRW2100046484.1.16",
          "form": "가고싶은데 못가네...",
          "original_form": "가고싶은데 못가네...",
          "speaker_id": "2",
          "time": "20211025 18:00"
        },
        {
          "id": "MDRW2100046484.1.17",
          "form": "비올 때",
          "original_form": "비올 때",
          "speaker_id": "2",
          "time": "20211025 18:00"
        },
        {
          "id": "MDRW2100046484.1.18",
          "form": "글램핑가보셨어요?",
          "original_form": "글램핑가보셨어요?",
          "speaker_id": "2",
          "time": "20211025 18:00"
        },
        {
          "id": "MDRW2100046484.1.19",
          "form": "비올때 글램핑 분위기 진짜 좋죠",
          "original_form": "비올때 글램핑 분위기 진짜 좋죠",
          "speaker_id": "1",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.20",
          "form": "크아",
          "original_form": "크아",
          "speaker_id": "2",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.21",
          "form": "비오는소리 들으면 술한잔",
          "original_form": "비오는소리 들으면 술한잔",
          "speaker_id": "1",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.22",
          "form": "저도 첨 글램핑 갔을 때",
          "original_form": "저도 첨 글램핑 갔을 때",
          "speaker_id": "2",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.23",
          "form": "비와서",
          "original_form": "비와서",
          "speaker_id": "2",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.24",
          "form": "망했다싶었는데",
          "original_form": "망했다싶었는데",
          "speaker_id": "2",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.25",
          "form": "오히려...너무너무좋았던 기억이 ㅎㅎ",
          "original_form": "오히려...너무너무좋았던 기억이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.26",
          "form": "그 이후로는 오히려",
          "original_form": "그 이후로는 오히려",
          "speaker_id": "2",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.27",
          "form": "비오는 날을 노려서 가게되는거같아여ㅋㅋㅋ",
          "original_form": "비오는 날을 노려서 가게되는거같아여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.28",
          "form": "크아 빗소리 들으면서 소주한잔 최고죠",
          "original_form": "크아 빗소리 들으면서 소주한잔 최고죠",
          "speaker_id": "2",
          "time": "20211025 18:01"
        },
        {
          "id": "MDRW2100046484.1.29",
          "form": "맞아요 술도 더안취하는거 같구요",
          "original_form": "맞아요 술도 더안취하는거 같구요",
          "speaker_id": "1",
          "time": "20211025 18:02"
        },
        {
          "id": "MDRW2100046484.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211025 18:02"
        },
        {
          "id": "MDRW2100046484.1.31",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20211025 18:02"
        },
        {
          "id": "MDRW2100046484.1.32",
          "form": "힘들면 잠깐자면되잖아요 ㅎ",
          "original_form": "힘들면 잠깐자면되잖아요 ㅎ",
          "speaker_id": "2",
          "time": "20211025 18:02"
        },
        {
          "id": "MDRW2100046484.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211025 18:03"
        },
        {
          "id": "MDRW2100046484.1.34",
          "form": "그런데 리조트에서 먹는 술과 글램핑하면서 자연에서 먹는 술맛이 진짜 다른거 같아요",
          "original_form": "그런데 리조트에서 먹는 술과 글램핑하면서 자연에서 먹는 술맛이 진짜 다른거 같아요",
          "speaker_id": "1",
          "time": "20211025 18:04"
        },
        {
          "id": "MDRW2100046484.1.35",
          "form": "정 말",
          "original_form": "정 말",
          "speaker_id": "2",
          "time": "20211025 18:04"
        },
        {
          "id": "MDRW2100046484.1.36",
          "form": "정말 다루죠....",
          "original_form": "정말 다루죠....",
          "speaker_id": "2",
          "time": "20211025 18:04"
        },
        {
          "id": "MDRW2100046484.1.37",
          "form": "다르죠..",
          "original_form": "다르죠..",
          "speaker_id": "2",
          "time": "20211025 18:04"
        },
        {
          "id": "MDRW2100046484.1.38",
          "form": "같은 여행인데도 다르니..",
          "original_form": "같은 여행인데도 다르니..",
          "speaker_id": "1",
          "time": "20211025 18:05"
        },
        {
          "id": "MDRW2100046484.1.39",
          "form": "요즘 글램핑 좋아져서 더욱더 글램핑 찾는 사람들이 많아졌어요",
          "original_form": "요즘 글램핑 좋아져서 더욱더 글램핑 찾는 사람들이 많아졌어요",
          "speaker_id": "1",
          "time": "20211025 18:05"
        },
        {
          "id": "MDRW2100046484.1.40",
          "form": "그 매력에 한 번 빠지면",
          "original_form": "그 매력에 한 번 빠지면",
          "speaker_id": "2",
          "time": "20211025 18:07"
        },
        {
          "id": "MDRW2100046484.1.41",
          "form": "나오기 어려운거 같아요 글램핑이 특히~\r\n",
          "original_form": "나오기 어려운거 같아요 글램핑이 특히~\r\n",
