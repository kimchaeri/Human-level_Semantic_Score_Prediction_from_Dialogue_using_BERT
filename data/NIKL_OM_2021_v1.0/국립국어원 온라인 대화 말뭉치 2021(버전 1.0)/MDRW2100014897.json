      "utterance": [
        {
          "id": "MDRW2100014897.1.1",
          "form": "엄마 삼겹살 먹고싶다 그치?",
          "original_form": "엄마 삼겹살 먹고싶다 그치?",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014897.1.2",
          "form": "응 삼겹살 너무 먹고싶다",
          "original_form": "응 삼겹살 너무 먹고싶다",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014897.1.3",
          "form": "요즘 배달 삼겹살도 잘 되어있대",
          "original_form": "요즘 배달 삼겹살도 잘 되어있대",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014897.1.4",
          "form": "난 배달음식보다 식당가는게 좋다",
          "original_form": "난 배달음식보다 식당가는게 좋다",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014897.1.5",
          "form": "알지만 식당가기 힘들잖아",
          "original_form": "알지만 식당가기 힘들잖아",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014897.1.6",
          "form": "그래서 못먹고있지",
          "original_form": "그래서 못먹고있지",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014897.1.7",
          "form": "우리가 구워먹어도 되는데...",
          "original_form": "우리가 구워먹어도 되는데...",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014897.1.8",
          "form": "내가 준비해야 되는데 귀찮아",
          "original_form": "내가 준비해야 되는데 귀찮아",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014897.1.9",
          "form": "손이 많이 가는 음식이긴하지",
          "original_form": "손이 많이 가는 음식이긴하지",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014897.1.10",
          "form": "특히 기름 튀면 닦는게 힘들어",
          "original_form": "특히 기름 튀면 닦는게 힘들어",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014897.1.11",
          "form": "맞아 바닥이 난리나면 닦을때 고생해",
          "original_form": "맞아 바닥이 난리나면 닦을때 고생해",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014897.1.12",
          "form": "그래도 집에서 굽는게 제일 맛있긴해",
          "original_form": "그래도 집에서 굽는게 제일 맛있긴해",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014897.1.13",
          "form": "그건그래",
          "original_form": "그건그래",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014897.1.14",
          "form": "언제 날 잡고 해먹자",
          "original_form": "언제 날 잡고 해먹자",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014897.1.15",
          "form": "좋아용",
          "original_form": "좋아용",
          "speaker_id": "1",
