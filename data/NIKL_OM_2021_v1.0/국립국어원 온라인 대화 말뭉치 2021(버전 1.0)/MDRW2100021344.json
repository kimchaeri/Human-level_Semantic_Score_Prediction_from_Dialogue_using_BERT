      "utterance": [
        {
          "id": "MDRW2100021344.1.1",
          "form": "네 ㅋㅋ근데 또 이제 매운게 땡겨서 저녁땐 쭈꾸미 먹으려구요",
          "original_form": "네 ㅋㅋ근데 또 이제 매운게 땡겨서 저녁땐 쭈꾸미 먹으려구요",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021344.1.2",
          "form": "ㅋㅋㅋㅋ하남쭈꾸미인가요",
          "original_form": "ㅋㅋㅋㅋ하남쭈꾸미인가요",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021344.1.3",
          "form": "!!!!!!!!!!!!!!!!!!!",
          "original_form": "!!!!!!!!!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021344.1.4",
          "form": "세상에나",
          "original_form": "세상에나",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021344.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021344.1.6",
          "form": "정답입니다",
          "original_form": "정답입니다",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021344.1.7",
          "form": "넼ㅋㅋㅋㅋ언뜻 하남쭈꾸미 추천해주셨던게 기억이 나네요ㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋ언뜻 하남쭈꾸미 추천해주셨던게 기억이 나네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021344.1.8",
          "form": "ㅋㅋㅋㅋ맞아요 ㅋㅋㅋ 오늘도 맛있게 먹었습니다",
          "original_form": "ㅋㅋㅋㅋ맞아요 ㅋㅋㅋ 오늘도 맛있게 먹었습니다",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021344.1.9",
          "form": "저녁 드셨나용?ㅋㅋㅋㅋㅋ",
          "original_form": "저녁 드셨나용?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021344.1.10",
          "form": "네넹 아주 간단히 계란후라이랑 먹었어요",
          "original_form": "네넹 아주 간단히 계란후라이랑 먹었어요",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021344.1.11",
          "form": "ㅋㅋ맞아요 점심에 외식하면 저녁땐 많이 못먹겠더라구요",
          "original_form": "ㅋㅋ맞아요 점심에 외식하면 저녁땐 많이 못먹겠더라구요",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021344.1.12",
          "form": "크크 내일은 츠케멘 먹으러 갈거에요",
          "original_form": "크크 내일은 츠케멘 먹으러 갈거에요",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021344.1.13",
          "form": "우와 츠케멘은 일본라멘이에요? 저 처음들어봤어요!",
          "original_form": "우와 츠케멘은 일본라멘이에요? 저 처음들어봤어요!",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021344.1.14",
          "form": "네 라멘 종류인데 육수에 면 담가먹는 종류에요",
          "original_form": "네 라멘 종류인데 육수에 면 담가먹는 종류에요",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021344.1.15",
          "form": "우와 완전 맛있을것같아요 그러면 육수는 따뜻한 육수인거죠?",
          "original_form": "우와 완전 맛있을것같아요 그러면 육수는 따뜻한 육수인거죠?",
          "speaker_id": "1",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100021344.1.16",
          "form": "네네 된장육수나 간장육수 둘중에 골라서!",
          "original_form": "네네 된장육수나 간장육수 둘중에 골라서!",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100021344.1.17",
          "form": "면이 엄청 오동통통한 면이에요",
          "original_form": "면이 엄청 오동통통한 면이에요",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100021344.1.18",
          "form": "우와 저도 꼭 먹어볼래요!",
          "original_form": "우와 저도 꼭 먹어볼래요!",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100021344.1.19",
          "form": "면 담가먹는 우동은 먹어봤는데 라멘은 아직 못먹어봤어요!",
          "original_form": "면 담가먹는 우동은 먹어봤는데 라멘은 아직 못먹어봤어요!",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100021344.1.20",
          "form": "오 우동도 있군요! 근데 면은 비슷할거 같아요 ㅋㅋㅋ",
          "original_form": "오 우동도 있군요! 근데 면은 비슷할거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:38"
        }
      ]
