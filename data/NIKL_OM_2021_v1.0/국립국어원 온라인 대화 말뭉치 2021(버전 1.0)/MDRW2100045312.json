      "utterance": [
        {
          "id": "MDRW2100045312.1.1",
          "form": "name2님은 name3이 이 만질수 있나요!",
          "original_form": "&name2&님은 &name3&이 이 만질수 있나요!",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045312.1.2",
          "form": "엇.. 아뇨.. 저도 잘 못 만지게 해요..",
          "original_form": "엇.. 아뇨.. 저도 잘 못 만지게 해요..",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045312.1.3",
          "form": "맨정신일땐 불가능하고 name3이 자고있거나 졸려서 비몽사몽일때나 가능해욬ㅋㅋㅋ",
          "original_form": "맨정신일땐 불가능하고 &name3&이 자고있거나 졸려서 비몽사몽일때나 가능해욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045312.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ쉽지않군요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ쉽지않군요!",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045312.1.5",
          "form": "녜.. name3이 자기가 치대는건 괜찮지만 집사가 치대는건 안되는 고양이라서 그런지..",
          "original_form": "녜.. &name3&이 자기가 치대는건 괜찮지만 집사가 치대는건 안되는 고양이라서 그런지..",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045312.1.6",
          "form": "함부로 못 만지게 하기도 하고 그래욬ㅋㅋㅋ쿠ㅜ",
          "original_form": "함부로 못 만지게 하기도 하고 그래욬ㅋㅋㅋ쿠ㅜ",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045312.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 우시는거 아니죵 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 우시는거 아니죵 ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045312.1.8",
          "form": "name3는 지금 자나요?",
          "original_form": "&name3&는 지금 자나요?",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045312.1.9",
          "form": "퓨ㅠㅠ",
          "original_form": "퓨ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045312.1.10",
          "form": "네 name3이 지금 제 침대에서 자고 있어요 ㅋㅋㅋ",
          "original_form": "네 &name3&이 지금 제 침대에서 자고 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045312.1.11",
          "form": "저한테 등돌리고 있슴돠..",
          "original_form": "저한테 등돌리고 있슴돠..",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045312.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ잉 부러워요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ잉 부러워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045312.1.13",
          "form": "몸 붙이고 있어요?",
          "original_form": "몸 붙이고 있어요?",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045312.1.14",
          "form": "아니요오ㅠㅜ",
          "original_form": "아니요오ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045312.1.15",
          "form": "name3이 더운거 안좋아해서 잘 안붙어서 자요 ㅋㅋㅋ",
          "original_form": "&name3&이 더운거 안좋아해서 잘 안붙어서 자요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045312.1.16",
          "form": "끕...name3 차도남이네요ㅠ",
          "original_form": "끕...&name3& 차도남이네요ㅠ",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045312.1.17",
          "form": "그러게욬ㅋㅋ쿠ㅜ",
          "original_form": "그러게욬ㅋㅋ쿠ㅜ",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045312.1.18",
          "form": "좀 더 집사한테 치대줘도 좋을텐데 말이에요ㅜ",
          "original_form": "좀 더 집사한테 치대줘도 좋을텐데 말이에요ㅜ",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045312.1.19",
          "form": "아직 두살이라 그런걸수도 있지 않을까요! 고양이에 대해 모르지만요ㅠㅋㅋㅋㅋ",
          "original_form": "아직 두살이라 그런걸수도 있지 않을까요! 고양이에 대해 모르지만요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045312.1.20",
          "form": "앟ㅋㅋㅋ 정말 그런거면 좋겠어욬ㅋㅋㅋ",
          "original_form": "앟ㅋㅋㅋ 정말 그런거면 좋겠어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045312.1.21",
          "form": "그럼 name3이가 먼저 다가올때는 밥이나 간식 달라고 할땐가요?",
          "original_form": "그럼 &name3&이가 먼저 다가올때는 밥이나 간식 달라고 할땐가요?",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045312.1.22",
          "form": "네 대체로 그런 편이에욬ㅋㅋㅋ",
          "original_form": "네 대체로 그런 편이에욬ㅋㅋㅋ",
          "speaker_id": "2",
