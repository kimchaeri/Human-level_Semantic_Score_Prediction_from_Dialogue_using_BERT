      "utterance": [
        {
          "id": "MDRW2100036217.1.1",
          "form": "아 오늘 마 난리다.",
          "original_form": "아 오늘 마 난리다.",
          "speaker_id": "1",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100036217.1.2",
          "form": "오늘 지안이 자주 노는 친구 2명 놀러왔다가",
          "original_form": "오늘 지안이 자주 노는 친구 2명 놀러왔다가",
          "speaker_id": "1",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100036217.1.3",
          "form": "돌아가면서 언성높이고 싸워가꼬 셋 다 혼내고",
          "original_form": "돌아가면서 언성높이고 싸워가꼬 셋 다 혼내고",
          "speaker_id": "1",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100036217.1.4",
          "form": "아 친구 집에 데려오는 건 피곤하다",
          "original_form": "아 친구 집에 데려오는 건 피곤하다",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100036217.1.5",
          "form": "나도 자주 투친구들 집에 데려오는데",
          "original_form": "나도 자주 투친구들 집에 데려오는데",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036217.1.6",
          "form": "의도치 않게 세 명을 집에 데려온 적이 있어",
          "original_form": "의도치 않게 세 명을 집에 데려온 적이 있어",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036217.1.7",
          "form": "투포함해서 네 명이지",
          "original_form": "투포함해서 네 명이지",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036217.1.8",
          "form": "다신 데리 오지 않으리 다짐했다",
          "original_form": "다신 데리 오지 않으리 다짐했다",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036217.1.9",
          "form": "ㅋㅋㅋㅋ아 웃겨",
          "original_form": "ㅋㅋㅋㅋ아 웃겨",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036217.1.10",
          "form": "ㅋㅋㅋㅋㅋ오늘 그렇게 싸울거면 놀지말라고 함ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ오늘 그렇게 싸울거면 놀지말라고 함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036217.1.11",
          "form": "그러니깐 다들 조용..",
          "original_form": "그러니깐 다들 조용..",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036217.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 기분좋게 갔나??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 기분좋게 갔나??",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036217.1.13",
          "form": "안그래도 북적거리는데 그날까지 원이 놀이터안가고 바로 집에 온거야",
          "original_form": "안그래도 북적거리는데 그날까지 원이 놀이터안가고 바로 집에 온거야",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036217.1.14",
          "form": "더 정신없음",
          "original_form": "더 정신없음",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036217.1.15",
          "form": "ㅋㅋㅋㅋㅋ맞다맞다",
          "original_form": "ㅋㅋㅋㅋㅋ맞다맞다",
          "speaker_id": "1",
          "time": "20210831 08:38"
        },
        {
          "id": "MDRW2100036217.1.16",
          "form": "그렇게놀다가 또 놀이터가서",
          "original_form": "그렇게놀다가 또 놀이터가서",
          "speaker_id": "1",
          "time": "20210831 08:38"
        },
        {
          "id": "MDRW2100036217.1.17",
          "form": "놀고",
          "original_form": "놀고",
          "speaker_id": "1",
          "time": "20210831 08:38"
        },
        {
          "id": "MDRW2100036217.1.18",
          "form": "7시넘어가 헤어짐",
          "original_form": "7시넘어가 헤어짐",
          "speaker_id": "1",
          "time": "20210831 08:38"
        },
        {
          "id": "MDRW2100036217.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 으아 7시 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 으아 7시 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100036217.1.20",
          "form": "대단하네 ㅋㅋㅋㅋㄱ",
          "original_form": "대단하네 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100036217.1.21",
          "form": "그날 내가 투 친구들 갈때",
          "original_form": "그날 내가 투 친구들 갈때",
          "speaker_id": "2",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100036217.1.22",
          "form": "애들아 잘가^^ 다음에 또와",
          "original_form": "애들아 잘가^^ 다음에 또와",
          "speaker_id": "2",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100036217.1.23",
          "form": "해놓고 들어와서",
          "original_form": "해놓고 들어와서",
          "speaker_id": "2",
          "time": "20210831 09:06"
        },
        {
          "id": "MDRW2100036217.1.24",
          "form": "우리투한테",
          "original_form": "우리투한테",
          "speaker_id": "2",
          "time": "20210831 09:06"
        },
        {
          "id": "MDRW2100036217.1.25",
          "form": "이제 친구들데리고 집에못와 했더니",
          "original_form": "이제 친구들데리고 집에못와 했더니",
          "speaker_id": "2",
          "time": "20210831 09:06"
        },
        {
          "id": "MDRW2100036217.1.26",
          "form": "원이 하는 말\n 엄마 아까는 애들 갈 때 나중에 또 놀러와 했잖아\n 왜 거짓말해?? 라고 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "원이 하는 말\n 엄마 아까는 애들 갈 때 나중에 또 놀러와 했잖아\n 왜 거짓말해?? 라고 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:07"
        },
        {
          "id": "MDRW2100036217.1.27",
          "form": "ㅋㅋㅋㅋ아 웃겨 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아 웃겨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100036217.1.28",
          "form": "엄마가 잘못했네 ㅋㅋ",
          "original_form": "엄마가 잘못했네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100036217.1.29",
          "form": "왜 거짓말하누 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "왜 거짓말하누 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100036217.1.30",
          "form": "하긴 이해 간다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "하긴 이해 간다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:37"
        },
        {
          "id": "MDRW2100036217.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 그래 너무 웃겼음 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 그래 너무 웃겼음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100036217.1.32",
          "form": "애들앞에서 무슨 말을 못한다 정말",
          "original_form": "애들앞에서 무슨 말을 못한다 정말",
          "speaker_id": "2",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100036217.1.33",
          "form": "날씨가 이래서 그런지 아침에 너무 졸리네;;",
          "original_form": "날씨가 이래서 그런지 아침에 너무 졸리네;;",
          "speaker_id": "2",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100036217.1.34",
          "form": "ㅋㅋㅋ맞다 맞다. ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맞다 맞다. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100036217.1.35",
          "form": "애들 진짜 웃긴게 맨날 싸우고,  맨날 만나고 놀고 ㅋㅋ",
          "original_form": "애들 진짜 웃긴게 맨날 싸우고,  맨날 만나고 놀고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100036217.1.36",
          "form": "어느 장단에 맞춰야되는지 ㅋㅋㅋ",
          "original_form": "어느 장단에 맞춰야되는지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100036217.1.37",
          "form": "ㅋㅋㅋㄱㅋㄱ 어제도 수랑 대판 싸워서",
          "original_form": "ㅋㅋㅋㄱㅋㄱ 어제도 수랑 대판 싸워서",
          "speaker_id": "2",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100036217.1.38",
          "form": "싸웠다기 보다는 아들이 잘못해가",
          "original_form": "싸웠다기 보다는 아들이 잘못해가",
          "speaker_id": "2",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100036217.1.39",
          "form": "어후....진짜...",
          "original_form": "어후....진짜...",
          "speaker_id": "2",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100036217.1.40",
          "form": "웅웅 ㅋㅋㅋㅋ 아 웃겨 ㅋㅋ",
          "original_form": "웅웅 ㅋㅋㅋㅋ 아 웃겨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100036217.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아들이 잘못해가 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아들이 잘못해가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036217.1.42",
          "form": "수 폰에 있는 카톡이랑 무한의계단을 몰래 지워놓은거야",
          "original_form": "수 폰에 있는 카톡이랑 무한의계단을 몰래 지워놓은거야",
          "speaker_id": "2",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036217.1.43",
          "form": "지는 장난이지만",
          "original_form": "지는 장난이지만",
          "speaker_id": "2",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036217.1.44",
          "form": "웅웅 아이구",
          "original_form": "웅웅 아이구",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036217.1.45",
          "form": "ㅋㅋㅋㅋㅋ그랬네 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그랬네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036217.1.46",
          "form": "수 입장에서는 얼마나 열받노",
          "original_form": "수 입장에서는 얼마나 열받노",
          "speaker_id": "2",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036217.1.47",
          "form": "그렇징 ㅋㅋㅋㅋ",
          "original_form": "그렇징 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036217.1.48",
          "form": "내가 막 열받아가지고",
          "original_form": "내가 막 열받아가지고",
          "speaker_id": "2",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036217.1.49",
          "form": "무한의 계단 기록새운다고 그 고생(ㅋㅋㅋ)을 했는데",
          "original_form": "무한의 계단 기록새운다고 그 고생(ㅋㅋㅋ)을 했는데",
          "speaker_id": "2",
