      "utterance": [
        {
          "id": "MDRW2100018074.1.1",
          "form": "키워보고 싶은 동물 있어?",
          "original_form": "키워보고 싶은 동물 있어?",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018074.1.2",
          "form": "나는 고양이",
          "original_form": "나는 고양이",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018074.1.3",
          "form": "나도 고양이 키워보고 싶어",
          "original_form": "나도 고양이 키워보고 싶어",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018074.1.4",
          "form": "귀여운 애들은 진짜 너무 귀엽더라",
          "original_form": "귀여운 애들은 진짜 너무 귀엽더라",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018074.1.5",
          "form": "고양이 애기일 때 넘 귀여워",
          "original_form": "고양이 애기일 때 넘 귀여워",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018074.1.6",
          "form": "어린 고양이 진짜 너무 귀여워",
          "original_form": "어린 고양이 진짜 너무 귀여워",
          "speaker_id": "1",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018074.1.7",
          "form": "그니까 키워보고 싶어",
          "original_form": "그니까 키워보고 싶어",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018074.1.8",
          "form": "그리고 고양이는 혼자서도 잘 놀아서 더 좋은 것 같아",
          "original_form": "그리고 고양이는 혼자서도 잘 놀아서 더 좋은 것 같아",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018074.1.9",
          "form": "그리고 잘 안와서 좋아",
          "original_form": "그리고 잘 안와서 좋아",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018074.1.10",
          "form": "그치 막 엉겨 붙고 그러지 않으니까",
          "original_form": "그치 막 엉겨 붙고 그러지 않으니까",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018074.1.11",
          "form": "맞아 그냥 조용히 와 있어서 좋아",
          "original_form": "맞아 그냥 조용히 와 있어서 좋아",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018074.1.12",
          "form": "조용이 와서 좀 놀아주다가 조용히 가고",
          "original_form": "조용이 와서 좀 놀아주다가 조용히 가고",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018074.1.13",
          "form": "ㅋㅋㅋㅋ 그래서 좋아",
          "original_form": "ㅋㅋㅋㅋ 그래서 좋아",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018074.1.14",
          "form": "만약에 애완동물 기르면 이름 뭘로 하고 싶어?",
          "original_form": "만약에 애완동물 기르면 이름 뭘로 하고 싶어?",
