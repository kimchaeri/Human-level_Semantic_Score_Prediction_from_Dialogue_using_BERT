      "utterance": [
        {
          "id": "MDRW2100018070.1.1",
          "form": "결혼했어?",
          "original_form": "결혼했어?",
          "speaker_id": "1",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018070.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018070.1.3",
          "form": "결혼하셨나요",
          "original_form": "결혼하셨나요",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018070.1.4",
          "form": "저도 안했어요",
          "original_form": "저도 안했어요",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018070.1.5",
          "form": "결혼은 꼭 해야 할까요?",
          "original_form": "결혼은 꼭 해야 할까요?",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018070.1.6",
          "form": "남자와 여자가 결혼에대해 생각차이가 많이 나더라구요",
          "original_form": "남자와 여자가 결혼에대해 생각차이가 많이 나더라구요",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018070.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018070.1.8",
          "form": "혹시 성별이 ??",
          "original_form": "혹시 성별이 ??",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018070.1.9",
          "form": "전 남자에요",
          "original_form": "전 남자에요",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018070.1.10",
          "form": "전 여자에요",
          "original_form": "전 여자에요",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018070.1.11",
          "form": "님은 결혼에 대해서 어떻게 생각하시나요",
          "original_form": "님은 결혼에 대해서 어떻게 생각하시나요",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018070.1.12",
          "form": "저는 반반입니다",
          "original_form": "저는 반반입니다",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018070.1.13",
          "form": "해도 좋고 안해도 좋고",
          "original_form": "해도 좋고 안해도 좋고",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100018070.1.14",
          "form": "왜 결혼은 꼭 해야하는거죠?",
          "original_form": "왜 결혼은 꼭 해야하는거죠?",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100018070.1.15",
          "form": "음 정말 사랑하는사람끼리 결혼해도 싸우기는 하더라구요",
          "original_form": "음 정말 사랑하는사람끼리 결혼해도 싸우기는 하더라구요",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100018070.1.16",
          "form": "인간은 불완전한 존재이기때문에 서로 보완해줄 수 있는 존재가 필요하다 봅니다",
          "original_form": "인간은 불완전한 존재이기때문에 서로 보완해줄 수 있는 존재가 필요하다 봅니다",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018070.1.17",
          "form": "멋진 말씀이네요",
          "original_form": "멋진 말씀이네요",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018070.1.18",
          "form": "심리적 안정에는 좋은 거 같긴해여",
          "original_form": "심리적 안정에는 좋은 거 같긴해여",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018070.1.19",
          "form": "그리고 나이가 먹을수록 믿고의지할 수 있는 사람이 없으면 외로움과 쓸쓸함 허탈감 같은 부정적인 생각이 쌓일거구요",
          "original_form": "그리고 나이가 먹을수록 믿고의지할 수 있는 사람이 없으면 외로움과 쓸쓸함 허탈감 같은 부정적인 생각이 쌓일거구요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018070.1.20",
          "form": "미운정도 정 이라는 말이 있잖아요?",
          "original_form": "미운정도 정 이라는 말이 있잖아요?",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018070.1.21",
          "form": "그렇겠죠?",
          "original_form": "그렇겠죠?",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018070.1.22",
          "form": "나아니면 얘랑 누가 살아주냐",
          "original_form": "나아니면 얘랑 누가 살아주냐",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018070.1.23",
          "form": "이런마음 가지고 사는분들도 더러 계시더라구요",
          "original_form": "이런마음 가지고 사는분들도 더러 계시더라구요",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018070.1.24",
          "form": "아 조만간 결혼 계획이 있으신가요?",
          "original_form": "아 조만간 결혼 계획이 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018070.1.25",
          "form": "아뇨 20대후반이라",
          "original_form": "아뇨 20대후반이라",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018070.1.26",
          "form": "30대 초중쯤 할겁니다",
          "original_form": "30대 초중쯤 할겁니다",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018070.1.27",
          "form": "선생님은 나이가 어떻게되시길래",
          "original_form": "선생님은 나이가 어떻게되시길래",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018070.1.28",
          "form": "고민중이신가요",
          "original_form": "고민중이신가요",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018070.1.29",
          "form": "선생님이라뇨",
          "original_form": "선생님이라뇨",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018070.1.30",
          "form": "저는 삼십대 초중쯤이에요",
          "original_form": "저는 삼십대 초중쯤이에요",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018070.1.31",
          "form": "아 제가 모르는분 일면식없는분 아니면 초면인분 들은 선생님 이라는 호칭으로 부릅니다",
          "original_form": "아 제가 모르는분 일면식없는분 아니면 초면인분 들은 선생님 이라는 호칭으로 부릅니다",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018070.1.32",
          "form": "듣는분들 기분안나쁘게",
          "original_form": "듣는분들 기분안나쁘게",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018070.1.33",
          "form": "네 아주 바람직하시네요",
          "original_form": "네 아주 바람직하시네요",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018070.1.34",
          "form": "초면에 반말 죄송해요",
          "original_form": "초면에 반말 죄송해요",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018070.1.35",
          "form": "아까전에",
          "original_form": "아까전에",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018070.1.36",
          "form": "간호사님도 이모 언니 이렇게 부르시는분들이 계시더라구요",
          "original_form": "간호사님도 이모 언니 이렇게 부르시는분들이 계시더라구요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018070.1.37",
          "form": "아 아니에요",
          "original_form": "아 아니에요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018070.1.38",
          "form": "아니 오늘 첨 깔았는데 다들 반말 하더라구여",
          "original_form": "아니 오늘 첨 깔았는데 다들 반말 하더라구여",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018070.1.39",
          "form": "룰인줄",
          "original_form": "룰인줄",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018070.1.40",
          "form": "허심탄회하게 속마음을 털어내시고 싶으신거같은데",
          "original_form": "허심탄회하게 속마음을 털어내시고 싶으신거같은데",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018070.1.41",
          "form": "제가여?",
          "original_form": "제가여?",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018070.1.42",
          "form": "편하게하셔도되요",
          "original_form": "편하게하셔도되요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018070.1.43",
          "form": "사실 20 30이",
          "original_form": "사실 20 30이",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018070.1.44",
          "form": "3포 5포세대다",
          "original_form": "3포 5포세대다",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018070.1.45",
          "form": "말이나와서",
          "original_form": "말이나와서",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018070.1.46",
          "form": "제 주변은 결혼 한 사람 반 안한사람 반이라",
          "original_form": "제 주변은 결혼 한 사람 반 안한사람 반이라",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018070.1.47",
          "form": "진지하게 고민중이신가 넘겨짚었네요",
          "original_form": "진지하게 고민중이신가 넘겨짚었네요",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018070.1.48",
          "form": "죄송합니다",
          "original_form": "죄송합니다",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018070.1.49",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018070.1.50",
          "form": "아닙니다",
          "original_form": "아닙니다",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018070.1.51",
          "form": "음 저는 부모님의 영향도 있어서",
          "original_form": "음 저는 부모님의 영향도 있어서",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018070.1.52",
          "form": "암든 아직 어리시니 행복한 연애 많이 하시고",
          "original_form": "암든 아직 어리시니 행복한 연애 많이 하시고",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018070.1.53",
          "form": "결혼을 아주 좋게보는편이라",
          "original_form": "결혼을 아주 좋게보는편이라",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018070.1.54",
          "form": "계획대로 결혼하시길 바래여",
          "original_form": "계획대로 결혼하시길 바래여",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018070.1.55",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018070.1.56",
          "form": "살아보니",
          "original_form": "살아보니",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018070.1.57",
          "form": "금전적인게 풍족하면",
          "original_form": "금전적인게 풍족하면",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018070.1.58",
          "form": "결혼도 행복하고",
          "original_form": "결혼도 행복하고",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018070.1.59",
          "form": "부족하면 덜 행복해 보이더라구여",
          "original_form": "부족하면 덜 행복해 보이더라구여",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018070.1.60",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018070.1.61",
          "form": "마음의 여유는 지갑의 두께에서 나오는거죠",
          "original_form": "마음의 여유는 지갑의 두께에서 나오는거죠",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018070.1.62",
          "form": "부정안합니다",
          "original_form": "부정안합니다",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018070.1.63",
          "form": "열심히 노력해서 각자 맘도 돈도 부자가 되길 바래여",
          "original_form": "열심히 노력해서 각자 맘도 돈도 부자가 되길 바래여",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018070.1.64",
          "form": "초면이지만 응원 합니다",
          "original_form": "초면이지만 응원 합니다",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
