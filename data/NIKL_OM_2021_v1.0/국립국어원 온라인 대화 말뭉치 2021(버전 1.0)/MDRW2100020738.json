      "utterance": [
        {
          "id": "MDRW2100020738.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020738.1.2",
          "form": "다이어트 해본 적 있어요?",
          "original_form": "다이어트 해본 적 있어요?",
          "speaker_id": "1",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020738.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020738.1.4",
          "form": "딱히 안하는 편이에요",
          "original_form": "딱히 안하는 편이에요",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020738.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020738.1.6",
          "form": "저는 성공했는데 너무 좋아서 유지중이에요",
          "original_form": "저는 성공했는데 너무 좋아서 유지중이에요",
          "speaker_id": "1",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020738.1.7",
          "form": "어떤식으로 하셨나요?",
          "original_form": "어떤식으로 하셨나요?",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020738.1.8",
          "form": "간헐적단식을 필수로 하구요",
          "original_form": "간헐적단식을 필수로 하구요",
          "speaker_id": "1",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020738.1.9",
          "form": "탄수화물만 최대한 적게 먹으려고 노력했어요",
          "original_form": "탄수화물만 최대한 적게 먹으려고 노력했어요",
          "speaker_id": "1",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020738.1.10",
          "form": "단백질같은건 안드시구요?",
          "original_form": "단백질같은건 안드시구요?",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020738.1.11",
          "form": "단백질/지방 위주로 식사했어욯ㅎ",
          "original_form": "단백질/지방 위주로 식사했어욯ㅎ",
          "speaker_id": "1",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020738.1.12",
          "form": "그래서 고기/채소 많이 먹었네용",
          "original_form": "그래서 고기/채소 많이 먹었네용",
          "speaker_id": "1",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100020738.1.13",
          "form": "아 탄수화물을 적게",
          "original_form": "아 탄수화물을 적게",
          "speaker_id": "2",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100020738.1.14",
          "form": "운동도 병행하셨나욤9",
          "original_form": "운동도 병행하셨나욤9",
          "speaker_id": "2",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100020738.1.15",
          "form": "초창기 땐 운동도 병행했어요 ㅎㅎ 그때가 가을~겨울쯤이여서",
          "original_form": "초창기 땐 운동도 병행했어요 ㅎㅎ 그때가 가을~겨울쯤이여서",
          "speaker_id": "1",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100020738.1.16",
          "form": "그냥 유산소 위주로만 했어요 걷기..달리기 끝..",
          "original_form": "그냥 유산소 위주로만 했어요 걷기..달리기 끝..",
          "speaker_id": "1",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100020738.1.17",
          "form": "확실히 식단이 중요하네요..",
          "original_form": "확실히 식단이 중요하네요..",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100020738.1.18",
          "form": "지금도 그렇게 드세요?",
          "original_form": "지금도 그렇게 드세요?",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100020738.1.19",
          "form": "지금은 좀 풀어져가지고 빵도 먹고 떡도 먹고 그랬어요 ㅜㅜ",
          "original_form": "지금은 좀 풀어져가지고 빵도 먹고 떡도 먹고 그랬어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 12:01"
        },
        {
          "id": "MDRW2100020738.1.20",
          "form": "케이크도 먹고 ㅋㅋㅋ 그래도 잘 유지가 되더라구요?",
          "original_form": "케이크도 먹고 ㅋㅋㅋ 그래도 잘 유지가 되더라구요?",
          "speaker_id": "1",
          "time": "20210828 12:01"
        },
        {
          "id": "MDRW2100020738.1.21",
          "form": "완전 성공하셨네요~~",
          "original_form": "완전 성공하셨네요~~",
          "speaker_id": "2",
          "time": "20210828 12:02"
        },
        {
          "id": "MDRW2100020738.1.22",
          "form": "빼는게 정말 어렵던데",
          "original_form": "빼는게 정말 어렵던데",
          "speaker_id": "2",
          "time": "20210828 12:02"
        },
        {
          "id": "MDRW2100020738.1.23",
          "form": "군살은 몇년걸려야 겨우빠지더라고요",
          "original_form": "군살은 몇년걸려야 겨우빠지더라고요",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100020738.1.24",
          "form": "앗 군살 ㅋㅋㅋ 진짜 스트레스에요",
          "original_form": "앗 군살 ㅋㅋㅋ 진짜 스트레스에요",
          "speaker_id": "1",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100020738.1.25",
          "form": "제가 어제 곰곰히 생각해봤는데 근량이 부족해서 군살 있는 것 같아요 ㅜㅜ",
          "original_form": "제가 어제 곰곰히 생각해봤는데 근량이 부족해서 군살 있는 것 같아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 12:03"
        }
      ]
