      "utterance": [
        {
          "id": "MDRW2100006482.1.1",
          "form": "오늘 이상민이 나오는 그거 재밌었어",
          "original_form": "오늘 이상민이 나오는 그거 재밌었어",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006482.1.2",
          "form": "아 에스비에스에서 하는거요??",
          "original_form": "아 에스비에스에서 하는거요??",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006482.1.3",
          "form": "응 돌싱어쩌구 프로 던데 이름이 기억이 안나",
          "original_form": "응 돌싱어쩌구 프로 던데 이름이 기억이 안나",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006482.1.4",
          "form": "아 김준호도 나와요",
          "original_form": "아 김준호도 나와요",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006482.1.5",
          "form": "맞아   탁재훈 도 나와",
          "original_form": "맞아   탁재훈 도 나와",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006482.1.6",
          "form": "춤도 추던데요",
          "original_form": "춤도 추던데요",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006482.1.7",
          "form": "그건 지난주에 나왔어",
          "original_form": "그건 지난주에 나왔어",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006482.1.8",
          "form": "오늘은 소개팅 했어",
          "original_form": "오늘은 소개팅 했어",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006482.1.9",
          "form": "어이쿠",
          "original_form": "어이쿠",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006482.1.10",
          "form": "잘 아시네여",
          "original_form": "잘 아시네여",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006482.1.11",
          "form": "매번 틀 때마다 해",
          "original_form": "매번 틀 때마다 해",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006482.1.12",
          "form": "그런거 같아요",
          "original_form": "그런거 같아요",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006482.1.13",
          "form": "미우새 나오는 사람들이 다 나와",
          "original_form": "미우새 나오는 사람들이 다 나와",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006482.1.14",
          "form": "미우새가 인기가 많아서 그런가봐요",
          "original_form": "미우새가 인기가 많아서 그런가봐요",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006482.1.15",
          "form": "그런가봐  걔네들이 인기가 많아져서 나오나봐",
          "original_form": "그런가봐  걔네들이 인기가 많아져서 나오나봐",
          "speaker_id": "2",
          "time": "20210908 00:51"
        }
