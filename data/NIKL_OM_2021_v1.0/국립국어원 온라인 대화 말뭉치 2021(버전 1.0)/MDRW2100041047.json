      "utterance": [
        {
          "id": "MDRW2100041047.1.1",
          "form": "그여자의 바다에서도 주연을 하고",
          "original_form": "그여자의 바다에서도 주연을 하고",
          "speaker_id": "2",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100041047.1.2",
          "form": "그 여자의 바다?  드라마를 모르겠네요",
          "original_form": "그 여자의 바다?  드라마를 모르겠네요",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100041047.1.3",
          "form": "벌써 결혼을 했구나",
          "original_form": "벌써 결혼을 했구나",
          "speaker_id": "2",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100041047.1.4",
          "form": "내가 알던 애가 아니네요",
          "original_form": "내가 알던 애가 아니네요",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100041047.1.5",
          "form": "KBS1 아침드라마라서 몰랐을껄",
          "original_form": "KBS1 아침드라마라서 몰랐을껄",
          "speaker_id": "2",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100041047.1.6",
          "form": "레인보우 오승아",
          "original_form": "레인보우 오승아",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041047.1.7",
          "form": "88번지 라는 드라마도 있엇네",
          "original_form": "88번지 라는 드라마도 있엇네",
          "speaker_id": "2",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041047.1.8",
          "form": "가수했던 애 였네요",
          "original_form": "가수했던 애 였네요",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041047.1.9",
          "form": "결혼 안했어요",
          "original_form": "결혼 안했어요",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041047.1.10",
          "form": "가수 탤랜트라고 하니 맞는거 같은데",
          "original_form": "가수 탤랜트라고 하니 맞는거 같은데",
          "speaker_id": "2",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041047.1.11",
          "form": "얼굴 오늘 처음 봤어요",
          "original_form": "얼굴 오늘 처음 봤어요",
          "speaker_id": "1",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100041047.1.12",
          "form": "레인보우 맞데 난도 첨본다",
          "original_form": "레인보우 맞데 난도 첨본다",
          "speaker_id": "2",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100041047.1.13",
          "form": "ㅋㅋ  여아이돌 다 연기하네",
          "original_form": "ㅋㅋ  여아이돌 다 연기하네",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100041047.1.14",
          "form": "근데 출연작이 엄청 많네 거의 주연이었고",
          "original_form": "근데 출연작이 엄청 많네 거의 주연이었고",
