      "utterance": [
        {
          "id": "MDRW2100036625.1.1",
          "form": "아뇨 그랬으면 스마일데이 세일이럴때",
          "original_form": "아뇨 그랬으면 스마일데이 세일이럴때",
          "speaker_id": "2",
          "time": "20210831 02:36"
        },
        {
          "id": "MDRW2100036625.1.2",
          "form": "체크해서 잘샀을텐데..",
          "original_form": "체크해서 잘샀을텐데..",
          "speaker_id": "2",
          "time": "20210831 02:36"
        },
        {
          "id": "MDRW2100036625.1.3",
          "form": "ㅎㅎ 다음 빅스마일데이를 노려보시는건 어떠세요?",
          "original_form": "ㅎㅎ 다음 빅스마일데이를 노려보시는건 어떠세요?",
          "speaker_id": "1",
          "time": "20210831 02:36"
        },
        {
          "id": "MDRW2100036625.1.4",
          "form": "그냥 옷가개구경하듯 마트구경하러 자주갈쁀입니다..",
          "original_form": "그냥 옷가개구경하듯 마트구경하러 자주갈쁀입니다..",
          "speaker_id": "2",
          "time": "20210831 02:36"
        },
        {
          "id": "MDRW2100036625.1.5",
          "form": "아하 저도 그래요ㅋㅋㅋㅋ",
          "original_form": "아하 저도 그래요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:37"
        },
        {
          "id": "MDRW2100036625.1.6",
          "form": "근데도 놓치는게 많네요",
          "original_form": "근데도 놓치는게 많네요",
          "speaker_id": "1",
          "time": "20210831 02:37"
        },
        {
          "id": "MDRW2100036625.1.7",
          "form": "그래야겠어요 ㅜ 계획형이아니라서",
          "original_form": "그래야겠어요 ㅜ 계획형이아니라서",
          "speaker_id": "2",
          "time": "20210831 02:37"
        },
        {
          "id": "MDRW2100036625.1.8",
          "form": "또 까먹을거같긴한데",
          "original_form": "또 까먹을거같긴한데",
          "speaker_id": "2",
          "time": "20210831 02:37"
        },
        {
          "id": "MDRW2100036625.1.9",
          "form": "혹시 갤럭시 버즈 구매하셨어요?",
          "original_form": "혹시 갤럭시 버즈 구매하셨어요?",
          "speaker_id": "1",
          "time": "20210831 02:37"
        },
        {
          "id": "MDRW2100036625.1.10",
          "form": "네 공짜로받았어요",
          "original_form": "네 공짜로받았어요",
          "speaker_id": "2",
          "time": "20210831 02:37"
        },
        {
          "id": "MDRW2100036625.1.11",
          "form": "저 요즘 이게 너무 갖고싶은데 내일 할인률이 높으면 사볼까 하거든요",
          "original_form": "저 요즘 이게 너무 갖고싶은데 내일 할인률이 높으면 사볼까 하거든요",
          "speaker_id": "1",
          "time": "20210831 02:37"
        },
        {
          "id": "MDRW2100036625.1.12",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210831 02:37"
        },
        {
          "id": "MDRW2100036625.1.13",
          "form": "저 이번년도에 폰을 샀었어서",
          "original_form": "저 이번년도에 폰을 샀었어서",
          "speaker_id": "2",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100036625.1.14",
          "form": "21사니까 주드라구여",
          "original_form": "21사니까 주드라구여",
          "speaker_id": "2",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100036625.1.15",
          "form": "아하 혹시 돈주고 구매하라고 했다면",
          "original_form": "아하 혹시 돈주고 구매하라고 했다면",
          "speaker_id": "1",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100036625.1.16",
          "form": "사셨을것같나요?",
          "original_form": "사셨을것같나요?",
          "speaker_id": "1",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100036625.1.17",
          "form": "전 이런거 돈주고잘안사는데 50퍼할인쿠폰있음 샀을거같아요",
          "original_form": "전 이런거 돈주고잘안사는데 50퍼할인쿠폰있음 샀을거같아요",
          "speaker_id": "2",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100036625.1.18",
          "form": "오~ 나쁘진 않나보네요!",
          "original_form": "오~ 나쁘진 않나보네요!",
          "speaker_id": "1",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100036625.1.19",
          "form": "대체상품이있는애들이잖아요 유선쓰면되고",
          "original_form": "대체상품이있는애들이잖아요 유선쓰면되고",
          "speaker_id": "2",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036625.1.20",
          "form": "저는 화이트로 사고싶은데 때 탈까봐 걱정이네요",
          "original_form": "저는 화이트로 사고싶은데 때 탈까봐 걱정이네요",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036625.1.21",
          "form": "아하 그래서...!",
          "original_form": "아하 그래서...!",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036625.1.22",
          "form": "근데 확실히 편하긴하더라구여",
          "original_form": "근데 확실히 편하긴하더라구여",
          "speaker_id": "2",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036625.1.23",
          "form": "쓰고나니 이제유선못써여 ㅋㅋㅋㅋ",
          "original_form": "쓰고나니 이제유선못써여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036625.1.24",
          "form": "내일 좀만 더 고민해봐야겠어요ㅋㅋㅋ",
          "original_form": "내일 좀만 더 고민해봐야겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036625.1.25",
          "form": "오~~~",
          "original_form": "오~~~",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036625.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036625.1.27",
          "form": "운동할때넘좋아요",
          "original_form": "운동할때넘좋아요",
