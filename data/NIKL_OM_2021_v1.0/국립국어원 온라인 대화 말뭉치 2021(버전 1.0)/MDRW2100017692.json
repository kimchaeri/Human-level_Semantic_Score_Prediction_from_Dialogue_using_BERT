      "utterance": [
        {
          "id": "MDRW2100017692.1.1",
          "form": "최근에 여행 다녀오셧나요?",
          "original_form": "최근에 여행 다녀오셧나요?",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017692.1.2",
          "form": "최근엔 못갔어요 ㅜㅜ",
          "original_form": "최근엔 못갔어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017692.1.3",
          "form": "조만간 다녀오실 계획은 있으신가요??",
          "original_form": "조만간 다녀오실 계획은 있으신가요??",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017692.1.4",
          "form": "코로롱 좀 나아지면 해외여행가고싶어요~",
          "original_form": "코로롱 좀 나아지면 해외여행가고싶어요~",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017692.1.5",
          "form": "너무 귀엽네요 코로롱",
          "original_form": "너무 귀엽네요 코로롱",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017692.1.6",
          "form": "어디 제일 먼저 가고싶으신가요?",
          "original_form": "어디 제일 먼저 가고싶으신가요?",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017692.1.7",
          "form": "앜ㅋㅋㅋ 저는 호주 가보고싶어요~",
          "original_form": "앜ㅋㅋㅋ 저는 호주 가보고싶어요~",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017692.1.8",
          "form": "호주 너무 좋을 것 같아요",
          "original_form": "호주 너무 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017692.1.9",
          "form": "쿼카?",
          "original_form": "쿼카?",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017692.1.10",
          "form": "실제로 봐보고싶어요",
          "original_form": "실제로 봐보고싶어요",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017692.1.11",
          "form": "앜ㅋㅋㅋ헐 저희집에쿼카인형있는데 넘귀여워욬ㅋㅋ",
          "original_form": "앜ㅋㅋㅋ헐 저희집에쿼카인형있는데 넘귀여워욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017692.1.12",
          "form": "대박ㅌㅋㅋㅋㅋㅋ name2도 쿼카때문에 호주 가고싶으신건가요??",
          "original_form": "대박ㅌㅋㅋㅋㅋㅋ &name2&도 쿼카때문에 호주 가고싶으신건가요??",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017692.1.13",
          "form": "음 꼭그것뿐만은 아니에욬ㅋㅋㅋ 전 크리스마스때 꼭 가보고싶어요~분위기가 어떨지 궁금하거든요",
          "original_form": "음 꼭그것뿐만은 아니에욬ㅋㅋㅋ 전 크리스마스때 꼭 가보고싶어요~분위기가 어떨지 궁금하거든요",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017692.1.14",
          "form": "오 호주가 크리스마스 이벤트? 같은걸 많이하나봐요?",
          "original_form": "오 호주가 크리스마스 이벤트? 같은걸 많이하나봐요?",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017692.1.15",
          "form": "마지막으로 간 여행지는 어디엿나요?",
          "original_form": "마지막으로 간 여행지는 어디엿나요?",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017692.1.16",
          "form": "아 호주가 저희랑 계절이 반대라 더운크리스마스가 궁금하더라구요~",
          "original_form": "아 호주가 저희랑 계절이 반대라 더운크리스마스가 궁금하더라구요~",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017692.1.17",
          "form": "저는 젤마지막으로 스페인다녀왓어요 ㅎㅎ",
          "original_form": "저는 젤마지막으로 스페인다녀왓어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017692.1.18",
          "form": "아 물론 코로롱 전에요",
          "original_form": "아 물론 코로롱 전에요",
          "speaker_id": "2",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017692.1.19",
          "form": "스페인 너무 좋았을 것 같아요!",
          "original_form": "스페인 너무 좋았을 것 같아요!",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017692.1.20",
          "form": "좋았어요! 오렌지 실컷먹고왔어요 ㅋㅋ",
          "original_form": "좋았어요! 오렌지 실컷먹고왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:03"
        }
      ]
