      "utterance": [
        {
          "id": "MDRW2100046925.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211108 11:58"
        },
        {
          "id": "MDRW2100046925.1.2",
          "form": "안녕하세요! 주말은 어떻게 보내셨나요",
          "original_form": "안녕하세요! 주말은 어떻게 보내셨나요",
          "speaker_id": "2",
          "time": "20211108 11:59"
        },
        {
          "id": "MDRW2100046925.1.3",
          "form": "주말에 출근했어요!",
          "original_form": "주말에 출근했어요!",
          "speaker_id": "1",
          "time": "20211109 00:40"
        },
        {
          "id": "MDRW2100046925.1.4",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20211109 00:40"
        },
        {
          "id": "MDRW2100046925.1.5",
          "form": "부럽죠?",
          "original_form": "부럽죠?",
          "speaker_id": "1",
          "time": "20211109 00:40"
        },
        {
          "id": "MDRW2100046925.1.6",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20211109 00:40"
        },
        {
          "id": "MDRW2100046925.1.7",
          "form": "아이고 ㅠㅠ 원래 매주 주말에 출근하시나요?",
          "original_form": "아이고 ㅠㅠ 원래 매주 주말에 출근하시나요?",
          "speaker_id": "2",
          "time": "20211109 00:40"
        },
        {
          "id": "MDRW2100046925.1.8",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046925.1.9",
          "form": "이번주는 일이 바빠서",
          "original_form": "이번주는 일이 바빠서",
          "speaker_id": "1",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046925.1.10",
          "form": "많이 바쁘셨나봐요 ㅠㅠ",
          "original_form": "많이 바쁘셨나봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046925.1.11",
          "form": "어쩔수없이",
          "original_form": "어쩔수없이",
          "speaker_id": "1",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046925.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046925.1.13",
          "form": "그래도 이번주는 쉬어요",
          "original_form": "그래도 이번주는 쉬어요",
          "speaker_id": "1",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046925.1.14",
          "form": "오 다행입니다",
          "original_form": "오 다행입니다",
          "speaker_id": "2",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046925.1.15",
          "form": "뭐하면서 쉬실 예정인가요",
          "original_form": "뭐하면서 쉬실 예정인가요",
          "speaker_id": "2",
          "time": "20211109 00:41"
        },
        {
          "id": "MDRW2100046925.1.16",
          "form": "name2님도 주말에 출근한적있어신가요",
          "original_form": "&name2&님도 주말에 출근한적있어신가요",
          "speaker_id": "1",
          "time": "20211109 00:42"
        },
        {
          "id": "MDRW2100046925.1.17",
          "form": "집에서 티비보고 휴대폰보면서 뒹굴뒹굴 할꺼에요",
          "original_form": "집에서 티비보고 휴대폰보면서 뒹굴뒹굴 할꺼에요",
          "speaker_id": "1",
          "time": "20211109 00:42"
        },
        {
          "id": "MDRW2100046925.1.18",
          "form": "아뇨 저는.. 교수님 호출 없으면 나갈 일이 없어요..",
          "original_form": "아뇨 저는.. 교수님 호출 없으면 나갈 일이 없어요..",
          "speaker_id": "2",
          "time": "20211109 00:42"
        },
        {
          "id": "MDRW2100046925.1.19",
          "form": "학교에서 일하시나봐요?",
          "original_form": "학교에서 일하시나봐요?",
          "speaker_id": "1",
          "time": "20211109 00:42"
        },
        {
          "id": "MDRW2100046925.1.20",
          "form": "넵.. ㅋㅋ",
          "original_form": "넵.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20211109 00:43"
        },
        {
          "id": "MDRW2100046925.1.21",
          "form": "역시 쉴 때는 침대를 벗어나면 안돼요",
          "original_form": "역시 쉴 때는 침대를 벗어나면 안돼요",
          "speaker_id": "2",
          "time": "20211109 00:43"
        },
        {
          "id": "MDRW2100046925.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211109 00:45"
        },
        {
          "id": "MDRW2100046925.1.23",
          "form": "침대랑 한몸이 되고싶어요",
          "original_form": "침대랑 한몸이 되고싶어요",
          "speaker_id": "1",
          "time": "20211109 00:45"
        },
        {
          "id": "MDRW2100046925.1.24",
          "form": "모쪼록 잘 쉬시길 바랍니다!",
          "original_form": "모쪼록 잘 쉬시길 바랍니다!",
          "speaker_id": "2",
