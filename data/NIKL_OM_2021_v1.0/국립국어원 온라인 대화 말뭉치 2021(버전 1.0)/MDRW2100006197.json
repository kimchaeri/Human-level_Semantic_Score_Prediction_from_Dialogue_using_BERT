      "utterance": [
        {
          "id": "MDRW2100006197.1.1",
          "form": "비가 많이 오네요",
          "original_form": "비가 많이 오네요",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006197.1.2",
          "form": "여기도 하루종일 비가 오네요",
          "original_form": "여기도 하루종일 비가 오네요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006197.1.3",
          "form": "너무 많이와요",
          "original_form": "너무 많이와요",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006197.1.4",
          "form": "지금은 조금씩 오고 있네요",
          "original_form": "지금은 조금씩 오고 있네요",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100006197.1.5",
          "form": "계속 와서 ㅎㅎ",
          "original_form": "계속 와서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006197.1.6",
          "form": "가을 장마라고 하더니 비가 자주오네요",
          "original_form": "가을 장마라고 하더니 비가 자주오네요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006197.1.7",
          "form": "그러게요ㅎㅎ그만오지",
          "original_form": "그러게요ㅎㅎ그만오지",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006197.1.8",
          "form": "날씨도점점 추워지네요",
          "original_form": "날씨도점점 추워지네요",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006197.1.9",
          "form": "이제 가을이네요 진짜",
          "original_form": "이제 가을이네요 진짜",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006197.1.10",
          "form": "여름 안녕",
          "original_form": "여름 안녕",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006197.1.11",
          "form": "좀 있으면 겨울 이겠죠",
          "original_form": "좀 있으면 겨울 이겠죠",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006197.1.12",
          "form": "벌써 겨울이라니",
          "original_form": "벌써 겨울이라니",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006197.1.13",
          "form": "일년이 빨리 지나가서 겨울 문턱 이네요",
          "original_form": "일년이 빨리 지나가서 겨울 문턱 이네요",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006197.1.14",
          "form": "진짜 봄여름가을겨울",
          "original_form": "진짜 봄여름가을겨울",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006197.1.15",
          "form": "순식간이네요",
          "original_form": "순식간이네요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006197.1.16",
          "form": "사계절이 순식간으로 지나가 버렸네요",
          "original_form": "사계절이 순식간으로 지나가 버렸네요",
          "speaker_id": "2",
          "time": "20210907 22:09"
        }
