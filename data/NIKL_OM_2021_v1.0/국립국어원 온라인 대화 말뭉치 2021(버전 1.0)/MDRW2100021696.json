      "utterance": [
        {
          "id": "MDRW2100021696.1.1",
          "form": "중고거래 자주하세요?",
          "original_form": "중고거래 자주하세요?",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021696.1.2",
          "form": "중고거래 귀찮아서 안해요",
          "original_form": "중고거래 귀찮아서 안해요",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021696.1.3",
          "form": "이거 보여달라 저거 보여달라 뭐 인증해달라",
          "original_form": "이거 보여달라 저거 보여달라 뭐 인증해달라",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021696.1.4",
          "form": "왜 세상이 이렇게 됐나요 ㅠ",
          "original_form": "왜 세상이 이렇게 됐나요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021696.1.5",
          "form": "아 맞아요..",
          "original_form": "아 맞아요..",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021696.1.6",
          "form": "꼭 나쁜 사람들이 와서 판을 바꾸는거같아요",
          "original_form": "꼭 나쁜 사람들이 와서 판을 바꾸는거같아요",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021696.1.7",
          "form": "사실 저도 중고거래 하다가 사기 당해본 적 있어요 ㅜㅜ",
          "original_form": "사실 저도 중고거래 하다가 사기 당해본 적 있어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021696.1.8",
          "form": "헉 어떤거 구매하시다가요?ㅜㅠ",
          "original_form": "헉 어떤거 구매하시다가요?ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021696.1.9",
          "form": "그 당시에 스마트폰 자급제 구매하다가요 ㅜㅜ",
          "original_form": "그 당시에 스마트폰 자급제 구매하다가요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021696.1.10",
          "form": "어쩐지 시세가 엄청 싸더라구여....",
          "original_form": "어쩐지 시세가 엄청 싸더라구여....",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021696.1.11",
          "form": "아..ㅜㅠ",
          "original_form": "아..ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021696.1.12",
          "form": "진짜 나쁜사람들이에요",
          "original_form": "진짜 나쁜사람들이에요",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021696.1.13",
          "form": "님은 사기 당해본 적 없으세요?",
          "original_form": "님은 사기 당해본 적 없으세요?",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021696.1.14",
          "form": "네 감사하게도 아직까진 없어요",
          "original_form": "네 감사하게도 아직까진 없어요",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021696.1.15",
          "form": "그래도 늘 조마조마해요",
          "original_form": "그래도 늘 조마조마해요",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100021696.1.16",
          "form": "그건 다행이에요 ㅠㅠ",
          "original_form": "그건 다행이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021696.1.17",
          "form": "전 아직도 사기당한거 돈 못돌려받았어요 ..",
          "original_form": "전 아직도 사기당한거 돈 못돌려받았어요 ..",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021696.1.18",
          "form": "신고하셨어요??",
          "original_form": "신고하셨어요??",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021696.1.19",
          "form": "고소 했어요 사기죄로 ㅎㅎ",
          "original_form": "고소 했어요 사기죄로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021696.1.20",
          "form": "재판까지 갔고 형사처벌도 받았는데 ㅏ잠수 ^^",
          "original_form": "재판까지 갔고 형사처벌도 받았는데 ㅏ잠수 ^^",
          "speaker_id": "1",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021696.1.21",
          "form": "우와.. 그거 가능한거예요?",
          "original_form": "우와.. 그거 가능한거예요?",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021696.1.22",
          "form": "처벌 받았는데?",
          "original_form": "처벌 받았는데?",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021696.1.23",
          "form": "형사처벌은 처벌이고",
          "original_form": "형사처벌은 처벌이고",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021696.1.24",
          "form": "피해금액은 따로 민사고소 해야돼요",
          "original_form": "피해금액은 따로 민사고소 해야돼요",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021696.1.25",
          "form": "손해배상으로.",
          "original_form": "손해배상으로.",
          "speaker_id": "1",
          "time": "20210828 23:46"
        }
      ]
    }
