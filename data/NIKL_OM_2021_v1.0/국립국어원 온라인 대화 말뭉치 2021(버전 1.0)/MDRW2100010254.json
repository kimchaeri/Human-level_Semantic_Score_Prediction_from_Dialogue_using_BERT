      "utterance": [
        {
          "id": "MDRW2100010254.1.1",
          "form": "좋아하는 책있나",
          "original_form": "좋아하는 책있나",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010254.1.2",
          "form": "책이라...육아서적을 읽지요",
          "original_form": "책이라...육아서적을 읽지요",
          "speaker_id": "2",
          "time": "20210823 18:06"
        },
        {
          "id": "MDRW2100010254.1.3",
          "form": "요새는 육아책도 안읽지만...",
          "original_form": "요새는 육아책도 안읽지만...",
          "speaker_id": "2",
          "time": "20210823 18:09"
        },
        {
          "id": "MDRW2100010254.1.4",
          "form": "책 좀 읽어요??",
          "original_form": "책 좀 읽어요??",
          "speaker_id": "2",
          "time": "20210823 18:09"
        },
        {
          "id": "MDRW2100010254.1.5",
          "form": "책읽자 뭐라도ㅎ",
          "original_form": "책읽자 뭐라도ㅎ",
          "speaker_id": "1",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010254.1.6",
          "form": "난 지금 메타버스ㅋㅋ",
          "original_form": "난 지금 메타버스ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010254.1.7",
          "form": "책읽고있다",
          "original_form": "책읽고있다",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010254.1.8",
          "form": "도서관가니 신작 많이나왔더라",
          "original_form": "도서관가니 신작 많이나왔더라",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010254.1.9",
          "form": "ㅋㅋㅋㅋㅋㄱ 그놈의 메타버스",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 그놈의 메타버스",
          "speaker_id": "2",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100010254.1.10",
          "form": "도서관 언제 갔는지 기억도 안남",
          "original_form": "도서관 언제 갔는지 기억도 안남",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010254.1.11",
          "form": "우리집 원 책 빌리러 도서관 가야되는데",
          "original_form": "우리집 원 책 빌리러 도서관 가야되는데",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010254.1.12",
          "form": "ㅋㅋ 도서관 쾌적함",
          "original_form": "ㅋㅋ 도서관 쾌적함",
          "speaker_id": "1",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010254.1.13",
          "form": "애들책도 많다",
          "original_form": "애들책도 많다",
          "speaker_id": "1",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010254.1.14",
          "form": "너를위한책봤는데",
          "original_form": "너를위한책봤는데",
          "speaker_id": "1",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010254.1.15",
          "form": "나를 위한 책이라니 ㅋㅋㅋㅋ",
          "original_form": "나를 위한 책이라니 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010254.1.16",
          "form": "뭔가요??",
          "original_form": "뭔가요??",
          "speaker_id": "2",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010254.1.17",
          "form": "여기못올리네 카톡으로보내주께",
          "original_form": "여기못올리네 카톡으로보내주께",
          "speaker_id": "1",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100010254.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100010254.1.19",
          "form": "단체톡에",
          "original_form": "단체톡에",
          "speaker_id": "1",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100010254.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 도대체 무슨 책을 본겁니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 도대체 무슨 책을 본겁니까",
          "speaker_id": "2",
          "time": "20210823 21:10"
        },
        {
          "id": "MDRW2100010254.1.21",
          "form": "이언니야",
          "original_form": "이언니야",
          "speaker_id": "2",
          "time": "20210823 21:10"
        },
        {
          "id": "MDRW2100010254.1.22",
          "form": "올렸다ㅋㅋㅋㅋ",
          "original_form": "올렸다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:11"
        },
        {
          "id": "MDRW2100010254.1.23",
          "form": "아 미치겠다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "아 미치겠다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010254.1.24",
          "form": "내눈을 바라보면서 책 내용을 얘기해줘",
          "original_form": "내눈을 바라보면서 책 내용을 얘기해줘",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010254.1.25",
          "form": "좋은 책이야 정말",
          "original_form": "좋은 책이야 정말",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010254.1.26",
          "form": "좋은책이지 9살 말 습관 사전이란 책이야",
          "original_form": "좋은책이지 9살 말 습관 사전이란 책이야",
          "speaker_id": "1",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010254.1.27",
          "form": "꼭 읽어봐ㅋㅋ",
          "original_form": "꼭 읽어봐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010254.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ 9살 말 습관",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 9살 말 습관",
          "speaker_id": "2",
          "time": "20210823 21:18"
        },
        {
          "id": "MDRW2100010254.1.29",
          "form": "39살 되면 꼭 읽어보께요 ㅋㅋ",
          "original_form": "39살 되면 꼭 읽어보께요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:18"
        },
        {
          "id": "MDRW2100010254.1.30",
          "form": "요샌 진짜 육아책도 안 읽는듯",
          "original_form": "요샌 진짜 육아책도 안 읽는듯",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100010254.1.31",
          "form": "책만 쌓여있고",
          "original_form": "책만 쌓여있고",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100010254.1.32",
          "form": "소설책도 읽고싶네",
          "original_form": "소설책도 읽고싶네",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100010254.1.33",
          "form": "그래 나도 책욕심만있어아",
          "original_form": "그래 나도 책욕심만있어아",
          "speaker_id": "1",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010254.1.34",
          "form": "대여는 잘함ㅋㅋ",
          "original_form": "대여는 잘함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010254.1.35",
          "form": "ㅋㅋㅋㅋ 읽어보고 재밌는책 얘기해줘요",
          "original_form": "ㅋㅋㅋㅋ 읽어보고 재밌는책 얘기해줘요",
          "speaker_id": "2",
          "time": "20210823 21:22"
        },
        {
          "id": "MDRW2100010254.1.36",
          "form": "얼마전에 에쿠니가오리 책 커피숍에서 보다가 말았는데",
          "original_form": "얼마전에 에쿠니가오리 책 커피숍에서 보다가 말았는데",
          "speaker_id": "2",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010254.1.37",
          "form": "다시 가니 그책 없어짐 ㅠ 한창 재밌었는디",
          "original_form": "다시 가니 그책 없어짐 ㅠ 한창 재밌었는디",
          "speaker_id": "2",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010254.1.38",
          "form": "소설책은 안읽은지 꽤되서ㅋㅋ",
          "original_form": "소설책은 안읽은지 꽤되서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:55"
        },
        {
          "id": "MDRW2100010254.1.39",
          "form": "내가보는책은 재미없으",
          "original_form": "내가보는책은 재미없으",
          "speaker_id": "1",
          "time": "20210823 21:55"
        }
      ]
    }
