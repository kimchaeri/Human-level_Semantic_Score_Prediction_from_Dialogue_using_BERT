      "utterance": [
        {
          "id": "MDRW2100010562.1.1",
          "form": "잔잔한 게임류를 좋아한다 할라했지만",
          "original_form": "잔잔한 게임류를 좋아한다 할라했지만",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010562.1.2",
          "form": "술래잡기가 박진감 넘칠것같군요",
          "original_form": "술래잡기가 박진감 넘칠것같군요",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010562.1.3",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010562.1.4",
          "form": "핸드폰 게임도 잘 안 하시나요?",
          "original_form": "핸드폰 게임도 잘 안 하시나요?",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010562.1.5",
          "form": "허헙...넵...",
          "original_form": "허헙...넵...",
          "speaker_id": "2",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010562.1.6",
          "form": "잠만 저 리듬게임은 합니다",
          "original_form": "잠만 저 리듬게임은 합니다",
          "speaker_id": "2",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010562.1.7",
          "form": "슈스엠",
          "original_form": "슈스엠",
          "speaker_id": "2",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010562.1.8",
          "form": "리듬게임이 약간 제가 성장캐가 된 것 같이 느끼게하더라고요",
          "original_form": "리듬게임이 약간 제가 성장캐가 된 것 같이 느끼게하더라고요",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010562.1.9",
          "form": "하면 느는 것이",
          "original_form": "하면 느는 것이",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010562.1.10",
          "form": "와 저는 리듬게임을 제일 못해요",
          "original_form": "와 저는 리듬게임을 제일 못해요",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010562.1.11",
          "form": "즐기기 전에 맨날 제 실력에 상처 받고 포기한 장르...ㅋㅋㅋㅋ",
          "original_form": "즐기기 전에 맨날 제 실력에 상처 받고 포기한 장르...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010562.1.12",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010562.1.13",
          "form": "너무 처참해서 못하겠더라고요ㅋㅋㅋㅋㅋ",
          "original_form": "너무 처참해서 못하겠더라고요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010562.1.14",
          "form": "휴 다행이다 하는 게임 존재해서",
          "original_form": "휴 다행이다 하는 게임 존재해서",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010562.1.15",
          "form": "저 피아노 리듬게임도 해요",
          "original_form": "저 피아노 리듬게임도 해요",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010562.1.16",
          "form": "약간 복잡한 게임 못해서 그런가봐요",
          "original_form": "약간 복잡한 게임 못해서 그런가봐요",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010562.1.17",
          "form": "쓰읍..",
          "original_form": "쓰읍..",
          "speaker_id": "2",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010562.1.18",
          "form": "오 피아노라고 할 수 있는지는 모르겠는데",
          "original_form": "오 피아노라고 할 수 있는지는 모르겠는데",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010562.1.19",
          "form": "피아노 타일?",
          "original_form": "피아노 타일?",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010562.1.20",
          "form": "그건 예전에 재밌게 했어요",
          "original_form": "그건 예전에 재밌게 했어요",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010562.1.21",
          "form": "헐 맞아요 피아노타일!@!",
          "original_form": "헐 맞아요 피아노타일!@!",
          "speaker_id": "2",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010562.1.22",
          "form": "그거 재밌죠! 완전 중독성 있고",
          "original_form": "그거 재밌죠! 완전 중독성 있고",
          "speaker_id": "1",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010562.1.23",
          "form": "그건 타이밍 맞추는게 아니라 빨리 누르기만 하면 돼서 재밌게 했어요",
          "original_form": "그건 타이밍 맞추는게 아니라 빨리 누르기만 하면 돼서 재밌게 했어요",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010562.1.24",
          "form": "앜ㅋㅋㅋㅋ 스킬보다는 스피드를 좋아하시는군요?!!",
          "original_form": "앜ㅋㅋㅋㅋ 스킬보다는 스피드를 좋아하시는군요?!!",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010562.1.25",
          "form": "피아노 타일 잼써요 근데 그건 약간 고인물들은 할게 없다하더라고요",
          "original_form": "피아노 타일 잼써요 근데 그건 약간 고인물들은 할게 없다하더라고요",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010562.1.26",
          "form": "근데 게임 고인물들은 원래 할 게 없어요",
          "original_form": "근데 게임 고인물들은 원래 할 게 없어요",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010562.1.27",
          "form": "맞아요ㅋㅋㅋ 저도 한참 하고 그만 뒀어요",
          "original_form": "맞아요ㅋㅋㅋ 저도 한참 하고 그만 뒀어요",
          "speaker_id": "1",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010562.1.28",
          "form": "저는 그 총쏘는 게임 앞으로도 못가서",
          "original_form": "저는 그 총쏘는 게임 앞으로도 못가서",
          "speaker_id": "2",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100010562.1.29",
          "form": "바로 퇴출당했었답니다",
          "original_form": "바로 퇴출당했었답니다",
          "speaker_id": "2",
          "time": "20210823 20:10"
        },
