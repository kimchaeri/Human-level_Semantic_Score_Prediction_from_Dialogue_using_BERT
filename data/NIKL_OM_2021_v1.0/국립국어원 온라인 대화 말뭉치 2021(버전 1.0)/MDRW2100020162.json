      "utterance": [
        {
          "id": "MDRW2100020162.1.1",
          "form": "네가연애중이니깐",
          "original_form": "네가연애중이니깐",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020162.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇군요",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020162.1.3",
          "form": "이상형에대해",
          "original_form": "이상형에대해",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020162.1.4",
          "form": "이야기해볼까요??",
          "original_form": "이야기해볼까요??",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020162.1.5",
          "form": "이상형 좋아용",
          "original_form": "이상형 좋아용",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020162.1.6",
          "form": "한가지씩",
          "original_form": "한가지씩",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020162.1.7",
          "form": "말해봅시다",
          "original_form": "말해봅시다",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020162.1.8",
          "form": "name1 이상형은?",
          "original_form": "&name1& 이상형은?",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020162.1.9",
          "form": "유머러스한사람",
          "original_form": "유머러스한사람",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020162.1.10",
          "form": "너눈요??",
          "original_form": "너눈요??",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020162.1.11",
          "form": "유머러스한 사람",
          "original_form": "유머러스한 사람",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020162.1.12",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020162.1.13",
          "form": "아그럼요",
          "original_form": "아그럼요",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020162.1.14",
          "form": "그리고뮤직뮤직하는사람",
          "original_form": "그리고뮤직뮤직하는사람",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020162.1.15",
          "form": "저는... 저보다 키큰 사람! 그리고 담배 안피는 사람!",
          "original_form": "저는... 저보다 키큰 사람! 그리고 담배 안피는 사람!",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020162.1.16",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020162.1.17",
          "form": "뮤직인마이라이프",
          "original_form": "뮤직인마이라이프",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020162.1.18",
          "form": "아그럼요그럼요",
          "original_form": "아그럼요그럼요",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020162.1.19",
          "form": "담배안피는건",
          "original_form": "담배안피는건",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020162.1.20",
          "form": "중요하죠",
          "original_form": "중요하죠",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020162.1.21",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020162.1.22",
          "form": "처음부터 담배안피었으면 좋겠다",
          "original_form": "처음부터 담배안피었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020162.1.23",
          "form": "생각했는데",
          "original_form": "생각했는데",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020162.1.24",
          "form": "지금 남친이 그래서 좋습니다",
          "original_form": "지금 남친이 그래서 좋습니다",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020162.1.25",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020162.1.26",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020162.1.27",
