      "utterance": [
        {
          "id": "MDRW2100018961.1.1",
          "form": "코로나 전에 화병으로 쓰러질 듯",
          "original_form": "코로나 전에 화병으로 쓰러질 듯",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018961.1.2",
          "form": "그니까 아",
          "original_form": "그니까 아",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018961.1.3",
          "form": "코로나가올해시작된것도아닌데",
          "original_form": "코로나가올해시작된것도아닌데",
          "speaker_id": "1",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018961.1.4",
          "form": "왜이렇게왔다갔다함",
          "original_form": "왜이렇게왔다갔다함",
          "speaker_id": "1",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018961.1.5",
          "form": "물론 내 교양안 다 사이버긴 함",
          "original_form": "물론 내 교양안 다 사이버긴 함",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018961.1.6",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018961.1.7",
          "form": "작년에 해봤는데 왜 이래",
          "original_form": "작년에 해봤는데 왜 이래",
          "speaker_id": "2",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018961.1.8",
          "form": "작년보다확진자수는더많은데",
          "original_form": "작년보다확진자수는더많은데",
          "speaker_id": "1",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018961.1.9",
          "form": "어휴 진짜 감 떨어졌다 짜식들이....",
          "original_form": "어휴 진짜 감 떨어졌다 짜식들이....",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018961.1.10",
          "form": "근데갑자기비오네",
          "original_form": "근데갑자기비오네",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018961.1.11",
          "form": "당황했네",
          "original_form": "당황했네",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018961.1.12",
          "form": "뮤ㅓ야 비와?",
          "original_form": "뮤ㅓ야 비와?",
          "speaker_id": "2",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018961.1.13",
          "form": "ㅇㅇㅇ창문으로봐버",
          "original_form": "ㅇㅇㅇ창문으로봐버",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018961.1.14",
          "form": "왐마야",
          "original_form": "왐마야",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018961.1.15",
          "form": "이게 뭐야 깜짝아",
          "original_form": "이게 뭐야 깜짝아",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018961.1.16",
          "form": "진짜뜬금없이비오네",
          "original_form": "진짜뜬금없이비오네",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018961.1.17",
          "form": "그니까 나 어제 이래서 비맞고 다녔는데",
          "original_form": "그니까 나 어제 이래서 비맞고 다녔는데",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018961.1.18",
          "form": "어제도비왔어?",
          "original_form": "어제도비왔어?",
          "speaker_id": "1",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018961.1.19",
          "form": "ㅇㅇㅇ 어제 오후에 갑자기 옴",
          "original_form": "ㅇㅇㅇ 어제 오후에 갑자기 옴",
          "speaker_id": "2",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018961.1.20",
          "form": "너 밖이야?",
          "original_form": "너 밖이야?",
          "speaker_id": "2",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018961.1.21",
          "form": "ㄴㄴㄴ집임",
          "original_form": "ㄴㄴㄴ집임",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100018961.1.22",
          "form": "그냥빗소리들려서보니까",
          "original_form": "그냥빗소리들려서보니까",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018961.1.23",
          "form": "비오고있었음",
          "original_form": "비오고있었음",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018961.1.24",
          "form": "아 ㅇㅋㅇㅋ",
          "original_form": "아 ㅇㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018961.1.25",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018961.1.26",
          "form": "갑자기그거생각남",
          "original_form": "갑자기그거생각남",
