      "utterance": [
        {
          "id": "MDRW2100018322.1.1",
          "form": "가족이랑 같이 사세요??",
          "original_form": "가족이랑 같이 사세요??",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018322.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018322.1.3",
          "form": "따로 사시나요?",
          "original_form": "따로 사시나요?",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018322.1.4",
          "form": "저도 같이 살고 있어요",
          "original_form": "저도 같이 살고 있어요",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018322.1.5",
          "form": "독립하기엔",
          "original_form": "독립하기엔",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018322.1.6",
          "form": "비용이 많이 들죠",
          "original_form": "비용이 많이 들죠",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018322.1.7",
          "form": "월세 못내요 흑흑",
          "original_form": "월세 못내요 흑흑",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018322.1.8",
          "form": "제 자산으로 룸컨디션이...",
          "original_form": "제 자산으로 룸컨디션이...",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018322.1.9",
          "form": "그것까지 내면 더 그지돼요 ㅠㅠ",
          "original_form": "그것까지 내면 더 그지돼요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018322.1.10",
          "form": "그냥 엄빠랑 사는것도",
          "original_form": "그냥 엄빠랑 사는것도",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018322.1.11",
          "form": "나쁘지않아요ㅋ",
          "original_form": "나쁘지않아요ㅋ",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018322.1.12",
          "form": "맞아여 모아야죠",
          "original_form": "맞아여 모아야죠",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018322.1.13",
          "form": "버는것도 적어서 모아야",
          "original_form": "버는것도 적어서 모아야",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018322.1.14",
          "form": "티끌모아야 티끌이긴 한데",
          "original_form": "티끌모아야 티끌이긴 한데",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018322.1.15",
          "form": "그나마 인간답게 살수 있어여",
          "original_form": "그나마 인간답게 살수 있어여",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018322.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018322.1.17",
          "form": "혹시 생활비 내세요?",
          "original_form": "혹시 생활비 내세요?",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018322.1.18",
          "form": "전 안내여",
          "original_form": "전 안내여",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018322.1.19",
          "form": "현물로???내는편이에여",
          "original_form": "현물로???내는편이에여",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018322.1.20",
          "form": "저도 안내고 그냥 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 안내고 그냥 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018322.1.21",
          "form": "엄빠가 이것저것 인터넷으로 시키라고 하면 사는정도",
          "original_form": "엄빠가 이것저것 인터넷으로 시키라고 하면 사는정도",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018322.1.22",
          "form": "그래도 홍삼도 사드리고 그래여",
          "original_form": "그래도 홍삼도 사드리고 그래여",
          "speaker_id": "2",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018322.1.23",
          "form": "저도저도",
          "original_form": "저도저도",
          "speaker_id": "2",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018322.1.24",
          "form": "효녀시네요",
          "original_form": "효녀시네요",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018322.1.25",
          "form": "우린 지극히 정상이네요",
          "original_form": "우린 지극히 정상이네요",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018322.1.26",
          "form": "ㅋㅋㅋ 떨어졌다고 하시니 당연히 ...",
          "original_form": "ㅋㅋㅋ 떨어졌다고 하시니 당연히 ...",
          "speaker_id": "2",
          "time": "20210826 23:53"
        }
      ]
    }
