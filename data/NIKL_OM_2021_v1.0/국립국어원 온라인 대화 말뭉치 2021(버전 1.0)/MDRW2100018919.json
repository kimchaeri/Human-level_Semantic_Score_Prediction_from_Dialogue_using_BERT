      "utterance": [
        {
          "id": "MDRW2100018919.1.1",
          "form": "그룹 넬 아시나요?",
          "original_form": "그룹 넬 아시나요?",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018919.1.2",
          "form": "네 알아여 넬 노래 넘 좋아요",
          "original_form": "네 알아여 넬 노래 넘 좋아요",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018919.1.3",
          "form": "저도 너무 좋아해요",
          "original_form": "저도 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018919.1.4",
          "form": "좋아하는 가수 있나요?",
          "original_form": "좋아하는 가수 있나요?",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018919.1.5",
          "form": "음 지금은 없구 전에 워너원 좋아했어요!",
          "original_form": "음 지금은 없구 전에 워너원 좋아했어요!",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018919.1.6",
          "form": "넬도 그때 알게됐는대 아직도 너래 들어요 ㅋㅋ",
          "original_form": "넬도 그때 알게됐는대 아직도 너래 들어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018919.1.7",
          "form": "역시 최근에 공연이나 전시회 본적 있으세요",
          "original_form": "역시 최근에 공연이나 전시회 본적 있으세요",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018919.1.8",
          "form": "넹! 저번에 서울가서 전시회 봤었어요",
          "original_form": "넹! 저번에 서울가서 전시회 봤었어요",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018919.1.9",
          "form": "어떤전시회였나요",
          "original_form": "어떤전시회였나요",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018919.1.10",
          "form": "뮤지엄 오브 컬러 라구 색깔마다 방들이 있는데",
          "original_form": "뮤지엄 오브 컬러 라구 색깔마다 방들이 있는데",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018919.1.11",
          "form": "파란방은 바다 창문, 초록색은 화원, 핑크색은 네온사인 등등",
          "original_form": "파란방은 바다 창문, 초록색은 화원, 핑크색은 네온사인 등등",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018919.1.12",
          "form": "방금 찾아봤는데",
          "original_form": "방금 찾아봤는데",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018919.1.13",
          "form": "전시물들이 감각적이다",
          "original_form": "전시물들이 감각적이다",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018919.1.14",
          "form": "화려하고",
          "original_form": "화려하고",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018919.1.15",
          "form": "재미있었겠어요",
          "original_form": "재미있었겠어요",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018919.1.16",
          "form": "마자여 진짜 예쁘고 좋더라구요 가서 사진도 백만장 찍었어요",
          "original_form": "마자여 진짜 예쁘고 좋더라구요 가서 사진도 백만장 찍었어요",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018919.1.17",
          "form": "좋은 시간이었겠다",
          "original_form": "좋은 시간이었겠다",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018919.1.18",
          "form": "누구랑 가셨어요?",
          "original_form": "누구랑 가셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018919.1.19",
          "form": "전 친구랑 갔었어요",
          "original_form": "전 친구랑 갔었어요",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018919.1.20",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210827 13:47"
        }
      ]
