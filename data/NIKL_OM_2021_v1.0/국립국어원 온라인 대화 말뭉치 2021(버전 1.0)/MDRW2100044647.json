      "utterance": [
        {
          "id": "MDRW2100044647.1.1",
          "form": "셉이 중순에 컴백했으니까 이제 우리각섰잖아",
          "original_form": "셉이 중순에 컴백했으니까 이제 우리각섰잖아",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044647.1.2",
          "form": "콘에서",
          "original_form": "콘에서",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044647.1.3",
          "form": "선공개하자",
          "original_form": "선공개하자",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044647.1.4",
          "form": "그러고 컴백해라",
          "original_form": "그러고 컴백해라",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044647.1.5",
          "form": "가수 세명뿐인데",
          "original_form": "가수 세명뿐인데",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044647.1.6",
          "form": "무슨 말이 더필요해",
          "original_form": "무슨 말이 더필요해",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044647.1.7",
          "form": "프나 셉 늉인데",
          "original_form": "프나 셉 늉인데",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044647.1.8",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044647.1.9",
          "form": "그게좋겠다",
          "original_form": "그게좋겠다",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044647.1.10",
          "form": "프가 누구더라....?",
          "original_form": "프가 누구더라....?",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044647.1.11",
          "form": "셉과 늉밖에 기억이 안나",
          "original_form": "셉과 늉밖에 기억이 안나",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044647.1.12",
          "form": "플미스나인",
          "original_form": "플미스나인",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044647.1.13",
          "form": "프로미스",
          "original_form": "프로미스",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044647.1.14",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044647.1.15",
          "form": "아 걔네 플디였지",
          "original_form": "아 걔네 플디였지",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044647.1.16",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044647.1.17",
          "form": "걔네 스톤에있다가 이제 슬그머니 플디로옴",
          "original_form": "걔네 스톤에있다가 이제 슬그머니 플디로옴",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044647.1.18",
          "form": "스톤에있는척했지만",
          "original_form": "스톤에있는척했지만",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044647.1.19",
          "form": "실질적인 관리는 플디인력 ㅇㅇ......",
          "original_form": "실질적인 관리는 플디인력 ㅇㅇ......",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044647.1.20",
          "form": "디용",
          "original_form": "디용",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044647.1.21",
          "form": "매니지먼트는 스톤이 하고",
          "original_form": "매니지먼트는 스톤이 하고",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044647.1.22",
          "form": "플디 한승연건물에있을때 옥상이 프로미스나인 숙소였어",
          "original_form": "플디 한승연건물에있을때 옥상이 프로미스나인 숙소였어",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044647.1.23",
          "form": "뭐 어쩌고저쩌고 그런줗 알았는데",
          "original_form": "뭐 어쩌고저쩌고 그런줗 알았는데",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044647.1.24",
          "form": "ㅇㅇㅇ 그거 기억나",
          "original_form": "ㅇㅇㅇ 그거 기억나",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044647.1.25",
          "form": "말은 그렇게하는데 스톤거의 손안댐",
          "original_form": "말은 그렇게하는데 스톤거의 손안댐",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044647.1.26",
          "form": "한성수작품임 그냥",
          "original_form": "한성수작품임 그냥",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044647.1.27",
          "form": "아 그거 얘기헀나여",
          "original_form": "아 그거 얘기헀나여",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044647.1.28",
          "form": "엔하이픈",
          "original_form": "엔하이픈",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044647.1.29",
          "form": "그거 빌리프랩인가 하는 소속사 어차피 그것동 하이브인데",
          "original_form": "그거 빌리프랩인가 하는 소속사 어차피 그것동 하이브인데",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044647.1.30",
          "form": "ㄱ이번에 걔네 다 코로나걸렸다며",
          "original_form": "ㄱ이번에 걔네 다 코로나걸렸다며",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044647.1.31",
          "form": "?????",
          "original_form": "?????",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044647.1.32",
          "form": "에반데",
          "original_form": "에반데",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044647.1.33",
          "form": "아 말씀안드렸나요",
          "original_form": "아 말씀안드렸나요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044647.1.34",
          "form": "7ㅁㅇ중 5명 코로나확진ㄴ",
