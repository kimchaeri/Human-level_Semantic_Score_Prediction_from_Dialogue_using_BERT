      "utterance": [
        {
          "id": "MDRW2100009569.1.1",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009569.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 08:02"
        },
        {
          "id": "MDRW2100009569.1.3",
          "form": "데이트할때 어떤거 하시나요?",
          "original_form": "데이트할때 어떤거 하시나요?",
          "speaker_id": "2",
          "time": "20210823 08:02"
        },
        {
          "id": "MDRW2100009569.1.4",
          "form": "주로 밥먹고 영화보고\r\n카페가고 했던거 같아요:)",
          "original_form": "주로 밥먹고 영화보고\r\n카페가고 했던거 같아요:)",
          "speaker_id": "1",
          "time": "20210823 08:06"
        },
        {
          "id": "MDRW2100009569.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210823 08:06"
        },
        {
          "id": "MDRW2100009569.1.6",
          "form": "저는 주로 카페가 많은데 너무 변화가 없어서",
          "original_form": "저는 주로 카페가 많은데 너무 변화가 없어서",
          "speaker_id": "2",
          "time": "20210823 08:08"
        },
        {
          "id": "MDRW2100009569.1.7",
          "form": "다른 데이트방법 있나 궁금해서 여쭤봤어요",
          "original_form": "다른 데이트방법 있나 궁금해서 여쭤봤어요",
          "speaker_id": "2",
          "time": "20210823 08:08"
        },
        {
          "id": "MDRW2100009569.1.8",
          "form": "코로나 때문에\r\n더 제약사항이 많아졌죠? ㅠ",
          "original_form": "코로나 때문에\r\n더 제약사항이 많아졌죠? ㅠ",
          "speaker_id": "1",
          "time": "20210823 08:08"
        },
        {
          "id": "MDRW2100009569.1.9",
          "form": "네 아무래도 그렇죠",
          "original_form": "네 아무래도 그렇죠",
          "speaker_id": "2",
          "time": "20210823 08:09"
        },
        {
          "id": "MDRW2100009569.1.10",
          "form": "연애하신지는 얼마나 되셨어요?",
          "original_form": "연애하신지는 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210823 08:10"
        },
        {
          "id": "MDRW2100009569.1.11",
          "form": "저는 작년에 결혼했어요",
          "original_form": "저는 작년에 결혼했어요",
          "speaker_id": "2",
          "time": "20210823 08:10"
        },
        {
          "id": "MDRW2100009569.1.12",
          "form": "작년 12월에했는데",
          "original_form": "작년 12월에했는데",
          "speaker_id": "2",
          "time": "20210823 08:10"
        },
        {
          "id": "MDRW2100009569.1.13",
          "form": "그때도 코로나환자 갑자기 늘어서",
          "original_form": "그때도 코로나환자 갑자기 늘어서",
          "speaker_id": "2",
          "time": "20210823 08:11"
        },
        {
          "id": "MDRW2100009569.1.14",
          "form": "결혼식 잘할까 걱정이었는데",
          "original_form": "결혼식 잘할까 걱정이었는데",
          "speaker_id": "2",
          "time": "20210823 08:11"
        },
        {
          "id": "MDRW2100009569.1.15",
          "form": "요즘 확진자보며 안미루고 그때 식치른게 다행인것 같아요",
          "original_form": "요즘 확진자보며 안미루고 그때 식치른게 다행인것 같아요",
          "speaker_id": "2",
          "time": "20210823 08:11"
        },
        {
          "id": "MDRW2100009569.1.16",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "2",
          "time": "20210823 08:12"
        },
        {
          "id": "MDRW2100009569.1.17",
          "form": "그러게요... 오히려 점점 더\r\n안좋아지는 상황이라 잘하셨었네요",
          "original_form": "그러게요... 오히려 점점 더\r\n안좋아지는 상황이라 잘하셨었네요",
          "speaker_id": "1",
          "time": "20210823 08:12"
        },
        {
          "id": "MDRW2100009569.1.18",
          "form": "아니요 저는 7년차입니다ㅋㅋㅈ",
          "original_form": "아니요 저는 7년차입니다ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210823 08:12"
        },
        {
          "id": "MDRW2100009569.1.19",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210823 08:13"
        },
        {
          "id": "MDRW2100009569.1.20",
          "form": "결혼생활은 만족하시나요?",
          "original_form": "결혼생활은 만족하시나요?",
          "speaker_id": "2",
          "time": "20210823 08:13"
        },
        {
          "id": "MDRW2100009569.1.21",
          "form": "응 너무 사적인 질문이네요",
          "original_form": "응 너무 사적인 질문이네요",
          "speaker_id": "2",
          "time": "20210823 08:14"
        },
        {
          "id": "MDRW2100009569.1.22",
          "form": "배우자랑 다툼이나 의견차이가 있을때",
          "original_form": "배우자랑 다툼이나 의견차이가 있을때",
          "speaker_id": "2",
          "time": "20210823 08:14"
        },
        {
          "id": "MDRW2100009569.1.23",
          "form": "어떻게 대처하면 좋을까요?",
          "original_form": "어떻게 대처하면 좋을까요?",
          "speaker_id": "2",
          "time": "20210823 08:15"
        },
        {
          "id": "MDRW2100009569.1.24",
          "form": "결혼생활은 좋은점도\r\n나쁜점도 있는거 같아요",
          "original_form": "결혼생활은 좋은점도\r\n나쁜점도 있는거 같아요",
          "speaker_id": "1",
          "time": "20210823 08:20"
        },
        {
          "id": "MDRW2100009569.1.25",
          "form": "서로의 다름을 인정하고\r\n받아들이기 시작할때부터\r\n싸우는 일은 없어지는데",
          "original_form": "서로의 다름을 인정하고\r\n받아들이기 시작할때부터\r\n싸우는 일은 없어지는데",
          "speaker_id": "1",
          "time": "20210823 08:22"
        },
        {
          "id": "MDRW2100009569.1.26",
          "form": "그러기까지 많은 시간이\r\n걸렸던거 같아요ㅜ",
          "original_form": "그러기까지 많은 시간이\r\n걸렸던거 같아요ㅜ",
          "speaker_id": "1",
          "time": "20210823 08:22"
        },
        {
          "id": "MDRW2100009569.1.27",
          "form": "그쵸 상대를 내 기준에 맞추려고하면 안되는데",
          "original_form": "그쵸 상대를 내 기준에 맞추려고하면 안되는데",
          "speaker_id": "2",
          "time": "20210823 08:23"
        },
        {
          "id": "MDRW2100009569.1.28",
          "form": "알고있지만 어려운 일인거 같아요:)",
          "original_form": "알고있지만 어려운 일인거 같아요:)",
          "speaker_id": "1",
          "time": "20210823 08:27"
        },
        {
          "id": "MDRW2100009569.1.29",
          "form": "name2님은 작년 12월에 하셨으면\r\n알콩달콩 하실때 아닌가용?",
          "original_form": "&name2&님은 작년 12월에 하셨으면\r\n알콩달콩 하실때 아닌가용?",
          "speaker_id": "1",
          "time": "20210823 08:30"
        },
        {
          "id": "MDRW2100009569.1.30",
          "form": "네 중간중간 다툰적도 있기는하지만",
          "original_form": "네 중간중간 다툰적도 있기는하지만",
          "speaker_id": "2",
          "time": "20210823 08:31"
        },
        {
          "id": "MDRW2100009569.1.31",
          "form": "그래도 즐겁게 잘 생화하고 있어요",
          "original_form": "그래도 즐겁게 잘 생화하고 있어요",
          "speaker_id": "2",
          "time": "20210823 08:32"
        }
      ]
