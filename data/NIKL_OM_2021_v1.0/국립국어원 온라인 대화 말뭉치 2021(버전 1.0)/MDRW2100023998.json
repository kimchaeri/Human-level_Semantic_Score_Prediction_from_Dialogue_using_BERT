      "utterance": [
        {
          "id": "MDRW2100023998.1.1",
          "form": "안녕하세용! 식사는 하셨나용~?",
          "original_form": "안녕하세용! 식사는 하셨나용~?",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023998.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023998.1.3",
          "form": "저는 저녁으로 샌드위치를 먹었어요",
          "original_form": "저는 저녁으로 샌드위치를 먹었어요",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023998.1.4",
          "form": "저녁 뭐 드셨나요?",
          "original_form": "저녁 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023998.1.5",
          "form": "오! 무슨 샌드위치 드셨어요?",
          "original_form": "오! 무슨 샌드위치 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023998.1.6",
          "form": "파리바게트 샌드위치요!",
          "original_form": "파리바게트 샌드위치요!",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023998.1.7",
          "form": "아아 저도 저번주에 요기요에서",
          "original_form": "아아 저도 저번주에 요기요에서",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023998.1.8",
          "form": "파리바게트 세일 크게하길래",
          "original_form": "파리바게트 세일 크게하길래",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023998.1.9",
          "form": "빵만 먹었어용ㅋㅋㅋ",
          "original_form": "빵만 먹었어용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023998.1.10",
          "form": "ㅋㅋㅋㅋㅋ 빵맛있죠",
          "original_form": "ㅋㅋㅋㅋㅋ 빵맛있죠",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023998.1.11",
          "form": "세일 안하면 사기 그런데",
          "original_form": "세일 안하면 사기 그런데",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100023998.1.12",
          "form": "맞아요~ 세일하면 이만한 간편식이 없더라구요",
          "original_form": "맞아요~ 세일하면 이만한 간편식이 없더라구요",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023998.1.13",
          "form": "먹을게 없어서 파바갔어요...ㅎ",
          "original_form": "먹을게 없어서 파바갔어요...ㅎ",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023998.1.14",
          "form": "저는 수제버거 먹었는데",
          "original_form": "저는 수제버거 먹었는데",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023998.1.15",
          "form": "어디서요?",
          "original_form": "어디서요?",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023998.1.16",
          "form": "그 체인은 아닌 혼자 운영하시는",
          "original_form": "그 체인은 아닌 혼자 운영하시는",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023998.1.17",
          "form": "동네 수제버거집에서 포장했어요ㅎㅎ",
          "original_form": "동네 수제버거집에서 포장했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023998.1.18",
          "form": "수제버거 좋죠",
          "original_form": "수제버거 좋죠",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023998.1.19",
          "form": "쉑쉑버거 드셔보셨어용?",
          "original_form": "쉑쉑버거 드셔보셨어용?",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023998.1.20",
          "form": "쉐이크가 맛있더라고요",
          "original_form": "쉐이크가 맛있더라고요",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023998.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023998.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023998.1.23",
          "form": "감자튀김 쉐이크에 찍어먹으면",
          "original_form": "감자튀김 쉐이크에 찍어먹으면",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023998.1.24",
          "form": "근데 버거 사이즈 생각보다 작아서 놀랐네요 ㅎㅎ",
          "original_form": "근데 버거 사이즈 생각보다 작아서 놀랐네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023998.1.25",
          "form": "단짠단짠해요ㅋㅋ",
          "original_form": "단짠단짠해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023998.1.26",
          "form": "아~~ 맞아요ㅠㅠ",
          "original_form": "아~~ 맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023998.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ인정!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ인정!",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023998.1.28",
          "form": "사이드를 시키게하려는 전략?같아요ㅋㅋㅋ",
          "original_form": "사이드를 시키게하려는 전략?같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:16"
        }
      ]
