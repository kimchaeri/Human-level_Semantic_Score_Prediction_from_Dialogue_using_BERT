        },
        {
          "id": "MDRW2100021342.1.3",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021342.1.4",
          "form": "아뇨 아직요! name1님은요?",
          "original_form": "아뇨 아직요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021342.1.5",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021342.1.6",
          "form": "뭐드실건가요?",
          "original_form": "뭐드실건가요?",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021342.1.7",
          "form": "다이어트중이라 고민이에요...맛있는게 먹고싶은데ㅠㅠ",
          "original_form": "다이어트중이라 고민이에요...맛있는게 먹고싶은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021342.1.8",
          "form": "동지네요 ㅜㅜㅜ",
          "original_form": "동지네요 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021342.1.9",
          "form": "근데 전 냉동실에 마카롱이 있어서 넘 고민돼요 ㅋㅋㅋ",
          "original_form": "근데 전 냉동실에 마카롱이 있어서 넘 고민돼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021342.1.10",
          "form": "토요일이니까 하나쯤은 먹어도되지않을까요?",
          "original_form": "토요일이니까 하나쯤은 먹어도되지않을까요?",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021342.1.11",
          "form": "하 문제는",
          "original_form": "하 문제는",
          "speaker_id": "1",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021342.1.12",
          "form": "이미낮에 하나먹었답니다..",
          "original_form": "이미낮에 하나먹었답니다..",
          "speaker_id": "1",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021342.1.13",
          "form": "ㅋㅋㅋ앗 그럼 참고 저녁을 맛난걸로 드세여!!",
          "original_form": "ㅋㅋㅋ앗 그럼 참고 저녁을 맛난걸로 드세여!!",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021342.1.14",
          "form": "감사합니다 허락받았네요(? ㅋㅋㅋㅋ",
          "original_form": "감사합니다 허락받았네요(? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021342.1.15",
          "form": "그럼 식단하시는건가요?!",
          "original_form": "그럼 식단하시는건가요?!",
          "speaker_id": "1",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100021342.1.16",
          "form": "아뇨 그렇게 철처하게는 아니라서....은근 먹을건 다 먹고있네요?",
          "original_form": "아뇨 그렇게 철처하게는 아니라서....은근 먹을건 다 먹고있네요?",
