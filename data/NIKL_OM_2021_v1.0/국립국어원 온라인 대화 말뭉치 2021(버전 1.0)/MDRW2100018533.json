      "utterance": [
        {
          "id": "MDRW2100018533.1.1",
          "form": "치킨 좋아하세요!",
          "original_form": "치킨 좋아하세요!",
          "speaker_id": "1",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018533.1.2",
          "form": "치킨 좋아하시나요?",
          "original_form": "치킨 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018533.1.3",
          "form": "아이고 치킨은 사랑입니다",
          "original_form": "아이고 치킨은 사랑입니다",
          "speaker_id": "2",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018533.1.4",
          "form": "치킨 안먹는사람이랑 친구 안합니다",
          "original_form": "치킨 안먹는사람이랑 친구 안합니다",
          "speaker_id": "2",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018533.1.5",
          "form": "저도 그런 사람과는 겸상 안해요ㅎ",
          "original_form": "저도 그런 사람과는 겸상 안해요ㅎ",
          "speaker_id": "1",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018533.1.6",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018533.1.7",
          "form": "어느 브랜드 좋아하세요",
          "original_form": "어느 브랜드 좋아하세요",
          "speaker_id": "1",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018533.1.8",
          "form": "어머 그런 실례되는 질문을",
          "original_form": "어머 그런 실례되는 질문을",
          "speaker_id": "2",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018533.1.9",
          "form": "교촌 레드오리지널",
          "original_form": "교촌 레드오리지널",
          "speaker_id": "2",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018533.1.10",
          "form": "헉 저랑 완전 통했네요",
          "original_form": "헉 저랑 완전 통했네요",
          "speaker_id": "1",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018533.1.11",
          "form": "대개 레드하면 레드콤보 말하는데",
          "original_form": "대개 레드하면 레드콤보 말하는데",
          "speaker_id": "1",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018533.1.12",
          "form": "다양한 부위 좋아하시나요",
          "original_form": "다양한 부위 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018533.1.13",
          "form": "멕시카나 ..  청양마요도 개존맛",
          "original_form": "멕시카나 ..  청양마요도 개존맛",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018533.1.14",
          "form": "많이 맵지는 않은가요",
          "original_form": "많이 맵지는 않은가요",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018533.1.15",
          "form": "청양마요 시리즈 중에 멕시카나가 제일 맛있었어요",
          "original_form": "청양마요 시리즈 중에 멕시카나가 제일 맛있었어요",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018533.1.16",
          "form": "오오 저도 청양마요 좋아하는데",
          "original_form": "오오 저도 청양마요 좋아하는데",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018533.1.17",
          "form": "도전해봐야겠어요",
          "original_form": "도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018533.1.18",
          "form": "맵기는요. 치킨은 매워봤자 치킨",
          "original_form": "맵기는요. 치킨은 매워봤자 치킨{emoji:♡}",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018533.1.19",
          "form": "ㅎㅎ 매워도 사랑이죠ㅎ",
          "original_form": "ㅎㅎ 매워도 사랑이죠ㅎ",
          "speaker_id": "1",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018533.1.20",
          "form": "저 다리부위 좋아해요",
          "original_form": "저 다리부위 좋아해요",
          "speaker_id": "2",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018533.1.21",
          "form": "요즘 gs에서 닭봉도 많이 사먹어요",
          "original_form": "요즘 gs에서 닭봉도 많이 사먹어요",
          "speaker_id": "2",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018533.1.22",
