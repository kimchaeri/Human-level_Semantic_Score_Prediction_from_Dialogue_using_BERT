      "utterance": [
        {
          "id": "MDRW2100035001.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ다양하게 하시네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ다양하게 하시네요",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100035001.1.2",
          "form": "요즘은 과사람들 잘안만나시나요  길에서",
          "original_form": "요즘은 과사람들 잘안만나시나요  길에서",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100035001.1.3",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100035001.1.4",
          "form": "다양함을추구합니다",
          "original_form": "다양함을추구합니다",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100035001.1.5",
          "form": "네 이제 과사람들 다들 부산에 없기도 하고",
          "original_form": "네 이제 과사람들 다들 부산에 없기도 하고",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100035001.1.6",
          "form": "이제 다들 차타고 다녀서 그런지 목격이 어려워졋고",
          "original_form": "이제 다들 차타고 다녀서 그런지 목격이 어려워졋고",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100035001.1.7",
          "form": "더 어린 과사람들은 얼굴을 모르죠",
          "original_form": "더 어린 과사람들은 얼굴을 모르죠",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100035001.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035001.1.9",
          "form": "세상 어디에나있는 학과사람들이엇는데",
          "original_form": "세상 어디에나있는 학과사람들이엇는데",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035001.1.10",
          "form": "이제없네요흑흑",
          "original_form": "이제없네요흑흑",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035001.1.11",
          "form": "네ㅋㅋㅋㅋ좀 더 편합니다",
          "original_form": "네ㅋㅋㅋㅋ좀 더 편합니다",
          "speaker_id": "2",
          "time": "20210829 08:16"
        },
        {
          "id": "MDRW2100035001.1.12",
          "form": "과 친구들 아직 연락하시나여",
          "original_form": "과 친구들 아직 연락하시나여",
          "speaker_id": "2",
          "time": "20210829 09:34"
        },
        {
          "id": "MDRW2100035001.1.13",
          "form": "연락하는친구는거의없어요",
          "original_form": "연락하는친구는거의없어요",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035001.1.14",
          "form": "잘사는지모르겠네요",
          "original_form": "잘사는지모르겠네요",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035001.1.15",
          "form": "저도 그냥 결혼할때나 연락받고ㅋㅋㅋ 계모임할때나 소식 듣는데",
          "original_form": "저도 그냥 결혼할때나 연락받고ㅋㅋㅋ 계모임할때나 소식 듣는데",
          "speaker_id": "2",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100035001.1.16",
          "form": "계모임을 계속 못해서",
          "original_form": "계모임을 계속 못해서",
          "speaker_id": "2",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100035001.1.17",
          "form": "계모임언제할수있을까요코로나이자식",
          "original_form": "계모임언제할수있을까요코로나이자식",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100035001.1.18",
          "form": "ㅋㅋㅋㅋ이제 다음 모임은 내가 결혼을 해야 가능할지도",
          "original_form": "ㅋㅋㅋㅋ이제 다음 모임은 내가 결혼을 해야 가능할지도",
          "speaker_id": "2",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100035001.1.19",
          "form": "대박  뭔가 그시기가 금방 올지도모른다는생각이드네",
          "original_form": "대박  뭔가 그시기가 금방 올지도모른다는생각이드네",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100035001.1.20",
          "form": "과연?....",
          "original_form": "과연?....",
          "speaker_id": "2",
          "time": "20210830 09:37"
        },
        {
          "id": "MDRW2100035001.1.21",
          "form": "ㅋㅋㅋㅋㄱ기대하여봅니당",
          "original_form": "ㅋㅋㅋㅋㄱ기대하여봅니당",
