      "utterance": [
        {
          "id": "MDRW2100001474.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 11:23"
        },
        {
          "id": "MDRW2100001474.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100001474.1.3",
          "form": "백신은 맞으셨어요?",
          "original_form": "백신은 맞으셨어요?",
          "speaker_id": "2",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100001474.1.4",
          "form": "2차까지 다 맞았어요!",
          "original_form": "2차까지 다 맞았어요!",
          "speaker_id": "1",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100001474.1.5",
          "form": "오 화이자로 맞으셨나요? 모더나로 맞으셨나요?",
          "original_form": "오 화이자로 맞으셨나요? 모더나로 맞으셨나요?",
          "speaker_id": "2",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100001474.1.6",
          "form": "화이자요!",
          "original_form": "화이자요!",
          "speaker_id": "1",
          "time": "20211015 11:24"
        },
        {
          "id": "MDRW2100001474.1.7",
          "form": "맞고 나서 부작용 같은거 경험하셨나요?",
          "original_form": "맞고 나서 부작용 같은거 경험하셨나요?",
          "speaker_id": "2",
          "time": "20211015 11:25"
        },
        {
          "id": "MDRW2100001474.1.8",
          "form": "팔이 아픈거 말고는 없었어요!",
          "original_form": "팔이 아픈거 말고는 없었어요!",
          "speaker_id": "1",
          "time": "20211015 11:25"
        },
        {
          "id": "MDRW2100001474.1.9",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "2",
          "time": "20211015 11:26"
        },
        {
          "id": "MDRW2100001474.1.10",
          "form": "요즘 백신 부작용 관련해서 무서운 뉴스들이 몇개 보이다보니까",
          "original_form": "요즘 백신 부작용 관련해서 무서운 뉴스들이 몇개 보이다보니까",
          "speaker_id": "2",
          "time": "20211015 11:26"
        },
        {
          "id": "MDRW2100001474.1.11",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 11:26"
        },
        {
          "id": "MDRW2100001474.1.12",
          "form": "백신 맞기 무서운 건 어쩔 수 없는 거 같아요 ㅠㅠ",
          "original_form": "백신 맞기 무서운 건 어쩔 수 없는 거 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211015 11:27"
        },
        {
          "id": "MDRW2100001474.1.13",
          "form": "맞아요 ㅠㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20211015 11:28"
        },
        {
          "id": "MDRW2100001474.1.14",
          "form": "그래도 혹시 모르니까 백신 맞는게 좋죠!",
          "original_form": "그래도 혹시 모르니까 백신 맞는게 좋죠!",
          "speaker_id": "1",
          "time": "20211015 11:28"
        },
        {
          "id": "MDRW2100001474.1.15",
          "form": "그쵸그쵸 ㅎㅎ",
          "original_form": "그쵸그쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 11:29"
        },
        {
          "id": "MDRW2100001474.1.16",
          "form": "부스트샷에 대한 얘기도 나오던데",
          "original_form": "부스트샷에 대한 얘기도 나오던데",
          "speaker_id": "2",
          "time": "20211015 11:29"
        },
        {
          "id": "MDRW2100001474.1.17",
          "form": "그거에 대해선 어떻게 생각하세요?",
          "original_form": "그거에 대해선 어떻게 생각하세요?",
          "speaker_id": "2",
          "time": "20211015 11:29"
        },
        {
          "id": "MDRW2100001474.1.18",
          "form": "안전하다는 것만 확인되면 맞아도 될 것 같아요!",
          "original_form": "안전하다는 것만 확인되면 맞아도 될 것 같아요!",
          "speaker_id": "1",
          "time": "20211015 11:30"
        },
        {
          "id": "MDRW2100001474.1.19",
          "form": "안전이 최우선이죠",
          "original_form": "안전이 최우선이죠",
          "speaker_id": "1",
          "time": "20211015 11:30"
        },
        {
          "id": "MDRW2100001474.1.20",
          "form": "맞아요 안전이 최우선이에요~",
          "original_form": "맞아요 안전이 최우선이에요~",
          "speaker_id": "2",
          "time": "20211015 11:30"
        },
        {
          "id": "MDRW2100001474.1.21",
          "form": "건강이 최고다..!",
          "original_form": "건강이 최고다..!",
          "speaker_id": "2",
          "time": "20211015 11:30"
        }
