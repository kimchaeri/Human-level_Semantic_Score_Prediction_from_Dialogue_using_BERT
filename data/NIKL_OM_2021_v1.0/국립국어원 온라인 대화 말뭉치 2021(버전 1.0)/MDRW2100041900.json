      "utterance": [
        {
          "id": "MDRW2100041900.1.1",
          "form": "넹 중식 최근에도 먹었네요",
          "original_form": "넹 중식 최근에도 먹었네요",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041900.1.2",
          "form": "중식 짬뽕 짜장",
          "original_form": "중식 짬뽕 짜장",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041900.1.3",
          "form": "둘중 뭐 좋아하세요",
          "original_form": "둘중 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041900.1.4",
          "form": "전.. 요즘 다시 짬뽕",
          "original_form": "전.. 요즘 다시 짬뽕",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041900.1.5",
          "form": "계속 왔다갔다~",
          "original_form": "계속 왔다갔다~",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041900.1.6",
          "form": "아하 둘다군요",
          "original_form": "아하 둘다군요",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041900.1.7",
          "form": "전 짬뽕입니다",
          "original_form": "전 짬뽕입니다",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041900.1.8",
          "form": "짬뽕 굿굿",
          "original_form": "짬뽕 굿굿",
          "speaker_id": "2",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041900.1.9",
          "form": "저 둘은 맨날 갈리네요",
          "original_form": "저 둘은 맨날 갈리네요",
          "speaker_id": "2",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041900.1.10",
          "form": "그래서 짬짜면이 있죠",
          "original_form": "그래서 짬짜면이 있죠",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041900.1.11",
          "form": "짜장은 은근 질리더라구요",
          "original_form": "짜장은 은근 질리더라구요",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041900.1.12",
          "form": "음 ㅠ 맞아요",
          "original_form": "음 ㅠ 맞아요",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041900.1.13",
          "form": "짬짜면은 혁명.",
          "original_form": "짬짜면은 혁명.",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041900.1.14",
          "form": "진짜 혁명이요",
          "original_form": "진짜 혁명이요",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041900.1.15",
          "form": "탕수육은",
          "original_form": "탕수육은",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041900.1.16",
          "form": "부먹인가요 찍먹인가요",
          "original_form": "부먹인가요 찍먹인가요",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041900.1.17",
          "form": "전 찍먹.. 그 간장 찍어먹는 거 좋아해요",
          "original_form": "전 찍먹.. 그 간장 찍어먹는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041900.1.18",
          "form": "저도 찍먹이요 !!!!",
          "original_form": "저도 찍먹이요 !!!!",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041900.1.19",
          "form": "탕수육도 간장 찍어드세요?",
          "original_form": "탕수육도 간장 찍어드세요?",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041900.1.20",
          "form": "간장 찍으면 .. 맛있더라고요",
          "original_form": "간장 찍으면 .. 맛있더라고요",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041900.1.21",
          "form": "오 새로운 레시피를 알게 된 느낌이에요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 새로운 레시피를 알게 된 느낌이에요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041900.1.22",
          "form": "악ㅋㅋㅋㅋㅋㅋ 하긴 별로 이렇게 안 먹으니까요..",
          "original_form": "악ㅋㅋㅋㅋㅋㅋ 하긴 별로 이렇게 안 먹으니까요..",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041900.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ네 신기하네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ네 신기하네요",
