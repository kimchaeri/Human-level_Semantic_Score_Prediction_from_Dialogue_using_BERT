      "utterance": [
        {
          "id": "MDRW2100025244.1.1",
          "form": "이상형 있으세요?",
          "original_form": "이상형 있으세요?",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025244.1.2",
          "form": "이해심 깊은 사람이요 ㅋㅋ",
          "original_form": "이해심 깊은 사람이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025244.1.3",
          "form": "그건 모두가 공통적으로 하는 생각 아닐까요ㅋㅋ 저두요",
          "original_form": "그건 모두가 공통적으로 하는 생각 아닐까요ㅋㅋ 저두요",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025244.1.4",
          "form": "음 낭비 안해야되고",
          "original_form": "음 낭비 안해야되고",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025244.1.5",
          "form": "여행 가치관 맞는 사람이요 ㅎㅎ",
          "original_form": "여행 가치관 맞는 사람이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025244.1.6",
          "form": "여행가치관이 뚜렷하신가봐요",
          "original_form": "여행가치관이 뚜렷하신가봐요",
          "speaker_id": "2",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025244.1.7",
          "form": "음 막 고급스럽게 한번 가느니",
          "original_form": "음 막 고급스럽게 한번 가느니",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025244.1.8",
          "form": "조금 힘들어도 두번!",
          "original_form": "조금 힘들어도 두번!",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025244.1.9",
          "form": "이 제 가치관이라서ㅋㅋ",
          "original_form": "이 제 가치관이라서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025244.1.10",
          "form": "자주 다니고 싶어서요",
          "original_form": "자주 다니고 싶어서요",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025244.1.11",
          "form": "저랑 비슷하시네요ㅋㅋ 여행 한번에 돈 쏟아붓는건 아깝고",
          "original_form": "저랑 비슷하시네요ㅋㅋ 여행 한번에 돈 쏟아붓는건 아깝고",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025244.1.12",
          "form": "여러군데 다양하게 다녀보고 싶어요",
          "original_form": "여러군데 다양하게 다녀보고 싶어요",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025244.1.13",
          "form": "네 럭셔리하게 한번 vs 조금 힘들어도 두번 ㅋㅋ",
          "original_form": "네 럭셔리하게 한번 vs 조금 힘들어도 두번 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:24"
        },
        {
          "id": "MDRW2100025244.1.14",
          "form": "후자입니다ㅋㅋ",
          "original_form": "후자입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025244.1.15",
          "form": "음식도 현지식도 좀 먹어보고 ㅎㅎ",
          "original_form": "음식도 현지식도 좀 먹어보고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025244.1.16",
          "form": "맞아요 여행가면 현지식을 먹어야죠",
          "original_form": "맞아요 여행가면 현지식을 먹어야죠",
          "speaker_id": "2",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100025244.1.17",
          "form": "동남아가서",
          "original_form": "동남아가서",
          "speaker_id": "1",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100025244.1.18",
          "form": "레스토랑",
          "original_form": "레스토랑",
          "speaker_id": "1",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100025244.1.19",
          "form": "가고 이런거 안좋아해서요 ㅎㅎ",
          "original_form": "가고 이런거 안좋아해서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100025244.1.20",
          "form": "저도 현지 느낌 물씬 나는곳이 좋지 고급진 데는 아닌거 같아요",
          "original_form": "저도 현지 느낌 물씬 나는곳이 좋지 고급진 데는 아닌거 같아요",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025244.1.21",
          "form": "좋은 취향이십니다!!",
          "original_form": "좋은 취향이십니다!!",
          "speaker_id": "1",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100025244.1.22",
          "form": "뭐 사람바이사람 이니까 사실",
          "original_form": "뭐 사람바이사람 이니까 사실",
          "speaker_id": "1",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100025244.1.23",
          "form": "맞는 사람들끼리 여행 다니면 되죠 ㅎㅎ",
          "original_form": "맞는 사람들끼리 여행 다니면 되죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100025244.1.24",
          "form": "맞아요ㅎㅎ 정 안되면 혼자 가도 되구요",
          "original_form": "맞아요ㅎㅎ 정 안되면 혼자 가도 되구요",
