      "utterance": [
        {
          "id": "MDRW2100009960.1.1",
          "form": "싸이월드 부활하면 다들 잘 할까?",
          "original_form": "싸이월드 부활하면 다들 잘 할까?",
          "speaker_id": "1",
          "time": "20210823 12:17"
        },
        {
          "id": "MDRW2100009960.1.2",
          "form": "으 별로인 듯해요",
          "original_form": "으 별로인 듯해요",
          "speaker_id": "2",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009960.1.3",
          "form": "흑역사가 많겠지?",
          "original_form": "흑역사가 많겠지?",
          "speaker_id": "2",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009960.1.4",
          "form": "응 당장 name2씨부터 ㅋㅋ흑역사 생성",
          "original_form": "응 당장 &name2&씨부터 ㅋㅋ흑역사 생성",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009960.1.5",
          "form": "전 떳떳해요ㅋㅋ",
          "original_form": "전 떳떳해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009960.1.6",
          "form": "부활되었나요?",
          "original_form": "부활되었나요?",
          "speaker_id": "2",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009960.1.7",
          "form": "당장은 아니지 않나?",
          "original_form": "당장은 아니지 않나?",
          "speaker_id": "1",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009960.1.8",
          "form": "싸이 하셨어요?",
          "original_form": "싸이 하셨어요?",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009960.1.9",
          "form": "도토리 열심히 모았죠",
          "original_form": "도토리 열심히 모았죠",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009960.1.10",
          "form": "부활되면 재미도 있을것같은데 ㅎㅎ",
          "original_form": "부활되면 재미도 있을것같은데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009960.1.11",
          "form": "일촌 맺을래요?",
          "original_form": "일촌 맺을래요?",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009960.1.12",
          "form": "커플은 못해요",
          "original_form": "커플은 못해요",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009960.1.13",
          "form": "ㅋㅋ 아무래도 그렇죠",
          "original_form": "ㅋㅋ 아무래도 그렇죠",
          "speaker_id": "2",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009960.1.14",
          "form": "맺어져 있는거 아닙니까?",
          "original_form": "맺어져 있는거 아닙니까?",
          "speaker_id": "2",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009960.1.15",
          "form": "그렇겠죠",
          "original_form": "그렇겠죠",
          "speaker_id": "1",
          "time": "20210823 12:31"
        },
        {
          "id": "MDRW2100009960.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009960.1.17",
          "form": "싸이가 안 없어지는게 대단하네요",
          "original_form": "싸이가 안 없어지는게 대단하네요",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009960.1.18",
          "form": "사람은 죽어도 싸이는 남네요",
          "original_form": "사람은 죽어도 싸이는 남네요",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009960.1.19",
          "form": "ㅋㅋㅋ싸이없어진다고해서 사진파일 저장했는데 일기들은--",
          "original_form": "ㅋㅋㅋ싸이없어진다고해서 사진파일 저장했는데 일기들은--",
          "speaker_id": "1",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100009960.1.20",
          "form": "사진 저장했어요? 궁금하네요",
          "original_form": "사진 저장했어요? 궁금하네요",
          "speaker_id": "2",
          "time": "20210823 13:37"
        }
