      "utterance": [
        {
          "id": "MDRW2100041400.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041400.1.2",
          "form": "안녕하세요 직장 다니시나요?",
          "original_form": "안녕하세요 직장 다니시나요?",
          "speaker_id": "2",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100041400.1.3",
          "form": "네 출근준비중이요!",
          "original_form": "네 출근준비중이요!",
          "speaker_id": "1",
          "time": "20210906 07:57"
        },
        {
          "id": "MDRW2100041400.1.4",
          "form": "직장은 적성에 맞으세요?",
          "original_form": "직장은 적성에 맞으세요?",
          "speaker_id": "2",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100041400.1.5",
          "form": "ㅋㅋ 적성.. ㅋㅋ먹고 살라고ㅋㅋ",
          "original_form": "ㅋㅋ 적성.. ㅋㅋ먹고 살라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 09:13"
        },
        {
          "id": "MDRW2100041400.1.6",
          "form": "직장다니시나요?",
          "original_form": "직장다니시나요?",
          "speaker_id": "1",
          "time": "20210906 09:14"
        },
        {
          "id": "MDRW2100041400.1.7",
          "form": "ㅎㅎ 네 다닙니다",
          "original_form": "ㅎㅎ 네 다닙니다",
          "speaker_id": "2",
          "time": "20210906 09:14"
        },
        {
          "id": "MDRW2100041400.1.8",
          "form": "원하던 일 하시나요?",
          "original_form": "원하던 일 하시나요?",
          "speaker_id": "1",
          "time": "20210906 09:16"
        },
        {
          "id": "MDRW2100041400.1.9",
          "form": "ㅎ..원했던 일인데",
          "original_form": "ㅎ..원했던 일인데",
          "speaker_id": "2",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100041400.1.10",
          "form": "지금은 아니네요ㅜㅜㅜ",
          "original_form": "지금은 아니네요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100041400.1.11",
          "form": "첫번째 직장인가요",
          "original_form": "첫번째 직장인가요",
          "speaker_id": "2",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100041400.1.12",
          "form": "ㅋㅋ 아뇨 지금 네번째?? 아마 마지막 직장이 될거같아요",
          "original_form": "ㅋㅋ 아뇨 지금 네번째?? 아마 마지막 직장이 될거같아요",
          "speaker_id": "1",
          "time": "20210906 10:00"
        },
        {
          "id": "MDRW2100041400.1.13",
          "form": "아ㅎ 마음 먹으셨군요\n.!",
          "original_form": "아ㅎ 마음 먹으셨군요\n.!",
          "speaker_id": "2",
          "time": "20210906 10:01"
        },
        {
          "id": "MDRW2100041400.1.14",
          "form": "부럽네요ㅜ",
          "original_form": "부럽네요ㅜ",
          "speaker_id": "2",
          "time": "20210906 10:01"
        },
        {
          "id": "MDRW2100041400.1.15",
          "form": "ㅋㅋ 그냥 나이가 있어서 더이상 이직이 안될거같아요ㅠㅡㅋㅋ",
          "original_form": "ㅋㅋ 그냥 나이가 있어서 더이상 이직이 안될거같아요ㅠㅡㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 10:03"
        },
        {
          "id": "MDRW2100041400.1.16",
          "form": "ㅜㅜ전 이직하고는 싶은데",
          "original_form": "ㅜㅜ전 이직하고는 싶은데",
          "speaker_id": "2",
          "time": "20210906 10:04"
        },
        {
          "id": "MDRW2100041400.1.17",
          "form": "어차피 이 업계가 그나물에 그밥이네요",
          "original_form": "어차피 이 업계가 그나물에 그밥이네요",
          "speaker_id": "2",
          "time": "20210906 10:04"
        },
        {
          "id": "MDRW2100041400.1.18",
          "form": "ㅜㅜ 근데 익숙해져서 크게 연봉점프아님 메리트가 없더라고요ㅜㅠ",
          "original_form": "ㅜㅜ 근데 익숙해져서 크게 연봉점프아님 메리트가 없더라고요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 10:10"
        },
        {
          "id": "MDRW2100041400.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 10:12"
        },
        {
          "id": "MDRW2100041400.1.20",
          "form": "이직하면 적응기도 필요하고",
          "original_form": "이직하면 적응기도 필요하고",
          "speaker_id": "2",
          "time": "20210906 10:12"
        }
      ]
