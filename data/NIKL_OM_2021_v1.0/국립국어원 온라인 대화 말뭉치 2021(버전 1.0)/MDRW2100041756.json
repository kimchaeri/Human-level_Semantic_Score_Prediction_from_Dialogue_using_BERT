      "utterance": [
        {
          "id": "MDRW2100041756.1.1",
          "form": "심지어 감독이 영화 감독이라 걔 얼굴을 너무 진득하게 보여줌",
          "original_form": "심지어 감독이 영화 감독이라 걔 얼굴을 너무 진득하게 보여줌",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041756.1.2",
          "form": "그래서 더 짜증남",
          "original_form": "그래서 더 짜증남",
          "speaker_id": "2",
