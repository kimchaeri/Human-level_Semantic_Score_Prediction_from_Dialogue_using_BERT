      "utterance": [
        {
          "id": "MDRW2100018116.1.1",
          "form": "네ㅋㅋ신기해요",
          "original_form": "네ㅋㅋ신기해요",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100018116.1.2",
          "form": "근데 수영장에서 확진자 나왔다고 오늘 문자왔네요..",
          "original_form": "근데 수영장에서 확진자 나왔다고 오늘 문자왔네요..",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100018116.1.3",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100018116.1.4",
          "form": "그래서 조퇴하고 검사받으러 가는 길이에요ㅋㅋㅋ",
          "original_form": "그래서 조퇴하고 검사받으러 가는 길이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100018116.1.5",
          "form": "음성 나오실 거예요!",
          "original_form": "음성 나오실 거예요!",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100018116.1.6",
          "form": "ㅋㅋㅋ이거 받아보셨어요? 코가 엄청 아프네욬ㅋㅋ",
          "original_form": "ㅋㅋㅋ이거 받아보셨어요? 코가 엄청 아프네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100018116.1.7",
          "form": "한번 받아봤어요",
          "original_form": "한번 받아봤어요",
          "speaker_id": "1",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100018116.1.8",
          "form": "저도 너무 아파서 눈물 찔끔...",
          "original_form": "저도 너무 아파서 눈물 찔끔...",
          "speaker_id": "1",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100018116.1.9",
          "form": "전 사실 세번째인데 적응이 안 되네요ㅋㅋㅋ",
          "original_form": "전 사실 세번째인데 적응이 안 되네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100018116.1.10",
          "form": "예방주사 맞으셨어요?",
          "original_form": "예방주사 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100018116.1.11",
          "form": "네 백신 1차는 맞았어요",
          "original_form": "네 백신 1차는 맞았어요",
          "speaker_id": "1",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100018116.1.12",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100018116.1.13",
          "form": "저는 아직요!",
          "original_form": "저는 아직요!",
          "speaker_id": "2",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100018116.1.14",
          "form": "어떠셨어요??",
          "original_form": "어떠셨어요??",
          "speaker_id": "2",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100018116.1.15",
          "form": "저는 화이자 맞았는데 3일정도 주사 맞은 팔에 근육통있고 미열있었어요! 크게 아프진 않았는데 일 하기에는 좀 힘들더라고요!",
          "original_form": "저는 화이자 맞았는데 3일정도 주사 맞은 팔에 근육통있고 미열있었어요! 크게 아프진 않았는데 일 하기에는 좀 힘들더라고요!",
          "speaker_id": "1",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100018116.1.16",
          "form": "미열이면 참을만 하셨겠네요!",
          "original_form": "미열이면 참을만 하셨겠네요!",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100018116.1.17",
          "form": "2차가 더 아프다더라고요ㅜ",
          "original_form": "2차가 더 아프다더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100018116.1.18",
          "form": "맞아요 그래서 살짝 걱정돼요ㅋ큐ㅠ",
          "original_form": "맞아요 그래서 살짝 걱정돼요ㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100018116.1.19",
          "form": "2차는 언제 맞으세요??",
          "original_form": "2차는 언제 맞으세요??",
          "speaker_id": "2",
