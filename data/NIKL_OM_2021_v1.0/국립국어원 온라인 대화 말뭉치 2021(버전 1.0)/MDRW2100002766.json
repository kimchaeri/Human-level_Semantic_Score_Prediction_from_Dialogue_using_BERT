      "utterance": [
        {
          "id": "MDRW2100002766.1.1",
          "form": "여행 좋아요?",
          "original_form": "여행 좋아요?",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002766.1.2",
          "form": "네 여행 좋아하죠!",
          "original_form": "네 여행 좋아하죠!",
          "speaker_id": "2",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002766.1.3",
          "form": "해외여행은 한번도 안가보긴했는데, 꼭 한번 가보고 싶을 정도죠!",
          "original_form": "해외여행은 한번도 안가보긴했는데, 꼭 한번 가보고 싶을 정도죠!",
          "speaker_id": "2",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002766.1.4",
          "form": "국내는 좀 다녀본거같아요 ㅎㅎ",
          "original_form": "국내는 좀 다녀본거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002766.1.5",
          "form": "코로나 이후로는 해외는 생각도 못해요",
          "original_form": "코로나 이후로는 해외는 생각도 못해요",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002766.1.6",
          "form": "그쵸 코로나때문에 가고싶어도 못가는게 현실이죠",
          "original_form": "그쵸 코로나때문에 가고싶어도 못가는게 현실이죠",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002766.1.7",
          "form": "국내도 지금 불안해서 요즘 여행을 못다니고 있네요 ㅠㅠ",
          "original_form": "국내도 지금 불안해서 요즘 여행을 못다니고 있네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002766.1.8",
          "form": "네 너무해요",
          "original_form": "네 너무해요",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002766.1.9",
          "form": "오늘도 확진자 폭증이라는데",
          "original_form": "오늘도 확진자 폭증이라는데",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002766.1.10",
          "form": "제주도는 다시 한번 꼭 가보고 싶은데",
          "original_form": "제주도는 다시 한번 꼭 가보고 싶은데",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002766.1.11",
          "form": "가볼수있을련지..모르겠네요",
          "original_form": "가볼수있을련지..모르겠네요",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002766.1.12",
          "form": "제주 좋죠",
          "original_form": "제주 좋죠",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002766.1.13",
          "form": "제주도를 수학여행때 2번 가봤는데 좋더라고요",
          "original_form": "제주도를 수학여행때 2번 가봤는데 좋더라고요",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002766.1.14",
          "form": "그래서 나중에 성인되면 꼭 가봐야지 라고 생각해서",
          "original_form": "그래서 나중에 성인되면 꼭 가봐야지 라고 생각해서",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002766.1.15",
          "form": "만약 코로나 종식되면",
          "original_form": "만약 코로나 종식되면",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002766.1.16",
          "form": "제주도 꼭 갈듯싶어요!",
          "original_form": "제주도 꼭 갈듯싶어요!",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002766.1.17",
          "form": "저도 꼭",
          "original_form": "저도 꼭",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002766.1.18",
          "form": "종식되면 국내든 해외든",
          "original_form": "종식되면 국내든 해외든",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002766.1.19",
          "form": "여행은 꼭 가보자고요~!",
          "original_form": "여행은 꼭 가보자고요~!",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002766.1.20",
          "form": "마스크 꼭 쓰고",
          "original_form": "마스크 꼭 쓰고",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002766.1.21",
          "form": "넵 마스크도 꼭 쓰고요!",
          "original_form": "넵 마스크도 꼭 쓰고요!",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002766.1.22",
          "form": "잠잠해지면 국내여행도 좀 다니고싶네요~",
          "original_form": "잠잠해지면 국내여행도 좀 다니고싶네요~",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002766.1.23",
          "form": "남해좋아요",
          "original_form": "남해좋아요",
          "speaker_id": "1",
          "time": "20210820 15:38"
        }
      ]
