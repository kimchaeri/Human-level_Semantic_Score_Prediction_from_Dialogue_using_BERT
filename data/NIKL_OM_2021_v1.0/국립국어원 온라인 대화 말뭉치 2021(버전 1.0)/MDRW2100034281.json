      "utterance": [
        {
          "id": "MDRW2100034281.1.1",
          "form": "그니까요 살빼려고 운동하면 근육도 빠진다그러더라구요ㅠ",
          "original_form": "그니까요 살빼려고 운동하면 근육도 빠진다그러더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100034281.1.2",
          "form": "그래서 더 근력운동을 열심히 해야할거같아요",
          "original_form": "그래서 더 근력운동을 열심히 해야할거같아요",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100034281.1.3",
          "form": "맞아요 골라서 뺄수있음 좋겠어요",
          "original_form": "맞아요 골라서 뺄수있음 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100034281.1.4",
          "form": "하 근데 근력운동은 안하게돼요ㅜㅠㅜ",
          "original_form": "하 근데 근력운동은 안하게돼요ㅜㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100034281.1.5",
          "form": "그니까요!",
          "original_form": "그니까요!",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100034281.1.6",
          "form": "그나마 하던게 팔 덤벨이엇는데..ㅋㅋㅋ",
          "original_form": "그나마 하던게 팔 덤벨이엇는데..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100034281.1.7",
          "form": "힘들죠 근력운동ㅠ",
          "original_form": "힘들죠 근력운동ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100034281.1.8",
          "form": "맞아여 ㅜ",
          "original_form": "맞아여 ㅜ",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100034281.1.9",
          "form": "헬스장에선 근력운동 뭐뭐하셧었어요?",
          "original_form": "헬스장에선 근력운동 뭐뭐하셧었어요?",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100034281.1.10",
          "form": "데드리프트나 스퀴트 하구요",
          "original_form": "데드리프트나 스퀴트 하구요",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100034281.1.11",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100034281.1.12",
          "form": "스쿼트는 어디서도 해야하는군여 ...",
          "original_form": "스쿼트는 어디서도 해야하는군여 ...",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100034281.1.13",
          "form": "그 다리로 하는거 있는데 이름은 모르겠네요",
          "original_form": "그 다리로 하는거 있는데 이름은 모르겠네요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100034281.1.14",
          "form": "흠 뭔지 궁금해지네요 ㅋㅋㅋㅋ",
          "original_form": "흠 뭔지 궁금해지네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100034281.1.15",
          "form": "근데 하기싫다고하면 다른거 시키지 않을까요?",
          "original_form": "근데 하기싫다고하면 다른거 시키지 않을까요?",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100034281.1.16",
          "form": "확실히 거기서하는게 집보다 낫죠?",
          "original_form": "확실히 거기서하는게 집보다 낫죠?",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100034281.1.17",
          "form": "훨씬 낫죠",
          "original_form": "훨씬 낫죠",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100034281.1.18",
          "form": "아 그런가요? 전 피티를 안 받아봐서요 ㅋㅋㅋ",
          "original_form": "아 그런가요? 전 피티를 안 받아봐서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100034281.1.19",
