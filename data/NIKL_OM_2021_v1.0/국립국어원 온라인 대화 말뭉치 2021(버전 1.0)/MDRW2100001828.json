      "utterance": [
        {
          "id": "MDRW2100001828.1.1",
          "form": "요즘 재택근무 어케 생각하시나요",
          "original_form": "요즘 재택근무 어케 생각하시나요",
          "speaker_id": "1",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001828.1.2",
          "form": "아주 훌륭하다고 생각합니다. 모든게 아름답게 보여요",
          "original_form": "아주 훌륭하다고 생각합니다. 모든게 아름답게 보여요",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001828.1.3",
          "form": "출퇴근 지옥안겪어도 좋은거 같아요",
          "original_form": "출퇴근 지옥안겪어도 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001828.1.4",
          "form": "네 그리고 저는 드립커피를 만들어 마시는데 그것으로 하루 시작하니까 아주 행복합니다",
          "original_form": "네 그리고 저는 드립커피를 만들어 마시는데 그것으로 하루 시작하니까 아주 행복합니다",
          "speaker_id": "2",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100001828.1.5",
          "form": "우와 멋지네요",
          "original_form": "우와 멋지네요",
          "speaker_id": "1",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001828.1.6",
          "form": "그것이 재택근무 현대인의 근무",
          "original_form": "그것이 재택근무 현대인의 근무",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001828.1.7",
          "form": "누워서도 할수이시고",
          "original_form": "누워서도 할수이시고",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001828.1.8",
          "form": "누워서는... 안해봤네요",
          "original_form": "누워서는... 안해봤네요",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001828.1.9",
          "form": "저는 온라인수업 누워서 ㅋㅋ",
          "original_form": "저는 온라인수업 누워서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001828.1.10",
          "form": "카메라 체크 하지않나요",
          "original_form": "카메라 체크 하지않나요",
          "speaker_id": "2",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001828.1.11",
          "form": "하는건 앉아서 하는데 안켜도되는게 더많아서",
          "original_form": "하는건 앉아서 하는데 안켜도되는게 더많아서",
          "speaker_id": "1",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001828.1.12",
          "form": "아 그렇군요 저도 이참에 드르렁 해야겠습니다",
          "original_form": "아 그렇군요 저도 이참에 드르렁 해야겠습니다",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001828.1.13",
          "form": "ㅋㅋㅋ 그러세요 ㅎㅎ",
          "original_form": "ㅋㅋㅋ 그러세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:26"
        }
