      "utterance": [
        {
          "id": "MDRW2100023046.1.1",
          "form": "음 스포츠 좋아하세요",
          "original_form": "음 스포츠 좋아하세요",
          "speaker_id": "1",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100023046.1.2",
          "form": "운동을 잘 못해서요 ㅎㅎㅎ",
          "original_form": "운동을 잘 못해서요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100023046.1.3",
          "form": "저는 스포츠 애니는 좋아해요 ㅋㅋ",
          "original_form": "저는 스포츠 애니는 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:20"
        },
        {
          "id": "MDRW2100023046.1.4",
          "form": "아니면 한때 야구 직관한적 있기도해요",
          "original_form": "아니면 한때 야구 직관한적 있기도해요",
          "speaker_id": "1",
          "time": "20210831 11:21"
        },
        {
          "id": "MDRW2100023046.1.5",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210831 11:21"
        },
        {
          "id": "MDRW2100023046.1.6",
          "form": "저는 두어번 가본게 다에요",
          "original_form": "저는 두어번 가본게 다에요",
          "speaker_id": "2",
          "time": "20210831 11:21"
        },
        {
          "id": "MDRW2100023046.1.7",
          "form": "저도 많이가보진 않았네요 ㅋㅋ",
          "original_form": "저도 많이가보진 않았네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:22"
        },
        {
          "id": "MDRW2100023046.1.8",
          "form": "유일한 직관경험은 그게 다인듯",
          "original_form": "유일한 직관경험은 그게 다인듯",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100023046.1.9",
          "form": "잘안보였어요 ㅜㅜ 그래도뭔가 떠들썩한게 좋았어요",
          "original_form": "잘안보였어요 ㅜㅜ 그래도뭔가 떠들썩한게 좋았어요",
          "speaker_id": "2",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100023046.1.10",
          "form": "커다란 화면이 있긴하던데 ㅋㅋ",
          "original_form": "커다란 화면이 있긴하던데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:24"
        },
        {
          "id": "MDRW2100023046.1.11",
          "form": "티비가 사실더 잘보이긴해요 ㅋㅋ",
          "original_form": "티비가 사실더 잘보이긴해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100023046.1.12",
          "form": "네 그건그래요",
          "original_form": "네 그건그래요",
          "speaker_id": "2",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100023046.1.13",
          "form": "근데 저는 봐도 뭐가 스트라이크인지 잘 모르겠더라구요",
          "original_form": "근데 저는 봐도 뭐가 스트라이크인지 잘 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100023046.1.14",
          "form": "음 사실 직관은 그 분위기때문에 가는건가 싶더라고요",
          "original_form": "음 사실 직관은 그 분위기때문에 가는건가 싶더라고요",
          "speaker_id": "1",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100023046.1.15",
          "form": "네 그죠",
          "original_form": "네 그죠",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100023046.1.16",
          "form": "여럿이 가면 재밌는...?",
          "original_form": "여럿이 가면 재밌는...?",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100023046.1.17",
          "form": "아님 치킨과 맥주? ㅋㅋㅋ",
          "original_form": "아님 치킨과 맥주? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100023046.1.18",
          "form": "네 그것도있구요 ㅋㅋ 먹는재미로 ㅋㅋㅋ",
          "original_form": "네 그것도있구요 ㅋㅋ 먹는재미로 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100023046.1.19",
          "form": "요즘 시국엔 이런저런걸 다 못즐겨서 슬프긴해요",
          "original_form": "요즘 시국엔 이런저런걸 다 못즐겨서 슬프긴해요",
          "speaker_id": "1",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100023046.1.20",
          "form": "올림픽도 그냥저냥끝났고 ㅋㅋ",
          "original_form": "올림픽도 그냥저냥끝났고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:36"
        }
      ]
