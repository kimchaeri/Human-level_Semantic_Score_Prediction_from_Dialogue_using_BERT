        {
          "id": "MDRW2100046758.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 12:31"
        },
        {
          "id": "MDRW2100046758.1.3",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 12:31"
        },
        {
          "id": "MDRW2100046758.1.4",
          "form": "좋은 아침입니다 ㅎㅎ",
          "original_form": "좋은 아침입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 07:03"
        },
        {
          "id": "MDRW2100046758.1.5",
          "form": "요즘 보는 콘텐츠 있으세요? 저는 어제 드라마 보다 잠들었어요",
          "original_form": "요즘 보는 콘텐츠 있으세요? 저는 어제 드라마 보다 잠들었어요",
          "speaker_id": "2",
          "time": "20211104 07:03"
        },
        {
          "id": "MDRW2100046758.1.6",
          "form": "쇼미더머니요 ㅎㅎ",
          "original_form": "쇼미더머니요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 07:10"
        },
        {
          "id": "MDRW2100046758.1.7",
          "form": "오 랩이랑 힙합은 항상 멋진거같아요",
          "original_form": "오 랩이랑 힙합은 항상 멋진거같아요",
          "speaker_id": "2",
          "time": "20211104 07:58"
        },
        {
          "id": "MDRW2100046758.1.8",
          "form": "곡이나 래퍼 추천해주시면 저 출근할 때 들을래요 ㅋㅋ",
          "original_form": "곡이나 래퍼 추천해주시면 저 출근할 때 들을래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 08:20"
        },
        {
          "id": "MDRW2100046758.1.9",
          "form": "비오",
          "original_form": "비오",
          "speaker_id": "1",
          "time": "20211104 22:50"
        },
        {
          "id": "MDRW2100046758.1.10",
          "form": "카운팅스타 좋아요 ㅎㅎㅎㅎ",
          "original_form": "카운팅스타 좋아요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 22:50"
        },
        {
          "id": "MDRW2100046758.1.11",
          "form": "호미들 아세요?",
          "original_form": "호미들 아세요?",
          "speaker_id": "1",
          "time": "20211104 22:50"
        },
        {
          "id": "MDRW2100046758.1.12",
          "form": "사이렌이나 300도 좋아요",
          "original_form": "사이렌이나 300도 좋아요",
          "speaker_id": "1",
          "time": "20211104 22:51"
        },
        {
          "id": "MDRW2100046758.1.13",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20211104 22:57"
        },
        {
          "id": "MDRW2100046758.1.14",
          "form": "제가 래퍼를 잘 몰라서 항상 클립 올라오면 그것만 봤거든요",
          "original_form": "제가 래퍼를 잘 몰라서 항상 클립 올라오면 그것만 봤거든요",
          "speaker_id": "2",
          "time": "20211104 22:57"
        },
        {
          "id": "MDRW2100046758.1.15",
          "form": "가사가 잘 안들리기도 하고 ㅠ",
          "original_form": "가사가 잘 안들리기도 하고 ㅠ",
          "speaker_id": "2",
          "time": "20211104 22:58"
        },
        {
          "id": "MDRW2100046758.1.16",
          "form": "딕션 좋아서 잘들리실거에요",
          "original_form": "딕션 좋아서 잘들리실거에요",
          "speaker_id": "1",
          "time": "20211104 23:00"
        },
        {
          "id": "MDRW2100046758.1.17",
          "form": "호미들 노래 좋아요",
          "original_form": "호미들 노래 좋아요",
          "speaker_id": "1",
          "time": "20211104 23:00"
        },
        {
          "id": "MDRW2100046758.1.18",
          "form": "래퍼들은 실력 볼 때 주로 어떤걸보나요?",
          "original_form": "래퍼들은 실력 볼 때 주로 어떤걸보나요?",
          "speaker_id": "2",
          "time": "20211104 23:00"
        },
        {
          "id": "MDRW2100046758.1.19",
          "form": "가사나 플로우",
          "original_form": "가사나 플로우",
          "speaker_id": "1",
          "time": "20211104 23:01"
        },
        {
          "id": "MDRW2100046758.1.20",
          "form": "보이스나",
          "original_form": "보이스나",
          "speaker_id": "1",
          "time": "20211104 23:01"
        },
        {
          "id": "MDRW2100046758.1.21",
          "form": "걍 내뱉는 스킬요 ㅋㅋㅋ",
          "original_form": "걍 내뱉는 스킬요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 23:01"
        },
        {
          "id": "MDRW2100046758.1.22",
          "form": "앗ㅋㅋㅋㅋ그렇군요",
          "original_form": "앗ㅋㅋㅋㅋ그렇군요",
          "speaker_id": "2",
          "time": "20211104 23:24"
        },
        {
          "id": "MDRW2100046758.1.23",
          "form": "영상 좀 보고 왔는데",
          "original_form": "영상 좀 보고 왔는데",
          "speaker_id": "2",
          "time": "20211104 23:24"
        },
        {
          "id": "MDRW2100046758.1.24",
          "form": "밑에 가사가 보이니까 잘 들리는 것도 같네요",
