      "utterance": [
        {
          "id": "MDRW2100024868.1.1",
          "form": "ㅋㅋㅋㄱ너무 궁금해요!!?",
          "original_form": "ㅋㅋㅋㄱ너무 궁금해요!!?",
          "speaker_id": "2",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100024868.1.2",
          "form": "어휴 아녜요 그게 현실이잖아용",
          "original_form": "어휴 아녜요 그게 현실이잖아용",
          "speaker_id": "2",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100024868.1.3",
          "form": "학겨에서 단체로 할머니 할아버지들 무료 급식 봉사하러 갔는데요",
          "original_form": "학겨에서 단체로 할머니 할아버지들 무료 급식 봉사하러 갔는데요",
          "speaker_id": "1",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100024868.1.4",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024868.1.5",
          "form": "밥 나르고 반찬 퍼주고 이런 거 다 했는데 한참 있다가 시의원이 왔어요",
          "original_form": "밥 나르고 반찬 퍼주고 이런 거 다 했는데 한참 있다가 시의원이 왔어요",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100024868.1.6",
          "form": "뉴스에 그런 사진 보면 밥 퍼주는 사진이 젤 사진 잘 나오잖아요",
          "original_form": "뉴스에 그런 사진 보면 밥 퍼주는 사진이 젤 사진 잘 나오잖아요",
          "speaker_id": "1",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100024868.1.7",
          "form": "네네! 본적있어요",
          "original_form": "네네! 본적있어요",
          "speaker_id": "2",
          "time": "20210831 20:08"
        },
        {
          "id": "MDRW2100024868.1.8",
          "form": "시의원이 와 가지고 밥 퍼는 거 5분 정도 하더니 사진 찍고 갔어요",
          "original_form": "시의원이 와 가지고 밥 퍼는 거 5분 정도 하더니 사진 찍고 갔어요",
          "speaker_id": "1",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100024868.1.9",
          "form": "와ㅋㅋㅋㅋ드라마 그대로네요",
          "original_form": "와ㅋㅋㅋㅋ드라마 그대로네요",
          "speaker_id": "2",
          "time": "20210831 20:13"
        },
        {
          "id": "MDRW2100024868.1.10",
          "form": "네 허무하고 웃겼어요 일은 우리가 다 하고 사진은 시의원이 다 찍고",
          "original_form": "네 허무하고 웃겼어요 일은 우리가 다 하고 사진은 시의원이 다 찍고",
          "speaker_id": "1",
          "time": "20210831 20:13"
        },
        {
          "id": "MDRW2100024868.1.11",
          "form": "진짜요ㅋㅋㅋㅋ",
          "original_form": "진짜요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024868.1.12",
          "form": "사진 5분에..ㅋㅋㅋ",
          "original_form": "사진 5분에..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024868.1.13",
          "form": "세상은 참 이면이 많네요",
          "original_form": "세상은 참 이면이 많네요",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024868.1.14",
          "form": "남 들러리 세우고 남 불쌍한 사람 잘 만드는 사람들이 정치의 소질이 있는 거 같아요",
          "original_form": "남 들러리 세우고 남 불쌍한 사람 잘 만드는 사람들이 정치의 소질이 있는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024868.1.15",
          "form": "진짜...",
          "original_form": "진짜...",
          "speaker_id": "2",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024868.1.16",
          "form": "그 자리에 올라가도 다 그렇게 바뀌기도 하는거같아요",
          "original_form": "그 자리에 올라가도 다 그렇게 바뀌기도 하는거같아요",
          "speaker_id": "2",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100024868.1.17",
          "form": "그리고 그 사람이 정말 착하고 정의로운 사람이면 그 밑에 사람이 안 붙는 거 같아요",
          "original_form": "그리고 그 사람이 정말 착하고 정의로운 사람이면 그 밑에 사람이 안 붙는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100024868.1.18",
          "form": "아ㅜㅜㅜㅜㅠ 진짜 슬프네요",
          "original_form": "아ㅜㅜㅜㅜㅠ 진짜 슬프네요",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100024868.1.19",
          "form": "봉사를 하는곳에서도 엿볼 수 있네요",
          "original_form": "봉사를 하는곳에서도 엿볼 수 있네요",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100024868.1.20",
          "form": "그러니까요 ㅠㅠㅠ",
          "original_form": "그러니까요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100024868.1.21",
          "form": "저는 그냥 어린이집이나 도서관 봉사시간 받으려고 해본거밖에 없어서 그런건 몰랐어요",
          "original_form": "저는 그냥 어린이집이나 도서관 봉사시간 받으려고 해본거밖에 없어서 그런건 몰랐어요",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024868.1.22",
          "form": "놀랍기도하고 씁쓸하기도하네요",
          "original_form": "놀랍기도하고 씁쓸하기도하네요",
          "speaker_id": "2",
          "time": "20210831 20:29"
        }
      ]
    }
