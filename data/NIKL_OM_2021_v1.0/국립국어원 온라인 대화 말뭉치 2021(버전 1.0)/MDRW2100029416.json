      "utterance": [
        {
          "id": "MDRW2100029416.1.1",
          "form": "내가 사람들이랑 이야기 해보니까",
          "original_form": "내가 사람들이랑 이야기 해보니까",
          "speaker_id": "2",
          "time": "20210903 07:31"
        },
        {
          "id": "MDRW2100029416.1.2",
          "form": "이게 돈내고 피티 받는 것 뿐 아니라",
          "original_form": "이게 돈내고 피티 받는 것 뿐 아니라",
          "speaker_id": "2",
          "time": "20210903 07:31"
        },
        {
          "id": "MDRW2100029416.1.3",
          "form": "영상 보고 따라하는 것도 포함이더라",
          "original_form": "영상 보고 따라하는 것도 포함이더라",
          "speaker_id": "2",
          "time": "20210903 07:32"
        },
        {
          "id": "MDRW2100029416.1.4",
          "form": "아 저도 나중에 알았어오",
          "original_form": "아 저도 나중에 알았어오",
          "speaker_id": "1",
          "time": "20210903 07:32"
        },
        {
          "id": "MDRW2100029416.1.5",
          "form": "땅끄부부 같은거요",
          "original_form": "땅끄부부 같은거요",
          "speaker_id": "1",
          "time": "20210903 07:32"
        },
        {
          "id": "MDRW2100029416.1.6",
          "form": "그래.. 그럼 우리도 한거잖아??",
          "original_form": "그래.. 그럼 우리도 한거잖아??",
          "speaker_id": "2",
          "time": "20210903 07:32"
        },
        {
          "id": "MDRW2100029416.1.7",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210903 07:32"
        },
        {
          "id": "MDRW2100029416.1.8",
          "form": "완전 새로운 무언가인줄 알았더니 그게 아니라 워래 하던거였어",
          "original_form": "완전 새로운 무언가인줄 알았더니 그게 아니라 워래 하던거였어",
          "speaker_id": "2",
          "time": "20210903 07:33"
        },
        {
          "id": "MDRW2100029416.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 07:33"
        },
        {
          "id": "MDRW2100029416.1.10",
          "form": "그럼 그거 보면서 홈트도 하고 하면 좋잖아",
          "original_form": "그럼 그거 보면서 홈트도 하고 하면 좋잖아",
          "speaker_id": "2",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100029416.1.11",
          "form": "엄마는 혼자 안하시고 꼭 같이 해야 하나요?",
          "original_form": "엄마는 혼자 안하시고 꼭 같이 해야 하나요?",
          "speaker_id": "1",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100029416.1.12",
          "form": "같이해야 재미있지.. 혼자하면 지루해",
          "original_form": "같이해야 재미있지.. 혼자하면 지루해",
          "speaker_id": "2",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100029416.1.13",
          "form": "전 운동 자체가 지루해요",
          "original_form": "전 운동 자체가 지루해요",
          "speaker_id": "1",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029416.1.14",
          "form": "사람이 운동을 해야한다",
          "original_form": "사람이 운동을 해야한다",
          "speaker_id": "2",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029416.1.15",
          "form": "알아요",
          "original_form": "알아요",
          "speaker_id": "1",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029416.1.16",
          "form": "넌 너무 안움직여서 병나겠어",
          "original_form": "넌 너무 안움직여서 병나겠어",
          "speaker_id": "2",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029416.1.17",
          "form": "산책시작했요",
          "original_form": "산책시작했요",
          "speaker_id": "1",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029416.1.18",
          "form": "물론 그거라도 하기 시작했으니 다행이긴 한데 더 해야지",
          "original_form": "물론 그거라도 하기 시작했으니 다행이긴 한데 더 해야지",
          "speaker_id": "2",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029416.1.19",
          "form": "무리 노노요",
          "original_form": "무리 노노요",
          "speaker_id": "1",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029416.1.20",
          "form": "무리하라는ㄴ게 아니라 강도를 좀 높이라고",
          "original_form": "무리하라는ㄴ게 아니라 강도를 좀 높이라고",
          "speaker_id": "2",
          "time": "20210903 07:38"
        },
        {
          "id": "MDRW2100029416.1.21",
          "form": "지금 충분해요",
          "original_form": "지금 충분해요",
          "speaker_id": "1",
          "time": "20210903 07:38"
        }
