      "utterance": [
        {
          "id": "MDRW2100047696.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 02:00"
        },
        {
          "id": "MDRW2100047696.1.2",
          "form": "안녕하세요~ 오늘 야식 드셨나요?",
          "original_form": "안녕하세요~ 오늘 야식 드셨나요?",
          "speaker_id": "2",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100047696.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100047696.1.4",
          "form": "제가 저녁을 햄버걸먹었는데 어디껄까요?",
          "original_form": "제가 저녁을 햄버걸먹었는데 어디껄까요?",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100047696.1.5",
          "form": "맥도날드인가요? 저는 햄버거브랜드 중에 맥도날드를 제일 좋아해요",
          "original_form": "맥도날드인가요? 저는 햄버거브랜드 중에 맥도날드를 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100047696.1.6",
          "form": "땡!",
          "original_form": "땡!",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100047696.1.7",
          "form": "저도 맥날이좋지만",
          "original_form": "저도 맥날이좋지만",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100047696.1.8",
          "form": "정답은 아니랍니다",
          "original_form": "정답은 아니랍니다",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100047696.1.9",
          "form": "그럼 버거킹? 버거킹도 요즘 행사 많이 하더라구요.",
          "original_form": "그럼 버거킹? 버거킹도 요즘 행사 많이 하더라구요.",
          "speaker_id": "2",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100047696.1.10",
          "form": "ㅋㅋㅋ오 정답입니다",
          "original_form": "ㅋㅋㅋ오 정답입니다",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100047696.1.11",
          "form": "무슨버거를 먹었을까요?!",
          "original_form": "무슨버거를 먹었을까요?!",
          "speaker_id": "1",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100047696.1.12",
          "form": "기네스 와퍼 드셨나요? 전 먹어보고 싶은데 아직 못 먹어봤어요",
          "original_form": "기네스 와퍼 드셨나요? 전 먹어보고 싶은데 아직 못 먹어봤어요",
          "speaker_id": "2",
          "time": "20210827 02:45"
        },
        {
          "id": "MDRW2100047696.1.13",
          "form": "힌트",
          "original_form": "힌트",
          "speaker_id": "1",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100047696.1.14",
          "form": "새우가 들어간답니다!!!!!",
          "original_form": "새우가 들어간답니다!!!!!",
          "speaker_id": "1",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100047696.1.15",
          "form": "통새우와퍼! 맞나요?",
          "original_form": "통새우와퍼! 맞나요?",
          "speaker_id": "2",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100047696.1.16",
          "form": "전 한번도 안 먹어봤어요",
          "original_form": "전 한번도 안 먹어봤어요",
          "speaker_id": "2",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100047696.1.17",
          "form": "따흑",
          "original_form": "따흑",
          "speaker_id": "1",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100047696.1.18",
          "form": "틀렸답니다",
          "original_form": "틀렸답니다",
          "speaker_id": "1",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100047696.1.19",
          "form": "상어버거를 먹었답니다",
          "original_form": "상어버거를 먹었답니다",
          "speaker_id": "1",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100047696.1.20",
          "form": "앟ㅎ 새우버거 드셨군요~ 그거 맛있나요?",
          "original_form": "앟ㅎ 새우버거 드셨군요~ 그거 맛있나요?",
          "speaker_id": "2",
          "time": "20210827 02:48"
        },
        {
          "id": "MDRW2100047696.1.21",
          "form": "여기서 더",
          "original_form": "여기서 더",
          "speaker_id": "1",
          "time": "20210827 02:49"
        },
        {
          "id": "MDRW2100047696.1.22",
          "form": "문제가있답니다",
          "original_form": "문제가있답니다",
          "speaker_id": "1",
          "time": "20210827 02:49"
        },
        {
          "id": "MDRW2100047696.1.23",
          "form": "1 아기상어\n2 엄마상어\n3 아빠상어\n무엇을 먹었을까요",
          "original_form": "1 아기상어\n2 엄마상어\n3 아빠상어\n무엇을 먹었을까요",
          "speaker_id": "1",
          "time": "20210827 02:50"
        },
        {
          "id": "MDRW2100047696.1.24",
          "form": "엄마상어 새우버거! 맞나요? 할라피뇨 들어가서 제일 맛있을 것 같아요",
          "original_form": "엄마상어 새우버거! 맞나요? 할라피뇨 들어가서 제일 맛있을 것 같아요",
          "speaker_id": "2",
          "time": "20210827 02:51"
        },
        {
          "id": "MDRW2100047696.1.25",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210827 02:53"
        },
        {
          "id": "MDRW2100047696.1.26",
          "form": "한번에 맞추셨네요",
          "original_form": "한번에 맞추셨네요",
          "speaker_id": "1",
          "time": "20210827 02:54"
        },
        {
          "id": "MDRW2100047696.1.27",
          "form": "신기하네요ㅎㅎ 저도 생각난 김에 내일 햄버거 먹어야겠어요",
          "original_form": "신기하네요ㅎㅎ 저도 생각난 김에 내일 햄버거 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210827 02:56"
        }
      ]
    }
