      "utterance": [
        {
          "id": "MDRW2100024451.1.1",
          "form": "회사 다니시나요?",
          "original_form": "회사 다니시나요?",
          "speaker_id": "2",
          "time": "20210831 10:20"
        },
        {
          "id": "MDRW2100024451.1.2",
          "form": "회사 혹은 학교 밥 어때요?",
          "original_form": "회사 혹은 학교 밥 어때요?",
          "speaker_id": "2",
          "time": "20210831 10:20"
        },
        {
          "id": "MDRW2100024451.1.3",
          "form": "회사  다녀요",
          "original_form": "회사  다녀요",
          "speaker_id": "1",
          "time": "20210831 10:21"
        },
        {
          "id": "MDRW2100024451.1.4",
          "form": "지역상생 어쩌구 하며 구내식당을 안 만든게 함정ㅜ",
          "original_form": "지역상생 어쩌구 하며 구내식당을 안 만든게 함정ㅜ",
          "speaker_id": "1",
          "time": "20210831 10:21"
        },
        {
          "id": "MDRW2100024451.1.5",
          "form": "회사 주변 식당은 쏘쏘예요ㅜ",
          "original_form": "회사 주변 식당은 쏘쏘예요ㅜ",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024451.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024451.1.7",
          "form": "회사밥 진짜",
          "original_form": "회사밥 진짜",
          "speaker_id": "2",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024451.1.8",
          "form": "너무합니다",
          "original_form": "너무합니다",
          "speaker_id": "2",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024451.1.9",
          "form": "중소기업진흥법인가 뭐시기인가해서",
          "original_form": "중소기업진흥법인가 뭐시기인가해서",
          "speaker_id": "2",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024451.1.10",
          "form": "대기업은 못들어오거든요?",
          "original_form": "대기업은 못들어오거든요?",
          "speaker_id": "2",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024451.1.11",
          "form": "그래서 맛없어요 변화가 없습닏ㄴ",
          "original_form": "그래서 맛없어요 변화가 없습닏ㄴ",
          "speaker_id": "2",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024451.1.12",
          "form": "그쵸... 그래서 삼성 난리났던 거고ㅜ",
          "original_form": "그쵸... 그래서 삼성 난리났던 거고ㅜ",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024451.1.13",
          "form": "맛이 너무 없어요",
          "original_form": "맛이 너무 없어요",
          "speaker_id": "2",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024451.1.14",
          "form": "아이고 형편없나봐요",
          "original_form": "아이고 형편없나봐요",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024451.1.15",
          "form": "식대는 얼마인가요",
          "original_form": "식대는 얼마인가요",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024451.1.16",
          "form": "내 그래서 맨날 사먹어요",
          "original_form": "내 그래서 맨날 사먹어요",
          "speaker_id": "2",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024451.1.17",
          "form": "식대는 5000원 입니다",
          "original_form": "식대는 5000원 입니다",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024451.1.18",
          "form": "싸지요?",
          "original_form": "싸지요?",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024451.1.19",
          "form": "구내식당인거 생각하믄 쏘쏘?",
          "original_form": "구내식당인거 생각하믄 쏘쏘?",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100024451.1.20",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100024451.1.21",
          "form": "음 그럼 메뉴 너무 별로인 날은 밖에 나가시나요",
          "original_form": "음 그럼 메뉴 너무 별로인 날은 밖에 나가시나요",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100024451.1.22",
          "form": "일주일에 몇번정도?",
          "original_form": "일주일에 몇번정도?",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100024451.1.23",
          "form": "전회사(아워홈)이었는데 대학교라 메뉴가 두개씩은 나왓서요",
          "original_form": "전회사(아워홈)이었는데 대학교라 메뉴가 두개씩은 나왓서요",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100024451.1.24",
          "form": "이직하고는 주 4회 나가서 먹어요",
          "original_form": "이직하고는 주 4회 나가서 먹어요",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024451.1.25",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024451.1.26",
          "form": "아니 매일이잖아요 정말 ㅋㅋㅋ",
          "original_form": "아니 매일이잖아요 정말 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024451.1.27",
          "form": "아 매일이네여 ㅋㅋㅋ",
          "original_form": "아 매일이네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024451.1.28",
          "form": "창원 도 식 대가 꽤 비쌉니다",
          "original_form": "창원 도 식 대가 꽤 비쌉니다",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024451.1.29",
          "form": "근처밥은 맛있나요",
          "original_form": "근처밥은 맛있나요",
          "speaker_id": "1",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024451.1.30",
          "form": "서울이 더 맛있을 거 같아요",
          "original_form": "서울이 더 맛있을 거 같아요",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024451.1.31",
          "form": "서울도 서울 나름이라ㅜ",
          "original_form": "서울도 서울 나름이라ㅜ",
          "speaker_id": "1",
          "time": "20210831 10:27"
        }
      ]
    }
