      "utterance": [
        {
          "id": "MDRW2100028523.1.1",
          "form": "초면이야",
          "original_form": "초면이야",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028523.1.2",
          "form": "그럴법도",
          "original_form": "그럴법도",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028523.1.3",
          "form": "작년에 잘하고 올해 말아먹어서",
          "original_form": "작년에 잘하고 올해 말아먹어서",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028523.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028523.1.5",
          "form": "제발 잘해라... 제가 님 좋아한단 말이에요",
          "original_form": "제발 잘해라... 제가 님 좋아한단 말이에요",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028523.1.6",
          "form": "힘냅시다",
          "original_form": "힘냅시다",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028523.1.7",
          "form": "그유오",
          "original_form": "그유오",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028523.1.8",
          "form": "맞지맞지",
          "original_form": "맞지맞지",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028523.1.9",
          "form": "최동환 매력은 역시 저거지",
          "original_form": "최동환 매력은 역시 저거지",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028523.1.10",
          "form": "묵직해서 치더라도 멀리 안 나가는",
          "original_form": "묵직해서 치더라도 멀리 안 나가는",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028523.1.11",
          "form": "배부르다",
          "original_form": "배부르다",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028523.1.12",
          "form": "다음에 더 잘햇음 좋겠다...",
          "original_form": "다음에 더 잘햇음 좋겠다...",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028523.1.13",
          "form": "그러게... 넘 아쉬운",
          "original_form": "그러게... 넘 아쉬운",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028523.1.14",
          "form": "잘했는데...",
          "original_form": "잘했는데...",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028523.1.15",
          "form": "아니 웃기다",
          "original_form": "아니 웃기다",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028523.1.16",
          "form": "우리팀 너무 오랫동안 큰 점수차로 이겨서",
          "original_form": "우리팀 너무 오랫동안 큰 점수차로 이겨서",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028523.1.17",
          "form": "필승조가 너무 오래 쉬어서 지금 나오네",
          "original_form": "필승조가 너무 오래 쉬어서 지금 나오네",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028523.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028523.1.19",
          "form": "이게 우리팀이 맞냐",
          "original_form": "이게 우리팀이 맞냐",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028523.1.20",
          "form": "오랜만에 보는 얼굴",
          "original_form": "오랜만에 보는 얼굴",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028523.1.21",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028523.1.22",
          "form": "낯설다 이런 팀...",
