      "utterance": [
        {
          "id": "MDRW2100017802.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017802.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017802.1.3",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017802.1.4",
          "form": "전 지금 디저트로 나니스크림 먹었어요",
          "original_form": "전 지금 디저트로 나니스크림 먹었어요",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017802.1.5",
          "form": "저 에머에서 쌀국수 먹고 파파존스 브라우니로 입가심 했습니다!",
          "original_form": "저 에머에서 쌀국수 먹고 파파존스 브라우니로 입가심 했습니다!",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017802.1.6",
          "form": "나니스크림은 뭔가요?",
          "original_form": "나니스크림은 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017802.1.7",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017802.1.8",
          "form": "아이스크림요 ㅜㅜ",
          "original_form": "아이스크림요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017802.1.9",
          "form": "아 ㅋㅋㅋㅋㅋ 저는 ㅋㅋㅋㅋ새로운 브랜드의 아이스크림줄 알고 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ 저는 ㅋㅋㅋㅋ새로운 브랜드의 아이스크림줄 알고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017802.1.10",
          "form": "와 쌀국수에 파파존스 디저트까지 완벽하네요",
          "original_form": "와 쌀국수에 파파존스 디저트까지 완벽하네요",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017802.1.11",
          "form": "일부러 브랜드 이름 엄청 말했요",
          "original_form": "일부러 브랜드 이름 엄청 말했요",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017802.1.12",
          "form": "말했네요 ㅋㅋㅋㅋ",
          "original_form": "말했네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017802.1.13",
          "form": "제가 오해를 불렀네요 ㅋㅋ",
          "original_form": "제가 오해를 불렀네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017802.1.14",
          "form": "어머 이게 잼나네요 ㅋㅋ말이 아니라 글리라",
          "original_form": "어머 이게 잼나네요 ㅋㅋ말이 아니라 글리라",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017802.1.15",
          "form": "글이라",
          "original_form": "글이라",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017802.1.16",
          "form": "베스킨 아몬드 봉봉",
          "original_form": "베스킨 아몬드 봉봉",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017802.1.17",
          "form": "먹었어요",
          "original_form": "먹었어요",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017802.1.18",
          "form": "저녁은 동네장터 치킨요 ㅎㅎ",
          "original_form": "저녁은 동네장터 치킨요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017802.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 오늘은 저희둘 모두 저녁은 외식으로 했군요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 오늘은 저희둘 모두 저녁은 외식으로 했군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017802.1.20",
          "form": "네 ㅎㅎ그렇죠",
          "original_form": "네 ㅎㅎ그렇죠",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017802.1.21",
          "form": "밥도 했는데 어쩌다 치킨요",
          "original_form": "밥도 했는데 어쩌다 치킨요",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017802.1.22",
          "form": "아들이 사왔어요",
          "original_form": "아들이 사왔어요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017802.1.23",
          "form": "여긴 파파존스 없는데 브라우니 맛난가요?",
          "original_form": "여긴 파파존스 없는데 브라우니 맛난가요?",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017802.1.24",
          "form": "첨알았어요 파는거",
          "original_form": "첨알았어요 파는거",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017802.1.25",
          "form": "앗 약간 파파존스 매니아분들이",
          "original_form": "앗 약간 파파존스 매니아분들이",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017802.1.26",
          "form": "파파존스는 브라우니먹으러 간다고 해서",
          "original_form": "파파존스는 브라우니먹으러 간다고 해서",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017802.1.27",
          "form": "저도 호기심에 먹었는데",
          "original_form": "저도 호기심에 먹었는데",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017802.1.28",
          "form": "웬만한 카페보다 나은거 같아요",
          "original_form": "웬만한 카페보다 나은거 같아요",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017802.1.29",
          "form": "와 부러워요",
          "original_form": "와 부러워요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017802.1.30",
          "form": "여긴 지방 없어요 ㅜㅜ",
          "original_form": "여긴 지방 없어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017802.1.31",
          "form": "저도 한번 먹어보고 싶네요",
          "original_form": "저도 한번 먹어보고 싶네요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017802.1.32",
          "form": "브라우니 좋아해요 ㅎㅎ",
          "original_form": "브라우니 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017802.1.33",
          "form": "커피숍에서 잘먹었어요 ㅎㅎ",
          "original_form": "커피숍에서 잘먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017802.1.34",
          "form": "저도 브라우니 좋고 치즈케이크도 좋아해요!",
          "original_form": "저도 브라우니 좋고 치즈케이크도 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017802.1.35",
          "form": "쿠폰도 받은게 있는데 쓸 체인이 없어서 못쓰고 그래요",
          "original_form": "쿠폰도 받은게 있는데 쓸 체인이 없어서 못쓰고 그래요",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017802.1.36",
          "form": "네 저도요 치즈케이크 좋아해요",
          "original_form": "네 저도요 치즈케이크 좋아해요",
          "speaker_id": "2",
          "time": "20210826 21:19"
        }
      ]
    }
