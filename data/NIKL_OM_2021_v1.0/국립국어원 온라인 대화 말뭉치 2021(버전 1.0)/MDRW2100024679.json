        {
          "id": "MDRW2100024679.1.1",
          "form": "저녁 뭐 먹어요",
          "original_form": "저녁 뭐 먹어요",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024679.1.2",
          "form": "닭가슴살 호박잎",
          "original_form": "닭가슴살 호박잎",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100024679.1.3",
          "form": "양은...!",
          "original_form": "양은...!",
          "speaker_id": "1",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100024679.1.4",
          "form": "저는 양배추 아보카도 토마토랑 빵 먹었슴다ㅋㅋㅋ",
          "original_form": "저는 양배추 아보카도 토마토랑 빵 먹었슴다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100024679.1.5",
          "form": "무슨빵?",
          "original_form": "무슨빵?",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100024679.1.6",
          "form": "평거탑에서 어떤사람이 아보카도10개5천원주고샀다던데",
          "original_form": "평거탑에서 어떤사람이 아보카도10개5천원주고샀다던데",
          "speaker_id": "2",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100024679.1.7",
          "form": "우리 선물로 들어온 빵",
          "original_form": "우리 선물로 들어온 빵",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100024679.1.8",
          "form": "아무도 안 먹어서 저 혼자 먹고 있잖아요...",
          "original_form": "아무도 안 먹어서 저 혼자 먹고 있잖아요...",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100024679.1.9",
          "form": "아보카도 싸게 샀네요ㅋㅋㅋ와",
          "original_form": "아보카도 싸게 샀네요ㅋㅋㅋ와",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100024679.1.10",
          "form": "맞제 니생각나더라 ㅋ\r\n난 아보카도 안무니깐",
          "original_form": "맞제 니생각나더라 ㅋ\r\n난 아보카도 안무니깐",
          "speaker_id": "2",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100024679.1.11",
          "form": "아보카도 맛있는디...",
          "original_form": "아보카도 맛있는디...",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100024679.1.12",
          "form": "무슨맛인데?",
          "original_form": "무슨맛인데?",
          "speaker_id": "2",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100024679.1.13",
          "form": "기름맛?",
          "original_form": "기름맛?",
          "speaker_id": "2",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100024679.1.14",
          "form": "표현하기 힘든 맛인데",
          "original_form": "표현하기 힘든 맛인데",
          "speaker_id": "1",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100024679.1.15",
          "form": "아무리 설명해도",
          "original_form": "아무리 설명해도",
          "speaker_id": "1",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100024679.1.16",
          "form": "안먹어보면",
          "original_form": "안먹어보면",
          "speaker_id": "1",
          "time": "20210901 11:57"
        },
        {
