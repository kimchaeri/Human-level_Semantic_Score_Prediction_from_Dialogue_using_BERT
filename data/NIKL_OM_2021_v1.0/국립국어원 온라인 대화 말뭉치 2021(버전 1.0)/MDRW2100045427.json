      "utterance": [
        {
          "id": "MDRW2100045427.1.1",
          "form": "거기 그거도맛집이야",
          "original_form": "거기 그거도맛집이야",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045427.1.2",
          "form": "경주빵?황남빵?",
          "original_form": "경주빵?황남빵?",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045427.1.3",
          "form": "거기 황남빵맛있어",
          "original_form": "거기 황남빵맛있어",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045427.1.4",
          "form": "에? 그런것도팔아?",
          "original_form": "에? 그런것도팔아?",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045427.1.5",
          "form": "거기 아저씨가 그거 만드는거로 유명쓰",
          "original_form": "거기 아저씨가 그거 만드는거로 유명쓰",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045427.1.6",
          "form": "아저씨가 아니라 학부모님^^ ....",
          "original_form": "아저씨가 아니라 학부모님^^ ....",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045427.1.7",
          "form": "아 그 아저씨가 현대호텔 베이커리에서 그 황남빵만들었을걸",
          "original_form": "아 그 아저씨가 현대호텔 베이커리에서 그 황남빵만들었을걸",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045427.1.8",
          "form": "아....오 한번도 못봤어!",
          "original_form": "아....오 한번도 못봤어!",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045427.1.9",
          "form": "그 옛날에 대학병원에 입구에 황남빵같은거 팔던거기억남?",
          "original_form": "그 옛날에 대학병원에 입구에 황남빵같은거 팔던거기억남?",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045427.1.10",
          "form": "응응! 기억남",
          "original_form": "응응! 기억남",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045427.1.11",
          "form": "지금도 그런거 파는것같던데",
          "original_form": "지금도 그런거 파는것같던데",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045427.1.12",
          "form": "그게 현대호텔꺼아님?",
          "original_form": "그게 현대호텔꺼아님?",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045427.1.13",
          "form": "그래?????",
          "original_form": "그래?????",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045427.1.14",
          "form": "맞을걸 ㅋㅋㅋ 그리고 그 황남방 만든걸로",
          "original_form": "맞을걸 ㅋㅋㅋ 그리고 그 황남방 만든걸로",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045427.1.15",
          "form": "그아저씨 지역방송에도 나왔음 ㅋㅋㅋㅋ",
          "original_form": "그아저씨 지역방송에도 나왔음 ㅋㅋㅋㅋ",
