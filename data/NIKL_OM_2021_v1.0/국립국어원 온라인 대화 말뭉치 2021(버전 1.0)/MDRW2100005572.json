      "utterance": [
        {
          "id": "MDRW2100005572.1.1",
          "form": "좋아하는 책 있어?",
          "original_form": "좋아하는 책 있어?",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005572.1.2",
          "form": "나는 파울로코헬료 작품 다 좋아해",
          "original_form": "나는 파울로코헬료 작품 다 좋아해",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005572.1.3",
          "form": "뭐가 있지?",
          "original_form": "뭐가 있지?",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005572.1.4",
          "form": "유명한 건 연금술사",
          "original_form": "유명한 건 연금술사",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005572.1.5",
          "form": "아 연금술사 알지 알지",
          "original_form": "아 연금술사 알지 알지",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005572.1.6",
          "form": "파울로 코헬료 책들은 대체로 느낌이 비슷하더라고",
          "original_form": "파울로 코헬료 책들은 대체로 느낌이 비슷하더라고",
          "speaker_id": "1",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005572.1.7",
          "form": "어떻게 비슷한데",
          "original_form": "어떻게 비슷한데",
          "speaker_id": "2",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005572.1.8",
          "form": "뭔가 자아를 찾고",
          "original_form": "뭔가 자아를 찾고",
          "speaker_id": "1",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005572.1.9",
          "form": "아 자아",
          "original_form": "아 자아",
          "speaker_id": "2",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005572.1.10",
          "form": "깨달음을 주는 그런 느낌",
          "original_form": "깨달음을 주는 그런 느낌",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005572.1.11",
          "form": "자기개발서 느낌인가",
          "original_form": "자기개발서 느낌인가",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005572.1.12",
          "form": "소설이긴 한데 뭔가 좋아 ㅋㅋ",
          "original_form": "소설이긴 한데 뭔가 좋아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005572.1.13",
          "form": "재밌어?",
          "original_form": "재밌어?",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005572.1.14",
          "form": "응 재미있어",
          "original_form": "응 재미있어",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005572.1.15",
          "form": "다 흥미로워",
          "original_form": "다 흥미로워",
          "speaker_id": "1",
