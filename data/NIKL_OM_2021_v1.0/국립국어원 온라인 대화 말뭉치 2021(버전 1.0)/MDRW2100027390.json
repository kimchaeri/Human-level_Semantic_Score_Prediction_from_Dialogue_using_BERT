          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027390.1.6",
          "form": "벌써요",
          "original_form": "벌써요",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027390.1.7",
