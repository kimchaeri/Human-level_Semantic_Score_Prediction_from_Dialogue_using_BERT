      "utterance": [
        {
          "id": "MDRW2100022302.1.1",
          "form": "종교가 뭐예요?",
          "original_form": "종교가 뭐예요?",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022302.1.2",
          "form": "불교입니다",
          "original_form": "불교입니다",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022302.1.3",
          "form": "때로는 기독교 이기도하고요~",
          "original_form": "때로는 기독교 이기도하고요~",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022302.1.4",
          "form": "네 전 카톨릭",
          "original_form": "네 전 카톨릭",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022302.1.5",
          "form": "천주교는 어떤점이 좋아요?",
          "original_form": "천주교는 어떤점이 좋아요?",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022302.1.6",
          "form": "맘이 편안해져요",
          "original_form": "맘이 편안해져요",
          "speaker_id": "2",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022302.1.7",
          "form": "불교는 자유로운 분위가 좋아요~~",
          "original_form": "불교는 자유로운 분위가 좋아요~~",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022302.1.8",
          "form": "또다른건요?",
          "original_form": "또다른건요?",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022302.1.9",
          "form": "성체를 모셔서 좋아요",
          "original_form": "성체를 모셔서 좋아요",
          "speaker_id": "2",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022302.1.10",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022302.1.11",
          "form": "불교의 장점은요",
          "original_form": "불교의 장점은요",
          "speaker_id": "2",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022302.1.12",
          "form": "성체가모에요?",
          "original_form": "성체가모에요?",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022302.1.13",
          "form": "누구나 부처가 될수있다는 자유의지~",
          "original_form": "누구나 부처가 될수있다는 자유의지~",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022302.1.14",
          "form": "좀거창하죠",
          "original_form": "좀거창하죠",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022302.1.15",
          "form": "응  미사시간에 제병을 먹어요~~ 제병을 일종의 성체 하는님의 몸??",
          "original_form": "응  미사시간에 제병을 먹어요~~ 제병을 일종의 성체 하는님의 몸??",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022302.1.16",
          "form": "하느님을 모시는 거지요",
          "original_form": "하느님을 모시는 거지요",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022302.1.17",
          "form": "네 자유의지 좋아요",
          "original_form": "네 자유의지 좋아요",
          "speaker_id": "2",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022302.1.18",
          "form": "오그렇군요 오늘새로운걸 알았네요",
          "original_form": "오그렇군요 오늘새로운걸 알았네요",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022302.1.19",
          "form": "그럼 늘 하느님이겠네요~~",
          "original_form": "그럼 늘 하느님이겠네요~~",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022302.1.20",
          "form": "법경중에 좋은 구절은요?",
          "original_form": "법경중에 좋은 구절은요?",
          "speaker_id": "2",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022302.1.21",
          "form": "무소의 뿔처럼 혼자서가라~~",
          "original_form": "무소의 뿔처럼 혼자서가라~~",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022302.1.22",
          "form": "항상 하느님과 함께 있는거지요~~",
          "original_form": "항상 하느님과 함께 있는거지요~~",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022302.1.23",
          "form": "멋지네요~~",
          "original_form": "멋지네요~~",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022302.1.24",
          "form": "무소의 뿔은 무슨 뜻이예요?",
          "original_form": "무소의 뿔은 무슨 뜻이예요?",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022302.1.25",
          "form": "자기해탈??",
          "original_form": "자기해탈??",
          "speaker_id": "2",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022302.1.26",
          "form": "무소는 코뿔소인데",
          "original_form": "무소는 코뿔소인데",
          "speaker_id": "1",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022302.1.27",
          "form": "네 혼자서 가라",
          "original_form": "네 혼자서 가라",
          "speaker_id": "2",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022302.1.28",
          "form": "그냥 수행자가 가야하는길인데,,",
          "original_form": "그냥 수행자가 가야하는길인데,,",
          "speaker_id": "1",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100022302.1.29",
          "form": "초기경전 내용인데 깊이있는 내용까지 파악못했네요",
          "original_form": "초기경전 내용인데 깊이있는 내용까지 파악못했네요",
          "speaker_id": "1",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100022302.1.30",
          "form": "수행자가 혼자서 가요~~ 지침 없이",
          "original_form": "수행자가 혼자서 가요~~ 지침 없이",
          "speaker_id": "2",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022302.1.31",
          "form": "쓸쓴한길인거 같아요",
          "original_form": "쓸쓴한길인거 같아요",
          "speaker_id": "1",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022302.1.32",
          "form": "종교는 어려워요~~",
          "original_form": "종교는 어려워요~~",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022302.1.33",
          "form": "네 어려워요",
          "original_form": "네 어려워요",
          "speaker_id": "2",
          "time": "20210829 19:29"
        }
      ]
    }
