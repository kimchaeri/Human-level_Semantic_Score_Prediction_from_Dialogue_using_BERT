      "utterance": [
        {
          "id": "MDRW2100017872.1.1",
          "form": "카페메뉴 무얼 좋아하시나요?",
          "original_form": "카페메뉴 무얼 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017872.1.2",
          "form": "아아 마셨죠",
          "original_form": "아아 마셨죠",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017872.1.3",
          "form": "전 달달한게 좋은데 건강에 안좋다고하니 ㅜㅠ 속상합니다",
          "original_form": "전 달달한게 좋은데 건강에 안좋다고하니 ㅜㅠ 속상합니다",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017872.1.4",
          "form": "조금 덜 단걸 마셔요 밀크티추천합니다",
          "original_form": "조금 덜 단걸 마셔요 밀크티추천합니다",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017872.1.5",
          "form": "밀크티도 썩 건강엔...",
          "original_form": "밀크티도 썩 건강엔...",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017872.1.6",
          "form": "아.. 그런데 건강생각하면 먹을게 별로 없어요ㅜ",
          "original_form": "아.. 그런데 건강생각하면 먹을게 별로 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017872.1.7",
          "form": "ㅋㅋ 아이스크림라떼 좋아요",
          "original_form": "ㅋㅋ 아이스크림라떼 좋아요",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017872.1.8",
          "form": "스트레스가 더 안좋으니 좋아하는걸 드세요",
          "original_form": "스트레스가 더 안좋으니 좋아하는걸 드세요",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017872.1.9",
          "form": "아인슈패너랑",
          "original_form": "아인슈패너랑",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017872.1.10",
          "form": "말만 들어도 맛있겠어요",
          "original_form": "말만 들어도 맛있겠어요",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017872.1.11",
          "form": "컴포즈꺼 추천하더라고요",
          "original_form": "컴포즈꺼 추천하더라고요",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017872.1.12",
          "form": "저도 아인슈패너 좋아합니당",
          "original_form": "저도 아인슈패너 좋아합니당",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017872.1.13",
          "form": "장승배기역에 있어요",
          "original_form": "장승배기역에 있어요",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017872.1.14",
          "form": "이번에 새로 생겼나요?",
          "original_form": "이번에 새로 생겼나요?",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017872.1.15",
          "form": "참고할게요",
          "original_form": "참고할게요",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017872.1.16",
          "form": "메가커피의 디저트도 맛있어요",
          "original_form": "메가커피의 디저트도 맛있어요",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017872.1.17",
          "form": "감자빵 드셔보셔나요?",
          "original_form": "감자빵 드셔보셔나요?",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017872.1.18",
          "form": "감자빵 원조 먹어봤어요",
          "original_form": "감자빵 원조 먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
