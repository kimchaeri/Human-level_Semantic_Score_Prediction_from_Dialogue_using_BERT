      "utterance": [
        {
          "id": "MDRW2100002237.1.1",
          "form": "넌 연애계획있어?",
          "original_form": "넌 연애계획있어?",
          "speaker_id": "2",
          "time": "20210820 12:29"
        },
        {
          "id": "MDRW2100002237.1.2",
          "form": "아니 없어",
          "original_form": "아니 없어",
          "speaker_id": "1",
          "time": "20210820 12:29"
        },
        {
          "id": "MDRW2100002237.1.3",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "1",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002237.1.4",
          "form": "나도 없어",
          "original_form": "나도 없어",
          "speaker_id": "2",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002237.1.5",
          "form": "연애는 나한테 너무 먼 이야기야",
          "original_form": "연애는 나한테 너무 먼 이야기야",
          "speaker_id": "2",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002237.1.6",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "1",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002237.1.7",
          "form": "진짜 어릴때는 연애가 로망이었는데",
          "original_form": "진짜 어릴때는 연애가 로망이었는데",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002237.1.8",
          "form": "지금은 전혀...",
          "original_form": "지금은 전혀...",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002237.1.9",
          "form": "맞아 현실을 깨달았달까",
          "original_form": "맞아 현실을 깨달았달까",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002237.1.10",
          "form": "사람을 못믿겠더라고",
          "original_form": "사람을 못믿겠더라고",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002237.1.11",
          "form": "그게 제일 커",
          "original_form": "그게 제일 커",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002237.1.12",
          "form": "속터지는 연애 썰 보면",
          "original_form": "속터지는 연애 썰 보면",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002237.1.13",
          "form": "연애 하려던 마음이 싹 사라져",
          "original_form": "연애 하려던 마음이 싹 사라져",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002237.1.14",
          "form": "맞아 그런 이야기 많더라",
          "original_form": "맞아 그런 이야기 많더라",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002237.1.15",
          "form": "연애하지말고 같이 잘 놀러다니자",
          "original_form": "연애하지말고 같이 잘 놀러다니자",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002237.1.16",
          "form": "응 그래야지",
          "original_form": "응 그래야지",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002237.1.17",
          "form": "다른 친구들은 요즘 연애 잘 하고 있으려나?",
          "original_form": "다른 친구들은 요즘 연애 잘 하고 있으려나?",
          "speaker_id": "2",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002237.1.18",
          "form": "잘할것 같은데",
          "original_form": "잘할것 같은데",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002237.1.19",
          "form": "이왕 하는 연애 잘 사귀었으면 좋겠다",
          "original_form": "이왕 하는 연애 잘 사귀었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210820 12:39"
        }
      ]
    }
