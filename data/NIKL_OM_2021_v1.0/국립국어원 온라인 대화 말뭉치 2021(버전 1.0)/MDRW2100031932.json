      "utterance": [
        {
          "id": "MDRW2100031932.1.1",
          "form": "스벅이랑 이디야 자주가요",
          "original_form": "스벅이랑 이디야 자주가요",
          "speaker_id": "2",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100031932.1.2",
          "form": "스벅은 굿즈 모으고있어요",
          "original_form": "스벅은 굿즈 모으고있어요",
          "speaker_id": "2",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100031932.1.3",
          "form": "그렇구나 저도 스벅 자주가는데",
          "original_form": "그렇구나 저도 스벅 자주가는데",
          "speaker_id": "1",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100031932.1.4",
          "form": "집근처에 있어서",
          "original_form": "집근처에 있어서",
          "speaker_id": "1",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100031932.1.5",
          "form": "예쁜 카페들 찾아가고 하시나요?",
          "original_form": "예쁜 카페들 찾아가고 하시나요?",
          "speaker_id": "1",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100031932.1.6",
          "form": "예쁜 카페보다는 날씨가 워낙 더워서",
          "original_form": "예쁜 카페보다는 날씨가 워낙 더워서",
          "speaker_id": "2",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031932.1.7",
          "form": "시원한곳찾다보니깐요",
          "original_form": "시원한곳찾다보니깐요",
          "speaker_id": "2",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031932.1.8",
          "form": "자주가게됐어요",
          "original_form": "자주가게됐어요",
          "speaker_id": "2",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031932.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031932.1.10",
          "form": "저도 카페들 다 분위기도 비슷비슷하고",
          "original_form": "저도 카페들 다 분위기도 비슷비슷하고",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031932.1.11",
          "form": "음료도 다 평준화돼서",
          "original_form": "음료도 다 평준화돼서",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031932.1.12",
          "form": "스벅이 제일 좋아요",
          "original_form": "스벅이 제일 좋아요",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031932.1.13",
          "form": "맛집은 잘 찾아다니시나요?",
          "original_form": "맛집은 잘 찾아다니시나요?",
          "speaker_id": "1",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031932.1.14",
          "form": "맛집찾아다니는거좋아해요",
          "original_form": "맛집찾아다니는거좋아해요",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031932.1.15",
          "form": "근데 요즘은 잘 안가요",
          "original_form": "근데 요즘은 잘 안가요",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031932.1.16",
          "form": "코로나때문에?",
          "original_form": "코로나때문에?",
          "speaker_id": "1",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031932.1.17",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "1",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031932.1.18",
          "form": "네 잘안돌아다녀요ㅜㅜ",
          "original_form": "네 잘안돌아다녀요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 09:13"
        },
        {
          "id": "MDRW2100031932.1.19",
          "form": "스벅만 자주가요",
          "original_form": "스벅만 자주가요",
          "speaker_id": "2",
          "time": "20210905 09:13"
        },
        {
          "id": "MDRW2100031932.1.20",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210905 09:14"
        },
        {
          "id": "MDRW2100031932.1.21",
          "form": "스벅가면 주로 뭐 주문하세요?",
          "original_form": "스벅가면 주로 뭐 주문하세요?",
          "speaker_id": "1",
          "time": "20210905 09:14"
        },
        {
          "id": "MDRW2100031932.1.22",
          "form": "딸기요거트블렌딩먹어요",
          "original_form": "딸기요거트블렌딩먹어요",
          "speaker_id": "2",
          "time": "20210905 10:01"
        },
        {
          "id": "MDRW2100031932.1.23",
          "form": "저도 그거 좋아하는데",
          "original_form": "저도 그거 좋아하는데",
          "speaker_id": "1",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100031932.1.24",
          "form": "어제 새로나온 망고바나나블랜디드 먹어봤는데",
          "original_form": "어제 새로나온 망고바나나블랜디드 먹어봤는데",
          "speaker_id": "1",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100031932.1.25",
          "form": "너무맛있었어요",
          "original_form": "너무맛있었어요",
          "speaker_id": "1",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100031932.1.26",
          "form": "그거도 트라이해보세요",
          "original_form": "그거도 트라이해보세요",
          "speaker_id": "1",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100031932.1.27",
          "form": "아진짜요?",
          "original_form": "아진짜요?",
          "speaker_id": "2",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100031932.1.28",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100031932.1.29",
          "form": "쿨라임피지오 즐겨먹는데 당분간은 계속 그거 먹을거같아요",
          "original_form": "쿨라임피지오 즐겨먹는데 당분간은 계속 그거 먹을거같아요",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100031932.1.30",
          "form": "그것도 맛있어요!!",
          "original_form": "그것도 맛있어요!!",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100031932.1.31",
          "form": "저지금스벅왔어요",
          "original_form": "저지금스벅왔어요",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100031932.1.32",
          "form": "스벅놀러왔어요",
          "original_form": "스벅놀러왔어요",
          "speaker_id": "2",
          "time": "20210905 13:52"
        }
      ]
    }
