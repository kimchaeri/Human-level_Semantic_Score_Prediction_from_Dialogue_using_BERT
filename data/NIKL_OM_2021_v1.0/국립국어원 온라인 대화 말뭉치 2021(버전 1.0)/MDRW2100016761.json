      "utterance": [
        {
          "id": "MDRW2100016761.1.1",
          "form": "ㅎㅎㅎ 사과에 푹 빠졌네",
          "original_form": "ㅎㅎㅎ 사과에 푹 빠졌네",
          "speaker_id": "2",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016761.1.2",
          "form": "이렇게 매번 사과달라는 적 처음이네",
          "original_form": "이렇게 매번 사과달라는 적 처음이네",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016761.1.3",
          "form": "반으로 나눠서 먹어 오늘도?",
          "original_form": "반으로 나눠서 먹어 오늘도?",
          "speaker_id": "2",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016761.1.4",
          "form": "ㅇㅇ 통째로 들고 ㅎㅎ 맛있댕",
          "original_form": "ㅇㅇ 통째로 들고 ㅎㅎ 맛있댕",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016761.1.5",
          "form": "아주 영양간식이네~ 많이 먹으라고 해",
          "original_form": "아주 영양간식이네~ 많이 먹으라고 해",
          "speaker_id": "2",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016761.1.6",
          "form": "사과는 이미 먹고 있고 고구마는 에어프라이기에 돌리는 중",
          "original_form": "사과는 이미 먹고 있고 고구마는 에어프라이기에 돌리는 중",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016761.1.7",
          "form": "오늘 에어프라이기 바쁘네 ㅎㅎ",
          "original_form": "오늘 에어프라이기 바쁘네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016761.1.8",
          "form": "점심엔 삼겹살 오후엔 고구마 ㅎㅎ",
          "original_form": "점심엔 삼겹살 오후엔 고구마 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016761.1.9",
          "form": "잘 사용하니까 좋네",
          "original_form": "잘 사용하니까 좋네",
          "speaker_id": "2",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016761.1.10",
          "form": "이젠 없으면 안돼~ 간식줄 때 필수야",
          "original_form": "이젠 없으면 안돼~ 간식줄 때 필수야",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016761.1.11",
          "form": "미리 튀긴 음식엔 특히 더 좋은 것 같아",
          "original_form": "미리 튀긴 음식엔 특히 더 좋은 것 같아",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016761.1.12",
          "form": "맞아 돈까스나 치킨 데워 먹을 때 기름 쫙 빠짐",
          "original_form": "맞아 돈까스나 치킨 데워 먹을 때 기름 쫙 빠짐",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016761.1.13",
          "form": "색시가 부지런해서 애들이 잘 먹고 있네 ㅎㅎ",
          "original_form": "색시가 부지런해서 애들이 잘 먹고 있네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016761.1.14",
          "form": "내가 안줘도 애들이 먼저 달라고 해 ㅋㅋ",
          "original_form": "내가 안줘도 애들이 먼저 달라고 해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016761.1.15",
          "form": "안봐도 머릿속에 그려지케",
          "original_form": "안봐도 머릿속에 그려지케",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016761.1.16",
          "form": "그려지네 ㅋㅋ",
          "original_form": "그려지네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016761.1.17",
          "form": "메뉴까지 먼저 다 생각해서 말하는데 뭘",
          "original_form": "메뉴까지 먼저 다 생각해서 말하는데 뭘",
