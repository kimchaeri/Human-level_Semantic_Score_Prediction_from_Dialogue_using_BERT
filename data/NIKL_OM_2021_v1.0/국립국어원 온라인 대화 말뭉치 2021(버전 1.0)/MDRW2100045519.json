      "utterance": [
        {
          "id": "MDRW2100045519.1.1",
          "form": "생선전?",
          "original_form": "생선전?",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100045519.1.2",
          "form": "동태전같은거?",
          "original_form": "동태전같은거?",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100045519.1.3",
          "form": "어어어어 ㅜㅜ 엄마가안해줌",
          "original_form": "어어어어 ㅜㅜ 엄마가안해줌",
          "speaker_id": "1",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100045519.1.4",
          "form": "우리집은 그런걸 해본 역사가없어",
          "original_form": "우리집은 그런걸 해본 역사가없어",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045519.1.5",
          "form": "ㅜㅜ우리집은 또 하겟지..",
          "original_form": "ㅜㅜ우리집은 또 하겟지..",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045519.1.6",
          "form": "동태전굽기힘들어ㅜㅜ",
          "original_form": "동태전굽기힘들어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045519.1.7",
          "form": "잘부셔짐",
          "original_form": "잘부셔짐",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045519.1.8",
          "form": "그거어려운거구나...",
          "original_form": "그거어려운거구나...",
          "speaker_id": "1",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045519.1.9",
          "form": "그래서엄마가안해주는구나...",
          "original_form": "그래서엄마가안해주는구나...",
          "speaker_id": "1",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100045519.1.10",
          "form": "난진짜좋아하는데",
          "original_form": "난진짜좋아하는데",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100045519.1.11",
          "form": "어려웡....어렵고 귀찮아..",
          "original_form": "어려웡....어렵고 귀찮아..",
          "speaker_id": "2",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100045519.1.12",
          "form": "육전먹어봄?",
          "original_form": "육전먹어봄?",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100045519.1.13",
          "form": "맨날하는건데...?",
          "original_form": "맨날하는건데...?",
          "speaker_id": "2",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100045519.1.14",
          "form": "너네 차례지네?",
          "original_form": "너네 차례지네?",
          "speaker_id": "1",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100045519.1.15",
          "form": "아닝??? 그냥 해서 먹엉ㅋㅋㅋ",
          "original_form": "아닝??? 그냥 해서 먹엉ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100045519.1.16",
          "form": "명절이니까 명절음식으로ㅋㅋ 전부치고 떡하고.....",
          "original_form": "명절이니까 명절음식으로ㅋㅋ 전부치고 떡하고.....",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100045519.1.17",
          "form": "나 육전 못먹어봄 ㅜ",
          "original_form": "나 육전 못먹어봄 ㅜ",
          "speaker_id": "1",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100045519.1.18",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100045519.1.19",
          "form": "우린 아예안하거든 ㅜ",
          "original_form": "우린 아예안하거든 ㅜ",
          "speaker_id": "1",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100045519.1.20",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100045519.1.21",
          "form": "명절에 소고기무국이랑 임연수 정도?",
          "original_form": "명절에 소고기무국이랑 임연수 정도?",
          "speaker_id": "1",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100045519.1.22",
          "form": "그래도 사위오면 좀 다르지않을까?!!",
          "original_form": "그래도 사위오면 좀 다르지않을까?!!",
          "speaker_id": "2",
