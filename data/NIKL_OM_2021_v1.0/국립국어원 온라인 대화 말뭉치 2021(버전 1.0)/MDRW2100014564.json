      "utterance": [
        {
          "id": "MDRW2100014564.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014564.1.2",
          "form": "게임ㅇ좋아하시나요?",
          "original_form": "게임ㅇ좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014564.1.3",
          "form": "악 아뇨name1님은 좋아하시나요?",
          "original_form": "악 아뇨{emoji:…}&name1&님은 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014564.1.4",
          "form": "헉 저는 취미가 게임이에요..",
          "original_form": "헉 저는 취미가 게임이에요..",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014564.1.5",
          "form": "제가 주제선택을 잘못햇네요..ㅠ",
          "original_form": "제가 주제선택을 잘못햇네요..ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014564.1.6",
          "form": "어떤 게임 좋아하세요?",
          "original_form": "어떤 게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014564.1.7",
          "form": "저는 보드게임은 조금 해봤는데!",
          "original_form": "저는 보드게임은 조금 해봤는데!",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014564.1.8",
          "form": "전 온라인게임좋아하는데",
          "original_form": "전 온라인게임좋아하는데",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014564.1.9",
          "form": "보드게임도 좋아해요 ㅎㅎ",
          "original_form": "보드게임도 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014564.1.10",
          "form": "사실 보드게임도 그렇게 잘하는 건 아니에요",
          "original_form": "사실 보드게임도 그렇게 잘하는 건 아니에요",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014564.1.11",
          "form": "온라인 게임은 뭐하세요? 저는 옛날에 물풍선 터지는 게임 했었어요",
          "original_form": "온라인 게임은 뭐하세요? 저는 옛날에 물풍선 터지는 게임 했었어요",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014564.1.12",
          "form": "혹시 크아인가요??",
          "original_form": "혹시 크아인가요??",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014564.1.13",
          "form": "맞습니다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞습니다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014564.1.14",
          "form": "그거랑 테일즈러너...? 초원 밍밍 이런 캐릭터로 뛰는 게임 했었는데 이름이 잘 기억안나네요",
          "original_form": "그거랑 테일즈러너...? 초원 밍밍 이런 캐릭터로 뛰는 게임 했었는데 이름이 잘 기억안나네요",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014564.1.15",
          "form": "와 저도 엄청어릴때 했었던 게임이에요 ㅋㅋㅋ",
          "original_form": "와 저도 엄청어릴때 했었던 게임이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014564.1.16",
          "form": "테일즈 러너 맞아요 ㅎㅎ",
          "original_form": "테일즈 러너 맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014564.1.17",
          "form": "요즘은 게임안하시면 뭐하면서 여가시간을 보내시나요??",
          "original_form": "요즘은 게임안하시면 뭐하면서 여가시간을 보내시나요??",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014564.1.18",
          "form": "요즘 유행하는 게임은 뭐가 있어요? 오버워치 엄청하는 거 같았는데",
          "original_form": "요즘 유행하는 게임은 뭐가 있어요? 오버워치 엄청하는 거 같았는데",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014564.1.19",
          "form": "바뀌었나요?",
          "original_form": "바뀌었나요?",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014564.1.20",
          "form": "오버워치 옛날에 유행하다 배그나오면서 거품이 좀 빠지긴했어요 ㅎㅎ",
          "original_form": "오버워치 옛날에 유행하다 배그나오면서 거품이 좀 빠지긴했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014564.1.21",
          "form": "아 맞아요 친구들 모바일 배그하는거 봤어요",
          "original_form": "아 맞아요 친구들 모바일 배그하는거 봤어요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014564.1.22",
          "form": "배그도 어려워보이던데 막 맨몸으로 떨어져서 아이템 주워서 생존하는...? 그런 게임이죠?",
          "original_form": "배그도 어려워보이던데 막 맨몸으로 떨어져서 아이템 주워서 생존하는...? 그런 게임이죠?",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014564.1.23",
          "form": "ㅋㅋㅋㅋㅋ저도 안해봤지만 맞을거에요!",
          "original_form": "ㅋㅋㅋㅋㅋ저도 안해봤지만 맞을거에요!",
          "speaker_id": "1",
          "time": "20210825 20:18"
        }
      ]
    }
