      "utterance": [
        {
          "id": "MDRW2100009360.1.1",
          "form": "요즘 보신 영화있으세요?",
          "original_form": "요즘 보신 영화있으세요?",
          "speaker_id": "1",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009360.1.2",
          "form": "공연같은거",
          "original_form": "공연같은거",
          "speaker_id": "1",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009360.1.3",
          "form": "요즘 최근에본거는 원더운먼봤었어요ㅎ",
          "original_form": "요즘 최근에본거는 원더운먼봤었어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009360.1.4",
          "form": "가서본건아니고 집에서요ㅎ",
          "original_form": "가서본건아니고 집에서요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009360.1.5",
          "form": "name1님은 영화 보신거 있으세요?",
          "original_form": "&name1&님은 영화 보신거 있으세요?",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009360.1.6",
          "form": "아 sf영화인가요?",
          "original_form": "아 sf영화인가요?",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009360.1.7",
          "form": "스파이더맨같은",
          "original_form": "스파이더맨같은",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009360.1.8",
          "form": "액션이랑 sf섞인거죠ㅎㅎ",
          "original_form": "액션이랑 sf섞인거죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009360.1.9",
          "form": "슈퍼히어로물이요ㅎ",
          "original_form": "슈퍼히어로물이요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009360.1.10",
          "form": "네 맞아요ㅎ 스파이더맨ㅎ 마블 라이벌 회사에서 만들었어요ㅎ",
          "original_form": "네 맞아요ㅎ 스파이더맨ㅎ 마블 라이벌 회사에서 만들었어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009360.1.11",
          "form": "마블영화죠?",
          "original_form": "마블영화죠?",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009360.1.12",
          "form": "마블은 매니아층이 있는거같던데 제취향은 아니라서ㅋ",
          "original_form": "마블은 매니아층이 있는거같던데 제취향은 아니라서ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009360.1.13",
          "form": "ㅎㅎ 마블영화는아니에요ㅎ",
          "original_form": "ㅎㅎ 마블영화는아니에요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009360.1.14",
          "form": "원더우먼 1944네요ㅎㅎ",
          "original_form": "원더우먼 1944네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009360.1.15",
          "form": "아니 1984 ㅎㅎㅎㅎ",
          "original_form": "아니 1984 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009360.1.16",
          "form": "비스무리한거군요ㅎㅎ",
          "original_form": "비스무리한거군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009360.1.17",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009360.1.18",
          "form": "전 얼마전 모가디슈봤어요",
          "original_form": "전 얼마전 모가디슈봤어요",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009360.1.19",
          "form": "재밌어서 보기도했지만 갤 가돗 때문에 봤어요ㅎㅎ",
          "original_form": "재밌어서 보기도했지만 갤 가돗 때문에 봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009360.1.20",
          "form": "천원예매권있어서 이번달 한번보러가야되는데",
          "original_form": "천원예매권있어서 이번달 한번보러가야되는데",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009360.1.21",
          "form": "모가디슈 좋은 영화라고 하던데요ㅎ",
          "original_form": "모가디슈 좋은 영화라고 하던데요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009360.1.22",
          "form": "영화가 잼난게 없네요",
          "original_form": "영화가 잼난게 없네요",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009360.1.23",
          "form": "싱크홀이나 봐야될지",
          "original_form": "싱크홀이나 봐야될지",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009360.1.24",
          "form": "요즘 영화관도 안되서ㅠ",
          "original_form": "요즘 영화관도 안되서ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009360.1.25",
          "form": "주말엔 그래도 꽤있던데요",
          "original_form": "주말엔 그래도 꽤있던데요",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009360.1.26",
          "form": "코로나때문에 제작 안하는거아닌가몰라요ㅍ",
          "original_form": "코로나때문에 제작 안하는거아닌가몰라요ㅍ",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009360.1.27",
          "form": "아 그래요?ㅎ",
          "original_form": "아 그래요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009360.1.28",
          "form": "오히려 사람 없으니 보긴 좋아요",
          "original_form": "오히려 사람 없으니 보긴 좋아요",
          "speaker_id": "1",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009360.1.29",
          "form": "ㅎㅎ 맞아요ㅎ",
          "original_form": "ㅎㅎ 맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009360.1.30",
          "form": "코로나라 사람들이 집콕하니ㅎ 그런 재미가있겟네요ㅎ",
          "original_form": "코로나라 사람들이 집콕하니ㅎ 그런 재미가있겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009360.1.31",
          "form": "혼자 영화보러 가셨어요?",
          "original_form": "혼자 영화보러 가셨어요?",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009360.1.32",
          "form": "아니요 친구랑 둘이요 혼영 못해요",
          "original_form": "아니요 친구랑 둘이요 혼영 못해요",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009360.1.33",
          "form": "저번에 첨 혼자봤었네요 상영관사람 없을때ㅋ",
          "original_form": "저번에 첨 혼자봤었네요 상영관사람 없을때ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009360.1.34",
          "form": "혼영하니 생각나네요ㅠ",
          "original_form": "혼영하니 생각나네요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009360.1.35",
          "form": "뭐가요?",
          "original_form": "뭐가요?",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009360.1.36",
          "form": "이런말 실례인줄 알지만.. 기분나쁘시면 차단하셔요ㅠ",
          "original_form": "이런말 실례인줄 알지만.. 기분나쁘시면 차단하셔요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009360.1.37",
          "form": "슬픈추억이 있으신가요?",
          "original_form": "슬픈추억이 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009360.1.38",
          "form": "총각때...",
          "original_form": "총각때...",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009360.1.39",
          "form": "아니요 슬픈이야기라고해야하나?",
          "original_form": "아니요 슬픈이야기라고해야하나?",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009360.1.40",
          "form": "꼭 보고싶은 영화가 있어서ㅎ 혼자라도 가서 보려고 예매했었거든요",
          "original_form": "꼭 보고싶은 영화가 있어서ㅎ 혼자라도 가서 보려고 예매했었거든요",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009360.1.41",
          "form": "그런데요?",
          "original_form": "그런데요?",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009360.1.42",
          "form": "이리봐도 커플 저리봐도 커플.... 저혼자 영화 끝가지 보고 나온 기억이있어요ㅎ",
          "original_form": "이리봐도 커플 저리봐도 커플.... 저혼자 영화 끝가지 보고 나온 기억이있어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009360.1.43",
          "form": "근데 그게 좀 야한장면이있는 영화였어요... 얼마나 민망하던지...",
          "original_form": "근데 그게 좀 야한장면이있는 영화였어요... 얼마나 민망하던지...",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009360.1.44",
          "form": "아 ㅎ 저보러갔을때도 그랬어요",
          "original_form": "아 ㅎ 저보러갔을때도 그랬어요",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009360.1.45",
          "form": "사람들이 신경안쓰나봐요ㅎ",
          "original_form": "사람들이 신경안쓰나봐요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009360.1.46",
          "form": "혼자보러가셨음 민망하셨겠어요ㅎㅎ",
          "original_form": "혼자보러가셨음 민망하셨겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009360.1.47",
          "form": "옆에서 쪽.... 뒤에서 쪽....",
          "original_form": "옆에서 쪽.... 뒤에서 쪽....",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009360.1.48",
          "form": "요즘은 혼자보러 많이 오시던데",
          "original_form": "요즘은 혼자보러 많이 오시던데",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009360.1.49",
          "form": "많이가는것같더라구요ㅎㅎ",
          "original_form": "많이가는것같더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009360.1.50",
          "form": "영화관에서 옆자리 다보이는데 사실 전 그런거 이해가 안가요ㅎ",
          "original_form": "영화관에서 옆자리 다보이는데 사실 전 그런거 이해가 안가요ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:18"
        }
      ]
