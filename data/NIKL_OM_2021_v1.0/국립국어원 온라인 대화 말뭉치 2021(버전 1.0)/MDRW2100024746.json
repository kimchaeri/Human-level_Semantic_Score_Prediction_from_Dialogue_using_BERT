      "utterance": [
        {
          "id": "MDRW2100024746.1.1",
          "form": "혹시 싸이월드해보셨나요?",
          "original_form": "혹시 싸이월드해보셨나요?",
          "speaker_id": "1",
          "time": "20210831 18:03"
        },
        {
          "id": "MDRW2100024746.1.2",
          "form": "싸이월드 세대가 아니신가요?",
          "original_form": "싸이월드 세대가 아니신가요?",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024746.1.3",
          "form": "저 아바타 무료로 열심히 꾸몄어요 융님은여",
          "original_form": "저 아바타 무료로 열심히 꾸몄어요 융님은여",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024746.1.4",
          "form": "전 열심히 도토리받아서 음악만샀던 기억이 있네요ㅋ",
          "original_form": "전 열심히 도토리받아서 음악만샀던 기억이 있네요ㅋ",
          "speaker_id": "1",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024746.1.5",
          "form": "저도 음악들으려고 남의홈피 켜놓고 딴짓했어요",
          "original_form": "저도 음악들으려고 남의홈피 켜놓고 딴짓했어요",
          "speaker_id": "2",
          "time": "20210831 19:20"
        },
        {
          "id": "MDRW2100024746.1.6",
          "form": "ㅎㅎㅎ아직도 홈피에 있던 노래는 종종듣고 있어요ㅎㅎ",
          "original_form": "ㅎㅎㅎ아직도 홈피에 있던 노래는 종종듣고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:12"
        },
        {
          "id": "MDRW2100024746.1.7",
          "form": "그시절 워너비는 변치않죠ㅎ",
          "original_form": "그시절 워너비는 변치않죠ㅎ",
          "speaker_id": "2",
          "time": "20210831 20:18"
        },
        {
          "id": "MDRW2100024746.1.8",
          "form": "박정현 달아요도 좋아요ㅋㅋ",
          "original_form": "박정현 달아요도 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100024746.1.9",
          "form": "모르는곡",
          "original_form": "모르는곡",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024746.1.10",
          "form": "도토리는 기억하시나요?",
          "original_form": "도토리는 기억하시나요?",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024746.1.11",
          "form": "그라무요",
          "original_form": "그라무요",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024746.1.12",
          "form": "내 아바타 색동한복 입고있음",
          "original_form": "내 아바타 색동한복 입고있음",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024746.1.13",
          "form": "아직 살아 있나요?",
          "original_form": "아직 살아 있나요?",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024746.1.14",
          "form": "복원해봐야 알지유",
          "original_form": "복원해봐야 알지유",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024746.1.15",
          "form": "아 싸이월드 복원이 되나요?",
          "original_form": "아 싸이월드 복원이 되나요?",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024746.1.16",
          "form": "지금 먼저 신청한분부터 데이터 복원해준대요",
          "original_form": "지금 먼저 신청한분부터 데이터 복원해준대요",
          "speaker_id": "2",
          "time": "20210831 22:10"
        }
