      "utterance": [
        {
          "id": "MDRW2100036622.1.1",
          "form": "반숙란중에 감동란이라고 아세요?",
          "original_form": "반숙란중에 감동란이라고 아세요?",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100036622.1.2",
          "form": "네네 감동란사먹을땐 반숙먹네여 ㅋㅋㅋㅋㅋ",
          "original_form": "네네 감동란사먹을땐 반숙먹네여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:30"
        },
        {
          "id": "MDRW2100036622.1.3",
          "form": "그거 되게 좋아해서 아예 계란 한판을 사서 일주일만에 다 먹은적 있어요ㅋㅋㅋ",
          "original_form": "그거 되게 좋아해서 아예 계란 한판을 사서 일주일만에 다 먹은적 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:30"
        },
        {
          "id": "MDRW2100036622.1.4",
          "form": "ㅋㅋㅋㅋㅋ와우",
          "original_form": "ㅋㅋㅋㅋㅋ와우",
          "speaker_id": "2",
          "time": "20210831 02:30"
        },
        {
          "id": "MDRW2100036622.1.5",
          "form": "저도 훈제꽂힐때있어서",
          "original_form": "저도 훈제꽂힐때있어서",
          "speaker_id": "2",
          "time": "20210831 02:30"
        },
        {
          "id": "MDRW2100036622.1.6",
          "form": "근데 요즘 계란값이 너무 비싸서 못사먹고 있네요ㅠㅠ",
          "original_form": "근데 요즘 계란값이 너무 비싸서 못사먹고 있네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 02:30"
        },
        {
          "id": "MDRW2100036622.1.7",
          "form": "한판 일주일에 다쪄먹을때있었어요",
          "original_form": "한판 일주일에 다쪄먹을때있었어요",
          "speaker_id": "2",
          "time": "20210831 02:30"
        },
        {
          "id": "MDRW2100036622.1.8",
          "form": "훈제는 찜질방가서 주로 먹었는데",
          "original_form": "훈제는 찜질방가서 주로 먹었는데",
          "speaker_id": "1",
          "time": "20210831 02:30"
        },
        {
          "id": "MDRW2100036622.1.9",
          "form": "맞아요ㅜㅜ진짜 넘비싸요 작년만해도 4처넌돈아니었나요",
          "original_form": "맞아요ㅜㅜ진짜 넘비싸요 작년만해도 4처넌돈아니었나요",
          "speaker_id": "2",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.10",
          "form": "식혜에다가 먹는게 최곤데",
          "original_form": "식혜에다가 먹는게 최곤데",
          "speaker_id": "1",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.11",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.12",
          "form": "크 ㅜ",
          "original_form": "크 ㅜ",
          "speaker_id": "2",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.13",
          "form": "근데 요즘 기본 만원넘어요ㅠ",
          "original_form": "근데 요즘 기본 만원넘어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.14",
          "form": "찜방도 못간지오래네요",
          "original_form": "찜방도 못간지오래네요",
          "speaker_id": "2",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.15",
          "form": "거기서먹는 계란최곤데ㅜ",
          "original_form": "거기서먹는 계란최곤데ㅜ",
          "speaker_id": "2",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.16",
          "form": "그러게요ㅋㅋㅋㅋ",
          "original_form": "그러게요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.17",
          "form": "거기서 맨날 육개장 사발면 먹었는데",
          "original_form": "거기서 맨날 육개장 사발면 먹었는데",
          "speaker_id": "1",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.18",
          "form": "그니까요 계란값이 넘비싸요ㅜㅜ",
          "original_form": "그니까요 계란값이 넘비싸요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 02:31"
        },
        {
          "id": "MDRW2100036622.1.19",
          "form": "후기 감사해요!",
          "original_form": "후기 감사해요!",
          "speaker_id": "1",
          "time": "20210831 02:39"
        }
      ]
