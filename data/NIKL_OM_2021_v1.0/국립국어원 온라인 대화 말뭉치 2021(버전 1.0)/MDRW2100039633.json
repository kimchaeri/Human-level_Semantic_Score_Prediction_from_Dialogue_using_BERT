      "utterance": [
        {
          "id": "MDRW2100039633.1.1",
          "form": "안녕하세요! name2님은 노래 자주 들으시나요?",
          "original_form": "안녕하세요! &name2&님은 노래 자주 들으시나요?",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039633.1.2",
          "form": "아 안녕하세요",
          "original_form": "아 안녕하세요",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039633.1.3",
          "form": "네 자주 들어요",
          "original_form": "네 자주 들어요",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039633.1.4",
          "form": "장르는 가리지 않고 좋아해요",
          "original_form": "장르는 가리지 않고 좋아해요",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039633.1.5",
          "form": "오! 최애 가수가 누구예요!??@",
          "original_form": "오! 최애 가수가 누구예요!??@",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039633.1.6",
          "form": "저는 아이돌 팬이기도 해서",
          "original_form": "저는 아이돌 팬이기도 해서",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039633.1.7",
          "form": "간단하게 얘기 하자면",
          "original_form": "간단하게 얘기 하자면",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039633.1.8",
          "form": "악뮤나 헤이즈, 아이유, 슈주 정도?",
          "original_form": "악뮤나 헤이즈, 아이유, 슈주 정도?",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039633.1.9",
          "form": "헐 저도 아이유 좋아래요!",
          "original_form": "헐 저도 아이유 좋아래요!",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039633.1.10",
          "form": "아이돌은 누구 좋아하셔요!?!? 슈주?!",
          "original_form": "아이돌은 누구 좋아하셔요!?!? 슈주?!",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039633.1.11",
          "form": "전 엔시티 좋아하거든요!",
          "original_form": "전 엔시티 좋아하거든요!",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039633.1.12",
          "form": "앗 사실 약간 잡덕이기도 혀요",
          "original_form": "앗 사실 약간 잡덕이기도 혀요",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039633.1.13",
          "form": "아 저는 아이돌은 슈주만 파서",
          "original_form": "아 저는 아이돌은 슈주만 파서",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039633.1.14",
          "form": "엔시티면 같은 sm이네요 ㅎㅎㅎ",
          "original_form": "엔시티면 같은 sm이네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039633.1.15",
          "form": "헛 그러게요!!! ㅎ.ㅎ",
          "original_form": "헛 그러게요!!! ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039633.1.16",
          "form": "저 규현 ㅠ ㅠ 정말 좋아해요!! 그 광화문에서였나 그 노래 첨 나왔을 때",
          "original_form": "저 규현 ㅠ ㅠ 정말 좋아해요!! 그 광화문에서였나 그 노래 첨 나왔을 때",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039633.1.17",
          "form": "맨날 노래방 들락날락 했거든요",
          "original_form": "맨날 노래방 들락날락 했거든요",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039633.1.18",
          "form": "광화문에서 진짜 좋죠",
          "original_form": "광화문에서 진짜 좋죠",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039633.1.19",
          "form": "신서유기 때문에 뭔가 짠한 이미지가 되었지만...",
          "original_form": "신서유기 때문에 뭔가 짠한 이미지가 되었지만...",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039633.1.20",
          "form": "아 그니까요 ㅋㅋㅋㅋㅋ !!!! 그거 볼 때마다 너무 웃겨요",
          "original_form": "아 그니까요 ㅋㅋㅋㅋㅋ !!!! 그거 볼 때마다 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039633.1.21",
          "form": "name2님의 아이유 최애곡은 뭔가요!?",
          "original_form": "&name2&님의 아이유 최애곡은 뭔가요!?",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039633.1.22",
          "form": "밤편지요",
          "original_form": "밤편지요",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039633.1.23",
          "form": "밤에 듣기도 좋고",
          "original_form": "밤에 듣기도 좋고",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039633.1.24",
          "form": "괜히 또 혼자 설레는 느낌이 좋더라구요",
          "original_form": "괜히 또 혼자 설레는 느낌이 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039633.1.25",
          "form": "헛 맞아요!",
          "original_form": "헛 맞아요!",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039633.1.26",
          "form": "저는 그 가을아침?",
          "original_form": "저는 그 가을아침?",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039633.1.27",
          "form": "이 되게 설레더라고요!",
          "original_form": "이 되게 설레더라고요!",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039633.1.28",
          "form": "뭔가 좋은 하루를 보낼 수 있을 것 같은 기분!",
          "original_form": "뭔가 좋은 하루를 보낼 수 있을 것 같은 기분!",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039633.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039633.1.30",
          "form": "산뜻하고 발랄해서 아침에 딱이죠",
          "original_form": "산뜻하고 발랄해서 아침에 딱이죠",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039633.1.31",
          "form": "혹시 엔시티 곡 중 추천해주실만한 곡이 있을까요???",
          "original_form": "혹시 엔시티 곡 중 추천해주실만한 곡이 있을까요???",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039633.1.32",
          "form": "약간 발랄한 곡 좋아하시면",
          "original_form": "약간 발랄한 곡 좋아하시면",
          "speaker_id": "1",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039633.1.33",
          "form": "발랄한 곡 좋아하시면 터치! 추천하고요",
          "original_form": "발랄한 곡 좋아하시면 터치! 추천하고요",
