      "utterance": [
        {
          "id": "MDRW2100008263.1.1",
          "form": "누구 나왔어?",
          "original_form": "누구 나왔어?",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008263.1.2",
          "form": "서이숙배우",
          "original_form": "서이숙배우",
          "speaker_id": "1",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008263.1.3",
          "form": "모르겠다",
          "original_form": "모르겠다",
          "speaker_id": "2",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008263.1.4",
          "form": "부부의 세계에 나온 왜 카리스마 있는 조연배우",
          "original_form": "부부의 세계에 나온 왜 카리스마 있는 조연배우",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008263.1.5",
          "form": "아 알겠다 카리스마 있으신분",
          "original_form": "아 알겠다 카리스마 있으신분",
          "speaker_id": "2",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008263.1.6",
          "form": "응 맞아 김용만이랑 동갑이래",
          "original_form": "응 맞아 김용만이랑 동갑이래",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008263.1.7",
          "form": "찰떡 같은 비유다",
          "original_form": "찰떡 같은 비유다",
          "speaker_id": "1",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008263.1.8",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100008263.1.9",
          "form": "개는 훌륭하다 어제 봤는데 비글이 너무 귀업더라",
          "original_form": "개는 훌륭하다 어제 봤는데 비글이 너무 귀업더라",
          "speaker_id": "1",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100008263.1.10",
          "form": "이경규가 개한테 물림",
          "original_form": "이경규가 개한테 물림",
          "speaker_id": "2",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100008263.1.11",
          "form": "진짜 어쩌다가 그랬데",
          "original_form": "진짜 어쩌다가 그랬데",
          "speaker_id": "1",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100008263.1.12",
          "form": "한순간에 돌변해서 물림",
          "original_form": "한순간에 돌변해서 물림",
          "speaker_id": "2",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100008263.1.13",
          "form": "ㅋㅋ동물은 그래서 조심해야함 크게 다친거야?",
          "original_form": "ㅋㅋ동물은 그래서 조심해야함 크게 다친거야?",
          "speaker_id": "1",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100008263.1.14",
          "form": "그냥 살짝 물렸더라 화면보니",
          "original_form": "그냥 살짝 물렸더라 화면보니",
          "speaker_id": "2",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100008263.1.15",
          "form": "뭐지 뉴스에 부산대 별점 테러하겠다는 내용 나오는데",
          "original_form": "뭐지 뉴스에 부산대 별점 테러하겠다는 내용 나오는데",
