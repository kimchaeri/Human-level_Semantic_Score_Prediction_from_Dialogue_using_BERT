      "utterance": [
        {
          "id": "MDRW2100027779.1.1",
          "form": "점심 뭐 드셨나요",
          "original_form": "점심 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027779.1.2",
          "form": "회사식당에서 샐러드메뉴 먹었어요",
          "original_form": "회사식당에서 샐러드메뉴 먹었어요",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027779.1.3",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027779.1.4",
          "form": "저는 불닭 끓여먹었어요",
          "original_form": "저는 불닭 끓여먹었어요",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027779.1.5",
          "form": "매운 걸 잘 드시나봐여",
          "original_form": "매운 걸 잘 드시나봐여",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027779.1.6",
          "form": "잘먹는 편은 아니에요",
          "original_form": "잘먹는 편은 아니에요",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027779.1.7",
          "form": "울면서 먹어요ㅋㅋ",
          "original_form": "울면서 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027779.1.8",
          "form": "ㅋㅋㅋ저도 못 먹는데 좋아하는 편이라서",
          "original_form": "ㅋㅋㅋ저도 못 먹는데 좋아하는 편이라서",
          "speaker_id": "1",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027779.1.9",
          "form": "매운 떡볶이 같은거 가끔 먹어요",
          "original_form": "매운 떡볶이 같은거 가끔 먹어요",
          "speaker_id": "1",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027779.1.10",
          "form": "매운 떡볶이 좋아하시는군요",
          "original_form": "매운 떡볶이 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027779.1.11",
          "form": "어디걸로 드시나요?",
          "original_form": "어디걸로 드시나요?",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027779.1.12",
          "form": "집 근처에 윤옥연할매떡볶이라고 있는데",
          "original_form": "집 근처에 윤옥연할매떡볶이라고 있는데",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027779.1.13",
          "form": "후추 매운맛 나는 떡볶이예요 ㅋㅋ",
          "original_form": "후추 매운맛 나는 떡볶이예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027779.1.14",
          "form": "프랜차이즈는 아니죠?",
          "original_form": "프랜차이즈는 아니죠?",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027779.1.15",
          "form": "대구에만 몇개 있는 프랜차이즈 일거예요!",
          "original_form": "대구에만 몇개 있는 프랜차이즈 일거예요!",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100027779.1.16",
          "form": "ㅠ 대구 너무 멀어요",
          "original_form": "ㅠ 대구 너무 멀어요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027779.1.17",
          "form": "ㅜㅜ...나중에 여행오면 한번 도전해보세여!",
          "original_form": "ㅜㅜ...나중에 여행오면 한번 도전해보세여!",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100027779.1.18",
          "form": "네 어디 메모해 놓을게요",
          "original_form": "네 어디 메모해 놓을게요",
          "speaker_id": "2",
          "time": "20210902 18:30"
        }
      ]
