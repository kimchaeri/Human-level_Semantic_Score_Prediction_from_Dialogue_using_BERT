      "utterance": [
        {
          "id": "MDRW2100043602.1.1",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043602.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043602.1.3",
          "form": "저도 완전요 ㅋㅋㅋ 아메 라떼 다 잘 먹어요",
          "original_form": "저도 완전요 ㅋㅋㅋ 아메 라떼 다 잘 먹어요",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043602.1.4",
          "form": "전 라떼파예요",
          "original_form": "전 라떼파예요",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043602.1.5",
          "form": "아 저도 라떼를 좀 더 좋아해요",
          "original_form": "아 저도 라떼를 좀 더 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043602.1.6",
          "form": "근데 유당불내증이라 엄청 먹고플때만 ㅋㅋ",
          "original_form": "근데 유당불내증이라 엄청 먹고플때만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043602.1.7",
          "form": "저는 유당불내증은 없는데 카페인에 예민해서",
          "original_form": "저는 유당불내증은 없는데 카페인에 예민해서",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043602.1.8",
          "form": "커피를 좋아하는 것만큼 많이 마실수가 없어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "커피를 좋아하는 것만큼 많이 마실수가 없어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043602.1.9",
          "form": "아 저런.. 그럼 디카페인 드시고 그런건 아니고 그냥 조금 드시나요",
          "original_form": "아 저런.. 그럼 디카페인 드시고 그런건 아니고 그냥 조금 드시나요",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043602.1.10",
          "form": "하루에 오전 한잔만 카페인으로 마시고 오후에는 디카페인 마셔요 ㅋㅋㅋㅋ",
          "original_form": "하루에 오전 한잔만 카페인으로 마시고 오후에는 디카페인 마셔요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043602.1.11",
          "form": "아하 어디 젤 많이 가세요 카페",
          "original_form": "아하 어디 젤 많이 가세요 카페",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043602.1.12",
          "form": "전 라떼파라서",
          "original_form": "전 라떼파라서",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043602.1.13",
          "form": "폴바셋 좋아하는데 매장이 별로 없 ㅠㅠ",
          "original_form": "폴바셋 좋아하는데 매장이 별로 없 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043602.1.14",
          "form": "아 폴바셋 라떼 최고죠",
          "original_form": "아 폴바셋 라떼 최고죠",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043602.1.15",
          "form": "맞아요 매장이 너무 없는 ㅠㅠ",
          "original_form": "맞아요 매장이 너무 없는 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043602.1.16",
          "form": "할리스 바닐라 딜라이트도 좋아합니다",
          "original_form": "할리스 바닐라 딜라이트도 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043602.1.17",
          "form": "그거 향이 약간 있는거 맞죠? 달다구리한",
          "original_form": "그거 향이 약간 있는거 맞죠? 달다구리한",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043602.1.18",
          "form": "네 달달구리한 바닐라향 ㅋㅋㅋㅋㅋ",
          "original_form": "네 달달구리한 바닐라향 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043602.1.19",
          "form": "그쵸 저도 전에 먹어본거 같아요 하도 유명해서 ㅋㅋ",
          "original_form": "그쵸 저도 전에 먹어본거 같아요 하도 유명해서 ㅋㅋ",
