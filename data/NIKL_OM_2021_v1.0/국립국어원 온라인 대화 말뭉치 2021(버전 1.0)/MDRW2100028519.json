      "utterance": [
        {
          "id": "MDRW2100028519.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028519.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028519.1.3",
          "form": "동물 키우세요??",
          "original_form": "동물 키우세요??",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028519.1.4",
          "form": "아니요 안키워요 ㅎㅎ키우고싶은데 혼자두는게 조금 그래서ㅠ",
          "original_form": "아니요 안키워요 ㅎㅎ키우고싶은데 혼자두는게 조금 그래서ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028519.1.5",
          "form": "님은 키우시나요?",
          "original_form": "님은 키우시나요?",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028519.1.6",
          "form": "에구...",
          "original_form": "에구...",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028519.1.7",
          "form": "저 고양이 키워요",
          "original_form": "저 고양이 키워요",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028519.1.8",
          "form": "오 고양이 요즘 많이들 키우시던데",
          "original_form": "오 고양이 요즘 많이들 키우시던데",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028519.1.9",
          "form": "키우면서 뭐가 제일 좋으신가용",
          "original_form": "키우면서 뭐가 제일 좋으신가용",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028519.1.10",
          "form": "전 원래 고양이를 좋아해서 그냥 옆에 있는 것만으로 좋아요ㅎㅎ",
          "original_form": "전 원래 고양이를 좋아해서 그냥 옆에 있는 것만으로 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028519.1.11",
          "form": "고양이 좋아하세요?",
          "original_form": "고양이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028519.1.12",
          "form": "네네 너무 귀여워서 키우고싶어요 !!!",
          "original_form": "네네 너무 귀여워서 키우고싶어요 !!!",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028519.1.13",
          "form": "가끔 길냥이들한테 먹이도 준답니당ㅎㅎ",
          "original_form": "가끔 길냥이들한테 먹이도 준답니당ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028519.1.14",
          "form": "헛 너무 멋져요",
          "original_form": "헛 너무 멋져요",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028519.1.15",
          "form": "길냥이들도 다 너무 귀여워요...",
          "original_form": "길냥이들도 다 너무 귀여워요...",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028519.1.16",
          "form": "근데 집 배우는 시간이 꽤 기신가봐요",
          "original_form": "근데 집 배우는 시간이 꽤 기신가봐요",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028519.1.17",
          "form": "(비우는...)",
          "original_form": "(비우는...)",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028519.1.18",
          "form": "네 맞아요 주로 밖에 있다 보니까 길냥이들 밥도 주고 그러는 것 같아요",
          "original_form": "네 맞아요 주로 밖에 있다 보니까 길냥이들 밥도 주고 그러는 것 같아요",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028519.1.19",
          "form": "그래도 키우고 싶어요ㅠ",
          "original_form": "그래도 키우고 싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028519.1.20",
          "form": "키우는데 힘드신 점은 없나오?",
          "original_form": "키우는데 힘드신 점은 없나오?",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028519.1.21",
          "form": "저는 2개월 쯤에 유기묘 입양해온 거라... 애기인 게 제일 힘든 것 같아요",
          "original_form": "저는 2개월 쯤에 유기묘 입양해온 거라... 애기인 게 제일 힘든 것 같아요",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028519.1.22",
          "form": "에너지가 넘쳐서 맨날 놀아줘야 하구",
          "original_form": "에너지가 넘쳐서 맨날 놀아줘야 하구",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028519.1.23",
          "form": "사고 칠까봐 눈을 못 떼고 그런...",
          "original_form": "사고 칠까봐 눈을 못 떼고 그런...",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028519.1.24",
          "form": "그래도 예뻐요",
          "original_form": "그래도 예뻐요",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028519.1.25",
          "form": "헉 너무 예뻐요 진짜ㅠ제일 예쁠 때잖아요",
          "original_form": "헉 너무 예뻐요 진짜ㅠ제일 예쁠 때잖아요",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028519.1.26",
          "form": "혹시 애기 이름은 뭐예요??",
          "original_form": "혹시 애기 이름은 뭐예요??",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028519.1.27",
          "form": "이름 name3에요!",
          "original_form": "이름 &name3&에요!",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028519.1.28",
          "form": "집 온지 한 3개월 됐는데 아직도 쪼꼬맣네요",
          "original_form": "집 온지 한 3개월 됐는데 아직도 쪼꼬맣네요",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028519.1.29",
          "form": "이름부터 너무 귀엽네요,,,저도 여유로워지면 꼭 키울래요ㅠ",
          "original_form": "이름부터 너무 귀엽네요,,,저도 여유로워지면 꼭 키울래요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028519.1.30",
          "form": "부럽습니당 ㅠㅠㅎㅎ",
          "original_form": "부럽습니당 ㅠㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028519.1.31",
          "form": "ㅠㅠ진짜 꼭 키우세요 고양이 좋아하시면 후회 안 하실 거예요",
          "original_form": "ㅠㅠ진짜 꼭 키우세요 고양이 좋아하시면 후회 안 하실 거예요",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028519.1.32",
          "form": "네네 저도 꼭 한 번 키워볼게요 ㅎㅎ",
          "original_form": "네네 저도 꼭 한 번 키워볼게요 ㅎㅎ",
