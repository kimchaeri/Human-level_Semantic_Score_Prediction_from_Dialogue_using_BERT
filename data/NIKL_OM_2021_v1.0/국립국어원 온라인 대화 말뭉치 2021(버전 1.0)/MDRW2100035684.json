      "utterance": [
        {
          "id": "MDRW2100035684.1.1",
          "form": "아ㅋㅋㅋㅋ 저도 요리는 못할것같긴해요....",
          "original_form": "아ㅋㅋㅋㅋ 저도 요리는 못할것같긴해요....",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035684.1.2",
          "form": "부모님이랑 살면",
          "original_form": "부모님이랑 살면",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035684.1.3",
          "form": "다 해주시려고 하셔서 좋긴하죠",
          "original_form": "다 해주시려고 하셔서 좋긴하죠",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035684.1.4",
          "form": "ㅋㅋㅋ 잘먹는게 짱입니다",
          "original_form": "ㅋㅋㅋ 잘먹는게 짱입니다",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035684.1.5",
          "form": "네네 투정안하고",
          "original_form": "네네 투정안하고",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035684.1.6",
          "form": "30대 넘으시면 진짜 잘드셔야하니까요",
          "original_form": "30대 넘으시면 진짜 잘드셔야하니까요",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035684.1.7",
          "form": "그저 감사히...",
          "original_form": "그저 감사히...",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035684.1.8",
          "form": "이제 내일이면 서른이라",
          "original_form": "이제 내일이면 서른이라",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035684.1.9",
          "form": "요리 많이 해서 먹어야하는데",
          "original_form": "요리 많이 해서 먹어야하는데",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035684.1.10",
          "form": "오 내일 생일이신가요 축하합니다",
          "original_form": "오 내일 생일이신가요 축하합니다",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035684.1.11",
          "form": "쉽지않네요ㅠㅠㅋㅋㅋ",
          "original_form": "쉽지않네요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035684.1.12",
          "form": "앗 서른이여!ㅋㅋㅋㅋ",
          "original_form": "앗 서른이여!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035684.1.13",
          "form": "name2님은 자취 몇살때 시작하셨어요?",
          "original_form": "&name2&님은 자취 몇살때 시작하셨어요?",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035684.1.14",
          "form": "저는 20살때요!",
          "original_form": "저는 20살때요!",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035684.1.15",
          "form": "대학가면서 했지요!",
          "original_form": "대학가면서 했지요!",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035684.1.16",
          "form": "천둥벌거숭이처럼 살았습니다",
          "original_form": "천둥벌거숭이처럼 살았습니다",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035684.1.17",
          "form": "아하 완전 애기때부터 시작하셨네용",
          "original_form": "아하 완전 애기때부터 시작하셨네용",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035684.1.18",
          "form": "으핰ㅋㅋㅋㅋ",
          "original_form": "으핰ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035684.1.19",
          "form": "그 나이때는",
          "original_form": "그 나이때는",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035684.1.20",
          "form": "고시원에살기도 하고",
          "original_form": "고시원에살기도 하고",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035684.1.21",
          "form": "혼자 살게되면",
          "original_form": "혼자 살게되면",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035684.1.22",
          "form": "다 그런것같아요",
          "original_form": "다 그런것같아요",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035684.1.23",
          "form": "원룸일때도 있고요",
          "original_form": "원룸일때도 있고요",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035684.1.24",
          "form": "하우스 메이트가 있었던적도 있고요",
          "original_form": "하우스 메이트가 있었던적도 있고요",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035684.1.25",
          "form": "고시원은 지내기 많이 불편하다던데",
          "original_form": "고시원은 지내기 많이 불편하다던데",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035684.1.26",
          "form": "고생 많이 하셨네요ㅠㅠ",
          "original_form": "고생 많이 하셨네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:26"
        }
      ]
