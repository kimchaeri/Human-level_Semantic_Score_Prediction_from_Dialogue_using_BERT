      "utterance": [
        {
          "id": "MDRW2100011330.1.1",
          "form": "저 돈까스 좋아해요!!",
          "original_form": "저 돈까스 좋아해요!!",
          "speaker_id": "1",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100011330.1.2",
          "form": "돈까스 맛있죠 ㅎ",
          "original_form": "돈까스 맛있죠 ㅎ",
          "speaker_id": "2",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100011330.1.3",
          "form": "오늘점심은 뭐드실예정?",
          "original_form": "오늘점심은 뭐드실예정?",
          "speaker_id": "2",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100011330.1.4",
          "form": "초밥 먹으려구요ㅎㅎ",
          "original_form": "초밥 먹으려구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 11:14"
        },
        {
          "id": "MDRW2100011330.1.5",
          "form": "오 초밥 저도 좋아하는데 맛나겠어요",
          "original_form": "오 초밥 저도 좋아하는데 맛나겠어요",
          "speaker_id": "2",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100011330.1.6",
          "form": "어떤종류 좋아해요?",
          "original_form": "어떤종류 좋아해요?",
          "speaker_id": "2",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100011330.1.7",
          "form": "참치도 좋아하고 회종류는 다좋아요ㅎㅎ",
          "original_form": "참치도 좋아하고 회종류는 다좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 11:16"
        },
        {
          "id": "MDRW2100011330.1.8",
          "form": "전 광어나 우럭 새우ㅎ",
          "original_form": "전 광어나 우럭 새우ㅎ",
          "speaker_id": "2",
          "time": "20210824 11:16"
        },
        {
          "id": "MDRW2100011330.1.9",
          "form": "와 세개 다좋아요ㅋㅋㅋㅋ",
          "original_form": "와 세개 다좋아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011330.1.10",
          "form": "연어도 맛있죠ㅎㅎ",
          "original_form": "연어도 맛있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100011330.1.11",
          "form": "급배곱",
          "original_form": "급배곱",
          "speaker_id": "2",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100011330.1.12",
          "form": "연어 와 침나오네요",
          "original_form": "연어 와 침나오네요",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100011330.1.13",
          "form": "근처 맛집이 있어요?ㅎ",
          "original_form": "근처 맛집이 있어요?ㅎ",
          "speaker_id": "2",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100011330.1.14",
          "form": "코스트코에서 연어 쟁여놓으면 완전 좋은데여",
          "original_form": "코스트코에서 연어 쟁여놓으면 완전 좋은데여",
          "speaker_id": "1",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100011330.1.15",
          "form": "코스트코는 대량구매해야돼서 못가요ㅋ",
          "original_form": "코스트코는 대량구매해야돼서 못가요ㅋ",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100011330.1.16",
          "form": "회원도아니고",
          "original_form": "회원도아니고",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100011330.1.17",
          "form": "아 맞아요 회비도 있고ㅠㅠ",
          "original_form": "아 맞아요 회비도 있고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011330.1.18",
          "form": "소고기가 그렇게 싸다던데오ㅡ",
          "original_form": "소고기가 그렇게 싸다던데오ㅡ",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011330.1.19",
          "form": "네네 술도싸고",
          "original_form": "네네 술도싸고",
          "speaker_id": "1",
          "time": "20210824 11:23"
        }
      ]
