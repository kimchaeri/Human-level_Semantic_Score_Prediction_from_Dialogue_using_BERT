      "utterance": [
        {
          "id": "MDRW2100023196.1.1",
          "form": "뮤지컬 좋아하세요?",
          "original_form": "뮤지컬 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023196.1.2",
          "form": "네! 근데 안보러간지 오래됐더요ㅜㅜ",
          "original_form": "네! 근데 안보러간지 오래됐더요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023196.1.3",
          "form": "뮤지컬 좋아하세여??",
          "original_form": "뮤지컬 좋아하세여??",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023196.1.4",
          "form": "네 좋아해요 근데 지방 살아서 잘 못 가요ㅠㅠ",
          "original_form": "네 좋아해요 근데 지방 살아서 잘 못 가요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023196.1.5",
          "form": "윽 ㅜㅜㅜ 오가는시간이 길면 공연보기 진짜 진빠지죠ㅠㅠ",
          "original_form": "윽 ㅜㅜㅜ 오가는시간이 길면 공연보기 진짜 진빠지죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023196.1.6",
          "form": "저는 낮공 밤공 보는날이면 시간보내는게 애매하더라구요~",
          "original_form": "저는 낮공 밤공 보는날이면 시간보내는게 애매하더라구요~",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023196.1.7",
          "form": "중간에 뜨는 시간들 애매하죠 ㅋㅋ 최근에 뭐 보셨어요?",
          "original_form": "중간에 뜨는 시간들 애매하죠 ㅋㅋ 최근에 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023196.1.8",
          "form": "저 안본지 오래됐어요ㅠ 코로나이전에도 안보러다녔거든요~ 기억이 잘 안나요..",
          "original_form": "저 안본지 오래됐어요ㅠ 코로나이전에도 안보러다녔거든요~ 기억이 잘 안나요..",
          "speaker_id": "2",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023196.1.9",
          "form": "최근에 보신것 있으세요?",
          "original_form": "최근에 보신것 있으세요?",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023196.1.10",
          "form": "최근에 위키드 봤어요",
          "original_form": "최근에 위키드 봤어요",
          "speaker_id": "1",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023196.1.11",
          "form": "오 누구나오는걸로 보셨어용??",
          "original_form": "오 누구나오는걸로 보셨어용??",
          "speaker_id": "2",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100023196.1.12",
          "form": "손승연 출연하는 거로요",
          "original_form": "손승연 출연하는 거로요",
          "speaker_id": "1",
          "time": "20210829 23:14"
        },
        {
          "id": "MDRW2100023196.1.13",
          "form": "옥주현이었으면 좋았겠지만 다 매진이더라구요",
          "original_form": "옥주현이었으면 좋았겠지만 다 매진이더라구요",
          "speaker_id": "1",
          "time": "20210829 23:14"
        },
        {
          "id": "MDRW2100023196.1.14",
          "form": "아아 옥주현 공연은 진짜 늘 매진이더라구요",
          "original_form": "아아 옥주현 공연은 진짜 늘 매진이더라구요",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023196.1.15",
          "form": "그래도 손승연님도 노래 엄청잘하시잖아요!!!",
          "original_form": "그래도 손승연님도 노래 엄청잘하시잖아요!!!",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023196.1.16",
          "form": "맞아요 넘 감탄하면서 공연 봐서 공연 끝날 땐 턱이 빠졌잖아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 넘 감탄하면서 공연 봐서 공연 끝날 땐 턱이 빠졌잖아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023196.1.17",
          "form": "레알로 턱이 빠지셨나요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "레알로 턱이 빠지셨나요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023196.1.18",
          "form": "공연은 생생함이 전달되어서 공연보는사람까지 희열을 느끼는것 같아요",
          "original_form": "공연은 생생함이 전달되어서 공연보는사람까지 희열을 느끼는것 같아요",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023196.1.19",
          "form": "에너지가 막 느껴지는 것 같죠 왜 사람들이 회전문 도는지 알 것 같더라고요",
          "original_form": "에너지가 막 느껴지는 것 같죠 왜 사람들이 회전문 도는지 알 것 같더라고요",
          "speaker_id": "1",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023196.1.20",
          "form": "완전요 ㅋㅋㅋㅋㅋ 중간 텀에 낮공 곱씹으며 힐링하고 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "완전요 ㅋㅋㅋㅋㅋ 중간 텀에 낮공 곱씹으며 힐링하고 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100023196.1.21",
          "form": "저는 중극장 대학로 위주로 돌았거든요!",
          "original_form": "저는 중극장 대학로 위주로 돌았거든요!",
          "speaker_id": "2",
          "time": "20210829 23:30"
        }
      ]
    }
