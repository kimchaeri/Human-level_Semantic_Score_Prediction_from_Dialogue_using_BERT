      "utterance": [
        {
          "id": "MDRW2100024149.1.1",
          "form": "여행은 어디로 가고 싶으신가요?",
          "original_form": "여행은 어디로 가고 싶으신가요?",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024149.1.2",
          "form": "저요 음.. 대만요",
          "original_form": "저요 음.. 대만요",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024149.1.3",
          "form": "코로나 종식이라더 그나마안전한 대가 대만 같아요 ㅋㅋㅋ 구리고 그 대만 드라마 상견니를 재미나게 봐서 ㅋㅋ",
          "original_form": "코로나 종식이라더 그나마안전한 대가 대만 같아요 ㅋㅋㅋ 구리고 그 대만 드라마 상견니를 재미나게 봐서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024149.1.4",
          "form": "대만!\r\n세븐일레븐커피 맛있어요ㅋㅋㅋ",
          "original_form": "대만!\r\n세븐일레븐커피 맛있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024149.1.5",
          "form": "근데  음식이 너무 맛없어요ㅠㅠ",
          "original_form": "근데  음식이 너무 맛없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024149.1.6",
          "form": "맛없어요?",
          "original_form": "맛없어요?",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024149.1.7",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024149.1.8",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024149.1.9",
          "form": "생선도..그냥 스팀조림..",
          "original_form": "생선도..그냥 스팀조림..",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024149.1.10",
          "form": "아....안되겠다 거기 ㅋㅋㅋㅋ",
          "original_form": "아....안되겠다 거기 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024149.1.11",
          "form": "제가 다 잘먹어서",
          "original_form": "제가 다 잘먹어서",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024149.1.12",
          "form": "먹긴 먹었는데",
          "original_form": "먹긴 먹었는데",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024149.1.13",
          "form": "맛있는건 없었어요ㅋㅋㅋㅋ",
          "original_form": "맛있는건 없었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024149.1.14",
          "form": "저도 홍콩가서 제일 맛난게",
          "original_form": "저도 홍콩가서 제일 맛난게",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024149.1.15",
          "form": "근데 과자나 커피랑 우롱차랑 버블티는 맛있어요ㅋㅋㅋㅋ",
          "original_form": "근데 과자나 커피랑 우롱차랑 버블티는 맛있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024149.1.16",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024149.1.17",
          "form": "맥도날드랑...새우탕면 딤섬뿐",
          "original_form": "맥도날드랑...새우탕면 딤섬뿐",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024149.1.18",
          "form": "맥도날드가 최고맛났던 기억이",
          "original_form": "맥도날드가 최고맛났던 기억이",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024149.1.19",
          "form": "아 거기 버블티 과일주스 싸고 맛나서 물처럼 마신다면서요 대만이 ㅋㅋㅋ 아는언니 구래서 쪄서온 ㅋㅋㅋ",
          "original_form": "아 거기 버블티 과일주스 싸고 맛나서 물처럼 마신다면서요 대만이 ㅋㅋㅋ 아는언니 구래서 쪄서온 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024149.1.20",
          "form": "새우탕면ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "새우탕면ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024149.1.21",
          "form": "맞아요ㅋㅋ물대용으로 마셔요",
          "original_form": "맞아요ㅋㅋ물대용으로 마셔요",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024149.1.22",
          "form": "중국처럼 수질이 별로라 물대신 차마신다고 하더라궁소",
          "original_form": "중국처럼 수질이 별로라 물대신 차마신다고 하더라궁소",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024149.1.23",
          "form": "역시... 중화권은... 위생 조심",
          "original_form": "역시... 중화권은... 위생 조심",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024149.1.24",
          "form": "태국도 가보고픈데 뭐든 사재기를 하고싶은 걸까..",
          "original_form": "태국도 가보고픈데 뭐든 사재기를 하고싶은 걸까..",
          "speaker_id": "2",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024149.1.25",
          "form": "결국 여행은 쇼핑이죠",
          "original_form": "결국 여행은 쇼핑이죠",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024149.1.26",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024149.1.27",
          "form": "마자요 ㅋㅋㅋㅋㅋ쇼핑 하고 오면근데 가서 뭐하고 왔나 싶지만 ㅋㅋㅋㅋ 물욕 좀 버려야하는데 ㅋㅋ",
          "original_form": "마자요 ㅋㅋㅋㅋㅋ쇼핑 하고 오면근데 가서 뭐하고 왔나 싶지만 ㅋㅋㅋㅋ 물욕 좀 버려야하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024149.1.28",
          "form": "그거 쉽게 안버려지더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "그거 쉽게 안버려지더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:59"
        }
      ]
    }
