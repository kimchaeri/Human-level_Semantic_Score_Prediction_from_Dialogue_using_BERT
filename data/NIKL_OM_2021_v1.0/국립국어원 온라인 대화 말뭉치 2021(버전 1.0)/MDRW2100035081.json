        {
          "id": "MDRW2100035081.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210829 02:11"
        },
        {
          "id": "MDRW2100035081.1.2",
          "form": "나 머리 염색하려고",
          "original_form": "나 머리 염색하려고",
          "speaker_id": "1",
          "time": "20210829 02:11"
        },
        {
          "id": "MDRW2100035081.1.3",
          "form": "무슨 색으로 할거야",
          "original_form": "무슨 색으로 할거야",
          "speaker_id": "2",
          "time": "20210829 02:11"
        },
        {
          "id": "MDRW2100035081.1.4",
          "form": "그게 고민이야 골라줄래",
          "original_form": "그게 고민이야 골라줄래",
          "speaker_id": "1",
          "time": "20210829 02:11"
        },
        {
          "id": "MDRW2100035081.1.5",
          "form": "밝은 색갈 거야 어두운 색갈 거야",
          "original_form": "밝은 색갈 거야 어두운 색갈 거야",
          "speaker_id": "2",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100035081.1.6",
          "form": "일단 밝은 색을 한다면 노란색을 할거야",
          "original_form": "일단 밝은 색을 한다면 노란색을 할거야",
          "speaker_id": "1",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100035081.1.7",
          "form": "금발머리는 좀 힘들지 않나",
          "original_form": "금발머리는 좀 힘들지 않나",
          "speaker_id": "2",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100035081.1.8",
          "form": "그래서 어두운 머리도 고민중",
          "original_form": "그래서 어두운 머리도 고민중",
          "speaker_id": "1",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100035081.1.9",
          "form": "갈색 머리도 괜찮긴 하지",
          "original_form": "갈색 머리도 괜찮긴 하지",
          "speaker_id": "2",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100035081.1.10",
          "form": "내가 얼굴이 노란편이라서",
          "original_form": "내가 얼굴이 노란편이라서",
          "speaker_id": "1",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100035081.1.11",
          "form": "그럼 노란 머리는 안 어울리겠네",
          "original_form": "그럼 노란 머리는 안 어울리겠네",
          "speaker_id": "2",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100035081.1.12",
          "form": "애쉬는 어떻게 생각해",
          "original_form": "애쉬는 어떻게 생각해",
          "speaker_id": "1",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100035081.1.13",
          "form": "나도 애쉬 색깔 한번 해보고 싶더라",
          "original_form": "나도 애쉬 색깔 한번 해보고 싶더라",
          "speaker_id": "2",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100035081.1.14",
          "form": "해보고 후기 알려줄게",
          "original_form": "해보고 후기 알려줄게",
          "speaker_id": "1",
          "time": "20210829 02:13"
