      "utterance": [
        {
          "id": "MDRW2100015683.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015683.1.2",
          "form": "음 뭐 요새 안나가서",
          "original_form": "음 뭐 요새 안나가서",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015683.1.3",
          "form": "집에서 맨날 쇼핑하죠 ㅎㅎ",
          "original_form": "집에서 맨날 쇼핑하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015683.1.4",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015683.1.5",
          "form": "저도 쇼핑 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저도 쇼핑 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015683.1.6",
          "form": "ㅋㅋ거의 맨날와요",
          "original_form": "ㅋㅋ거의 맨날와요",
          "speaker_id": "1",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015683.1.7",
          "form": "저도맨날택배뜯고 ㅋㅋㅋㅋ",
          "original_form": "저도맨날택배뜯고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015683.1.8",
          "form": "이것저것 싸면사고",
          "original_form": "이것저것 싸면사고",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015683.1.9",
          "form": "저도요 박스가 ㅋㅋ",
          "original_form": "저도요 박스가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015683.1.10",
          "form": "장난아니예요",
          "original_form": "장난아니예요",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015683.1.11",
          "form": "특가를 못참고 손이 ㅋㅋ막가요",
          "original_form": "특가를 못참고 손이 ㅋㅋ막가요",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015683.1.12",
          "form": "택배기사님한테 맨날 배송문자오고 ㅋㅋㅋㅋㅋ",
          "original_form": "택배기사님한테 맨날 배송문자오고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015683.1.13",
          "form": "저도요 대한통운 ㅋㅋ",
          "original_form": "저도요 대한통운 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015683.1.14",
          "form": "아 가끔민망해요",
          "original_form": "아 가끔민망해요",
          "speaker_id": "1",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015683.1.15",
          "form": "맞아요 ㅋㅋㅋㅋ민망하지만 멈출수없음ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ민망하지만 멈출수없음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015683.1.16",
          "form": "어제는 뭔 쿠폰 100원이길래 어느새 제가 누르고 있었어요 ㅋㅋ",
          "original_form": "어제는 뭔 쿠폰 100원이길래 어느새 제가 누르고 있었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015683.1.17",
          "form": "아 마스크샀어요",
          "original_form": "아 마스크샀어요",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015683.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ본능적인 손",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ본능적인 손",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015683.1.19",
          "form": "마스크 중요하죠",
          "original_form": "마스크 중요하죠",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015683.1.20",
          "form": "근데 집에 많은데 ㅜㅜ안쓰고 샀네요",
          "original_form": "근데 집에 많은데 ㅜㅜ안쓰고 샀네요",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015683.1.21",
          "form": "마스크도 종류따라 불편한것도 잇고 ㅠㅠ",
          "original_form": "마스크도 종류따라 불편한것도 잇고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015683.1.22",
          "form": "아무리 많아도 쓰는것만써요",
          "original_form": "아무리 많아도 쓰는것만써요",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015683.1.23",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015683.1.24",
          "form": "맞아요 결국 쓰는건 한종류 ㅋㅋㅋㅋ",
          "original_form": "맞아요 결국 쓰는건 한종류 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015683.1.25",
          "form": "저마스크 어쩐데요",
          "original_form": "저마스크 어쩐데요",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015683.1.26",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015683.1.27",
          "form": "그때ㅠ대란에 달리기해서 산것도 ㅋㅋ그대로",
          "original_form": "그때ㅠ대란에 달리기해서 산것도 ㅋㅋ그대로",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015683.1.28",
          "form": "참 미쳤었죠",
          "original_form": "참 미쳤었죠",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015683.1.29",
          "form": "써보고 마음에 안드시는건 당근하시는건 어때요?",
          "original_form": "써보고 마음에 안드시는건 당근하시는건 어때요?",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015683.1.30",
          "form": "에어 데이즈만사요 ㅎㅎ",
          "original_form": "에어 데이즈만사요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015683.1.31",
          "form": "그럴까요",
          "original_form": "그럴까요",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015683.1.32",
          "form": "근데 요새 마스크가 넘쳐나서 팔릴지 모르겠어요 ㅜㅜ",
          "original_form": "근데 요새 마스크가 넘쳐나서 팔릴지 모르겠어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015683.1.33",
          "form": "마스크 뭐쓰세요?",
          "original_form": "마스크 뭐쓰세요?",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015683.1.34",
          "form": "좀 아까워도 가격을 좀 내리면 팔릴것같아요",
          "original_form": "좀 아까워도 가격을 좀 내리면 팔릴것같아요",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015683.1.35",
          "form": "네 그럴께요 ㅜㅜ",
          "original_form": "네 그럴께요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015683.1.36",
          "form": "팔아야지 넘 많아요",
          "original_form": "팔아야지 넘 많아요",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015683.1.37",
          "form": "저는 에어퀸마스크 줄곧 썼어요",
          "original_form": "저는 에어퀸마스크 줄곧 썼어요",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015683.1.38",
          "form": "아 저도 있어요 ㅋㅋ",
          "original_form": "아 저도 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015683.1.39",
          "form": "바꾸시게요?",
          "original_form": "바꾸시게요?",
          "speaker_id": "1",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015683.1.40",
          "form": "아니요 ㅋㅋㅋㅋ 한가득 쟁여놔서 바꿀수없어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋㅋ 한가득 쟁여놔서 바꿀수없어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015683.1.41",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015683.1.42",
          "form": "아ㅠ진짜 마스크 늪이네요",
          "original_form": "아ㅠ진짜 마스크 늪이네요",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015683.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ분명히 집에 잔뜩 잇는걸 아는데 계속 사게되는 늪",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ분명히 집에 잔뜩 잇는걸 아는데 계속 사게되는 늪",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015683.1.44",
          "form": "맞아요 진짜 어쩔수 없어요",
          "original_form": "맞아요 진짜 어쩔수 없어요",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015683.1.45",
          "form": "또쟁이고",
          "original_form": "또쟁이고",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015683.1.46",
          "form": "쌓이고",
          "original_form": "쌓이고",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015683.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 마스크 담아놓는 상자 늘 풀로 꽉차있고 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 마스크 담아놓는 상자 늘 풀로 꽉차있고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015683.1.48",
          "form": "마를새가 없어요",
          "original_form": "마를새가 없어요",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015683.1.49",
          "form": "언제 마스크 대란 이였나 싶어요",
          "original_form": "언제 마스크 대란 이였나 싶어요",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015683.1.50",
          "form": "맞아요 ㅋㅋㅋㅋ 마스크 대란때 그렇게 간절하게 몇장 구하고 그랫엇는데",
          "original_form": "맞아요 ㅋㅋㅋㅋ 마스크 대란때 그렇게 간절하게 몇장 구하고 그랫엇는데",
          "speaker_id": "2",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015683.1.51",
          "form": "그러니까요 참 이렇게 100원짜리도 있는날 오다니",
          "original_form": "그러니까요 참 이렇게 100원짜리도 있는날 오다니",
          "speaker_id": "1",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015683.1.52",
          "form": "굿밤되세요 ㅎㅎ낼봐요 ㅎㅎ",
          "original_form": "굿밤되세요 ㅎㅎ낼봐요 ㅎㅎ",
