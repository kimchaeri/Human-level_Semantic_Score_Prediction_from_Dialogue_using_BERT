      "utterance": [
        {
          "id": "MDRW2100020028.1.1",
          "form": "name1님은 포지션이 무엇인가요??",
          "original_form": "&name1&님은 포지션이 무엇인가요??",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020028.1.2",
          "form": "전 기다리기 싫어서 걍 탱커 아님 지원 하네요",
          "original_form": "전 기다리기 싫어서 걍 탱커 아님 지원 하네요",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020028.1.3",
          "form": "ㅠ 딜러가 재밌는데",
          "original_form": "ㅠ 딜러가 재밌는데",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020028.1.4",
          "form": "딜러 뭐하세요?",
          "original_form": "딜러 뭐하세요?",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020028.1.5",
          "form": "저도 기다리는 거 싫을 땐 힐러 해요",
          "original_form": "저도 기다리는 거 싫을 땐 힐러 해요",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020028.1.6",
          "form": "그나마 모이라나 아나는 할만해서",
          "original_form": "그나마 모이라나 아나는 할만해서",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020028.1.7",
          "form": "모이라 아나 재밌죠 ㅋㅋ",
          "original_form": "모이라 아나 재밌죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020028.1.8",
          "form": "딜러는 위도우메이커 한조 많이 했어요",
          "original_form": "딜러는 위도우메이커 한조 많이 했어요",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020028.1.9",
          "form": "전 루슈나 멜시 해요",
          "original_form": "전 루슈나 멜시 해요",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020028.1.10",
          "form": "위메 한조 오",
          "original_form": "위메 한조 오",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.11",
          "form": "실력있으신가봐요? ㅋㅋ",
          "original_form": "실력있으신가봐요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.12",
          "form": "멜시 어렵지 않나요??",
          "original_form": "멜시 어렵지 않나요??",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.13",
          "form": "헤드 잘 따여서",
          "original_form": "헤드 잘 따여서",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.14",
          "form": "그냥 잘숨고 따라가기",
          "original_form": "그냥 잘숨고 따라가기",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.15",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.16",
          "form": "이니에요 실력은... 심해인데ㅋㅋㅋㅋ",
          "original_form": "이니에요 실력은... 심해인데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.17",
          "form": "위메 한조가 죽여요",
          "original_form": "위메 한조가 죽여요",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그렇죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그렇죠",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.20",
          "form": "name2님이 죽이나봐요",
          "original_form": "&name2&님이 죽이나봐요",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020028.1.21",
