          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028899.1.5",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028899.1.6",
          "form": "얼마나 되셨어요",
          "original_form": "얼마나 되셨어요",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028899.1.7",
          "form": "8년이요",
          "original_form": "8년이요",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028899.1.8",
          "form": "자제분도있으시겠어요",
          "original_form": "자제분도있으시겠어요",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028899.1.9",
          "form": "초딩이요",
