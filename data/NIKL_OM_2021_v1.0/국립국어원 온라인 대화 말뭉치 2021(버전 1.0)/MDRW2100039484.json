      "utterance": [
        {
          "id": "MDRW2100039484.1.1",
          "form": "팝 장르예요!!! 약간 잔잔하게 시작해서 하이라이트가 막 팡팡 터지는!",
          "original_form": "팝 장르예요!!! 약간 잔잔하게 시작해서 하이라이트가 막 팡팡 터지는!",
          "speaker_id": "2",
          "time": "20210903 18:09"
        },
        {
          "id": "MDRW2100039484.1.2",
          "form": "헉 저도 좋아할 것 같은데요!!!",
          "original_form": "헉 저도 좋아할 것 같은데요!!!",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039484.1.3",
          "form": "저도 들어볼게요 헤헤",
          "original_form": "저도 들어볼게요 헤헤",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039484.1.4",
          "form": "뭔가 불꽃놀이같은 음악이군요!!",
          "original_form": "뭔가 불꽃놀이같은 음악이군요!!",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039484.1.5",
          "form": "네네!!! 데뷔곡은 잔잔해요!!!!",
          "original_form": "네네!!! 데뷔곡은 잔잔해요!!!!",
          "speaker_id": "2",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039484.1.6",
          "form": "약간 전남친 저격송으로 유명할 걸요?!",
          "original_form": "약간 전남친 저격송으로 유명할 걸요?!",
          "speaker_id": "2",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039484.1.7",
          "form": "아 넘웃기네요",
          "original_form": "아 넘웃기네요",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039484.1.8",
          "form": "최근 2년 사이에 뭔가 전남친 저격송 많이 나온거같지 않나요?",
          "original_form": "최근 2년 사이에 뭔가 전남친 저격송 많이 나온거같지 않나요?",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039484.1.9",
          "form": "저는 그",
          "original_form": "저는 그",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039484.1.10",
          "form": "therapist란 노래를 알아욬ㅋㅋㅋ",
          "original_form": "therapist란 노래를 알아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039484.1.11",
          "form": "헛 처음 들어봐요! 가수 누군가뇨?!",
          "original_form": "헛 처음 들어봐요! 가수 누군가뇨?!",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039484.1.12",
          "form": "Mae Muller라는 가수인데",
          "original_form": "Mae Muller라는 가수인데",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039484.1.13",
          "form": "유튜브에 검색하면",
          "original_form": "유튜브에 검색하면",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039484.1.14",
          "form": "가사 해석한거 나와요!!",
          "original_form": "가사 해석한거 나와요!!",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039484.1.15",
          "form": "저도 name2님이 추천해주신 거 들어볼게요!",
          "original_form": "저도 &name2&님이 추천해주신 거 들어볼게요!",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039484.1.16",
          "form": "헷 네! 저도 name1님 추천한 거 들어볼게요 ㅎ.ㅎ !!!",
          "original_form": "헷 네! 저도 &name1&님 추천한 거 들어볼게요 ㅎ.ㅎ !!!",
          "speaker_id": "2",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039484.1.17",
          "form": "name1님 또 좋아하시는 팝가수 잇으세요?!",
          "original_form": "&name1&님 또 좋아하시는 팝가수 잇으세요?!",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039484.1.18",
          "form": "전 약간 예전 팝 좋아해서 테일러 완전 좋아래요",
          "original_form": "전 약간 예전 팝 좋아해서 테일러 완전 좋아래요",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039484.1.19",
          "form": "헐 저도 좋아해요",
          "original_form": "헐 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039484.1.20",
          "form": "지금 드라이버 라이센스 듣고있어요!!!",
          "original_form": "지금 드라이버 라이센스 듣고있어요!!!",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039484.1.21",
          "form": "저 유튜브 프리미엄인데",
          "original_form": "저 유튜브 프리미엄인데",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039484.1.22",
          "form": "다른거 하면서 뮤비 볼 수 있어서 너무 좋아요",
          "original_form": "다른거 하면서 뮤비 볼 수 있어서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039484.1.23",
          "form": "그 팝은 아닌데 저는",
          "original_form": "그 팝은 아닌데 저는",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039484.1.24",
          "form": "Tom Misch 도 진자 좋아해요!",
          "original_form": "Tom Misch 도 진자 좋아해요!",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039484.1.25",
          "form": "와 이 분 디즈니 출신이시구나...",
          "original_form": "와 이 분 디즈니 출신이시구나...",
          "speaker_id": "1",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100039484.1.26",
          "form": "헐 맞아요 저도 그래서 유튜브 프리미엄 못 끊어요 ㅎ.ㅎ",
          "original_form": "헐 맞아요 저도 그래서 유튜브 프리미엄 못 끊어요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100039484.1.27",
          "form": "네네!!! 디즈니 출신 남자 배우가 전남친이란",
          "original_form": "네네!!! 디즈니 출신 남자 배우가 전남친이란",
          "speaker_id": "2",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100039484.1.28",
          "form": "썰도 있더라고요",
          "original_form": "썰도 있더라고요",
          "speaker_id": "2",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100039484.1.29",
          "form": "아 헐헐",
          "original_form": "아 헐헐",
          "speaker_id": "1",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100039484.1.30",
          "form": "우와 그런 스토리가 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우와 그런 스토리가 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100039484.1.31",
          "form": "심지어 그 두 번째 곡이 아마 데자부인데",
          "original_form": "심지어 그 두 번째 곡이 아마 데자부인데",
          "speaker_id": "2",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039484.1.32",
          "form": "그게 전남친의 현여친 디스곡인데",
          "original_form": "그게 전남친의 현여친 디스곡인데",
          "speaker_id": "2",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039484.1.33",
          "form": "그 여친도 디즈니 아역배우라는 소리가..!",
          "original_form": "그 여친도 디즈니 아역배우라는 소리가..!",
          "speaker_id": "2",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039484.1.34",
          "form": "허억?!?@",
          "original_form": "허억?!?@",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039484.1.35",
          "form": "꿈과 희망의 디즈니에서",
          "original_form": "꿈과 희망의 디즈니에서",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039484.1.36",
          "form": "정말 많은 일이 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "정말 많은 일이 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039484.1.37",
          "form": "저는 디즈니 하면",
          "original_form": "저는 디즈니 하면",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039484.1.38",
          "form": "셀레나 고메즈랑 아리아나 그란데밖에",
          "original_form": "셀레나 고메즈랑 아리아나 그란데밖에",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039484.1.39",
          "form": "몰랐유요!",
          "original_form": "몰랐유요!",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039484.1.40",
          "form": "저도 그러다가",
          "original_form": "저도 그러다가",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039484.1.41",
          "form": "저도 그러다가 이 가수 알고 ㅎ.ㅎ",
          "original_form": "저도 그러다가 이 가수 알고 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039484.1.42",
          "form": "알게 됐어요!!!",
          "original_form": "알게 됐어요!!!",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039484.1.43",
          "form": "그 전남친 저격송? 이 너무 신기해서.. ㅎㅎ",
          "original_form": "그 전남친 저격송? 이 너무 신기해서.. ㅎㅎ",
