      "utterance": [
        {
          "id": "MDRW2100023018.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023018.1.2",
          "form": "좋아하는 드라마 있나요",
          "original_form": "좋아하는 드라마 있나요",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023018.1.3",
          "form": "저는 일드좋아합니다",
          "original_form": "저는 일드좋아합니다",
          "speaker_id": "1",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023018.1.4",
          "form": "일드중에서 트릭? 좋아해요",
          "original_form": "일드중에서 트릭? 좋아해요",
          "speaker_id": "1",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023018.1.5",
          "form": "아 저는 일드는 전혀 모르네요 ㅠ 한국 예능도 안보시나요,",
          "original_form": "아 저는 일드는 전혀 모르네요 ㅠ 한국 예능도 안보시나요,",
          "speaker_id": "2",
          "time": "20210830 05:38"
        },
        {
          "id": "MDRW2100023018.1.6",
          "form": "예능 많이 보죠!",
          "original_form": "예능 많이 보죠!",
          "speaker_id": "1",
          "time": "20210830 10:50"
        },
        {
          "id": "MDRW2100023018.1.7",
          "form": "예능은 어떤 프로 보시나요,",
          "original_form": "예능은 어떤 프로 보시나요,",
          "speaker_id": "2",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100023018.1.8",
          "form": "대탈출? 런닝맨? 맛있는녀석들 골고루봐요ㅋㅋㅋ",
          "original_form": "대탈출? 런닝맨? 맛있는녀석들 골고루봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100023018.1.9",
          "form": "장르불문하고",
          "original_form": "장르불문하고",
          "speaker_id": "1",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100023018.1.10",
          "form": "그렇군요^^",
          "original_form": "그렇군요^^",
          "speaker_id": "2",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100023018.1.11",
          "form": "좋아하는 연애인있으세요?",
          "original_form": "좋아하는 연애인있으세요?",
          "speaker_id": "1",
          "time": "20210830 11:10"
        },
        {
          "id": "MDRW2100023018.1.12",
          "form": "저는 없어요 레비야트님은요?",
          "original_form": "저는 없어요 레비야트님은요?",
          "speaker_id": "2",
          "time": "20210830 12:04"
        },
        {
          "id": "MDRW2100023018.1.13",
          "form": "저는 한국쪽은없고 일본배우 좋아해요",
          "original_form": "저는 한국쪽은없고 일본배우 좋아해요",
          "speaker_id": "1",
          "time": "20210830 12:06"
        },
        {
          "id": "MDRW2100023018.1.14",
          "form": "타카하타 미츠키",
          "original_form": "타카하타 미츠키",
          "speaker_id": "1",
          "time": "20210830 12:06"
        },
        {
          "id": "MDRW2100023018.1.15",
          "form": "어머..저는 문외한이네요 ㅠ 가수는요?",
          "original_form": "어머..저는 문외한이네요 ㅠ 가수는요?",
          "speaker_id": "2",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023018.1.16",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "1",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100023018.1.17",
          "form": "가수는...없어요",
          "original_form": "가수는...없어요",
          "speaker_id": "1",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100023018.1.18",
          "form": "그러시군요 저는 환승연애 보는중이예요",
