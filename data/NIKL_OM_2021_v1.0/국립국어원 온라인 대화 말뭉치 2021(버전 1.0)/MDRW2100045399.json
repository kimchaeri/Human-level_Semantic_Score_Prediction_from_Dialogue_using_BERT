      "utterance": [
        {
          "id": "MDRW2100045399.1.1",
          "form": "저는 게임 입문을 심즈로",
          "original_form": "저는 게임 입문을 심즈로",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045399.1.2",
          "form": "심즈 재미있나요",
          "original_form": "심즈 재미있나요",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045399.1.3",
          "form": "내 완전",
          "original_form": "내 완전",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045399.1.4",
          "form": "저도 하는 거만 봤는데 재밌더라고요",
          "original_form": "저도 하는 거만 봤는데 재밌더라고요",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045399.1.5",
          "form": "직접 하면 더 재미있어요",
          "original_form": "직접 하면 더 재미있어요",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045399.1.6",
          "form": "몇 탄 추천하세요",
          "original_form": "몇 탄 추천하세요",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045399.1.7",
          "form": "다 재미있는데 제일 최근 거요",
          "original_form": "다 재미있는데 제일 최근 거요",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045399.1.8",
          "form": "지금도 하시는지",
          "original_form": "지금도 하시는지",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045399.1.9",
          "form": "요즘에는 컴퓨터 없어서 못해요",
          "original_form": "요즘에는 컴퓨터 없어서 못해요",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045399.1.10",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045399.1.11",
          "form": "알아보고 있긴 한데",
          "original_form": "알아보고 있긴 한데",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045399.1.12",
          "form": "값이 올랐던데요",
          "original_form": "값이 올랐던데요",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045399.1.13",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "2",
          "time": "20210908 02:22"
        }
