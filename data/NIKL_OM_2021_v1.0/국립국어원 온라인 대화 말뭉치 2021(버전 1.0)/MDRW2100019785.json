      "utterance": [
        {
          "id": "MDRW2100019785.1.1",
          "form": "메타버스라고 아세요?",
          "original_form": "메타버스라고 아세요?",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019785.1.2",
          "form": "가상세계 서비스인가요",
          "original_form": "가상세계 서비스인가요",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019785.1.3",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019785.1.4",
          "form": "가상세계 서비스가 왜 필요할까요",
          "original_form": "가상세계 서비스가 왜 필요할까요",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019785.1.5",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019785.1.6",
          "form": "현실에서 성실히 잘 살면되는 거 아닌가요?",
          "original_form": "현실에서 성실히 잘 살면되는 거 아닌가요?",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019785.1.7",
          "form": "제생각엔 보고싶은데 못보는 사람을 볼 수 있고",
          "original_form": "제생각엔 보고싶은데 못보는 사람을 볼 수 있고",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019785.1.8",
          "form": "지금같이 어디도 못갈때 여행다녀올 수있지않을까요",
          "original_form": "지금같이 어디도 못갈때 여행다녀올 수있지않을까요",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019785.1.9",
          "form": "그럴 수도 있겠네요",
          "original_form": "그럴 수도 있겠네요",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019785.1.10",
          "form": "저도 엄마가 돌아가셔서 사실 지금도 가끔은 믿기지않을 때가 많은데 실상은 볼 수 없으니 너무 그립거든요",
          "original_form": "저도 엄마가 돌아가셔서 사실 지금도 가끔은 믿기지않을 때가 많은데 실상은 볼 수 없으니 너무 그립거든요",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019785.1.11",
          "form": "그래서 꿈에서 만나긴하지만 브이알같은 걸로라도 보고싶어요",
          "original_form": "그래서 꿈에서 만나긴하지만 브이알같은 걸로라도 보고싶어요",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019785.1.12",
          "form": "아... 제가 경솔했네요",
          "original_form": "아... 제가 경솔했네요",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019785.1.13",
          "form": "아녜요",
          "original_form": "아녜요",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019785.1.14",
          "form": "필요한 사람이 있으니 발전이 있죠",
          "original_form": "필요한 사람이 있으니 발전이 있죠",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019785.1.15",
          "form": "힘내세요",
          "original_form": "힘내세요",
          "speaker_id": "2",
          "time": "20210827 22:01"
        }
