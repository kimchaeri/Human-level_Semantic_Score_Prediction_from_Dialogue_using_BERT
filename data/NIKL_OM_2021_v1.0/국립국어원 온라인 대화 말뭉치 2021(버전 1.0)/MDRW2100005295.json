      "utterance": [
        {
          "id": "MDRW2100005295.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005295.1.2",
          "form": "집안일 빨래나 설거지는 해요!",
          "original_form": "집안일 빨래나 설거지는 해요!",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005295.1.3",
          "form": "전 청소",
          "original_form": "전 청소",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005295.1.4",
          "form": "아하 청소도 하죠!",
          "original_form": "아하 청소도 하죠!",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005295.1.5",
          "form": "화장실이 대박이죠",
          "original_form": "화장실이 대박이죠",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005295.1.6",
          "form": "엄청 힘들긴하죠 화장실이..",
          "original_form": "엄청 힘들긴하죠 화장실이..",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005295.1.7",
          "form": "락스냄새 싫어요",
          "original_form": "락스냄새 싫어요",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005295.1.8",
          "form": "그 특유의 쏘는 냄새?",
          "original_form": "그 특유의 쏘는 냄새?",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005295.1.9",
          "form": "몸 해치는 느낌나서 별로긴하죠 ㅠㅠ",
          "original_form": "몸 해치는 느낌나서 별로긴하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005295.1.10",
          "form": "머리도 아파뇨",
          "original_form": "머리도 아파뇨",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005295.1.11",
          "form": "맞아요 ㅠㅠ 너무 머리아파요",
          "original_form": "맞아요 ㅠㅠ 너무 머리아파요",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005295.1.12",
          "form": "몸이 거부하는 듯",
          "original_form": "몸이 거부하는 듯",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005295.1.13",
          "form": "그런것도 있고",
          "original_form": "그런것도 있고",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005295.1.14",
          "form": "화학물질도 많다고 들었어요",
          "original_form": "화학물질도 많다고 들었어요",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005295.1.15",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210822 08:40"
        }
      ]
