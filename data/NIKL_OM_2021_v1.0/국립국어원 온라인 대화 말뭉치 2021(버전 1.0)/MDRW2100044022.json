      "utterance": [
        {
          "id": "MDRW2100044022.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044022.1.2",
          "form": "좋아하는 배달음식 있으세요?",
          "original_form": "좋아하는 배달음식 있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044022.1.3",
          "form": "안녕하세요 name1님!!",
          "original_form": "안녕하세요 &name1&님!!",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044022.1.4",
          "form": "저는 뼈해장국 좋아해요",
          "original_form": "저는 뼈해장국 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044022.1.5",
          "form": "근처에 맛있는 집이 있어서!!",
          "original_form": "근처에 맛있는 집이 있어서!!",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044022.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044022.1.7",
          "form": "추천해주실수있나요",
          "original_form": "추천해주실수있나요",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044022.1.8",
          "form": "감자탕 짱맛있죠",
          "original_form": "감자탕 짱맛있죠",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044022.1.9",
          "form": "맛있어용!!!",
          "original_form": "맛있어용!!!",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044022.1.10",
          "form": "요즘 맨날 비와서 국물 먹고싶어요 ㅋㅋ",
          "original_form": "요즘 맨날 비와서 국물 먹고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044022.1.11",
          "form": "조마루 감자탕인데 지점마다 맛이 다르더라고요",
          "original_form": "조마루 감자탕인데 지점마다 맛이 다르더라고요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044022.1.12",
          "form": "그쵸?? 저는 어묵탕도 좀 땡겨요",
          "original_form": "그쵸?? 저는 어묵탕도 좀 땡겨요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044022.1.13",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044022.1.14",
          "form": "들어봤어요",
          "original_form": "들어봤어요",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044022.1.15",
          "form": "체인인데 맛이다르다니.. 뽑기운이 필요하군요",
          "original_form": "체인인데 맛이다르다니.. 뽑기운이 필요하군요",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044022.1.16",
          "form": "ㅋㅋㅋ 지점마다 맛이 다 다르더라고요",
          "original_form": "ㅋㅋㅋ 지점마다 맛이 다 다르더라고요",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044022.1.17",
          "form": "지금 날씨엔 잔치국수도 땡겨요!!",
          "original_form": "지금 날씨엔 잔치국수도 땡겨요!!",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044022.1.18",
          "form": "궁물이 최고에요 ㅎㅎ",
          "original_form": "궁물이 최고에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044022.1.19",
          "form": "역시한국인인가..",
          "original_form": "역시한국인인가..",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044022.1.20",
          "form": "그쵸 역시 쌀쌀해지면",
          "original_form": "그쵸 역시 쌀쌀해지면",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044022.1.21",
          "form": "들이켜고 싶네요",
          "original_form": "들이켜고 싶네요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044022.1.22",
          "form": "벌써 좀 쌀쌀하니 곧 국과 탕의 시기가 오겠어요",
          "original_form": "벌써 좀 쌀쌀하니 곧 국과 탕의 시기가 오겠어요",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044022.1.23",
          "form": "좋아하시는 국 혹시?",
          "original_form": "좋아하시는 국 혹시?",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044022.1.24",
          "form": "소고깃국이요!",
          "original_form": "소고깃국이요!",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044022.1.25",
          "form": "무 넣은 소고깃국 말씀하시는거져",
          "original_form": "무 넣은 소고깃국 말씀하시는거져",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044022.1.26",
          "form": "와 맛있죠",
          "original_form": "와 맛있죠",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044022.1.27",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044022.1.28",
          "form": "오래 끓여야 맛있는국",
          "original_form": "오래 끓여야 맛있는국",
          "speaker_id": "1",
          "time": "20210907 21:41"
        }
      ]
