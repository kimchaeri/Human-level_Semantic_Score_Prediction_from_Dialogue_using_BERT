      "utterance": [
        {
          "id": "MDRW2100036309.1.1",
          "form": "네네 와 면 쿠커도 잇나요",
          "original_form": "네네 와 면 쿠커도 잇나요",
          "speaker_id": "2",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036309.1.2",
          "form": "첨들엉6",
          "original_form": "첨들엉6",
          "speaker_id": "2",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036309.1.3",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036309.1.4",
          "form": "이거 여기다 물넣고",
          "original_form": "이거 여기다 물넣고",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036309.1.5",
          "form": "파스타면 넣고",
          "original_form": "파스타면 넣고",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036309.1.6",
          "form": "전자레인지에 돌리기만하면",
          "original_form": "전자레인지에 돌리기만하면",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036309.1.7",
          "form": "익어용ㅋㅋㅋㅋㅋ",
          "original_form": "익어용ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036309.1.8",
          "form": "역시 사람은 많이 자주 먹으려면 도구를 사야하더랴구요ㅋㅋㅋㅋ",
          "original_form": "역시 사람은 많이 자주 먹으려면 도구를 사야하더랴구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036309.1.9",
          "form": "우와 당장 찾아봐야지",
          "original_form": "우와 당장 찾아봐야지",
          "speaker_id": "2",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036309.1.10",
          "form": "소스는 만들어쓰세요?",
          "original_form": "소스는 만들어쓰세요?",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036309.1.11",
          "form": "소스는 정말 가뭄에 콩나듯 만들어먹는데",
          "original_form": "소스는 정말 가뭄에 콩나듯 만들어먹는데",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036309.1.12",
          "form": "요리고수의 향기가!",
          "original_form": "요리고수의 향기가!",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036309.1.13",
          "form": "제가 만든 노동력 이런거에 비해서 시판 소스가 더 맛있는것같아서",
          "original_form": "제가 만든 노동력 이런거에 비해서 시판 소스가 더 맛있는것같아서",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036309.1.14",
          "form": "그냥 시판써요ㅜㅜㅋㅋㅋㅋ",
          "original_form": "그냥 시판써요ㅜㅜㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036309.1.15",
          "form": "그것도 그래요ㅠㅠㅋㅋㅋㅋ맞아요",
          "original_form": "그것도 그래요ㅠㅠㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036309.1.16",
          "form": "내가 힘들게 만든것보다 가격도 심지어 더 저렴한",
          "original_form": "내가 힘들게 만든것보다 가격도 심지어 더 저렴한",
          "speaker_id": "1",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100036309.1.17",
          "form": "완제품이 더 맛있을때의 허무함이란ㅋㅋㅋ",
          "original_form": "완제품이 더 맛있을때의 허무함이란ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100036309.1.18",
          "form": "맞습니다 또르르",
          "original_form": "맞습니다 또르르",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036309.1.19",
          "form": "쿠커정말 신기해욤",
          "original_form": "쿠커정말 신기해욤",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036309.1.20",
          "form": "히히 이거 말고도 계란 요리 좋아하시면",
          "original_form": "히히 이거 말고도 계란 요리 좋아하시면",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036309.1.21",
          "form": "계란 쿠커도 있는데 추천드려요!",
          "original_form": "계란 쿠커도 있는데 추천드려요!",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036309.1.22",
          "form": "계란쿠커는 뭐지",
          "original_form": "계란쿠커는 뭐지",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036309.1.23",
          "form": "반숙 완숙 조절이 힘드실때",
          "original_form": "반숙 완숙 조절이 힘드실때",
          "speaker_id": "1",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100036309.1.24",
          "form": "이거 쓰시면 계란 요리를 금방 할 수 있어용",
          "original_form": "이거 쓰시면 계란 요리를 금방 할 수 있어용",
          "speaker_id": "1",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100036309.1.25",
          "form": "저는 이걸로 계란 삶아서 계란 샐러드도 해먹고",
          "original_form": "저는 이걸로 계란 삶아서 계란 샐러드도 해먹고",
          "speaker_id": "1",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100036309.1.26",
          "form": "슬라이스해서 샌드위치도 해먹어요!",
          "original_form": "슬라이스해서 샌드위치도 해먹어요!",
          "speaker_id": "1",
          "time": "20210830 22:28"
        },
        {
          "id": "MDRW2100036309.1.27",
          "form": "오 샐러드 샌드위치 두가지 가능",
          "original_form": "오 샐러드 샌드위치 두가지 가능",
          "speaker_id": "2",
          "time": "20210830 22:33"
        }
      ]
    }
  ]
