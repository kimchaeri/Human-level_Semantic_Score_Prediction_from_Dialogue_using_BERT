      "utterance": [
        {
          "id": "MDRW2100037267.1.1",
          "form": "갑자기 바다는 왜",
          "original_form": "갑자기 바다는 왜",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.2",
          "form": "티비에서 나오는데 넘 좋아보인다",
          "original_form": "티비에서 나오는데 넘 좋아보인다",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.3",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.4",
          "form": "ㅠㅠ 장마 끝나면 가자~",
          "original_form": "ㅠㅠ 장마 끝나면 가자~",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.5",
          "form": "그러자",
          "original_form": "그러자",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.6",
          "form": "더 추워지기전에",
          "original_form": "더 추워지기전에",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.7",
          "form": "그래 겨울엔 넘 춥다",
          "original_form": "그래 겨울엔 넘 춥다",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.8",
          "form": "웅웅 ㅋㅋ 가을바다~",
          "original_form": "웅웅 ㅋㅋ 가을바다~",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.9",
          "form": "그래 ㅋㅋ 동해?",
          "original_form": "그래 ㅋㅋ 동해?",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.10",
          "form": "동해도 좋고~ 다른데도 좋고",
          "original_form": "동해도 좋고~ 다른데도 좋고",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037267.1.11",
          "form": "가까운 제부도 가던지",
          "original_form": "가까운 제부도 가던지",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100037267.1.12",
          "form": "오 ㅋ 좋네",
          "original_form": "오 ㅋ 좋네",
          "speaker_id": "1",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100037267.1.13",
          "form": "전에 갔던곳",
          "original_form": "전에 갔던곳",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100037267.1.14",
          "form": "웅웅 거기 좋았다",
          "original_form": "웅웅 거기 좋았다",
          "speaker_id": "1",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100037267.1.15",
          "form": "바다가서 모래사장 위에 걷고 싶다",
          "original_form": "바다가서 모래사장 위에 걷고 싶다",
          "speaker_id": "1",
          "time": "20210831 22:05"
