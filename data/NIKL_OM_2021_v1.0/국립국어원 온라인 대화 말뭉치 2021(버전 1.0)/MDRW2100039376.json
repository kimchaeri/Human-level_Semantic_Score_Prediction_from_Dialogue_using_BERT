      "utterance": [
        {
          "id": "MDRW2100039376.1.1",
          "form": "담주 회담은 봉리에서 해야할듯요~",
          "original_form": "담주 회담은 봉리에서 해야할듯요~",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039376.1.2",
          "form": "나도 맨날 투가 역할극 해 달라고 하거든",
          "original_form": "나도 맨날 투가 역할극 해 달라고 하거든",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039376.1.3",
          "form": "나 목욜까지 여름꺼 다 반품보내야되서ㅠ",
          "original_form": "나 목욜까지 여름꺼 다 반품보내야되서ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039376.1.4",
          "form": "맨날 인형놀이 하고",
          "original_form": "맨날 인형놀이 하고",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039376.1.5",
          "form": "너무 싫은 거야",
          "original_form": "너무 싫은 거야",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039376.1.6",
          "form": "역할극..ㅠㅠ",
          "original_form": "역할극..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039376.1.7",
          "form": "그래서 맨날 아빠한테 해달라하라고 했는데",
          "original_form": "그래서 맨날 아빠한테 해달라하라고 했는데",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039376.1.8",
          "form": "요며칠 반성하고 계속 해주고 있음",
          "original_form": "요며칠 반성하고 계속 해주고 있음",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039376.1.9",
          "form": "그마음 쭉 가야되요 온니;",
          "original_form": "그마음 쭉 가야되요 온니;",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039376.1.10",
          "form": "요즘 투가",
          "original_form": "요즘 투가",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039376.1.11",
          "form": "자연관찰책을 엄청 좋아해",
          "original_form": "자연관찰책을 엄청 좋아해",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039376.1.12",
          "form": "그 자연관찰 뒤에 노래가 있거든",
          "original_form": "그 자연관찰 뒤에 노래가 있거든",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039376.1.13",
          "form": "노래를 좋아해서 그 노래를 따라 부르다 보니까",
          "original_form": "노래를 좋아해서 그 노래를 따라 부르다 보니까",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.14",
          "form": "발음이 조금씩 나아지는 거야",
          "original_form": "발음이 조금씩 나아지는 거야",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.15",
          "form": "근데 똑같은 책을",
          "original_form": "근데 똑같은 책을",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.16",
          "form": "오~좋다좋다~",
          "original_form": "오~좋다좋다~",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.17",
          "form": "진짜 열 번 이상 읽어줬는데도",
          "original_form": "진짜 열 번 이상 읽어줬는데도",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.18",
          "form": "내용은 이해를 못해",
          "original_form": "내용은 이해를 못해",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.19",
          "form": "그냥 그때 듣기만 하고",
          "original_form": "그냥 그때 듣기만 하고",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.20",
          "form": "다음날 모르면 몰라",
          "original_form": "다음날 모르면 몰라",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ자연 관찰은 어려우니깐요~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ자연 관찰은 어려우니깐요~",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.22",
          "form": "근데 한두 번 읽어준 책이 아니거든",
          "original_form": "근데 한두 번 읽어준 책이 아니거든",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.23",
          "form": "항상 몰라",
          "original_form": "항상 몰라",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.24",
          "form": "그래서 내가 요새 심각성을 느꼈다니까",
          "original_form": "그래서 내가 요새 심각성을 느꼈다니까",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.25",
          "form": "영상을 많이 본 애들은",
          "original_form": "영상을 많이 본 애들은",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039376.1.26",
          "form": "진짜 엄마도 쉽지 않구만요 ㅠ",
          "original_form": "진짜 엄마도 쉽지 않구만요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100039376.1.27",
          "form": "근데 영상 많이 본날은 짜증 윽시많이 내요;;",
          "original_form": "근데 영상 많이 본날은 짜증 윽시많이 내요;;",
          "speaker_id": "2",
          "time": "20210903 16:55"
        }
      ]
    }
