      "utterance": [
        {
          "id": "MDRW2100025275.1.1",
          "form": "온라인 커뮤니티 하시나요",
          "original_form": "온라인 커뮤니티 하시나요",
          "speaker_id": "1",
          "time": "20210901 06:32"
        },
        {
          "id": "MDRW2100025275.1.2",
          "form": "커뮤니티라기보단 네이버카페?",
          "original_form": "커뮤니티라기보단 네이버카페?",
          "speaker_id": "2",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025275.1.3",
          "form": "눈팅을 주로해요 ㅎㅎ",
          "original_form": "눈팅을 주로해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025275.1.4",
          "form": "그러시군요 네이버카페 좋죠",
          "original_form": "그러시군요 네이버카페 좋죠",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100025275.1.5",
          "form": "카페가 제일 접근하기 쉬워서 ㅋㅋ",
          "original_form": "카페가 제일 접근하기 쉬워서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100025275.1.6",
          "form": "네 그리고 규모도 크고 정보많은 곳도 많은듯해요",
          "original_form": "네 그리고 규모도 크고 정보많은 곳도 많은듯해요",
          "speaker_id": "1",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100025275.1.7",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100025275.1.8",
          "form": "정보 얻으려고 보통 가입하죠",
          "original_form": "정보 얻으려고 보통 가입하죠",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100025275.1.9",
          "form": "차를 사도 폰을사도 이사를 해도",
          "original_form": "차를 사도 폰을사도 이사를 해도",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100025275.1.10",
          "form": "카페가입이 필수 ㅋㅋ",
          "original_form": "카페가입이 필수 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100025275.1.11",
          "form": "그렇죠 정보는 블로그보다도 카페가 더 믿을만하더라구요",
          "original_form": "그렇죠 정보는 블로그보다도 카페가 더 믿을만하더라구요",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100025275.1.12",
          "form": "실시간으로 계속 글이 올라오기도 하구",
          "original_form": "실시간으로 계속 글이 올라오기도 하구",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100025275.1.13",
          "form": "예전에 시간많을땐 카페에서 사람들하고 댓글달고 놀고 했는데",
          "original_form": "예전에 시간많을땐 카페에서 사람들하고 댓글달고 놀고 했는데",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100025275.1.14",
          "form": "일하니까 이제 그런데 시간쓸 여력이 안되더라구요",
          "original_form": "일하니까 이제 그런데 시간쓸 여력이 안되더라구요",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100025275.1.15",
          "form": "그렇죠 네이버카페 보다보면 시간이 잘 가더라구요",
          "original_form": "그렇죠 네이버카페 보다보면 시간이 잘 가더라구요",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100025275.1.16",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100025275.1.17",
          "form": "전 보통 정보얻으러 가서",
          "original_form": "전 보통 정보얻으러 가서",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100025275.1.18",
          "form": "질문하고 답을얻고 아는거 답해주고",
          "original_form": "질문하고 답을얻고 아는거 답해주고",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100025275.1.19",
          "form": "이정도에요",
          "original_form": "이정도에요",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100025275.1.20",
          "form": "소통의 장소로 서로에게 유용한듯해요",
          "original_form": "소통의 장소로 서로에게 유용한듯해요",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100025275.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100025275.1.22",
          "form": "요즘은 정모같은걸 못하니깐",
          "original_form": "요즘은 정모같은걸 못하니깐",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100025275.1.23",
          "form": "주로 들어가는 카페 분야는 어느쪽인가요",
          "original_form": "주로 들어가는 카페 분야는 어느쪽인가요",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100025275.1.24",
          "form": "저는 최근에 휴대폰 새로사서",
          "original_form": "저는 최근에 휴대폰 새로사서",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100025275.1.25",
          "form": "휴대폰카페랑",
          "original_form": "휴대폰카페랑",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100025275.1.26",
          "form": "새차산지 5개월이라 그정보 얻으러 자동차카페요 ㅎㅎ",
          "original_form": "새차산지 5개월이라 그정보 얻으러 자동차카페요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100025275.1.27",
          "form": "아 혹시 갤럭시 플립? 폴드?",
          "original_form": "아 혹시 갤럭시 플립? 폴드?",
