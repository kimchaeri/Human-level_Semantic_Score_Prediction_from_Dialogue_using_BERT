      "utterance": [
        {
          "id": "MDRW2100021351.1.1",
          "form": "부산은 오신적 있으세요?",
          "original_form": "부산은 오신적 있으세요?",
          "speaker_id": "1",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021351.1.2",
          "form": "넵! 세번정도 가본거같아요!",
          "original_form": "넵! 세번정도 가본거같아요!",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021351.1.3",
          "form": "어디어디 가셨나요?",
          "original_form": "어디어디 가셨나요?",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021351.1.4",
          "form": "해운대 광안리 이런데만 가봤어요",
          "original_form": "해운대 광안리 이런데만 가봤어요",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021351.1.5",
          "form": "저 근데 그..",
          "original_form": "저 근데 그..",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021351.1.6",
          "form": "물에 비치게 사진찍는 곳",
          "original_form": "물에 비치게 사진찍는 곳",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021351.1.7",
          "form": "세번을 가봐도 못찾았어요 ..",
          "original_form": "세번을 가봐도 못찾았어요 ..",
          "speaker_id": "2",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100021351.1.8",
          "form": "아 그 해운대에 있어요",
          "original_form": "아 그 해운대에 있어요",
          "speaker_id": "1",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100021351.1.9",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100021351.1.10",
          "form": "그 두베이인가",
          "original_form": "그 두베이인가",
          "speaker_id": "2",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021351.1.11",
          "form": "뻔쩍뻔쩍한곳..  거기 있다고 해서 찾아갔는데...",
          "original_form": "뻔쩍뻔쩍한곳..  거기 있다고 해서 찾아갔는데...",
          "speaker_id": "2",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021351.1.12",
          "form": "더베이 101. 말씀하시는거죠",
          "original_form": "더베이 101. 말씀하시는거죠",
          "speaker_id": "1",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021351.1.13",
          "form": "아 ㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋ 두베이라했네요 전 멍청입니다,,",
          "original_form": "아 ㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋ 두베이라했네요 전 멍청입니다,,",
          "speaker_id": "2",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021351.1.14",
          "form": "헷갈릴수있죠ㅋㅋㅋ",
          "original_form": "헷갈릴수있죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.16",
          "form": "거기 맞나요 근데?",
          "original_form": "거기 맞나요 근데?",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.17",
          "form": "해운대 바닷가에서 멀지않을텐데 왜 못찾으셨을까ㅠㅠ",
          "original_form": "해운대 바닷가에서 멀지않을텐데 왜 못찾으셨을까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.19",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.20",
          "form": "거기맞아요 근데 부산사람입장에서",
          "original_form": "거기맞아요 근데 부산사람입장에서",
          "speaker_id": "1",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.21",
          "form": "택시기사님들도 못찾으시더라고요",
          "original_form": "택시기사님들도 못찾으시더라고요",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.22",
          "form": "말씀드리면 거기 별로예요...",
          "original_form": "말씀드리면 거기 별로예요...",
          "speaker_id": "1",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.23",
          "form": "ㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱ",
          "original_form": "ㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021351.1.24",
          "form": "사진말고는 아무것도...건질게...",
          "original_form": "사진말고는 아무것도...건질게...",
          "speaker_id": "1",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100021351.1.25",
          "form": "안그래도 거기 물 가져와서 붓고 찍는다면서요,,??",
          "original_form": "안그래도 거기 물 가져와서 붓고 찍는다면서요,,??",
          "speaker_id": "2",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100021351.1.26",
          "form": "그죠...진짜 사진용인데",
          "original_form": "그죠...진짜 사진용인데",
          "speaker_id": "1",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100021351.1.27",
          "form": "그럴꺼면 그냥 빌딩많은곳에 대야놓고 찍는게 좋지않나 하는게",
          "original_form": "그럴꺼면 그냥 빌딩많은곳에 대야놓고 찍는게 좋지않나 하는게",
          "speaker_id": "1",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100021351.1.28",
          "form": "솔직한 부산사람의 입장입니다",
          "original_form": "솔직한 부산사람의 입장입니다",
          "speaker_id": "1",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100021351.1.29",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100021351.1.30",
          "form": "솔직한 부산사람 입장에서 어디 추천하시나요",
          "original_form": "솔직한 부산사람 입장에서 어디 추천하시나요",
          "speaker_id": "2",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100021351.1.31",
          "form": "저는 흰여울길이요",
          "original_form": "저는 흰여울길이요",
          "speaker_id": "1",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100021351.1.32",
          "form": "헐 첨들어봐요",
          "original_form": "헐 첨들어봐요",
          "speaker_id": "2",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100021351.1.33",
          "form": "부산사람으로써 맛집이랑 이런거",
          "original_form": "부산사람으로써 맛집이랑 이런거",
          "speaker_id": "1",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100021351.1.34",
          "form": "추천해드릴까요?",
          "original_form": "추천해드릴까요?",
          "speaker_id": "1",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100021351.1.35",
          "form": "네 완전 좋아요",
          "original_form": "네 완전 좋아요",
          "speaker_id": "2",
          "time": "20210828 18:04"
        }
      ]
    }
