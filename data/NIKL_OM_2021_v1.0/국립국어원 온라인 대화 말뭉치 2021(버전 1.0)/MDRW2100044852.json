      "utterance": [
        {
          "id": "MDRW2100044852.1.1",
          "form": "요즘 보는 프로그램 있으신가요?",
          "original_form": "요즘 보는 프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044852.1.2",
          "form": "저 심야괴담회 좋아해요 ㅋㅋㅋ",
          "original_form": "저 심야괴담회 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044852.1.3",
          "form": "헉 저 그거 못봐요ㅠㅍ",
          "original_form": "헉 저 그거 못봐요ㅠㅍ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044852.1.4",
          "form": "귀신 나오죠?",
          "original_form": "귀신 나오죠?",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044852.1.5",
          "form": "네 나와요 ㅋㅋ섬뜩합니다 ㅋㅋㅋㅋ",
          "original_form": "네 나와요 ㅋㅋ섬뜩합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044852.1.6",
          "form": "무서운거 못보세요?",
          "original_form": "무서운거 못보세요?",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044852.1.7",
          "form": "네 쫄보입니다",
          "original_form": "네 쫄보입니다",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044852.1.8",
          "form": "심야괴담회 다 보시나요...?",
          "original_form": "심야괴담회 다 보시나요...?",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044852.1.9",
          "form": "당연하죠! ㅋㅋㅋ 켜면 다 보는거 아닌가요..?",
          "original_form": "당연하죠! ㅋㅋㅋ 켜면 다 보는거 아닌가요..?",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044852.1.10",
          "form": "대단하시네요....",
          "original_form": "대단하시네요....",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044852.1.11",
          "form": "제 친구도 보긴하는데 가리면서 본대요ㅎㅎㅎ",
          "original_form": "제 친구도 보긴하는데 가리면서 본대요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044852.1.12",
          "form": "아 좀 무섭긴해요 ㅋㅋㅋㅋㅋ 근데 하나도 안무서운편들도 꽤있어요...",
          "original_form": "아 좀 무섭긴해요 ㅋㅋㅋㅋㅋ 근데 하나도 안무서운편들도 꽤있어요...",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044852.1.13",
          "form": "맞아요 신기한 얘기들도 몇개 있더라구요",
          "original_form": "맞아요 신기한 얘기들도 몇개 있더라구요",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044852.1.14",
          "form": "그래도 무서워서 안봐요ㅎㅎ",
          "original_form": "그래도 무서워서 안봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044852.1.15",
          "form": "공포영화본적없으세요?",
          "original_form": "공포영화본적없으세요?",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044852.1.16",
          "form": "보긴봤어요ㅎㅎ",
          "original_form": "보긴봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044852.1.17",
          "form": "눈 가리고...",
          "original_form": "눈 가리고...",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044852.1.18",
          "form": "어떤거보셨어요?",
          "original_form": "어떤거보셨어요?",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044852.1.19",
          "form": "그 군인나오는...",
          "original_form": "그 군인나오는...",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044852.1.20",
          "form": "감우성이랑...",
          "original_form": "감우성이랑...",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044852.1.21",
          "form": "아 아 아 그 뭐더라 ㅋㅋㅋ 외국배경이잖아요아",
          "original_form": "아 아 아 그 뭐더라 ㅋㅋㅋ 외국배경이잖아요아",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044852.1.22",
          "form": "알포인트!",
          "original_form": "알포인트!",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044852.1.23",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044852.1.24",
          "form": "그거 무서운데 귀신 나오는건 별로 없어서 잘봤어요",
          "original_form": "그거 무서운데 귀신 나오는건 별로 없어서 잘봤어요",
          "speaker_id": "1",
          "time": "20210908 00:36"
        }
      ]
    }
