        {
          "id": "MDRW2100000633.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000633.1.2",
          "form": "사실 가본적은 없는데 요즘 확 꽂혔어요ㅋㅋ",
          "original_form": "사실 가본적은 없는데 요즘 확 꽂혔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000633.1.3",
          "form": "혼자 좀 사람없는곳으로 떠나고싶어요",
          "original_form": "혼자 좀 사람없는곳으로 떠나고싶어요",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000633.1.4",
          "form": "저도요ㅋㅋㅋㅋ중학생 때 제주도 간 이후로 여행가본 적이 없네요ㅜ",
          "original_form": "저도요ㅋㅋㅋㅋ중학생 때 제주도 간 이후로 여행가본 적이 없네요ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000633.1.5",
          "form": "헉 제주도 올레길 한바퀴 여행 이런거 딱 알아보고있었는데",
          "original_form": "헉 제주도 올레길 한바퀴 여행 이런거 딱 알아보고있었는데",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000633.1.6",
          "form": "ㅋㅋㅋ제주도 올레길 좋져 자전거 타기에도 좋구요",
          "original_form": "ㅋㅋㅋ제주도 올레길 좋져 자전거 타기에도 좋구요",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000633.1.7",
          "form": "한 일주일정도 자전거타고 돌아다니고싶네요ㅋㅋㅋ",
          "original_form": "한 일주일정도 자전거타고 돌아다니고싶네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000633.1.8",
          "form": "name2님은 여행 별로 관심 없으세요?",
          "original_form": "&name2&님은 여행 별로 관심 없으세요?",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000633.1.9",
          "form": "여행 저도 가고싶은데 여건이 안되네요ㅜ 갈 수 있다면 제주도 한 번 더 가고싶어요ㅋㅋ",
          "original_form": "여행 저도 가고싶은데 여건이 안되네요ㅜ 갈 수 있다면 제주도 한 번 더 가고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000633.1.10",
          "form": "해외여행도!!",
          "original_form": "해외여행도!!",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000633.1.11",
          "form": "코로나만 끝나면 진짜",
          "original_form": "코로나만 끝나면 진짜",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000633.1.12",
          "form": "80일간의 세계일주 찍어버리는데",
          "original_form": "80일간의 세계일주 찍어버리는데",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000633.1.13",
          "form": "오ㅋㅋㅋ저는 일단 국내부터 싹 돌아다닐거같아요ㅋㅋ",
          "original_form": "오ㅋㅋㅋ저는 일단 국내부터 싹 돌아다닐거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000633.1.14",
          "form": "아 갑자기 고민되네요",
          "original_form": "아 갑자기 고민되네요",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000633.1.15",
          "form": "자전거타고 전국일주도 땡기는데",
          "original_form": "자전거타고 전국일주도 땡기는데",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000633.1.16",
          "form": "자전거 타는거 좋아하세요?",
          "original_form": "자전거 타는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000633.1.17",
          "form": "자전거타고 전국일주ㄷㄷ한데요?",
          "original_form": "자전거타고 전국일주ㄷㄷ한데요?",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000633.1.18",
          "form": "자전거타고 장거리는 가본적 없긴한데",
          "original_form": "자전거타고 장거리는 가본적 없긴한데",
