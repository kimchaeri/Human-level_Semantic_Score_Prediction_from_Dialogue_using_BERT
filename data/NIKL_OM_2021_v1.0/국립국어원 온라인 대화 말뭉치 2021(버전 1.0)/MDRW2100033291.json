      "utterance": [
        {
          "id": "MDRW2100033291.1.1",
          "form": "나는 올해 일본가고 싶엉",
          "original_form": "나는 올해 일본가고 싶엉",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100033291.1.2",
          "form": "그리고 다음에 호주도 가서 코알라를 볼거야",
          "original_form": "그리고 다음에 호주도 가서 코알라를 볼거야",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100033291.1.3",
          "form": "대박~~ 호주 좋지",
          "original_form": "대박~~ 호주 좋지",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.4",
          "form": "안가봤지만 히히",
          "original_form": "안가봤지만 히히",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.5",
          "form": "나두 히히",
          "original_form": "나두 히히",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.6",
          "form": "일본을 간다면",
          "original_form": "일본을 간다면",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.7",
          "form": "일단 홋카이도를 가야 하고",
          "original_form": "일단 홋카이도를 가야 하고",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.8",
          "form": "오키나와도 가고 싶어",
          "original_form": "오키나와도 가고 싶어",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.9",
          "form": "극과 극이지",
          "original_form": "극과 극이지",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.10",
          "form": "나는 다음에는 교토도 가보고 싶어",
          "original_form": "나는 다음에는 교토도 가보고 싶어",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.11",
          "form": "오 나두",
          "original_form": "오 나두",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.13",
          "form": "오 교토",
          "original_form": "오 교토",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.15",
          "form": "오키나와는 일본의 수학여행지인것같던데",
          "original_form": "오키나와는 일본의 수학여행지인것같던데",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.16",
          "form": "교토야말로 일본의 정수!",
          "original_form": "교토야말로 일본의 정수!",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033291.1.17",
          "form": "그리고 난 언니를 데리고 나가사키를 가고 싶엉",
          "original_form": "그리고 난 언니를 데리고 나가사키를 가고 싶엉",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033291.1.18",
          "form": "왜녀묜 같이 나가사키짬뽕이랑 카스테라 사먹어야하거등",
          "original_form": "왜녀묜 같이 나가사키짬뽕이랑 카스테라 사먹어야하거등",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033291.1.19",
          "form": "너무 좋다앙",
          "original_form": "너무 좋다앙",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033291.1.20",
          "form": "언닌 name1랑 여행 가는 게 넘 잼써",
          "original_form": "언닌 &name1&랑 여행 가는 게 넘 잼써",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033291.1.21",
          "form": "나가사키 짬뽕은 양배추때문에 좀 달아",
          "original_form": "나가사키 짬뽕은 양배추때문에 좀 달아",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033291.1.22",
          "form": "독특한 맛이얌",
          "original_form": "독특한 맛이얌",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033291.1.23",
          "form": "나두~~",
          "original_form": "나두~~",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033291.1.24",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100033291.1.25",
          "form": "대만도 가야해",
          "original_form": "대만도 가야해",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100033291.1.26",
          "form": "나가사키 짬뽕이 우리가 먹은 그거랑 다른 거야?!",
          "original_form": "나가사키 짬뽕이 우리가 먹은 그거랑 다른 거야?!",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100033291.1.27",
          "form": "충격이구만",
          "original_form": "충격이구만",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100033291.1.28",
          "form": "대만은 가서",
          "original_form": "대만은 가서",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100033291.1.29",
          "form": "펑리수를 잔뜩 사올 거야",
          "original_form": "펑리수를 잔뜩 사올 거야",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100033291.1.30",
          "form": "대만에 가서 에그타르트랑 버블티랑 이것저것 먹어야해",
          "original_form": "대만에 가서 에그타르트랑 버블티랑 이것저것 먹어야해",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100033291.1.31",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100033291.1.32",
          "form": "글구 name1는 과일도 마니 먹어야 해",
          "original_form": "글구 &name1&는 과일도 마니 먹어야 해",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100033291.1.33",
          "form": "마쟝",
          "original_form": "마쟝",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100033291.1.34",
          "form": "나는 현지 음식이 입에 잘 맞으니까 상관 없지만 말야",
          "original_form": "나는 현지 음식이 입에 잘 맞으니까 상관 없지만 말야",
