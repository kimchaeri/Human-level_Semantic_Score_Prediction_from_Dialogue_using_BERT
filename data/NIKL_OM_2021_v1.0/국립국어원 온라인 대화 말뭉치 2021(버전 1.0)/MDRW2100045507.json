      "utterance": [
        {
          "id": "MDRW2100045507.1.1",
          "form": "과일아니고 덩굴 아니고 흰색이고 이파리 없는",
          "original_form": "과일아니고 덩굴 아니고 흰색이고 이파리 없는",
          "speaker_id": "1",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100045507.1.2",
          "form": "식물이라니",
          "original_form": "식물이라니",
          "speaker_id": "1",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100045507.1.3",
          "form": "주로 생으로 먹나요?",
          "original_form": "주로 생으로 먹나요?",
          "speaker_id": "1",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100045507.1.4",
          "form": "아뇨 보통 살짝 데치거나 요리해먹어요",
          "original_form": "아뇨 보통 살짝 데치거나 요리해먹어요",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100045507.1.5",
          "form": "말랑말랑한가요?",
          "original_form": "말랑말랑한가요?",
          "speaker_id": "1",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100045507.1.6",
          "form": "아뇨 굽히면 똑 부러져요",
          "original_form": "아뇨 굽히면 똑 부러져요",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100045507.1.7",
          "form": "으아아아 무슨 야채지",
          "original_form": "으아아아 무슨 야채지",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045507.1.8",
          "form": "혹시 버섯?",
          "original_form": "혹시 버섯?",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045507.1.9",
          "form": "아뇨 버섯은 아니에요!",
          "original_form": "아뇨 버섯은 아니에요!",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045507.1.10",
          "form": "국끓여먹습니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "국끓여먹습니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045507.1.11",
          "form": "아아아아 뭐지",
          "original_form": "아아아아 뭐지",
          "speaker_id": "1",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045507.1.12",
          "form": "국이 핵심이에요!",
          "original_form": "국이 핵심이에요!",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100045507.1.13",
          "form": "국 국 국 국 국...국..",
          "original_form": "국 국 국 국 국...국..",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045507.1.14",
          "form": "제철이 언제인가요?",
          "original_form": "제철이 언제인가요?",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045507.1.15",
          "form": "제철이 딱히 없어요",
          "original_form": "제철이 딱히 없어요",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100045507.1.16",
          "form": "으아아아 국 끓여먹는 하얀게 뭐죠",
          "original_form": "으아아아 국 끓여먹는 하얀게 뭐죠",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100045507.1.17",
          "form": "힌트 하나만 더요!",
          "original_form": "힌트 하나만 더요!",
          "speaker_id": "1",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100045507.1.18",
          "form": "투톤이에요!",
          "original_form": "투톤이에요!",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100045507.1.19",
          "form": "파??????????",
          "original_form": "파??????????",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100045507.1.20",
          "form": "대파요!",
          "original_form": "대파요!",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100045507.1.21",
          "form": "대파맞쥬",
          "original_form": "대파맞쥬",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100045507.1.22",
          "form": "봉투에 담아파는거!",
          "original_form": "봉투에 담아파는거!",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100045507.1.23",
          "form": "그건 파국이잖아여....",
          "original_form": "그건 파국이잖아여....",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100045507.1.24",
          "form": "ㅋㅋㅌㅋㅋㅋ제 정답도 파국으로 흘러가네요",
          "original_form": "ㅋㅋㅌㅋㅋㅋ제 정답도 파국으로 흘러가네요",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100045507.1.25",
          "form": "봉투에 담아 파는 하얗고 뚝 부러지는 것이라니",
          "original_form": "봉투에 담아 파는 하얗고 뚝 부러지는 것이라니",
          "speaker_id": "1",
          "time": "20210908 03:12"
        }
      ]
    }
