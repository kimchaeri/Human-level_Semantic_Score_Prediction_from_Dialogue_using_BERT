      "utterance": [
        {
          "id": "MDRW2100029641.1.1",
          "form": "그래도 게임에서 서로",
          "original_form": "그래도 게임에서 서로",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029641.1.2",
          "form": "부모욕은 안합니다",
          "original_form": "부모욕은 안합니다",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029641.1.3",
          "form": "힘드네요",
          "original_form": "힘드네요",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.4",
          "form": "그건아니되죠",
          "original_form": "그건아니되죠",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.5",
          "form": "토닥토닥",
          "original_form": "토닥토닥",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.6",
          "form": "음성채팅은",
          "original_form": "음성채팅은",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.7",
          "form": "게임에서나마 만나세요",
          "original_form": "게임에서나마 만나세요",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.8",
          "form": "여러모로 잼난거같아요",
          "original_form": "여러모로 잼난거같아요",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.9",
          "form": "정겹게",
          "original_form": "정겹게",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.10",
          "form": "그것도 음성되는거 아셨나묘",
          "original_form": "그것도 음성되는거 아셨나묘",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.11",
          "form": "메타버스로 만납시다",
          "original_form": "메타버스로 만납시다",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.12",
          "form": "아니요 음성안해봐서",
          "original_form": "아니요 음성안해봐서",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.13",
          "form": "그건 유료긴하지많요",
          "original_form": "그건 유료긴하지많요",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.14",
          "form": "잘모릅니다",
          "original_form": "잘모릅니다",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029641.1.15",
          "form": "롤은 음성무료",
          "original_form": "롤은 음성무료",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029641.1.16",
          "form": "캐치마인드는유료",
          "original_form": "캐치마인드는유료",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029641.1.17",
          "form": "카트라이더도 무료",
          "original_form": "카트라이더도 무료",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029641.1.18",
          "form": "한번해보세요",
          "original_form": "한번해보세요",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029641.1.19",
          "form": "디스코드는 휴대폰으로도 깔수 있어오",
          "original_form": "디스코드는 휴대폰으로도 깔수 있어오",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.20",
          "form": "누구랑 대화하죠",
          "original_form": "누구랑 대화하죠",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.21",
          "form": "친구가없어요",
          "original_form": "친구가없어요",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.22",
          "form": "제가해드릴까요",
          "original_form": "제가해드릴까요",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.23",
          "form": "요즘엔 카톡 오픈채팅방에서",
          "original_form": "요즘엔 카톡 오픈채팅방에서",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.24",
          "form": "남친과 코드맞아 부럽군요",
          "original_form": "남친과 코드맞아 부럽군요",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.25",
          "form": "게임들어가서",
          "original_form": "게임들어가서",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.26",
          "form": "같이할수도 있어요",
          "original_form": "같이할수도 있어요",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.27",
          "form": "저 전화목소리 좋습니다",
          "original_form": "저 전화목소리 좋습니다",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.28",
          "form": "익명이니까",
          "original_form": "익명이니까",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029641.1.29",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029641.1.30",
          "form": "디스코드로 많이하죠",
          "original_form": "디스코드로 많이하죠",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029641.1.31",
          "form": "저도 좋습니다",
          "original_form": "저도 좋습니다",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029641.1.32",
          "form": "초딩도 많겠군요",
          "original_form": "초딩도 많겠군요",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029641.1.33",
          "form": "솔음입니다",
          "original_form": "솔음입니다",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029641.1.34",
          "form": "디코 잼납니다",
          "original_form": "디코 잼납니다",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029641.1.35",
          "form": "파티원모집이라고",
          "original_form": "파티원모집이라고",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029641.1.36",
          "form": "생각하세요",
          "original_form": "생각하세요",
          "speaker_id": "2",
          "time": "20210903 11:15"
        }
      ]
    }
