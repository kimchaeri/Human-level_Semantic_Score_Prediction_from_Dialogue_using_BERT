      "utterance": [
        {
          "id": "MDRW2100013753.1.1",
          "form": "시간 날 때 가는 편이에요",
          "original_form": "시간 날 때 가는 편이에요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013753.1.2",
          "form": "카페 좋아하시나요",
          "original_form": "카페 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013753.1.3",
          "form": "넹 저는 좋아해요",
          "original_form": "넹 저는 좋아해요",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013753.1.4",
          "form": "주로 공부할 때 가는 편이에요",
          "original_form": "주로 공부할 때 가는 편이에요",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013753.1.5",
          "form": "가면 주로 뭐 드세요?",
          "original_form": "가면 주로 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013753.1.6",
          "form": "잠 깨려고 아메리카노 자주 먹어요",
          "original_form": "잠 깨려고 아메리카노 자주 먹어요",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013753.1.7",
          "form": "좋아하는 프랜차이즈 있으신가요",
          "original_form": "좋아하는 프랜차이즈 있으신가요",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013753.1.8",
          "form": "넹 저는 이디야 자주 가요",
          "original_form": "넹 저는 이디야 자주 가요",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013753.1.9",
          "form": "이디야 너무 좋죠",
          "original_form": "이디야 너무 좋죠",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013753.1.10",
          "form": "이디야 쿠키 참 맛있어요",
          "original_form": "이디야 쿠키 참 맛있어요",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013753.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013753.1.12",
          "form": "거기 쿠키맛집이더라구요",
          "original_form": "거기 쿠키맛집이더라구요",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013753.1.13",
          "form": "사실 그걸 먹으러가요",
          "original_form": "사실 그걸 먹으러가요",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013753.1.14",
          "form": "커피는 핑계에요",
          "original_form": "커피는 핑계에요",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013753.1.15",
          "form": "저는 거기 허니브레드도 좋아해요ㅎ",
          "original_form": "저는 거기 허니브레드도 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013753.1.16",
          "form": "약간 이디야는 디져트 맛집",
          "original_form": "약간 이디야는 디져트 맛집",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013753.1.17",
          "form": "맞아요 이디야는 디저트가 맛있어요",
          "original_form": "맞아요 이디야는 디저트가 맛있어요",
