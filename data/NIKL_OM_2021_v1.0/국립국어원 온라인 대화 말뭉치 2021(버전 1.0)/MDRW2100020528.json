      "utterance": [
        {
          "id": "MDRW2100020528.1.1",
          "form": "사실 가장 불호인건 맛살인데",
          "original_form": "사실 가장 불호인건 맛살인데",
          "speaker_id": "2",
          "time": "20210828 09:02"
        },
        {
          "id": "MDRW2100020528.1.2",
          "form": "이건 저만 그렇더라구요",
          "original_form": "이건 저만 그렇더라구요",
          "speaker_id": "2",
          "time": "20210828 09:03"
        },
        {
          "id": "MDRW2100020528.1.3",
          "form": "저도 안좋아합니다",
          "original_form": "저도 안좋아합니다",
          "speaker_id": "1",
          "time": "20210828 09:03"
        },
        {
          "id": "MDRW2100020528.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:03"
        },
        {
          "id": "MDRW2100020528.1.5",
          "form": "빼고 먹기 귀찮아서 첨부터 제껀 빼고만들어요",
          "original_form": "빼고 먹기 귀찮아서 첨부터 제껀 빼고만들어요",
          "speaker_id": "2",
          "time": "20210828 09:03"
        },
        {
          "id": "MDRW2100020528.1.6",
          "form": "약간 물컹거리는거 너무싫어서",
          "original_form": "약간 물컹거리는거 너무싫어서",
          "speaker_id": "1",
          "time": "20210828 09:03"
        },
        {
          "id": "MDRW2100020528.1.7",
          "form": "ㅋㅋㅋㅋ저는 반해물파라",
          "original_form": "ㅋㅋㅋㅋ저는 반해물파라",
          "speaker_id": "2",
          "time": "20210828 09:03"
        },
        {
          "id": "MDRW2100020528.1.8",
          "form": "맛살도 용납이 안되더라구요",
          "original_form": "맛살도 용납이 안되더라구요",
          "speaker_id": "2",
          "time": "20210828 09:03"
        },
        {
          "id": "MDRW2100020528.1.9",
          "form": "아 그러고보니 맛살도..해물이네요",
          "original_form": "아 그러고보니 맛살도..해물이네요",
          "speaker_id": "1",
          "time": "20210828 09:04"
        },
        {
          "id": "MDRW2100020528.1.10",
          "form": "그럼 어묵도 안드시겠어요",
          "original_form": "그럼 어묵도 안드시겠어요",
          "speaker_id": "1",
          "time": "20210828 09:04"
        },
        {
          "id": "MDRW2100020528.1.11",
          "form": "그런말하면 안되지만 알러지 있으신거 부럽네요...",
          "original_form": "그런말하면 안되지만 알러지 있으신거 부럽네요...",
          "speaker_id": "2",
          "time": "20210828 09:04"
        },
        {
          "id": "MDRW2100020528.1.12",
          "form": "괜찮습니다",
          "original_form": "괜찮습니다",
          "speaker_id": "1",
          "time": "20210828 09:04"
        },
        {
          "id": "MDRW2100020528.1.13",
          "form": "왜 안먹냐고 물을 때마다",
          "original_form": "왜 안먹냐고 물을 때마다",
          "speaker_id": "2",
          "time": "20210828 09:04"
        },
        {
          "id": "MDRW2100020528.1.14",
          "form": "알러지로 대답하면 사람들이 이해해줄거같아요...",
          "original_form": "알러지로 대답하면 사람들이 이해해줄거같아요...",
          "speaker_id": "2",
          "time": "20210828 09:04"
        },
        {
          "id": "MDRW2100020528.1.15",
          "form": "음 그런질문 싫어요",
          "original_form": "음 그런질문 싫어요",
          "speaker_id": "1",
          "time": "20210828 09:04"
        },
        {
          "id": "MDRW2100020528.1.16",
          "form": "혹시 집에서 먹는걸 강요핫나요",
          "original_form": "혹시 집에서 먹는걸 강요핫나요",
          "speaker_id": "1",
          "time": "20210828 09:04"
        },
        {
          "id": "MDRW2100020528.1.17",
          "form": "하시나요",
          "original_form": "하시나요",
          "speaker_id": "1",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.18",
          "form": "어휴 해물 넣고선",
          "original_form": "어휴 해물 넣고선",
          "speaker_id": "2",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.19",
          "form": "안넣은척 엄청 하십니다",
          "original_form": "안넣은척 엄청 하십니다",
          "speaker_id": "2",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.20",
          "form": "전 개인적으로 안먹는 개인의 취향을 존중해야 한다고 생각하는 편이라",
          "original_form": "전 개인적으로 안먹는 개인의 취향을 존중해야 한다고 생각하는 편이라",
          "speaker_id": "1",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.21",
          "form": "저희집도 그래요",
          "original_form": "저희집도 그래요",
          "speaker_id": "1",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.22",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.23",
          "form": "ㅋㅋㅋㅋ 부모님 다 똑같은가봐요",
          "original_form": "ㅋㅋㅋㅋ 부모님 다 똑같은가봐요",
          "speaker_id": "1",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.24",
          "form": "저 엊그제 김치안먹는다고 혼났습니다",
          "original_form": "저 엊그제 김치안먹는다고 혼났습니다",
          "speaker_id": "1",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.25",
          "form": "저희집만 그런게 아니네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저희집만 그런게 아니네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.26",
          "form": "나이가 몇개인데..",
          "original_form": "나이가 몇개인데..",
          "speaker_id": "1",
          "time": "20210828 09:05"
        },
        {
          "id": "MDRW2100020528.1.27",
          "form": "김치안먹는걸로 혼내는지..",
          "original_form": "김치안먹는걸로 혼내는지..",
          "speaker_id": "1",
          "time": "20210828 09:06"
        },
        {
          "id": "MDRW2100020528.1.28",
          "form": "앗 저도 나이가 몇갠데",
          "original_form": "앗 저도 나이가 몇갠데",
          "speaker_id": "2",
          "time": "20210828 09:06"
        }
      ]
    }
  ]
