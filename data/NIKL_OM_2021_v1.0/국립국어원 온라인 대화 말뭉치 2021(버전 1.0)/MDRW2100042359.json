      "utterance": [
        {
          "id": "MDRW2100042359.1.1",
          "form": "그러니까요 나도 나중에 알 면",
          "original_form": "그러니까요 나도 나중에 알 면",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042359.1.2",
          "form": "속아 주는 척 해야지 했는데",
          "original_form": "속아 주는 척 해야지 했는데",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042359.1.3",
          "form": "바보같이 속아요",
          "original_form": "바보같이 속아요",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042359.1.4",
          "form": "그래 잘 안됨",
          "original_form": "그래 잘 안됨",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042359.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042359.1.6",
          "form": "그러이 진심으로 속는다니까",
          "original_form": "그러이 진심으로 속는다니까",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.7",
          "form": "속아 주는 게 아니고",
          "original_form": "속아 주는 게 아니고",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.8",
          "form": "그러니까요 그게 문제지요",
          "original_form": "그러니까요 그게 문제지요",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.9",
          "form": "이제는 말해주는 사람도 없어서",
          "original_form": "이제는 말해주는 사람도 없어서",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.10",
          "form": "우리가 알 수도 없음",
          "original_form": "우리가 알 수도 없음",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.11",
          "form": "ㅋㅋㅋㅋㄱ  웃기노",
          "original_form": "ㅋㅋㅋㅋㄱ  웃기노",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.12",
          "form": "학교 안에서 뭔 일이 일어나는지",
          "original_form": "학교 안에서 뭔 일이 일어나는지",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.13",
          "form": "그래 선생님들은 진짜",
          "original_form": "그래 선생님들은 진짜",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.14",
          "form": "뭐 애가 밥을 먹던지 말던지",
          "original_form": "뭐 애가 밥을 먹던지 말던지",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.15",
          "form": "우산을 가져가던지 말던지",
          "original_form": "우산을 가져가던지 말던지",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.16",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.17",
          "form": "아 뭐 신경을 안 쓰네",
          "original_form": "아 뭐 신경을 안 쓰네",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.18",
          "form": "시간만 채우고 가면 되니까",
          "original_form": "시간만 채우고 가면 되니까",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.19",
          "form": "어떻게 그렇게 무심할 수가 있지??",
          "original_form": "어떻게 그렇게 무심할 수가 있지??",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042359.1.20",
          "form": "유치원이랑 차이가 너무 심한 거 같다",
          "original_form": "유치원이랑 차이가 너무 심한 거 같다",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.21",
          "form": "그러니까요 녹음기를 넣어놔야 되나",
          "original_form": "그러니까요 녹음기를 넣어놔야 되나",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.22",
          "form": "나도 진짜 그 생각 했다ㅋㅋㅋㄱㅋ",
          "original_form": "나도 진짜 그 생각 했다ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.23",
          "form": "그래도 지금은 name3이 선생님 좋다고 하니까",
          "original_form": "그래도 지금은 &name3&이 선생님 좋다고 하니까",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.24",
          "form": "언니가 살이 보내서 그래요",
          "original_form": "언니가 살이 보내서 그래요",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.25",
          "form": "사립",
          "original_form": "사립",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.26",
          "form": "괜찮은데",
          "original_form": "괜찮은데",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.28",
          "form": "저 국공립 보내고 나면",
          "original_form": "저 국공립 보내고 나면",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.29",
          "form": "내려놓습니다ㅋㅋㅋㅋ",
          "original_form": "내려놓습니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.30",
          "form": "그렇겠더라 정말",
          "original_form": "그렇겠더라 정말",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042359.1.31",
