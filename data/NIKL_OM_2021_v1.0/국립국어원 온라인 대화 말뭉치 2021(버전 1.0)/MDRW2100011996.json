      "utterance": [
        {
          "id": "MDRW2100011996.1.1",
          "form": "복숭아향이 좋은 것 같아요",
          "original_form": "복숭아향이 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011996.1.2",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011996.1.3",
          "form": "너무좋아요",
          "original_form": "너무좋아요",
          "speaker_id": "1",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011996.1.4",
          "form": "저 한 때 복숭아에 미쳐서",
          "original_form": "저 한 때 복숭아에 미쳐서",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011996.1.5",
          "form": "과일향 중에서도 유난히 달콤해요",
          "original_form": "과일향 중에서도 유난히 달콤해요",
          "speaker_id": "2",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011996.1.6",
          "form": "복숭아란 복숭아는 다 모았어요",
          "original_form": "복숭아란 복숭아는 다 모았어요",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011996.1.7",
          "form": "와 정말요!",
          "original_form": "와 정말요!",
          "speaker_id": "2",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011996.1.8",
          "form": "네 정말요 ㅋㅋ 그랬더니 그만 사라고",
          "original_form": "네 정말요 ㅋㅋ 그랬더니 그만 사라고",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011996.1.9",
          "form": "주위에서 뭐리 그래서 멈췄습니다",
          "original_form": "주위에서 뭐리 그래서 멈췄습니다",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011996.1.10",
          "form": "ㅋㅋㅋㅋ인간 복숭아였겠어요!",
          "original_form": "ㅋㅋㅋㅋ인간 복숭아였겠어요!",
          "speaker_id": "2",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011996.1.11",
          "form": "맞아요 그러나 주위사람들은 인정해주지않았죠 (눈물)",
          "original_form": "맞아요 그러나 주위사람들은 인정해주지않았죠 (눈물)",
          "speaker_id": "1",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011996.1.12",
          "form": "복숭아향이 얼마나 좋은데ㅠㅠ",
          "original_form": "복숭아향이 얼마나 좋은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011996.1.13",
          "form": "복숭아 제품 중 만족한게 있으신가요",
          "original_form": "복숭아 제품 중 만족한게 있으신가요",
          "speaker_id": "1",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011996.1.14",
          "form": "저는 사실 제품을 많이 안써봐서 모르겠어요",
          "original_form": "저는 사실 제품을 많이 안써봐서 모르겠어요",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011996.1.15",
          "form": "추천템있나요?",
          "original_form": "추천템있나요?",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011996.1.16",
          "form": "아무래도 가장 유명한 더블유 드레스룸?",
          "original_form": "아무래도 가장 유명한 더블유 드레스룸?",
          "speaker_id": "1",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100011996.1.17",
          "form": "나쁘지않아요",
          "original_form": "나쁘지않아요",
          "speaker_id": "1",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100011996.1.18",
          "form": "기억해둘게요ㅎㅎㅎ",
          "original_form": "기억해둘게요ㅎㅎㅎ",
          "speaker_id": "2",
