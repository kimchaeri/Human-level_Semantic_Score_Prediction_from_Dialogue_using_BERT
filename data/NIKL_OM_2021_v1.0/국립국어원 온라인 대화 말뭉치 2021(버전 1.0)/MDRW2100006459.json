      "utterance": [
        {
          "id": "MDRW2100006459.1.1",
          "form": "안녕 너 봉사해본 적 있니?",
          "original_form": "안녕 너 봉사해본 적 있니?",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100006459.1.2",
          "form": "아 작년에 했었지",
          "original_form": "아 작년에 했었지",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100006459.1.3",
          "form": "무슨 봉사?",
          "original_form": "무슨 봉사?",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006459.1.4",
          "form": "아 원래 의료봉사였는데 코로나 때문에 그냥 코로나 키트 전달하는 걸로 바뀌었어",
          "original_form": "아 원래 의료봉사였는데 코로나 때문에 그냥 코로나 키트 전달하는 걸로 바뀌었어",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006459.1.5",
          "form": "아쉬웠지만 참 뿌듯했단다",
          "original_form": "아쉬웠지만 참 뿌듯했단다",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006459.1.6",
          "form": "넌 해본적있니",
          "original_form": "넌 해본적있니",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006459.1.7",
          "form": "우와 재밌겠다 그런 정보는 어디서 얻어?",
          "original_form": "우와 재밌겠다 그런 정보는 어디서 얻어?",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006459.1.8",
          "form": "나는 봉사라고는 고등학교 도서관 책 정리 봉사?",
          "original_form": "나는 봉사라고는 고등학교 도서관 책 정리 봉사?",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006459.1.9",
          "form": "에타에서도 찾을 수 있고 뭔 사이트 있는데 까먹었단다",
          "original_form": "에타에서도 찾을 수 있고 뭔 사이트 있는데 까먹었단다",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006459.1.10",
          "form": "오 너 사서선생님역할했구나",
          "original_form": "오 너 사서선생님역할했구나",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006459.1.11",
          "form": "사실 도서관가서 책정리하는 봉사하다가",
          "original_form": "사실 도서관가서 책정리하는 봉사하다가",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006459.1.12",
          "form": "너무 못해서 어린이 도서관으로 쫓겨났어",
          "original_form": "너무 못해서 어린이 도서관으로 쫓겨났어",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006459.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 어린이 도서관 ,,,",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 어린이 도서관 ,,,",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006459.1.14",
          "form": "어린이들이 시끄럽게 굴진 않았니?",
          "original_form": "어린이들이 시끄럽게 굴진 않았니?",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006459.1.15",
          "form": "오히려 내가 농땡이 깐 적이 더 많아서..",
          "original_form": "오히려 내가 농땡이 깐 적이 더 많아서..",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006459.1.16",
          "form": "눈치가 더 보이더라고",
          "original_form": "눈치가 더 보이더라고",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006459.1.17",
          "form": "사실 어린이들 솔직히 책보러안오고 친구만나러 오는거라",
          "original_form": "사실 어린이들 솔직히 책보러안오고 친구만나러 오는거라",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006459.1.18",
          "form": "책도 잘 안읽어",
          "original_form": "책도 잘 안읽어",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006459.1.19",
          "form": "그래서 정리할 거도 별로없어서 숨어서 카톡하고 놀았어",
          "original_form": "그래서 정리할 거도 별로없어서 숨어서 카톡하고 놀았어",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006459.1.20",
          "form": "너 그러면 안돼",
          "original_form": "너 그러면 안돼",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006459.1.21",
          "form": "열심히 봉사를 해야지",
          "original_form": "열심히 봉사를 해야지",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006459.1.22",
          "form": "그래야 더 뿌듯한법이라구요",
          "original_form": "그래야 더 뿌듯한법이라구요",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006459.1.23",
          "form": "뿌듯보다는 봉사시간 채우기 바빴는데",
          "original_form": "뿌듯보다는 봉사시간 채우기 바빴는데",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006459.1.24",
          "form": "지금 생각하니까 나쁘네요",
          "original_form": "지금 생각하니까 나쁘네요",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006459.1.25",
          "form": "그래도 요양원 봉사한적 있었는데 그때는 뿌듯하더라고요 ㅎ",
          "original_form": "그래도 요양원 봉사한적 있었는데 그때는 뿌듯하더라고요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006459.1.26",
          "form": "오오 저도 요양원 청소 봉사한 적 있어요",
          "original_form": "오오 저도 요양원 청소 봉사한 적 있어요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006459.1.27",
          "form": "어땠어요",
          "original_form": "어땠어요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006459.1.28",
          "form": "그때 대문 청소하는 것도 하고 전체적인 요양원 청소도 했었는데",
          "original_form": "그때 대문 청소하는 것도 하고 전체적인 요양원 청소도 했었는데",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006459.1.29",
          "form": "대문닦이는 너어어어어무 힘들었어요",
          "original_form": "대문닦이는 너어어어어무 힘들었어요",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006459.1.30",
          "form": "닦기요 오타에요",
          "original_form": "닦기요 오타에요",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006459.1.31",
          "form": "대문닦이가 되셨군요",
          "original_form": "대문닦이가 되셨군요",
          "speaker_id": "2",
          "time": "20210908 00:41"
        }
      ]
