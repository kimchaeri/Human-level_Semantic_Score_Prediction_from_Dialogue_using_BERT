      "utterance": [
        {
          "id": "MDRW2100016288.1.1",
          "form": "ㅎㅎ 저번에 김밥 썬 거 보니 김이 거의 찢겼더만",
          "original_form": "ㅎㅎ 저번에 김밥 썬 거 보니 김이 거의 찢겼더만",
          "speaker_id": "2",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016288.1.2",
          "form": "ㅇㅇ 써는 게 아니라 찢어져",
          "original_form": "ㅇㅇ 써는 게 아니라 찢어져",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016288.1.3",
          "form": "돌 사면 내가 갈아줄께",
          "original_form": "돌 사면 내가 갈아줄께",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016288.1.4",
          "form": "거친거랑 매끈한 거 두개 사야하나?",
          "original_form": "거친거랑 매끈한 거 두개 사야하나?",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016288.1.5",
          "form": "그거 아마 두개가 붙어있을거야 세트로",
          "original_form": "그거 아마 두개가 붙어있을거야 세트로",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016288.1.6",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016288.1.7",
          "form": "ㅇㅇ 앞뒤로 붙어 있는 것도 있고",
          "original_form": "ㅇㅇ 앞뒤로 붙어 있는 것도 있고",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016288.1.8",
          "form": "상품평 좋은 거 찾아봐야겠다",
          "original_form": "상품평 좋은 거 찾아봐야겠다",
          "speaker_id": "1",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016288.1.9",
          "form": "칼 주기적으로 갈아야지 그냥 쓰면 답답하지",
          "original_form": "칼 주기적으로 갈아야지 그냥 쓰면 답답하지",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016288.1.10",
          "form": "ㅇㅇ 야채는 잘 썰리는데 고기같은 건 속터짐 ㅎㅎ",
          "original_form": "ㅇㅇ 야채는 잘 썰리는데 고기같은 건 속터짐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100016288.1.11",
          "form": "그 돌 뭐라고 검색해야 되지?",
          "original_form": "그 돌 뭐라고 검색해야 되지?",
          "speaker_id": "1",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100016288.1.12",
          "form": "그냥 칼갈이라고 치면 될걸?",
          "original_form": "그냥 칼갈이라고 치면 될걸?",
          "speaker_id": "2",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100016288.1.13",
          "form": "아 칼갈이 ㅋㅋ 오키",
          "original_form": "아 칼갈이 ㅋㅋ 오키",
          "speaker_id": "1",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100016288.1.14",
          "form": "종류가 많으니 잘 찾아봐 ㅎㅎ",
          "original_form": "종류가 많으니 잘 찾아봐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100016288.1.15",
