      "utterance": [
        {
          "id": "MDRW2100026668.1.1",
          "form": "미니포레스트",
          "original_form": "미니포레스트",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026668.1.2",
          "form": "님이에요",
          "original_form": "님이에요",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026668.1.3",
          "form": "와 수도꼭지도 만드셨네요",
          "original_form": "와 수도꼭지도 만드셨네요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026668.1.4",
          "form": "디테일이 최고네요",
          "original_form": "디테일이 최고네요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026668.1.5",
          "form": "제작한 미니어쳐 칼과 조리도구로 김장도 하고 김밥도 싸고..ㄴ",
          "original_form": "제작한 미니어쳐 칼과 조리도구로 김장도 하고 김밥도 싸고..ㄴ",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026668.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026668.1.7",
          "form": "김장이요?ㅋㅋ",
          "original_form": "김장이요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026668.1.8",
          "form": "구독자수도 진짜 많네요ㅎㅎ",
          "original_form": "구독자수도 진짜 많네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026668.1.9",
          "form": "네 영상보면 대단해요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 영상보면 대단해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026668.1.10",
          "form": "돈까스도 만드시고요 시식도 해요",
          "original_form": "돈까스도 만드시고요 시식도 해요",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026668.1.11",
          "form": "와 영상미랑 음원이 진짜 멋지네요",
          "original_form": "와 영상미랑 음원이 진짜 멋지네요",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026668.1.12",
          "form": "돈가스를 직접 만드시나요?",
          "original_form": "돈가스를 직접 만드시나요?",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026668.1.13",
          "form": "네 생고기 절여서 화롯불에 기름에 튀겨서요",
          "original_form": "네 생고기 절여서 화롯불에 기름에 튀겨서요",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026668.1.14",
          "form": "거의 요리영상이나 마찬가지예요 본격적으로 하고 계세요 ㅋㅋㄱ",
          "original_form": "거의 요리영상이나 마찬가지예요 본격적으로 하고 계세요 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026668.1.15",
          "form": "요즘 유투브 채널 비슷한게 많은데 이건 진짜 특이하네요!",
          "original_form": "요즘 유투브 채널 비슷한게 많은데 이건 진짜 특이하네요!",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026668.1.16",
          "form": "어떻게 만들었는지 넘 궁금ㅋㅋ",
          "original_form": "어떻게 만들었는지 넘 궁금ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026668.1.17",
          "form": "보시다보면 큐엔에이 영상도 있어요 ㅋㅋㄱ",
          "original_form": "보시다보면 큐엔에이 영상도 있어요 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026668.1.18",
          "form": "귀농후 농번기에 심심해서 시작한게 일이 커졌다고 본거같아용",
          "original_form": "귀농후 농번기에 심심해서 시작한게 일이 커졌다고 본거같아용",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026668.1.19",
          "form": "와 심심해서 했는데 퀄리티가 ㅋㅋㅈ",
          "original_form": "와 심심해서 했는데 퀄리티가 ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026668.1.20",
          "form": "진짜 금손인 사람들 많아요ㅈ부럽",
          "original_form": "진짜 금손인 사람들 많아요ㅈ부럽",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026668.1.21",
          "form": "그렇죠..? 손재주가 있으니까 소일거리로 한게 잘된거 같아요ㅋㅋㅋㅋ",
          "original_form": "그렇죠..? 손재주가 있으니까 소일거리로 한게 잘된거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026668.1.22",
          "form": "저두요 손재주있는분들 부러워요ㅎㅎㅎ",
          "original_form": "저두요 손재주있는분들 부러워요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026668.1.23",
          "form": "친구도 유투브 해보려고 하던데 촬영도 그렇지만 편집이 엄청나더라구요",
          "original_form": "친구도 유투브 해보려고 하던데 촬영도 그렇지만 편집이 엄청나더라구요",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026668.1.24",
          "form": "그런거같아요.. 초창기에 1인으로 하던 사람들도 편집팀이 붙는거보면요..",
          "original_form": "그런거같아요.. 초창기에 1인으로 하던 사람들도 편집팀이 붙는거보면요..",
          "speaker_id": "2",
          "time": "20210901 23:47"
        }
      ]
