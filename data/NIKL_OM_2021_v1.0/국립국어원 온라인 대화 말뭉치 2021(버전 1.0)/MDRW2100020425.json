      "utterance": [
        {
          "id": "MDRW2100020425.1.1",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 05:38"
        },
        {
          "id": "MDRW2100020425.1.2",
          "form": "저는 아직 배달앱을 안깔았어요 ㅎㅎ",
          "original_form": "저는 아직 배달앱을 안깔았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 06:15"
        },
        {
          "id": "MDRW2100020425.1.3",
          "form": "저는 배달앱 3개나 깔았어요 왜 안 까셨어요?",
          "original_form": "저는 배달앱 3개나 깔았어요 왜 안 까셨어요?",
          "speaker_id": "1",
          "time": "20210831 06:56"
        },
        {
          "id": "MDRW2100020425.1.4",
          "form": "헐 3개요 전 깔면 막 시킬까봐서요 ㅎㅎ",
          "original_form": "헐 3개요 전 깔면 막 시킬까봐서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:12"
        },
        {
          "id": "MDRW2100020425.1.5",
          "form": "3개면 배민 요기요 또 뭔가요?",
          "original_form": "3개면 배민 요기요 또 뭔가요?",
          "speaker_id": "2",
          "time": "20210831 07:12"
        },
        {
          "id": "MDRW2100020425.1.6",
          "form": "저는 싼곳 돌아가면서 시켜요 쿠팡잇츠요",
          "original_form": "저는 싼곳 돌아가면서 시켜요 쿠팡잇츠요",
          "speaker_id": "1",
          "time": "20210831 07:12"
        },
        {
          "id": "MDRW2100020425.1.7",
          "form": "아~ 쿠팡도 시작했죠",
          "original_form": "아~ 쿠팡도 시작했죠",
          "speaker_id": "2",
          "time": "20210831 07:16"
        },
        {
          "id": "MDRW2100020425.1.8",
          "form": "그 중에 뭐가 제일 낫나요?",
          "original_form": "그 중에 뭐가 제일 낫나요?",
          "speaker_id": "2",
          "time": "20210831 07:16"
        },
        {
          "id": "MDRW2100020425.1.9",
          "form": "요기요가 할인이 많구요 쿠팡잇츠는 빨라요 배민은 편안한 느낌?ㅎ",
          "original_form": "요기요가 할인이 많구요 쿠팡잇츠는 빨라요 배민은 편안한 느낌?ㅎ",
          "speaker_id": "1",
          "time": "20210831 07:33"
        },
        {
          "id": "MDRW2100020425.1.10",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210831 07:35"
        },
        {
          "id": "MDRW2100020425.1.11",
          "form": "네~배달앱깔면 할인쿠폰도 있고 편해요",
          "original_form": "네~배달앱깔면 할인쿠폰도 있고 편해요",
          "speaker_id": "1",
          "time": "20210831 07:53"
        },
        {
          "id": "MDRW2100020425.1.12",
          "form": "아 그래서 고민중이에이 할인행사 많이 한다더데",
          "original_form": "아 그래서 고민중이에이 할인행사 많이 한다더데",
          "speaker_id": "2",
          "time": "20210831 08:10"
        },
        {
          "id": "MDRW2100020425.1.13",
          "form": "처음엔 10000원 쿠폰줘요~",
          "original_form": "처음엔 10000원 쿠폰줘요~",
          "speaker_id": "1",
          "time": "20210831 08:16"
        },
        {
          "id": "MDRW2100020425.1.14",
          "form": "오~ 솔깃하네요",
          "original_form": "오~ 솔깃하네요",
          "speaker_id": "2",
          "time": "20210831 08:20"
        },
        {
          "id": "MDRW2100020425.1.15",
          "form": "네 한번 해보세요",
          "original_form": "네 한번 해보세요",
          "speaker_id": "1",
          "time": "20210831 08:26"
        },
        {
          "id": "MDRW2100020425.1.16",
          "form": "시작하면 자제 못하고 막 시킬까봐 선뜻 안깔게 되지만",
          "original_form": "시작하면 자제 못하고 막 시킬까봐 선뜻 안깔게 되지만",
          "speaker_id": "2",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100020425.1.17",
          "form": "근데 배달음식 계속 먹으면 지겨워서요ㅜ",
          "original_form": "근데 배달음식 계속 먹으면 지겨워서요ㅜ",
