      "utterance": [
        {
          "id": "MDRW2100007414.1.1",
          "form": "집안일 하시나요",
          "original_form": "집안일 하시나요",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007414.1.2",
          "form": "형제있어요?",
          "original_form": "형제있어요?",
          "speaker_id": "2",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007414.1.3",
          "form": "네네내",
          "original_form": "네네내",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007414.1.4",
          "form": "위에 그 하숙생같은 남자 하나 있는데",
          "original_form": "위에 그 하숙생같은 남자 하나 있는데",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007414.1.5",
          "form": "오빠있으시구나ㅋㅋㅋㅋ",
          "original_form": "오빠있으시구나ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.6",
          "form": "결혼해서 나갔슴다",
          "original_form": "결혼해서 나갔슴다",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.7",
          "form": "너무",
          "original_form": "너무",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.8",
          "form": "새언니가 너무 감사해요",
          "original_form": "새언니가 너무 감사해요",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.9",
          "form": "헉 행복하시겠다",
          "original_form": "헉 행복하시겠다",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.10",
          "form": "저는 행복한데",
          "original_form": "저는 행복한데",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.11",
          "form": "둘은 모르겠어요 관심없어서",
          "original_form": "둘은 모르겠어요 관심없어서",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.12",
          "form": "저는 행복해요",
          "original_form": "저는 행복해요",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.14",
          "form": "그럼 된거져ㅋㅋㅋㅋ",
          "original_form": "그럼 된거져ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.15",
          "form": "저는 남동생있어요",
          "original_form": "저는 남동생있어요",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.16",
          "form": "형제 있으신가요",
          "original_form": "형제 있으신가요",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.17",
          "form": "헙",
          "original_form": "헙",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.18",
          "form": "몇살아래에여?!",
          "original_form": "몇살아래에여?!",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.19",
          "form": "혹시 어릴 때 싸워보셨나요",
          "original_form": "혹시 어릴 때 싸워보셨나요",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.20",
          "form": "4살아래인데",
          "original_form": "4살아래인데",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007414.1.21",
          "form": "저희는 둘 중 하나 사라지기 직전까지",
          "original_form": "저희는 둘 중 하나 사라지기 직전까지",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.22",
          "form": "엄청 맞아는 봤습니다",
          "original_form": "엄청 맞아는 봤습니다",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.23",
          "form": "싸웠어요",
          "original_form": "싸웠어요",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.25",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.26",
          "form": "왜때려",
          "original_form": "왜때려",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.27",
          "form": "아 저는 8살 차이라서",
          "original_form": "아 저는 8살 차이라서",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.28",
          "form": "그 약간",
          "original_form": "그 약간",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.29",
          "form": "아 나이차이 많이 나는구나",
          "original_form": "아 나이차이 많이 나는구나",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.30",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.31",
          "form": "그럼 약간 싸운다기보단",
          "original_form": "그럼 약간 싸운다기보단",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.32",
          "form": "제가 그 부모님을 이름으로 부르는데",
          "original_form": "제가 그 부모님을 이름으로 부르는데",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.33",
          "form": "혼난다는 느낌이겠다",
          "original_form": "혼난다는 느낌이겠다",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.34",
          "form": "오빠는 엄마",
          "original_form": "오빠는 엄마",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.35",
          "form": "아빠",
          "original_form": "아빠",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.36",
          "form": "일케 불러서",
          "original_form": "일케 불러서",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007414.1.37",
          "form": "이름으로 부를때마다 벌서고 그랬어요",
          "original_form": "이름으로 부를때마다 벌서고 그랬어요",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007414.1.38",
          "form": "하지만 지금도 이름으로 불러요",
          "original_form": "하지만 지금도 이름으로 불러요",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007414.1.39",
          "form": "ㅋㅋㅋㅋ 지조있으시네요",
          "original_form": "ㅋㅋㅋㅋ 지조있으시네요",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007414.1.40",
          "form": "이름이 귀.여.우.니.까",
          "original_form": "이름이 귀.여.우.니.까",
          "speaker_id": "1",
