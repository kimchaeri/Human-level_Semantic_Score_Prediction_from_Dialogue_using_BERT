      "utterance": [
        {
          "id": "MDRW2100009581.1.1",
          "form": "대한민국 청소년이라면 한번쯤 해본다는 메이플..",
          "original_form": "대한민국 청소년이라면 한번쯤 해본다는 메이플..",
          "speaker_id": "1",
          "time": "20210823 01:03"
        },
        {
          "id": "MDRW2100009581.1.2",
          "form": "저희",
          "original_form": "저희",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009581.1.3",
          "form": "동년배인가요?",
          "original_form": "동년배인가요?",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009581.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009581.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009581.1.6",
          "form": "제 친구 중에",
          "original_form": "제 친구 중에",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009581.1.7",
          "form": "메이플 전설 있었는데",
          "original_form": "메이플 전설 있었는데",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009581.1.8",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.9",
          "form": "랭킹 안에 있고 막",
          "original_form": "랭킹 안에 있고 막",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.10",
          "form": "탑을찍으셨구나",
          "original_form": "탑을찍으셨구나",
          "speaker_id": "1",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.12",
          "form": "걔가 자기 탑이래서",
          "original_form": "걔가 자기 탑이래서",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.13",
          "form": "허언인가 했는데",
          "original_form": "허언인가 했는데",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.14",
          "form": "진짜였어요",
          "original_form": "진짜였어요",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.15",
          "form": "들어가니까",
          "original_form": "들어가니까",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.16",
          "form": "공지로 사람들이",
          "original_form": "공지로 사람들이",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.17",
          "form": "전당에있던가요",
          "original_form": "전당에있던가요",
          "speaker_id": "1",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.18",
          "form": "걔 채널 어디에 있다고 소리치고",
          "original_form": "걔 채널 어디에 있다고 소리치고",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.19",
          "form": "그랬을 걸요?  아마?",
          "original_form": "그랬을 걸요?  아마?",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.20",
          "form": "와우 메이플 대 스타",
          "original_form": "와우 메이플 대 스타",
          "speaker_id": "1",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.21",
          "form": "사람들 우수수 와서",
          "original_form": "사람들 우수수 와서",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.22",
          "form": "제 컴터 렉 걸리고",
          "original_form": "제 컴터 렉 걸리고",
          "speaker_id": "2",
          "time": "20210823 01:06"
        },
        {
          "id": "MDRW2100009581.1.23",
          "form": "한번에 수백명이",
          "original_form": "한번에 수백명이",
          "speaker_id": "2",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.24",
          "form": "와우 인기스타의 삶이란",
          "original_form": "와우 인기스타의 삶이란",
          "speaker_id": "1",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.25",
          "form": "아이템 하나만 달라고 구걸했어요",
          "original_form": "아이템 하나만 달라고 구걸했어요",
          "speaker_id": "2",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.27",
          "form": "ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.28",
          "form": "결말이..",
          "original_form": "결말이..",
          "speaker_id": "1",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.29",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.30",
          "form": "이것이 네 유명세라면",
          "original_form": "이것이 네 유명세라면",
          "speaker_id": "2",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.31",
          "form": "전혀 부럽지 않다",
          "original_form": "전혀 부럽지 않다",
          "speaker_id": "2",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.32",
          "form": "예상치 못 한 전개",
          "original_form": "예상치 못 한 전개",
          "speaker_id": "1",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.34",
          "form": "구걸이라니 쓰읍..",
          "original_form": "구걸이라니 쓰읍..",
          "speaker_id": "1",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.35",
          "form": "메이플 얼마나 하셨어요?",
          "original_form": "메이플 얼마나 하셨어요?",
          "speaker_id": "2",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009581.1.36",
          "form": "저는 하다 말다 하다 말다 해서ㅋㅋㅋㅋ",
          "original_form": "저는 하다 말다 하다 말다 해서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 01:07"
        }
      ]
    }
