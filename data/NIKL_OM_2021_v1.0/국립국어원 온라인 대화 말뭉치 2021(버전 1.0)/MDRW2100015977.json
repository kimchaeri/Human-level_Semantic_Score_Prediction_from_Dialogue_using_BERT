      "utterance": [
        {
          "id": "MDRW2100015977.1.1",
          "form": "너 사는 지역에 태풍 피해는 없었니?",
          "original_form": "너 사는 지역에 태풍 피해는 없었니?",
          "speaker_id": "2",
          "time": "20210826 04:34"
        },
        {
          "id": "MDRW2100015977.1.2",
          "form": "내가 있던 곳에 태풍영향으로 비가너무많이왔어",
          "original_form": "내가 있던 곳에 태풍영향으로 비가너무많이왔어",
          "speaker_id": "1",
          "time": "20210826 04:34"
        },
        {
          "id": "MDRW2100015977.1.3",
          "form": "이번 태풍 이름이 오마이스 였더라",
          "original_form": "이번 태풍 이름이 오마이스 였더라",
          "speaker_id": "2",
          "time": "20210826 04:34"
        },
        {
          "id": "MDRW2100015977.1.4",
          "form": "부산 지역 뉴스나온거봤어?",
          "original_form": "부산 지역 뉴스나온거봤어?",
          "speaker_id": "1",
          "time": "20210826 04:34"
        },
        {
          "id": "MDRW2100015977.1.5",
          "form": "응 부산 지역은 피해가 극심하더라ㅜㅜ",
          "original_form": "응 부산 지역은 피해가 극심하더라ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 04:35"
        },
        {
          "id": "MDRW2100015977.1.6",
          "form": "차가 잠길 정도더라고",
          "original_form": "차가 잠길 정도더라고",
          "speaker_id": "1",
          "time": "20210826 04:35"
        },
        {
          "id": "MDRW2100015977.1.7",
          "form": "이번 태풍지나니까 가을장마 시작된거래",
          "original_form": "이번 태풍지나니까 가을장마 시작된거래",
          "speaker_id": "2",
          "time": "20210826 04:35"
        },
        {
          "id": "MDRW2100015977.1.8",
          "form": "장마 끝나면 선선하겠구나",
          "original_form": "장마 끝나면 선선하겠구나",
          "speaker_id": "1",
          "time": "20210826 04:35"
        },
        {
          "id": "MDRW2100015977.1.9",
          "form": "맞아 이제 진짜 가을시작이네",
          "original_form": "맞아 이제 진짜 가을시작이네",
          "speaker_id": "2",
          "time": "20210826 04:36"
        },
        {
          "id": "MDRW2100015977.1.10",
          "form": "가을 되면 단풍도 피겠다",
          "original_form": "가을 되면 단풍도 피겠다",
          "speaker_id": "1",
          "time": "20210826 04:37"
        },
        {
          "id": "MDRW2100015977.1.11",
          "form": "나는 가을 날씨가 제일 좋더라",
          "original_form": "나는 가을 날씨가 제일 좋더라",
          "speaker_id": "2",
          "time": "20210826 04:37"
        },
        {
          "id": "MDRW2100015977.1.12",
          "form": "나는 봄이 제일 좋아",
          "original_form": "나는 봄이 제일 좋아",
          "speaker_id": "1",
          "time": "20210826 04:38"
        },
        {
          "id": "MDRW2100015977.1.13",
          "form": "맞아 봄이랑 가을이랑 날씨 좋긴해",
          "original_form": "맞아 봄이랑 가을이랑 날씨 좋긴해",
          "speaker_id": "2",
          "time": "20210826 04:38"
        },
        {
          "id": "MDRW2100015977.1.14",
          "form": "봄이 점점 짧아지는것같아서 아쉬워",
          "original_form": "봄이 점점 짧아지는것같아서 아쉬워",
          "speaker_id": "1",
          "time": "20210826 04:38"
        }
