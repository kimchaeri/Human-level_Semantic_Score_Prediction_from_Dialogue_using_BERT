      "utterance": [
        {
          "id": "MDRW2100025631.1.1",
          "form": "첫 출근은 어땠어요?",
          "original_form": "첫 출근은 어땠어요?",
          "speaker_id": "1",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100025631.1.2",
          "form": "비 맞은상태로 들어가서 그런지 다들 깜짝 놀라던데요",
          "original_form": "비 맞은상태로 들어가서 그런지 다들 깜짝 놀라던데요",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025631.1.3",
          "form": "다들 따뜻하게 맞아 주시던가요?",
          "original_form": "다들 따뜻하게 맞아 주시던가요?",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100025631.1.4",
          "form": "네 제 상태보니깐 어쩔줄몰라하더라구요",
          "original_form": "네 제 상태보니깐 어쩔줄몰라하더라구요",
          "speaker_id": "2",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100025631.1.5",
          "form": "아침부터 힘드셨겠네요",
          "original_form": "아침부터 힘드셨겠네요",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100025631.1.6",
          "form": "칼퇴하셨나요?",
          "original_form": "칼퇴하셨나요?",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100025631.1.7",
          "form": "출근하고밌는중인데 비가안오네요ㅋㄱㅋ",
          "original_form": "출근하고밌는중인데 비가안오네요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210902 07:04"
        },
        {
          "id": "MDRW2100025631.1.8",
          "form": "퇴근할때는 혹시 비올까봐",
          "original_form": "퇴근할때는 혹시 비올까봐",
          "speaker_id": "2",
          "time": "20210902 07:04"
        },
        {
          "id": "MDRW2100025631.1.9",
          "form": "직원이 빌려줬거든요",
          "original_form": "직원이 빌려줬거든요",
          "speaker_id": "2",
          "time": "20210902 07:04"
        },
        {
          "id": "MDRW2100025631.1.10",
          "form": "단단히들고 버스를타고 출근하고있어요",
          "original_form": "단단히들고 버스를타고 출근하고있어요",
          "speaker_id": "2",
          "time": "20210902 07:04"
        },
        {
          "id": "MDRW2100025631.1.11",
          "form": "잊어버리면 큰일나서 꼭꼭 소중하게 들고있어요",
          "original_form": "잊어버리면 큰일나서 꼭꼭 소중하게 들고있어요",
          "speaker_id": "2",
          "time": "20210902 07:04"
        },
        {
          "id": "MDRW2100025631.1.12",
          "form": "꽉잡고있네요",
          "original_form": "꽉잡고있네요",
          "speaker_id": "2",
          "time": "20210902 07:05"
        },
        {
          "id": "MDRW2100025631.1.13",
          "form": "엄청 일찍 출근하시네요",
          "original_form": "엄청 일찍 출근하시네요",
          "speaker_id": "1",
          "time": "20210902 08:16"
        },
        {
          "id": "MDRW2100025631.1.14",
          "form": "힘들지 않으세요?",
          "original_form": "힘들지 않으세요?",
          "speaker_id": "1",
          "time": "20210902 08:16"
        },
        {
          "id": "MDRW2100025631.1.15",
          "form": "힘들어요....ㅠㅠ눈이 계속 감겨요",
          "original_form": "힘들어요....ㅠㅠ눈이 계속 감겨요",
          "speaker_id": "2",
          "time": "20210902 09:19"
        },
        {
          "id": "MDRW2100025631.1.16",
          "form": "견디세요 ㅎㅎㅎㅎ",
          "original_form": "견디세요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100025631.1.17",
          "form": "견뎌야해요",
          "original_form": "견뎌야해요",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100025631.1.18",
          "form": "과자로 버티고있어요ㅜㅜ",
          "original_form": "과자로 버티고있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100025631.1.19",
          "form": "먹는걸로버티기",
          "original_form": "먹는걸로버티기",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100025631.1.20",
          "form": "아직 일을 안주시나요?",
          "original_form": "아직 일을 안주시나요?",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100025631.1.21",
          "form": "교육이라던가",
          "original_form": "교육이라던가",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100025631.1.22",
          "form": "지금 쉬고 있어요....그게 제 유일한 자유시간이에요",
          "original_form": "지금 쉬고 있어요....그게 제 유일한 자유시간이에요",
          "speaker_id": "2",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100025631.1.23",
          "form": "퇴근하기전에 또 1시간쉬고",
          "original_form": "퇴근하기전에 또 1시간쉬고",
          "speaker_id": "2",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100025631.1.24",
          "form": "요즘 구직 어려운데 그래도 직장을 구하셨네요",
          "original_form": "요즘 구직 어려운데 그래도 직장을 구하셨네요",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100025631.1.25",
          "form": "6개월기간제.....",
          "original_form": "6개월기간제.....",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100025631.1.26",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100025631.1.27",
          "form": "그래도 시간 여유를 벌었잖아요",
          "original_form": "그래도 시간 여유를 벌었잖아요",
          "speaker_id": "1",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100025631.1.28",
          "form": "그 사이에 좋은 일자리 찾는거에요",
          "original_form": "그 사이에 좋은 일자리 찾는거에요",
          "speaker_id": "1",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100025631.1.29",
          "form": "6개월끝나면...실업자ㅠㅠ",
          "original_form": "6개월끝나면...실업자ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100025631.1.30",
          "form": "앞으로 좋아질 거에요 지금은 어쨌든 일하니까",
          "original_form": "앞으로 좋아질 거에요 지금은 어쨌든 일하니까",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100025631.1.31",
          "form": "힘내세요",
          "original_form": "힘내세요",
          "speaker_id": "1",
          "time": "20210902 10:50"
        }
      ]
    }
