      "utterance": [
        {
          "id": "MDRW2100042704.1.1",
          "form": "조심해서 나쁠 거 없지",
          "original_form": "조심해서 나쁠 거 없지",
          "speaker_id": "2",
          "time": "20210907 02:13"
        },
        {
          "id": "MDRW2100042704.1.2",
          "form": "새로 옮겨서 실적 챙기려고 할 수도 있고",
          "original_form": "새로 옮겨서 실적 챙기려고 할 수도 있고",
          "speaker_id": "2",
          "time": "20210907 02:13"
        },
        {
          "id": "MDRW2100042704.1.3",
          "form": "ㅇㅇ..아 내친구 엄청 놀라고 속상할듯",
          "original_form": "ㅇㅇ..아 내친구 엄청 놀라고 속상할듯",
          "speaker_id": "1",
          "time": "20210907 02:14"
        },
        {
          "id": "MDRW2100042704.1.4",
          "form": "그렇겠다 진짜",
          "original_form": "그렇겠다 진짜",
          "speaker_id": "2",
          "time": "20210907 02:14"
        },
        {
          "id": "MDRW2100042704.1.5",
          "form": "세계로도 잘하는데",
          "original_form": "세계로도 잘하는데",
          "speaker_id": "2",
          "time": "20210907 02:14"
        },
        {
          "id": "MDRW2100042704.1.6",
          "form": "거기도 한군데 더 가보라고해",
          "original_form": "거기도 한군데 더 가보라고해",
          "speaker_id": "2",
          "time": "20210907 02:14"
        },
        {
          "id": "MDRW2100042704.1.7",
          "form": "안그래도 서울갈거 아님",
          "original_form": "안그래도 서울갈거 아님",
          "speaker_id": "1",
          "time": "20210907 02:15"
        },
        {
          "id": "MDRW2100042704.1.8",
          "form": "한군데라도 더 가보고 결정하라고",
          "original_form": "한군데라도 더 가보고 결정하라고",
          "speaker_id": "1",
          "time": "20210907 02:15"
        },
        {
          "id": "MDRW2100042704.1.9",
          "form": "세계로도 가보라고 했어",
          "original_form": "세계로도 가보라고 했어",
          "speaker_id": "1",
          "time": "20210907 02:15"
        },
        {
          "id": "MDRW2100042704.1.10",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "2",
          "time": "20210907 02:16"
        },
        {
          "id": "MDRW2100042704.1.11",
          "form": "거기 의사가 설명도 잘해주고 괜찮더라",
          "original_form": "거기 의사가 설명도 잘해주고 괜찮더라",
          "speaker_id": "2",
          "time": "20210907 02:16"
        },
        {
          "id": "MDRW2100042704.1.12",
          "form": "과잉진료도 안하고",
          "original_form": "과잉진료도 안하고",
          "speaker_id": "2",
          "time": "20210907 02:16"
        },
        {
          "id": "MDRW2100042704.1.13",
          "form": "ㅇㅇ그 얘기도 해줌",
          "original_form": "ㅇㅇ그 얘기도 해줌",
          "speaker_id": "1",
          "time": "20210907 02:16"
        },
        {
          "id": "MDRW2100042704.1.14",
          "form": "그래서 가본대? 어떻게한대",
          "original_form": "그래서 가본대? 어떻게한대",
          "speaker_id": "2",
          "time": "20210907 02:16"
        },
        {
          "id": "MDRW2100042704.1.15",
          "form": "어머니 모시고 낼 세계로 가본대",
          "original_form": "어머니 모시고 낼 세계로 가본대",
          "speaker_id": "1",
          "time": "20210907 02:17"
        },
        {
          "id": "MDRW2100042704.1.16",
          "form": "그래 가보고 믿음가는데서 치료받아야지",
          "original_form": "그래 가보고 믿음가는데서 치료받아야지",
          "speaker_id": "2",
          "time": "20210907 02:17"
        },
        {
          "id": "MDRW2100042704.1.17",
          "form": "엄마는 아프지마ㅠㅜ",
          "original_form": "엄마는 아프지마ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042704.1.18",
          "form": "니가 속안썩이면 괜찮다",
          "original_form": "니가 속안썩이면 괜찮다",
          "speaker_id": "2",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042704.1.19",
          "form": "ㅡㅡ꼴뵈기시렁",
          "original_form": "ㅡㅡ꼴뵈기시렁",
          "speaker_id": "1",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042704.1.20",
          "form": "내가 속을 언제썩였다고",
          "original_form": "내가 속을 언제썩였다고",
          "speaker_id": "1",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042704.1.21",
          "form": "니 친구 기도 많이 해라",
          "original_form": "니 친구 기도 많이 해라",
          "speaker_id": "2",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042704.1.22",
          "form": "ㅇㅇ아부지도 아프신데 진짜 걱정된다ㅠ",
          "original_form": "ㅇㅇ아부지도 아프신데 진짜 걱정된다ㅠ",
          "speaker_id": "1",
          "time": "20210907 02:20"
        }
