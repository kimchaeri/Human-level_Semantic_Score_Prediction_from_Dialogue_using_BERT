      "utterance": [
        {
          "id": "MDRW2100019775.1.1",
          "form": "게임좋아하세요?",
          "original_form": "게임좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019775.1.2",
          "form": "네 게임 좋아하죠!",
          "original_form": "네 게임 좋아하죠!",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019775.1.3",
          "form": "어떤 게임 하시나요?",
          "original_form": "어떤 게임 하시나요?",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019775.1.4",
          "form": "배틀그라운드 롤 오버워치 이터널 리턴요!",
          "original_form": "배틀그라운드 롤 오버워치 이터널 리턴요!",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019775.1.5",
          "form": "와 영원회귀도 하시는구나",
          "original_form": "와 영원회귀도 하시는구나",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019775.1.6",
          "form": "네넹 ㅎㅎ",
          "original_form": "네넹 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019775.1.7",
          "form": "가끔해요 친구들이랑 할때",
          "original_form": "가끔해요 친구들이랑 할때",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019775.1.8",
          "form": "영원회귀 저는 초반에 잠깐해봤어요",
          "original_form": "영원회귀 저는 초반에 잠깐해봤어요",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019775.1.9",
          "form": "어떤캐릭터 하세요?",
          "original_form": "어떤캐릭터 하세요?",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019775.1.10",
          "form": "저 혜진 리오 아야요!",
          "original_form": "저 혜진 리오 아야요!",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019775.1.11",
          "form": "요즘은 리오만 거의해요",
          "original_form": "요즘은 리오만 거의해요",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019775.1.12",
          "form": "리오는 신캐인가요? 혜진하고 아야는 기억나는데...",
          "original_form": "리오는 신캐인가요? 혜진하고 아야는 기억나는데...",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019775.1.13",
          "form": "네 신캐에요!",
          "original_form": "네 신캐에요!",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019775.1.14",
          "form": "오버워치도 자주하세요?",
          "original_form": "오버워치도 자주하세요?",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019775.1.15",
          "form": "저는 최근까지 했었어요",
          "original_form": "저는 최근까지 했었어요",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019775.1.16",
          "form": "가끔해요!",
          "original_form": "가끔해요!",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019775.1.17",
          "form": "디바 본부케합 200~300시간 정도 하규",
          "original_form": "디바 본부케합 200~300시간 정도 하규",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019775.1.18",
          "form": "요즘은 그냥 재미로 하는편..",
          "original_form": "요즘은 그냥 재미로 하는편..",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019775.1.19",
          "form": "그럼 탱유저신가요?",
          "original_form": "그럼 탱유저신가요?",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019775.1.20",
          "form": "탱힐딜 다하긴하지만",
          "original_form": "탱힐딜 다하긴하지만",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019775.1.21",
          "form": "주로 탱유저긴하죠!",
          "original_form": "주로 탱유저긴하죠!",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019775.1.22",
          "form": "저도 올라운더인데 주로하는건 탱이예요",
          "original_form": "저도 올라운더인데 주로하는건 탱이예요",
          "speaker_id": "1",
          "time": "20210827 22:15"
        }
      ]
