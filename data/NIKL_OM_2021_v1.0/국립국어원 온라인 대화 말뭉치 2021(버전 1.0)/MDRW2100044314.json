      "utterance": [
        {
          "id": "MDRW2100044314.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044314.1.2",
          "form": "거기 티도 맛나잖아요",
          "original_form": "거기 티도 맛나잖아요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044314.1.3",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044314.1.4",
          "form": "가고싶지않나요",
          "original_form": "가고싶지않나요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044314.1.5",
          "form": "마리아쥬 티 먹어보고 반했었죠",
          "original_form": "마리아쥬 티 먹어보고 반했었죠",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044314.1.6",
          "form": "오~ 마리아쥬 뭐요",
          "original_form": "오~ 마리아쥬 뭐요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044314.1.7",
          "form": "근데 해크니 아직도 있나요?? 거기 가본게 몇년전이라 ㅋㅋㅋㅋ",
          "original_form": "근데 해크니 아직도 있나요?? 거기 가본게 몇년전이라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044314.1.8",
          "form": "잇지않을까요 ㅋㅋ",
          "original_form": "잇지않을까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044314.1.9",
          "form": "타조아하심 ?",
          "original_form": "타조아하심 ?",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044314.1.10",
          "form": "네네 티 좋아해요",
          "original_form": "네네 티 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044314.1.11",
          "form": "한때 모으기도 했어요",
          "original_form": "한때 모으기도 했어요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044314.1.12",
          "form": "오 머조아해요",
          "original_form": "오 머조아해요",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044314.1.13",
          "form": "저는 이것저것 가향티 마셔보니.. 결국 그냥 잉블이 젤 난거같아요",
          "original_form": "저는 이것저것 가향티 마셔보니.. 결국 그냥 잉블이 젤 난거같아요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044314.1.14",
          "form": "역시 클래식이죠!",
          "original_form": "역시 클래식이죠!",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044314.1.15",
          "form": "크 클래식 최고죠",
          "original_form": "크 클래식 최고죠",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044314.1.16",
          "form": "요즘 안드세요?",
          "original_form": "요즘 안드세요?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044314.1.17",
          "form": "네.. 요즘은 차 내려먹는것도 귀찮네요",
          "original_form": "네.. 요즘은 차 내려먹는것도 귀찮네요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044314.1.18",
          "form": "밀크티는 어때요",
          "original_form": "밀크티는 어때요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044314.1.19",
          "form": "밀크티도 좋아해요 예전엔 냄비에 끓여먹기도 했어요",
          "original_form": "밀크티도 좋아해요 예전엔 냄비에 끓여먹기도 했어요",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044314.1.20",
          "form": "오 부지런하셧네요",
          "original_form": "오 부지런하셧네요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044314.1.21",
          "form": "냉침은요",
          "original_form": "냉침은요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044314.1.22",
          "form": "냉침도 해봤죠 그건 은은해서 좋아요~",
          "original_form": "냉침도 해봤죠 그건 은은해서 좋아요~",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044314.1.23",
          "form": "같이먹고싶어요",
          "original_form": "같이먹고싶어요",
          "speaker_id": "1",
          "time": "20210907 22:23"
        }
      ]
