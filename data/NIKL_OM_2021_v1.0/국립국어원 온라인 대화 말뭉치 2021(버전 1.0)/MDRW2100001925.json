      "utterance": [
        {
          "id": "MDRW2100001925.1.1",
          "form": "형제가 어떻게되세욘 ?",
          "original_form": "형제가 어떻게되세욘 ?",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001925.1.2",
          "form": "남동생하나있어요",
          "original_form": "남동생하나있어요",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001925.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001925.1.4",
          "form": "아항 저는 언니한명이요 !",
          "original_form": "아항 저는 언니한명이요 !",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001925.1.5",
          "form": "남동생 개구져요? ㅋㅋ",
          "original_form": "남동생 개구져요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001925.1.6",
          "form": "음 조금요?",
          "original_form": "음 조금요?",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001925.1.7",
          "form": "근데 어릴 때 저한테 많이 줘터져서ㅋㅋ",
          "original_form": "근데 어릴 때 저한테 많이 줘터져서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001925.1.8",
          "form": "남동생은 뭔가 그런느낌이있어요",
          "original_form": "남동생은 뭔가 그런느낌이있어요",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001925.1.9",
          "form": "함부로는 못해요",
          "original_form": "함부로는 못해요",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001925.1.10",
          "form": "오빠는 뭔가 츤데레일거같고 ㅋㅋ",
          "original_form": "오빠는 뭔가 츤데레일거같고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001925.1.11",
          "form": "언니는 어때요?",
          "original_form": "언니는 어때요?",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001925.1.12",
          "form": "전 제가 맏이라서",
          "original_form": "전 제가 맏이라서",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001925.1.13",
          "form": "언니가 궁금해요",
          "original_form": "언니가 궁금해요",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001925.1.14",
          "form": "아아 저는 언니랑 6살차이나서 잘놀지않아서 안친햇는데",
          "original_form": "아아 저는 언니랑 6살차이나서 잘놀지않아서 안친햇는데",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001925.1.15",
          "form": "나이드니깐 그냥 편하고 좋더라구요 ㅎㅎ",
          "original_form": "나이드니깐 그냥 편하고 좋더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001925.1.16",
          "form": "가족이 그런가바여",
          "original_form": "가족이 그런가바여",
          "speaker_id": "1",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001925.1.17",
          "form": "의지가 많이 되겠어오",
          "original_form": "의지가 많이 되겠어오",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001925.1.18",
          "form": "특히 동성형제라 공유할 것도 많을 것 같구",
          "original_form": "특히 동성형제라 공유할 것도 많을 것 같구",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001925.1.19",
          "form": "남동생이랑은 그런게 없어서",
          "original_form": "남동생이랑은 그런게 없어서",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001925.1.20",
          "form": "그래두 힘들땐 의지가 될거예요 !",
          "original_form": "그래두 힘들땐 의지가 될거예요 !",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001925.1.21",
          "form": "가족분들과 같이사세용?",
          "original_form": "가족분들과 같이사세용?",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001925.1.22",
          "form": "네 저는 본가에 살아요",
          "original_form": "네 저는 본가에 살아요",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001925.1.23",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001925.1.24",
          "form": "저두요 아직은 ㅎㅎ 독립안하고싶으세영 ?",
          "original_form": "저두요 아직은 ㅎㅎ 독립안하고싶으세영 ?",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001925.1.25",
          "form": "자취를 한 번했었는데.. 음..ㅎㅎ 돈이 안모여서 다시 들어왔어요",
          "original_form": "자취를 한 번했었는데.. 음..ㅎㅎ 돈이 안모여서 다시 들어왔어요",
          "speaker_id": "2",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001925.1.26",
          "form": "독립하면 편하고 좋긴 한데.. 제 경제사정이 뒷받침이 될 때 해야 할 것 같더라구요",
          "original_form": "독립하면 편하고 좋긴 한데.. 제 경제사정이 뒷받침이 될 때 해야 할 것 같더라구요",
