        },
        {
          "id": "MDRW2100045417.1.2",
          "form": "김명민이 오랜만에 나와서",
          "original_form": "김명민이 오랜만에 나와서",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045417.1.3",
          "form": "역시 연기파 배우",
          "original_form": "역시 연기파 배우",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045417.1.4",
          "form": "그러게 그리고 늙지도 않는다",
          "original_form": "그러게 그리고 늙지도 않는다",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045417.1.5",
          "form": "엉 똑같다 정말로",
          "original_form": "엉 똑같다 정말로",
          "speaker_id": "2",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045417.1.6",
          "form": "김범도 이제 연기 잘하고",
          "original_form": "김범도 이제 연기 잘하고",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045417.1.7",
          "form": "경력이 얼만데",
          "original_form": "경력이 얼만데",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045417.1.8",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045417.1.9",
          "form": "마지막 보니 시즌제는 안할듯",
          "original_form": "마지막 보니 시즌제는 안할듯",
          "speaker_id": "2",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045417.1.10",
          "form": "모르지 시청자가 요구해서 또 할듯",
          "original_form": "모르지 시청자가 요구해서 또 할듯",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045417.1.11",
          "form": "김명민 교수로 나오니 찰떡궁합",
          "original_form": "김명민 교수로 나오니 찰떡궁합",
          "speaker_id": "2",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045417.1.12",
          "form": "매회 반전이 있어서",
          "original_form": "매회 반전이 있어서",
