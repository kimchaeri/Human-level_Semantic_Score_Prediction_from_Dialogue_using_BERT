      "utterance": [
        {
          "id": "MDRW2100033789.1.1",
          "form": "ㅋㅋㅋ안자나ㅋㅋ",
          "original_form": "ㅋㅋㅋ안자나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100033789.1.2",
          "form": "오늘 늦네ㅋ",
          "original_form": "오늘 늦네ㅋ",
          "speaker_id": "2",
          "time": "20210825 01:48"
        },
        {
          "id": "MDRW2100033789.1.3",
          "form": "이제 자야지 ㅋㅋ 니도 중간에 깨서 ㅋㅋ",
          "original_form": "이제 자야지 ㅋㅋ 니도 중간에 깨서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:48"
        },
        {
          "id": "MDRW2100033789.1.4",
          "form": "타임 놓쳤네 ㅋ 오빠는 자나?",
          "original_form": "타임 놓쳤네 ㅋ 오빠는 자나?",
          "speaker_id": "1",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033789.1.5",
          "form": "응 오빠도 아직안자",
          "original_form": "응 오빠도 아직안자",
          "speaker_id": "2",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033789.1.6",
          "form": "이제 자야지ㅋㅋ",
          "original_form": "이제 자야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033789.1.7",
          "form": "와 오늘은 다 늦게 응응 어여자 낼 보장",
          "original_form": "와 오늘은 다 늦게 응응 어여자 낼 보장",
          "speaker_id": "1",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100033789.1.8",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "2",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100033789.1.9",
          "form": "잘자잉ㅋ",
          "original_form": "잘자잉ㅋ",
          "speaker_id": "2",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100033789.1.10",
          "form": "응응 너무 굿밤",
          "original_form": "응응 너무 굿밤",
          "speaker_id": "1",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100033789.1.11",
          "form": "너두",
          "original_form": "너두",
          "speaker_id": "1",
          "time": "20210825 01:51"
        },
        {
          "id": "MDRW2100033789.1.12",
          "form": "아침부터 머리감고 화장실 청소 싹하고나옴ㅋ",
          "original_form": "아침부터 머리감고 화장실 청소 싹하고나옴ㅋ",
          "speaker_id": "2",
          "time": "20210825 08:16"
        },
        {
          "id": "MDRW2100033789.1.13",
          "form": "카톡송금이벤트는 못하겠다. 간단하게 하는건데도 눈치주는느낌.",
          "original_form": "카톡송금이벤트는 못하겠다. 간단하게 하는건데도 눈치주는느낌.",
          "speaker_id": "2",
          "time": "20210825 10:15"
        },
        {
          "id": "MDRW2100033789.1.14",
          "form": "짜증남.",
          "original_form": "짜증남.",
          "speaker_id": "2",
          "time": "20210825 10:16"
        },
        {
          "id": "MDRW2100033789.1.15",
          "form": "빨리 내폰도. 이칸다.",
          "original_form": "빨리 내폰도. 이칸다.",
          "speaker_id": "2",
          "time": "20210825 10:16"
        },
        {
          "id": "MDRW2100033789.1.16",
          "form": "아효",
          "original_form": "아효",
          "speaker_id": "2",
          "time": "20210825 10:16"
        },
        {
          "id": "MDRW2100033789.1.17",
          "form": "같이하면 빠른데",
          "original_form": "같이하면 빠른데",
          "speaker_id": "1",
          "time": "20210825 10:53"
        },
        {
          "id": "MDRW2100033789.1.18",
          "form": "안도와주나? 나도혼자하긴 해 오빠잘때",
          "original_form": "안도와주나? 나도혼자하긴 해 오빠잘때",
          "speaker_id": "1",
          "time": "20210825 10:53"
        },
        {
          "id": "MDRW2100033789.1.19",
          "form": "ㅋㅋㅋ 오빠가 늦게자서 ㅋ",
          "original_form": "ㅋㅋㅋ 오빠가 늦게자서 ㅋ",
          "speaker_id": "2",
          "time": "20210825 11:22"
        },
        {
          "id": "MDRW2100033789.1.20",
          "form": "옆에서 계속 폰기다림ㅋㅋ",
          "original_form": "옆에서 계속 폰기다림ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:22"
        },
        {
          "id": "MDRW2100033789.1.21",
          "form": "윽시 늦게 잤나부네ㅎㅎ",
          "original_form": "윽시 늦게 잤나부네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100033789.1.22",
          "form": "쿠팡와써?",
          "original_form": "쿠팡와써?",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100033789.1.23",
          "form": "아직ㅋㅋㅋ기다리는중",
          "original_form": "아직ㅋㅋㅋ기다리는중",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100033789.1.24",
          "form": "밥은 ?ㅋ",
          "original_form": "밥은 ?ㅋ",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100033789.1.25",
          "form": "우와 된다,.된다.. ㅋㅋ신기하다 ㅋㅋㅋ",
          "original_form": "우와 된다,.된다.. ㅋㅋ신기하다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100033789.1.26",
          "form": "오 좋다 좋다 ㅋㅋㅋ",
          "original_form": "오 좋다 좋다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033789.1.27",
          "form": "밥 이제 먹으려고 ㅋㅋ",
          "original_form": "밥 이제 먹으려고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033789.1.28",
          "form": "넌 먹었어?",
          "original_form": "넌 먹었어?",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033789.1.29",
          "form": "히히히 밥이늦네ㅎ",
          "original_form": "히히히 밥이늦네ㅎ",
          "speaker_id": "1",
          "time": "20210825 14:56"
        }
      ]
