      "utterance": [
        {
          "id": "MDRW2100034617.1.1",
          "form": "컴백이라고 노래 많이 부리네",
          "original_form": "컴백이라고 노래 많이 부리네",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034617.1.2",
          "form": "그런갑다. 안대는 그대로 했네",
          "original_form": "그런갑다. 안대는 그대로 했네",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034617.1.3",
          "form": "눈병났나",
          "original_form": "눈병났나",
          "speaker_id": "2",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100034617.1.4",
          "form": "ㅋㅋㅋ 컨셉으로 했겠지",
          "original_form": "ㅋㅋㅋ 컨셉으로 했겠지",
          "speaker_id": "1",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100034617.1.5",
          "form": "ㅋㅋ춤은 잘 추네 다들",
          "original_form": "ㅋㅋ춤은 잘 추네 다들",
          "speaker_id": "2",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100034617.1.6",
          "form": "밥만 먹고 춤만 추니깐",
          "original_form": "밥만 먹고 춤만 추니깐",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100034617.1.7",
          "form": "가수가 노래를 기본으로 잘해야하는데 우리나라는 보여주는 퍼포먼스 위주",
          "original_form": "가수가 노래를 기본으로 잘해야하는데 우리나라는 보여주는 퍼포먼스 위주",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100034617.1.8",
          "form": "머야 벌써 끝?",
          "original_form": "머야 벌써 끝?",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034617.1.9",
          "form": "레드벨벳 나왔다",
          "original_form": "레드벨벳 나왔다",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034617.1.10",
          "form": "집계어쩌고 하던데",
          "original_form": "집계어쩌고 하던데",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034617.1.11",
          "form": "1위가 레드벨벳?",
          "original_form": "1위가 레드벨벳?",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100034617.1.12",
          "form": "1위발표를 못봄",
          "original_form": "1위발표를 못봄",
          "speaker_id": "2",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100034617.1.13",
          "form": "나혼산 재방 하네",
          "original_form": "나혼산 재방 하네",
          "speaker_id": "1",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100034617.1.14",
          "form": "어제꺼 재방인갑네",
          "original_form": "어제꺼 재방인갑네",
