      "utterance": [
        {
          "id": "MDRW2100019734.1.1",
          "form": "최근 본 책 있나요",
          "original_form": "최근 본 책 있나요",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019734.1.2",
          "form": "책 많이조시나요?",
          "original_form": "책 많이조시나요?",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019734.1.3",
          "form": "한강 채식주의자 최근에 봤어요",
          "original_form": "한강 채식주의자 최근에 봤어요",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019734.1.4",
          "form": "사피엔스가 가장 기억남네유..",
          "original_form": "사피엔스가 가장 기억남네유..",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019734.1.5",
          "form": "채식주의자가 한강 살아요?",
          "original_form": "채식주의자가 한강 살아요?",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019734.1.6",
          "form": "작가이름이 한강이에요",
          "original_form": "작가이름이 한강이에요",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019734.1.7",
          "form": "아 사피엔스 전 못 봤어요",
          "original_form": "아 사피엔스 전 못 봤어요",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019734.1.8",
          "form": "아.. 저 무식한거 들켯네..",
          "original_form": "아.. 저 무식한거 들켯네..",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019734.1.9",
          "form": "채식주의자는 소설이에여?",
          "original_form": "채식주의자는 소설이에여?",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019734.1.10",
          "form": "네네 소설이에요",
          "original_form": "네네 소설이에요",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019734.1.11",
          "form": "사피엔스 보시는데 뭐가 무식해요ㅠㅠ 똑똑이이십니다",
          "original_form": "사피엔스 보시는데 뭐가 무식해요ㅠㅠ 똑똑이이십니다",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019734.1.12",
          "form": "문학상 받았대서 봤는데 제스탈 아니에요ㅠ",
          "original_form": "문학상 받았대서 봤는데 제스탈 아니에요ㅠ",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019734.1.13",
          "form": "제목이 먼가...",
          "original_form": "제목이 먼가...",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019734.1.14",
          "form": "풀먹으라는 내용이에요?",
          "original_form": "풀먹으라는 내용이에요?",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019734.1.15",
          "form": "아내가 채식주의자가 되면서 말라가는 이야기에요",
          "original_form": "아내가 채식주의자가 되면서 말라가는 이야기에요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019734.1.16",
          "form": "기괴해요ㅠ",
          "original_form": "기괴해요ㅠ",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019734.1.17",
          "form": "아.. 제목이랑 완전 딴 내용이네요.. 상상도 못함..",
          "original_form": "아.. 제목이랑 완전 딴 내용이네요.. 상상도 못함..",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019734.1.18",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019734.1.19",
          "form": "상받지않았다면 안봤을거엥6",
          "original_form": "상받지않았다면 안봤을거엥6",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019734.1.20",
          "form": "그래도 글은 잘쓰시는 분인가봐요?",
          "original_form": "그래도 글은 잘쓰시는 분인가봐요?",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019734.1.21",
          "form": "네네 문장력은 좋아요",
          "original_form": "네네 문장력은 좋아요",
