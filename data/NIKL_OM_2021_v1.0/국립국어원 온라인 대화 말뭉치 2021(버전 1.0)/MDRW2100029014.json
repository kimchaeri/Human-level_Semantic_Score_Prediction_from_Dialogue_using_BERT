      "utterance": [
        {
          "id": "MDRW2100029014.1.1",
          "form": "아 호불호 강한 음식들의 향연",
          "original_form": "아 호불호 강한 음식들의 향연",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100029014.1.2",
          "form": "구운 과일을 안..좋아한...",
          "original_form": "구운 과일을 안..좋아한...",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029014.1.3",
          "form": "ㅋㅋㅋ ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029014.1.4",
          "form": "저도 싫어해료^^!!!!!",
          "original_form": "저도 싫어해료^^!!!!!",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029014.1.5",
          "form": "어유  파인애플을 왜따뜻하게먹는지.",
          "original_form": "어유  파인애플을 왜따뜻하게먹는지.",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029014.1.6",
          "form": "다행이다^^!",
          "original_form": "다행이다^^!",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029014.1.7",
          "form": "저는 과일에 소스 묻은 것도 안좋아해서",
          "original_form": "저는 과일에 소스 묻은 것도 안좋아해서",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029014.1.8",
          "form": "처음에 파인애플 피자 먹고 충격이었어요",
          "original_form": "처음에 파인애플 피자 먹고 충격이었어요",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029014.1.9",
          "form": "하 진짜",
          "original_form": "하 진짜",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029014.1.10",
          "form": "왜 그 상콤달콤한 과일을 달달....하게 먹는걸까요",
          "original_form": "왜 그 상콤달콤한 과일을 달달....하게 먹는걸까요",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029014.1.11",
          "form": "탕후루도 불호였어요ㅠ",
          "original_form": "탕후루도 불호였어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029014.1.12",
          "form": "맛없는 딸기에 설탕만 한가득 ㅠㅠ",
          "original_form": "맛없는 딸기에 설탕만 한가득 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029014.1.13",
          "form": "저는 과일은 과일 본연의 맛을 지켜야한다고 생각해요",
          "original_form": "저는 과일은 과일 본연의 맛을 지켜야한다고 생각해요",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029014.1.14",
          "form": "왜 가공을 하는걸까요.",
          "original_form": "왜 가공을 하는걸까요.",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029014.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029014.1.16",
          "form": "같은 이유로 과일들어간 디저트도 안먹는",
          "original_form": "같은 이유로 과일들어간 디저트도 안먹는",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029014.1.17",
          "form": "딸기 너무 좋아하는데",
          "original_form": "딸기 너무 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029014.1.18",
          "form": "딸기맛 어쩌구 들을 정말 싫어해요",
          "original_form": "딸기맛 어쩌구 들을 정말 싫어해요",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029014.1.19",
          "form": "저는 과일 생크림 케이크도 비슷한 이유로...",
          "original_form": "저는 과일 생크림 케이크도 비슷한 이유로...",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029014.1.20",
          "form": "딸기라떼는 극과극이더라고요 맛있는 곳과 정말 맛없는 곳",
          "original_form": "딸기라떼는 극과극이더라고요 맛있는 곳과 정말 맛없는 곳",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029014.1.21",
          "form": "생크림이 첨가되어서 과일 맛이!!",
          "original_form": "생크림이 첨가되어서 과일 맛이!!",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029014.1.22",
          "form": "전 생크림이 싫어요ㅠ",
          "original_form": "전 생크림이 싫어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029014.1.23",
          "form": "안좋아!",
          "original_form": "안좋아!",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029014.1.24",
          "form": "헉 저두요",
          "original_form": "헉 저두요",
          "speaker_id": "2",
          "time": "20210902 23:24"
        }
      ]
