      "utterance": [
        {
          "id": "MDRW2100023560.1.1",
          "form": "요즘 뭐 산거 있어요",
          "original_form": "요즘 뭐 산거 있어요",
          "speaker_id": "1",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100023560.1.2",
          "form": "저는 어제 켈로그 시리얼을 샀어요",
          "original_form": "저는 어제 켈로그 시리얼을 샀어요",
          "speaker_id": "2",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100023560.1.3",
          "form": "",
          "original_form": "{censored}",
          "speaker_id": "2",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100023560.1.4",
          "form": "맛있어요?",
          "original_form": "맛있어요?",
          "speaker_id": "1",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023560.1.5",
          "form": "당연하죠 그리고",
          "original_form": "당연하죠 그리고",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023560.1.6",
          "form": "엄청 달진 않아서",
          "original_form": "엄청 달진 않아서",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023560.1.7",
          "form": "죄책감이 덜해요,,,",
          "original_form": "죄책감이 덜해요,,,",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023560.1.8",
          "form": "전 방금 탄산수 1박스를 샀어요",
          "original_form": "전 방금 탄산수 1박스를 샀어요",
          "speaker_id": "1",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100023560.1.9",
          "form": "탄산수?!?!",
          "original_form": "탄산수?!?!",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023560.1.10",
          "form": "인터넷으로 시키신건가요??",
          "original_form": "인터넷으로 시키신건가요??",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023560.1.11",
          "form": "마트가서 한두병씩 사느니 아예 1박스를 산거죠",
          "original_form": "마트가서 한두병씩 사느니 아예 1박스를 산거죠",
          "speaker_id": "1",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023560.1.12",
          "form": "네, 인쇼로",
          "original_form": "네, 인쇼로",
          "speaker_id": "1",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023560.1.13",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023560.1.14",
          "form": "탄산수도  먹어도 죄책감이 없죠",
          "original_form": "탄산수도  먹어도 죄책감이 없죠",
          "speaker_id": "1",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023560.1.15",
          "form": "네이버페이 오늘",
          "original_form": "네이버페이 오늘",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023560.1.16",
          "form": "행사한대서",
          "original_form": "행사한대서",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023560.1.17",
          "form": "사신건가요?!",
          "original_form": "사신건가요?!",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100023560.1.18",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023560.1.19",
          "form": "잘 사셨네요!!!",
          "original_form": "잘 사셨네요!!!",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023560.1.20",
          "form": "덧붙여 장바구니에 넣어둔 자켓도 하나 질렀어요",
          "original_form": "덧붙여 장바구니에 넣어둔 자켓도 하나 질렀어요",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023560.1.21",
          "form": "자켓 오면 보여주세요",
          "original_form": "자켓 오면 보여주세요",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023560.1.22",
          "form": "기대할게요!!!",
          "original_form": "기대할게요!!!",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023560.1.23",
          "form": "한번 입고 올게요",
          "original_form": "한번 입고 올게요",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023560.1.24",
          "form": "저는 맨날 아디다스 트레이닝복,,,",
          "original_form": "저는 맨날 아디다스 트레이닝복,,,",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023560.1.25",
          "form": "옷괜찮으면 아마 출장가서 입을듯",
          "original_form": "옷괜찮으면 아마 출장가서 입을듯",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100023560.1.26",
          "form": "오~~~ 저는 그런 언니의 모습을",
          "original_form": "오~~~ 저는 그런 언니의 모습을",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023560.1.27",
          "form": "zoom으로",
          "original_form": "zoom으로",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023560.1.28",
          "form": "감상하겠죠?!",
          "original_form": "감상하겠죠?!{emoji:☆}",
          "speaker_id": "2",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023560.1.29",
          "form": "ㅋㅋㅋㅋ 화면발이 잘 받아야 할텐데",
          "original_form": "ㅋㅋㅋㅋ 화면발이 잘 받아야 할텐데",
          "speaker_id": "1",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023560.1.30",
          "form": "그러고보니까 화장품도 사야하나 싶어졌단 말이죠",
          "original_form": "그러고보니까 화장품도 사야하나 싶어졌단 말이죠",
          "speaker_id": "1",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100023560.1.31",
          "form": "무슨 화장품 사고싶으세요?!",
          "original_form": "무슨 화장품 사고싶으세요?!",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023560.1.32",
          "form": "로션만 바르고 산지 어언 5년?",
          "original_form": "로션만 바르고 산지 어언 5년?",
          "speaker_id": "1",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023560.1.33",
          "form": "오오 궁금해지네요",
          "original_form": "오오 궁금해지네요",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023560.1.34",
          "form": "언니 선크림도 잘 안바르지 않으세요?!",
          "original_form": "언니 선크림도 잘 안바르지 않으세요?!",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023560.1.35",
          "form": "오~~~!!!",
          "original_form": "오~~~!!!",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023560.1.36",
          "form": "걍 무난하게 쓸만한 섀도우팔레트랑 파데",
          "original_form": "걍 무난하게 쓸만한 섀도우팔레트랑 파데",
          "speaker_id": "1",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023560.1.37",
          "form": "오~! 완전",
          "original_form": "오~! 완전",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023560.1.38",
          "form": "콤팩트한 조합이네요!",
          "original_form": "콤팩트한 조합이네요!",
          "speaker_id": "2",
          "time": "20210830 11:40"
        },
        {
          "id": "MDRW2100023560.1.39",
          "form": "요즘은 메이크업도 미니멀리즘이어서",
          "original_form": "요즘은 메이크업도 미니멀리즘이어서",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023560.1.40",
          "form": "간단하게만 하고 다니더라구요!",
          "original_form": "간단하게만 하고 다니더라구요!",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023560.1.41",
          "form": "그쵸, 어차피 마스크쓰고 할거면 눈만 좀 하려고요",
          "original_form": "그쵸, 어차피 마스크쓰고 할거면 눈만 좀 하려고요",
          "speaker_id": "1",
          "time": "20210830 11:41"
        }
