      "utterance": [
        {
          "id": "MDRW2100016769.1.1",
          "form": "저녁에 자장면 먹었더니 넘 배부르다 ㅎㅎ",
          "original_form": "저녁에 자장면 먹었더니 넘 배부르다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100016769.1.2",
          "form": "나도 ㅎㅎ 너무 배불러",
          "original_form": "나도 ㅎㅎ 너무 배불러",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100016769.1.3",
          "form": "오랜만에 바깥음식 먹었더니 끝도 없이 들어가네 ㅋㅋ",
          "original_form": "오랜만에 바깥음식 먹었더니 끝도 없이 들어가네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100016769.1.4",
          "form": "그동안 너무 건강식만 먹었지?",
          "original_form": "그동안 너무 건강식만 먹었지?",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100016769.1.5",
          "form": "ㅇㅇ 자장면이 얼마나 먹고 싶던지 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 자장면이 얼마나 먹고 싶던지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100016769.1.6",
          "form": "ㅎㅎㅎ 나도 오늘 다이어트 결심이 무나져 내렸어 ㅠ",
          "original_form": "ㅎㅎㅎ 나도 오늘 다이어트 결심이 무나져 내렸어 ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100016769.1.7",
          "form": "여기 자장면 왜케 맛있는거야",
          "original_form": "여기 자장면 왜케 맛있는거야",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100016769.1.8",
          "form": "곱배기 시키면 양념 따로 주는 것도 너무 좋아 ㅎㅎ",
          "original_form": "곱배기 시키면 양념 따로 주는 것도 너무 좋아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100016769.1.9",
          "form": "맞아 진정한 곱배기~ 면도 두 배 양념도 두 배",
          "original_form": "맞아 진정한 곱배기~ 면도 두 배 양념도 두 배",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100016769.1.10",
          "form": "양이 다른데랑은 비교도 안돼",
          "original_form": "양이 다른데랑은 비교도 안돼",
          "speaker_id": "2",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100016769.1.11",
          "form": "맞아 ㅎㅎ 여기 먹다가 다른데 못먹음",
          "original_form": "맞아 ㅎㅎ 여기 먹다가 다른데 못먹음",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100016769.1.12",
          "form": "간만에 배불리 먹었네 ㅋㅋ",
          "original_form": "간만에 배불리 먹었네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100016769.1.13",
          "form": "그동안 냉장고 파먹기만 했더니 너무 먹고 싶었나봐 ㅎㅎ",
          "original_form": "그동안 냉장고 파먹기만 했더니 너무 먹고 싶었나봐 ㅎㅎ",
