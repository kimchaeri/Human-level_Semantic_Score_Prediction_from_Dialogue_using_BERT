        {
          "id": "MDRW2100002047.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100002047.1.2",
          "form": "네 저희집은 강아지 키우고있어요",
          "original_form": "네 저희집은 강아지 키우고있어요",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100002047.1.3",
          "form": "부지런하시네여",
          "original_form": "부지런하시네여",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100002047.1.4",
          "form": "동물키우세요?",
          "original_form": "동물키우세요?",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100002047.1.5",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100002047.1.6",
          "form": "키우고 싶은동물 있으세요?",
          "original_form": "키우고 싶은동물 있으세요?",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100002047.1.7",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100002047.1.8",
          "form": "강아지 어떤종 좋아하세요?",
          "original_form": "강아지 어떤종 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100002047.1.9",
          "form": "종은 잘 몰라서뇨",
          "original_form": "종은 잘 몰라서뇨",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100002047.1.10",
          "form": "저희집은 시츄키우는데 좋은거같아요",
          "original_form": "저희집은 시츄키우는데 좋은거같아요",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100002047.1.11",
          "form": "어렵지 않아요?",
          "original_form": "어렵지 않아요?",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100002047.1.12",
          "form": "털 날리는거 빼면 참 좋은거같아요",
          "original_form": "털 날리는거 빼면 참 좋은거같아요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100002047.1.13",
          "form": "잘때도 옆에 누워서 자네요",
          "original_form": "잘때도 옆에 누워서 자네요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100002047.1.14",
          "form": "오 귀요미",
          "original_form": "오 귀요미",
          "speaker_id": "1",
          "time": "20210821 17:17"
