      "utterance": [
        {
          "id": "MDRW2100008707.1.1",
          "form": "엄마가 사왔어",
          "original_form": "엄마가 사왔어",
          "speaker_id": "2",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008707.1.2",
          "form": "쌀로 만든 빵 맛집이래",
          "original_form": "쌀로 만든 빵 맛집이래",
          "speaker_id": "2",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008707.1.3",
          "form": "빵좋아하시나 부내",
          "original_form": "빵좋아하시나 부내",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008707.1.4",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008707.1.5",
          "form": "빵빵",
          "original_form": "빵빵",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008707.1.6",
          "form": "카스테라 넘 맛나",
          "original_form": "카스테라 넘 맛나",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008707.1.7",
          "form": "다먹엇지만...",
          "original_form": "다먹엇지만...",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008707.1.8",
          "form": "사라쟛어?",
          "original_form": "사라쟛어?",
          "speaker_id": "1",
          "time": "20210822 18:44"
        },
        {
          "id": "MDRW2100008707.1.9",
          "form": "카스테라 달콤해요~",
          "original_form": "카스테라 달콤해요~",
          "speaker_id": "1",
          "time": "20210822 18:44"
        },
        {
          "id": "MDRW2100008707.1.10",
          "form": "마자 스타벅스 블루베리 치즈케이크맛나드라",
          "original_form": "마자 스타벅스 블루베리 치즈케이크맛나드라",
          "speaker_id": "1",
          "time": "20210822 18:44"
        },
        {
          "id": "MDRW2100008707.1.11",
          "form": "사라졋지..",
          "original_form": "사라졋지..",
          "speaker_id": "2",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008707.1.12",
          "form": "뭔데 니 다이어트 포기햇나",
          "original_form": "뭔데 니 다이어트 포기햇나",
          "speaker_id": "2",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008707.1.13",
          "form": "이름만 듷어도 별로다",
          "original_form": "이름만 듷어도 별로다",
          "speaker_id": "2",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008707.1.14",
          "form": "루팡됏어",
          "original_form": "루팡됏어",
          "speaker_id": "1",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008707.1.15",
          "form": "한입식 훔쳐먹어",
          "original_form": "한입식 훔쳐먹어",
          "speaker_id": "1",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008707.1.16",
          "form": "우",
          "original_form": "우",
          "speaker_id": "1",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008707.1.17",
          "form": "고망 먹어",
          "original_form": "고망 먹어",
          "speaker_id": "2",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008707.1.18",
          "form": "저녁은 얌전히 바나나묵엇어",
          "original_form": "저녁은 얌전히 바나나묵엇어",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008707.1.19",
          "form": "맛잇겟지",
          "original_form": "맛잇겟지",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008707.1.20",
          "form": "치삐",
          "original_form": "치삐",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008707.1.21",
          "form": "보내지네",
          "original_form": "보내지네",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008707.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008707.1.23",
          "form": "바나나밖에 안먹었어?",
          "original_form": "바나나밖에 안먹었어?",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008707.1.24",
          "form": "비나나우유에갈아먹으면진짜맛있는데",
          "original_form": "비나나우유에갈아먹으면진짜맛있는데",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008707.1.25",
          "form": "바나나랑 두부!",
          "original_form": "바나나랑 두부!",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008707.1.26",
          "form": "빠나나 우유만나?",
          "original_form": "빠나나 우유만나?",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008707.1.27",
          "form": "옹.. 조합이 신기하네",
          "original_form": "옹.. 조합이 신기하네",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008707.1.28",
          "form": "그것보다 훨씬 맛잇지",
          "original_form": "그것보다 훨씬 맛잇지",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008707.1.29",
          "form": "닭쮜쮜먹기싫엇어",
          "original_form": "닭쮜쮜먹기싫엇어",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008707.1.30",
          "form": "만나겟다",
          "original_form": "만나겟다",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008707.1.31",
          "form": "꿀도 빙 둘러서 갈갈갈 하면",
          "original_form": "꿀도 빙 둘러서 갈갈갈 하면",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008707.1.32",
          "form": "아주 천국이라구",
          "original_form": "아주 천국이라구",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008707.1.33",
          "form": "맛잇는거 묵네..",
          "original_form": "맛잇는거 묵네..",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008707.1.34",
          "form": "나두 줘",
          "original_form": "나두 줘",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008707.1.35",
          "form": "아니 저희집엔 바나나가 없는데요",
          "original_form": "아니 저희집엔 바나나가 없는데요",
          "speaker_id": "2",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100008707.1.36",
          "form": "왜업어",
          "original_form": "왜업어",
          "speaker_id": "1",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008707.1.37",
          "form": "맛잇단말야",
          "original_form": "맛잇단말야",
          "speaker_id": "1",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100008707.1.38",
          "form": "단호박 맛잇겟다",
          "original_form": "단호박 맛잇겟다",
          "speaker_id": "1",
          "time": "20210822 22:01"
        }
      ]
