      "utterance": [
        {
          "id": "MDRW2100007622.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210822 08:31"
        },
        {
          "id": "MDRW2100007622.1.2",
          "form": "저는 자취중인데 name2님은 어떠신가요!",
          "original_form": "저는 자취중인데 &name2&님은 어떠신가요!",
          "speaker_id": "1",
          "time": "20210822 08:31"
        },
        {
          "id": "MDRW2100007622.1.3",
          "form": "저는 본가에 살고있어요ㅎㅎ",
          "original_form": "저는 본가에 살고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 08:32"
        },
        {
          "id": "MDRW2100007622.1.4",
          "form": "자취하시면 집안일 힘드실거같아요",
          "original_form": "자취하시면 집안일 힘드실거같아요",
          "speaker_id": "2",
          "time": "20210822 08:32"
        },
        {
          "id": "MDRW2100007622.1.5",
          "form": "아무래도 혼자 하니까 시간이 참 많이 들어요 ㅋㅋㅋㅋ",
          "original_form": "아무래도 혼자 하니까 시간이 참 많이 들어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:33"
        },
        {
          "id": "MDRW2100007622.1.6",
          "form": "부모님께 큰 감사를 느끼는중...",
          "original_form": "부모님께 큰 감사를 느끼는중...",
          "speaker_id": "1",
          "time": "20210822 08:33"
        },
        {
          "id": "MDRW2100007622.1.7",
          "form": "본가에 사시면 아무래도 더 편하려나요!",
          "original_form": "본가에 사시면 아무래도 더 편하려나요!",
          "speaker_id": "1",
          "time": "20210822 08:33"
        },
        {
          "id": "MDRW2100007622.1.8",
          "form": "집안일은 해도해도 티가 안나서 더그런거같아요ㅎㅎㅎ",
          "original_form": "집안일은 해도해도 티가 안나서 더그런거같아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 08:33"
        },
        {
          "id": "MDRW2100007622.1.9",
          "form": "아무래도 나눠서 하니까  그런면이ㅎㅎ",
          "original_form": "아무래도 나눠서 하니까  그런면이ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 08:33"
        },
        {
          "id": "MDRW2100007622.1.10",
          "form": "ㅎㅎㅎㅎ 아 집안일 같이 하시는군요!",
          "original_form": "ㅎㅎㅎㅎ 아 집안일 같이 하시는군요!",
          "speaker_id": "1",
          "time": "20210822 08:34"
        },
        {
          "id": "MDRW2100007622.1.11",
          "form": "저는 본가 내려가면 아무것도 안하게돼요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 본가 내려가면 아무것도 안하게돼요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:34"
        },
        {
          "id": "MDRW2100007622.1.12",
          "form": "서로 바쁘니까 다같이 안하면 집안이 난리가 나더라구요ㅎㅎ",
          "original_form": "서로 바쁘니까 다같이 안하면 집안이 난리가 나더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 08:34"
        },
        {
          "id": "MDRW2100007622.1.13",
          "form": "아아 그러면 역시 같이 해야죠!",
          "original_form": "아아 그러면 역시 같이 해야죠!",
          "speaker_id": "1",
          "time": "20210822 08:35"
        },
        {
          "id": "MDRW2100007622.1.14",
          "form": "개인적으로 전 빨래가 제일 재미납니다...",
          "original_form": "개인적으로 전 빨래가 제일 재미납니다...",
          "speaker_id": "1",
          "time": "20210822 08:35"
        },
        {
          "id": "MDRW2100007622.1.15",
          "form": "어 전 재밌는건 없는거같아요ㅠㅠ다 지겨워서ㅎㅎ",
          "original_form": "어 전 재밌는건 없는거같아요ㅠㅠ다 지겨워서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 08:35"
        },
        {
          "id": "MDRW2100007622.1.16",
          "form": "앗 정말요? 하긴 매일 같은 일이라ㅜ",
          "original_form": "앗 정말요? 하긴 매일 같은 일이라ㅜ",
          "speaker_id": "1",
          "time": "20210822 08:37"
        },
        {
          "id": "MDRW2100007622.1.17",
          "form": "왜 다들 건조기나 로봇청소기를 사는지 알겠어요ㅎㅎ",
          "original_form": "왜 다들 건조기나 로봇청소기를 사는지 알겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 08:38"
        },
        {
          "id": "MDRW2100007622.1.18",
          "form": "아 정말 저는 그거 너무 갖고싶어요",
          "original_form": "아 정말 저는 그거 너무 갖고싶어요",
          "speaker_id": "1",
          "time": "20210822 08:38"
        },
        {
          "id": "MDRW2100007622.1.19",
          "form": "로봇청소기보다도 건조기 제발",
          "original_form": "로봇청소기보다도 건조기 제발",
          "speaker_id": "1",
          "time": "20210822 08:38"
        },
        {
          "id": "MDRW2100007622.1.20",
          "form": "집이 너무 습해요 ㅋㅋㅋ",
          "original_form": "집이 너무 습해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:38"
        },
        {
          "id": "MDRW2100007622.1.21",
          "form": "맞아요 에어컨기능으로는 한계가 있더라구요ㅠ",
          "original_form": "맞아요 에어컨기능으로는 한계가 있더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 08:39"
        },
        {
          "id": "MDRW2100007622.1.22",
          "form": "맞아요ㅜㅜ 특히 또 요즘은 에어컨 그렇게 켜두면 추워요...",
          "original_form": "맞아요ㅜㅜ 특히 또 요즘은 에어컨 그렇게 켜두면 추워요...",
          "speaker_id": "1",
          "time": "20210822 08:43"
        }
      ]
    }
