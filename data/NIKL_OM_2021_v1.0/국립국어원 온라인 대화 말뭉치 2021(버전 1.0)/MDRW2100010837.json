      "utterance": [
        {
          "id": "MDRW2100010837.1.1",
          "form": "여기 지금 천둥쳐",
          "original_form": "여기 지금 천둥쳐",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010837.1.2",
          "form": "태풍 오나봐",
          "original_form": "태풍 오나봐",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010837.1.3",
          "form": "헐 어디야? 여기는 아직 비만 와",
          "original_form": "헐 어디야? 여기는 아직 비만 와",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010837.1.4",
          "form": "여기 부산이야",
          "original_form": "여기 부산이야",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010837.1.5",
          "form": "ㅋㅋ name2은 어디야??",
          "original_form": "ㅋㅋ &name2&은 어디야??",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010837.1.6",
          "form": "헐 진짜? 나는 서울이야",
          "original_form": "헐 진짜? 나는 서울이야",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010837.1.7",
          "form": "나 부산사람인데ㅋㅋㅋㅋ",
          "original_form": "나 부산사람인데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010837.1.8",
          "form": "오 여기 지금 천둥 쳐!",
          "original_form": "오 여기 지금 천둥 쳐!",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010837.1.9",
          "form": "오오 부산사람이군ㅋㅋㅋ",
          "original_form": "오오 부산사람이군ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010837.1.10",
          "form": "반갑다",
          "original_form": "반갑다",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010837.1.11",
          "form": "시원하게 치겠다ㅋㅋㅋㅋ",
          "original_form": "시원하게 치겠다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010837.1.12",
          "form": "나두 반가워~~~",
          "original_form": "나두 반가워~~~",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010837.1.13",
          "form": "그래도 태풍 오고나면 확실히 시원해질거같아",
          "original_form": "그래도 태풍 오고나면 확실히 시원해질거같아",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010837.1.14",
          "form": "여름좀 끝났음 좋겠어ㅋㅋㅋ",
          "original_form": "여름좀 끝났음 좋겠어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010837.1.15",
          "form": "아 맞아 지금도 밤에는 약간 서늘한데",
          "original_form": "아 맞아 지금도 밤에는 약간 서늘한데",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010837.1.16",
          "form": "태풍 가고나면 본격적으로 가을이겠다ㅎㅎ",
          "original_form": "태풍 가고나면 본격적으로 가을이겠다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010837.1.17",
          "form": "여름 싫어해?",
          "original_form": "여름 싫어해?",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010837.1.18",
          "form": "더운걸 싫어해서ㅜㅜ 올 여름에 너무 더웠어",
          "original_form": "더운걸 싫어해서ㅜㅜ 올 여름에 너무 더웠어",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010837.1.19",
          "form": "지구 온난화때문에 점점 더워지는거 같기도 하고",
          "original_form": "지구 온난화때문에 점점 더워지는거 같기도 하고",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010837.1.20",
          "form": "아 맞아 이번에 진짜 폭염이었어ㅠㅠ",
          "original_form": "아 맞아 이번에 진짜 폭염이었어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010837.1.21",
          "form": "응 올해는 좀 다르더라 정말",
          "original_form": "응 올해는 좀 다르더라 정말",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010837.1.22",
          "form": "한 십년뒤엔 어찌될지 궁금해",
          "original_form": "한 십년뒤엔 어찌될지 궁금해",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010837.1.23",
          "form": "응 나도 생각보다 빠르게 변하는 거 같아서",
          "original_form": "응 나도 생각보다 빠르게 변하는 거 같아서",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010837.1.24",
          "form": "봄 가을도 짧아지고",
          "original_form": "봄 가을도 짧아지고",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010837.1.25",
          "form": "무섭다",
          "original_form": "무섭다",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010837.1.26",
          "form": "십년뒤엔 여름 겨울만 남을 듯ㅋㅋㅋ",
          "original_form": "십년뒤엔 여름 겨울만 남을 듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010837.1.27",
          "form": "사계절이 진짜 우리나라 자랑거리였는데",
          "original_form": "사계절이 진짜 우리나라 자랑거리였는데",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010837.1.28",
          "form": "안 그랬으면 좋겠어ㅠㅠ",
          "original_form": "안 그랬으면 좋겠어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010837.1.29",
          "form": "그러게 이제 교과서도 바뀌려나ㅜ",
          "original_form": "그러게 이제 교과서도 바뀌려나ㅜ",
          "speaker_id": "1",
          "time": "20210823 22:39"
        }
      ]
    }
