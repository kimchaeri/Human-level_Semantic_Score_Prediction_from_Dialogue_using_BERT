      "utterance": [
        {
          "id": "MDRW2100025076.1.1",
          "form": "와 춘천도",
          "original_form": "와 춘천도",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025076.1.2",
          "form": "퇴근길에 비 미친듯이오더라",
          "original_form": "퇴근길에 비 미친듯이오더라",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025076.1.3",
          "form": "올해는 비가 많이 오는것같아",
          "original_form": "올해는 비가 많이 오는것같아",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025076.1.4",
          "form": "가을장마인가",
          "original_form": "가을장마인가",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025076.1.5",
          "form": "근데 비 많이오니까 병원에 환자는 없드라ㅎㅎ",
          "original_form": "근데 비 많이오니까 병원에 환자는 없드라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025076.1.6",
          "form": "요즘 너한테는 가을비가 좋은거네",
          "original_form": "요즘 너한테는 가을비가 좋은거네",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025076.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런듯",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런듯",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025076.1.8",
          "form": "아침에 추워서 얇은 가디건입었는데",
          "original_form": "아침에 추워서 얇은 가디건입었는데",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025076.1.9",
          "form": "다행히 퇴근길에도 잘 입었다ㅋㅋ",
          "original_form": "다행히 퇴근길에도 잘 입었다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025076.1.10",
          "form": "이제 가디건의 계절이 오는구만",
          "original_form": "이제 가디건의 계절이 오는구만",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025076.1.11",
          "form": "그치그치 가디건 입으니까 낫더라고ㅋㅋㅋㅋ",
          "original_form": "그치그치 가디건 입으니까 낫더라고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025076.1.12",
          "form": "좀더 쌀쌀하면 두꺼운가디건입어야지...ㅎㅎ",
          "original_form": "좀더 쌀쌀하면 두꺼운가디건입어야지...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025076.1.13",
          "form": "아 나는 가디건 무조건 부드러워야해 조금이라도 까슬하면 못입겠어",
          "original_form": "아 나는 가디건 무조건 부드러워야해 조금이라도 까슬하면 못입겠어",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025076.1.14",
          "form": "아 알지알지",
          "original_form": "아 알지알지",
          "speaker_id": "2",
          "time": "20210901 07:42"
        },
        {
          "id": "MDRW2100025076.1.15",
          "form": "부드러워야 촉감도 좋아ㅋㅋㅋ",
          "original_form": "부드러워야 촉감도 좋아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 07:43"
        },
        {
          "id": "MDRW2100025076.1.16",
          "form": "아 날씨 칙칙하다",
          "original_form": "아 날씨 칙칙하다",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100025076.1.17",
          "form": "그니까 아까 1시에 퇴근했는데 그때도 비오더라",
          "original_form": "그니까 아까 1시에 퇴근했는데 그때도 비오더라",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025076.1.18",
          "form": "아우 언제 맑은 가을날씨되나",
          "original_form": "아우 언제 맑은 가을날씨되나",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025076.1.19",
          "form": "내말이 이러다가 겨울된다...ㅎㅎ",
          "original_form": "내말이 이러다가 겨울된다...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025076.1.20",
          "form": "중간이없는 날씨..",
          "original_form": "중간이없는 날씨..",
