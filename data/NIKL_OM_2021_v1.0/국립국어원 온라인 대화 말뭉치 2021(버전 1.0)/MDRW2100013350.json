      "utterance": [
        {
          "id": "MDRW2100013350.1.1",
          "form": "요즘 비가정말자주내리죠",
          "original_form": "요즘 비가정말자주내리죠",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013350.1.2",
          "form": "앗 맞아용! 이번에 태풍 때문인지 비 너무 많이 내리더라구요ㅠㅠ",
          "original_form": "앗 맞아용! 이번에 태풍 때문인지 비 너무 많이 내리더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013350.1.3",
          "form": "이런 비에는 우산도 소용이 없는거같아요",
          "original_form": "이런 비에는 우산도 소용이 없는거같아요",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013350.1.4",
          "form": "비오는날 신발은 주로 뭐 신으시나요?",
          "original_form": "비오는날 신발은 주로 뭐 신으시나요?",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013350.1.5",
          "form": "아무래도 좀 젖기싫으니까 슬리퍼챙겼다가 갈아신어요",
          "original_form": "아무래도 좀 젖기싫으니까 슬리퍼챙겼다가 갈아신어요",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013350.1.6",
          "form": "장화신으면좋은데 답답하기도 하고 물 차면 답이 없어서요",
          "original_form": "장화신으면좋은데 답답하기도 하고 물 차면 답이 없어서요",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013350.1.7",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013350.1.8",
          "form": "저는 샌들 신어요!",
          "original_form": "저는 샌들 신어요!",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013350.1.9",
          "form": "날씨 좋으면 운동화 신는데ㅠㅠ",
          "original_form": "날씨 좋으면 운동화 신는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013350.1.10",
          "form": "아무래도 비오는 날엔 운동화가 꺼려지죠",
          "original_form": "아무래도 비오는 날엔 운동화가 꺼려지죠",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013350.1.11",
          "form": "운동화는 빗물 먹으면 냄새 너무나서",
          "original_form": "운동화는 빗물 먹으면 냄새 너무나서",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013350.1.12",
          "form": "맞아요 잘 마르지도않아요",
          "original_form": "맞아요 잘 마르지도않아요",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013350.1.13",
          "form": "8월만해도 햇볕 너무 쎄서",
          "original_form": "8월만해도 햇볕 너무 쎄서",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013350.1.14",
          "form": "양산살지 양우산 살지 고민하고있었는데",
          "original_form": "양산살지 양우산 살지 고민하고있었는데",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013350.1.15",
          "form": "지금날씨 보면 역시 우산이 짱입니다...",
          "original_form": "지금날씨 보면 역시 우산이 짱입니다...",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013350.1.16",
          "form": "역시 그렇죠?",
          "original_form": "역시 그렇죠?",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013350.1.17",
          "form": "우산도 좀 비싸고 튼튼한게..",
          "original_form": "우산도 좀 비싸고 튼튼한게..",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013350.1.18",
          "form": "이런비에는 싼거 쓰면 뒤집어지더라구요",
          "original_form": "이런비에는 싼거 쓰면 뒤집어지더라구요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013350.1.19",
          "form": "ㅠㅠ비바람 부는게 젤 힘든거같아요",
          "original_form": "ㅠㅠ비바람 부는게 젤 힘든거같아요",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013350.1.20",
          "form": "우산 뒤집어지고 바지 다 젖고ㅠㅠ",
          "original_form": "우산 뒤집어지고 바지 다 젖고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013350.1.21",
          "form": "저 예전에 다이소 우산썼다가 뒤집어져서",
          "original_form": "저 예전에 다이소 우산썼다가 뒤집어져서",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013350.1.22",
          "form": "너무 허무해서 그자리에서 헛웃음 나더라구요",
          "original_form": "너무 허무해서 그자리에서 헛웃음 나더라구요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013350.1.23",
          "form": "헉...다이소우산.. 많이구리구나ㅋㅋ...",
          "original_form": "헉...다이소우산.. 많이구리구나ㅋㅋ...",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013350.1.24",
          "form": "사자마자 뒤집어지는..",
          "original_form": "사자마자 뒤집어지는..",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013350.1.25",
          "form": "아까운 5천원..",
          "original_form": "아까운 5천원..",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013350.1.26",
          "form": "바람이 심한 날이었나요?",
          "original_form": "바람이 심한 날이었나요?",
          "speaker_id": "2",
          "time": "20210825 14:33"
        }
      ]
    }
