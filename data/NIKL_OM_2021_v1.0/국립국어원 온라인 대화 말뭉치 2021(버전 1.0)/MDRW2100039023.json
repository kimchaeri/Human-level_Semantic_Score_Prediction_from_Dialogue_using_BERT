      "utterance": [
        {
          "id": "MDRW2100039023.1.1",
          "form": "안녕라새여",
          "original_form": "안녕라새여",
          "speaker_id": "1",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100039023.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100039023.1.3",
          "form": "지금 가족이랑 같이 있나요?",
          "original_form": "지금 가족이랑 같이 있나요?",
          "speaker_id": "1",
          "time": "20210903 02:04"
        },
        {
          "id": "MDRW2100039023.1.4",
          "form": "가족이랑 같이 살고 있죠\r\n하지만 방에 혼자 있어요",
          "original_form": "가족이랑 같이 살고 있죠\r\n하지만 방에 혼자 있어요",
          "speaker_id": "2",
          "time": "20210903 02:08"
        },
        {
          "id": "MDRW2100039023.1.5",
          "form": "저는 옆에 언니가 잇죠ㅠ",
          "original_form": "저는 옆에 언니가 잇죠ㅠ",
          "speaker_id": "1",
          "time": "20210903 02:09"
        },
        {
          "id": "MDRW2100039023.1.6",
          "form": "앗 그러시군요",
          "original_form": "앗 그러시군요",
          "speaker_id": "2",
          "time": "20210903 02:09"
        },
        {
          "id": "MDRW2100039023.1.7",
          "form": "공동공간에 있나봐요",
          "original_form": "공동공간에 있나봐요",
          "speaker_id": "2",
          "time": "20210903 02:09"
        },
        {
          "id": "MDRW2100039023.1.8",
          "form": "진짜 짜증나요ㅠ",
          "original_form": "진짜 짜증나요ㅠ",
          "speaker_id": "1",
          "time": "20210903 02:09"
        },
        {
          "id": "MDRW2100039023.1.9",
          "form": "왜죠?",
          "original_form": "왜죠?",
          "speaker_id": "2",
          "time": "20210903 02:10"
        },
        {
          "id": "MDRW2100039023.1.10",
          "form": "자꾸 때리고....ㅠㅠㅠ",
          "original_form": "자꾸 때리고....ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 02:11"
        },
        {
          "id": "MDRW2100039023.1.11",
          "form": "네...??",
          "original_form": "네...??",
          "speaker_id": "2",
          "time": "20210903 02:11"
        },
        {
          "id": "MDRW2100039023.1.12",
          "form": "때린다구요?",
          "original_form": "때린다구요?",
          "speaker_id": "2",
          "time": "20210903 02:11"
        },
        {
          "id": "MDRW2100039023.1.13",
          "form": "기분좋다고 때 리 고 꼬집고 ㅡㅡ",
          "original_form": "기분좋다고 때 리 고 꼬집고 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210903 02:12"
        },
        {
          "id": "MDRW2100039023.1.14",
          "form": "애정표현이 좀 과한편인가봐요",
          "original_form": "애정표현이 좀 과한편인가봐요",
          "speaker_id": "2",
          "time": "20210903 02:13"
        },
        {
          "id": "MDRW2100039023.1.15",
          "form": "네 하는짓이 거의 초딩이죠",
          "original_form": "네 하는짓이 거의 초딩이죠",
          "speaker_id": "1",
          "time": "20210903 02:14"
        },
        {
          "id": "MDRW2100039023.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 02:14"
        },
        {
          "id": "MDRW2100039023.1.17",
          "form": "가끔은 어린아이처럼 지내는게 편할때도 있어요",
          "original_form": "가끔은 어린아이처럼 지내는게 편할때도 있어요",
          "speaker_id": "2",
          "time": "20210903 02:15"
        },
        {
          "id": "MDRW2100039023.1.18",
          "form": "저는 편하지 않다는게 중요하져ㅠ",
          "original_form": "저는 편하지 않다는게 중요하져ㅠ",
          "speaker_id": "1",
          "time": "20210903 02:19"
        }
      ]
