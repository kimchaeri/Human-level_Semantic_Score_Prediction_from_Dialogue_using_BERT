      "utterance": [
        {
          "id": "MDRW2100034484.1.1",
          "form": "오 바깥은 더웠나보네",
          "original_form": "오 바깥은 더웠나보네",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100034484.1.2",
          "form": "실내는 서늘해",
          "original_form": "실내는 서늘해",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100034484.1.3",
          "form": "응 바깥은 햇볕 쨍쨍",
          "original_form": "응 바깥은 햇볕 쨍쨍",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034484.1.4",
          "form": "걷다보면 덥더라",
          "original_form": "걷다보면 덥더라",
          "speaker_id": "1",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100034484.1.5",
          "form": "지하철타지 더울텐데 ㅜ",
          "original_form": "지하철타지 더울텐데 ㅜ",
          "speaker_id": "2",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100034484.1.6",
          "form": "케이크까지 들고오느라 고생했네",
          "original_form": "케이크까지 들고오느라 고생했네",
          "speaker_id": "2",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100034484.1.7",
          "form": "걷기 시작할때는 선선했지",
          "original_form": "걷기 시작할때는 선선했지",
          "speaker_id": "1",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100034484.1.8",
          "form": "모처럼 날도 맑고",
          "original_form": "모처럼 날도 맑고",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034484.1.9",
          "form": "아하 이제 공원 길은 안막아뒀어?",
          "original_form": "아하 이제 공원 길은 안막아뒀어?",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034484.1.10",
          "form": "아니 막아둬서",
          "original_form": "아니 막아둬서",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034484.1.11",
          "form": "부천시민의 강 따라 맑은 날씨 즐기며 왔지",
          "original_form": "부천시민의 강 따라 맑은 날씨 즐기며 왔지",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034484.1.12",
          "form": "그랬구나 잘했네 계절을 만끽하셨군",
          "original_form": "그랬구나 잘했네 계절을 만끽하셨군",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100034484.1.13",
          "form": "응 그치만 좀 더웠다",
          "original_form": "응 그치만 좀 더웠다",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034484.1.14",
          "form": "이런 더운 날씨엔 아이스크림이지",
          "original_form": "이런 더운 날씨엔 아이스크림이지",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034484.1.15",
          "form": "바람이 아직 좀 덥쥬?",
          "original_form": "바람이 아직 좀 덥쥬?",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034484.1.16",
          "form": "그러게 바닥에서 열이 올라오네",
          "original_form": "그러게 바닥에서 열이 올라오네",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034484.1.17",
          "form": "아직은 더워 햇볕이",
          "original_form": "아직은 더워 햇볕이",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034484.1.18",
          "form": "공기는 좀 식었어도",
          "original_form": "공기는 좀 식었어도",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034484.1.19",
          "form": "그렇네 확실히",
          "original_form": "그렇네 확실히",
          "speaker_id": "2",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034484.1.20",
          "form": "마스크땜에 갑갑하기도 하고",
          "original_form": "마스크땜에 갑갑하기도 하고",
          "speaker_id": "2",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034484.1.21",
          "form": "응 그래도 습도는 덜하다",
          "original_form": "응 그래도 습도는 덜하다",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034484.1.22",
          "form": "그나마 다행이지",
          "original_form": "그나마 다행이지",
          "speaker_id": "2",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100034484.1.23",
          "form": "비가 무섭게 쏟아지는구만",
          "original_form": "비가 무섭게 쏟아지는구만",
          "speaker_id": "1",
          "time": "20210831 16:33"
        }
      ]
