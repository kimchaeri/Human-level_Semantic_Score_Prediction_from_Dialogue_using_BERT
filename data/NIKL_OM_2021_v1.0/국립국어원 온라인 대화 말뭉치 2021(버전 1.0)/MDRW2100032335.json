      "utterance": [
        {
          "id": "MDRW2100032335.1.1",
          "form": "돌싱 무슨 연애프로 하는 짤 봤니",
          "original_form": "돌싱 무슨 연애프로 하는 짤 봤니",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032335.1.2",
          "form": "나 뭐 잘못누른거같아",
          "original_form": "나 뭐 잘못누른거같아",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032335.1.3",
          "form": "아 넘어갔구나",
          "original_form": "아 넘어갔구나",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032335.1.4",
          "form": "난 티비를 못봐여....",
          "original_form": "난 티비를 못봐여....",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032335.1.5",
          "form": "괜찮아",
          "original_form": "괜찮아",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032335.1.6",
          "form": "난 트위터에서 본 거",
          "original_form": "난 트위터에서 본 거",
          "speaker_id": "2",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032335.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 유튜브도 무음감상",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 유튜브도 무음감상",
          "speaker_id": "1",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032335.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032335.1.9",
          "form": "자막 잘 나오니",
          "original_form": "자막 잘 나오니",
          "speaker_id": "2",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032335.1.10",
          "form": "자막이 꽤 잘맞더라 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "자막이 꽤 잘맞더라 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032335.1.11",
          "form": "블루투스 이어폰을 샀거든",
          "original_form": "블루투스 이어폰을 샀거든",
          "speaker_id": "1",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032335.1.12",
          "form": "근데 그게 불빛이 번쩍번쩍해서 못씀",
          "original_form": "근데 그게 불빛이 번쩍번쩍해서 못씀",
          "speaker_id": "1",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032335.1.13",
          "form": "!!!저런",
          "original_form": "!!!저런",
          "speaker_id": "2",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032335.1.14",
          "form": "애기 재우고 몰래 유튜브 봐야되는데 번쩍거리면 애들 깰까봐",
          "original_form": "애기 재우고 몰래 유튜브 봐야되는데 번쩍거리면 애들 깰까봐",
          "speaker_id": "1",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032335.1.15",
          "form": "결혼은 개인의 취미생활에 이런 악영향을 줍니다",
          "original_form": "결혼은 개인의 취미생활에 이런 악영향을 줍니다",
          "speaker_id": "2",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032335.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032335.1.17",
          "form": "주제에 정말 적합한 발언이군요",
          "original_form": "주제에 정말 적합한 발언이군요",
          "speaker_id": "1",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032335.1.18",
          "form": "고롬고롬",
          "original_form": "고롬고롬",
          "speaker_id": "2",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032335.1.19",
          "form": "딱히 갖고있던 취미생활이라곤 없었는데",
          "original_form": "딱히 갖고있던 취미생활이라곤 없었는데",
          "speaker_id": "1",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100032335.1.20",
          "form": "해봤자 덕질",
          "original_form": "해봤자 덕질",
          "speaker_id": "1",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100032335.1.21",
          "form": "덕질도 못하고 새로나온 음원도 출퇴근길에서만 감상가능",
          "original_form": "덕질도 못하고 새로나온 음원도 출퇴근길에서만 감상가능",
          "speaker_id": "1",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100032335.1.22",
          "form": "콘서트도 못 가잖아",
          "original_form": "콘서트도 못 가잖아",
          "speaker_id": "2",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100032335.1.23",
          "form": "콘서트나 뮤지컬 갈 돈도 없고 ..",
          "original_form": "콘서트나 뮤지컬 갈 돈도 없고 ..",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032335.1.24",
          "form": "이번달 진짜 마이너스였는데",
          "original_form": "이번달 진짜 마이너스였는데",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032335.1.25",
          "form": "근데 결혼보단 출산육아가 생활변화에 영향이 큰 거 같애",
          "original_form": "근데 결혼보단 출산육아가 생활변화에 영향이 큰 거 같애",
          "speaker_id": "2",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032335.1.26",
          "form": "명절휴가비 10일에 준대 이히",
          "original_form": "명절휴가비 10일에 준대 이히",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032335.1.27",
          "form": "어 상여금 ㅋㅋㅋ",
          "original_form": "어 상여금 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032335.1.28",
          "form": "그치그치 출산 때문인거지 이건",
          "original_form": "그치그치 출산 때문인거지 이건",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032335.1.29",
          "form": "난 못 받지만...",
          "original_form": "난 못 받지만...",
          "speaker_id": "2",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032335.1.30",
          "form": "남편 받아와랏",
          "original_form": "남편 받아와랏",
          "speaker_id": "2",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032335.1.31",
          "form": "돈만주면 우울증 치유 가능",
          "original_form": "돈만주면 우울증 치유 가능",
          "speaker_id": "1",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100032335.1.32",
          "form": "ㅋㅋㅋㅋ요새 복권 좀 안 샀다. 자꾸 안 맞드라고",
