      "utterance": [
        {
          "id": "MDRW2100024232.1.1",
          "form": "여행  좋아하세요?",
          "original_form": "여행  좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100024232.1.2",
          "form": "좋아하는데 자주는 못가요",
          "original_form": "좋아하는데 자주는 못가요",
          "speaker_id": "1",
          "time": "20210831 08:06"
        },
        {
          "id": "MDRW2100024232.1.3",
          "form": "다 그렇죠 머",
          "original_form": "다 그렇죠 머",
          "speaker_id": "2",
          "time": "20210831 08:25"
        },
        {
          "id": "MDRW2100024232.1.4",
          "form": "코로나땜에 더",
          "original_form": "코로나땜에 더",
          "speaker_id": "2",
          "time": "20210831 08:25"
        },
        {
          "id": "MDRW2100024232.1.5",
          "form": "네! 시간이있으면 돈이없고 요즘엔 이도저도 못하고",
          "original_form": "네! 시간이있으면 돈이없고 요즘엔 이도저도 못하고",
          "speaker_id": "1",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100024232.1.6",
          "form": "최근에 가신 여행은?",
          "original_form": "최근에 가신 여행은?",
          "speaker_id": "1",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100024232.1.7",
          "form": "최근엔 어디 간곳이 없네여ㅜㅜ",
          "original_form": "최근엔 어디 간곳이 없네여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100024232.1.8",
          "form": "요즘 계속 비오고 어딜 가도 가게문도 일찍 문닫고...",
          "original_form": "요즘 계속 비오고 어딜 가도 가게문도 일찍 문닫고...",
          "speaker_id": "1",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024232.1.9",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024232.1.10",
          "form": "9시가 뭔가요",
          "original_form": "9시가 뭔가요",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024232.1.11",
          "form": "그니까요.. 저녁먹으면서 급하게 반주하는게 고작",
          "original_form": "그니까요.. 저녁먹으면서 급하게 반주하는게 고작",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024232.1.12",
          "form": "술 좋아하세요?",
          "original_form": "술 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024232.1.13",
          "form": "싫어하지는 않아요",
          "original_form": "싫어하지는 않아요",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100024232.1.14",
          "form": "전 요즘 그냥 주말에 맥주한캔 하는게 술마시는거 다에요",
          "original_form": "전 요즘 그냥 주말에 맥주한캔 하는게 술마시는거 다에요",
          "speaker_id": "2",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024232.1.15",
          "form": "낮술은 싫더라구요",
          "original_form": "낮술은 싫더라구요",
          "speaker_id": "2",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024232.1.16",
          "form": "주말에 낮술하고 낮잠자는 꿀잼을 모르시다니",
          "original_form": "주말에 낮술하고 낮잠자는 꿀잼을 모르시다니",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024232.1.17",
          "form": "ㅋㅋㅋㅋㅋ 그게 꿀잼이에요?",
          "original_form": "ㅋㅋㅋㅋㅋ 그게 꿀잼이에요?",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024232.1.18",
          "form": "넹 평일에는 할수없는",
          "original_form": "넹 평일에는 할수없는",
          "speaker_id": "1",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024232.1.19",
          "form": "술 좋아하시는거 같은데요?ㅋㅋ",
          "original_form": "술 좋아하시는거 같은데요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100024232.1.20",
          "form": "싫어하진 않아요",
          "original_form": "싫어하진 않아요",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024232.1.21",
          "form": "전 체질적으로 못마셔서 ㅠㅠ",
          "original_form": "전 체질적으로 못마셔서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100024232.1.22",
          "form": "저도 빨게지는 체질이지만 마셔요",
          "original_form": "저도 빨게지는 체질이지만 마셔요",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100024232.1.23",
          "form": "괜찮아요?",
          "original_form": "괜찮아요?",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100024232.1.24",
          "form": "주량이 어떻게 되세요?",
          "original_form": "주량이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100024232.1.25",
          "form": "한병정도?",
          "original_form": "한병정도?",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100024232.1.26",
          "form": "적당하네요",
          "original_form": "적당하네요",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100024232.1.27",
          "form": "전 반병?",
          "original_form": "전 반병?",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100024232.1.28",
          "form": "적당량 을 넘치게 마셔서 문제",
          "original_form": "적당량 을 넘치게 마셔서 문제",
          "speaker_id": "1",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100024232.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100024232.1.30",
          "form": "여행가서 숯불에 고기구워서 술한잔하면 좋은데",
          "original_form": "여행가서 숯불에 고기구워서 술한잔하면 좋은데",
          "speaker_id": "2",
          "time": "20210901 09:55"
        },
        {
          "id": "MDRW2100024232.1.31",
          "form": "참 할수있는게 없네요",
          "original_form": "참 할수있는게 없네요",
          "speaker_id": "2",
          "time": "20210901 09:55"
        },
        {
          "id": "MDRW2100024232.1.32",
          "form": "혹시캠핑하세요?",
          "original_form": "혹시캠핑하세요?",
          "speaker_id": "1",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100024232.1.33",
          "form": "친구랑 자주했었는데",
          "original_form": "친구랑 자주했었는데",
          "speaker_id": "2",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100024232.1.34",
          "form": "자는게 너무 불편해서",
          "original_form": "자는게 너무 불편해서",
          "speaker_id": "2",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100024232.1.35",
          "form": "요즘은 안가요 ㅎㅎ",
          "original_form": "요즘은 안가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:40"
        },
        {
          "id": "MDRW2100024232.1.36",
          "form": "오~ 캠핑족이셨군요",
          "original_form": "오~ 캠핑족이셨군요",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100024232.1.37",
          "form": "전 차피 밤샘이라 자는건 괜찮은데 벌레가 너무 싫어요",
          "original_form": "전 차피 밤샘이라 자는건 괜찮은데 벌레가 너무 싫어요",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100024232.1.38",
          "form": "아 벌레.... 특히 나방",
          "original_form": "아 벌레.... 특히 나방",
          "speaker_id": "2",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100024232.1.39",
          "form": "새같은 나방이...",
          "original_form": "새같은 나방이...",
          "speaker_id": "2",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100024232.1.40",
          "form": "거기에 모기",
          "original_form": "거기에 모기",
          "speaker_id": "2",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100024232.1.41",
          "form": "새같은 나방 ㅋㅋ",
          "original_form": "새같은 나방 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100024232.1.42",
          "form": "가만히있어도 근지러운 느낌",
          "original_form": "가만히있어도 근지러운 느낌",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100024232.1.43",
          "form": "모기 잘물려요?",
          "original_form": "모기 잘물려요?",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100024232.1.44",
          "form": "네 열있는 체질이라 모기를 부르는 체질이예요",
          "original_form": "네 열있는 체질이라 모기를 부르는 체질이예요",
          "speaker_id": "1",
          "time": "20210901 14:37"
        }
      ]
    }
  ]
