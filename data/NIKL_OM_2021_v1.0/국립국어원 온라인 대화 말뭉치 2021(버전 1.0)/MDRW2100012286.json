      "utterance": [
        {
          "id": "MDRW2100012286.1.1",
          "form": "오늘 날씨 좋다",
          "original_form": "오늘 날씨 좋다",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012286.1.2",
          "form": "오늘 비바람 쳤는데..?",
          "original_form": "오늘 비바람 쳤는데..?",
          "speaker_id": "2",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012286.1.3",
          "form": "오늘 비바람 쳤어?",
          "original_form": "오늘 비바람 쳤어?",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012286.1.4",
          "form": "응응 창문 안봤어..?",
          "original_form": "응응 창문 안봤어..?",
          "speaker_id": "2",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012286.1.5",
          "form": "난 집에만 있어서 몰랐어",
          "original_form": "난 집에만 있어서 몰랐어",
          "speaker_id": "1",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012286.1.6",
          "form": "엥ㅋㅋㅋㅋㅋ집순이구낭",
          "original_form": "엥ㅋㅋㅋㅋㅋ집순이구낭",
          "speaker_id": "2",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012286.1.7",
          "form": "맞아 상쾌한 기분이라 날씨가 좋은줄 알았는데 아니었구나",
          "original_form": "맞아 상쾌한 기분이라 날씨가 좋은줄 알았는데 아니었구나",
          "speaker_id": "1",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012286.1.8",
          "form": "넌 비오는 날 좋아해?",
          "original_form": "넌 비오는 날 좋아해?",
          "speaker_id": "2",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012286.1.9",
          "form": "비오는날 뽀송뽀송한 상태로 집에서 창문보는건 좋아해",
          "original_form": "비오는날 뽀송뽀송한 상태로 집에서 창문보는건 좋아해",
          "speaker_id": "1",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100012286.1.10",
          "form": "헐 너 나랑 취향 똑같다ㅎㅎㅎ",
          "original_form": "헐 너 나랑 취향 똑같다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100012286.1.11",
          "form": "다들 그렇지 뭐 ㅎㅎ 너는 무슨 계절이 좋아?",
          "original_form": "다들 그렇지 뭐 ㅎㅎ 너는 무슨 계절이 좋아?",
          "speaker_id": "1",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100012286.1.12",
          "form": "난 겨울ㅎㅎㅎ",
          "original_form": "난 겨울ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012286.1.13",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012286.1.14",
          "form": "눈이 소복하게 쌓여있는 풍경이 이쁘더라구",
          "original_form": "눈이 소복하게 쌓여있는 풍경이 이쁘더라구",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012286.1.15",
          "form": "옛날에는 그랬는데 나이 들고 보니까 질척거리고 별루야",
          "original_form": "옛날에는 그랬는데 나이 들고 보니까 질척거리고 별루야",
          "speaker_id": "1",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100012286.1.16",
          "form": "ㅋㅋㅋㅋㅋ넌 무슨 계절이 좋은데?",
          "original_form": "ㅋㅋㅋㅋㅋ넌 무슨 계절이 좋은데?",
          "speaker_id": "2",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100012286.1.17",
          "form": "나는 선선한 가을!",
          "original_form": "나는 선선한 가을!",
          "speaker_id": "1",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100012286.1.18",
          "form": "오 좀 있음 가을이잖아!",
          "original_form": "오 좀 있음 가을이잖아!",
          "speaker_id": "2",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012286.1.19",
          "form": "맞아. 여름은 벌레가 많아서 너무 싫어",
          "original_form": "맞아. 여름은 벌레가 많아서 너무 싫어",
          "speaker_id": "1",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012286.1.20",
          "form": "ㅋㅋㅋㅋ여름은 땀나고 끈적끈적해",
          "original_form": "ㅋㅋㅋㅋ여름은 땀나고 끈적끈적해",
          "speaker_id": "2",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100012286.1.21",
          "form": "사실 여름만 아니면 돼 ㅎㅎ",
          "original_form": "사실 여름만 아니면 돼 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100012286.1.22",
          "form": "ㅋㅋㅋㅋㅋ아 사실 나도",
          "original_form": "ㅋㅋㅋㅋㅋ아 사실 나도",
          "speaker_id": "2",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100012286.1.23",
          "form": "여름은 벌레가 많아서 싫어",
          "original_form": "여름은 벌레가 많아서 싫어",
          "speaker_id": "1",
          "time": "20210824 21:01"
        }
      ]
