      "utterance": [
        {
          "id": "MDRW2100026695.1.1",
          "form": "그 나 알바 같이 하는 언니 있잖아",
          "original_form": "그 나 알바 같이 하는 언니 있잖아",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026695.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026695.1.3",
          "form": "마감때 같이하는언니?",
          "original_form": "마감때 같이하는언니?",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026695.1.4",
          "form": "그 언니가 좋아서 마감 도와주러 간다고 했었잖아",
          "original_form": "그 언니가 좋아서 마감 도와주러 간다고 했었잖아",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026695.1.5",
          "form": "맞아맞아 배드민턴 언니",
          "original_form": "맞아맞아 배드민턴 언니",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026695.1.6",
          "form": "그 언니가 원래 11시까지 하고",
          "original_form": "그 언니가 원래 11시까지 하고",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026695.1.7",
          "form": "그 이후에 오빠가 새벽 3시까지 하는건데",
          "original_form": "그 이후에 오빠가 새벽 3시까지 하는건데",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026695.1.8",
          "form": "ㅇ응",
          "original_form": "ㅇ응",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026695.1.9",
          "form": "그 오빠가 도망가서",
          "original_form": "그 오빠가 도망가서",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026695.1.10",
          "form": "와 너네카페 24시간이야?",
          "original_form": "와 너네카페 24시간이야?",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.11",
          "form": "그 언니가 마감하는 거란 말이야",
          "original_form": "그 언니가 마감하는 거란 말이야",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.12",
          "form": "덜덜?",
          "original_form": "덜덜?",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.13",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.14",
          "form": "거의 24시간...",
          "original_form": "거의 24시간...",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.15",
          "form": "근데 그 언니는 당연히 안하던 일을 하니까 힘들잖아",
          "original_form": "근데 그 언니는 당연히 안하던 일을 하니까 힘들잖아",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.16",
          "form": "일이 더 늘어난 거니까",
          "original_form": "일이 더 늘어난 거니까",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.17",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.18",
          "form": "그래서 매니저언니한테 말했는데",
          "original_form": "그래서 매니저언니한테 말했는데",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.19",
          "form": "사장 와이프가 전화와서",
          "original_form": "사장 와이프가 전화와서",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.20",
          "form": "반말하면서",
          "original_form": "반말하면서",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.21",
          "form": "???",
          "original_form": "???",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026695.1.22",
          "form": "원래 심야타임이 빠지면",
          "original_form": "원래 심야타임이 빠지면",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026695.1.23",
          "form": "니가 해야하는 거라고",
          "original_form": "니가 해야하는 거라고",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026695.1.24",
          "form": "힘든게 당연하다",
          "original_form": "힘든게 당연하다",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026695.1.25",
          "form": "안힘든 일이 어디있냐고",
