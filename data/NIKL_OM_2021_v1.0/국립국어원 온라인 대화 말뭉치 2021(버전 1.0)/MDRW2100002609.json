      "utterance": [
        {
          "id": "MDRW2100002609.1.1",
          "form": "오늘 h패션몰 주말적립금 주나봐요",
          "original_form": "오늘 h패션몰 주말적립금 주나봐요",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002609.1.2",
          "form": "네 안그래도 저도 그거보고 룰렛돌려서 포인트추가로 받았어요",
          "original_form": "네 안그래도 저도 그거보고 룰렛돌려서 포인트추가로 받았어요",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002609.1.3",
          "form": "출석은 다 해놓으셨어요?",
          "original_form": "출석은 다 해놓으셨어요?",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002609.1.4",
          "form": "네 만근할려고 노력중인데 아직은 성공중이요",
          "original_form": "네 만근할려고 노력중인데 아직은 성공중이요",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002609.1.5",
          "form": "전 3천포 룰렛나와서 지금 딱2만원됐는데 낼살까요?",
          "original_form": "전 3천포 룰렛나와서 지금 딱2만원됐는데 낼살까요?",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002609.1.6",
          "form": "저도 지금까지 출석 성공적이라서...",
          "original_form": "저도 지금까지 출석 성공적이라서...",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002609.1.7",
          "form": "3천포 애매하네요",
          "original_form": "3천포 애매하네요",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002609.1.8",
          "form": "뭐 사셨어요?",
          "original_form": "뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002609.1.9",
          "form": "타미힐피거 우먼에 티셔츠샀어요",
          "original_form": "타미힐피거 우먼에 티셔츠샀어요",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002609.1.10",
          "form": "반팔티사신거예요?",
          "original_form": "반팔티사신거예요?",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002609.1.11",
          "form": "패션몰은 타미가 젤만만한거같아요",
          "original_form": "패션몰은 타미가 젤만만한거같아요",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002609.1.12",
          "form": "다른브랜드 포인트를 못쓰더라구요",
          "original_form": "다른브랜드 포인트를 못쓰더라구요",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002609.1.13",
          "form": "네 반팔티 긴팔사고싶은데 클리어런스 중에서 고르니 반팔뿐 ㅎㅎ",
          "original_form": "네 반팔티 긴팔사고싶은데 클리어런스 중에서 고르니 반팔뿐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002609.1.14",
          "form": "긴팔은 더비싸서 실결제 있어야될듯요",
          "original_form": "긴팔은 더비싸서 실결제 있어야될듯요",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002609.1.15",
          "form": "저도 죄다 타미 반팔티ㅋ",
          "original_form": "저도 죄다 타미 반팔티ㅋ",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002609.1.16",
          "form": "98000원짜리 쿠폰 적립금 쓸어모아서 3천원에샀어요 ㅎ",
          "original_form": "98000원짜리 쿠폰 적립금 쓸어모아서 3천원에샀어요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002609.1.17",
          "form": "네 긴팔은 아직싸지않더라구요",
          "original_form": "네 긴팔은 아직싸지않더라구요",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002609.1.18",
          "form": "오ㅎ 대박인데요 그비싼걸 3천원에 사다니",
          "original_form": "오ㅎ 대박인데요 그비싼걸 3천원에 사다니",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002609.1.19",
          "form": "알뜰쇼핑하셨네요",
          "original_form": "알뜰쇼핑하셨네요",
