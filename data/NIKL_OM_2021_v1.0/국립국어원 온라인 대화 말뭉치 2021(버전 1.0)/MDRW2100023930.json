      "utterance": [
        {
          "id": "MDRW2100023930.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100023930.1.2",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023930.1.3",
          "form": "넵 안녕하세요 저랑 코로나 백신 얘길 하실래요?",
          "original_form": "넵 안녕하세요 저랑 코로나 백신 얘길 하실래요?",
          "speaker_id": "1",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023930.1.4",
          "form": "좋아요~~",
          "original_form": "좋아요~~",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023930.1.5",
          "form": "혹시 백신 맞으셨나요??",
          "original_form": "혹시 백신 맞으셨나요??",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023930.1.6",
          "form": "아니오 곧 맞는데 부작용이 있을까봐 걱정돼요",
          "original_form": "아니오 곧 맞는데 부작용이 있을까봐 걱정돼요",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023930.1.7",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023930.1.8",
          "form": "저는 9월 중순 쯤에 예약했어요!!",
          "original_form": "저는 9월 중순 쯤에 예약했어요!!",
          "speaker_id": "2",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100023930.1.9",
          "form": "저도 부작용 뉴스가 계속 나와서 불안하긴 해요...",
          "original_form": "저도 부작용 뉴스가 계속 나와서 불안하긴 해요...",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023930.1.10",
          "form": "그런데 안 맞기는 좀 그렇고..",
          "original_form": "그런데 안 맞기는 좀 그렇고..",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023930.1.11",
          "form": "그르네용~",
          "original_form": "그르네용~",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023930.1.12",
          "form": "저는 맞으라니 맞긴 하는데 불신이 가득해욬ㅋ",
          "original_form": "저는 맞으라니 맞긴 하는데 불신이 가득해욬ㅋ",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023930.1.13",
          "form": "문제생기면 부작용인정되기가 진짜 어려울 게 보 이니",
          "original_form": "문제생기면 부작용인정되기가 진짜 어려울 게 보 이니",
          "speaker_id": "1",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023930.1.14",
          "form": "맞아요~ㅠ",
          "original_form": "맞아요~ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023930.1.15",
          "form": "계속 기저질환?",
          "original_form": "계속 기저질환?",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023930.1.16",
          "form": "이라고 하고 ㅠㅠ",
          "original_form": "이라고 하고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023930.1.17",
          "form": "그래서 더 불안해요~..",
          "original_form": "그래서 더 불안해요~..",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023930.1.18",
          "form": "누구나 다 기저질환이 있는데 ㅋㅋ 죽을만하니 죽었단 말만하니",
          "original_form": "누구나 다 기저질환이 있는데 ㅋㅋ 죽을만하니 죽었단 말만하니",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023930.1.19",
          "form": "그러니까요~~!!",
          "original_form": "그러니까요~~!!",
          "speaker_id": "2",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023930.1.20",
          "form": "백신 다 맞으라면서",
          "original_form": "백신 다 맞으라면서",
          "speaker_id": "2",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023930.1.21",
          "form": "그러면 어쩌자는건지... ㅠ",
          "original_form": "그러면 어쩌자는건지... ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023930.1.22",
          "form": "그런데 맞아야죠 뭐.. ㅋㅋ 큐ㅜㅠ",
          "original_form": "그런데 맞아야죠 뭐.. ㅋㅋ 큐ㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023930.1.23",
          "form": "맞긴 맞는데 불신상태인 딱 그정도 ㅋㅋㅋ",
          "original_form": "맞긴 맞는데 불신상태인 딱 그정도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100023930.1.24",
          "form": "ㅂㅐㄱ신 부작용도 가족이 투쟁하고 시위하고 청와대 청원해야 기사도나지",
          "original_form": "ㅂㅐㄱ신 부작용도 가족이 투쟁하고 시위하고 청와대 청원해야 기사도나지",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100023930.1.25",
          "form": "아니면 기사도 잘 안나요 ㅋ",
          "original_form": "아니면 기사도 잘 안나요 ㅋ",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100023930.1.26",
          "form": "음 맞아요~",
          "original_form": "음 맞아요~",
          "speaker_id": "2",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023930.1.27",
          "form": "이번 화이자 백신에",
          "original_form": "이번 화이자 백신에",
          "speaker_id": "2",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023930.1.28",
          "form": "생리에 관한 부작용도 많다고 인터넷에서 봤는데",
          "original_form": "생리에 관한 부작용도 많다고 인터넷에서 봤는데",
          "speaker_id": "2",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023930.1.29",
          "form": "뉴스에는 안 나오더라구요~",
          "original_form": "뉴스에는 안 나오더라구요~",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023930.1.30",
          "form": "외국선 보고가 많이 됐구요",
          "original_form": "외국선 보고가 많이 됐구요",
          "speaker_id": "1",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023930.1.31",
          "form": "한국에선 보고자체가 번거루워서 부작용 신고가 들어간 것 자체가 아직까진 없댔어요",
          "original_form": "한국에선 보고자체가 번거루워서 부작용 신고가 들어간 것 자체가 아직까진 없댔어요",
          "speaker_id": "1",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023930.1.32",
          "form": "그렇구나~",
          "original_form": "그렇구나~",
          "speaker_id": "2",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100023930.1.33",
          "form": "왜 번거롭게 만든건지 ㅠ",
          "original_form": "왜 번거롭게 만든건지 ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100023930.1.34",
          "form": "의문이네요..ㅠ",
          "original_form": "의문이네요..ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100023930.1.35",
          "form": "ㅋㅋ그래야 부작용 통계가 줄고",
          "original_form": "ㅋㅋ그래야 부작용 통계가 줄고",
          "speaker_id": "1",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100023930.1.36",
          "form": "보상해줄 필요도 없어서,라고 생각하면",
          "original_form": "보상해줄 필요도 없어서,라고 생각하면",
          "speaker_id": "1",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100023930.1.37",
          "form": "아니라고 할 사람이 많겠죠?ㅋㅋ",
          "original_form": "아니라고 할 사람이 많겠죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100023930.1.38",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100023930.1.39",
          "form": "그렇게 생각하는 사람 많을 것 같아요~",
          "original_form": "그렇게 생각하는 사람 많을 것 같아요~",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023930.1.40",
          "form": "계속 부작용 인정을 안해주니깐~",
          "original_form": "계속 부작용 인정을 안해주니깐~",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023930.1.41",
          "form": "ㅋㅋㅋㅋ ㅠㅠ 웃프네용",
          "original_form": "ㅋㅋㅋㅋ ㅠㅠ 웃프네용",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023930.1.42",
          "form": "전 다음뉴스를 ㅡ주로 보는데",
          "original_form": "전 다음뉴스를 ㅡ주로 보는데",
          "speaker_id": "1",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023930.1.43",
          "form": "거긴 문재인 지지자가 많다보니깐ㅋㅋ",
          "original_form": "거긴 문재인 지지자가 많다보니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023930.1.44",
          "form": "백신 이후 사망자는 다 죽을 사람이 죽은거라구 해서",
          "original_form": "백신 이후 사망자는 다 죽을 사람이 죽은거라구 해서",
          "speaker_id": "1",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023930.1.45",
          "form": "아 이거 부작용생겨도 인정 받을라면 국가말고 이 사람들이랑도 싸워야 되는구나 막막했어요",
          "original_form": "아 이거 부작용생겨도 인정 받을라면 국가말고 이 사람들이랑도 싸워야 되는구나 막막했어요",
          "speaker_id": "1",
          "time": "20210830 20:44"
        }
      ]
