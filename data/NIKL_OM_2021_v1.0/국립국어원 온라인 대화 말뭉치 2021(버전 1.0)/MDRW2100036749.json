      "utterance": [
        {
          "id": "MDRW2100036749.1.1",
          "form": "회사식당이유",
          "original_form": "회사식당이유",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100036749.1.2",
          "form": "뭐먹어",
          "original_form": "뭐먹어",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100036749.1.3",
          "form": "고민중임돠 메뉴모예요!  가봐야아나요",
          "original_form": "고민중임돠 메뉴모예요!  가봐야아나요",
          "speaker_id": "1",
          "time": "20210831 11:24"
        },
        {
          "id": "MDRW2100036749.1.4",
          "form": "식당 노맛이었어요",
          "original_form": "식당 노맛이었어요",
          "speaker_id": "2",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036749.1.5",
          "form": "맛없었어요?뭐였어요",
          "original_form": "맛없었어요?뭐였어요",
          "speaker_id": "1",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100036749.1.6",
          "form": "육개장 생선까스 비빔칼국수",
          "original_form": "육개장 생선까스 비빔칼국수",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100036749.1.7",
          "form": "뭐드셨나요",
          "original_form": "뭐드셨나요",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100036749.1.8",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036749.1.9",
          "form": "저커피우유!",
          "original_form": "저커피우유!",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036749.1.10",
          "form": "엥 그게다에요???",
          "original_form": "엥 그게다에요???",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036749.1.11",
          "form": "네 ! ..배고파디지겟어요",
          "original_form": "네 ! ..배고파디지겟어요",
          "speaker_id": "1",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036749.1.12",
          "form": "저도 좀 고프네여",
          "original_form": "저도 좀 고프네여",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036749.1.13",
          "form": "진 다빠짐",
          "original_form": "진 다빠짐",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036749.1.14",
          "form": "집갔어요?",
          "original_form": "집갔어요?",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100036749.1.15",
          "form": "아니유",
          "original_form": "아니유",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100036749.1.16",
          "form": "그래도내일은 재택",
          "original_form": "그래도내일은 재택",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100036749.1.17",
          "form": "여태못써서 3일 생긴건데 좀 놈팽이같네",
          "original_form": "여태못써서 3일 생긴건데 좀 놈팽이같네",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100036749.1.18",
          "form": "ㅋㅋㄱ놈팽ㅇ는무슨  일많이히ㅡ는거알고야",
          "original_form": "ㅋㅋㄱ놈팽ㅇ는무슨  일많이히ㅡ는거알고야",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100036749.1.19",
          "form": "저녁뭐먹어용",
          "original_form": "저녁뭐먹어용",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100036749.1.20",
          "form": "저녁 아마도 카레",
          "original_form": "저녁 아마도 카레",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100036749.1.21",
          "form": "카레요리사인가요",
          "original_form": "카레요리사인가요",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100036749.1.22",
          "form": "아 그러네요 ㅋㅋ",
          "original_form": "아 그러네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100036749.1.23",
          "form": "카레퀸입니다",
          "original_form": "카레퀸입니다",
          "speaker_id": "2",
          "time": "20210831 18:37"
        }
