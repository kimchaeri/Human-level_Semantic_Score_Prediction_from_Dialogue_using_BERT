      "utterance": [
        {
          "id": "MDRW2100029337.1.1",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "2",
          "time": "20210903 01:50"
        },
        {
          "id": "MDRW2100029337.1.2",
          "form": "백신 맞으셨나요??",
          "original_form": "백신 맞으셨나요??",
          "speaker_id": "2",
          "time": "20210903 01:50"
        },
        {
          "id": "MDRW2100029337.1.3",
          "form": "아직요 맞으샷어요?",
          "original_form": "아직요 맞으샷어요?",
          "speaker_id": "1",
          "time": "20210903 01:50"
        },
        {
          "id": "MDRW2100029337.1.4",
          "form": "저 수요일에 1차 맞았어요ㅎㅎ",
          "original_form": "저 수요일에 1차 맞았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:50"
        },
        {
          "id": "MDRW2100029337.1.5",
          "form": "맞고 오늘까지 쉬어서 그렇게 힘들지는 않더라고요",
          "original_form": "맞고 오늘까지 쉬어서 그렇게 힘들지는 않더라고요",
          "speaker_id": "2",
          "time": "20210903 01:51"
        },
        {
          "id": "MDRW2100029337.1.6",
          "form": "화이자요?",
          "original_form": "화이자요?",
          "speaker_id": "1",
          "time": "20210903 01:52"
        },
        {
          "id": "MDRW2100029337.1.7",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210903 01:52"
        },
        {
          "id": "MDRW2100029337.1.8",
          "form": "근육통 갠차낫어요?",
          "original_form": "근육통 갠차낫어요?",
          "speaker_id": "1",
          "time": "20210903 01:53"
        },
        {
          "id": "MDRW2100029337.1.9",
          "form": "근육통은 없었는데 왼팔이 아파서 왼쪽으로 누울 수가 없었고... 열이 37도정도 올랐어요. 그리고 그냥 힘이 좀 없었어요",
          "original_form": "근육통은 없었는데 왼팔이 아파서 왼쪽으로 누울 수가 없었고... 열이 37도정도 올랐어요. 그리고 그냥 힘이 좀 없었어요",
          "speaker_id": "2",
          "time": "20210903 01:55"
        },
        {
          "id": "MDRW2100029337.1.10",
          "form": "아아 ㅜㅜ 무서워용...",
          "original_form": "아아 ㅜㅜ 무서워용...",
          "speaker_id": "1",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100029337.1.11",
          "form": "왼팔아픈게 근육통이랑 다른느낌?",
          "original_form": "왼팔아픈게 근육통이랑 다른느낌?",
          "speaker_id": "1",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100029337.1.12",
          "form": "네 약간 멍들어서 아픈 느낌이에요",
          "original_form": "네 약간 멍들어서 아픈 느낌이에요",
          "speaker_id": "2",
          "time": "20210903 01:57"
        },
        {
          "id": "MDRW2100029337.1.13",
          "form": "백신 맞고 다음날 일정이 있으면 좀 힘들것같기는 해요",
          "original_form": "백신 맞고 다음날 일정이 있으면 좀 힘들것같기는 해요",
          "speaker_id": "2",
          "time": "20210903 01:58"
        },
        {
          "id": "MDRW2100029337.1.14",
          "form": "아아... 전 일부러 금욜로 잡아놓긴햇어요",
          "original_form": "아아... 전 일부러 금욜로 잡아놓긴햇어요",
          "speaker_id": "1",
          "time": "20210903 01:58"
        },
        {
          "id": "MDRW2100029337.1.15",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210903 01:58"
        },
        {
          "id": "MDRW2100029337.1.16",
          "form": "집에서 푹 쉬세용",
          "original_form": "집에서 푹 쉬세용",
          "speaker_id": "2",
          "time": "20210903 01:58"
        },
        {
          "id": "MDRW2100029337.1.17",
          "form": "그러려구요 추석때라 ㅋㅋ",
          "original_form": "그러려구요 추석때라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:59"
        },
        {
          "id": "MDRW2100029337.1.18",
          "form": "근데2차가 더 아프담서요..",
          "original_form": "근데2차가 더 아프담서요..",
          "speaker_id": "1",
          "time": "20210903 01:59"
        },
        {
          "id": "MDRW2100029337.1.19",
          "form": "그러더라고요....ㅠㅜ 좀 걱정돼요",
          "original_form": "그러더라고요....ㅠㅜ 좀 걱정돼요",
          "speaker_id": "2",
          "time": "20210903 01:59"
        },
        {
          "id": "MDRW2100029337.1.20",
          "form": "가족과함께사시나요?",
          "original_form": "가족과함께사시나요?",
          "speaker_id": "1",
          "time": "20210903 02:00"
        },
        {
          "id": "MDRW2100029337.1.21",
          "form": "혼자잇으면 백신맞기 더 무서울듯요",
          "original_form": "혼자잇으면 백신맞기 더 무서울듯요",
