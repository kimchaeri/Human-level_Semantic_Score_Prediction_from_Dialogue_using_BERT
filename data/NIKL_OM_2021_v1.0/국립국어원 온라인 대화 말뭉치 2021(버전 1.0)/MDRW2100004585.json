      "utterance": [
        {
          "id": "MDRW2100004585.1.1",
          "form": "운동 하시는거있나요?",
          "original_form": "운동 하시는거있나요?",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004585.1.2",
          "form": "운동 예전엔",
          "original_form": "운동 예전엔",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004585.1.3",
          "form": "축구 같은 구기종목들 자주했는데",
          "original_form": "축구 같은 구기종목들 자주했는데",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004585.1.4",
          "form": "요즘엔 코로나 때문에 홈트위주로 하는거 같아요 ㅎㅎ",
          "original_form": "요즘엔 코로나 때문에 홈트위주로 하는거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004585.1.5",
          "form": "다들 홈트 많이하시네요",
          "original_form": "다들 홈트 많이하시네요",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004585.1.6",
          "form": "전 게을러서 못하겠던데",
          "original_form": "전 게을러서 못하겠던데",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004585.1.7",
          "form": "저도 안하다가",
          "original_form": "저도 안하다가",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004585.1.8",
          "form": "확찐자가 되버린지라..ㅋㅋㅋㅋ",
          "original_form": "확찐자가 되버린지라..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004585.1.9",
          "form": "저도요 코로나가 절 이렇게 만들었죠",
          "original_form": "저도요 코로나가 절 이렇게 만들었죠",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004585.1.10",
          "form": "원래 마른 체형이었는데",
          "original_form": "원래 마른 체형이었는데",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004585.1.11",
          "form": "어느순간 찌기 시작한거에 놀랐어요",
          "original_form": "어느순간 찌기 시작한거에 놀랐어요",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004585.1.12",
          "form": "확실히 운동하구 움직여야되는것 같아요",
          "original_form": "확실히 운동하구 움직여야되는것 같아요",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004585.1.13",
          "form": "홈트면 어떤거 하시는거예요?",
          "original_form": "홈트면 어떤거 하시는거예요?",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004585.1.14",
          "form": "유튜브 보면서 주로 하는게",
          "original_form": "유튜브 보면서 주로 하는게",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004585.1.15",
          "form": "플랭크라던지 그런것도 하고",
          "original_form": "플랭크라던지 그런것도 하고",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004585.1.16",
          "form": "최근에 짐볼이용한 운동이랑",
          "original_form": "최근에 짐볼이용한 운동이랑",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004585.1.17",
          "form": "요가 조금 하고있네요~",
          "original_form": "요가 조금 하고있네요~",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004585.1.18",
          "form": "오 다양하게 하시네요",
          "original_form": "오 다양하게 하시네요",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004585.1.19",
          "form": "네네 처음엔 몇개로 시작해서",
          "original_form": "네네 처음엔 몇개로 시작해서",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004585.1.20",
          "form": "좀 운동을 다양하게 하네요",
          "original_form": "좀 운동을 다양하게 하네요",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004585.1.21",
          "form": "습관이 중요한것같아요",
          "original_form": "습관이 중요한것같아요",
          "speaker_id": "1",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004585.1.22",
          "form": "건강에도 좋으니 운동은~",
          "original_form": "건강에도 좋으니 운동은~",
          "speaker_id": "1",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004585.1.23",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004585.1.24",
          "form": "습관이 엄청 중요하죠",
          "original_form": "습관이 엄청 중요하죠",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004585.1.25",
          "form": "전 돈이라도 들여야 아까워서 가게되더라구요ㅎ",
          "original_form": "전 돈이라도 들여야 아까워서 가게되더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004585.1.26",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004585.1.27",
          "form": "헬스같은거 하시는건가용?",
          "original_form": "헬스같은거 하시는건가용?",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004585.1.28",
          "form": "순환피티라고 그룹으로 하는거 괜찮던데",
          "original_form": "순환피티라고 그룹으로 하는거 괜찮던데",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004585.1.29",
          "form": "맞아요! 그것도 괜찮더라고요",
          "original_form": "맞아요! 그것도 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004585.1.30",
          "form": "코로나땜에 못가겠어요",
          "original_form": "코로나땜에 못가겠어요",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004585.1.31",
          "form": "저도 헬스를 끊을려고 하다가",
          "original_form": "저도 헬스를 끊을려고 하다가",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004585.1.32",
          "form": "코로나 확산세가 너무 무서워서",
          "original_form": "코로나 확산세가 너무 무서워서",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004585.1.33",
          "form": "포기 ㅠㅠ",
          "original_form": "포기 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:27"
        }
      ]
    }
