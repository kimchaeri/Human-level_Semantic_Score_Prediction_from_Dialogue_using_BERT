      "utterance": [
        {
          "id": "MDRW2100011424.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100011424.1.2",
          "form": "네 키우죠!",
          "original_form": "네 키우죠!",
          "speaker_id": "2",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100011424.1.3",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011424.1.4",
          "form": "어떤건가요?",
          "original_form": "어떤건가요?",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011424.1.5",
          "form": "고양이요!",
          "original_form": "고양이요!",
          "speaker_id": "2",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011424.1.6",
          "form": "집사가 됬습니다 ㅎㅎ",
          "original_form": "집사가 됬습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011424.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011424.1.8",
          "form": "길냥이 인가요? 아님 다른경로로?",
          "original_form": "길냥이 인가요? 아님 다른경로로?",
          "speaker_id": "1",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011424.1.9",
          "form": "아버지 아는 친구분한테 1마리 분양받았어요!",
          "original_form": "아버지 아는 친구분한테 1마리 분양받았어요!",
          "speaker_id": "2",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100011424.1.10",
          "form": "무슨색인가요??",
          "original_form": "무슨색인가요??",
          "speaker_id": "1",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100011424.1.11",
          "form": "삼색인데",
          "original_form": "삼색인데",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100011424.1.12",
          "form": "흰색 갈색 검은색? 이에용",
          "original_form": "흰색 갈색 검은색? 이에용",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100011424.1.13",
          "form": "고양이는",
          "original_form": "고양이는",
          "speaker_id": "1",
          "time": "20210824 13:02"
        },
        {
          "id": "MDRW2100011424.1.14",
          "form": "하루에 몇시간자나요?",
          "original_form": "하루에 몇시간자나요?",
          "speaker_id": "1",
          "time": "20210824 13:02"
        },
        {
          "id": "MDRW2100011424.1.15",
          "form": "저희집냥이는",
          "original_form": "저희집냥이는",
          "speaker_id": "2",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100011424.1.16",
          "form": "좀 많이 자는거같긴해요",
          "original_form": "좀 많이 자는거같긴해요",
          "speaker_id": "2",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100011424.1.17",
          "form": "한 10시간?",
          "original_form": "한 10시간?",
          "speaker_id": "2",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100011424.1.18",
          "form": "주로 낮에 자나요 새벽에",
          "original_form": "주로 낮에 자나요 새벽에",
          "speaker_id": "1",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011424.1.19",
          "form": "자나요?",
          "original_form": "자나요?",
          "speaker_id": "1",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011424.1.20",
          "form": "새벽에도 자고 낮에도 자는데",
          "original_form": "새벽에도 자고 낮에도 자는데",
          "speaker_id": "2",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011424.1.21",
          "form": "새벽에 푹자고 낮에는 낮잠개념으로 좀 자는거같아용",
          "original_form": "새벽에 푹자고 낮에는 낮잠개념으로 좀 자는거같아용",
          "speaker_id": "2",
          "time": "20210824 13:10"
        }
      ]
