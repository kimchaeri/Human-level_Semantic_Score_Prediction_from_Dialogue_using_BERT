      "utterance": [
        {
          "id": "MDRW2100031041.1.1",
          "form": "쌀이랑 먹었니?",
          "original_form": "쌀이랑 먹었니?",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100031041.1.2",
          "form": "응 쌀이랑 같이 먹었어",
          "original_form": "응 쌀이랑 같이 먹었어",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100031041.1.3",
          "form": "국도 있었니",
          "original_form": "국도 있었니",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100031041.1.4",
          "form": "아니 국은 안 먹었어",
          "original_form": "아니 국은 안 먹었어",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031041.1.5",
          "form": "넌 따뜻한 밥 좋아하니",
          "original_form": "넌 따뜻한 밥 좋아하니",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031041.1.6",
          "form": "응 따뜻한 거 먹는게 좋아",
          "original_form": "응 따뜻한 거 먹는게 좋아",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031041.1.7",
          "form": "나도 따뜻한 밥이 너무좋아",
          "original_form": "나도 따뜻한 밥이 너무좋아",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100031041.1.8",
          "form": "넌 밥먹을때 반찬 많이 두고 먹니",
          "original_form": "넌 밥먹을때 반찬 많이 두고 먹니",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031041.1.9",
          "form": "많이가 여러 종류니 한 종류 다량이니",
          "original_form": "많이가 여러 종류니 한 종류 다량이니",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031041.1.10",
          "form": "여러종류로",
          "original_form": "여러종류로",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031041.1.11",
          "form": "난 한종류로 많이 먹는편이야",
          "original_form": "난 한종류로 많이 먹는편이야",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100031041.1.12",
          "form": "여러 종류로 두루두루 먹는 편인데 귀찮으면 한종류 많이 먹어",
          "original_form": "여러 종류로 두루두루 먹는 편인데 귀찮으면 한종류 많이 먹어",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031041.1.13",
          "form": "점심 뭐 먹었게",
          "original_form": "점심 뭐 먹었게",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031041.1.14",
          "form": "카레라이스 어때",
          "original_form": "카레라이스 어때",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031041.1.15",
          "form": "카레 맛있지",
          "original_form": "카레 맛있지",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031041.1.16",
          "form": "니 점심 맞춰볼게",
          "original_form": "니 점심 맞춰볼게",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031041.1.17",
          "form": "쌀 먹었니",
          "original_form": "쌀 먹었니",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031041.1.18",
