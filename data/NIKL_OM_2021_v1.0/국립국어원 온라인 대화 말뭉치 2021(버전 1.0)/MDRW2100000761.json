      "utterance": [
        {
          "id": "MDRW2100000761.1.1",
          "form": "전 아이돌 여럿 좋아했어요",
          "original_form": "전 아이돌 여럿 좋아했어요",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000761.1.2",
          "form": "최근까지는 엔시티인데 혹시 아시나요?",
          "original_form": "최근까지는 엔시티인데 혹시 아시나요?",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000761.1.3",
          "form": "네 들어봤어요!",
          "original_form": "네 들어봤어요!",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000761.1.4",
          "form": "회사가 sm 이였나?",
          "original_form": "회사가 sm 이였나?",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000761.1.5",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "1",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000761.1.6",
          "form": "한때 에스엠 가수들 좋아해서",
          "original_form": "한때 에스엠 가수들 좋아해서",
          "speaker_id": "1",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000761.1.7",
          "form": "취업도 에스엠으로 하고싶어했어요",
          "original_form": "취업도 에스엠으로 하고싶어했어요",
          "speaker_id": "1",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000761.1.8",
          "form": "와 진짜요?",
          "original_form": "와 진짜요?",
          "speaker_id": "2",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000761.1.9",
          "form": "그럼 직업이.. 예체능쪽?",
          "original_form": "그럼 직업이.. 예체능쪽?",
          "speaker_id": "2",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000761.1.10",
          "form": "아니요 현실은 다르더라구요",
          "original_form": "아니요 현실은 다르더라구요",
          "speaker_id": "1",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100000761.1.11",
          "form": "그냥 전공과 다른 길을 걷고있어요ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 전공과 다른 길을 걷고있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100000761.1.12",
          "form": "아하 그럼 어니쪽 희망하세요?",
          "original_form": "아하 그럼 어니쪽 희망하세요?",
          "speaker_id": "2",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100000761.1.13",
          "form": "어느쪽 희망하세요?",
          "original_form": "어느쪽 희망하세요?",
          "speaker_id": "2",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000761.1.14",
          "form": "그냥 사무직이에요,, 전공 어떤거세요?",
          "original_form": "그냥 사무직이에요,, 전공 어떤거세요?",
          "speaker_id": "1",
          "time": "20210819 19:24"
        },
        {
          "id": "MDRW2100000761.1.15",
          "form": "저는 인공지능입니다!",
          "original_form": "저는 인공지능입니다!",
          "speaker_id": "2",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000761.1.16",
          "form": "아직 전공과목을 배우지는 않았지만..",
          "original_form": "아직 전공과목을 배우지는 않았지만..",
          "speaker_id": "2",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000761.1.17",
          "form": "우와 되게 멋있으세요......",
          "original_form": "우와 되게 멋있으세요......",
          "speaker_id": "1",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000761.1.18",
          "form": "저랑 전공이 맞을지 걱정되네요..",
          "original_form": "저랑 전공이 맞을지 걱정되네요..",
          "speaker_id": "2",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000761.1.19",
          "form": "전공이랑 안맞으면 힘들지만",
          "original_form": "전공이랑 안맞으면 힘들지만",
          "speaker_id": "1",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000761.1.20",
          "form": "아직 안해보셨으니까 걱정마세요!",
          "original_form": "아직 안해보셨으니까 걱정마세요!",
          "speaker_id": "1",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000761.1.21",
          "form": "혹시 직장 다니세요..?",
          "original_form": "혹시 직장 다니세요..?",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000761.1.22",
