      "utterance": [
        {
          "id": "MDRW2100009583.1.1",
          "form": "이번 학기를 시작하며 다짐을 들어볼게요",
          "original_form": "이번 학기를 시작하며 다짐을 들어볼게요",
          "speaker_id": "1",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009583.1.2",
          "form": "나 도서관 근로하잖아",
          "original_form": "나 도서관 근로하잖아",
          "speaker_id": "2",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009583.1.3",
          "form": "우리 1학년때 기억남?",
          "original_form": "우리 1학년때 기억남?",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009583.1.4",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009583.1.5",
          "form": "약간?",
          "original_form": "약간?",
          "speaker_id": "1",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009583.1.6",
          "form": "도서관 1층에서 무슨 당첨 이벤트 한다고 사람 바글바글했던거",
          "original_form": "도서관 1층에서 무슨 당첨 이벤트 한다고 사람 바글바글했던거",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009583.1.7",
          "form": "왔다갔다 하다보면 가끔 그때가 떠오름",
          "original_form": "왔다갔다 하다보면 가끔 그때가 떠오름",
          "speaker_id": "2",
          "time": "20210823 01:05"
        },
        {
          "id": "MDRW2100009583.1.8",
          "form": "뭐였지?",
          "original_form": "뭐였지?",
          "speaker_id": "1",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009583.1.9",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "1",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009583.1.10",
          "form": "1학년 때 술 마시고",
          "original_form": "1학년 때 술 마시고",
          "speaker_id": "1",
          "time": "20210823 01:07"
        },
        {
          "id": "MDRW2100009583.1.11",
          "form": "1교시 제낀 기억 말고는 잘......",
          "original_form": "1교시 제낀 기억 말고는 잘......",
          "speaker_id": "1",
          "time": "20210823 01:08"
        },
        {
          "id": "MDRW2100009583.1.12",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210823 01:08"
        },
        {
          "id": "MDRW2100009583.1.13",
