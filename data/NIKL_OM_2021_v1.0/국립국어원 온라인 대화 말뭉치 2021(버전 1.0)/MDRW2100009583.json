          "original_form": "의미가 없어서",
          "speaker_id": "1",
          "time": "20210823 01:14"
        },
        {
          "id": "MDRW2100009583.1.40",
          "form": "결국 들을 거예요",
          "original_form": "결국 들을 거예요",
          "speaker_id": "1",
          "time": "20210823 01:14"
        },
        {
          "id": "MDRW2100009583.1.41",
          "form": "어떻게든",
          "original_form": "어떻게든",
          "speaker_id": "2",
          "time": "20210823 01:14"
        },
        {
          "id": "MDRW2100009583.1.42",
