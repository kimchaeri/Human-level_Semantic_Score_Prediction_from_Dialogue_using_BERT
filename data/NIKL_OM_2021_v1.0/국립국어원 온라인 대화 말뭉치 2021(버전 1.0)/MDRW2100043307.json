      "utterance": [
        {
          "id": "MDRW2100043307.1.1",
          "form": "굽네도 빨리 먹어야하는데",
          "original_form": "굽네도 빨리 먹어야하는데",
          "speaker_id": "2",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100043307.1.2",
          "form": "ㅋㅋㅋ저도 한참 남았어요...ㅠ",
          "original_form": "ㅋㅋㅋ저도 한참 남았어요...ㅠ",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100043307.1.3",
          "form": "까마득하네요 ㅋㅋㅋ",
          "original_form": "까마득하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100043307.1.4",
          "form": "그니깐요ㅜㅜ굽네 맛있을것같다ㅋㅋㄱ",
          "original_form": "그니깐요ㅜㅜ굽네 맛있을것같다ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043307.1.5",
          "form": "그것도 치킨이니깐요",
          "original_form": "그것도 치킨이니깐요",
          "speaker_id": "2",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043307.1.6",
          "form": "ㅋㅋ마블 9일까지니까 남은 날 동안 또 열심히 해봐야겠네요",
          "original_form": "ㅋㅋ마블 9일까지니까 남은 날 동안 또 열심히 해봐야겠네요",
          "speaker_id": "1",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100043307.1.7",
          "form": "그니칸요!!언젠가 또 당첨 될꺼라고 생각하면서 낼도 도전해봥야죵",
          "original_form": "그니칸요!!언젠가 또 당첨 될꺼라고 생각하면서 낼도 도전해봥야죵",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100043307.1.8",
          "form": "계속 꽝만 되다가 당첨되니깐 기분좋았어요",
          "original_form": "계속 꽝만 되다가 당첨되니깐 기분좋았어요",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100043307.1.9",
          "form": "ㅋㅋ담번엔 모찌되시길!!",
          "original_form": "ㅋㅋ담번엔 모찌되시길!!",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043307.1.10",
          "form": "모찌도 어떤 맛인지 궁금하네요",
          "original_form": "모찌도 어떤 맛인지 궁금하네요",
          "speaker_id": "2",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043307.1.11",
          "form": "빨리당첨돼봐야죵",
          "original_form": "빨리당첨돼봐야죵",
          "speaker_id": "2",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043307.1.12",
          "form": "맛이 여러갠데 골라서 갈 수 있나봐요",
          "original_form": "맛이 여러갠데 골라서 갈 수 있나봐요",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043307.1.13",
          "form": "크림치즈맛이랑 초코맛이랑 또 뭐가 있다던데요",
          "original_form": "크림치즈맛이랑 초코맛이랑 또 뭐가 있다던데요",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043307.1.14",
          "form": "아진짜요?우선당첨만 골고루 한개씩 다됐으면좋겠어요",
          "original_form": "아진짜요?우선당첨만 골고루 한개씩 다됐으면좋겠어요",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043307.1.15",
          "form": "저두요 매일 한 개씩 더 되서 종류별로 쓸어오길ㅋㅋ",
          "original_form": "저두요 매일 한 개씩 더 되서 종류별로 쓸어오길ㅋㅋ",
