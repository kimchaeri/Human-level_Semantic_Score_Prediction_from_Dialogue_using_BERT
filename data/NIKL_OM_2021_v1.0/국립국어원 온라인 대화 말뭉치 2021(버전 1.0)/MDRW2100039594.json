      "utterance": [
        {
          "id": "MDRW2100039594.1.1",
          "form": "니네랑 제주도도 가고 싶은데",
          "original_form": "니네랑 제주도도 가고 싶은데",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039594.1.2",
          "form": "가서 뭐할 건 딱히 없지만",
          "original_form": "가서 뭐할 건 딱히 없지만",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039594.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039594.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ언젠가는 가겠지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ언젠가는 가겠지",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039594.1.5",
          "form": "여행 가고 싶다",
          "original_form": "여행 가고 싶다",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039594.1.6",
          "form": "자꾸 심심이에서 여행 얘기하니 어쩔 수 없다",
          "original_form": "자꾸 심심이에서 여행 얘기하니 어쩔 수 없다",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039594.1.7",
          "form": "여행얘기만 한다발 하는거 같음",
          "original_form": "여행얘기만 한다발 하는거 같음",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039594.1.8",
          "form": "그러게 아니면 밥 먹었냔 얘기",
          "original_form": "그러게 아니면 밥 먹었냔 얘기",
          "speaker_id": "1",
          "time": "20210903 19:46"
        },
        {
          "id": "MDRW2100039594.1.9",
          "form": "맞아 식사 하셨어요로 하기 넘 만만함",
          "original_form": "맞아 식사 하셨어요로 하기 넘 만만함",
          "speaker_id": "2",
          "time": "20210903 19:46"
        },
        {
          "id": "MDRW2100039594.1.10",
          "form": "아침에 머 먹었냐고 물어보고 조금 얘기하고 잠수타더니",
          "original_form": "아침에 머 먹었냐고 물어보고 조금 얘기하고 잠수타더니",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039594.1.11",
          "form": "오후에 나와서 점심/저녁 먹었냐구 물아봐",
          "original_form": "오후에 나와서 점심/저녁 먹었냐구 물아봐",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039594.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039594.1.13",
          "form": "아깝다",
          "original_form": "아깝다",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039594.1.14",
          "form": "아침점심저녁으로 3개 제출하면 되는데",
          "original_form": "아침점심저녁으로 3개 제출하면 되는데",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039594.1.15",
          "form": "그니까 붙어있으면 3개하는데 바쁘신가보다...ㅎㅎㅎ",
          "original_form": "그니까 붙어있으면 3개하는데 바쁘신가보다...ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100039594.1.16",
          "form": "짜증나 또 차단당함ㅠ",
          "original_form": "짜증나 또 차단당함ㅠ",
          "speaker_id": "2",
          "time": "20210906 11:09"
        },
        {
          "id": "MDRW2100039594.1.17",
          "form": "나두 영문 모를 차단 하나 더 있어 ㅋㅋㅋ",
          "original_form": "나두 영문 모를 차단 하나 더 있어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 11:19"
        },
        {
          "id": "MDRW2100039594.1.18",
          "form": "저기 저는 왜 답장 안 해주세요?",
          "original_form": "저기 저는 왜 답장 안 해주세요?",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100039594.1.19",
          "form": "별로 할 말이 없어서",
          "original_form": "별로 할 말이 없어서",
          "speaker_id": "2",
          "time": "20210907 18:42"
        },
        {
          "id": "MDRW2100039594.1.20",
          "form": "여행에 관한 말 해줘 어디 갔더니 좋았다던가... 그런거라도...",
          "original_form": "여행에 관한 말 해줘 어디 갔더니 좋았다던가... 그런거라도...",
          "speaker_id": "1",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100039594.1.21",
          "form": "제주도에소 일주일 놀고싶다",
          "original_form": "제주도에소 일주일 놀고싶다",
          "speaker_id": "2",
