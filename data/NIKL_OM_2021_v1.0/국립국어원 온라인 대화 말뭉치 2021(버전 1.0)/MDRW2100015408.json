      "utterance": [
        {
          "id": "MDRW2100015408.1.1",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015408.1.2",
          "form": "보고 싶은 작품 있어?",
          "original_form": "보고 싶은 작품 있어?",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015408.1.3",
          "form": "딱히 없는데",
          "original_form": "딱히 없는데",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015408.1.4",
          "form": "난 보고 싶은 게 너무 많아",
          "original_form": "난 보고 싶은 게 너무 많아",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015408.1.5",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015408.1.6",
          "form": "프랑켄슈타인 레베카 위키드 킹키 부츠",
          "original_form": "프랑켄슈타인 레베카 위키드 킹키 부츠",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015408.1.7",
          "form": "오 많네",
          "original_form": "오 많네",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015408.1.8",
          "form": "응 본 것도 있고 안 본 것도 있고",
          "original_form": "응 본 것도 있고 안 본 것도 있고",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015408.1.9",
          "form": "젤 보고 싶은건?",
          "original_form": "젤 보고 싶은건?",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015408.1.10",
          "form": "딱 하나만 공짜로 보여준다고 하면 킹키 부츠",
          "original_form": "딱 하나만 공짜로 보여준다고 하면 킹키 부츠",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015408.1.11",
          "form": "무슨 내용이야?",
          "original_form": "무슨 내용이야?",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015408.1.12",
          "form": "내용이 좀 복잡한데 엄청 멋진 부츠에 관한 이야기야 유투브에 한번 찾아봐봐",
          "original_form": "내용이 좀 복잡한데 엄청 멋진 부츠에 관한 이야기야 유투브에 한번 찾아봐봐",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015408.1.13",
          "form": "찾아봐야겠다",
          "original_form": "찾아봐야겠다",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015408.1.14",
          "form": "응 스토리도 스토린데 노래랑 춤이 엄청 화려하고 멋있어",
          "original_form": "응 스토리도 스토린데 노래랑 춤이 엄청 화려하고 멋있어",
          "speaker_id": "1",
          "time": "20210825 23:06"
        }
