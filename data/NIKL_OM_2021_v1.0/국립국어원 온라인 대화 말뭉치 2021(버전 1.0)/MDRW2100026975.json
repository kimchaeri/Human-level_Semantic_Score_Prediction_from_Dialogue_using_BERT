      },
      "utterance": [
        {
          "id": "MDRW2100026975.1.1",
          "form": "나 체했는지 머리도 아프고 속도 안 좋아..",
          "original_form": "나 체했는지 머리도 아프고 속도 안 좋아..",
          "speaker_id": "2",
          "time": "20210902 02:19"
        },
        {
          "id": "MDRW2100026975.1.2",
          "form": "아이고 어째. 약은 먹었고?",
          "original_form": "아이고 어째. 약은 먹었고?",
          "speaker_id": "1",
          "time": "20210902 02:19"
        },
        {
          "id": "MDRW2100026975.1.3",
          "form": "응 타이레놀 두 알 먹었어",
          "original_form": "응 타이레놀 두 알 먹었어",
          "speaker_id": "2",
          "time": "20210902 02:19"
        },
        {
          "id": "MDRW2100026975.1.4",
          "form": "약 효과는 드는 것 같아?",
          "original_form": "약 효과는 드는 것 같아?",
          "speaker_id": "1",
          "time": "20210902 02:19"
        },
        {
          "id": "MDRW2100026975.1.5",
          "form": "아니 별로 효과 없는 것 같어",
          "original_form": "아니 별로 효과 없는 것 같어",
          "speaker_id": "2",
          "time": "20210902 02:20"
        },
        {
          "id": "MDRW2100026975.1.6",
          "form": "계속 아파",
          "original_form": "계속 아파",
          "speaker_id": "2",
          "time": "20210902 02:20"
        },
        {
          "id": "MDRW2100026975.1.7",
          "form": "머리 말고 더 아픈 덴 없고?",
          "original_form": "머리 말고 더 아픈 덴 없고?",
          "speaker_id": "1",
          "time": "20210902 02:20"
        },
        {
          "id": "MDRW2100026975.1.8",
          "form": "온 몸에 근육통도 있는 것 같아",
          "original_form": "온 몸에 근육통도 있는 것 같아",
          "speaker_id": "2",
          "time": "20210902 02:20"
        },
        {
          "id": "MDRW2100026975.1.9",
          "form": "일어났더니 팔 다리가 아프네",
          "original_form": "일어났더니 팔 다리가 아프네",
          "speaker_id": "2",
          "time": "20210902 02:20"
        },
        {
          "id": "MDRW2100026975.1.10",
          "form": "몸살감기인가. 내일 병원 가봐야겠다",
          "original_form": "몸살감기인가. 내일 병원 가봐야겠다",
          "speaker_id": "1",
          "time": "20210902 02:21"
        },
        {
          "id": "MDRW2100026975.1.11",
          "form": "글쎄... 열은 안 나는데 머리가 너무 아프니까 아무것도 못하겠네",
          "original_form": "글쎄... 열은 안 나는데 머리가 너무 아프니까 아무것도 못하겠네",
          "speaker_id": "2",
          "time": "20210902 02:22"
        },
        {
          "id": "MDRW2100026975.1.12",
          "form": "체한 것 같으면 속청도 먹어봐",
          "original_form": "체한 것 같으면 속청도 먹어봐",
          "speaker_id": "1",
          "time": "20210902 02:22"
        },
        {
          "id": "MDRW2100026975.1.13",
          "form": "어 집에 소화제는 없어서 그거라도 먹어볼게",
          "original_form": "어 집에 소화제는 없어서 그거라도 먹어볼게",
          "speaker_id": "2",
          "time": "20210902 02:22"
        },
        {
          "id": "MDRW2100026975.1.14",
          "form": "그래. 새벽에라도 심하게 아프면 전화해. 응급실이라도 가게",
          "original_form": "그래. 새벽에라도 심하게 아프면 전화해. 응급실이라도 가게",
          "speaker_id": "1",
          "time": "20210902 02:23"
        },
        {
          "id": "MDRW2100026975.1.15",
          "form": "그정도로 아픈 건 아니구..병원 가기는 싫으네",
          "original_form": "그정도로 아픈 건 아니구..병원 가기는 싫으네",
          "speaker_id": "2",
          "time": "20210902 02:24"
        },
        {
          "id": "MDRW2100026975.1.16",
          "form": "할머니 병원 다녀 오셨어?",
          "original_form": "할머니 병원 다녀 오셨어?",
          "speaker_id": "2",
