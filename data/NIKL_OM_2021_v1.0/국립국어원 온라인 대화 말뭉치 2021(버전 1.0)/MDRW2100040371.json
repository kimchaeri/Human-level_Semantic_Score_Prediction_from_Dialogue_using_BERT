      "utterance": [
        {
          "id": "MDRW2100040371.1.1",
          "form": "암튼 그 오배송된 회는 어떻게 됐을까요",
          "original_form": "암튼 그 오배송된 회는 어떻게 됐을까요",
          "speaker_id": "1",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100040371.1.2",
          "form": "어찌찾으셨나요",
          "original_form": "어찌찾으셨나요",
          "speaker_id": "2",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100040371.1.3",
          "form": "설마못찾..?!",
          "original_form": "설마못찾..?!",
          "speaker_id": "2",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040371.1.4",
          "form": "멍게도 있는 세트라 호불호 갈릴텐데",
          "original_form": "멍게도 있는 세트라 호불호 갈릴텐데",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040371.1.5",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040371.1.6",
          "form": "아뇨 배민서 재조리해 준다길래",
          "original_form": "아뇨 배민서 재조리해 준다길래",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040371.1.7",
          "form": "오배송된거 찾아 주셔도 된다 이랬는데",
          "original_form": "오배송된거 찾아 주셔도 된다 이랬는데",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040371.1.8",
          "form": "한 오분뒤 라이더랑 연결이 안된다고ㅎ",
          "original_form": "한 오분뒤 라이더랑 연결이 안된다고ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100040371.1.9",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100040371.1.10",
          "form": "그냥 재조리해 재배달을 권하더라고요",
          "original_form": "그냥 재조리해 재배달을 권하더라고요",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100040371.1.11",
          "form": "저야 상관없는데",
          "original_form": "저야 상관없는데",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100040371.1.12",
          "form": "제가 시킨게 치킨이나 피자도 아니잖아요",
          "original_form": "제가 시킨게 치킨이나 피자도 아니잖아요",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100040371.1.13",
          "form": "그쵸;-;",
          "original_form": "그쵸;-;",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100040371.1.14",
          "form": "호옥시 회 못먹는 사람 집 앞에 놓인 회 멍게 배달음식...",
          "original_form": "호옥시 회 못먹는 사람 집 앞에 놓인 회 멍게 배달음식...",
          "speaker_id": "1",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040371.1.15",
          "form": "이렇게 되면",
          "original_form": "이렇게 되면",
          "speaker_id": "1",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040371.1.16",
          "form": "헐 ㅋㅋㅋㅋ",
          "original_form": "헐 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040371.1.17",
          "form": "name2님 집 앞에",
          "original_form": "&name2&님 집 앞에",
          "speaker_id": "1",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040371.1.18",
          "form": "그리되면정말난감ㅋㅋㅋㅋ",
          "original_form": "그리되면정말난감ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040371.1.19",
          "form": "멍게 봉지가 놓였다 생각해봐요",
          "original_form": "멍게 봉지가 놓였다 생각해봐요",
          "speaker_id": "1",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040371.1.20",
          "form": "다음날 아침에 발견ㅎㅎ",
          "original_form": "다음날 아침에 발견ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040371.1.21",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100040371.1.22",
          "form": "ㄱㄱㅈㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱ",
          "original_form": "ㄱㄱㅈㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100040371.1.23",
          "form": "상상을해보았습니다..",
          "original_form": "상상을해보았습니다..",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100040371.1.24",
          "form": "음쓰를우리집앞에둔거나다름이없습니다..",
          "original_form": "음쓰를우리집앞에둔거나다름이없습니다..",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100040371.1.25",
          "form": "ㅎ.. 바로 그거죠!",
          "original_form": "ㅎ.. 바로 그거죠!",
          "speaker_id": "1",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100040371.1.26",
          "form": "그 라이더가 과연 시간 버려가며 그것을 찾아갔을지..",
          "original_form": "그 라이더가 과연 시간 버려가며 그것을 찾아갔을지..",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100040371.1.27",
          "form": "안 그럴 것 같아요ㅜ",
          "original_form": "안 그럴 것 같아요ㅜ",
          "speaker_id": "1",
          "time": "20210904 17:23"
        }
      ]
