      "utterance": [
        {
          "id": "MDRW2100023079.1.1",
          "form": "ㅇㅇ그니까",
          "original_form": "ㅇㅇ그니까",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023079.1.2",
          "form": "지금 티비에 보이는 개그맨이",
          "original_form": "지금 티비에 보이는 개그맨이",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023079.1.3",
          "form": "마지막 개그맨일듯",
          "original_form": "마지막 개그맨일듯",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023079.1.4",
          "form": "언제적 개그맨들이야 ㅋㅋㅋ",
          "original_form": "언제적 개그맨들이야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023079.1.5",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ사실",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ사실",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023079.1.6",
          "form": "나 어릴때 보던 얼굴들이 계속 나옴",
          "original_form": "나 어릴때 보던 얼굴들이 계속 나옴",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023079.1.7",
          "form": "극소수의 개그우먼 제외하곤",
          "original_form": "극소수의 개그우먼 제외하곤",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.8",
          "form": "프로그램 사라질때",
          "original_form": "프로그램 사라질때",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.9",
          "form": "같이 사라졌어도",
          "original_form": "같이 사라졌어도",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.10",
          "form": "무방한듯",
          "original_form": "무방한듯",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.11",
          "form": "ㅇㅇㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.12",
          "form": "누가 시키는 것도 아니고",
          "original_form": "누가 시키는 것도 아니고",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.13",
          "form": "그 개그도 컨텐츠라고 자기들이 짜오는건데",
          "original_form": "그 개그도 컨텐츠라고 자기들이 짜오는건데",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.14",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.15",
          "form": "그런 내용을 생각해오는건 걍",
          "original_form": "그런 내용을 생각해오는건 걍",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.16",
          "form": "미디어 공부가 부족한거지",
          "original_form": "미디어 공부가 부족한거지",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023079.1.17",
          "form": "그리고 다들 재미도 없음",
          "original_form": "그리고 다들 재미도 없음",
          "speaker_id": "2",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023079.1.18",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023079.1.19",
          "form": "개콘 흥행하던 시기랑",
          "original_form": "개콘 흥행하던 시기랑",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023079.1.20",
          "form": "벌써 10년이 지났는데",
          "original_form": "벌써 10년이 지났는데",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023079.1.21",
          "form": "관객이 바뀌었는데 했던 개그 또 우려먹으려고 함",
          "original_form": "관객이 바뀌었는데 했던 개그 또 우려먹으려고 함",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023079.1.22",
          "form": "ㅇㅇ그리고 했던거 우리는거라",
          "original_form": "ㅇㅇ그리고 했던거 우리는거라",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023079.1.23",
          "form": "그때보다도 노잼이야",
          "original_form": "그때보다도 노잼이야",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023079.1.24",
          "form": "ㅇㅇ 참신함조차 없음",
          "original_form": "ㅇㅇ 참신함조차 없음",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023079.1.25",
          "form": "간혹 인터넷에 유행하는거",
          "original_form": "간혹 인터넷에 유행하는거",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023079.1.26",
          "form": "자기가 처음만든것처럼 쓰는거 보면",
          "original_form": "자기가 처음만든것처럼 쓰는거 보면",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023079.1.27",
          "form": "경멸스러울지경",
          "original_form": "경멸스러울지경",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023079.1.28",
          "form": "ㅇㅇ 그래서 솔직히 요즘은",
          "original_form": "ㅇㅇ 그래서 솔직히 요즘은",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023079.1.29",
          "form": "웃긴거 보려면 개그프로보다는",
          "original_form": "웃긴거 보려면 개그프로보다는",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023079.1.30",
          "form": "인터넷 게시글을 더 보는듯",
          "original_form": "인터넷 게시글을 더 보는듯",
          "speaker_id": "1",
          "time": "20210829 21:33"
        }
      ]
    }
