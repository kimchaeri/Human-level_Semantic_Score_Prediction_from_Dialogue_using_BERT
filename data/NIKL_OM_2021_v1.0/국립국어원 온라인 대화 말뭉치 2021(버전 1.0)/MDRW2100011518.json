      "utterance": [
        {
          "id": "MDRW2100011518.1.1",
          "form": "혹시칵테일좋아하시나요",
          "original_form": "혹시칵테일좋아하시나요",
          "speaker_id": "2",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100011518.1.2",
          "form": "없어서 못먹죠ㅎㅎ",
          "original_form": "없어서 못먹죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011518.1.3",
          "form": "칵테일 어떤거 드시나요",
          "original_form": "칵테일 어떤거 드시나요",
          "speaker_id": "1",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011518.1.4",
          "form": "저는깔끔한거좋아해요",
          "original_form": "저는깔끔한거좋아해요",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011518.1.5",
          "form": "진토닉 같은거용",
          "original_form": "진토닉 같은거용",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011518.1.6",
          "form": "저는 어제 칵테일먹고 토했어요ㅎㅎ",
          "original_form": "저는 어제 칵테일먹고 토했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100011518.1.7",
          "form": "아니얼마나드셧어요ㅋㅋ",
          "original_form": "아니얼마나드셧어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100011518.1.8",
          "form": "6잔의 칵테일이요",
          "original_form": "6잔의 칵테일이요",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100011518.1.9",
          "form": "대단합니다",
          "original_form": "대단합니다",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100011518.1.10",
          "form": "무슨칵테일드셧나요",
          "original_form": "무슨칵테일드셧나요",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100011518.1.11",
          "form": "많아서 기억은 안나지만 깔루아밀크마시는데 커피콩이 씹히더라고요ㅋㅋ",
          "original_form": "많아서 기억은 안나지만 깔루아밀크마시는데 커피콩이 씹히더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100011518.1.12",
          "form": "오우제대로네요",
          "original_form": "오우제대로네요",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100011518.1.13",
          "form": "달달하고맛잇어요",
          "original_form": "달달하고맛잇어요",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100011518.1.14",
          "form": "네 다음엔 블랙러시안이라는 칵테일 도전하려고요",
          "original_form": "네 다음엔 블랙러시안이라는 칵테일 도전하려고요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100011518.1.15",
          "form": "그거엄청독할걸요",
          "original_form": "그거엄청독할걸요",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100011518.1.16",
          "form": "칵테일고수이신가봐요",
          "original_form": "칵테일고수이신가봐요",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100011518.1.17",
          "form": "멋있어요",
          "original_form": "멋있어요",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100011518.1.18",
          "form": "칵테일 입문자입니다",
          "original_form": "칵테일 입문자입니다",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100011518.1.19",
          "form": "알쓰라ㅎㅎ",
          "original_form": "알쓰라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100011518.1.20",
          "form": "칵테일추천좀",
          "original_form": "칵테일추천좀",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100011518.1.21",
          "form": "저도입문자요",
          "original_form": "저도입문자요",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100011518.1.22",
          "form": "애플마티니라는 칵테일이 어울리실듯요",
          "original_form": "애플마티니라는 칵테일이 어울리실듯요",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100011518.1.23",
          "form": "말만들어도시큼하네요",
          "original_form": "말만들어도시큼하네요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        }
      ]
