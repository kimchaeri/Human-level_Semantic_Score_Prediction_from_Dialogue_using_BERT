        },
        {
          "id": "MDRW2100008518.1.2",
          "form": "무슨음식좋아하세요?",
          "original_form": "무슨음식좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008518.1.3",
          "form": "저는 치킨을 좋아합니다",
          "original_form": "저는 치킨을 좋아합니다",
          "speaker_id": "1",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008518.1.4",
          "form": "점심식사는 하셨나요?",
          "original_form": "점심식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008518.1.5",
          "form": "저도치킨먹었어요",
          "original_form": "저도치킨먹었어요",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008518.1.6",
          "form": "무슨 치킨 드셨나요?",
          "original_form": "무슨 치킨 드셨나요?",
          "speaker_id": "1",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008518.1.7",
          "form": "처갓집치킨이요",
          "original_form": "처갓집치킨이요",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008518.1.8",
          "form": "어떤메뉴가 유명한가요?",
          "original_form": "어떤메뉴가 유명한가요?",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008518.1.9",
          "form": "슈프림양념치킨입니다",
          "original_form": "슈프림양념치킨입니다",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008518.1.10",
          "form": "저는 비비큐 좋아해요",
          "original_form": "저는 비비큐 좋아해요",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008518.1.11",
          "form": "비비큐무슨치킨이맛있나요?",
          "original_form": "비비큐무슨치킨이맛있나요?",
