      "utterance": [
        {
          "id": "MDRW2100033541.1.1",
          "form": "오 저 망했어요 ㅋㅋ",
          "original_form": "오 저 망했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100033541.1.2",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100033541.1.3",
          "form": "먼일있으세요?",
          "original_form": "먼일있으세요?",
          "speaker_id": "1",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100033541.1.4",
          "form": "다이어트 어제 시작해놓고",
          "original_form": "다이어트 어제 시작해놓고",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100033541.1.5",
          "form": "오늘 햄버거 묵음요 ㅋㅋ",
          "original_form": "오늘 햄버거 묵음요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100033541.1.6",
          "form": "ㅋㅋㅋ 점심에 드신거예요?",
          "original_form": "ㅋㅋㅋ 점심에 드신거예요?",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100033541.1.7",
          "form": "그럼 괜찮아요 ㅎ",
          "original_form": "그럼 괜찮아요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100033541.1.8",
          "form": "지금부터 굶으시면되쥬 ㅋ",
          "original_form": "지금부터 굶으시면되쥬 ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100033541.1.9",
          "form": "점심도 먹고 버거도 먹고 ㅋㅋ",
          "original_form": "점심도 먹고 버거도 먹고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100033541.1.10",
          "form": "와 일부러 집가서 점심 먹었는데ㅜ",
          "original_form": "와 일부러 집가서 점심 먹었는데ㅜ",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100033541.1.11",
          "form": "헉...점심먹고 버거추가요?",
          "original_form": "헉...점심먹고 버거추가요?",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100033541.1.12",
          "form": "점심 연두부랑 토마토랑 바나나랑 삶은 계란 먹었어요ㅜ",
          "original_form": "점심 연두부랑 토마토랑 바나나랑 삶은 계란 먹었어요ㅜ",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100033541.1.13",
          "form": "그래도 점심은 나름 성공적인데",
          "original_form": "그래도 점심은 나름 성공적인데",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033541.1.14",
          "form": "버거가...좀 그렇긴하네요",
          "original_form": "버거가...좀 그렇긴하네요",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033541.1.15",
          "form": "이미 먹은거 어쩌겠어요",
          "original_form": "이미 먹은거 어쩌겠어요",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033541.1.16",
          "form": "노브랜드 버거는 맛있었습니다 허허허",
          "original_form": "노브랜드 버거는 맛있었습니다 허허허",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100033541.1.17",
          "form": "앗...노브랜드꺼였군요",
          "original_form": "앗...노브랜드꺼였군요",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100033541.1.18",
          "form": "지날치수가 없으셨을듯요 ㅎ",
          "original_form": "지날치수가 없으셨을듯요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100033541.1.19",
          "form": "안그래도 궁금했었거든요ㅜ",
          "original_form": "안그래도 궁금했었거든요ㅜ",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100033541.1.20",
          "form": "감자튀김 맛있었어요ㅜㅜ",
          "original_form": "감자튀김 맛있었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033541.1.21",
          "form": "2천원의행복",
          "original_form": "2천원의행복",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033541.1.22",
          "form": "버거는 1900원",
          "original_form": "버거는 1900원",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033541.1.23",
          "form": "3900원에 또 졌다며 ㅋㅋㅋ",
          "original_form": "3900원에 또 졌다며 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033541.1.24",
          "form": "가성비값이쥬",
          "original_form": "가성비값이쥬",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033541.1.25",
          "form": "노브랜드는 사랑이예요",
          "original_form": "노브랜드는 사랑이예요",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033541.1.26",
          "form": "이제 진짜 집밥만 먹으려고요ㅜㅜ",
          "original_form": "이제 진짜 집밥만 먹으려고요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100033541.1.27",
          "form": "오늘로 진짜 빠이 안녕 밀가루 ㅋㅋ",
          "original_form": "오늘로 진짜 빠이 안녕 밀가루 ㅋㅋ",
