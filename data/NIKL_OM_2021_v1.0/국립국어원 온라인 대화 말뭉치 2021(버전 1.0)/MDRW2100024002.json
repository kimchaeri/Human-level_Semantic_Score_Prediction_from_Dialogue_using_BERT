      "utterance": [
        {
          "id": "MDRW2100024002.1.1",
          "form": "프랑스 요리중엔",
          "original_form": "프랑스 요리중엔",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100024002.1.2",
          "form": "오리를 오리 기름에 튀기는 요리도 있어서",
          "original_form": "오리를 오리 기름에 튀기는 요리도 있어서",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100024002.1.3",
          "form": "수입 식자재중에 오리기름도 있어요",
          "original_form": "수입 식자재중에 오리기름도 있어요",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100024002.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 오리가 기름이 많긴 한데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 오리가 기름이 많긴 한데",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100024002.1.5",
          "form": "왜죠 왜 오리를 오리기름에 튀기죠",
          "original_form": "왜죠 왜 오리를 오리기름에 튀기죠",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100024002.1.6",
          "form": "이유가 있나요 오리기름만의 풍미?",
          "original_form": "이유가 있나요 오리기름만의 풍미?",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100024002.1.7",
          "form": "사실 소기름도 이해가 안 가긴 해요...",
          "original_form": "사실 소기름도 이해가 안 가긴 해요...",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100024002.1.8",
          "form": "소기름 이해가 가실텐데",
          "original_form": "소기름 이해가 가실텐데",
          "speaker_id": "2",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100024002.1.9",
          "form": "육개장위에 둥둥 뚜는 기름이 소기름이어야",
          "original_form": "육개장위에 둥둥 뚜는 기름이 소기름이어야",
          "speaker_id": "2",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100024002.1.10",
          "form": "국 먹었을때 입가에 번드르르한 기름이 소맛이나죠",
          "original_form": "국 먹었을때 입가에 번드르르한 기름이 소맛이나죠",
          "speaker_id": "2",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100024002.1.11",
          "form": "육개장위에 콩기름이 떠봐요",
          "original_form": "육개장위에 콩기름이 떠봐요",
          "speaker_id": "2",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100024002.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 그게 소기름이었군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 그게 소기름이었군요",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100024002.1.13",
          "form": "근데 그거 일부러 넣은 기름이예요??",
          "original_form": "근데 그거 일부러 넣은 기름이예요??",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100024002.1.14",
          "form": "소고기 넣어서 자연스럽게 뜨는 기름이 아니라?",
          "original_form": "소고기 넣어서 자연스럽게 뜨는 기름이 아니라?",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100024002.1.15",
          "form": "기름많은 소고기를 많이 넣음 자연스럽게 뜨구요",
          "original_form": "기름많은 소고기를 많이 넣음 자연스럽게 뜨구요",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024002.1.16",
          "form": "고기가 좀 부족하면 소기름맛이 적을 수 있죠",
          "original_form": "고기가 좀 부족하면 소기름맛이 적을 수 있죠",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024002.1.17",
          "form": "탕수육도 돼지고기나 소기름을 좀 넣어 튀기면 고기풍미거 더살아요",
          "original_form": "탕수육도 돼지고기나 소기름을 좀 넣어 튀기면 고기풍미거 더살아요",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024002.1.18",
          "form": "우와 그건 몰랐어요ㅎㅎ",
          "original_form": "우와 그건 몰랐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024002.1.19",
          "form": "하긴 쓰는 기름 따라 맛이 달라지긴 하죠...",
          "original_form": "하긴 쓰는 기름 따라 맛이 달라지긴 하죠...",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024002.1.20",
          "form": "전 저렴한 거나 세일하는 상품을 쓰지만요ㅎㅎ",
          "original_form": "전 저렴한 거나 세일하는 상품을 쓰지만요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100024002.1.21",
          "form": "카놀라 해바라기 이런건 특유의 향은 거의 없어요",
          "original_form": "카놀라 해바라기 이런건 특유의 향은 거의 없어요",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024002.1.22",
          "form": "근데 돼지나 소는 향이 있어요",
          "original_form": "근데 돼지나 소는 향이 있어요",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024002.1.23",
          "form": "마저요ㅋㅋㅋ",
          "original_form": "마저요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024002.1.24",
          "form": "전 향 진한 건 싫어서 카놀라나 해바라기도 좋아요",
          "original_form": "전 향 진한 건 싫어서 카놀라나 해바라기도 좋아요",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024002.1.25",
          "form": "콩기름은 살짝 콩비린내가 나는데",
          "original_form": "콩기름은 살짝 콩비린내가 나는데",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100024002.1.26",
          "form": "기름에 절인 전이 아닌이상 딱히 향은 덜나는?",
          "original_form": "기름에 절인 전이 아닌이상 딱히 향은 덜나는?",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024002.1.27",
          "form": "아... 기름 축축한 전에서 나는 냄새 그거 알죠...",
          "original_form": "아... 기름 축축한 전에서 나는 냄새 그거 알죠...",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024002.1.28",
          "form": "괴롭죠...",
          "original_form": "괴롭죠...",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024002.1.29",
          "form": "그건 콩기름만의 냄새는 아닐수도 있어요",
          "original_form": "그건 콩기름만의 냄새는 아닐수도 있어요",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024002.1.30",
          "form": "기름으로 요리하구 그냥 두면 기름이 산패해서 불쾌한 맛을 내기두,,",
          "original_form": "기름으로 요리하구 그냥 두면 기름이 산패해서 불쾌한 맛을 내기두,,",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024002.1.31",
          "form": "콩기름의 특이취는 콩비린내에 가가워요",
          "original_form": "콩기름의 특이취는 콩비린내에 가가워요",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024002.1.32",
          "form": "ㅋㅋㅋ 넹 저희 집 제사 지낼 때 전은 걍 사다 쓰거든요",
          "original_form": "ㅋㅋㅋ 넹 저희 집 제사 지낼 때 전은 걍 사다 쓰거든요",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024002.1.33",
          "form": "콩비린내... 두유에서 나는 냄새 말하는 거죠? 그거 싫어서 두유도 안 먹는ㅋㅋㅋ",
          "original_form": "콩비린내... 두유에서 나는 냄새 말하는 거죠? 그거 싫어서 두유도 안 먹는ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024002.1.34",
          "form": "움 그럼 콩기름 안맞을수도 있어요",
          "original_form": "움 그럼 콩기름 안맞을수도 있어요",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100024002.1.35",
          "form": "근데 콩기름은 저렴하니깐 ㅋㅋ",
          "original_form": "근데 콩기름은 저렴하니깐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100024002.1.36",
          "form": "한 번 드셔보세요",
          "original_form": "한 번 드셔보세요",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100024002.1.37",
          "form": "고소한 맛도 있어요",
          "original_form": "고소한 맛도 있어요",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024002.1.38",
          "form": "ㅋㅋㅋ 마트에서 카놀라유 세일을 많이해서",
          "original_form": "ㅋㅋㅋ 마트에서 카놀라유 세일을 많이해서",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024002.1.39",
          "form": "요샌 카놀라유 써요",
          "original_form": "요샌 카놀라유 써요",
          "speaker_id": "1",
          "time": "20210830 21:21"
        }
      ]
    }
