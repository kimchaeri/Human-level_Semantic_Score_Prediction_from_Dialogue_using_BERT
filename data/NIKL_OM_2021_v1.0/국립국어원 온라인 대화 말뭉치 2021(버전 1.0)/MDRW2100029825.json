        {
          "id": "MDRW2100029825.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100029825.1.2",
          "form": "네 좋아해요 코로나 끝나면 가고싶네요",
          "original_form": "네 좋아해요 코로나 끝나면 가고싶네요",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100029825.1.3",
          "form": "가보셨던 곳 중에 어디가 제일 좋았어요?",
          "original_form": "가보셨던 곳 중에 어디가 제일 좋았어요?",
          "speaker_id": "2",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029825.1.4",
          "form": "호주여",
          "original_form": "호주여",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029825.1.5",
          "form": "저도 가보고 싶네요",
          "original_form": "저도 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029825.1.6",
          "form": "청정지역이더라구요 정말",
          "original_form": "청정지역이더라구요 정말",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029825.1.7",
          "form": "어떤 점이 제일 좋으셨나요?",
          "original_form": "어떤 점이 제일 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029825.1.8",
          "form": "오페라하우스 갔던게 정말 좋았어요",
          "original_form": "오페라하우스 갔던게 정말 좋았어요",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029825.1.9",
          "form": "오 저도 가보고싶어요",
          "original_form": "오 저도 가보고싶어요",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029825.1.10",
          "form": "멋지더라구요",
          "original_form": "멋지더라구요",
          "speaker_id": "1",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100029825.1.11",
          "form": "호주 담에 기회되면 가봐야겠어요",
          "original_form": "호주 담에 기회되면 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100029825.1.12",
          "form": "강추요",
          "original_form": "강추요",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100029825.1.13",
          "form": "네! 가봐야겠네요",
          "original_form": "네! 가봐야겠네요",
