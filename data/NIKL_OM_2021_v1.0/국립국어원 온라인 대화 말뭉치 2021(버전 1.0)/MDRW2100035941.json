      "utterance": [
        {
          "id": "MDRW2100035941.1.1",
          "form": "네 가끔 마셔요~",
          "original_form": "네 가끔 마셔요~",
          "speaker_id": "2",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100035941.1.2",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100035941.1.3",
          "form": "저도 오늘 소이라떼 해먹었어요!",
          "original_form": "저도 오늘 소이라떼 해먹었어요!",
          "speaker_id": "1",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100035941.1.4",
          "form": "맛있으셨겠어요~",
          "original_form": "맛있으셨겠어요~",
          "speaker_id": "2",
          "time": "20210830 12:30"
        },
        {
          "id": "MDRW2100035941.1.5",
          "form": "자주 해서 드시나요?",
          "original_form": "자주 해서 드시나요?",
          "speaker_id": "2",
          "time": "20210830 12:30"
        },
        {
          "id": "MDRW2100035941.1.6",
          "form": "우유가 좀 부담될때 ㅎㅎ 해먹어요 !",
          "original_form": "우유가 좀 부담될때 ㅎㅎ 해먹어요 !",
          "speaker_id": "1",
          "time": "20210830 12:30"
        },
        {
          "id": "MDRW2100035941.1.7",
          "form": "우유 당류가 높아서 ㅜㅜ",
          "original_form": "우유 당류가 높아서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100035941.1.8",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100035941.1.9",
          "form": "혹시 영양제 챙겨 드시는 거 있으세요?",
          "original_form": "혹시 영양제 챙겨 드시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100035941.1.10",
          "form": "네 있어요!",
          "original_form": "네 있어요!",
          "speaker_id": "1",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100035941.1.11",
          "form": "어떤 거 챙겨드세요?",
          "original_form": "어떤 거 챙겨드세요?",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100035941.1.12",
          "form": "유산균 오메가3 비타민 마그네슘 챙겨먹어요!",
          "original_form": "유산균 오메가3 비타민 마그네슘 챙겨먹어요!",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100035941.1.13",
          "form": "오메가3 좋은 거 같으세요? 챙겨야 하나 고민중이에요",
          "original_form": "오메가3 좋은 거 같으세요? 챙겨야 하나 고민중이에요",
          "speaker_id": "2",
          "time": "20210830 12:38"
        },
        {
          "id": "MDRW2100035941.1.14",
          "form": "네 저는 좋은거같아요 ㅎㅎ",
          "original_form": "네 저는 좋은거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100035941.1.15",
          "form": "근데 알약이 큰편이라서",
          "original_form": "근데 알약이 큰편이라서",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100035941.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100035941.1.17",
          "form": "약간 거부감들기도 해요 ㅜㅜ",
          "original_form": "약간 거부감들기도 해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100035941.1.18",
          "form": "꼭 식사하구 드시는게 나아요 !",
          "original_form": "꼭 식사하구 드시는게 나아요 !",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100035941.1.19",
          "form": "아하 그렇군요!",
          "original_form": "아하 그렇군요!",
