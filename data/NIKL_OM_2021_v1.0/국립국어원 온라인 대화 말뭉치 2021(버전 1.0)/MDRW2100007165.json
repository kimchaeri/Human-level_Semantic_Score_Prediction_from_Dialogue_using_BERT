      "utterance": [
        {
          "id": "MDRW2100007165.1.1",
          "form": "요즘 재택하세요?",
          "original_form": "요즘 재택하세요?",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007165.1.2",
          "form": "아뇨 저는 안해요ㅠㅠ",
          "original_form": "아뇨 저는 안해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007165.1.3",
          "form": "재택 하세요?",
          "original_form": "재택 하세요?",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007165.1.4",
          "form": "아 저는 주부에요ㅋㅋㅋ",
          "original_form": "아 저는 주부에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007165.1.5",
          "form": "재택부업 하고있어요",
          "original_form": "재택부업 하고있어요",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007165.1.6",
          "form": "생각보다 재택근무 하려는 찾아보면 마땅한게 없더라구요",
          "original_form": "생각보다 재택근무 하려는 찾아보면 마땅한게 없더라구요",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007165.1.7",
          "form": "죄다 광고고~",
          "original_form": "죄다 광고고~",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007165.1.8",
          "form": "맞아요 광고 너무 많죠?ㅠㅠ",
          "original_form": "맞아요 광고 너무 많죠?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007165.1.9",
          "form": "재택근무 일자리가 그래도 늘고있는거 같긴 한데..",
          "original_form": "재택근무 일자리가 그래도 늘고있는거 같긴 한데..",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007165.1.10",
          "form": "네 재택할수 있는 일이 좀더 많으지면 좋겠더라구요",
          "original_form": "네 재택할수 있는 일이 좀더 많으지면 좋겠더라구요",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007165.1.11",
          "form": "요즘은 코로나때문에 재택하는 친구들도 있고 하던데",
          "original_form": "요즘은 코로나때문에 재택하는 친구들도 있고 하던데",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007165.1.12",
          "form": "맞아요 앞으로는 좀 더 그런 생활이 열리지 않을까요?",
          "original_form": "맞아요 앞으로는 좀 더 그런 생활이 열리지 않을까요?",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007165.1.13",
          "form": "그럴거같아요. 코로나때문에 좀더 당겨진 느낌이에요",
          "original_form": "그럴거같아요. 코로나때문에 좀더 당겨진 느낌이에요",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007165.1.14",
          "form": "회사원들은 돌아가면서 하는거 같더라구요",
          "original_form": "회사원들은 돌아가면서 하는거 같더라구요",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007165.1.15",
          "form": "재택근무 하면 얼마나 좋을까요ㅠㅠ",
          "original_form": "재택근무 하면 얼마나 좋을까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007165.1.16",
          "form": "저는 막상 집에서 하니까 산만해서 잘 안되더라구요",
          "original_form": "저는 막상 집에서 하니까 산만해서 잘 안되더라구요",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007165.1.17",
          "form": "그건도 맞는 말이네요",
          "original_form": "그건도 맞는 말이네요",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007165.1.18",
          "form": "뭔가 일하는 기분이 안날거 같아요",
          "original_form": "뭔가 일하는 기분이 안날거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007165.1.19",
          "form": "왠지 널널하게 하다가 시간이 늘어질거같아요ㅎㅎ",
          "original_form": "왠지 널널하게 하다가 시간이 늘어질거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007165.1.20",
          "form": "그래도 재택하면 좋을거같아요",
          "original_form": "그래도 재택하면 좋을거같아요",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007165.1.21",
          "form": "어떤 직종이 재택근무가 많을까요??",
          "original_form": "어떤 직종이 재택근무가 많을까요??",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007165.1.22",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007165.1.23",
          "form": "막상 생각해보니 어려운거 같아요",
          "original_form": "막상 생각해보니 어려운거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007165.1.24",
          "form": "재택근무로 전환할 수있는 분야가 뭐가 있을까요?",
          "original_form": "재택근무로 전환할 수있는 분야가 뭐가 있을까요?",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007165.1.25",
          "form": "아무래도 협업하는것보단 개인작업인것들 이겠죠?",
          "original_form": "아무래도 협업하는것보단 개인작업인것들 이겠죠?",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007165.1.26",
          "form": "저는 지금 재봉일 하고있어요",
          "original_form": "저는 지금 재봉일 하고있어요",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007165.1.27",
          "form": "오 그런것도 재택업무로 괜찮네요",
          "original_form": "오 그런것도 재택업무로 괜찮네요",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007165.1.28",
          "form": "재택업무하면 화상회의도 많이 하던데",
          "original_form": "재택업무하면 화상회의도 많이 하던데",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007165.1.29",
          "form": "그것도 신경쓰일거 같아요",
          "original_form": "그것도 신경쓰일거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007165.1.30",
          "form": "생각보다 줌으로 하는거 답답하더라구요",
          "original_form": "생각보다 줌으로 하는거 답답하더라구요",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007165.1.31",
          "form": "내 얼굴이 너무 정면으로 나오니까 더 민망한느낌?",
          "original_form": "내 얼굴이 너무 정면으로 나오니까 더 민망한느낌?",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007165.1.32",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007165.1.33",
          "form": "렉도 많이 걸리구요",
          "original_form": "렉도 많이 걸리구요",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007165.1.34",
          "form": "맞아요 재택근무되려면 환경이 좀 개선되야할거같아요",
          "original_form": "맞아요 재택근무되려면 환경이 좀 개선되야할거같아요",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007165.1.35",
          "form": "방금 생각난건데 재택근무 하면 식비도 줄겠어요",
          "original_form": "방금 생각난건데 재택근무 하면 식비도 줄겠어요",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007165.1.36",
          "form": "친구가 코로나 때문에 1년넘게 재택중인데",
          "original_form": "친구가 코로나 때문에 1년넘게 재택중인데",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007165.1.37",
          "form": "챙겨먹는게 넘 힘들다고 하더라구요",
          "original_form": "챙겨먹는게 넘 힘들다고 하더라구요",
          "speaker_id": "2",
          "time": "20210822 01:03"
        }
