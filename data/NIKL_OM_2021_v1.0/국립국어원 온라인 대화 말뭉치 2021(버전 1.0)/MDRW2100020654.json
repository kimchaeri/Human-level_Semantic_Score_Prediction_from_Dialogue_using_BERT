      "utterance": [
        {
          "id": "MDRW2100020654.1.1",
          "form": "작은 아들 안약 넣어줬지?",
          "original_form": "작은 아들 안약 넣어줬지?",
          "speaker_id": "2",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020654.1.2",
          "form": "ㅇㅇ 아침에 안약 넣어줬지",
          "original_form": "ㅇㅇ 아침에 안약 넣어줬지",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020654.1.3",
          "form": "눈 주변도 차갑게 해주고",
          "original_form": "눈 주변도 차갑게 해주고",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020654.1.4",
          "form": "잘했네.. 증상이 확실히 줄어들었더라",
          "original_form": "잘했네.. 증상이 확실히 줄어들었더라",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020654.1.5",
          "form": "인공눈물도 냉장고에 넣어놨어 ㅋ",
          "original_form": "인공눈물도 냉장고에 넣어놨어 ㅋ",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020654.1.6",
          "form": "안약 넣을 때 엄청 차갑겠네 ㅎㅎ",
          "original_form": "안약 넣을 때 엄청 차갑겠네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020654.1.7",
          "form": "ㅇㅇ 차가운게 알러지에 효과 있다고 해서",
          "original_form": "ㅇㅇ 차가운게 알러지에 효과 있다고 해서",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020654.1.8",
          "form": "이것저것 뭐라도 해봐야지 뭐 ..",
          "original_form": "이것저것 뭐라도 해봐야지 뭐 ..",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020654.1.9",
          "form": "ㅇㅇ 눈 깜빡임만 사라질 수 있다면 뭐든지 해줄거야",
          "original_form": "ㅇㅇ 눈 깜빡임만 사라질 수 있다면 뭐든지 해줄거야",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020654.1.10",
          "form": "엄마가 노력해서 점점 나이지고 있으니 다행이야",
          "original_form": "엄마가 노력해서 점점 나이지고 있으니 다행이야",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020654.1.11",
          "form": "틱인가 해서 큰병원 가보려고 했는데 알러지인 것 같아 조금은 안심임",
          "original_form": "틱인가 해서 큰병원 가보려고 했는데 알러지인 것 같아 조금은 안심임",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020654.1.12",
          "form": "글치~차가운 거에 반응하는 것 보면 알러지 같아",
          "original_form": "글치~차가운 거에 반응하는 것 보면 알러지 같아",
          "speaker_id": "2",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020654.1.13",
          "form": "어쩜 알러지가 일년내내 갈 수가 있을까? ㅠ",
          "original_form": "어쩜 알러지가 일년내내 갈 수가 있을까? ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020654.1.14",
          "form": "아토피나 비염 있는 사람들 봐봐.. 1년이 아니라 평생 달고 살기도 하는데",
          "original_form": "아토피나 비염 있는 사람들 봐봐.. 1년이 아니라 평생 달고 살기도 하는데",
          "speaker_id": "2",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020654.1.15",
          "form": "크면서 제발 사라지면 좋겠어",
          "original_form": "크면서 제발 사라지면 좋겠어",
          "speaker_id": "1",
          "time": "20210828 10:53"
        }
