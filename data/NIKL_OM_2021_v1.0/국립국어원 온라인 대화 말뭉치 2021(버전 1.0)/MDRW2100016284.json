      "utterance": [
        {
          "id": "MDRW2100016284.1.1",
          "form": "군대다녀오셨어용?",
          "original_form": "군대다녀오셨어용?",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016284.1.2",
          "form": "전 군대 안가요",
          "original_form": "전 군대 안가요",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016284.1.3",
          "form": "다녀오셨나요?",
          "original_form": "다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016284.1.4",
          "form": "아니용ㅋㅋ",
          "original_form": "아니용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016284.1.5",
          "form": "알티는 해보고싶었는데 어쩌다보니 그냥 졸업했네용",
          "original_form": "알티는 해보고싶었는데 어쩌다보니 그냥 졸업했네용",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016284.1.6",
          "form": "그럼 면제이신건가요?",
          "original_form": "그럼 면제이신건가요?",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016284.1.7",
          "form": "아뇽! 저 여자라..",
          "original_form": "아뇽! 저 여자라..",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016284.1.8",
          "form": "면제라고하면 면제네요ㅋㄱ",
          "original_form": "면제라고하면 면제네요ㅋㄱ",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016284.1.9",
          "form": "아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016284.1.10",
          "form": "주변에 군대 가신 분 있으신가요?",
          "original_form": "주변에 군대 가신 분 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016284.1.11",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016284.1.12",
          "form": "대부분 군필이져",
          "original_form": "대부분 군필이져",
          "speaker_id": "2",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016284.1.13",
          "form": "그럼 군대 px부탁해보신 적 있으신가요?",
          "original_form": "그럼 군대 px부탁해보신 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016284.1.14",
          "form": "부탁한적은없는데 화장품같은거 가져다주더라구요ㅋㅋ",
          "original_form": "부탁한적은없는데 화장품같은거 가져다주더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016284.1.15",
          "form": "과자같은 것도 엄청 싸대요",
          "original_form": "과자같은 것도 엄청 싸대요",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016284.1.16",
          "form": "과자같은 건 얼마나 싼가요?",
          "original_form": "과자같은 건 얼마나 싼가요?",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016284.1.17",
          "form": "글쎄요ㅋㅋㅋ 시중보다 싸대용",
          "original_form": "글쎄요ㅋㅋㅋ 시중보다 싸대용",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016284.1.18",
          "form": "무슨 화장품 받으셨나요?",
          "original_form": "무슨 화장품 받으셨나요?",
