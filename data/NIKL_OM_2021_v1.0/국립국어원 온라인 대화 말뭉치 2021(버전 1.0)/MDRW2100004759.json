      "utterance": [
        {
          "id": "MDRW2100004759.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004759.1.2",
          "form": "집안일 해요!",
          "original_form": "집안일 해요!",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004759.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004759.1.4",
          "form": "집안일은 해도 해도 끝이 없는 거 같아요",
          "original_form": "집안일은 해도 해도 끝이 없는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004759.1.5",
          "form": "네 반벅이죠",
          "original_form": "네 반벅이죠",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004759.1.6",
          "form": "맞아요ㅠㅜ",
          "original_form": "맞아요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004759.1.7",
          "form": "화장실이 대박이죠",
          "original_form": "화장실이 대박이죠",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004759.1.8",
          "form": "저는 화장실보다는",
          "original_form": "저는 화장실보다는",
          "speaker_id": "2",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004759.1.9",
          "form": "주방이 힘든 거 같아요",
          "original_form": "주방이 힘든 거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004759.1.10",
          "form": "아\r\n그런가요?",
          "original_form": "아\r\n그런가요?",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004759.1.11",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004759.1.12",
          "form": "전 락스냄새 때문",
          "original_form": "전 락스냄새 때문",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004759.1.13",
          "form": "락스냄새 독하긴 하죠",
          "original_form": "락스냄새 독하긴 하죠",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004759.1.14",
          "form": "물에 희석하고 문 다 열어 놓으면 좀 사라져서 괜찮았어요",
          "original_form": "물에 희석하고 문 다 열어 놓으면 좀 사라져서 괜찮았어요",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004759.1.15",
          "form": "몸애 안 좋다고",
          "original_form": "몸애 안 좋다고",
          "speaker_id": "1",
          "time": "20210821 18:48"
        }
