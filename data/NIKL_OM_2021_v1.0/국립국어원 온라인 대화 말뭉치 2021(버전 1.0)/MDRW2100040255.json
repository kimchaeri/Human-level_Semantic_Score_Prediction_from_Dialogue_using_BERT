        {
          "id": "MDRW2100040255.1.1",
          "form": "요즘 보시는거잇나요",
          "original_form": "요즘 보시는거잇나요",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040255.1.2",
          "form": "요즘 챙겨보는 건",
          "original_form": "요즘 챙겨보는 건",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040255.1.3",
          "form": "슬의생2 정도밖에 없어요",
          "original_form": "슬의생2 정도밖에 없어요",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040255.1.4",
          "form": "앗 저도봐용",
          "original_form": "앗 저도봐용",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040255.1.5",
          "form": "이번주에 결방해서 넘 아쉬웠어요",
          "original_form": "이번주에 결방해서 넘 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040255.1.6",
          "form": "님 티비보세영?",
          "original_form": "님 티비보세영?",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100040255.1.7",
          "form": "넷플 보세요?",
          "original_form": "넷플 보세요?",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100040255.1.8",
          "form": "네 넷플 구독중",
          "original_form": "네 넷플 구독중",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100040255.1.9",
