      "utterance": [
        {
          "id": "MDRW2100041708.1.1",
          "form": "결제할려구 기다렷어요 히힛",
          "original_form": "결제할려구 기다렷어요 히힛",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041708.1.2",
          "form": "킹덤때문에 결제미루고미루고그랫는데",
          "original_form": "킹덤때문에 결제미루고미루고그랫는데",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041708.1.3",
          "form": "디피라니이..",
          "original_form": "디피라니이..",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041708.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041708.1.5",
          "form": "ㅋㅋ또 슬의생말구",
          "original_form": "ㅋㅋ또 슬의생말구",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041708.1.6",
          "form": "보시는거머잇나용",
          "original_form": "보시는거머잇나용",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041708.1.7",
          "form": "지금은 없구.. 갯마을봐야하나고민중이구\n인간실격? 그것도볼까말까고민중입니다ㅋㅋ",
          "original_form": "지금은 없구.. 갯마을봐야하나고민중이구\n인간실격? 그것도볼까말까고민중입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041708.1.8",
          "form": "고민만잔뜩하고ㅋㅋㄱㅋ",
          "original_form": "고민만잔뜩하고ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041708.1.9",
          "form": "인간실격 안그래두",
          "original_form": "인간실격 안그래두",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041708.1.10",
          "form": "후기보니까",
          "original_form": "후기보니까",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041708.1.11",
          "form": "기대햇엇는데",
          "original_form": "기대햇엇는데",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041708.1.12",
          "form": "살짝 지루한거같아서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "살짝 지루한거같아서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041708.1.13",
          "form": "지루하기도하고 너무어두운거같아서..",
          "original_form": "지루하기도하고 너무어두운거같아서..",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041708.1.14",
          "form": "일단보류!!",
          "original_form": "일단보류!!",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041708.1.15",
          "form": "jnj",
          "original_form": "jnj",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041708.1.16",
          "form": "쪼금보다말긴했어요ㅋㅋㅋ",
          "original_form": "쪼금보다말긴했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041708.1.17",
          "form": "저는 또 막 밝은거안좋아하는데 겟마을은",
          "original_form": "저는 또 막 밝은거안좋아하는데 겟마을은",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041708.1.18",
          "form": "겁나 청량하더라구요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "겁나 청량하더라구요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041708.1.19",
          "form": "보시거든 후기가르쳐주세욥!!!!",
          "original_form": "보시거든 후기가르쳐주세욥!!!!",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041708.1.20",
          "form": "저도요ㅋㅋㄱㅋ사실 겁나밝은건 또 잘 안보거든요ㅋㅋㄱㅋ",
          "original_form": "저도요ㅋㅋㄱㅋ사실 겁나밝은건 또 잘 안보거든요ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041708.1.21",
          "form": "한쪽은겁나밝고",
          "original_form": "한쪽은겁나밝고",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041708.1.22",
          "form": "한쪽은겁나어둡고",
          "original_form": "한쪽은겁나어둡고",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041708.1.23",
          "form": "극과극이었어요ㅋㅋㅋㄱ",
          "original_form": "극과극이었어요ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041708.1.24",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041708.1.25",
          "form": "요즘 덕질은 주로 누구하고계세용?",
          "original_form": "요즘 덕질은 주로 누구하고계세용?",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041708.1.26",
          "form": "김재환 덕질합니닼ㅋㅋㅋ근데 영상같은거 많이 올려주는데 요새 잘 보게 되네요...",
          "original_form": "김재환 덕질합니닼ㅋㅋㅋ근데 영상같은거 많이 올려주는데 요새 잘 보게 되네요...",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041708.1.27",
          "form": "저는 런닝맨 광수나갓던날",
          "original_form": "저는 런닝맨 광수나갓던날",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041708.1.28",
          "form": "잘 안보게",
          "original_form": "잘 안보게",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041708.1.29",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041708.1.30",
          "form": "올해 최고로많이울엇어요",
          "original_form": "올해 최고로많이울엇어요",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041708.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041708.1.32",
          "form": "아ㅠㅡㅠ",
          "original_form": "아ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041708.1.33",
          "form": "저그거예고편보고 많이울겟구나싶어서",
          "original_form": "저그거예고편보고 많이울겟구나싶어서",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041708.1.34",
          "form": "편지부분에선차마못보고ㅜㅜㄱㅋㅋ",
          "original_form": "편지부분에선차마못보고ㅜㅜㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041708.1.35",
          "form": "김재환!! 공연 많이 그리우시겟어요ㅜㅜㅜㅜㅜㅜ",
          "original_form": "김재환!! 공연 많이 그리우시겟어요ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041708.1.36",
          "form": "헝ㅠㅠ",
          "original_form": "헝ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041708.1.37",
          "form": "저진짜콘서트느무느무가고싶습니다....",
