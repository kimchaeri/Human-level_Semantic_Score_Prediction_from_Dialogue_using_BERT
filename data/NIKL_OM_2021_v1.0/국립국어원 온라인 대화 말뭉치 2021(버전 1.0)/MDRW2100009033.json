      "utterance": [
        {
          "id": "MDRW2100009033.1.1",
          "form": "이번에 몇학점 들어?",
          "original_form": "이번에 몇학점 들어?",
          "speaker_id": "1",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009033.1.2",
          "form": "이번에 17.25 학점 듣습니당",
          "original_form": "이번에 17.25 학점 듣습니당",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009033.1.3",
          "form": "0.25는 뭐야",
          "original_form": "0.25는 뭐야",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009033.1.4",
          "form": "그 우리 지도교수세미나라고",
          "original_form": "그 우리 지도교수세미나라고",
          "speaker_id": "2",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100009033.1.5",
          "form": "지도교수랑 면담하는 과목인데",
          "original_form": "지도교수랑 면담하는 과목인데",
          "speaker_id": "2",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100009033.1.6",
          "form": "1학년 때는 분명 1학점이었던 게",
          "original_form": "1학년 때는 분명 1학점이었던 게",
          "speaker_id": "2",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100009033.1.7",
          "form": "작년에 0.5학점 되더니",
          "original_form": "작년에 0.5학점 되더니",
          "speaker_id": "2",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100009033.1.8",
          "form": "올해는 0.25학점...을 듣게 되었습니다",
          "original_form": "올해는 0.25학점...을 듣게 되었습니다",
          "speaker_id": "2",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100009033.1.9",
          "form": "짜졌네...",
          "original_form": "짜졌네...",
          "speaker_id": "1",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009033.1.10",
          "form": "학점이 줄어들었으니 덜 귀찮게 하겠지..?ㅜ",
          "original_form": "학점이 줄어들었으니 덜 귀찮게 하겠지..?ㅜ",
          "speaker_id": "1",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009033.1.11",
          "form": "1학년 때는 단체로 모여서 무슨 강연같은 거 듣고 그랬는데",
          "original_form": "1학년 때는 단체로 모여서 무슨 강연같은 거 듣고 그랬는데",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009033.1.12",
          "form": "작년부터는 그냥 한 학기에 한번 지도교수 만나는 걸로 끝냄...",
          "original_form": "작년부터는 그냥 한 학기에 한번 지도교수 만나는 걸로 끝냄...",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009033.1.13",
          "form": "와.. 그건 좋다",
          "original_form": "와.. 그건 좋다",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009033.1.14",
          "form": "우리는 뭐 써서 제출해야됨ㅡㅡ",
          "original_form": "우리는 뭐 써서 제출해야됨ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009033.1.15",
          "form": "근데 이제 4학년 되면 취업 때문에 더 자주 볼 수도 있긴 한데",
          "original_form": "근데 이제 4학년 되면 취업 때문에 더 자주 볼 수도 있긴 한데",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009033.1.16",
          "form": "취업이라니",
          "original_form": "취업이라니",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009033.1.17",
          "form": "이건 그냥 패스 먹고들어가는 수업이라 괜찮습니다 굿",
          "original_form": "이건 그냥 패스 먹고들어가는 수업이라 괜찮습니다 굿",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009033.1.18",
          "form": "ㅠㅠㅠㅠ 그래도 1학년 때에 비해서는 많이 줄어들었네",
          "original_form": "ㅠㅠㅠㅠ 그래도 1학년 때에 비해서는 많이 줄어들었네",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009033.1.19",
          "form": "이것도 교바교라",
          "original_form": "이것도 교바교라",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009033.1.20",
          "form": "진짜 수시로 지도교수랑 지도학생들 줌으로라도 만나게 하는 교수도 있고...",
          "original_form": "진짜 수시로 지도교수랑 지도학생들 줌으로라도 만나게 하는 교수도 있고...",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009033.1.21",
          "form": "와.. 완전 우리 교수...",
          "original_form": "와.. 완전 우리 교수...",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009033.1.22",
          "form": "스쨩 걸리면 새벽부터 족구해야됨",
          "original_form": "스쨩 걸리면 새벽부터 족구해야됨",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009033.1.23",
          "form": "교수님 족구 혼자 하시라구요",
          "original_form": "교수님 족구 혼자 하시라구요",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009033.1.24",
          "form": "족구는 스크린 족구 없냐??? 있을리가 없지...",
          "original_form": "족구는 스크린 족구 없냐??? 있을리가 없지...",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009033.1.25",
          "form": "다른 교수들이 같이 안 해주나봐...",
          "original_form": "다른 교수들이 같이 안 해주나봐...",
