      "utterance": [
        {
          "id": "MDRW2100011318.1.1",
          "form": "영양제드시나요?",
          "original_form": "영양제드시나요?",
          "speaker_id": "1",
          "time": "20210824 11:02"
        },
        {
          "id": "MDRW2100011318.1.2",
          "form": "네! 종합 비타민 칼슘 챙겨먹어요",
          "original_form": "네! 종합 비타민 칼슘 챙겨먹어요",
          "speaker_id": "2",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100011318.1.3",
          "form": "누누님은요?",
          "original_form": "누누님은요?",
          "speaker_id": "2",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100011318.1.4",
          "form": "저는 종비랑 오메가요ㅋㅋㅋ",
          "original_form": "저는 종비랑 오메가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011318.1.5",
          "form": "종비 어디꺼드시나요?",
          "original_form": "종비 어디꺼드시나요?",
          "speaker_id": "1",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011318.1.6",
          "form": "기억이 안나네요 톡딜에서 산거라ㅋㅋㅋㅋ",
          "original_form": "기억이 안나네요 톡딜에서 산거라ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:13"
        },
        {
          "id": "MDRW2100011318.1.7",
          "form": "어디꺼드세요?",
          "original_form": "어디꺼드세요?",
          "speaker_id": "2",
          "time": "20210824 11:13"
        },
        {
          "id": "MDRW2100011318.1.8",
          "form": "ㅋㅋㅋ저도 사실기억이안나네요",
          "original_form": "ㅋㅋㅋ저도 사실기억이안나네요",
          "speaker_id": "1",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100011318.1.9",
          "form": "점심은 뭐드시나요?",
          "original_form": "점심은 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100011318.1.10",
          "form": "아직 생각안해뒀어요 추천해주실래요?",
          "original_form": "아직 생각안해뒀어요 추천해주실래요?",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011318.1.11",
          "form": "짬뽕 어떤가요?",
          "original_form": "짬뽕 어떤가요?",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011318.1.12",
          "form": "헉 비오는날에  딱 좋네요",
          "original_form": "헉 비오는날에  딱 좋네요",
          "speaker_id": "2",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011318.1.13",
          "form": "안그래도 짜장면보단 짬뽕좋아하는데 이런날은 더 땡겨요",
          "original_form": "안그래도 짜장면보단 짬뽕좋아하는데 이런날은 더 땡겨요",
          "speaker_id": "2",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011318.1.14",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011318.1.15",
          "form": "저도짬뽕파입니다",
          "original_form": "저도짬뽕파입니다",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011318.1.16",
          "form": "요즘은 차돌짬뽕이요",
          "original_form": "요즘은 차돌짬뽕이요",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011318.1.17",
          "form": "차돌짬뽕 진하고 맛있어요 맛잘알이시네",
          "original_form": "차돌짬뽕 진하고 맛있어요 맛잘알이시네",
          "speaker_id": "2",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100011318.1.18",
          "form": "저는 짬뽕은 면이 얇은게 좋더라구요",
          "original_form": "저는 짬뽕은 면이 얇은게 좋더라구요",
          "speaker_id": "2",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100011318.1.19",
          "form": "ㅋㅋㅋ 면얇은짬뽕집은",
          "original_form": "ㅋㅋㅋ 면얇은짬뽕집은",
          "speaker_id": "1",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100011318.1.20",
          "form": "잘없지않나요?",
          "original_form": "잘없지않나요?",
          "speaker_id": "1",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100011318.1.21",
          "form": "잘없어서 하나 발견하면 거기 엄청가요ㅋㅋㅋ",
          "original_form": "잘없어서 하나 발견하면 거기 엄청가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:46"
        },
        {
          "id": "MDRW2100011318.1.22",
          "form": "ㅋㅋㅋ좋네요",
          "original_form": "ㅋㅋㅋ좋네요",
          "speaker_id": "1",
          "time": "20210824 11:54"
        },
        {
          "id": "MDRW2100011318.1.23",
          "form": "저는 면킬러예요",
          "original_form": "저는 면킬러예요",
          "speaker_id": "1",
          "time": "20210824 11:54"
        }
      ]
    }
