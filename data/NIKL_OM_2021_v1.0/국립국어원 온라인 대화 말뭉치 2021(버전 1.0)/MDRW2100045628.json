      "utterance": [
        {
          "id": "MDRW2100045628.1.1",
          "form": "피자는 어떤거 젤 잘 드세요",
          "original_form": "피자는 어떤거 젤 잘 드세요",
          "speaker_id": "1",
          "time": "20210908 04:25"
        },
        {
          "id": "MDRW2100045628.1.2",
          "form": "베이컨포테이토피자 좋아해요",
          "original_form": "베이컨포테이토피자 좋아해요",
          "speaker_id": "2",
          "time": "20210908 04:26"
        },
        {
          "id": "MDRW2100045628.1.3",
          "form": "name1님은여?",
          "original_form": "&name1&님은여?",
          "speaker_id": "2",
          "time": "20210908 04:26"
        },
        {
          "id": "MDRW2100045628.1.4",
          "form": "오 저도 비슷한데 도미노 포테이토 피자요",
          "original_form": "오 저도 비슷한데 도미노 포테이토 피자요",
          "speaker_id": "1",
          "time": "20210908 04:27"
        },
        {
          "id": "MDRW2100045628.1.5",
          "form": "도미노 포테이토 맛잇죠",
          "original_form": "도미노 포테이토 맛잇죠",
          "speaker_id": "2",
          "time": "20210908 04:28"
        },
        {
          "id": "MDRW2100045628.1.6",
          "form": "네 여기에 감자추가 하는거 누가 알려줬는데 좋더라고요 ㅋㅋ",
          "original_form": "네 여기에 감자추가 하는거 누가 알려줬는데 좋더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:30"
        },
        {
          "id": "MDRW2100045628.1.7",
          "form": "오 감자추가도 돼요?",
          "original_form": "오 감자추가도 돼요?",
          "speaker_id": "2",
          "time": "20210908 04:31"
        },
        {
          "id": "MDRW2100045628.1.8",
          "form": "네네 토핑에서요 ㅋㅋㅋ 감자가 두배..",
          "original_form": "네네 토핑에서요 ㅋㅋㅋ 감자가 두배..",
          "speaker_id": "1",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045628.1.9",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045628.1.10",
          "form": "그걸몰랏네요",
          "original_form": "그걸몰랏네요",
          "speaker_id": "2",
          "time": "20210908 04:32"
        },
        {
          "id": "MDRW2100045628.1.11",
          "form": "그쵸 쩝짭박사님이 알려주시더군요..",
          "original_form": "그쵸 쩝짭박사님이 알려주시더군요..",
          "speaker_id": "1",
          "time": "20210908 04:33"
        },
        {
          "id": "MDRW2100045628.1.12",
          "form": "현명하신분..",
          "original_form": "현명하신분..",
          "speaker_id": "2",
          "time": "20210908 04:35"
        },
        {
          "id": "MDRW2100045628.1.13",
          "form": "그르니깐요 ㅋㅋㅋ 그럼 좋아하는 과자는 있으세요",
          "original_form": "그르니깐요 ㅋㅋㅋ 그럼 좋아하는 과자는 있으세요",
          "speaker_id": "1",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045628.1.14",
          "form": "네 감자칩좋아해요",
          "original_form": "네 감자칩좋아해요",
          "speaker_id": "2",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045628.1.15",
          "form": "오 저도 감자류 젤 좋아해요 포카칩 스윙칩 수미칩 오감자 등등",
