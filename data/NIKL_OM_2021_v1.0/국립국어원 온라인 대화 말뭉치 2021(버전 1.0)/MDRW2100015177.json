        },
        {
          "id": "MDRW2100015177.1.2",
          "form": "피자좋아하세요",
          "original_form": "피자좋아하세요",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015177.1.3",
          "form": "네 가리는거 없이 다 잘 먹고 좋아해요",
          "original_form": "네 가리는거 없이 다 잘 먹고 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015177.1.4",
          "form": "음...?",
          "original_form": "음...?",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015177.1.5",
          "form": "name1님은 피자 좋아하시나봐요?",
          "original_form": "&name1&님은 피자 좋아하시나봐요?",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015177.1.6",
          "form": "그럼 name2님은 하와이안 호 불호",
          "original_form": "그럼 &name2&님은 하와이안 호 불호",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015177.1.7",
          "form": "호!",
          "original_form": "호!",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015177.1.8",
          "form": "전 피자 좋아해요ㅎ",
          "original_form": "전 피자 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015177.1.9",
          "form": "오 호!",
          "original_form": "오 호!",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015177.1.10",
          "form": "따뜻한 과일 좋아하시나요",
          "original_form": "따뜻한 과일 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015177.1.11",
          "form": "익힌 파인애플 이런거",
          "original_form": "익힌 파인애플 이런거",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015177.1.12",
          "form": "그게 하와이안 피자 잖아요 ㅋㅋ 잘 먹어요",
          "original_form": "그게 하와이안 피자 잖아요 ㅋㅋ 잘 먹어요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015177.1.13",
          "form": "그럼 피자먹을 때 콜라 사이다",
          "original_form": "그럼 피자먹을 때 콜라 사이다",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015177.1.14",
          "form": "콜라요 근데 탄산 안좋아해서 먹어도 한모금이요 ㅋㅋ 사이다도 상관없구요",
          "original_form": "콜라요 근데 탄산 안좋아해서 먹어도 한모금이요 ㅋㅋ 사이다도 상관없구요",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015177.1.15",
          "form": "사이다 좋아하시나봐요",
          "original_form": "사이다 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015177.1.16",
          "form": "어떻게 아셨어요ㅋㅋ",
          "original_form": "어떻게 아셨어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015177.1.17",
          "form": "음 그럼 피클은 드세오?",
          "original_form": "음 그럼 피클은 드세오?",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015177.1.18",
          "form": "네 먹어요",
          "original_form": "네 먹어요",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015177.1.19",
          "form": "이렇게 묻고 답하는것도 재밌네요",
          "original_form": "이렇게 묻고 답하는것도 재밌네요",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015177.1.20",
          "form": "그렇죠ㅋㅋ",
          "original_form": "그렇죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015177.1.21",
          "form": "새콤 한 맛이 좋아서 드시나요",
          "original_form": "새콤 한 맛이 좋아서 드시나요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015177.1.22",
          "form": "마루는 사람 먹는 음식에 관심 안가져요?",
          "original_form": "마루는 사람 먹는 음식에 관심 안가져요?",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015177.1.23",
          "form": "음  가지는데 안주죠",
          "original_form": "음  가지는데 안주죠",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015177.1.24",
          "form": "먹으면 안되니까",
          "original_form": "먹으면 안되니까",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015177.1.25",
          "form": "아이스크림 좋아하시나요",
          "original_form": "아이스크림 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 22:14"
        }
      ]
