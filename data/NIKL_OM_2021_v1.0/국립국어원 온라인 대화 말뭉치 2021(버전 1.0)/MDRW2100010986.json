      "utterance": [
        {
          "id": "MDRW2100010986.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010986.1.2",
          "form": "아녀 잘 안해요",
          "original_form": "아녀 잘 안해요",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010986.1.3",
          "form": "이거저거 보고 고르기 힘들어서",
          "original_form": "이거저거 보고 고르기 힘들어서",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010986.1.4",
          "form": "전 당근",
          "original_form": "전 당근",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010986.1.5",
          "form": "얼마나 해보셨어요?",
          "original_form": "얼마나 해보셨어요?",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010986.1.6",
          "form": "3년이요",
          "original_form": "3년이요",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010986.1.7",
          "form": "오 오래하셨네요",
          "original_form": "오 오래하셨네요",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010986.1.8",
          "form": "주로 뭐사세요?",
          "original_form": "주로 뭐사세요?",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010986.1.9",
          "form": "옷이요",
          "original_form": "옷이요",
          "speaker_id": "1",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010986.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010986.1.11",
          "form": "질 괜찮나요?",
          "original_form": "질 괜찮나요?",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010986.1.12",
          "form": "니 만족이요",
          "original_form": "니 만족이요",
          "speaker_id": "1",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010986.1.13",
          "form": "글쿠나 번장이나 중고나라는 하세여?",
          "original_form": "글쿠나 번장이나 중고나라는 하세여?",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100010986.1.14",
          "form": "중고나라에 해여",
          "original_form": "중고나라에 해여",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010986.1.15",
          "form": "사기 안많나요? 워낙 사기썰이 많아서 하기무섭던데",
          "original_form": "사기 안많나요? 워낙 사기썰이 많아서 하기무섭던데",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010986.1.16",
          "form": "아직 괜찮아요",
          "original_form": "아직 괜찮아요",
          "speaker_id": "1",
