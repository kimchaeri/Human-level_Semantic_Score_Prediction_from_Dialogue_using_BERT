      "utterance": [
        {
          "id": "MDRW2100004300.1.1",
          "form": "주제가 학교생활이네",
          "original_form": "주제가 학교생활이네",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004300.1.2",
          "form": "수강신청은 끝났어??",
          "original_form": "수강신청은 끝났어??",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004300.1.3",
          "form": "응 끝났지~ 나 15학점 듣는다ㅋㅋㅋㅋ",
          "original_form": "응 끝났지~ 나 15학점 듣는다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004300.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004300.1.5",
          "form": "18.5 듣지~",
          "original_form": "18.5 듣지~",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004300.1.6",
          "form": "타과 전공 포기했어..",
          "original_form": "타과 전공 포기했어..",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004300.1.7",
          "form": "금요일 쭉 연강해서 힘든 것보다..",
          "original_form": "금요일 쭉 연강해서 힘든 것보다..",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004300.1.8",
          "form": "내년에 듣는게 좋을 거 같아서 취소했지",
          "original_form": "내년에 듣는게 좋을 거 같아서 취소했지",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004300.1.9",
          "form": "헐 3학년 2학기인데 많이 듣는다..",
          "original_form": "헐 3학년 2학기인데 많이 듣는다..",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004300.1.10",
          "form": "우리과는 그렇더라..",
          "original_form": "우리과는 그렇더라..",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004300.1.11",
          "form": "나는 이번에 좀 쉬엄쉬엄 하려고..",
          "original_form": "나는 이번에 좀 쉬엄쉬엄 하려고..",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004300.1.12",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004300.1.13",
          "form": "쉬엄쉬엄하는 거 좋지ㅠㅜ",
          "original_form": "쉬엄쉬엄하는 거 좋지ㅠㅜ",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004300.1.14",
          "form": "우린 약간",
          "original_form": "우린 약간",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004300.1.15",
          "form": "1,2학년때 전필 1개 필교, 교양으로 다 듣는다면",
          "original_form": "1,2학년때 전필 1개 필교, 교양으로 다 듣는다면",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004300.1.16",
          "form": "3학년때는 전선, 전필만 있고",
          "original_form": "3학년때는 전선, 전필만 있고",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004300.1.17",
          "form": "4학년때도 전선, 전필만 있다..",
          "original_form": "4학년때도 전선, 전필만 있다..",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004300.1.18",
          "form": "2,3학년때가 제일 많아",
          "original_form": "2,3학년때가 제일 많아",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004300.1.19",
          "form": "ㅠㅠ 보통 그렇다더라",
          "original_form": "ㅠㅠ 보통 그렇다더라",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004300.1.20",
          "form": "나는 2학년 때 최대한 땡겨 들었는데도 아직 전필 많네..",
          "original_form": "나는 2학년 때 최대한 땡겨 들었는데도 아직 전필 많네..",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004300.1.21",
          "form": "전선, 전필을 들어도 들어도 끝이 없다..",
          "original_form": "전선, 전필을 들어도 들어도 끝이 없다..",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004300.1.22",
          "form": "너무 많아..ㅠㅠ",
          "original_form": "너무 많아..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004300.1.23",
          "form": "그니까ㅠㅠ",
          "original_form": "그니까ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004300.1.24",
          "form": "4학년 전에는 끝내고 싶었는데",
          "original_form": "4학년 전에는 끝내고 싶었는데",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004300.1.25",
          "form": "절대 무리였다",
          "original_form": "절대 무리였다",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004300.1.26",
          "form": "우린 4학년 전에 끝내기는.. 어렵다..",
          "original_form": "우린 4학년 전에 끝내기는.. 어렵다..",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004300.1.27",
          "form": "ㅠㅠ 왜.. 4학년 전에 안 끝내지냐고...ㅜㅜ",
          "original_form": "ㅠㅠ 왜.. 4학년 전에 안 끝내지냐고...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004300.1.28",
          "form": "4학년 되면 할 거 많겠지..?",
          "original_form": "4학년 되면 할 거 많겠지..?",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004300.1.29",
          "form": "취업준비한다고 바쁘다 하는데",
          "original_form": "취업준비한다고 바쁘다 하는데",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004300.1.30",
          "form": "취업준비가 '뭘까' 싶은...",
          "original_form": "취업준비가 '뭘까' 싶은...",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004300.1.31",
          "form": "자소서, 이력서, 1급시험 등등이지만..",
          "original_form": "자소서, 이력서, 1급시험 등등이지만..",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004300.1.32",
          "form": "감자라서 지금은 아무 생각이 없다..",
          "original_form": "감자라서 지금은 아무 생각이 없다..",
          "speaker_id": "1",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004300.1.33",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004300.1.34",
          "form": "우리는 임용준비지만...",
          "original_form": "우리는 임용준비지만...",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004300.1.35",
          "form": "너무 어렵다...",
          "original_form": "너무 어렵다...",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004300.1.36",
          "form": "임용ㅠㅜ",
          "original_form": "임용ㅠㅜ",
          "speaker_id": "1",
          "time": "20210821 17:37"
        }
      ]
    }
