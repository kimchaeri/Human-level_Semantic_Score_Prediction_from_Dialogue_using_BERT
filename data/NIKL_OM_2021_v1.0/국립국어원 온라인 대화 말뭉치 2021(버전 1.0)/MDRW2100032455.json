      "utterance": [
        {
          "id": "MDRW2100032455.1.1",
          "form": "가성비 좋은 음식 뭐가 있을까",
          "original_form": "가성비 좋은 음식 뭐가 있을까",
          "speaker_id": "2",
          "time": "20210905 18:11"
        },
        {
          "id": "MDRW2100032455.1.2",
          "form": "우리집 냉장고에 꼭 있는 건 어묵 무 양배추",
          "original_form": "우리집 냉장고에 꼭 있는 건 어묵 무 양배추",
          "speaker_id": "1",
          "time": "20210905 18:12"
        },
        {
          "id": "MDRW2100032455.1.3",
          "form": "활용이 용이해서 좋아",
          "original_form": "활용이 용이해서 좋아",
          "speaker_id": "1",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100032455.1.4",
          "form": "요리하기 좋은 재료들이네",
          "original_form": "요리하기 좋은 재료들이네",
          "speaker_id": "2",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100032455.1.5",
          "form": "미역국 재료도 많이 필요 없고 여러끼 먹을 수 있어서 좋아",
          "original_form": "미역국 재료도 많이 필요 없고 여러끼 먹을 수 있어서 좋아",
          "speaker_id": "1",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100032455.1.6",
          "form": "미역국 맛있지",
          "original_form": "미역국 맛있지",
          "speaker_id": "2",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100032455.1.7",
          "form": "사골곰탕 레토르트 항상 사두는데",
          "original_form": "사골곰탕 레토르트 항상 사두는데",
          "speaker_id": "1",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100032455.1.8",
          "form": "나도나도 집에 있어 ㅋㅋㅋㅋ",
          "original_form": "나도나도 집에 있어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100032455.1.9",
          "form": "떡이나 만두 넣으면 떡국 만둣국 뚝딱",
          "original_form": "떡이나 만두 넣으면 떡국 만둣국 뚝딱",
          "speaker_id": "1",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100032455.1.10",
          "form": "거기에 파 송송 썰어 얹으면 맛있지",
          "original_form": "거기에 파 송송 썰어 얹으면 맛있지",
          "speaker_id": "2",
          "time": "20210905 18:15"
        },
        {
          "id": "MDRW2100032455.1.11",
          "form": "또 냉동 닭 안심살  1키로에 6천원 정도밖에 안하는데",
          "original_form": "또 냉동 닭 안심살  1키로에 6천원 정도밖에 안하는데",
          "speaker_id": "1",
          "time": "20210905 18:15"
        },
        {
          "id": "MDRW2100032455.1.12",
          "form": "저렴한 편인가",
          "original_form": "저렴한 편인가",
          "speaker_id": "2",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100032455.1.13",
          "form": "응 1키로면 양 꽤 돼서 활용",
          "original_form": "응 1키로면 양 꽤 돼서 활용",
          "speaker_id": "1",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100032455.1.14",
          "form": "하기도 좋아",
          "original_form": "하기도 좋아",
          "speaker_id": "1",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100032455.1.15",
          "form": "냉동이라 상할 걱정도 없겠네",
          "original_form": "냉동이라 상할 걱정도 없겠네",
          "speaker_id": "2",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100032455.1.16",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100032455.1.17",
          "form": "우유 살 때 생우유는 금방 상하잖아",
          "original_form": "우유 살 때 생우유는 금방 상하잖아",
