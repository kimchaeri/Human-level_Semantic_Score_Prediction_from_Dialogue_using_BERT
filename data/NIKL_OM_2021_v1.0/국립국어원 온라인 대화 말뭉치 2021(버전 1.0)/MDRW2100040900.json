      "utterance": [
        {
          "id": "MDRW2100040900.1.1",
          "form": "그니까요ㅋㅋ 회오리감자 얼마나 맛있는데요",
          "original_form": "그니까요ㅋㅋ 회오리감자 얼마나 맛있는데요",
          "speaker_id": "2",
          "time": "20210905 11:46"
        },
        {
          "id": "MDRW2100040900.1.2",
          "form": "개구리 드셧보셨나요?",
          "original_form": "개구리 드셧보셨나요?",
          "speaker_id": "2",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040900.1.3",
          "form": "닭고기 맛이라던데요",
          "original_form": "닭고기 맛이라던데요",
          "speaker_id": "2",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040900.1.4",
          "form": "네 정말 연한 닭고기 맛있어요",
          "original_form": "네 정말 연한 닭고기 맛있어요",
          "speaker_id": "1",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040900.1.5",
          "form": "일식집에서 곁반찬으로 내왔는데",
          "original_form": "일식집에서 곁반찬으로 내왔는데",
          "speaker_id": "1",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040900.1.6",
          "form": "뭔지 모르고 집어먹었죠..",
          "original_form": "뭔지 모르고 집어먹었죠..",
          "speaker_id": "1",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040900.1.7",
          "form": "매미님의 내가 이것까지 먹어봤다 품목 있나요?",
          "original_form": "매미님의 내가 이것까지 먹어봤다 품목 있나요?",
          "speaker_id": "1",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100040900.1.8",
          "form": "저는 흔하죠 뭐 개 고기?",
          "original_form": "저는 흔하죠 뭐 개 고기?",
          "speaker_id": "2",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100040900.1.9",
          "form": "아 어렸을때 보신하라고 많이 먹었죠..",
          "original_form": "아 어렸을때 보신하라고 많이 먹었죠..",
          "speaker_id": "1",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040900.1.10",
          "form": "사슴고기 먹어봤어요 중국가서!",
          "original_form": "사슴고기 먹어봤어요 중국가서!",
          "speaker_id": "1",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040900.1.11",
          "form": "오와",
          "original_form": "오와",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040900.1.12",
          "form": "그냥 소나 돼지 닭 오리가 좋습니다",
          "original_form": "그냥 소나 돼지 닭 오리가 좋습니다",
          "speaker_id": "1",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040900.1.13",
          "form": "어땠나요?",
          "original_form": "어땠나요?",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040900.1.14",
          "form": "뻐억뻐억 했던 기억만 나요",
          "original_form": "뻐억뻐억 했던 기억만 나요",
          "speaker_id": "1",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040900.1.15",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040900.1.16",
          "form": "ㅋㅋ아 생각해보니 저는 토끼 고기요",
          "original_form": "ㅋㅋ아 생각해보니 저는 토끼 고기요",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100040900.1.17",
          "form": "오오 토끼도 약간 누린내나지 않나요",
          "original_form": "오오 토끼도 약간 누린내나지 않나요",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100040900.1.18",
          "form": "약간 냄새나고 스펀지 같이 질겅질겅했어요",
          "original_form": "약간 냄새나고 스펀지 같이 질겅질겅했어요",
          "speaker_id": "2",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100040900.1.19",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100040900.1.20",
          "form": "인생에 끼니수가 정해져있다면",
          "original_form": "인생에 끼니수가 정해져있다면",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100040900.1.21",
          "form": "맛있는 것을 먹고싶어요ㅜ",
          "original_form": "맛있는 것을 먹고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100040900.1.22",
          "form": "저도요ㅋㅋ 이왕 먹는것 맛있는걸로요",
          "original_form": "저도요ㅋㅋ 이왕 먹는것 맛있는걸로요",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100040900.1.23",
          "form": "개고기는 성인돼서 첨 먹어봤어요",
          "original_form": "개고기는 성인돼서 첨 먹어봤어요",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100040900.1.24",
          "form": "무려 알바 사장님이..",
          "original_form": "무려 알바 사장님이..",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100040900.1.25",
          "form": "아니 혹시 그 돈까스 사장님이신가요",
          "original_form": "아니 혹시 그 돈까스 사장님이신가요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040900.1.26",
          "form": "네 전직 스튜어디스셨죠",
          "original_form": "네 전직 스튜어디스셨죠",
          "speaker_id": "2",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040900.1.27",
          "form": "심지어 여성분이셨군요",
          "original_form": "심지어 여성분이셨군요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040900.1.28",
          "form": "전직 스튜어디스인 돈까스 집 사장님이 돈까스 집에서 주신 개고기..",
          "original_form": "전직 스튜어디스인 돈까스 집 사장님이 돈까스 집에서 주신 개고기..",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100040900.1.29",
          "form": "양배추를 산처럼 채썰고 가셨대서 왠지 아저씨를 생각했는데 저의 편견이었어요",
          "original_form": "양배추를 산처럼 채썰고 가셨대서 왠지 아저씨를 생각했는데 저의 편견이었어요",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100040900.1.30",
          "form": "사장님도 계셨어요 사천왕 닮으신!!!",
          "original_form": "사장님도 계셨어요 사천왕 닮으신!!!",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100040900.1.31",
          "form": "저는 할아버지가 데려가주셔서 개고기며 산낙지 영재교육을 했습니다",
          "original_form": "저는 할아버지가 데려가주셔서 개고기며 산낙지 영재교육을 했습니다",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100040900.1.32",
          "form": "주로 배달하셨지만요ㅋㅋ",
          "original_form": "주로 배달하셨지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100040900.1.33",
          "form": "ㅋㅋㅋ사천왕 닮으신 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ사천왕 닮으신 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100040900.1.34",
          "form": "왠지 돈까스집 사장님 멋진 분이셨을것 같아요",
          "original_form": "왠지 돈까스집 사장님 멋진 분이셨을것 같아요",
          "speaker_id": "1",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100040900.1.35",
          "form": "개고기를 주셔서는 아니구..",
          "original_form": "개고기를 주셔서는 아니구..",
          "speaker_id": "1",
          "time": "20210905 11:54"
        }
      ]
    }
