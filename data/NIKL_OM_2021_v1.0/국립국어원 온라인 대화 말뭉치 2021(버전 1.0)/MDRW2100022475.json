      "utterance": [
        {
          "id": "MDRW2100022475.1.1",
          "form": "ㅋㅋㅋ 저 토끼도 키워봤어요",
          "original_form": "ㅋㅋㅋ 저 토끼도 키워봤어요",
          "speaker_id": "2",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022475.1.2",
          "form": "기르셨네요",
          "original_form": "기르셨네요",
          "speaker_id": "1",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022475.1.3",
          "form": "토끼는 어때요",
          "original_form": "토끼는 어때요",
          "speaker_id": "1",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022475.1.4",
          "form": "네 우리 엄마가 개는 못기르게 해서",
          "original_form": "네 우리 엄마가 개는 못기르게 해서",
          "speaker_id": "2",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022475.1.5",
          "form": "그때는 여러가지 키웠네요",
          "original_form": "그때는 여러가지 키웠네요",
          "speaker_id": "2",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022475.1.6",
          "form": "토끼 완전 귀여워요",
          "original_form": "토끼 완전 귀여워요",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.7",
          "form": "개는 안되는데 토끼는 된다..?",
          "original_form": "개는 안되는데 토끼는 된다..?",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.8",
          "form": "토끼는 그 사육장 안에 가두잖아요",
          "original_form": "토끼는 그 사육장 안에 가두잖아요",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.9",
          "form": "그리고 가격도 싸요",
          "original_form": "그리고 가격도 싸요",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.10",
          "form": "토끼가 더 손 많이가지않나",
          "original_form": "토끼가 더 손 많이가지않나",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.11",
          "form": "엄마는 개가 비싸서 안된대요",
          "original_form": "엄마는 개가 비싸서 안된대요",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.12",
          "form": "친구집 보면 방목하던데",
          "original_form": "친구집 보면 방목하던데",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.13",
          "form": "아.. 그러셨구나",
          "original_form": "아.. 그러셨구나",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.14",
          "form": "ㅋㅋㅋ토끼 진짜 어마무시하게 커져요",
          "original_form": "ㅋㅋㅋ토끼 진짜 어마무시하게 커져요",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.15",
          "form": "그래서 나중엔 옥상에 풀어놓고 키웠어요",
          "original_form": "그래서 나중엔 옥상에 풀어놓고 키웠어요",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.16",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.17",
          "form": "얼마나커져요",
          "original_form": "얼마나커져요",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.18",
          "form": "근데 똥도 엄청 많이 싸는거 있죠",
          "original_form": "근데 똥도 엄청 많이 싸는거 있죠",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022475.1.19",
          "form": "거의 갑티슈보다 더 커요",
          "original_form": "거의 갑티슈보다 더 커요",
          "speaker_id": "2",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100022475.1.20",
          "form": "헐 대박이다",
          "original_form": "헐 대박이다",
          "speaker_id": "1",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100022475.1.21",
          "form": "근데 크기만 크고 무게는 또 가벼워서",
          "original_form": "근데 크기만 크고 무게는 또 가벼워서",
          "speaker_id": "2",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100022475.1.22",
          "form": "되게 신기했어요",
          "original_form": "되게 신기했어요",
          "speaker_id": "2",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100022475.1.23",
          "form": "저도 신기해요",
          "original_form": "저도 신기해요",
          "speaker_id": "1",
          "time": "20210829 14:44"
        }
      ]
