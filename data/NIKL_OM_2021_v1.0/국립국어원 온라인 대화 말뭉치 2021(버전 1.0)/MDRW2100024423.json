      "utterance": [
        {
          "id": "MDRW2100024423.1.1",
          "form": "응응 출근하려고 이제 막 나왔어... ㅋㅋ큐",
          "original_form": "응응 출근하려고 이제 막 나왔어... ㅋㅋ큐",
          "speaker_id": "2",
          "time": "20210831 08:38"
        },
        {
          "id": "MDRW2100024423.1.2",
          "form": "언니는 출근 이미 했으려나?",
          "original_form": "언니는 출근 이미 했으려나?",
          "speaker_id": "2",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024423.1.3",
          "form": "아닝 나도 출근하러 가는중",
          "original_form": "아닝 나도 출근하러 가는중",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024423.1.4",
          "form": "버스타고 가?",
          "original_form": "버스타고 가?",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024423.1.5",
          "form": "ㅇㅎ 고생이다..",
          "original_form": "ㅇㅎ 고생이다..",
          "speaker_id": "2",
          "time": "20210831 08:40"
        },
        {
          "id": "MDRW2100024423.1.6",
          "form": "ㅇㅇ 버스타고! 사람 없길 비는 중이야 앉아서 가야만한다",
          "original_form": "ㅇㅇ 버스타고! 사람 없길 비는 중이야 앉아서 가야만한다",
          "speaker_id": "2",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100024423.1.7",
          "form": "ㅇㅎ 난 지하철타고가는데 배차간격 너무 넓어..",
          "original_form": "ㅇㅎ 난 지하철타고가는데 배차간격 너무 넓어..",
          "speaker_id": "1",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100024423.1.8",
          "form": "지각은 아니겠지...",
          "original_form": "지각은 아니겠지...",
          "speaker_id": "1",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100024423.1.9",
          "form": "ㅜ 설마.. 배차간격 진짜 싫다",
          "original_form": "ㅜ 설마.. 배차간격 진짜 싫다",
          "speaker_id": "2",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100024423.1.10",
          "form": "10시 출근이야?",
          "original_form": "10시 출근이야?",
          "speaker_id": "1",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100024423.1.11",
          "form": "응ㅇ 10시 출근",
          "original_form": "응ㅇ 10시 출근",
          "speaker_id": "2",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100024423.1.12",
          "form": "6시 퇴근?",
          "original_form": "6시 퇴근?",
          "speaker_id": "1",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100024423.1.13",
          "form": "시간은 있는데 나도 배차간격.. 싫",
          "original_form": "시간은 있는데 나도 배차간격.. 싫",
          "speaker_id": "2",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100024423.1.14",
          "form": "ㅇㅇ 6시 퇴근",
          "original_form": "ㅇㅇ 6시 퇴근",
          "speaker_id": "2",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100024423.1.15",
          "form": "근무시간 좀 짧아서 좋네",
          "original_form": "근무시간 좀 짧아서 좋네",
          "speaker_id": "1",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100024423.1.16",
          "form": "ㅋㅋㅋ 좀 짧은데 그것마저 길게 느껴지는 직장생활",
          "original_form": "ㅋㅋㅋ 좀 짧은데 그것마저 길게 느껴지는 직장생활",
          "speaker_id": "2",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100024423.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 요즘도 포장업무 해?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 요즘도 포장업무 해?",
          "speaker_id": "1",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100024423.1.18",
          "form": "언닌 9시 6시..?",
          "original_form": "언닌 9시 6시..?",
          "speaker_id": "2",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100024423.1.19",
          "form": "ㅇㅇ 나야 뭐 전형적이지 ...",
          "original_form": "ㅇㅇ 나야 뭐 전형적이지 ...",
          "speaker_id": "1",
          "time": "20210831 08:45"
        },
        {
          "id": "MDRW2100024423.1.20",
          "form": "ㅋㅋㅋㅋㅋ 창구업무 이번주는 안할거 같긴 한데",
          "original_form": "ㅋㅋㅋㅋㅋ 창구업무 이번주는 안할거 같긴 한데",
          "speaker_id": "2",
          "time": "20210831 08:45"
        },
        {
          "id": "MDRW2100024423.1.21",
          "form": "연가내는 사람 잇음 투입이겠지",
          "original_form": "연가내는 사람 잇음 투입이겠지",
          "speaker_id": "2",
          "time": "20210831 08:45"
        },
