        },
        {
          "id": "MDRW2100029374.1.4",
          "form": "백신접종하셨나요",
          "original_form": "백신접종하셨나요",
          "speaker_id": "2",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029374.1.5",
          "form": "네 2차까지 다 맞고 2주이상 지났어요",
          "original_form": "네 2차까지 다 맞고 2주이상 지났어요",
          "speaker_id": "1",
          "time": "20210903 03:35"
        },
        {
          "id": "MDRW2100029374.1.6",
          "form": "오 진짜 빠르시네요",
          "original_form": "오 진짜 빠르시네요",
          "speaker_id": "2",
          "time": "20210903 03:35"
        },
        {
          "id": "MDRW2100029374.1.7",
          "form": "언제맞으시는데요 ?",
          "original_form": "언제맞으시는데요 ?",
          "speaker_id": "1",
          "time": "20210903 03:36"
        },
        {
          "id": "MDRW2100029374.1.8",
          "form": "오늘 예정이에요",
          "original_form": "오늘 예정이에요",
          "speaker_id": "2",
          "time": "20210903 03:37"
        },
        {
          "id": "MDRW2100029374.1.9",
          "form": "무슨백신맞아요?",
          "original_form": "무슨백신맞아요?",
          "speaker_id": "1",
          "time": "20210903 03:40"
        },
        {
          "id": "MDRW2100029374.1.10",
          "form": "전 화이자요",
          "original_form": "전 화이자요",
          "speaker_id": "2",
          "time": "20210903 03:40"
        },
        {
          "id": "MDRW2100029374.1.11",
          "form": "몇시에 맞아요?",
          "original_form": "몇시에 맞아요?",
          "speaker_id": "1",
          "time": "20210903 03:40"
        },
        {
          "id": "MDRW2100029374.1.12",
          "form": "오후 2시에요",
          "original_form": "오후 2시에요",
          "speaker_id": "2",
          "time": "20210903 03:40"
        },
        {
          "id": "MDRW2100029374.1.13",
          "form": "어디서맞아요?",
          "original_form": "어디서맞아요?",
