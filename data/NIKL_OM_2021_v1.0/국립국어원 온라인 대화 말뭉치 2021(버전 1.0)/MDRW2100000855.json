      "utterance": [
        {
          "id": "MDRW2100000855.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000855.1.2",
          "form": "안녕하세요 회사 다니세요?",
          "original_form": "안녕하세요 회사 다니세요?",
          "speaker_id": "2",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000855.1.3",
          "form": "지금은.쉬고있어요",
          "original_form": "지금은.쉬고있어요",
          "speaker_id": "1",
          "time": "20210819 20:07"
        },
        {
          "id": "MDRW2100000855.1.4",
          "form": "다니시고잇나요",
          "original_form": "다니시고잇나요",
          "speaker_id": "1",
          "time": "20210819 20:07"
        },
        {
          "id": "MDRW2100000855.1.5",
          "form": "네 전 지금 다니고 있는데",
          "original_form": "네 전 지금 다니고 있는데",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000855.1.6",
          "form": "퇴사 하고싶네요",
          "original_form": "퇴사 하고싶네요",
          "speaker_id": "2",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000855.1.7",
          "form": "이직을 생각하시는건가요",
          "original_form": "이직을 생각하시는건가요",
          "speaker_id": "1",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000855.1.8",
          "form": "일을 해야 앞으로가 편한대 일을하다보면 스트레스가 심하고",
          "original_form": "일을 해야 앞으로가 편한대 일을하다보면 스트레스가 심하고",
          "speaker_id": "1",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000855.1.9",
          "form": "이직은 아니고 그냥 쉬고싶어요",
          "original_form": "이직은 아니고 그냥 쉬고싶어요",
          "speaker_id": "2",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000855.1.10",
          "form": "맞아요 일은 해야하지먼 스트레스 정말 휴",
          "original_form": "맞아요 일은 해야하지먼 스트레스 정말 휴",
          "speaker_id": "2",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000855.1.11",
          "form": "직장에선 업무스트레스가 심하신가료",
          "original_form": "직장에선 업무스트레스가 심하신가료",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000855.1.12",
          "form": "네 ㅎ 떨쳐내야하는데 힘들더라구요",
          "original_form": "네 ㅎ 떨쳐내야하는데 힘들더라구요",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000855.1.13",
          "form": "마음처럼 쉽지 않으니요 ㅠ",
          "original_form": "마음처럼 쉽지 않으니요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000855.1.14",
          "form": "제일 인생에서 고민이죠",
          "original_form": "제일 인생에서 고민이죠",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000855.1.15",
          "form": "저도 이겨내려고 해봤는대 그 스트레스가 엄청나더라구요",
          "original_form": "저도 이겨내려고 해봤는대 그 스트레스가 엄청나더라구요",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000855.1.16",
          "form": "어렵죠 스트레스로 인해 일을 그만 두는 사람들도",
          "original_form": "어렵죠 스트레스로 인해 일을 그만 두는 사람들도",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000855.1.17",
          "form": "아주 많으니까요",
          "original_form": "아주 많으니까요",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000855.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000855.1.19",
          "form": "주변에 보면 다들 그래요",
          "original_form": "주변에 보면 다들 그래요",
          "speaker_id": "1",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000855.1.20",
          "form": "맘 같아서는 돈 많은 백수를 하고싶네요 ㅎ",
          "original_form": "맘 같아서는 돈 많은 백수를 하고싶네요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000855.1.21",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100000855.1.22",
          "form": "아니면 그냥 원하는 좋은 곳에서 일하고도 싶네여",
          "original_form": "아니면 그냥 원하는 좋은 곳에서 일하고도 싶네여",
