      "utterance": [
        {
          "id": "MDRW2100041696.1.1",
          "form": "매장에 손님이 애기를 안고 왔는데",
          "original_form": "매장에 손님이 애기를 안고 왔는데",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041696.1.2",
          "form": "니 꿀 복 꿀 희어멈 알 제",
          "original_form": "니 꿀 복 꿀 희어멈 알 제",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041696.1.3",
          "form": "오늘 왔거든",
          "original_form": "오늘 왔거든",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041696.1.4",
          "form": "응응 ㅋㅋ 안다 안다",
          "original_form": "응응 ㅋㅋ 안다 안다",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041696.1.5",
          "form": "그분이 또 갔구만",
          "original_form": "그분이 또 갔구만",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041696.1.6",
          "form": "아기를 내가 안고 있었어 쇼핑한다고",
          "original_form": "아기를 내가 안고 있었어 쇼핑한다고",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041696.1.7",
          "form": "내가 막 귀엽다 카면서",
          "original_form": "내가 막 귀엽다 카면서",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041696.1.8",
          "form": "안고 있으니까 신랑이",
          "original_form": "안고 있으니까 신랑이",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041696.1.9",
          "form": "가증스러운 눈빛으로  날 보더라ㅋㅋㅋㅋ",
          "original_form": "가증스러운 눈빛으로  날 보더라ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041696.1.10",
          "form": "응응 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 웃겨",
          "original_form": "응응 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 웃겨",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041696.1.11",
          "form": "니 애나 잘 봐라 이런 눈빛ㅋㅋㅋ",
          "original_form": "니 애나 잘 봐라 이런 눈빛ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041696.1.12",
          "form": "근데 애긴 너무 작더라 뭐",
          "original_form": "근데 애긴 너무 작더라 뭐",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041696.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ아 웃겨 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 웃겨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041696.1.14",
          "form": "너무 웃기노",
          "original_form": "너무 웃기노",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041696.1.15",
          "form": "그래 ㅋㅋㅋ 신랑이 말하지 않아도",
          "original_form": "그래 ㅋㅋㅋ 신랑이 말하지 않아도",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041696.1.16",
          "form": "눈빛으로 통하지",
          "original_form": "눈빛으로 통하지",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041696.1.17",
          "form": "눈빛이 너무 느껴져서",
          "original_form": "눈빛이 너무 느껴져서",
          "speaker_id": "2",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041696.1.18",
          "form": "더 이상 멘트를 못 치겠더라ㅋㅋㅋ",
          "original_form": "더 이상 멘트를 못 치겠더라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041696.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 웃겨 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 웃겨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041696.1.20",
          "form": "그래서 옷 왕창 또 사갔나?",
          "original_form": "그래서 옷 왕창 또 사갔나?",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041696.1.21",
          "form": "야 여름 거가을 걸로 다 바까 갔다;",
          "original_form": "야 여름 거가을 걸로 다 바까 갔다;",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100041696.1.22",
          "form": "앞뒤 안 보고 사 갈 때 막사 가는데",
          "original_form": "앞뒤 안 보고 사 갈 때 막사 가는데",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100041696.1.23",
          "form": "은근히 교환해 가는 게 더 많다;",
          "original_form": "은근히 교환해 가는 게 더 많다;",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100041696.1.24",
          "form": "일만 많아짐ㅋㅋㅋ",
          "original_form": "일만 많아짐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100041696.1.25",
          "form": "아아앙~~",
          "original_form": "아아앙~~",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041696.1.26",
          "form": "일만 많아진대 ㅋㅋ 아 웃겨라",
          "original_form": "일만 많아진대 ㅋㅋ 아 웃겨라",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041696.1.27",
          "form": "그거 다시 다 비닐에 넣어 가지고 또 정리해야 되는데",
          "original_form": "그거 다시 다 비닐에 넣어 가지고 또 정리해야 되는데",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041696.1.28",
          "form": "예 한번 해주면 고맙다 생각하고 다신 안 해야 되는데",
          "original_form": "예 한번 해주면 고맙다 생각하고 다신 안 해야 되는데",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041696.1.29",
          "form": "한번 해주니까 한도 끝도 없네ㅋㅋㅋㅋㅋ",
          "original_form": "한번 해주니까 한도 끝도 없네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041696.1.30",
          "form": "응으 그렇지",
          "original_form": "응으 그렇지",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041696.1.31",
          "form": "그래 한번이 어렵지",
          "original_form": "그래 한번이 어렵지",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041696.1.32",
          "form": "싶은 생각이 있지않겠나",
          "original_form": "싶은 생각이 있지않겠나",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041696.1.33",
          "form": "그러니까 말이다",
          "original_form": "그러니까 말이다",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041696.1.34",
          "form": "아구",
          "original_form": "아구",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100041696.1.35",
          "form": "일부러 내 있는 시간에만 오잖아ㅋㅋㅋ",
          "original_form": "일부러 내 있는 시간에만 오잖아ㅋㅋㅋ",
