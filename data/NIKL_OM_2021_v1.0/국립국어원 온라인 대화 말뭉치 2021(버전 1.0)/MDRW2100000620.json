      "utterance": [
        {
          "id": "MDRW2100000620.1.1",
          "form": "근데 길냥이들은 예방접종같은거 많이 맞춰줘야 할 것 같다",
          "original_form": "근데 길냥이들은 예방접종같은거 많이 맞춰줘야 할 것 같다",
          "speaker_id": "2",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000620.1.2",
          "form": "집에서 키우려면 뭐든 다 그렇겠지",
          "original_form": "집에서 키우려면 뭐든 다 그렇겠지",
          "speaker_id": "2",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000620.1.3",
          "form": "동물병원 비용이 비싸더라고 동물들도 의료보험이 되면 좋겠어",
          "original_form": "동물병원 비용이 비싸더라고 동물들도 의료보험이 되면 좋겠어",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000620.1.4",
          "form": "반러동물은 어릴 때부터 키우는게 좋음",
          "original_form": "반러동물은 어릴 때부터 키우는게 좋음",
          "speaker_id": "1",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000620.1.5",
          "form": "동물병원도 잘 골라야 한다",
          "original_form": "동물병원도 잘 골라야 한다",
          "speaker_id": "1",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000620.1.6",
          "form": "그럼 역시 반려오리를 키워야 하나",
          "original_form": "그럼 역시 반려오리를 키워야 하나",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000620.1.7",
          "form": "오리 부화기에 넣고 알에서부터 깨어나는거야",
          "original_form": "오리 부화기에 넣고 알에서부터 깨어나는거야",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000620.1.8",
          "form": "병원마다 치료비 천차만별임",
          "original_form": "병원마다 치료비 천차만별임",
          "speaker_id": "1",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000620.1.9",
          "form": "오리 부화되는 거 보니까 엄청나더라",
          "original_form": "오리 부화되는 거 보니까 엄청나더라",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000620.1.10",
          "form": "반려오리라니 ㅋㅋ 집에 마당은 있는겨.",
          "original_form": "반려오리라니 ㅋㅋ 집에 마당은 있는겨.",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000620.1.11",
          "form": "그게 문제야 마당있으면 오리도 키우고 병아리도 키우고",
          "original_form": "그게 문제야 마당있으면 오리도 키우고 병아리도 키우고",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000620.1.12",
          "form": "시골에서나 가능한거당 ㅋㅋ",
          "original_form": "시골에서나 가능한거당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000620.1.13",
          "form": "오리나 병아리나 강아지는 다 마당이 있어야 맘껏 뛰어놓고 좋을 것 같다",
          "original_form": "오리나 병아리나 강아지는 다 마당이 있어야 맘껏 뛰어놓고 좋을 것 같다",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000620.1.14",
          "form": "내 친구 집 2000평 ㅋ 마당에 말도 있음",
          "original_form": "내 친구 집 2000평 ㅋ 마당에 말도 있음",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000620.1.15",
          "form": "오리랑 병아리 크는 속도가 엄청나더라고",
          "original_form": "오리랑 병아리 크는 속도가 엄청나더라고",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000620.1.16",
          "form": "대신 완전 시골 ㅋㅋ",
          "original_form": "대신 완전 시골 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000620.1.17",
          "form": "막 쑥쑥 자라",
          "original_form": "막 쑥쑥 자라",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000620.1.18",
          "form": "가금류는 그렇지 뭐 ㅋ",
          "original_form": "가금류는 그렇지 뭐 ㅋ",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000620.1.19",
          "form": "반려병아리였는데",
          "original_form": "반려병아리였는데",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000620.1.20",
          "form": "빨리 자라서 날 수 있어야 천적 피해서 도망가니까~",
          "original_form": "빨리 자라서 날 수 있어야 천적 피해서 도망가니까~",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
