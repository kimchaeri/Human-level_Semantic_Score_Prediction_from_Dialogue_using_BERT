      "utterance": [
        {
          "id": "MDRW2100036206.1.1",
          "form": "껠끄쇼즈...메모...",
          "original_form": "껠끄쇼즈...메모...",
          "speaker_id": "1",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100036206.1.2",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036206.1.3",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036206.1.4",
          "form": "방금 먹었어용",
          "original_form": "방금 먹었어용",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036206.1.5",
          "form": "카레 먹었어요ㅎㅎ",
          "original_form": "카레 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036206.1.6",
          "form": "오 ㅋㅋㅋ 직접 만드셨어요?",
          "original_form": "오 ㅋㅋㅋ 직접 만드셨어요?",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100036206.1.7",
          "form": "네네!ㅋㅋㅋ",
          "original_form": "네네!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100036206.1.8",
          "form": "저녁 뭐드셨나요?",
          "original_form": "저녁 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036206.1.9",
          "form": "우와 ㅋㅋㅋㅋ 저는 엄마가 감자전 해주셨어요 ㅋㅋ 직접 갈아서!!",
          "original_form": "우와 ㅋㅋㅋㅋ 저는 엄마가 감자전 해주셨어요 ㅋㅋ 직접 갈아서!!",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036206.1.10",
          "form": "완전 쫀뜩하고 맛있었어요 ㅋㅋㅋㅋ",
          "original_form": "완전 쫀뜩하고 맛있었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:45"
        },
        {
          "id": "MDRW2100036206.1.11",
          "form": "헐ㅠㅠ 요즘 햇감자로 감자전하면 그렇게 맛있다던데!!",
          "original_form": "헐ㅠㅠ 요즘 햇감자로 감자전하면 그렇게 맛있다던데!!",
          "speaker_id": "1",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100036206.1.12",
          "form": "저도 감자 좋아해용ㅎㅎ",
          "original_form": "저도 감자 좋아해용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100036206.1.13",
          "form": "맞아요 완전 쫀득하고 맛있었어요 ㅋㅋ",
          "original_form": "맞아요 완전 쫀득하고 맛있었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100036206.1.14",
          "form": "저는 찐감자는 별로 안좋아하는데 역시 튀기니까 맛있네요 ㅋㅋㅋ",
          "original_form": "저는 찐감자는 별로 안좋아하는데 역시 튀기니까 맛있네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100036206.1.15",
          "form": "바삭바삭ㅋㅋㅋ 카레에 고기 들어간거 좋아하시나용??",
          "original_form": "바삭바삭ㅋㅋㅋ 카레에 고기 들어간거 좋아하시나용??",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100036206.1.16",
          "form": "네!!!! 전 육식인간이라 끼니마다 고기있어야해요 ㅋㅋㅋㅋ",
          "original_form": "네!!!! 전 육식인간이라 끼니마다 고기있어야해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100036206.1.17",
          "form": "카레에도 고기듬뿍!!",
          "original_form": "카레에도 고기듬뿍!!",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100036206.1.18",
          "form": "엇 저두요!ㅋㅋㅋㅋ",
          "original_form": "엇 저두요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100036206.1.19",
          "form": "카레에 브로콜리 넣었다가",
          "original_form": "카레에 브로콜리 넣었다가",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100036206.1.20",
          "form": "비난받았어요 오늘ㅠㅠㅋㅋㅋ",
          "original_form": "비난받았어요 오늘ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100036206.1.21",
          "form": "ㅋㅋㅋㅋ앜ㅋㅋㅋㅋ먹어보진않았지만 잘게썰어넣으면 맛있을것같은데!!",
          "original_form": "ㅋㅋㅋㅋ앜ㅋㅋㅋㅋ먹어보진않았지만 잘게썰어넣으면 맛있을것같은데!!",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036206.1.22",
          "form": "이걸 누구먹으라고 만들었냐~ 브로콜리 너만 좋아한다~",
          "original_form": "이걸 누구먹으라고 만들었냐~ 브로콜리 너만 좋아한다~",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036206.1.23",
          "form": "놀리더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "놀리더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036206.1.24",
          "form": "브로콜리 안좋아하시죠?ㅠㅠㅠ",
          "original_form": "브로콜리 안좋아하시죠?ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036206.1.25",
          "form": "ㅋㅋㅋ브로콜리.. 좋아하진않지만 그래도 건강을위해서 억지로먹어요ㅠ ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ브로콜리.. 좋아하진않지만 그래도 건강을위해서 억지로먹어요ㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100036206.1.26",
          "form": "넹넹 이거 간식으로 드시면",
          "original_form": "넹넹 이거 간식으로 드시면",
          "speaker_id": "1",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100036206.1.27",
          "form": "전에 회사에서 브로콜리를튀겨서",
          "original_form": "전에 회사에서 브로콜리를튀겨서",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100036206.1.28",
          "form": "살도 안찌고 좋아요!",
          "original_form": "살도 안찌고 좋아요!",
          "speaker_id": "1",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100036206.1.29",
          "form": "브로콜리탕수육으로 나온적이있는데 역시나 튀겨서그런지",
          "original_form": "브로콜리탕수육으로 나온적이있는데 역시나 튀겨서그런지",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100036206.1.30",
          "form": "그건맛있었어요! ㅋㅋㅋㅋ",
          "original_form": "그건맛있었어요! ㅋㅋㅋㅋ",
