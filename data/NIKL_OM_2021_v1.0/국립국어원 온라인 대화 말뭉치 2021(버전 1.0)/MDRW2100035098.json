      "utterance": [
        {
          "id": "MDRW2100035098.1.1",
          "form": "맞아요 기술ㅋㅋㅋ",
          "original_form": "맞아요 기술ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035098.1.2",
          "form": "티비보는거 있으세요??",
          "original_form": "티비보는거 있으세요??",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035098.1.3",
          "form": "최근에 영화 뭐 보셨어용?",
          "original_form": "최근에 영화 뭐 보셨어용?",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035098.1.4",
          "form": "저저",
          "original_form": "저저",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035098.1.5",
          "form": "대탈출 런닝맨 심야괴담회",
          "original_form": "대탈출 런닝맨 심야괴담회",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035098.1.6",
          "form": "이렇게 봐용!",
          "original_form": "이렇게 봐용!",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035098.1.7",
          "form": "대탈출은 저번시즌까진 봤는데",
          "original_form": "대탈출은 저번시즌까진 봤는데",
          "speaker_id": "2",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100035098.1.8",
          "form": "이번시즌 다 그저그랬고 저번주가 진짜 재밌었어요ㅋㅋㅋㅋ",
          "original_form": "이번시즌 다 그저그랬고 저번주가 진짜 재밌었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100035098.1.9",
          "form": "무슨 해외재난영화같은거였는데 기억이안나네요ㅠ제목이",
          "original_form": "무슨 해외재난영화같은거였는데 기억이안나네요ㅠ제목이",
          "speaker_id": "2",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100035098.1.10",
          "form": "해외재난이용?",
          "original_form": "해외재난이용?",
          "speaker_id": "1",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100035098.1.11",
          "form": "심야괴담회보는분많네요",
          "original_form": "심야괴담회보는분많네요",
          "speaker_id": "2",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100035098.1.12",
          "form": "영화제목 ㅠㅠ",
          "original_form": "영화제목 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100035098.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100035098.1.14",
          "form": "궁금해용 재난영화도 좋아해서ㅋㅋㅋ",
          "original_form": "궁금해용 재난영화도 좋아해서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100035098.1.15",
          "form": "그 더락 나온 재난영화",
          "original_form": "그 더락 나온 재난영화",
          "speaker_id": "1",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100035098.1.16",
          "form": "진짜 재밌게 봤어요",
          "original_form": "진짜 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100035098.1.17",
          "form": "청각장애있는여자아이가",
          "original_form": "청각장애있는여자아이가",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100035098.1.18",
          "form": "세상?을 구하는거였는데 흠",
          "original_form": "세상?을 구하는거였는데 흠",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100035098.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100035098.1.20",
          "form": "헐 보고싶어요ㅋㅋㅋㅋ",
          "original_form": "헐 보고싶어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100035098.1.21",
          "form": "제가 본건",
          "original_form": "제가 본건",
          "speaker_id": "1",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100035098.1.22",
          "form": "샌 안드레아스에용",
          "original_form": "샌 안드레아스에용",
          "speaker_id": "1",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035098.1.23",
          "form": "시즌 2라했는데 ㅋㅋㅋ1이랑 연결안되서 그냥 옆에서 보길래 따라봣어요 ㅋㅋㅋ",
          "original_form": "시즌 2라했는데 ㅋㅋㅋ1이랑 연결안되서 그냥 옆에서 보길래 따라봣어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035098.1.24",
          "form": "엄청큰 괴수괴물이나오는영화인데 ㅋㅋ",
          "original_form": "엄청큰 괴수괴물이나오는영화인데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035098.1.25",
          "form": "ㅋㅋㅋ누가 보는거 같이 보다가 저도 빠져드는 타입이라ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ누가 보는거 같이 보다가 저도 빠져드는 타입이라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035098.1.26",
          "form": "영화 취향이 어떻게 되세용?",
          "original_form": "영화 취향이 어떻게 되세용?",
          "speaker_id": "1",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035098.1.27",
          "form": "저는 범죄 수사물 장르물 액션 좋아해요!",
          "original_form": "저는 범죄 수사물 장르물 액션 좋아해요!",
          "speaker_id": "1",
          "time": "20210829 02:40"
        }
      ]
    }
