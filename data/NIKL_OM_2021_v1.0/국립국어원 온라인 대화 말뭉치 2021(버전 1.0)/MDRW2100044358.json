      "utterance": [
        {
          "id": "MDRW2100044358.1.1",
          "form": "네 메뉴 다양하고 \r\n제가 조리 안하고 정말 좋아요",
          "original_form": "네 메뉴 다양하고 \r\n제가 조리 안하고 정말 좋아요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044358.1.2",
          "form": "지금 사촌동생분이 요리 잘하세요?",
          "original_form": "지금 사촌동생분이 요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044358.1.3",
          "form": "음 잘 모르겠어요 오늘 처음 얻어먹은 거라",
          "original_form": "음 잘 모르겠어요 오늘 처음 얻어먹은 거라",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044358.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044358.1.5",
          "form": "아~ㅋㅋㅋㅋ",
          "original_form": "아~ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044358.1.6",
          "form": "본인 말로는 뭐 이거저거 잘한대요..",
          "original_form": "본인 말로는 뭐 이거저거 잘한대요..",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044358.1.7",
          "form": "두분이서 번갈아하시나했는데 아니였네요ㅎ",
          "original_form": "두분이서 번갈아하시나했는데 아니였네요ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044358.1.8",
          "form": "ㅋㅋㅋ제가 더 많이 한 것 같아요",
          "original_form": "ㅋㅋㅋ제가 더 많이 한 것 같아요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044358.1.9",
          "form": "잘하는지 이거저거 만들어 달라해봐야겠네요ㅋㅋㅋ",
          "original_form": "잘하는지 이거저거 만들어 달라해봐야겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044358.1.10",
          "form": "저희집에 온 거라 대접한답시고ㅠ",
          "original_form": "저희집에 온 거라 대접한답시고ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044358.1.11",
          "form": "전 주로 밀키트였지만..",
          "original_form": "전 주로 밀키트였지만..",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044358.1.12",
          "form": "ㅋㅋㅋ name2님은 요리잘안하세오?",
          "original_form": "ㅋㅋㅋ &name2&님은 요리잘안하세오?",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044358.1.13",
          "form": "아~ㅋㅋ밀키트가 요즘 편리하게 잘 나오더라구요",
          "original_form": "아~ㅋㅋ밀키트가 요즘 편리하게 잘 나오더라구요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044358.1.14",
          "form": "전 요리 안해요ㅎ",
          "original_form": "전 요리 안해요ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044358.1.15",
          "form": "하는건좋은데 뒷정리 싫어해서 차라리 안해버려요",
          "original_form": "하는건좋은데 뒷정리 싫어해서 차라리 안해버려요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044358.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044358.1.17",
          "form": "저도 음식하는 거보다",
          "original_form": "저도 음식하는 거보다",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044358.1.18",
          "form": "치우는 게 더 싫더라구요",
          "original_form": "치우는 게 더 싫더라구요",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044358.1.19",
          "form": "맞아요 요리는 먹고나서가 더 일이라ㅋㅋ",
          "original_form": "맞아요 요리는 먹고나서가 더 일이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044358.1.20",
          "form": "혹시 설거지도 싫어하시나요",
          "original_form": "혹시 설거지도 싫어하시나요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044358.1.21",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044358.1.22",
          "form": "가끔 손님와서 요리하면 뒷정리는 맡겨요ㅋ",
          "original_form": "가끔 손님와서 요리하면 뒷정리는 맡겨요ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044358.1.23",
          "form": "기본 손맛은 있더라구요ㅋㅋ\r\n그래서 혼자 있을 땐 요리 잘안해요\r\n뒷정리 싫어서ㅋㅋ",
          "original_form": "기본 손맛은 있더라구요ㅋㅋ\r\n그래서 혼자 있을 땐 요리 잘안해요\r\n뒷정리 싫어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044358.1.24",
          "form": "ㅋㅋㅋ그쵸 요새 바쁜회사원들은",
          "original_form": "ㅋㅋㅋ그쵸 요새 바쁜회사원들은",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044358.1.25",
          "form": "거의 다 사먹는 것 같아요",
          "original_form": "거의 다 사먹는 것 같아요",
          "speaker_id": "1",
          "time": "20210907 22:33"
        }
      ]
