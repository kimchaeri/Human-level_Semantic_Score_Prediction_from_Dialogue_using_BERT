      "utterance": [
        {
          "id": "MDRW2100010615.1.1",
          "form": "오늘 날씨 어때요?",
          "original_form": "오늘 날씨 어때요?",
          "speaker_id": "1",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010615.1.2",
          "form": "비오고있어요",
          "original_form": "비오고있어요",
          "speaker_id": "2",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010615.1.3",
          "form": "전 비오는소리 좋던데",
          "original_form": "전 비오는소리 좋던데",
          "speaker_id": "1",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010615.1.4",
          "form": "저도 비오는 날 좋아요",
          "original_form": "저도 비오는 날 좋아요",
          "speaker_id": "2",
          "time": "20210823 20:05"
        },
        {
          "id": "MDRW2100010615.1.5",
          "form": "눈도 좋아하세요?",
          "original_form": "눈도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 20:05"
        },
        {
          "id": "MDRW2100010615.1.6",
          "form": "흠 눈은 볼때 예쁘긴 하죠 ㅎㅎ 그 후에 고통이",
          "original_form": "흠 눈은 볼때 예쁘긴 하죠 ㅎㅎ 그 후에 고통이",
          "speaker_id": "2",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010615.1.7",
          "form": "맞아요 저도 별로",
          "original_form": "맞아요 저도 별로",
          "speaker_id": "1",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010615.1.8",
          "form": "날이 금방 시원해졌어요",
          "original_form": "날이 금방 시원해졌어요",
          "speaker_id": "2",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010615.1.9",
          "form": "잠자리도 많더라구요",
          "original_form": "잠자리도 많더라구요",
          "speaker_id": "1",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010615.1.10",
          "form": "오잉 잠자리는 못봤네요 요즘. 에어컨 안틀어좋아요",
          "original_form": "오잉 잠자리는 못봤네요 요즘. 에어컨 안틀어좋아요",
          "speaker_id": "2",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010615.1.11",
          "form": "전 에어콘 아직 틀어요ㅠ",
          "original_form": "전 에어콘 아직 틀어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010615.1.12",
          "form": "종일요? 저흰 자기전 잠깐만요",
          "original_form": "종일요? 저흰 자기전 잠깐만요",
          "speaker_id": "2",
          "time": "20210823 20:40"
        },
        {
          "id": "MDRW2100010615.1.13",
          "form": "저도 잘때ㅎ",
          "original_form": "저도 잘때ㅎ",
          "speaker_id": "1",
