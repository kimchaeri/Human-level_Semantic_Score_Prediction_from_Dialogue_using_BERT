      "utterance": [
        {
          "id": "MDRW2100002396.1.1",
          "form": "영화 좋아하시나요",
          "original_form": "영화 좋아하시나요",
          "speaker_id": "1",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002396.1.2",
          "form": "네 엄청 좋아해요",
          "original_form": "네 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002396.1.3",
          "form": "장르 뭐 좋아하세요",
          "original_form": "장르 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002396.1.4",
          "form": "전 안가리고 다좋아하는데\n모가디슈 넘 보고싶네용 흑",
          "original_form": "전 안가리고 다좋아하는데\n모가디슈 넘 보고싶네용 흑",
          "speaker_id": "2",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002396.1.5",
          "form": "코로나 싫어요",
          "original_form": "코로나 싫어요",
          "speaker_id": "2",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002396.1.6",
          "form": "인생 영화 있으세요?",
          "original_form": "인생 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002396.1.7",
          "form": "저는 인생은 아름다워 요",
          "original_form": "저는 인생은 아름다워 요",
          "speaker_id": "2",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002396.1.8",
          "form": "저는 빅쇼트 좋아해요",
          "original_form": "저는 빅쇼트 좋아해요",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002396.1.9",
          "form": "전 못본거네요 주말에 봐바야 겠어요 ㅎㅎ",
          "original_form": "전 못본거네요 주말에 봐바야 겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002396.1.10",
          "form": "네 재밌어요 추천해요",
          "original_form": "네 재밌어요 추천해요",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002396.1.11",
          "form": "앗 기대되에요",
          "original_form": "앗 기대되에요",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002396.1.12",
          "form": "되네요",
          "original_form": "되네요",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002396.1.13",
          "form": "네 로맨틱 코미디도 좋아요",
          "original_form": "네 로맨틱 코미디도 좋아요",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002396.1.14",
          "form": "주말에 한번에 몰아보기 ㅎㅎ",
          "original_form": "주말에 한번에 몰아보기 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002396.1.15",
          "form": "넷플 보기 좋죠",
          "original_form": "넷플 보기 좋죠",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002396.1.16",
          "form": "근데 시간이 안가네여 ㅡㅡㅋ",
