      "utterance": [
        {
          "id": "MDRW2100046149.1.1",
          "form": "빵너무 좋아해요",
          "original_form": "빵너무 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046149.1.2",
          "form": "빵순이 ㅋ",
          "original_form": "빵순이 ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046149.1.3",
          "form": "저도요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046149.1.4",
          "form": "무슨 빵 좋아하세요?*",
          "original_form": "무슨 빵 좋아하세요?*",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046149.1.5",
          "form": "소보로요",
          "original_form": "소보로요",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046149.1.6",
          "form": "저도 소보로 참 좋아해요 크림빵도!!",
          "original_form": "저도 소보로 참 좋아해요 크림빵도!!",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046149.1.7",
          "form": "성심당 튀소 아세요?",
          "original_form": "성심당 튀소 아세요?",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046149.1.8",
          "form": "네 맛있더라고요 ㅠㅠ",
          "original_form": "네 맛있더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046149.1.9",
          "form": "멀어서 잘 못먹지만...",
          "original_form": "멀어서 잘 못먹지만...",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046149.1.10",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046149.1.11",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046149.1.12",
          "form": "대전가면 꼭 들렸다와요",
          "original_form": "대전가면 꼭 들렸다와요",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046149.1.13",
          "form": "진짜 맛있더라고요",
          "original_form": "진짜 맛있더라고요",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046149.1.14",
          "form": "부추같은거 든것도",
          "original_form": "부추같은거 든것도",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046149.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046149.1.16",
          "form": "부추빵도 나름별미",
          "original_form": "부추빵도 나름별미",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046149.1.17",
          "form": "네 그게 맛있었어요",
          "original_form": "네 그게 맛있었어요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046149.1.18",
          "form": "거기 케익도 맛있다더라고요",
          "original_form": "거기 케익도 맛있다더라고요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046149.1.19",
          "form": "다 맛나고",
          "original_form": "다 맛나고",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046149.1.20",
          "form": "지난번에 보니 곰세마리 빵도 귀엽더라구요",
          "original_form": "지난번에 보니 곰세마리 빵도 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046149.1.21",
          "form": "꺄!!!",
          "original_form": "꺄!!!",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046149.1.22",
          "form": "곰세마리가 그려져 있나요??",
          "original_form": "곰세마리가 그려져 있나요??",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046149.1.23",
          "form": "모양도 곰세마리요",
          "original_form": "모양도 곰세마리요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046149.1.24",
          "form": "안에 앙꼬는 초코같더라구요",
          "original_form": "안에 앙꼬는 초코같더라구요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046149.1.25",
          "form": "귀엽겠어요!!",
          "original_form": "귀엽겠어요!!",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046149.1.26",
          "form": "네 너무 앙증맞더라구요",
          "original_form": "네 너무 앙증맞더라구요",
          "speaker_id": "2",
          "time": "20210908 10:56"
        }
      ]
