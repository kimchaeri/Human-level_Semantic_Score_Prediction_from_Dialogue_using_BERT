      "utterance": [
        {
          "id": "MDRW2100005176.1.1",
          "form": "언니ㅋㅋ뭐해?",
          "original_form": "언니ㅋㅋ뭐해?",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005176.1.2",
          "form": "너는 누구냐 ㅋㅋㅋ",
          "original_form": "너는 누구냐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005176.1.3",
          "form": "name1지ㅋㅋㅋㅋ",
          "original_form": "&name1&지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005176.1.4",
          "form": "애들은 자?",
          "original_form": "애들은 자?",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005176.1.5",
          "form": "씻겼어?",
          "original_form": "씻겼어?",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005176.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ 엉 씻기고 왔다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 엉 씻기고 왔다",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005176.1.7",
          "form": "나도 씻고",
          "original_form": "나도 씻고",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005176.1.8",
          "form": "이시아 혼자 갔다온겨??",
          "original_form": "이시아 혼자 갔다온겨??",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005176.1.9",
          "form": "애들 재우고왔다.",
          "original_form": "애들 재우고왔다.",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005176.1.10",
          "form": "오늘 애들데리고 형님댁 다녀왔거든",
          "original_form": "오늘 애들데리고 형님댁 다녀왔거든",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005176.1.11",
          "form": "나도 방금 육퇴",
          "original_form": "나도 방금 육퇴",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005176.1.12",
          "form": "가는길에 이시아들렸지",
          "original_form": "가는길에 이시아들렸지",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005176.1.13",
          "form": "어이구",
          "original_form": "어이구",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005176.1.14",
          "form": "비슷하게 애들 재웠네ㅋ",
          "original_form": "비슷하게 애들 재웠네ㅋ",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.15",
          "form": "멀리갔다왔네이",
          "original_form": "멀리갔다왔네이",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.16",
          "form": "심심해서 다녀왔다.ㅋㅋ",
          "original_form": "심심해서 다녀왔다.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.17",
          "form": "우리 낮에 함께했는데",
          "original_form": "우리 낮에 함께했는데",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.18",
          "form": "밥 맛있게 먹고왔어ㅋㅋ",
          "original_form": "밥 맛있게 먹고왔어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.19",
          "form": "언제 형님댁도 갔노 ㅋㅋㅋㅋ",
          "original_form": "언제 형님댁도 갔노 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.20",
          "form": "ㅋㅋㄱ잘했네 주말에 밥하기 너무 귀찮음",
          "original_form": "ㅋㅋㄱ잘했네 주말에 밥하기 너무 귀찮음",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.21",
          "form": "ㅋㅋㅋㅋ애들도 우리도 심심해서ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ애들도 우리도 심심해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.22",
          "form": "응 둘째가 두그릇먹었음ㅋㅋ",
          "original_form": "응 둘째가 두그릇먹었음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.23",
          "form": "맛있었나봐ㅋㅋ",
          "original_form": "맛있었나봐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005176.1.24",
          "form": "어이쿠 잘먹네이",
          "original_form": "어이쿠 잘먹네이",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005176.1.25",
          "form": "응 잘먹었어.ㅋㅋ",
          "original_form": "응 잘먹었어.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005176.1.26",
          "form": "우리는 삼겹살 구워먹었는데",
          "original_form": "우리는 삼겹살 구워먹었는데",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005176.1.27",
          "form": "고기가 별로드라",
          "original_form": "고기가 별로드라",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005176.1.28",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005176.1.29",
          "form": "아구 어디서샀는데,",
          "original_form": "아구 어디서샀는데,",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005176.1.30",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005176.1.31",
          "form": "이제 거기 끊어야겠다 엠스퀘어 건물 고깃집",
          "original_form": "이제 거기 끊어야겠다 엠스퀘어 건물 고깃집",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005176.1.32",
          "form": "빵집옆에",
          "original_form": "빵집옆에",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005176.1.33",
          "form": "질겨가지고",
          "original_form": "질겨가지고",
          "speaker_id": "2",
          "time": "20210821 22:53"
        }
      ]
    }
