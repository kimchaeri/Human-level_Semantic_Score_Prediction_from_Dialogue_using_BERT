      "utterance": [
        {
          "id": "MDRW2100029154.1.1",
          "form": "노상업 노제휴",
          "original_form": "노상업 노제휴",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029154.1.2",
          "form": "영화는 한번ㅆㅣㄱ 봄",
          "original_form": "영화는 한번ㅆㅣㄱ 봄",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029154.1.3",
          "form": "월급도 엄청 오름",
          "original_form": "월급도 엄청 오름",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029154.1.4",
          "form": "내가",
          "original_form": "내가",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029154.1.5",
          "form": "알게돼서",
          "original_form": "알게돼서",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029154.1.6",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029154.1.7",
          "form": "신천지인거 전혀모르고",
          "original_form": "신천지인거 전혀모르고",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029154.1.8",
          "form": "동아리 활동 하듯이 만났나요",
          "original_form": "동아리 활동 하듯이 만났나요",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029154.1.9",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029154.1.10",
          "form": "학교는 잘 다녔대요",
          "original_form": "학교는 잘 다녔대요",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029154.1.11",
          "form": "회사다녔을때지",
          "original_form": "회사다녔을때지",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.12",
          "form": "성악은 계속하고 있어요",
          "original_form": "성악은 계속하고 있어요",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.13",
          "form": "걔를 어케알게됐나면",
          "original_form": "걔를 어케알게됐나면",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.14",
          "form": "회사 다녔을 때 구나",
          "original_form": "회사 다녔을 때 구나",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.15",
          "form": "고등학교",
          "original_form": "고등학교",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.16",
          "form": "짝꿍으로",
          "original_form": "짝꿍으로",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.17",
          "form": "한번만났던앤데",
          "original_form": "한번만났던앤데",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.18",
          "form": "고등학교 짝꿍이면 추억 많은데",
          "original_form": "고등학교 짝꿍이면 추억 많은데",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.19",
          "form": "대학교에 갔는데",
          "original_form": "대학교에 갔는데",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.20",
          "form": "걔를 동아리에서 본거야",
          "original_form": "걔를 동아리에서 본거야",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029154.1.21",
          "form": "반가웠겠다",
          "original_form": "반가웠겠다",
          "speaker_id": "2",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029154.1.22",
          "form": "영어 타임지 해석하고 공부하는",
          "original_form": "영어 타임지 해석하고 공부하는",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029154.1.23",
          "form": "동아리였지",
          "original_form": "동아리였지",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029154.1.24",
          "form": "멋있어 보였겠는데",
          "original_form": "멋있어 보였겠는데",
          "speaker_id": "2",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029154.1.25",
          "form": "거기서 만나서",
          "original_form": "거기서 만나서",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029154.1.26",
          "form": "친하게 지냈지",
          "original_form": "친하게 지냈지",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029154.1.27",
          "form": "술도 많이 마시고",
          "original_form": "술도 많이 마시고",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029154.1.28",
          "form": "멋짐ㅋㅋ",
          "original_form": "멋짐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:27"
        }
      ]
    }
