      "utterance": [
        {
          "id": "MDRW2100045645.1.1",
          "form": "무슨 감자칩 제일 좋아하세요?",
          "original_form": "무슨 감자칩 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045645.1.2",
          "form": "저는 스윙칩 빨간거요",
          "original_form": "저는 스윙칩 빨간거요",
          "speaker_id": "2",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045645.1.3",
          "form": "오 스윙칩 맛잇죠",
          "original_form": "오 스윙칩 맛잇죠",
          "speaker_id": "1",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045645.1.4",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210908 04:43"
        },
        {
          "id": "MDRW2100045645.1.5",
          "form": "전 수미칩이여 ㅋㅋㅋㅋ",
          "original_form": "전 수미칩이여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:43"
        },
        {
          "id": "MDRW2100045645.1.6",
          "form": "수미칩 사실 가격만 똑같으면 저도 수미칩 택할거 같아요",
          "original_form": "수미칩 사실 가격만 똑같으면 저도 수미칩 택할거 같아요",
          "speaker_id": "2",
          "time": "20210908 04:44"
        },
        {
          "id": "MDRW2100045645.1.7",
          "form": "수미칩 비싸고 맛잇죠 ㅠㅠ",
          "original_form": "수미칩 비싸고 맛잇죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:45"
        },
        {
          "id": "MDRW2100045645.1.8",
          "form": "그쵸 ㅋㅋ 가끔 세일할때 주워오는데",
          "original_form": "그쵸 ㅋㅋ 가끔 세일할때 주워오는데",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045645.1.9",
          "form": "저는 눈을감자나 쟈가비도 좋아해요",
          "original_form": "저는 눈을감자나 쟈가비도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 04:50"
        },
        {
          "id": "MDRW2100045645.1.10",
          "form": "저도 눈을감자 좋아해요",
          "original_form": "저도 눈을감자 좋아해요",
          "speaker_id": "1",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045645.1.11",
          "form": "오도독오도독",
          "original_form": "오도독오도독",
          "speaker_id": "1",
          "time": "20210908 04:52"
        },
        {
          "id": "MDRW2100045645.1.12",
          "form": "맞아요 식감이 있죠 ㅋㅋㅋ 근데 양이 너무 작아서 아쉽..",
          "original_form": "맞아요 식감이 있죠 ㅋㅋㅋ 근데 양이 너무 작아서 아쉽..",
          "speaker_id": "2",
          "time": "20210908 04:53"
        },
        {
          "id": "MDRW2100045645.1.13",
          "form": "맞아요 ㅠㅠ 감자과자는 양이 다들...",
          "original_form": "맞아요 ㅠㅠ 감자과자는 양이 다들...",
          "speaker_id": "1",
          "time": "20210908 04:53"
        },
        {
          "id": "MDRW2100045645.1.14",
          "form": "진짜 왜 그럴까요.. 감자가 많이 들어가나",
          "original_form": "진짜 왜 그럴까요.. 감자가 많이 들어가나",
          "speaker_id": "2",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100045645.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ질소만 많이넣고 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ질소만 많이넣고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100045645.1.16",
          "form": "진짜 우리나라만 과자 안전을 너무 신경쓰는거 같아요..",
          "original_form": "진짜 우리나라만 과자 안전을 너무 신경쓰는거 같아요..",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100045645.1.17",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100045645.1.18",
          "form": "감자과자말고 다른과자는 좋아하세요?",
          "original_form": "감자과자말고 다른과자는 좋아하세요?",
