      "utterance": [
        {
          "id": "MDRW2100043481.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043481.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043481.1.3",
          "form": "요즘 보는 프로 있으세요?",
          "original_form": "요즘 보는 프로 있으세요?",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043481.1.4",
          "form": "전 요즘 환승연애 대탈출 디피 요런거 보고있어요",
          "original_form": "전 요즘 환승연애 대탈출 디피 요런거 보고있어요",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043481.1.5",
          "form": "어떤걸주로 보시나요?",
          "original_form": "어떤걸주로 보시나요?",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043481.1.6",
          "form": "전 예능이나 드라마요!",
          "original_form": "전 예능이나 드라마요!",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043481.1.7",
          "form": "대탈출 저도 봐요\r\n넘 재밋어요",
          "original_form": "대탈출 저도 봐요\r\n넘 재밋어요",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043481.1.8",
          "form": "그쵸 ㅋㅋ 대탈출 재밌는데 무섭고 징그러워요",
          "original_form": "그쵸 ㅋㅋ 대탈출 재밌는데 무섭고 징그러워요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043481.1.9",
          "form": "너무 잘만드는거 같애요",
          "original_form": "너무 잘만드는거 같애요",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043481.1.10",
          "form": "좀비편에서 깜놀 많이 했어요",
          "original_form": "좀비편에서 깜놀 많이 했어요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043481.1.11",
          "form": "저도 진짜 숨어서봐요",
          "original_form": "저도 진짜 숨어서봐요",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043481.1.12",
          "form": "맞아요 그 무의식편도요ㅠㅠ",
          "original_form": "맞아요 그 무의식편도요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043481.1.13",
          "form": "악마가 방문열고 등장했을때 소름..",
          "original_form": "악마가 방문열고 등장했을때 소름..",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043481.1.14",
          "form": "와 거기 그 악마 진짜 무서웟어요",
          "original_form": "와 거기 그 악마 진짜 무서웟어요",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043481.1.15",
          "form": "그쵸 저도 진짜 악마 등장했을때 이어폰끼고있다가 깜짝놀랐어요 ㅋㅋ",
          "original_form": "그쵸 저도 진짜 악마 등장했을때 이어폰끼고있다가 깜짝놀랐어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043481.1.16",
          "form": "요즘 재밌게 보시는거 대탈출말곤 뭐 있으세요?",
          "original_form": "요즘 재밌게 보시는거 대탈출말곤 뭐 있으세요?",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
