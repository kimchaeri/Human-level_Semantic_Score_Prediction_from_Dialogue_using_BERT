      "utterance": [
        {
          "id": "MDRW2100021810.1.1",
          "form": "요새 날씨가 많이 쌀쌀해졌어요",
          "original_form": "요새 날씨가 많이 쌀쌀해졌어요",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021810.1.2",
          "form": "맞아요! 밤엔 문 다 닫고 자야할 정도에요",
          "original_form": "맞아요! 밤엔 문 다 닫고 자야할 정도에요",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021810.1.3",
          "form": "헛 ㅋㅋㅋ저도 방금 창문닫았어요",
          "original_form": "헛 ㅋㅋㅋ저도 방금 창문닫았어요",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021810.1.4",
          "form": "진짜 가을인가봐요",
          "original_form": "진짜 가을인가봐요",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021810.1.5",
          "form": "호빵먹고싶네요",
          "original_form": "호빵먹고싶네요",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021810.1.6",
          "form": "붕어빵도! 호떡도! 먹고싶네요 ㅋㅋㅋ",
          "original_form": "붕어빵도! 호떡도! 먹고싶네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021810.1.7",
          "form": "ㅋㅋㅋㅋ 역시\r\n날씨는 먹는데 중요해요",
          "original_form": "ㅋㅋㅋㅋ 역시\r\n날씨는 먹는데 중요해요",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021810.1.8",
          "form": "국물요리도 겨울에 더 맛있구요! ㅎㅎ 귤 한바구니 장판안에서 까먹고 싶네요",
          "original_form": "국물요리도 겨울에 더 맛있구요! ㅎㅎ 귤 한바구니 장판안에서 까먹고 싶네요",
          "speaker_id": "2",
          "time": "20210829 07:59"
        },
        {
          "id": "MDRW2100021810.1.9",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100021810.1.10",
          "form": "오뎅탕을 먹어도 날씨가중요해요",
          "original_form": "오뎅탕을 먹어도 날씨가중요해요",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100021810.1.11",
          "form": "오늘처럼 이렇게 비가 오면 또 전이 먹고싶네요 ㅋㅋ",
          "original_form": "오늘처럼 이렇게 비가 오면 또 전이 먹고싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100021810.1.12",
          "form": "파전같은거 좋아하세요?",
          "original_form": "파전같은거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100021810.1.13",
          "form": "막걸리랑",
          "original_form": "막걸리랑",
          "speaker_id": "1",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100021810.1.14",
          "form": "네!!! 해물파전 넘넘 좋아요! 감자전도 좋아합니다. name1은요?",
          "original_form": "네!!! 해물파전 넘넘 좋아요! 감자전도 좋아합니다. &name1&은요?",
          "speaker_id": "2",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100021810.1.15",
          "form": "헐 저도요 해물최애에요 ㅎㅎㅎ",
          "original_form": "헐 저도요 해물최애에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100021810.1.16",
          "form": "녹두전 기가막힌곳 가면 녹두전도 맛있어요",
          "original_form": "녹두전 기가막힌곳 가면 녹두전도 맛있어요",
          "speaker_id": "1",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100021810.1.17",
          "form": "녹두전은 안먹어본거같아요..! 담에 맛있는 녹두전 먹으러 검색하고 찾아가볼게요!!!@",
          "original_form": "녹두전은 안먹어본거같아요..! 담에 맛있는 녹두전 먹으러 검색하고 찾아가볼게요!!!@",
          "speaker_id": "2",
          "time": "20210830 14:22"
        },
        {
          "id": "MDRW2100021810.1.18",
          "form": "꼭 드세요 진짜 묘하게 맛있어요",
          "original_form": "꼭 드세요 진짜 묘하게 맛있어요",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100021810.1.19",
          "form": "맛의 신세계에요",
          "original_form": "맛의 신세계에요",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100021810.1.20",
          "form": "오늘도 비오네요",
          "original_form": "오늘도 비오네요",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100021810.1.21",
