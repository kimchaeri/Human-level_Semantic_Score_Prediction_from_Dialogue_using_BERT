      "utterance": [
        {
          "id": "MDRW2100019065.1.1",
          "form": "아 여행!!!!",
          "original_form": "아 여행!!!!",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019065.1.2",
          "form": "ㅋㅋㅋ 여행좋아하시나요?",
          "original_form": "ㅋㅋㅋ 여행좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019065.1.3",
          "form": "물론이죠!!",
          "original_form": "물론이죠!!",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019065.1.4",
          "form": "코로나만아니었다면...^^",
          "original_form": "코로나만아니었다면...^^",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019065.1.5",
          "form": "사진찍고 여향하는게 취미라",
          "original_form": "사진찍고 여향하는게 취미라",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019065.1.6",
          "form": "맞아요.. 저두 여행은 남는게 사진이라고 생각하는주의!!",
          "original_form": "맞아요.. 저두 여행은 남는게 사진이라고 생각하는주의!!",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019065.1.7",
          "form": "여행어디가보셨어요?",
          "original_form": "여행어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019065.1.8",
          "form": "오키나와랑 파리요!",
          "original_form": "오키나와랑 파리요!",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019065.1.9",
          "form": "국내는 거의 다 가본거같아여!",
          "original_form": "국내는 거의 다 가본거같아여!",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019065.1.10",
          "form": "우와~ 국내여행 추천지 있으신가요??",
          "original_form": "우와~ 국내여행 추천지 있으신가요??",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019065.1.11",
          "form": "음,,,저는 강릉이요!",
          "original_form": "음,,,저는 강릉이요!",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019065.1.12",
          "form": "흔한 추천지지만",
          "original_form": "흔한 추천지지만",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019065.1.13",
          "form": "괜찮더라구여!",
          "original_form": "괜찮더라구여!",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019065.1.14",
          "form": "아하~ 강릉 한번도안가봤는데 위시에 넣어봐야겠어욮ㅎㅎ",
          "original_form": "아하~ 강릉 한번도안가봤는데 위시에 넣어봐야겠어욮ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019065.1.15",
          "form": "여행하는거 좋아하세요??!!",
          "original_form": "여행하는거 좋아하세요??!!",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019065.1.16",
          "form": "네~ 국내해외 다 좋아해요 ㅎㅎ",
          "original_form": "네~ 국내해외 다 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100019065.1.17",
          "form": "어디어디가보셨나요~~!",
          "original_form": "어디어디가보셨나요~~!",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019065.1.18",
          "form": "음 다양하게가봤는데 젤 기억남는곳은 국내로는 여수였고 해외는 미국이요!",
          "original_form": "음 다양하게가봤는데 젤 기억남는곳은 국내로는 여수였고 해외는 미국이요!",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100019065.1.19",
          "form": "우아아아아미국!!!!",
          "original_form": "우아아아아미국!!!!",
          "speaker_id": "2",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100019065.1.20",
          "form": "앜ㅋㅋㅋ미국 좋아하시나봐요??",
          "original_form": "앜ㅋㅋㅋ미국 좋아하시나봐요??",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019065.1.21",
          "form": "가고싶어요 ㅠㅠㅠ",
          "original_form": "가고싶어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100019065.1.22",
          "form": "저두 또가고싶어요 ㅠㅠ 개인적으로 미국은 서부가 좋은거같아요",
          "original_form": "저두 또가고싶어요 ㅠㅠ 개인적으로 미국은 서부가 좋은거같아요",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019065.1.23",
          "form": "우앙...저는 진짜 꼭 가보고싶어요",
          "original_form": "우앙...저는 진짜 꼭 가보고싶어요",
