      "utterance": [
        {
          "id": "MDRW2100045862.1.1",
          "form": "우오 계란 샌드위치에 스크램블이라니 아침에 먹으면 기분 좋아질 것 같아요",
          "original_form": "우오 계란 샌드위치에 스크램블이라니 아침에 먹으면 기분 좋아질 것 같아요",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045862.1.2",
          "form": "앗 마지막에 보내신 거 잘려서 못 봤어요",
          "original_form": "앗 마지막에 보내신 거 잘려서 못 봤어요",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045862.1.3",
          "form": "뭐 드신다구요?",
          "original_form": "뭐 드신다구요?",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045862.1.4",
          "form": "여기에 밀크티도 같이 마셔요 아 밀크티 좋아하세요?",
          "original_form": "여기에 밀크티도 같이 마셔요 아 밀크티 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045862.1.5",
          "form": "네 밀크티 좋아해요 ㅎㅎㅎ",
          "original_form": "네 밀크티 좋아해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100045862.1.6",
          "form": "밀크 버블티를 특히 좋아하는데",
          "original_form": "밀크 버블티를 특히 좋아하는데",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045862.1.7",
          "form": "버블티도 좋아하세요?",
          "original_form": "버블티도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045862.1.8",
          "form": "비싸서 자주 먹지는 않지만 학교 앞에서 종종 사먹곤 했어요",
          "original_form": "비싸서 자주 먹지는 않지만 학교 앞에서 종종 사먹곤 했어요",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045862.1.9",
          "form": "앗 그럼 마실것도 직접 만들어 드시는 편인가요",
          "original_form": "앗 그럼 마실것도 직접 만들어 드시는 편인가요",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045862.1.10",
          "form": "생과일 주스는 사 먹어요. 쥬씨에서",
          "original_form": "생과일 주스는 사 먹어요. 쥬씨에서",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045862.1.11",
          "form": "최애 주스가 있나요?",
          "original_form": "최애 주스가 있나요?",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045862.1.12",
          "form": "제가 맞춰볼까요!",
          "original_form": "제가 맞춰볼까요!",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045862.1.13",
          "form": "바나나 들어가는 음료인가요?",
          "original_form": "바나나 들어가는 음료인가요?",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045862.1.14",
          "form": "아 그거 맛있죠 저는 봄과일 좋아해요",
          "original_form": "아 그거 맛있죠 저는 봄과일 좋아해요",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045862.1.15",
          "form": "앗 봄과일이라면",
          "original_form": "앗 봄과일이라면",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045862.1.16",
          "form": "빨간색인가요?",
          "original_form": "빨간색인가요?",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045862.1.17",
          "form": "네! 갈면 핑크빛이죠",
          "original_form": "네! 갈면 핑크빛이죠",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045862.1.18",
          "form": "온리 빨간과일만 들어가나요?",
          "original_form": "온리 빨간과일만 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045862.1.19",
          "form": "과일이랑 새콤한 요거트도 들어가요",
          "original_form": "과일이랑 새콤한 요거트도 들어가요",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045862.1.20",
          "form": "헉 정답! 딸기요거트!!!",
          "original_form": "헉 정답! 딸기요거트!!!",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045862.1.21",
          "form": "우와 잘 맞추시네요! 딸기 라떼도 맛있어요 3천원 짜리 혜자음료!",
          "original_form": "우와 잘 맞추시네요! 딸기 라떼도 맛있어요 3천원 짜리 혜자음료!",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100045862.1.22",
          "form": "헉 저렴하네요 쥬씨 거의 가본 적이 없어서 몰랐는데",
          "original_form": "헉 저렴하네요 쥬씨 거의 가본 적이 없어서 몰랐는데",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100045862.1.23",
          "form": "혜자스러운 가격이군요!",
          "original_form": "혜자스러운 가격이군요!",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100045862.1.24",
          "form": "빽다방도 저렴해요 전 가본적이 없지만 맛있고 가성비 좋다고 하더라고요.",
          "original_form": "빽다방도 저렴해요 전 가본적이 없지만 맛있고 가성비 좋다고 하더라고요.",
          "speaker_id": "2",
          "time": "20210908 09:42"
        }
      ]
    }
