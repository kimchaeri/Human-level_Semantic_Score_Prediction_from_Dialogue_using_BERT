      "utterance": [
        {
          "id": "MDRW2100047156.1.1",
          "form": "결국 내 지갑에서 나오는 돈으로 월급 받는거...",
          "original_form": "결국 내 지갑에서 나오는 돈으로 월급 받는거...",
          "speaker_id": "2",
          "time": "20210901 15:06"
        },
        {
          "id": "MDRW2100047156.1.2",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047156.1.3",
          "form": "그거에질려서",
          "original_form": "그거에질려서",
          "speaker_id": "1",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047156.1.4",
          "form": "안보기시작했는데",
          "original_form": "안보기시작했는데",
          "speaker_id": "1",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047156.1.5",
          "form": "여전하군용",
          "original_form": "여전하군용",
          "speaker_id": "1",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100047156.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ시장이 커져도 관계자들은 변하지 않아요....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ시장이 커져도 관계자들은 변하지 않아요....",
          "speaker_id": "2",
          "time": "20210901 15:11"
        },
        {
          "id": "MDRW2100047156.1.7",
          "form": "ㅋㄱㅋㅋㅋㄱㅋㄱ라만차할때 배우들",
          "original_form": "ㅋㄱㅋㅋㅋㄱㅋㄱ라만차할때 배우들",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100047156.1.8",
          "form": "울컥하면서 관객의소중함을 얘기하는데",
          "original_form": "울컥하면서 관객의소중함을 얘기하는데",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100047156.1.9",
          "form": "그거보면서",
          "original_form": "그거보면서",
          "speaker_id": "1",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100047156.1.10",
          "form": "분위기가 많이 달라지려나 했는데",
          "original_form": "분위기가 많이 달라지려나 했는데",
          "speaker_id": "1",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100047156.1.11",
          "form": "그것도아닌가...",
          "original_form": "그것도아닌가...",
          "speaker_id": "1",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100047156.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ모든 사람들의 마음이 같은건 아니니깐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ모든 사람들의 마음이 같은건 아니니깐",
          "speaker_id": "2",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100047156.1.13",
          "form": "원래 소수가 물 흐리기도하고",
          "original_form": "원래 소수가 물 흐리기도하고",
          "speaker_id": "2",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100047156.1.14",
          "form": "하긴",
          "original_form": "하긴",
          "speaker_id": "1",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100047156.1.15",
          "form": "그런 애들이 꼭 튀는 행동함",
          "original_form": "그런 애들이 꼭 튀는 행동함",
          "speaker_id": "2",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100047156.1.16",
          "form": "근데저는",
          "original_form": "근데저는",
          "speaker_id": "1",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100047156.1.17",
          "form": "당시에 갤밖에없었는데",
          "original_form": "당시에 갤밖에없었는데",
          "speaker_id": "1",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100047156.1.18",
          "form": "갤은너무싫었고",
          "original_form": "갤은너무싫었고",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100047156.1.19",
          "form": "관객도싫고 이 판도 지겨웠어서 탈덕했는데",
          "original_form": "관객도싫고 이 판도 지겨웠어서 탈덕했는데",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100047156.1.20",
          "form": "몇년뒤에 위키드 내한온거보고",
          "original_form": "몇년뒤에 위키드 내한온거보고",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100047156.1.21",
          "form": "약란 센치해졋었엌ㅋㄱㄱㅋ",
          "original_form": "약란 센치해졋었엌ㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100047156.1.22",
          "form": "내가좋아하던 뮤지컬은 이런건데",
          "original_form": "내가좋아하던 뮤지컬은 이런건데",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100047156.1.23",
          "form": "요샌 딱 라이트하게만 좋아하니",
          "original_form": "요샌 딱 라이트하게만 좋아하니",
          "speaker_id": "1",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100047156.1.24",
          "form": "맘이편함",
          "original_form": "맘이편함",
          "speaker_id": "1",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100047156.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서 지금은 취향극만 보는 반머글반덕후가 되신거군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서 지금은 취향극만 보는 반머글반덕후가 되신거군요",
          "speaker_id": "2",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100047156.1.26",
          "form": "근데 그게 행복해",
          "original_form": "근데 그게 행복해",
          "speaker_id": "2",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100047156.1.27",
          "form": "넹ㅋㅋㅋㅋㄱ 딱좋아",
          "original_form": "넹ㅋㅋㅋㅋㄱ 딱좋아",
          "speaker_id": "1",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100047156.1.28",
          "form": "남이랑 교류도안하고",
          "original_form": "남이랑 교류도안하고",
          "speaker_id": "1",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100047156.1.29",
          "form": "그냥내가본거고곱씹고",
          "original_form": "그냥내가본거고곱씹고",
          "speaker_id": "1",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100047156.1.30",
          "form": "가끔 뻐렁치는 마음을 나눌곳이 없어 심심하긴 한데",
          "original_form": "가끔 뻐렁치는 마음을 나눌곳이 없어 심심하긴 한데",
          "speaker_id": "1",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100047156.1.31",
          "form": "만족함ㅋㅋㄱㄱㅋㅋㅋ",
          "original_form": "만족함ㅋㅋㄱㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100047156.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뻐렁치는 마음 나누겠다고 덕친 만들었는데 취향 안맞으면 피곤해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뻐렁치는 마음 나누겠다고 덕친 만들었는데 취향 안맞으면 피곤해",
          "speaker_id": "2",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100047156.1.33",
          "form": "지금이 딱이여",
          "original_form": "지금이 딱이여",
          "speaker_id": "2",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100047156.1.34",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100047156.1.35",
          "form": "혼자 곱씹는게 최고야",
          "original_form": "혼자 곱씹는게 최고야",
          "speaker_id": "2",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100047156.1.36",
          "form": "내 취향 내가 존중한다",
          "original_form": "내 취향 내가 존중한다",
          "speaker_id": "2",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100047156.1.37",
          "form": "전원래 덕친을 안만들엌ㄱㅋㄱㄱㅋㄱㄱㅋㅋ",
          "original_form": "전원래 덕친을 안만들엌ㄱㅋㄱㄱㅋㄱㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:20"
        }
      ]
