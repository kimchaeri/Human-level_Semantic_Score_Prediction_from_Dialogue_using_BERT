      "utterance": [
        {
          "id": "MDRW2100004572.1.1",
          "form": "오늘 날씨가 완전 가을날씨죠?",
          "original_form": "오늘 날씨가 완전 가을날씨죠?",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004572.1.2",
          "form": "넵넵 맞아요",
          "original_form": "넵넵 맞아요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004572.1.3",
          "form": "이제 여름이 지나간 거 같아요",
          "original_form": "이제 여름이 지나간 거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004572.1.4",
          "form": "이렇게 여름이 지나가봐요!",
          "original_form": "이렇게 여름이 지나가봐요!",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004572.1.5",
          "form": "입추 지나고도 살짝 더웠는데 역시 처서 다가오니까ㅎㅎ",
          "original_form": "입추 지나고도 살짝 더웠는데 역시 처서 다가오니까ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004572.1.6",
          "form": "맞아요 이제 가을 장마가 시작됐다고 하더라규요",
          "original_form": "맞아요 이제 가을 장마가 시작됐다고 하더라규요",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004572.1.7",
          "form": "그래도 땀 뻘뻘 흘릴 일이 없어져서 좀 좋아요 ㅎㅎㅎ",
          "original_form": "그래도 땀 뻘뻘 흘릴 일이 없어져서 좀 좋아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004572.1.8",
          "form": "맞아요 진짜 올해 좀 더웠죠?",
          "original_form": "맞아요 진짜 올해 좀 더웠죠?",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004572.1.9",
          "form": "좀 시원해졌다고 다시 모기도 엄청 보이던데;;ㅣㅣ",
          "original_form": "좀 시원해졌다고 다시 모기도 엄청 보이던데;;ㅣㅣ",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004572.1.10",
          "form": "아 모기,,,,,;",
          "original_form": "아 모기,,,,,;",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004572.1.11",
          "form": "얼마전부터 생기더라구요",
          "original_form": "얼마전부터 생기더라구요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004572.1.12",
          "form": "날씨가 살짝 쌀쌀해지니 귀신같이 모기가 나오네요",
          "original_form": "날씨가 살짝 쌀쌀해지니 귀신같이 모기가 나오네요",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004572.1.13",
          "form": "그니까요 저 오늘 다섯마리나 잡았다는..",
          "original_form": "그니까요 저 오늘 다섯마리나 잡았다는..",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004572.1.14",
          "form": "여름모기는 옛말인가봐요",
          "original_form": "여름모기는 옛말인가봐요",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004572.1.15",
          "form": "헐 5마리요??????",
          "original_form": "헐 5마리요??????",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004572.1.16",
          "form": "모기는 진짜 없어져야돼요",
          "original_form": "모기는 진짜 없어져야돼요",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004572.1.17",
          "form": "날씨가 좀 선선해져서 좋았는데 모기라니요",
          "original_form": "날씨가 좀 선선해져서 좋았는데 모기라니요",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004572.1.18",
          "form": "그니까요 여름은 더운것도 있는데 벌레 많아서 싫고",
          "original_form": "그니까요 여름은 더운것도 있는데 벌레 많아서 싫고",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004572.1.19",
          "form": "가을에도 모기 걱정해야하는게ㅋㅋ",
          "original_form": "가을에도 모기 걱정해야하는게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004572.1.20",
          "form": "작년엔 10월까지 모기 봤거든요ㅠ",
          "original_form": "작년엔 10월까지 모기 봤거든요ㅠ",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004572.1.21",
          "form": "헉아직도 모기만날수 있는 시간이 2달 남았네요",
          "original_form": "헉아직도 모기만날수 있는 시간이 2달 남았네요",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004572.1.22",
          "form": "밤에 잘 때 모기 소리 나면 너뮤 끔찍해요",
          "original_form": "밤에 잘 때 모기 소리 나면 너뮤 끔찍해요",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004572.1.23",
          "form": "으 그래서 전 에프킬라 없으면 못살아요ㅎㅎ",
          "original_form": "으 그래서 전 에프킬라 없으면 못살아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004572.1.24",
          "form": "근데 가을이 또 금방 가버려서 요샌",
          "original_form": "근데 가을이 또 금방 가버려서 요샌",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004572.1.25",
          "form": "겨울 걱정도 동시에^^;;",
          "original_form": "겨울 걱정도 동시에^^;;",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004572.1.26",
          "form": "맞아요 오 가을이다 하면 겨울이에요",
          "original_form": "맞아요 오 가을이다 하면 겨울이에요",
          "speaker_id": "2",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004572.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ겨울되면 또 너무 추워서 귀 잘릴 거 같구",
          "original_form": "ㅋㅋㅋㅋㅋㅋ겨울되면 또 너무 추워서 귀 잘릴 거 같구",
          "speaker_id": "2",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004572.1.28",
          "form": "겨울은 아침에 일어나는 게 넘 힘들어요",
          "original_form": "겨울은 아침에 일어나는 게 넘 힘들어요",
          "speaker_id": "2",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004572.1.29",
          "form": "맞아요 롱패딩 모자 귀에 푹 눌러쓰고",
          "original_form": "맞아요 롱패딩 모자 귀에 푹 눌러쓰고",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004572.1.30",
          "form": "해가 늦게 떠서 뭔가 더 늦잠자게 되는 거 같아요",
