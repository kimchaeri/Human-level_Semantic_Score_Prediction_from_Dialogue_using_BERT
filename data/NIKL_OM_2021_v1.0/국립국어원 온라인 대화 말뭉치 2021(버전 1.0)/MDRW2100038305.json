      "utterance": [
        {
          "id": "MDRW2100038305.1.1",
          "form": "다이어트쉐이크 종류 다양하게있는데 다 안먹고 썩히고있음ㅜ",
          "original_form": "다이어트쉐이크 종류 다양하게있는데 다 안먹고 썩히고있음ㅜ",
          "speaker_id": "1",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100038305.1.2",
          "form": "ㅜㅜ그걸 먹어야하는데",
          "original_form": "ㅜㅜ그걸 먹어야하는데",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100038305.1.3",
          "form": "근데 맛들이 다 희한해",
          "original_form": "근데 맛들이 다 희한해",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038305.1.4",
          "form": "너무 새로운 시도들을 했어ㅜ",
          "original_form": "너무 새로운 시도들을 했어ㅜ",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038305.1.5",
          "form": "절찬 후회중",
          "original_form": "절찬 후회중",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038305.1.6",
          "form": "ㅋㅋㅋ 도전은 가상하나",
          "original_form": "ㅋㅋㅋ 도전은 가상하나",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100038305.1.7",
          "form": "결과가좀 ..ㅜㅜ",
          "original_form": "결과가좀 ..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100038305.1.8",
          "form": "민초 좋아해서 민초맛 샀는데",
          "original_form": "민초 좋아해서 민초맛 샀는데",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038305.1.9",
          "form": "사실 나는 민초 좋아한게 아니었던듯",
          "original_form": "사실 나는 민초 좋아한게 아니었던듯",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038305.1.10",
          "form": "민초가 아니라 민트초코크림을 좋아했던듭",
          "original_form": "민초가 아니라 민트초코크림을 좋아했던듭",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038305.1.11",
          "form": "깨달음을 축하합니다",
          "original_form": "깨달음을 축하합니다",
          "speaker_id": "2",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100038305.1.12",
          "form": "살찌는것만 골라좋아하는구만유",
          "original_form": "살찌는것만 골라좋아하는구만유",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038305.1.13",
          "form": "그리고 노니맛이있음",
          "original_form": "그리고 노니맛이있음",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038305.1.14",
          "form": "이건 뭐 네 다이어트쉐이크가 노니맛이에요",
          "original_form": "이건 뭐 네 다이어트쉐이크가 노니맛이에요",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038305.1.15",
          "form": "뭐라 더 할말이없네요",
          "original_form": "뭐라 더 할말이없네요",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
