      "utterance": [
        {
          "id": "MDRW2100039596.1.1",
          "form": "언니 요즘 몸은 좀 어때요?",
          "original_form": "언니 요즘 몸은 좀 어때요?",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039596.1.2",
          "form": "병원 다녀왔어요?",
          "original_form": "병원 다녀왔어요?",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039596.1.3",
          "form": "음 일단 내일 검사 받아봐야 알 것 같아ㅠㅠ",
          "original_form": "음 일단 내일 검사 받아봐야 알 것 같아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039596.1.4",
          "form": "어제 동네 병원은 갔다왔어",
          "original_form": "어제 동네 병원은 갔다왔어",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039596.1.5",
          "form": "에고ㅠㅠ 고생많으세요",
          "original_form": "에고ㅠㅠ 고생많으세요",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039596.1.6",
          "form": "진짜 요즘 다들 아픈거 같아요",
          "original_form": "진짜 요즘 다들 아픈거 같아요",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039596.1.7",
          "form": "ㅠㅠ뜻하지 않게 갑자기.....",
          "original_form": "ㅠㅠ뜻하지 않게 갑자기.....",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.8",
          "form": "저도 저번달에 약부작용 와가지고 고생했거든요ㅠㅜ",
          "original_form": "저도 저번달에 약부작용 와가지고 고생했거든요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.9",
          "form": "맞아 다들 요즘 주변 이야기 들으면 ㅠㅠ",
          "original_form": "맞아 다들 요즘 주변 이야기 들으면 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.10",
          "form": "그쵸ㅠㅠ 백신도 되게 복불복이에요",
          "original_form": "그쵸ㅠㅠ 백신도 되게 복불복이에요",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.11",
          "form": "헉 너도??ㅠㅠ",
          "original_form": "헉 너도??ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.12",
          "form": "백신 때문에??",
          "original_form": "백신 때문에??",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.13",
          "form": "네네! 저는 근데 백신말구 ppi부작용이었어용",
          "original_form": "네네! 저는 근데 백신말구 ppi부작용이었어용",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.14",
          "form": "아니면 다른 약??",
          "original_form": "아니면 다른 약??",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.15",
          "form": "앗 ppi가 뭐야??",
          "original_form": "앗 ppi가 뭐야??",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.16",
          "form": "다른약이요! 그거 위산약이에요",
          "original_form": "다른약이요! 그거 위산약이에요",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039596.1.17",
          "form": "처음 들어봐!",
          "original_form": "처음 들어봐!",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039596.1.18",
          "form": "앗 위산약 챙겨먹는구나ㅠㅠㅠ",
          "original_form": "앗 위산약 챙겨먹는구나ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039596.1.19",
          "form": "신경계 조절하는 약이라는데",
          "original_form": "신경계 조절하는 약이라는데",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039596.1.20",
          "form": "부작용 후기 보니까 저만 그러지는 않더라구요",
          "original_form": "부작용 후기 보니까 저만 그러지는 않더라구요",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039596.1.21",
          "form": "오... 꽤나 부작용이 있나보다ㅠㅠ",
          "original_form": "오... 꽤나 부작용이 있나보다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039596.1.22",
          "form": "네네ㅜㅜ 이젠 안먹어요 부작용 확 와서 끊었어요",
          "original_form": "네네ㅜㅜ 이젠 안먹어요 부작용 확 와서 끊었어요",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039596.1.23",
          "form": "다른 약으로 대체 가능하대??",
          "original_form": "다른 약으로 대체 가능하대??",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039596.1.24",
          "form": "네내ㅠㅠ 진짜 조심해야하구",
          "original_form": "네내ㅠㅠ 진짜 조심해야하구",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039596.1.25",
          "form": "앗 지금은 안 먹구나ㅠㅠㅠ",
          "original_form": "앗 지금은 안 먹구나ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039596.1.26",
          "form": "네 그냥 식이조절하고 제가 그냥 몸관리하구 그래용",
          "original_form": "네 그냥 식이조절하고 제가 그냥 몸관리하구 그래용",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039596.1.27",
          "form": "아구 식이조절이 어려운데ㅠㅠㅠ",
          "original_form": "아구 식이조절이 어려운데ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039596.1.28",
          "form": "그거 안먹으면 신물 올라오는데 차라리 올라오는게 나을거 같아서ㅋㅋㅋ",
          "original_form": "그거 안먹으면 신물 올라오는데 차라리 올라오는게 나을거 같아서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039596.1.29",
          "form": "앗 그정도야....??ㅠㅠㅠㅠㅠ",
          "original_form": "앗 그정도야....??ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039596.1.30",
          "form": "지금은 많이 나아져써요",
          "original_form": "지금은 많이 나아져써요",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039596.1.31",
          "form": "그럼 다른 약으로 대체해보는 게 낫지 않아??ㅠㅠ",
          "original_form": "그럼 다른 약으로 대체해보는 게 낫지 않아??ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039596.1.32",
          "form": "출판사랑 일하면서 스트레스 너무 많이 받았어서ㅠㅠ 스트레스성이었던거 같아요",
          "original_form": "출판사랑 일하면서 스트레스 너무 많이 받았어서ㅠㅠ 스트레스성이었던거 같아요",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039596.1.33",
          "form": "그래도 많이 괜찮아졌다니 다행이다 ㅠㅠ",
          "original_form": "그래도 많이 괜찮아졌다니 다행이다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039596.1.34",
          "form": "네네ㅠㅠ 일 쉬고 있으니 좀 나아용",
          "original_form": "네네ㅠㅠ 일 쉬고 있으니 좀 나아용",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039596.1.35",
          "form": "으아 스트레스성....ㅠㅠ 만병의 근원 같으니라고....",
          "original_form": "으아 스트레스성....ㅠㅠ 만병의 근원 같으니라고....",
          "speaker_id": "2",
          "time": "20210903 19:27"
        }
      ]
