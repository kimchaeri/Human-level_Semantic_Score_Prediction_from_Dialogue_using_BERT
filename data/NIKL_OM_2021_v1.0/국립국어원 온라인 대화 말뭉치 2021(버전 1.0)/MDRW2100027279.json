      "utterance": [
        {
          "id": "MDRW2100027279.1.1",
          "form": "아 회장님 가격안보시자나여 ㅋㅋ",
          "original_form": "아 회장님 가격안보시자나여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027279.1.2",
          "form": "무슨물회 드실거에요?",
          "original_form": "무슨물회 드실거에요?",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027279.1.3",
          "form": "저는 물회말고 그냥 회ㅎ",
          "original_form": "저는 물회말고 그냥 회ㅎ",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027279.1.4",
          "form": "철 지났지만 사랑하는 도다리와 전어 사이서 고민중이었는데",
          "original_form": "철 지났지만 사랑하는 도다리와 전어 사이서 고민중이었는데",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027279.1.5",
          "form": "지금 치과서 스케일링 당하니 입에서 피맛나 고민되네요ㅜ",
          "original_form": "지금 치과서 스케일링 당하니 입에서 피맛나 고민되네요ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027279.1.6",
          "form": "대뜸 스케일링 ㅂㄷㅂㄷ",
          "original_form": "대뜸 스케일링 ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027279.1.7",
          "form": "임플란트 잘 안착됐나 보러 오라 그러고선",
          "original_form": "임플란트 잘 안착됐나 보러 오라 그러고선",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027279.1.8",
          "form": "글구 제가 왜 회장이에욧1",
          "original_form": "글구 제가 왜 회장이에욧1",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027279.1.9",
          "form": "노비 그 자체인데ㅜㅇㅜ",
          "original_form": "노비 그 자체인데ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027279.1.10",
          "form": "아 늦게봤습니다 ㅠ 지성",
          "original_form": "아 늦게봤습니다 ㅠ 지성",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027279.1.11",
          "form": "스케일링하면 시원하이 좋더라구요",
          "original_form": "스케일링하면 시원하이 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027279.1.12",
          "form": "전어로 하시죠",
          "original_form": "전어로 하시죠",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027279.1.13",
          "form": "다금바리 먹는 노비는 없어욧",
          "original_form": "다금바리 먹는 노비는 없어욧",
          "speaker_id": "2",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100027279.1.14",
          "form": "임플란트 하신거는 물리적요인이었나요?",
          "original_form": "임플란트 하신거는 물리적요인이었나요?",
          "speaker_id": "2",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100027279.1.15",
          "form": "아뇨ㅜ 어렸을적 한 금니가 빠져서ㅜ",
          "original_form": "아뇨ㅜ 어렸을적 한 금니가 빠져서ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027279.1.16",
          "form": "이래서 어릴때 양치습관을 잘 들여야 합니다!",
          "original_form": "이래서 어릴때 양치습관을 잘 들여야 합니다!",
          "speaker_id": "1",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027279.1.17",
          "form": "아 맞습니다 ㅠ",
          "original_form": "아 맞습니다 ㅠ",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027279.1.18",
          "form": "아들 양치 습관 어찌할까 싶습니다",
          "original_form": "아들 양치 습관 어찌할까 싶습니다",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027279.1.19",
          "form": "과자먹고",
          "original_form": "과자먹고",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027279.1.20",
          "form": "분유먹고",
          "original_form": "분유먹고",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027279.1.21",
          "form": "양치 시킨다고 하는데",
          "original_form": "양치 시킨다고 하는데",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027279.1.22",
          "form": "안하려고해요 ㅠ",
          "original_form": "안하려고해요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027279.1.23",
          "form": "임플란트 하면 딱딱한 음식은 못먹죠?",
          "original_form": "임플란트 하면 딱딱한 음식은 못먹죠?",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027279.1.24",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027279.1.25",
          "form": "전.. 다 먹습니다",
          "original_form": "전.. 다 먹습니다",
          "speaker_id": "1",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027279.1.26",
          "form": "근데 앞니면 조심스러울 것 같아요",
          "original_form": "근데 앞니면 조심스러울 것 같아요",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100027279.1.27",
          "form": "사실 별반 문제를 못 느껴서요",
          "original_form": "사실 별반 문제를 못 느껴서요",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100027279.1.28",
          "form": "저는 회사의 노예가 아니라 위의 노예인듯ㅎ",
          "original_form": "저는 회사의 노예가 아니라 위의 노예인듯ㅎ",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027279.1.29",
          "form": "홈플러스서 닭발 사고 배달앱 고르면서 집에 오다 충동적으로 돈까스집 들어왔어요ㅎ",
          "original_form": "홈플러스서 닭발 사고 배달앱 고르면서 집에 오다 충동적으로 돈까스집 들어왔어요ㅎ",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027279.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027279.1.31",
          "form": "아 미팅이 많아서",
          "original_form": "아 미팅이 많아서",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027279.1.32",
          "form": "이제 봤습니다",
          "original_form": "이제 봤습니다",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027279.1.33",
          "form": "와 오늘 다이내믹하셨네요 ㅋㅋㅋ",
          "original_form": "와 오늘 다이내믹하셨네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027279.1.34",
          "form": "진정 먹신이시네요 ",
          "original_form": "진정 먹신이시네요 ",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027279.1.35",
          "form": "돈까스 어떻든가요?",
          "original_form": "돈까스 어떻든가요?",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027279.1.36",
          "form": "ㅎㅈㅎ 원래 배달 자주 시키던 곳이라 말모",
          "original_form": "ㅎㅈㅎ 원래 배달 자주 시키던 곳이라 말모",
          "speaker_id": "1",
          "time": "20210902 17:01"
        },
        {
          "id": "MDRW2100027279.1.37",
          "form": "수제소스가 끝내줘요ㅎㅎ",
          "original_form": "수제소스가 끝내줘요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:01"
        },
        {
          "id": "MDRW2100027279.1.38",
          "form": "물회는 성공적이었나욧!",
          "original_form": "물회는 성공적이었나욧!",
          "speaker_id": "1",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100027279.1.39",
          "form": "아 물회아니고",
          "original_form": "아 물회아니고",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100027279.1.40",
          "form": "라이진 이라는 편백집이었습니다",
          "original_form": "라이진 이라는 편백집이었습니다",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100027279.1.41",
          "form": "괜찮았어요 ㅎㅎ 맛있었습니다",
          "original_form": "괜찮았어요 ㅎㅎ 맛있었습니다",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100027279.1.42",
          "form": "바닷가 보면서 먹으니 더 좋앗네요",
          "original_form": "바닷가 보면서 먹으니 더 좋앗네요",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100027279.1.43",
          "form": "몸은 좀 어떠세요?",
          "original_form": "몸은 좀 어떠세요?",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100027279.1.44",
          "form": "헉 편백집! 뭘로 드셨어요?",
          "original_form": "헉 편백집! 뭘로 드셨어요?",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100027279.1.45",
          "form": "차돌이엇습니다",
          "original_form": "차돌이엇습니다",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100027279.1.46",
          "form": "차돌에 숙주?",
          "original_form": "차돌에 숙주?",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100027279.1.47",
          "form": "회사 사람들과 편백집이라니 좋은 회사",
          "original_form": "회사 사람들과 편백집이라니 좋은 회사",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100027279.1.48",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100027279.1.49",
          "form": "몸은요?",
          "original_form": "몸은요?",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100027279.1.50",
          "form": "괜찮나요?",
          "original_form": "괜찮나요?",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100027279.1.51",
          "form": "이상반응있으면 바로 병원가셔야됩니다",
          "original_form": "이상반응있으면 바로 병원가셔야됩니다",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027279.1.52",
          "form": "아직 모르겠어요",
          "original_form": "아직 모르겠어요",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027279.1.53",
          "form": "아!잠이 쏟아져서 집 오자마자 자다 지금 일어남",
          "original_form": "아!잠이 쏟아져서 집 오자마자 자다 지금 일어남",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027279.1.54",
          "form": "오 굿",
          "original_form": "오 굿",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027279.1.55",
          "form": "백신에 수면제성분이 있나 의심했어요ㅎㅎ",
          "original_form": "백신에 수면제성분이 있나 의심했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:12"
        }
      ]
