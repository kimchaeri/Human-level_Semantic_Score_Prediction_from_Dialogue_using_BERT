      "utterance": [
        {
          "id": "MDRW2100015235.1.1",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015235.1.2",
          "form": "하고있는 일 있우시나요?",
          "original_form": "하고있는 일 있우시나요?",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015235.1.3",
          "form": "알바하고있습니다",
          "original_form": "알바하고있습니다",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015235.1.4",
          "form": "아하 그러시구나ㅎㅎ",
          "original_form": "아하 그러시구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015235.1.5",
          "form": "어떤 알바하시나요?",
          "original_form": "어떤 알바하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015235.1.6",
          "form": "카페알바중입니다",
          "original_form": "카페알바중입니다",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015235.1.7",
          "form": "주말 오전에만 해요ㅎㅎ",
          "original_form": "주말 오전에만 해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015235.1.8",
          "form": "아하 카페알바하시는구나!",
          "original_form": "아하 카페알바하시는구나!",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015235.1.9",
          "form": "카페 알바는 하실만 하세요? 외울것도 많을거같은데",
          "original_form": "카페 알바는 하실만 하세요? 외울것도 많을거같은데",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015235.1.10",
          "form": "이것저것 많긴 한데",
          "original_form": "이것저것 많긴 한데",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015235.1.11",
          "form": "결국 뭐 비슷비슷해서 쉬워요",
          "original_form": "결국 뭐 비슷비슷해서 쉬워요",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015235.1.12",
          "form": "팥빙수가 복병이지...",
          "original_form": "팥빙수가 복병이지...",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015235.1.13",
          "form": "팥빙수 많이 어렵나요?ㅋㅋㅋㅋ",
          "original_form": "팥빙수 많이 어렵나요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015235.1.14",
          "form": "엄청 귀찮아요ㅋㅋ",
          "original_form": "엄청 귀찮아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015235.1.15",
          "form": "손님들이 뒷정리도 잘 안하구",
          "original_form": "손님들이 뒷정리도 잘 안하구",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015235.1.16",
          "form": "아 그러시겠네요ㅠㅠ",
          "original_form": "아 그러시겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015235.1.17",
          "form": "만드는것만 생각했는데 정리도 힘드시겠다..!",
          "original_form": "만드는것만 생각했는데 정리도 힘드시겠다..!",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015235.1.18",
          "form": "정리가 진짜 귀찮아요ㅋㅋㅋ",
          "original_form": "정리가 진짜 귀찮아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015235.1.19",
          "form": "영수증 북북 찢어서 그릇에 담는분도 계시구ㅋㄱㅋ",
          "original_form": "영수증 북북 찢어서 그릇에 담는분도 계시구ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015235.1.20",
          "form": "ㅋㅋㅋㅋ그럴거같아요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ그럴거같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015235.1.21",
          "form": "어이구 진짜 너무하다...!!",
          "original_form": "어이구 진짜 너무하다...!!",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015235.1.22",
          "form": "앞으로는 카페가면 정리좀 하고 나가야겠네요ㅠ",
          "original_form": "앞으로는 카페가면 정리좀 하고 나가야겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015235.1.23",
          "form": "아니요. 저희 가게만 아니면 지저분해도 됩니다",
          "original_form": "아니요. 저희 가게만 아니면 지저분해도 됩니다",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015235.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015235.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ그래야게ㅛ네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그래야게ㅛ네요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        }
      ]
