      "utterance": [
        {
          "id": "MDRW2100039699.1.1",
          "form": "요리똥손이시라믄서요~!",
          "original_form": "요리똥손이시라믄서요~!",
          "speaker_id": "2",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100039699.1.2",
          "form": "아니! 요알못이라는 말도 있고",
          "original_form": "아니! 요알못이라는 말도 있고",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039699.1.3",
          "form": "곰손이란 말도 있는데",
          "original_form": "곰손이란 말도 있는데",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039699.1.4",
          "form": "그렇게 원색적인 모욕을 주시다니ㅂㄷㅂㄷ",
          "original_form": "그렇게 원색적인 모욕을 주시다니ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039699.1.5",
          "form": "어머 그런 말이 있었나요?",
          "original_form": "어머 그런 말이 있었나요?",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039699.1.6",
          "form": "하지만 어차피 그게 그거라구요옹!!!",
          "original_form": "하지만 어차피 그게 그거라구요옹!!!",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039699.1.7",
          "form": "고기 한점이라도 주고 말하세욧!",
          "original_form": "고기 한점이라도 주고 말하세욧!",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039699.1.8",
          "form": "에프에 고기 돌리고 있나요",
          "original_form": "에프에 고기 돌리고 있나요",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039699.1.9",
          "form": "아니요 지금은",
          "original_form": "아니요 지금은",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039699.1.10",
          "form": "귀찮아서",
          "original_form": "귀찮아서",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039699.1.11",
          "form": "그냥 티비보고있어요 .     ....",
          "original_form": "그냥 티비보고있어요 .     ....",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039699.1.12",
          "form": "ㅜㅇㅜ 원래 입이 짧고 식탐이 없으신가요",
          "original_form": "ㅜㅇㅜ 원래 입이 짧고 식탐이 없으신가요",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039699.1.13",
          "form": "제가..",
          "original_form": "제가..",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039699.1.14",
          "form": "식탐이 없어보였나요....?",
          "original_form": "식탐이 없어보였나요....?",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039699.1.15",
          "form": "그렇게 많은 음식을 맛있어했는데.....",
          "original_form": "그렇게 많은 음식을 맛있어했는데.....",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039699.1.16",
          "form": "하지만...타르트로 끼니를 떼우고",
          "original_form": "하지만...타르트로 끼니를 떼우고",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039699.1.17",
          "form": "지금까지 안 먹다니",
          "original_form": "지금까지 안 먹다니",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039699.1.18",
          "form": "헉 아침에",
          "original_form": "헉 아침에",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039699.1.19",
          "form": "라면도 먹었었어요",
          "original_form": "라면도 먹었었어요",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039699.1.20",
          "form": "있을 수 없는 일이에요!",
          "original_form": "있을 수 없는 일이에요!",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039699.1.21",
          "form": "으하하하하하하하하하",
          "original_form": "으하하하하하하하하하",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039699.1.22",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039699.1.23",
          "form": "그건 아침이잖아요!",
          "original_form": "그건 아침이잖아요!",
          "speaker_id": "1",
