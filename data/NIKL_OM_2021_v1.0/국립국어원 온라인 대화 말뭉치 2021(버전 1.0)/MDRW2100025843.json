      "utterance": [
        {
          "id": "MDRW2100025843.1.1",
          "form": "여자친구가 서브웨이 시켜먹으라는데",
          "original_form": "여자친구가 서브웨이 시켜먹으라는데",
          "speaker_id": "2",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100025843.1.2",
          "form": "서브웨이 좋아하세요?",
          "original_form": "서브웨이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100025843.1.3",
          "form": "전엄청좋아해욥",
          "original_form": "전엄청좋아해욥",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025843.1.4",
          "form": "저녁메뉴 고민중이신가요",
          "original_form": "저녁메뉴 고민중이신가요",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025843.1.5",
          "form": "계란후라이에 김치 넣어 비벼먹을까",
          "original_form": "계란후라이에 김치 넣어 비벼먹을까",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025843.1.6",
          "form": "서브웨이 먹을까",
          "original_form": "서브웨이 먹을까",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025843.1.7",
          "form": "고민중이에요",
          "original_form": "고민중이에요",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025843.1.8",
          "form": "어제부터",
          "original_form": "어제부터",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025843.1.9",
          "form": "서브웨이 생각하고있긴했어요ㅋㅋㅋ",
          "original_form": "서브웨이 생각하고있긴했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025843.1.10",
          "form": "서브웨이 고고",
          "original_form": "서브웨이 고고",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025843.1.11",
          "form": "근데여자친구가 말해서 놀랐어요",
          "original_form": "근데여자친구가 말해서 놀랐어요",
          "speaker_id": "2",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025843.1.12",
          "form": "저는 씻고옵니다",
          "original_form": "저는 씻고옵니다",
          "speaker_id": "2",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025843.1.13",
