      "utterance": [
        {
          "id": "MDRW2100045713.1.1",
          "form": "따끈따끈한 타코야키너무 맛잇죠",
          "original_form": "따끈따끈한 타코야키너무 맛잇죠",
          "speaker_id": "1",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045713.1.2",
          "form": "먹고싶네요 지금...ㅋㅋㅋㅋ",
          "original_form": "먹고싶네요 지금...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045713.1.3",
          "form": "저도요... 타코야끼 팬 사서 집에서 구워먹을까도 5년째 고민중이예요",
          "original_form": "저도요... 타코야끼 팬 사서 집에서 구워먹을까도 5년째 고민중이예요",
          "speaker_id": "2",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045713.1.4",
          "form": "팬만 있으면 머하노 재료가 없는디",
          "original_form": "팬만 있으면 머하노 재료가 없는디",
          "speaker_id": "2",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045713.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아니 팬이 잇으면서 재료는 왜 없나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아니 팬이 잇으면서 재료는 왜 없나요",
          "speaker_id": "1",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045713.1.6",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋ아직 팬도 안 샀어요",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋ아직 팬도 안 샀어요",
          "speaker_id": "2",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045713.1.7",
          "form": "모든걸 자린고비처럼",
          "original_form": "모든걸 자린고비처럼",
          "speaker_id": "2",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045713.1.8",
          "form": "상상타코야끼 ㄷ구워먹는 중이예요",
          "original_form": "상상타코야끼 ㄷ구워먹는 중이예요",
          "speaker_id": "2",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045713.1.9",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045713.1.10",
          "form": "이미 사신줄 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이미 사신줄 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045713.1.11",
          "form": "ㅋㅋㅋㅋㅌㅋㅋ5년 쯤 되면 살 법도 한데",
          "original_form": "ㅋㅋㅋㅋㅌㅋㅋ5년 쯤 되면 살 법도 한데",
          "speaker_id": "2",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045713.1.12",
          "form": "막상 사려니 처박템 될 것 같은 기분이 들어서",
          "original_form": "막상 사려니 처박템 될 것 같은 기분이 들어서",
          "speaker_id": "2",
          "time": "20210908 05:55"
        },
        {
          "id": "MDRW2100045713.1.13",
          "form": "아직도 미루고 밖에서 사먹곤 해유ㅋㅋㅋㅋ",
          "original_form": "아직도 미루고 밖에서 사먹곤 해유ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:55"
        },
        {
          "id": "MDRW2100045713.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 사먹기만 합니다 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 사먹기만 합니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100045713.1.15",
          "form": "이번에는 맞춰보실래요?",
          "original_form": "이번에는 맞춰보실래요?",
          "speaker_id": "1",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100045713.1.16",
          "form": "제가 싫어하는 음식",
          "original_form": "제가 싫어하는 음식",
          "speaker_id": "1",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100045713.1.17",
          "form": "ㅋㅋㅋㅋㅋ좋아요 이번엔 리버스버전이네요",
          "original_form": "ㅋㅋㅋㅋㅋ좋아요 이번엔 리버스버전이네요",
          "speaker_id": "2",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045713.1.18",
          "form": "매운거예요?",
          "original_form": "매운거예요?",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045713.1.19",
          "form": "아니요 안매워요",
          "original_form": "아니요 안매워요",
          "speaker_id": "1",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045713.1.20",
          "form": "해산물인가요?",
          "original_form": "해산물인가요?",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045713.1.21",
          "form": "아니요 해산물 아니에요",
          "original_form": "아니요 해산물 아니에요",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045713.1.22",
          "form": "야채인가요?",
          "original_form": "야채인가요?",
          "speaker_id": "2",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045713.1.23",
          "form": "네 야채입니다",
          "original_form": "네 야채입니다",
          "speaker_id": "1",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045713.1.24",
          "form": "음.. 과일인가요?",
          "original_form": "음.. 과일인가요?",
          "speaker_id": "2",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045713.1.25",
          "form": "아니요 과일 아니에요",
