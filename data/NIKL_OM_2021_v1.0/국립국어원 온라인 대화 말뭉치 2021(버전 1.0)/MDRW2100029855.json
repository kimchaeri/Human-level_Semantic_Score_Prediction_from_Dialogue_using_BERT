      "utterance": [
        {
          "id": "MDRW2100029855.1.1",
          "form": "귀또리는 무슨 뜻인가요?",
          "original_form": "귀또리는 무슨 뜻인가요?",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100029855.1.2",
          "form": "귀뚜라미요",
          "original_form": "귀뚜라미요",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100029855.1.3",
          "form": "고딩때 국어시간에 배웟어요",
          "original_form": "고딩때 국어시간에 배웟어요",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100029855.1.4",
          "form": "아 귀뚜라미 ㅋㅋ",
          "original_form": "아 귀뚜라미 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100029855.1.5",
          "form": "요즘 귀뚜라미 많아요",
          "original_form": "요즘 귀뚜라미 많아요",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100029855.1.6",
          "form": "다리에 막 붙음 ㅋㅋ",
          "original_form": "다리에 막 붙음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100029855.1.7",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029855.1.8",
          "form": "다리에붙을정도인가요",
          "original_form": "다리에붙을정도인가요",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029855.1.9",
          "form": "왜케많져",
          "original_form": "왜케많져",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029855.1.10",
          "form": "며칠전에 23층에도 나타낫답니다",
          "original_form": "며칠전에 23층에도 나타낫답니다",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029855.1.11",
          "form": "ㅎㄷㄷ",
          "original_form": "ㅎㄷㄷ",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029855.1.12",
          "form": "ㅋㅋ전 귀뚜라미 못본지 오래에여",
          "original_form": "ㅋㅋ전 귀뚜라미 못본지 오래에여",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029855.1.13",
          "form": "한마리 잡아서 보내드릴까여?",
          "original_form": "한마리 잡아서 보내드릴까여?",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100029855.1.14",
          "form": "오노 싫습니다ㅋㅋ",
          "original_form": "오노 싫습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:03"
        },
        {
          "id": "MDRW2100029855.1.15",
          "form": "네 저도 사실 못잡아요 ㅋㅋㅋ",
          "original_form": "네 저도 사실 못잡아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029855.1.16",
          "form": "무서워요 ㅋㅋㅋ",
          "original_form": "무서워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029855.1.17",
          "form": "동물의 왕국 보는건 좋아하는데",
          "original_form": "동물의 왕국 보는건 좋아하는데",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029855.1.18",
          "form": "곤충 방송은 무서우워요",
          "original_form": "곤충 방송은 무서우워요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029855.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029855.1.20",
          "form": "무슨방송 좋아하서이홍?",
          "original_form": "무슨방송 좋아하서이홍?",
          "speaker_id": "2",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029855.1.21",
          "form": "저는 주로 영화봄다",
          "original_form": "저는 주로 영화봄다",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029855.1.22",
          "form": "저도 옛날에 홍콩영화 좋아했는데",
          "original_form": "저도 옛날에 홍콩영화 좋아했는데",
          "speaker_id": "2",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100029855.1.23",
          "form": "홍금보 성룡",
          "original_form": "홍금보 성룡",
          "speaker_id": "2",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100029855.1.24",
          "form": "따라 무술 연습도 하구요",
          "original_form": "따라 무술 연습도 하구요",
          "speaker_id": "2",
