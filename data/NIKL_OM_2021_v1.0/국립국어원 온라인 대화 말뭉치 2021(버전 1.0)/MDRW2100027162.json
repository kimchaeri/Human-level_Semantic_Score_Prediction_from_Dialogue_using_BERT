      "utterance": [
        {
          "id": "MDRW2100027162.1.1",
          "form": "안녕하세요 카우 보고 왔습니다!",
          "original_form": "안녕하세요 카우 보고 왔습니다!",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100027162.1.2",
          "form": "제일 싫어하는 집안일 있으세요?",
          "original_form": "제일 싫어하는 집안일 있으세요?",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100027162.1.3",
          "form": "전 화장실청소요",
          "original_form": "전 화장실청소요",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100027162.1.4",
          "form": "설거지도 별로 안좋아해요ㅋㅋㅋ",
          "original_form": "설거지도 별로 안좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100027162.1.5",
          "form": "저도 설거지 제일 안 좋아해요 ㅋㅋㅋ",
          "original_form": "저도 설거지 제일 안 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027162.1.6",
          "form": "제일 좋아하는 건요?",
          "original_form": "제일 좋아하는 건요?",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027162.1.7",
          "form": "음 전",
          "original_form": "음 전",
          "speaker_id": "2",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027162.1.8",
          "form": "빨래개는거요",
          "original_form": "빨래개는거요",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027162.1.9",
          "form": "뽀송뽀송한 옷감 만지는거라",
          "original_form": "뽀송뽀송한 옷감 만지는거라",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027162.1.10",
          "form": "기분이 좋더라구요",
          "original_form": "기분이 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027162.1.11",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027162.1.12",
          "form": "맞아요 냄새가 좋긴 해요",
          "original_form": "맞아요 냄새가 좋긴 해요",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027162.1.13",
          "form": "전 요리하는 걸 좋아해요!",
          "original_form": "전 요리하는 걸 좋아해요!",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027162.1.14",
          "form": "요리",
          "original_form": "요리",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027162.1.15",
          "form": "오 요리 재밌죠",
          "original_form": "오 요리 재밌죠",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027162.1.16",
          "form": "한끼 잘 차려먹으면 기분도 좋고 배도 부르고",
          "original_form": "한끼 잘 차려먹으면 기분도 좋고 배도 부르고",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027162.1.17",
          "form": "요리는 자취하면 느는것같아요",
          "original_form": "요리는 자취하면 느는것같아요",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027162.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027162.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027162.1.20",
          "form": "전에 교환학생 갔을 때 정말 많이 늘었어요 ㅋㅋㅋ",
          "original_form": "전에 교환학생 갔을 때 정말 많이 늘었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027162.1.21",
          "form": "이제 요리 천재가 되었습니다...",
          "original_form": "이제 요리 천재가 되었습니다...",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027162.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027162.1.23",
          "form": "진짜 요리가 어려운데",
          "original_form": "진짜 요리가 어려운데",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027162.1.24",
          "form": "살려면 느는것같아요",
          "original_form": "살려면 느는것같아요",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027162.1.25",
          "form": "해외에서 느셨다니",
          "original_form": "해외에서 느셨다니",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027162.1.26",
          "form": "갑자기 궁금해진게 있는데 그럼",
          "original_form": "갑자기 궁금해진게 있는데 그럼",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027162.1.27",
          "form": "외국 음식 위주로 많이 만드셨나요?",
          "original_form": "외국 음식 위주로 많이 만드셨나요?",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100027162.1.28",
          "form": "음 오히려 한식에 대한 갈망이 강해져서",
          "original_form": "음 오히려 한식에 대한 갈망이 강해져서",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027162.1.29",
          "form": "닭도리탕 같은 거나",
          "original_form": "닭도리탕 같은 거나",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027162.1.30",
          "form": "감자전을 많이 만들었어요 ㅋㅋㅋ",
          "original_form": "감자전을 많이 만들었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027162.1.31",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027162.1.32",
          "form": "감자전은 쉽게 믄들 수 있을것같은데",
          "original_form": "감자전은 쉽게 믄들 수 있을것같은데",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100027162.1.33",
          "form": "닭도리탕은 어려울것겉아요",
          "original_form": "닭도리탕은 어려울것겉아요",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027162.1.34",
          "form": "재료 구하기 힘드시지않았나요?",
          "original_form": "재료 구하기 힘드시지않았나요?",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027162.1.35",
          "form": "아시안마트가 있어서 고추장 같은 건 다 팔더라고요!",
          "original_form": "아시안마트가 있어서 고추장 같은 건 다 팔더라고요!",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027162.1.36",
          "form": "정말 좋은 시절이었죠...",
          "original_form": "정말 좋은 시절이었죠...",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027162.1.37",
          "form": "혼자 사세요??",
          "original_form": "혼자 사세요??",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027162.1.38",
          "form": "아뇨 가족들이랑 사는데",
          "original_form": "아뇨 가족들이랑 사는데",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027162.1.39",
          "form": "음식은 알아서 먹는편이라",
          "original_form": "음식은 알아서 먹는편이라",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027162.1.40",
          "form": "요리 조금씩 하고있어요",
          "original_form": "요리 조금씩 하고있어요",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027162.1.41",
          "form": "아하! 저도 가족들이랑 사는데",
          "original_form": "아하! 저도 가족들이랑 사는데",
          "speaker_id": "1",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027162.1.42",
          "form": "다들 밥 먹는 시간대가 달라서 제가 직접 해먹어요 ㅋㅋㅋ",
          "original_form": "다들 밥 먹는 시간대가 달라서 제가 직접 해먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027162.1.43",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100027162.1.44",
          "form": "전 처음에 양 조절하는게 힘들더라구요",
          "original_form": "전 처음에 양 조절하는게 힘들더라구요",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100027162.1.45",
          "form": "이젠 1인분은 꽤 하는데 1인분밖에 못해요ㅋㅋㅋ",
          "original_form": "이젠 1인분은 꽤 하는데 1인분밖에 못해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:32"
        }
      ]
