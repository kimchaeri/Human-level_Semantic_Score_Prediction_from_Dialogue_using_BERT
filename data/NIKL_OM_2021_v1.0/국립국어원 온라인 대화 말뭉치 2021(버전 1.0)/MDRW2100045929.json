      "utterance": [
        {
          "id": "MDRW2100045929.1.1",
          "form": "어제 저녁 뭐 드셨어요",
          "original_form": "어제 저녁 뭐 드셨어요",
          "speaker_id": "2",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045929.1.2",
          "form": "오늘 아침",
          "original_form": "오늘 아침",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045929.1.3",
          "form": "머드실꺼애요",
          "original_form": "머드실꺼애요",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045929.1.4",
          "form": "그냥 물마시다가 냉장고 기웃대고있어요",
          "original_form": "그냥 물마시다가 냉장고 기웃대고있어요",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045929.1.5",
          "form": "뭐드실거에요",
          "original_form": "뭐드실거에요",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045929.1.6",
          "form": "저는 저녁에 새우볶음밥에 반찬셋 청국장이요",
          "original_form": "저는 저녁에 새우볶음밥에 반찬셋 청국장이요",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045929.1.7",
          "form": "그러셨군요 어제 낙지볶음 포장해와서 먹었어요",
          "original_form": "그러셨군요 어제 낙지볶음 포장해와서 먹었어요",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045929.1.8",
          "form": "왜요 배안고프신가요",
          "original_form": "왜요 배안고프신가요",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045929.1.9",
          "form": "헐 완전 맛낫겟네요",
          "original_form": "헐 완전 맛낫겟네요",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045929.1.10",
          "form": "새우깡이랑 빵 사러가는김에요ㅋ",
          "original_form": "새우깡이랑 빵 사러가는김에요ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045929.1.11",
          "form": "그래서 배고파서 뒤져보고있어요",
          "original_form": "그래서 배고파서 뒤져보고있어요",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045929.1.12",
          "form": "지금 나가실려구요?",
          "original_form": "지금 나가실려구요?",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045929.1.13",
          "form": "저는 요거트 하나 묵었습니당",
          "original_form": "저는 요거트 하나 묵었습니당",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045929.1.14",
          "form": "아니요 어제사러갔다왔어요",
          "original_form": "아니요 어제사러갔다왔어요",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045929.1.15",
          "form": "오히려 님이 더배고프시겠네요",
          "original_form": "오히려 님이 더배고프시겠네요",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045929.1.16",
          "form": "저는 카피도마시는걸료",
          "original_form": "저는 카피도마시는걸료",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045929.1.17",
          "form": "커피가 식사가되나요 잘챙겨드세요",
          "original_form": "커피가 식사가되나요 잘챙겨드세요",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045929.1.18",
          "form": "아침에눈 일어나기바빠서",
          "original_form": "아침에눈 일어나기바빠서",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045929.1.19",
          "form": "물만 드시는분이 할소리인가요",
          "original_form": "물만 드시는분이 할소리인가요",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045929.1.20",
          "form": "옙",
          "original_form": "옙",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045929.1.21",
          "form": "점심에는 뭐드실려구요",
          "original_form": "점심에는 뭐드실려구요",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100045929.1.22",
          "form": "낙지볶음 조금 남았는데 그거먹을까해요 당신은요?",
          "original_form": "낙지볶음 조금 남았는데 그거먹을까해요 당신은요?",
          "speaker_id": "2",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100045929.1.23",
          "form": "볶음밥 해먹늠 짱이겠어요",
          "original_form": "볶음밥 해먹늠 짱이겠어요",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045929.1.24",
          "form": "치즈는 없나요 ㅜㅜ",
          "original_form": "치즈는 없나요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045929.1.25",
          "form": "치즈없어요ㅜㅜ아쉽네요",
          "original_form": "치즈없어요ㅜㅜ아쉽네요",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045929.1.26",
          "form": "그럼 눌러서 바삭하게 드세요",
          "original_form": "그럼 눌러서 바삭하게 드세요",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045929.1.27",
          "form": "예 그렇게할게요",
          "original_form": "예 그렇게할게요",
          "speaker_id": "2",
          "time": "20210908 09:07"
        }
      ]
    }
