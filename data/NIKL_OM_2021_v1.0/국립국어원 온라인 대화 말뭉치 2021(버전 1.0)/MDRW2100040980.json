      "utterance": [
        {
          "id": "MDRW2100040980.1.1",
          "form": "아니 그리고",
          "original_form": "아니 그리고",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.2",
          "form": "야에 미코",
          "original_form": "야에 미코",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.3",
          "form": "뭔데 진짜",
          "original_form": "뭔데 진짜",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.4",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.5",
          "form": "얜 심지어",
          "original_form": "얜 심지어",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.6",
          "form": "뭐하는짓인데",
          "original_form": "뭐하는짓인데",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.7",
          "form": "초대 삼봉행도 아닌데",
          "original_form": "초대 삼봉행도 아닌데",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.8",
          "form": "뭐가 어케 된거임",
          "original_form": "뭐가 어케 된거임",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.9",
          "form": "무슨관계임;;;;",
          "original_form": "무슨관계임;;;;",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.10",
          "form": "ㅇㅇ어디서 튀어나온거안",
          "original_form": "ㅇㅇ어디서 튀어나온거안",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.11",
          "form": "야",
          "original_form": "야",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.12",
          "form": "하는 말 보면",
          "original_form": "하는 말 보면",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.13",
          "form": "전대 라이덴도 아님",
          "original_form": "전대 라이덴도 아님",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100040980.1.14",
          "form": "라이덴이랑도 싸웟어?",
          "original_form": "라이덴이랑도 싸웟어?",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100040980.1.15",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100040980.1.16",
          "form": "그거 중간에 끊었다가",
          "original_form": "그거 중간에 끊었다가",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.17",
          "form": "그것도 좀",
          "original_form": "그것도 좀",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.18",
          "form": "어이없지 않니 ㅋㅋㅋㅋ",
          "original_form": "어이없지 않니 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.19",
          "form": "다 다시 할까봐",
          "original_form": "다 다시 할까봐",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.20",
          "form": "ㄹㅇㅎ",
          "original_form": "ㄹㅇㅎ",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.21",
          "form": "소년만화임?",
          "original_form": "소년만화임?",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.22",
          "form": "심지어",
          "original_form": "심지어",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.23",
          "form": "지구사람들이여 모두의 기를 나에게 나눠줘 임?",
          "original_form": "지구사람들이여 모두의 기를 나에게 나눠줘 임?",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.24",
          "form": "무상의 일태도 막은게",
          "original_form": "무상의 일태도 막은게",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.25",
          "form": "주인공도 아니고",
          "original_form": "주인공도 아니고",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.26",
          "form": "카즈하의 죽은 모브 친구라는게",
          "original_form": "카즈하의 죽은 모브 친구라는게",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.27",
          "form": "더 어이없어 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "더 어이없어 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.28",
          "form": "갑자기 왜 인간찬가가 되는건지....?",
          "original_form": "갑자기 왜 인간찬가가 되는건지....?",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040980.1.29",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100040980.1.30",
          "form": "스토리 퀄이 이따위면",
          "original_form": "스토리 퀄이 이따위면",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100040980.1.31",
          "form": "또 어디서 베껴온거냐",
          "original_form": "또 어디서 베껴온거냐",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100040980.1.32",
          "form": "소리말곤",
          "original_form": "소리말곤",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100040980.1.33",
          "form": "해줄게 없음",
          "original_form": "해줄게 없음",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100040980.1.34",
          "form": "그리고 트레일러로",
          "original_form": "그리고 트레일러로",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100040980.1.35",
          "form": "기억미화시키고 억지로 감정 끌고나오려들지 마라",
          "original_form": "기억미화시키고 억지로 감정 끌고나오려들지 마라",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040980.1.36",
          "form": "우리 그정도로 안친했다",
          "original_form": "우리 그정도로 안친했다",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040980.1.37",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040980.1.38",
          "form": "난 이나즈마에서 친숙함을 느껴본 적이 없는데",
          "original_form": "난 이나즈마에서 친숙함을 느껴본 적이 없는데",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040980.1.39",
          "form": "왜 갑자기 혼자....?",
          "original_form": "왜 갑자기 혼자....?",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040980.1.40",
          "form": "ㅇㅇ당장 우리 계속",
          "original_form": "ㅇㅇ당장 우리 계속",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040980.1.41",
          "form": "여긴 너무 공허하고 음침하고",
          "original_form": "여긴 너무 공허하고 음침하고",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040980.1.42",
          "form": "정이 안붙고",
          "original_form": "정이 안붙고",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040980.1.43",
          "form": "어쩌고저쩌고 했는데",
          "original_form": "어쩌고저쩌고 했는데",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100040980.1.44",
          "form": "갑자기 어디서 튀어나온지 모를",
          "original_form": "갑자기 어디서 튀어나온지 모를",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100040980.1.45",
          "form": "인간들이",
          "original_form": "인간들이",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100040980.1.46",
          "form": "와아아하고있음",
          "original_form": "와아아하고있음",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100040980.1.47",
          "form": "누구세요 니네",
          "original_form": "누구세요 니네",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100040980.1.48",
          "form": "난 모르는 사람이야...",
          "original_form": "난 모르는 사람이야...",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100040980.1.49",
          "form": "스토리 개연성 대체 어디로감 ㅋㅋㅋ",
          "original_form": "스토리 개연성 대체 어디로감 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:36"
        }
      ]
