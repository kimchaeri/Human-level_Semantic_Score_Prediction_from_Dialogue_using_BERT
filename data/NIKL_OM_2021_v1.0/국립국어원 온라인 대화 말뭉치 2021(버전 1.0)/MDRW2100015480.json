      "utterance": [
        {
          "id": "MDRW2100015480.1.1",
          "form": "이제 곧 가을인데 어떤 옷 살꺼야?",
          "original_form": "이제 곧 가을인데 어떤 옷 살꺼야?",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015480.1.2",
          "form": "나는 가디건 사고싶다",
          "original_form": "나는 가디건 사고싶다",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015480.1.3",
          "form": "아 진짜 어떤 가디건?",
          "original_form": "아 진짜 어떤 가디건?",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015480.1.4",
          "form": "볼레로 가디건 어때?",
          "original_form": "볼레로 가디건 어때?",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015480.1.5",
          "form": "아 그거 내가 사봤는데 좀 입기에 불편행ㅜ",
          "original_form": "아 그거 내가 사봤는데 좀 입기에 불편행ㅜ",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015480.1.6",
          "form": "아 정말?",
          "original_form": "아 정말?",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015480.1.7",
          "form": "환불했어?",
          "original_form": "환불했어?",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015480.1.8",
          "form": "응응 입어봣는데 나하고는 안맞더라구",
          "original_form": "응응 입어봣는데 나하고는 안맞더라구",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015480.1.9",
          "form": "어떤점이 불편한데?",
          "original_form": "어떤점이 불편한데?",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015480.1.10",
          "form": "가디건이 만들다 만 느낌이 들어ㅋㅋㅋ",
          "original_form": "가디건이 만들다 만 느낌이 들어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015480.1.11",
          "form": "ㅠㅠ 그럼 크롭 가디건은 어때?",
          "original_form": "ㅠㅠ 그럼 크롭 가디건은 어때?",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015480.1.12",
          "form": "웅웅ㅇ 나도 크롭 가디건은 여러개 있는데 좋더라",
          "original_form": "웅웅ㅇ 나도 크롭 가디건은 여러개 있는데 좋더라",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015480.1.13",
          "form": "오 무슨 색 있어?",
          "original_form": "오 무슨 색 있어?",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015480.1.14",
          "form": "검은색이랑 흰색!",
          "original_form": "검은색이랑 흰색!",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015480.1.15",
          "form": "검은색",
          "original_form": "검은색",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015480.1.16",
          "form": "나는 카키색 사고싶은데 어떨것같아?",
          "original_form": "나는 카키색 사고싶은데 어떨것같아?",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015480.1.17",
          "form": "가을에 입기에 무난하게 이쁠 것 같은데?",
          "original_form": "가을에 입기에 무난하게 이쁠 것 같은데?",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015480.1.18",
          "form": "그럼 카키로 사야겠다 너는 사고싶은 옷 있어?",
          "original_form": "그럼 카키로 사야겠다 너는 사고싶은 옷 있어?",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015480.1.19",
          "form": "응 난 요새 셋업 사려고 열심히 보는 중ㅋㅋㅋ",
          "original_form": "응 난 요새 셋업 사려고 열심히 보는 중ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015480.1.20",
          "form": "셋업이 뭐야?",
          "original_form": "셋업이 뭐야?",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015480.1.21",
          "form": "자켓이랑 스커트 세트로 있는 투피스??같은 느낌이야",
          "original_form": "자켓이랑 스커트 세트로 있는 투피스??같은 느낌이야",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015480.1.22",
          "form": "아하 나도 그런거 좋아해",
          "original_form": "아하 나도 그런거 좋아해",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015480.1.23",
          "form": "얼마정도 생각해?",
          "original_form": "얼마정도 생각해?",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015480.1.24",
          "form": "5 6만원 정도면 괜찮지안ㄹ아?",
          "original_form": "5 6만원 정도면 괜찮지안ㄹ아?",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015480.1.25",
          "form": "내가 본건 3만원대인데 괜찮더라고",
          "original_form": "내가 본건 3만원대인데 괜찮더라고",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015480.1.26",
          "form": "아 어떤건데?",
          "original_form": "아 어떤건데?",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015480.1.27",
          "form": "세미정장같은 느낌이야!",
          "original_form": "세미정장같은 느낌이야!",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015480.1.28",
          "form": "오 어떤 색?",
          "original_form": "오 어떤 색?",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015480.1.29",
          "form": "여러가지 있어",
          "original_form": "여러가지 있어",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015480.1.30",
          "form": "이제 곧 너 생일이네",
          "original_form": "이제 곧 너 생일이네",
          "speaker_id": "1",
          "time": "20210825 23:30"
        }
      ]
    }
