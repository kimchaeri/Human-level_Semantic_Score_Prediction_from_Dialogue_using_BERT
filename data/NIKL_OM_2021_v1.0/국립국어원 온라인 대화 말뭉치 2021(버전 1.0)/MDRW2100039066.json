      "utterance": [
        {
          "id": "MDRW2100039066.1.1",
          "form": "비오네 또",
          "original_form": "비오네 또",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039066.1.2",
          "form": "우산들고갔낭??",
          "original_form": "우산들고갔낭??",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039066.1.3",
          "form": "오늘 일하러갔구먼",
          "original_form": "오늘 일하러갔구먼",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100039066.1.4",
          "form": "응 언니 이제 일 마쳤다",
          "original_form": "응 언니 이제 일 마쳤다",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100039066.1.5",
          "form": "곧 밥먹으러가겠네ㅋㄲ",
          "original_form": "곧 밥먹으러가겠네ㅋㄲ",
          "speaker_id": "1",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100039066.1.6",
          "form": "수고했고 만",
          "original_form": "수고했고 만",
          "speaker_id": "2",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100039066.1.7",
          "form": "오늘 점심은 제육볶음",
          "original_form": "오늘 점심은 제육볶음",
          "speaker_id": "2",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100039066.1.8",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100039066.1.9",
          "form": "나는 그냥 볶음밥",
          "original_form": "나는 그냥 볶음밥",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100039066.1.10",
          "form": "저녁에는 미역국 먹을려고",
          "original_form": "저녁에는 미역국 먹을려고",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100039066.1.11",
          "form": "미역국 좋지",
          "original_form": "미역국 좋지",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100039066.1.12",
          "form": "어제는 너무 피곤해서",
          "original_form": "어제는 너무 피곤해서",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100039066.1.13",
          "form": "건조기에 빨래 넣어놓고 그냥 잤음 ㅎㅎ",
          "original_form": "건조기에 빨래 넣어놓고 그냥 잤음 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100039066.1.14",
          "form": "잘했다 ㅋㅋㅋ 나도 한번씩 그런다 ㅋㅋ",
          "original_form": "잘했다 ㅋㅋㅋ 나도 한번씩 그런다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:18"
        },
        {
          "id": "MDRW2100039066.1.15",
          "form": "나는 오늘 아침에 후딱",
          "original_form": "나는 오늘 아침에 후딱",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039066.1.16",
          "form": "해놔가꼬 ㅋㅋ 오늘은 좀 여유가 있네 ㅋㅋ",
          "original_form": "해놔가꼬 ㅋㅋ 오늘은 좀 여유가 있네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039066.1.17",
          "form": "좋다 좋아 ㅋㅋ",
          "original_form": "좋다 좋아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100039066.1.18",
          "form": "ㅎㅎ 좋네 오늘은 ㅎㅎ",
          "original_form": "ㅎㅎ 좋네 오늘은 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039066.1.19",
          "form": "평일 오후에 케이스마트가면 조용하나??",
          "original_form": "평일 오후에 케이스마트가면 조용하나??",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039066.1.20",
          "form": "응 애들 몇명 없더라 ㅋㅋ 어제는 7시 넘으니깐",
          "original_form": "응 애들 몇명 없더라 ㅋㅋ 어제는 7시 넘으니깐",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039066.1.21",
          "form": "우리만 있더라구",
          "original_form": "우리만 있더라구",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039066.1.22",
          "form": "채윤,지안,지오",
          "original_form": "채윤,지안,지오",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039066.1.23",
          "form": "옹 좋다 ㅋㅋㅋㅋㅋ 채윤이도 같이갔나?? ㅋㅋ",
          "original_form": "옹 좋다 ㅋㅋㅋㅋㅋ 채윤이도 같이갔나?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039066.1.24",
          "form": "담주 월요일 가까??",
          "original_form": "담주 월요일 가까??",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039066.1.25",
          "form": "화수금은 수업있어서 안되고",
          "original_form": "화수금은 수업있어서 안되고",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039066.1.26",
          "form": "월목가능 ㅎㅎ",
          "original_form": "월목가능 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039066.1.27",
          "form": "응응 지안이가 채윤이랑 합기도 마치고 만나자고 약속했나봐. 둘이서",
          "original_form": "응응 지안이가 채윤이랑 합기도 마치고 만나자고 약속했나봐. 둘이서",
          "speaker_id": "1",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100039066.1.28",
          "form": "내일 또 낮에 만나서 놀아라고 하니깐",
          "original_form": "내일 또 낮에 만나서 놀아라고 하니깐",
          "speaker_id": "1",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100039066.1.29",
          "form": "엄마 나는 장난감 안사주는건 참고, 양보할수있다.",
          "original_form": "엄마 나는 장난감 안사주는건 참고, 양보할수있다.",
          "speaker_id": "1",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100039066.1.30",
          "form": "근데 친구랑 약속은 양보못한다. 이래뿌...막무가내임.",
          "original_form": "근데 친구랑 약속은 양보못한다. 이래뿌...막무가내임.",
          "speaker_id": "1",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100039066.1.31",
          "form": "그래가 울 첫찌도....친구도..........서로......너무 ....놀고싶어해가 그냥 케이스마트 델꼬갔다가 밥 먹이고 보냄 ㅋㅋ",
          "original_form": "그래가 울 첫찌도....친구도..........서로......너무 ....놀고싶어해가 그냥 케이스마트 델꼬갔다가 밥 먹이고 보냄 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100039066.1.32",
          "form": "월,목 나두 가능하다 ㅋ",
          "original_form": "월,목 나두 가능하다 ㅋ",
          "speaker_id": "1",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100039066.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 친구랑 약속",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 친구랑 약속",
          "speaker_id": "2",
          "time": "20210903 14:33"
        },
        {
          "id": "MDRW2100039066.1.34",
          "form": "그래 양보못하지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래 양보못하지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:33"
        },
        {
          "id": "MDRW2100039066.1.35",
          "form": "좋다 ㅋㅋㅋㅋㅋㅋ 그럼 월요일 고고",
          "original_form": "좋다 ㅋㅋㅋㅋㅋㅋ 그럼 월요일 고고",
          "speaker_id": "2",
          "time": "20210903 14:33"
        },
        {
          "id": "MDRW2100039066.1.36",
          "form": "그러니깐은 하유",
          "original_form": "그러니깐은 하유",
          "speaker_id": "1",
          "time": "20210903 14:33"
        },
        {
          "id": "MDRW2100039066.1.37",
          "form": "아 언니 목욜가다",
          "original_form": "아 언니 목욜가다",
          "speaker_id": "1",
          "time": "20210903 14:34"
        },
        {
          "id": "MDRW2100039066.1.38",
          "form": "목욜 고고",
          "original_form": "목욜 고고",
          "speaker_id": "1",
          "time": "20210903 14:34"
        },
        {
          "id": "MDRW2100039066.1.39",
          "form": "엉 알았으 목욜 ㅋㅋ",
          "original_form": "엉 알았으 목욜 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:45"
        }
      ]
    }
