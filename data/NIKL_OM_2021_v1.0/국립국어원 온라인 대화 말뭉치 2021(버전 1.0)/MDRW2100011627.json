      "utterance": [
        {
          "id": "MDRW2100011627.1.1",
          "form": "여행 어디가고 싶어?",
          "original_form": "여행 어디가고 싶어?",
          "speaker_id": "1",
          "time": "20210824 14:39"
        },
        {
          "id": "MDRW2100011627.1.2",
          "form": "나는 해외가고싶다",
          "original_form": "나는 해외가고싶다",
          "speaker_id": "1",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011627.1.3",
          "form": "나도 해외여행",
          "original_form": "나도 해외여행",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011627.1.4",
          "form": "유럽쪽으로 나가고싶네",
          "original_form": "유럽쪽으로 나가고싶네",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011627.1.5",
          "form": "유럽어디?",
          "original_form": "유럽어디?",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011627.1.6",
          "form": "서유럽이면 좋지",
          "original_form": "서유럽이면 좋지",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011627.1.7",
          "form": "미국도 가고싶긴하도",
          "original_form": "미국도 가고싶긴하도",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011627.1.8",
          "form": "서유럽중에서도?",
          "original_form": "서유럽중에서도?",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011627.1.9",
          "form": "이번에 10년짜리 보험타서..여행갈 돈은 생기는데",
          "original_form": "이번에 10년짜리 보험타서..여행갈 돈은 생기는데",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011627.1.10",
          "form": "이탈리아",
          "original_form": "이탈리아",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011627.1.11",
          "form": "언니야는 어디가고싶노",
          "original_form": "언니야는 어디가고싶노",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011627.1.12",
          "form": "박서준..레스토랑 한곳어디지? 거기도 여행 가고싶은데",
          "original_form": "박서준..레스토랑 한곳어디지? 거기도 여행 가고싶은데",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011627.1.13",
          "form": "나는 동남아라도 가고싶다",
          "original_form": "나는 동남아라도 가고싶다",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011627.1.14",
          "form": "어디였지?",
          "original_form": "어디였지?",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011627.1.15",
          "form": "스페인",
          "original_form": "스페인",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011627.1.16",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011627.1.17",
          "form": "가고싶드라 너무 좋아보이더라",
          "original_form": "가고싶드라 너무 좋아보이더라",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011627.1.18",
          "form": "그러게 너무 좋더라",
          "original_form": "그러게 너무 좋더라",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011627.1.19",
          "form": "코로나끝나면..우린 멀리도 갈수있는데",
          "original_form": "코로나끝나면..우린 멀리도 갈수있는데",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011627.1.20",
          "form": "가자 멀리",
          "original_form": "가자 멀리",
          "speaker_id": "1",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011627.1.21",
          "form": "애 데리고 갈곳은.. 유럽이나",
          "original_form": "애 데리고 갈곳은.. 유럽이나",
          "speaker_id": "2",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011627.1.22",
          "form": "미국",
          "original_form": "미국",
          "speaker_id": "2",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011627.1.23",
