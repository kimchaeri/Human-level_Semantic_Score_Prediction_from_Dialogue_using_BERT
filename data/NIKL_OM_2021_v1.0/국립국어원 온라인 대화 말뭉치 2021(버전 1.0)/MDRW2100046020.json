      "utterance": [
        {
          "id": "MDRW2100046020.1.1",
          "form": "아니요 중식도 아닙니다~",
          "original_form": "아니요 중식도 아닙니다~",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046020.1.2",
          "form": "앗 그럼 한식이군요!",
          "original_form": "앗 그럼 한식이군요!",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046020.1.3",
          "form": "저는 역시 한국 사람인가 봐요",
          "original_form": "저는 역시 한국 사람인가 봐요",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046020.1.4",
          "form": "뼛속까지 한국인!",
          "original_form": "뼛속까지 한국인!",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046020.1.5",
          "form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046020.1.6",
          "form": "밥이 든든하잖아유",
          "original_form": "밥이 든든하잖아유",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046020.1.7",
          "form": "암 그럼요",
          "original_form": "암 그럼요",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046020.1.8",
          "form": "그렇다면~~ 계란이 들어가나요?",
          "original_form": "그렇다면~~ 계란이 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046020.1.9",
          "form": "네 들어갑니다!",
          "original_form": "네 들어갑니다!",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100046020.1.10",
          "form": "오예 맞췄다",
          "original_form": "오예 맞췄다",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100046020.1.11",
          "form": "구운 요리인가요?",
          "original_form": "구운 요리인가요?",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100046020.1.12",
          "form": "아니요 굽는 요리는 아니에요",
          "original_form": "아니요 굽는 요리는 아니에요",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046020.1.13",
          "form": "그럼 국물이 있는 요리인가요?",
          "original_form": "그럼 국물이 있는 요리인가요?",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046020.1.14",
          "form": "이것도 국물이라고 한다면 약간 있어요",
          "original_form": "이것도 국물이라고 한다면 약간 있어요",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046020.1.15",
          "form": "아주 쪼금이라니",
          "original_form": "아주 쪼금이라니",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046020.1.16",
          "form": "야채가 들어가나요?",
          "original_form": "야채가 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046020.1.17",
          "form": "들어갑니다!",
          "original_form": "들어갑니다!",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046020.1.18",
          "form": "많이는 안 들어가요",
          "original_form": "많이는 안 들어가요",
          "speaker_id": "2",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046020.1.19",
          "form": "으으으으으음",
          "original_form": "으으으으으음",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046020.1.20",
          "form": "고기가 들어가나요??",
          "original_form": "고기가 들어가나요??",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046020.1.21",
          "form": "들어가지는 않고 같이 먹기도해요!",
          "original_form": "들어가지는 않고 같이 먹기도해요!",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046020.1.22",
          "form": "맵지 않은 음식인가요!",
          "original_form": "맵지 않은 음식인가요!",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046020.1.23",
          "form": "맵찔이에게 매울 수도 있어요",
          "original_form": "맵찔이에게 매울 수도 있어요",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046020.1.24",
          "form": "앗 맵찔이ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 맵찔이ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046020.1.25",
          "form": "와 계란 들어가고 국물 없는데 매운 요리..",
          "original_form": "와 계란 들어가고 국물 없는데 매운 요리..",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046020.1.26",
          "form": "쌀이 들어가나요?",
          "original_form": "쌀이 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046020.1.27",
          "form": "쌀은 안 들어가요!",
          "original_form": "쌀은 안 들어가요!",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046020.1.28",
          "form": "으으으으으음 뭘까요 볶음이예요?",
          "original_form": "으으으으으음 뭘까요 볶음이예요?",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046020.1.29",
          "form": "아니요 볶음 요리는 아니요!",
          "original_form": "아니요 볶음 요리는 아니요!",
          "speaker_id": "2",
          "time": "20210908 09:59"
        }
      ]
    }
  ]
