      "utterance": [
        {
          "id": "MDRW2100025574.1.1",
          "form": "오늘 저녁은 뭐 드실건가용",
          "original_form": "오늘 저녁은 뭐 드실건가용",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025574.1.2",
          "form": "ㅠㅠ 전 오늘 좀 잡무가 남아서",
          "original_form": "ㅠㅠ 전 오늘 좀 잡무가 남아서",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025574.1.3",
          "form": "집에가고 싶어요",
          "original_form": "집에가고 싶어요",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025574.1.4",
          "form": "집에갈수있을 때가 되면 결정하려고요,,",
          "original_form": "집에갈수있을 때가 되면 결정하려고요,,",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025574.1.5",
          "form": "헉 야근하시구나ㅠ",
          "original_form": "헉 야근하시구나ㅠ",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025574.1.6",
          "form": "저는 운동가기전에 복숭아 먹었어요",
          "original_form": "저는 운동가기전에 복숭아 먹었어요",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025574.1.7",
          "form": "그런데도 배고프네요",
          "original_form": "그런데도 배고프네요",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025574.1.8",
          "form": "야근여부는 제 손이 정하는데 과연,,",
          "original_form": "야근여부는 제 손이 정하는데 과연,,",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025574.1.9",
          "form": "복숭아 하나니깐 배가 고프죠 밥이 아니니간요",
          "original_form": "복숭아 하나니깐 배가 고프죠 밥이 아니니간요",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025574.1.10",
          "form": "빨리 저녁 드셔야겠네요ㅋㅋㅋ",
          "original_form": "빨리 저녁 드셔야겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100025574.1.11",
          "form": "공복후 운동이니",
          "original_form": "공복후 운동이니",
          "speaker_id": "1",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100025574.1.12",
          "form": "운동하고 와서",
          "original_form": "운동하고 와서",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100025574.1.13",
          "form": "엄청 먹었어요",
          "original_form": "엄청 먹었어요",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100025574.1.14",
          "form": "지금 배터질거같아요",
          "original_form": "지금 배터질거같아요",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100025574.1.15",
          "form": "퇴근은 하셨나요",
          "original_form": "퇴근은 하셨나요",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100025574.1.16",
          "form": "ㅠㅠ 그냥 일과 함께 집으로 왔어요,,",
          "original_form": "ㅠㅠ 그냥 일과 함께 집으로 왔어요,,",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100025574.1.17",
          "form": "저녁은 맛있는걸 드셨나요?",
          "original_form": "저녁은 맛있는걸 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100025574.1.18",
          "form": "고생하셨어요ㅠ",
          "original_form": "고생하셨어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100025574.1.19",
          "form": "ㅠㅠ,,,",
          "original_form": "ㅠㅠ,,,",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100025574.1.20",
          "form": "name2님이라도 맛있는 걸 드셨길,,",
          "original_form": "&name2&님이라도 맛있는 걸 드셨길,,",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100025574.1.21",
          "form": "저녁은 깻잎김치하고 비엔나소세지 먹었어요",
          "original_form": "저녁은 깻잎김치하고 비엔나소세지 먹었어요",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100025574.1.22",
          "form": "저녁드셨어요!?",
          "original_form": "저녁드셨어요!?",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100025574.1.23",
          "form": "ㅠ집밥과 그냥,,함꼐했어요",
          "original_form": "ㅠ집밥과 그냥,,함꼐했어요",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100025574.1.24",
          "form": "반찬대충,,김과 함께요",
          "original_form": "반찬대충,,김과 함께요",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100025574.1.25",
          "form": "ㅠㅠㅎ 내일은 꼭 맛난거 드세요",
          "original_form": "ㅠㅠㅎ 내일은 꼭 맛난거 드세요",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100025574.1.26",
          "form": "김도 맛있지만",
          "original_form": "김도 맛있지만",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100025574.1.27",
          "form": "스트레스 풀릴 메뉴같은거로!",
          "original_form": "스트레스 풀릴 메뉴같은거로!",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100025574.1.28",
          "form": "내일은 그냥 사먹어야겠어요",
          "original_form": "내일은 그냥 사먹어야겠어요",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100025574.1.29",
          "form": "포장해서 와도 포장용기 씻느게 싫어서 포장은 잘 안하는데,,",
          "original_form": "포장해서 와도 포장용기 씻느게 싫어서 포장은 잘 안하는데,,",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100025574.1.30",
          "form": "아 맞아요 그거 씻는것도 일이죠",
          "original_form": "아 맞아요 그거 씻는것도 일이죠",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100025574.1.31",
          "form": "비닐붙어있는거 떼는것도 일이고ㅡㄴㆍ",
          "original_form": "비닐붙어있는거 떼는것도 일이고ㅡㄴㆍ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100025574.1.32",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100025574.1.33",
          "form": "네 그리고 분리수거하러 나가야되고",
          "original_form": "네 그리고 분리수거하러 나가야되고",
          "speaker_id": "1",
          "time": "20210901 21:53"
        }
      ]
