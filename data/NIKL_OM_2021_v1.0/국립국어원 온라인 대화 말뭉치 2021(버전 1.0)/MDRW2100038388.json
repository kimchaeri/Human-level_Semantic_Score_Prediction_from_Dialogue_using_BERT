      "utterance": [
        {
          "id": "MDRW2100038388.1.1",
          "form": "집에서 뒹굴거리면서 유튜브나보고싶어요..",
          "original_form": "집에서 뒹굴거리면서 유튜브나보고싶어요..",
          "speaker_id": "2",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100038388.1.2",
          "form": "ㅎㅎ저는 그러고 있답니다",
          "original_form": "ㅎㅎ저는 그러고 있답니다",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100038388.1.3",
          "form": "지금 어제 방송한",
          "original_form": "지금 어제 방송한",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100038388.1.4",
          "form": "골때녀 보고있어요",
          "original_form": "골때녀 보고있어요",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100038388.1.5",
          "form": "여자들 축구 존잼!",
          "original_form": "여자들 축구 존잼!",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100038388.1.6",
          "form": "오 그것도요즘인기짱이던데ㅋㅋㅋㅋ다들잘한다구 ㅋㄱㅋㅋㅋ진심이라구ㅋㅋㅋ",
          "original_form": "오 그것도요즘인기짱이던데ㅋㅋㅋㅋ다들잘한다구 ㅋㄱㅋㅋㅋ진심이라구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100038388.1.7",
          "form": "진짜요!",
          "original_form": "진짜요!",
          "speaker_id": "1",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100038388.1.8",
          "form": "다리가 멍투성이고ㅜㅜ",
          "original_form": "다리가 멍투성이고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100038388.1.9",
          "form": "작은 멍 아니고 허벅지 전체가요",
          "original_form": "작은 멍 아니고 허벅지 전체가요",
          "speaker_id": "1",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100038388.1.10",
          "form": "ㅠㅡㅠ 진짜노력하시는게보이네요",
          "original_form": "ㅠㅡㅠ 진짜노력하시는게보이네요",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100038388.1.11",
          "form": "다들 너무너무 열심히니까 진짜 랜선응원",
          "original_form": "다들 너무너무 열심히니까 진짜 랜선응원",
          "speaker_id": "1",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100038388.1.12",
          "form": "열심히 하고 있어요",
          "original_form": "열심히 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100038388.1.13",
          "form": "악ㅋㅋ특별히 응원하시는팀있나요",
          "original_form": "악ㅋㅋ특별히 응원하시는팀있나요",
          "speaker_id": "2",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100038388.1.14",
          "form": "개벤져스라고 개그우먼팀이었는데",
          "original_form": "개벤져스라고 개그우먼팀이었는데",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038388.1.15",
          "form": "조별리그 탈락해서ㅜㅜ",
          "original_form": "조별리그 탈락해서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038388.1.16",
          "form": "앗 스포했네여",
          "original_form": "앗 스포했네여",
          "speaker_id": "1",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100038388.1.17",
          "form": "ㅋㅋㅋㅋ그다음은 구척장신 모델팀이랑",
          "original_form": "ㅋㅋㅋㅋ그다음은 구척장신 모델팀이랑",
          "speaker_id": "1",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100038388.1.18",
          "form": "국가대표팀 응원해요!",
          "original_form": "국가대표팀 응원해요!",
          "speaker_id": "1",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100038388.1.19",
          "form": "물론 다른 팀들도!!!",
          "original_form": "물론 다른 팀들도!!!",
          "speaker_id": "1",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100038388.1.20",
          "form": "스포괜찮아요ㅋㅋㅋㄱ\r\n와 거의다응원하는데요\r\nㅋㅋㅋㅋㄱ",
          "original_form": "스포괜찮아요ㅋㅋㅋㄱ\r\n와 거의다응원하는데요\r\nㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210902 13:00"
        },
        {
          "id": "MDRW2100038388.1.21",
          "form": "보다보면 그렇게 돼요ㅋㅋㅋㅋ",
          "original_form": "보다보면 그렇게 돼요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100038388.1.22",
          "form": "ㅋㅋㄲㄱ노력하는게 그렇게까지 보이면 다응원할거같아요ㅠㅡㅠ",
          "original_form": "ㅋㅋㄲㄱ노력하는게 그렇게까지 보이면 다응원할거같아요ㅠㅡㅠ",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100038388.1.23",
          "form": "방금 재방 다 봤는데",
          "original_form": "방금 재방 다 봤는데",
          "speaker_id": "1",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038388.1.24",
          "form": "제가 응원하는 팀이 졌어요ㅠㅠ",
          "original_form": "제가 응원하는 팀이 졌어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038388.1.25",
          "form": "아아..이런슬픈소식...",
          "original_form": "아아..이런슬픈소식...",
          "speaker_id": "2",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100038388.1.26",
          "form": "응원하는팀이져서아쉬우셧겠는데요ㅠㅡㅠ",
          "original_form": "응원하는팀이져서아쉬우셧겠는데요ㅠㅡㅠ",
          "speaker_id": "2",
          "time": "20210902 16:18"
        }
      ]
