      "utterance": [
        {
          "id": "MDRW2100012394.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012394.1.2",
          "form": "없어요 님은요?",
          "original_form": "없어요 님은요?",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012394.1.3",
          "form": "저도 무교에요",
          "original_form": "저도 무교에요",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012394.1.4",
          "form": "이유 있으신가요",
          "original_form": "이유 있으신가요",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012394.1.5",
          "form": "신이 존재한다는게 믿어지지 않아서요",
          "original_form": "신이 존재한다는게 믿어지지 않아서요",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012394.1.6",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012394.1.7",
          "form": "내 앞가림이나 잘하고",
          "original_form": "내 앞가림이나 잘하고",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012394.1.8",
          "form": "나를 믿으면 된다고 생각해서요",
          "original_form": "나를 믿으면 된다고 생각해서요",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012394.1.9",
          "form": "저도 그렇게 생각해요",
          "original_form": "저도 그렇게 생각해요",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012394.1.10",
          "form": "부모님도 종교가 없으셔서 그런 영향도 있는것 같아요",
          "original_form": "부모님도 종교가 없으셔서 그런 영향도 있는것 같아요",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012394.1.11",
          "form": "교회나 절 가보신적 있으세요?",
          "original_form": "교회나 절 가보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012394.1.12",
          "form": "둘다 가본적은 있어요",
          "original_form": "둘다 가본적은 있어요",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012394.1.13",
          "form": "저는 봉은사 가봤어요",
          "original_form": "저는 봉은사 가봤어요",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012394.1.14",
          "form": "어디 지역에 있는 절인가요?",
          "original_form": "어디 지역에 있는 절인가요?",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012394.1.15",
          "form": "서울 강남이요",
          "original_form": "서울 강남이요",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012394.1.16",
          "form": "오잉 되게 의외네요 \n산골짜기에 있을줄  알았는데",
          "original_form": "오잉 되게 의외네요 \n산골짜기에 있을줄  알았는데",
          "speaker_id": "2",
          "time": "20210824 22:10"
        },
