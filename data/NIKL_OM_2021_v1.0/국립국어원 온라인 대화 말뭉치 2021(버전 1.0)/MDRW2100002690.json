      "utterance": [
        {
          "id": "MDRW2100002690.1.1",
          "form": "name3이 군대 언제 간댔지?",
          "original_form": "&name3&이 군대 언제 간댔지?",
          "speaker_id": "1",
          "time": "20210820 14:49"
        },
        {
          "id": "MDRW2100002690.1.2",
          "form": "30일인가에 입대한대",
          "original_form": "30일인가에 입대한대",
          "speaker_id": "2",
          "time": "20210820 14:51"
        },
        {
          "id": "MDRW2100002690.1.3",
          "form": "육군이랬나?",
          "original_form": "육군이랬나?",
          "speaker_id": "1",
          "time": "20210820 14:51"
        },
        {
          "id": "MDRW2100002690.1.4",
          "form": "어 근데 바다쪽인가봐",
          "original_form": "어 근데 바다쪽인가봐",
          "speaker_id": "2",
          "time": "20210820 14:51"
        },
        {
          "id": "MDRW2100002690.1.5",
          "form": "예전에 name4오빠 군대갔다가 엄청 타서 왔잖아ㅋㅋㅋ",
          "original_form": "예전에 &name4&오빠 군대갔다가 엄청 타서 왔잖아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002690.1.6",
          "form": "name3이는 지금도 피부 까만데.",
          "original_form": "&name3&이는 지금도 피부 까만데.",
          "speaker_id": "2",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002690.1.7",
          "form": "선크림 잘 바르라고 얘기해줘ㅋㅋㅋㅋ",
          "original_form": "선크림 잘 바르라고 얘기해줘ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002690.1.8",
          "form": "미리 사줄까",
          "original_form": "미리 사줄까",
          "speaker_id": "2",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002690.1.9",
          "form": "이모가 챙겨주지 않을까??",
          "original_form": "이모가 챙겨주지 않을까??",
          "speaker_id": "1",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002690.1.10",
          "form": "하긴 그러겠다",
          "original_form": "하긴 그러겠다",
          "speaker_id": "2",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002690.1.11",
          "form": "걔는 군대에서도 종교시간만 기다리겠네",
          "original_form": "걔는 군대에서도 종교시간만 기다리겠네",
          "speaker_id": "1",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002690.1.12",
          "form": "요즘도 군대에서 교회 가면 맛있는 거 주나?",
          "original_form": "요즘도 군대에서 교회 가면 맛있는 거 주나?",
          "speaker_id": "2",
          "time": "20210820 14:54"
        },
        {
          "id": "MDRW2100002690.1.13",
          "form": "응ㅋㅋㅋㅋㅋ 내 친구도 무굔데 군대에서는 주일마다 종교 믿음ㅋㅋㅋㅌ",
          "original_form": "응ㅋㅋㅋㅋㅋ 내 친구도 무굔데 군대에서는 주일마다 종교 믿음ㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002690.1.14",
          "form": "암튼 고생할까봐 걱정이야.",
          "original_form": "암튼 고생할까봐 걱정이야.",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002690.1.15",
          "form": "이모도 빨리 갔다 오라고 한다며",
          "original_form": "이모도 빨리 갔다 오라고 한다며",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002690.1.16",
          "form": "엄마가 이모보다 더 걱정하는 것 같아",
          "original_form": "엄마가 이모보다 더 걱정하는 것 같아",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002690.1.17",
          "form": "걔가 조카들 중에 제일 막내잖아",
          "original_form": "걔가 조카들 중에 제일 막내잖아",
          "speaker_id": "2",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002690.1.18",
          "form": "그렇긴 한데.. 암튼 한살이라도 어릴 때 빨리 군대 갔다가 제대하는 게 낫지",
          "original_form": "그렇긴 한데.. 암튼 한살이라도 어릴 때 빨리 군대 갔다가 제대하는 게 낫지",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002690.1.19",
          "form": "내 친구는 아직도 선임이랑 연락하더라",
          "original_form": "내 친구는 아직도 선임이랑 연락하더라",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002690.1.20",
          "form": "해병대라 그런가",
          "original_form": "해병대라 그런가",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002690.1.21",
          "form": "해병대가 원래 기수제도 엄격하고 끈끈하잖아",
          "original_form": "해병대가 원래 기수제도 엄격하고 끈끈하잖아",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002690.1.22",
          "form": "하긴 그렇지.",
          "original_form": "하긴 그렇지.",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002690.1.23",
          "form": "그래도 요즘엔 전보다 군생활 편해졌잖아~ 같이 고생하니까 더 친해지나 봐",
          "original_form": "그래도 요즘엔 전보다 군생활 편해졌잖아~ 같이 고생하니까 더 친해지나 봐",
          "speaker_id": "1",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002690.1.24",
          "form": "name3이는 벌써 100일 휴가만 생각한대",
          "original_form": "&name3&이는 벌써 100일 휴가만 생각한대",
          "speaker_id": "2",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002690.1.25",
          "form": "ㅋㅋㅋ벌써??",
          "original_form": "ㅋㅋㅋ벌써??",
          "speaker_id": "1",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002690.1.26",
          "form": "걔 아직 백신 안 맞았지",
          "original_form": "걔 아직 백신 안 맞았지",
          "speaker_id": "1",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002690.1.27",
          "form": "응 군대에서 연락왔는데 입대 후에 맞는다고 했댜",
          "original_form": "응 군대에서 연락왔는데 입대 후에 맞는다고 했댜",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002690.1.28",
          "form": "했대.",
          "original_form": "했대.",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002690.1.29",
          "form": "입대 후에 맞으면 이틀 휴가래",
          "original_form": "입대 후에 맞으면 이틀 휴가래",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002690.1.30",
          "form": "대박 좋겠다",
          "original_form": "대박 좋겠다",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002690.1.31",
          "form": "요즘 군대 밥도 맛있대",
          "original_form": "요즘 군대 밥도 맛있대",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002690.1.32",
          "form": "name3이는 살 좀 쪄야 하는데.",
          "original_form": "&name3&이는 살 좀 쪄야 하는데.",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002690.1.33",
          "form": "군대 밥 먹고 살이 찔 수 있으려나 모르겠다.",
          "original_form": "군대 밥 먹고 살이 찔 수 있으려나 모르겠다.",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002690.1.34",
          "form": "훈련이 고되니까 밥도 잘 넘어가지 않을까",
          "original_form": "훈련이 고되니까 밥도 잘 넘어가지 않을까",
          "speaker_id": "1",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002690.1.35",
          "form": "배고파서라도 많이 먹겠다",
          "original_form": "배고파서라도 많이 먹겠다",
          "speaker_id": "1",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002690.1.36",
          "form": "너무 덥지 않을 때 들어가니까 훈련할 때 덜 힘들겠지",
          "original_form": "너무 덥지 않을 때 들어가니까 훈련할 때 덜 힘들겠지",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002690.1.37",
          "form": "겨울 훈련이 함들게 생겼어.",
          "original_form": "겨울 훈련이 함들게 생겼어.",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002690.1.38",
          "form": "겨울 훈련이 힘들게 생겼어.",
          "original_form": "겨울 훈련이 힘들게 생겼어.",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002690.1.39",
          "form": "1년 8개월 금방 가~",
          "original_form": "1년 8개월 금방 가~",
          "speaker_id": "1",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002690.1.40",
          "form": "휴가 나올 때 연락하라고 해. 밥 사준다고ㅋㅋㅋ",
          "original_form": "휴가 나올 때 연락하라고 해. 밥 사준다고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002690.1.41",
          "form": "알겠어 휴가 나오기 전에 전화하라고 할게.",
          "original_form": "알겠어 휴가 나오기 전에 전화하라고 할게.",
