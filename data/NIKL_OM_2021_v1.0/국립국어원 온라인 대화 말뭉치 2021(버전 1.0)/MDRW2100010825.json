      "utterance": [
        {
          "id": "MDRW2100010825.1.1",
          "form": "게임 하는거 있어??",
          "original_form": "게임 하는거 있어??",
          "speaker_id": "1",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100010825.1.2",
          "form": "쿠키런킹덤만 하고 다른건 안해",
          "original_form": "쿠키런킹덤만 하고 다른건 안해",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010825.1.3",
          "form": "게임하는거 있어?",
          "original_form": "게임하는거 있어?",
          "speaker_id": "2",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010825.1.4",
          "form": "헐 나도 쿠키런 킹덤해",
          "original_form": "헐 나도 쿠키런 킹덤해",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010825.1.5",
          "form": "ㅋㅋㅋ반갑다",
          "original_form": "ㅋㅋㅋ반갑다",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010825.1.6",
          "form": "파르페맛 뽑았어?",
          "original_form": "파르페맛 뽑았어?",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010825.1.7",
          "form": "오오 아직 ..ㅋㅋ 파르페맛 빼고 다있어",
          "original_form": "오오 아직 ..ㅋㅋ 파르페맛 빼고 다있어",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010825.1.8",
          "form": "잘 안나오더라고..",
          "original_form": "잘 안나오더라고..",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010825.1.9",
          "form": "나는 하나 뽑긴했는데 점점 쿠키보단 사람같아져",
          "original_form": "나는 하나 뽑긴했는데 점점 쿠키보단 사람같아져",
          "speaker_id": "1",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010825.1.10",
          "form": "ㅋㅋ나는 바요뽑는데 고생좀했어 그냥 정가 함ㅜㅜ",
          "original_form": "ㅋㅋ나는 바요뽑는데 고생좀했어 그냥 정가 함ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010825.1.11",
          "form": "맞아 사실 게임 약간 질린 것 같은데.. 그만두면 나중에 뒤쳐질 것 같아서 계속 하는중이애",
          "original_form": "맞아 사실 게임 약간 질린 것 같은데.. 그만두면 나중에 뒤쳐질 것 같아서 계속 하는중이애",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010825.1.12",
          "form": "캐릭터 중에서 누구를 제일 좋아해?",
          "original_form": "캐릭터 중에서 누구를 제일 좋아해?",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010825.1.13",
          "form": "헐 나도.. 요즘 예전만큼 재미는없어ㅋㅋㅋ 나는 마들렌보고 시작했어!",
          "original_form": "헐 나도.. 요즘 예전만큼 재미는없어ㅋㅋㅋ 나는 마들렌보고 시작했어!",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010825.1.14",
          "form": "마들렌 목소리가 너무 좋아ㅋㅋ",
          "original_form": "마들렌 목소리가 너무 좋아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010825.1.15",
          "form": "맞아ㅋㅋㅋ 하 처음에 막나왔을땐 진짜 재밌었는데",
          "original_form": "맞아ㅋㅋㅋ 하 처음에 막나왔을땐 진짜 재밌었는데",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010825.1.16",
          "form": "약간 질리는 이유가 뭐같아??",
          "original_form": "약간 질리는 이유가 뭐같아??",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010825.1.17",
          "form": "뭔가 계속 반복되니까 지루하고.. 건설이랑 재료만드는 것도 오래 걸리고 .. ㅜㅜ",
          "original_form": "뭔가 계속 반복되니까 지루하고.. 건설이랑 재료만드는 것도 오래 걸리고 .. ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010825.1.18",
          "form": "각설탕 맨날 부족해서 재료 밀리는거 반복되니까 짜증나더라구..ㅜ",
          "original_form": "각설탕 맨날 부족해서 재료 밀리는거 반복되니까 짜증나더라구..ㅜ",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010825.1.19",
          "form": "아맞아 재료수급도 계속 밀리고 할게 너무 많은듯",
          "original_form": "아맞아 재료수급도 계속 밀리고 할게 너무 많은듯",
          "speaker_id": "1",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010825.1.20",
          "form": "좀 부담없이 하고싶은데",
          "original_form": "좀 부담없이 하고싶은데",
          "speaker_id": "1",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010825.1.21",
          "form": "그러니까.. 그리고 쿠키 새로 나오는건 확률 높아서 쉽게 뽑을 수 있는데",
          "original_form": "그러니까.. 그리고 쿠키 새로 나오는건 확률 높아서 쉽게 뽑을 수 있는데",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010825.1.22",
          "form": "시간 지나면 못 뽑으니까 뒤쳐지게 되는 점도 신규유저가 하기에는 좀 안좋은 부분 같아",
          "original_form": "시간 지나면 못 뽑으니까 뒤쳐지게 되는 점도 신규유저가 하기에는 좀 안좋은 부분 같아",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010825.1.23",
          "form": "아 맞아 신규유입이 좀 작고 점점 고인물화되는??",
          "original_form": "아 맞아 신규유입이 좀 작고 점점 고인물화되는??",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010825.1.24",
          "form": "이게 안좋은 징조라던데ㅋㅋㅋ",
          "original_form": "이게 안좋은 징조라던데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010825.1.25",
          "form": "데브가 벨런스조절 되게 못한다더니 진짠가봐",
          "original_form": "데브가 벨런스조절 되게 못한다더니 진짠가봐",
          "speaker_id": "1",
          "time": "20210823 22:38"
        }
      ]
