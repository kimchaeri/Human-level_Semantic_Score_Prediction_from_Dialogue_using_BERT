      "utterance": [
        {
          "id": "MDRW2100032007.1.1",
          "form": "점심 뭐드시나요",
          "original_form": "점심 뭐드시나요",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032007.1.2",
          "form": "연안식당 가려구요!.",
          "original_form": "연안식당 가려구요!.",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100032007.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100032007.1.4",
          "form": "닭가.슴살 도시락",
          "original_form": "닭가.슴살 도시락",
          "speaker_id": "2",
          "time": "20210905 10:55"
        },
        {
          "id": "MDRW2100032007.1.5",
          "form": "먹을까 고민중이에요",
          "original_form": "먹을까 고민중이에요",
          "speaker_id": "2",
          "time": "20210905 10:55"
        },
        {
          "id": "MDRW2100032007.1.6",
          "form": "아...정말 하나도 식욕 안 당기는 메뉴네요ㅜ",
          "original_form": "아...정말 하나도 식욕 안 당기는 메뉴네요ㅜ",
          "speaker_id": "1",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100032007.1.7",
          "form": "연안식당가서",
          "original_form": "연안식당가서",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032007.1.8",
          "form": "또 꼬막 보니까",
          "original_form": "또 꼬막 보니까",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032007.1.9",
          "form": "소주 일병하시겠어요",
          "original_form": "소주 일병하시겠어요",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032007.1.10",
          "form": "ㅎ..오늘은 일해야 해서 술과 내외합니다",
          "original_form": "ㅎ..오늘은 일해야 해서 술과 내외합니다",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100032007.1.11",
          "form": "님이 제몫까지",
          "original_form": "님이 제몫까지",
          "speaker_id": "1",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100032007.1.12",
          "form": "닭가슴과 소주를 드셔주시죠",
          "original_form": "닭가슴과 소주를 드셔주시죠",
          "speaker_id": "1",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100032007.1.13",
          "form": "오후에 먹을거라",
          "original_form": "오후에 먹을거라",
          "speaker_id": "2",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100032007.1.14",
          "form": "점심은 자제합니다",
          "original_form": "점심은 자제합니다",
          "speaker_id": "2",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100032007.1.15",
          "form": "그래서 오후에 고주망태가 되는 중이시군요!",
          "original_form": "그래서 오후에 고주망태가 되는 중이시군요!",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032007.1.16",
          "form": "아 정확하셨습니다",
          "original_form": "아 정확하셨습니다",
          "speaker_id": "2",
          "time": "20210906 08:38"
        },
        {
          "id": "MDRW2100032007.1.17",
          "form": "실신했어요",
          "original_form": "실신했어요",
          "speaker_id": "2",
          "time": "20210906 08:38"
        },
        {
          "id": "MDRW2100032007.1.18",
          "form": "아닠ㅋ 소주를 궤짝으로 드셨나요ㄷㄷ",
          "original_form": "아닠ㅋ 소주를 궤짝으로 드셨나요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210906 09:02"
        },
        {
          "id": "MDRW2100032007.1.19",
          "form": "세병인데ㅋㄲ",
          "original_form": "세병인데ㅋㄲ",
          "speaker_id": "2",
          "time": "20210906 09:18"
        },
        {
          "id": "MDRW2100032007.1.20",
          "form": "어우 피곤하다요",
          "original_form": "어우 피곤하다요",
          "speaker_id": "2",
          "time": "20210906 09:18"
        },
        {
          "id": "MDRW2100032007.1.21",
          "form": "ㄷㄷ 주량이 대체ㄷㄷ",
          "original_form": "ㄷㄷ 주량이 대체ㄷㄷ",
          "speaker_id": "1",
          "time": "20210906 09:20"
        },
        {
          "id": "MDRW2100032007.1.22",
          "form": "세병을 혼자 드셨나요?",
          "original_form": "세병을 혼자 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 09:20"
        },
        {
          "id": "MDRW2100032007.1.23",
          "form": "거기에 맥주도 섞여서ㅋㅋㄲ",
          "original_form": "거기에 맥주도 섞여서ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210906 09:24"
        },
        {
          "id": "MDRW2100032007.1.24",
          "form": "아 나이 생각해야되는데",
          "original_form": "아 나이 생각해야되는데",
          "speaker_id": "2",
          "time": "20210906 09:24"
        }
      ]
