      "utterance": [
        {
          "id": "MDRW2100017358.1.1",
          "form": "백신 맞으셨으면 오늘 푹 쉬셔야겠어요",
          "original_form": "백신 맞으셨으면 오늘 푹 쉬셔야겠어요",
          "speaker_id": "1",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100017358.1.2",
          "form": "건강 챙겨야죠",
          "original_form": "건강 챙겨야죠",
          "speaker_id": "1",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100017358.1.3",
          "form": "네 그래도 아직까지는 별 다른 증상이 없네요",
          "original_form": "네 그래도 아직까지는 별 다른 증상이 없네요",
          "speaker_id": "2",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017358.1.4",
          "form": "화이자 맞으신건가요?",
          "original_form": "화이자 맞으신건가요?",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017358.1.5",
          "form": "네 화이자 맞았어요",
          "original_form": "네 화이자 맞았어요",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017358.1.6",
          "form": "저도 화이자 맞았는데",
          "original_form": "저도 화이자 맞았는데",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017358.1.7",
          "form": "당일은 별 느낌 없다가 다음날 팔이 뻐근하더라고요",
          "original_form": "당일은 별 느낌 없다가 다음날 팔이 뻐근하더라고요",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017358.1.8",
          "form": "약간 손이 저린 증상이 있긴한데 괜찮았음 좋겠네요",
          "original_form": "약간 손이 저린 증상이 있긴한데 괜찮았음 좋겠네요",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017358.1.9",
          "form": "건강히 잘 넘어가시길 바라요",
          "original_form": "건강히 잘 넘어가시길 바라요",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017358.1.10",
          "form": "백신 접종 후 증상이 워낙 사람마다 달라서",
          "original_form": "백신 접종 후 증상이 워낙 사람마다 달라서",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017358.1.11",
          "form": "맞아요 항체 형성이나 잘 됐으면 좋겠어요 ㅋㅋ",
          "original_form": "맞아요 항체 형성이나 잘 됐으면 좋겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017358.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017358.1.13",
          "form": "저는 곧 2차 접종인데",
          "original_form": "저는 곧 2차 접종인데",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017358.1.14",
          "form": "화이자는 2차접종 후가 힘들대서 걱정이예요ㅜ",
          "original_form": "화이자는 2차접종 후가 힘들대서 걱정이예요ㅜ",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017358.1.15",
          "form": "그쵸 저도 2차가 두렵긴해요 ㅜㅜ",
          "original_form": "그쵸 저도 2차가 두렵긴해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017358.1.16",
          "form": "건강하려고 맞는건데 건강을 해치는 경우도 있대서ㅜ",
          "original_form": "건강하려고 맞는건데 건강을 해치는 경우도 있대서ㅜ",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017358.1.17",
          "form": "그래도 지금은 백신 맞는게 최선이니 ㅜ",
          "original_form": "그래도 지금은 백신 맞는게 최선이니 ㅜ",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017358.1.18",
          "form": "네네 코로나 걸릴 수는 없으니까요ㅜ",
          "original_form": "네네 코로나 걸릴 수는 없으니까요ㅜ",
          "speaker_id": "1",
          "time": "20210826 19:25"
        }
      ]
