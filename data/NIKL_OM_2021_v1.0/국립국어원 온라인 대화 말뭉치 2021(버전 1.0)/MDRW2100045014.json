        },
        {
          "id": "MDRW2100045014.1.2",
          "form": "잘 크고 있구만",
          "original_form": "잘 크고 있구만",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045014.1.3",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045014.1.4",
          "form": "쪼꼬미 들어왔을때",
          "original_form": "쪼꼬미 들어왔을때",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045014.1.5",
          "form": "name3가 다쳐서 예민할ㄹ대였는데",
          "original_form": "&name3&가 다쳐서 예민할ㄹ대였는데",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045014.1.6",
          "form": "이제는 무던한가봐",
          "original_form": "이제는 무던한가봐",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045014.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045014.1.8",
          "form": "ㄱㅇㅇ",
          "original_form": "ㄱㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045014.1.9",
          "form": "애기 전담으로 놀아주는 전담냥ㅇ도생기고",
          "original_form": "애기 전담으로 놀아주는 전담냥ㅇ도생기고",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045014.1.10",
          "form": "애들 나중에 마당으로 풀어놓으시려 할텐데",
          "original_form": "애들 나중에 마당으로 풀어놓으시려 할텐데",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045014.1.11",
          "form": "꼭 코요테 울타리 설치하시길...",
          "original_form": "꼭 코요테 울타리 설치하시길...",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045014.1.12",
          "form": "애들 자꾸 공격당해서 ㅠㅠㅠ",
          "original_form": "애들 자꾸 공격당해서 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045014.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045014.1.14",
          "form": "코요테요?",
          "original_form": "코요테요?",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045014.1.15",
          "form": "우리나라에??",
          "original_form": "우리나라에??",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045014.1.16",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045014.1.17",
          "form": "아 그게",
          "original_form": "아 그게",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045014.1.18",
          "form": "진짜 코요태 울타리가아니라",
          "original_form": "진짜 코요태 울타리가아니라",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045014.1.19",
          "form": "이름이그건데",
          "original_form": "이름이그건데",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045014.1.20",
          "form": "다른 짐승들 못들어오고 애들 못나가게막는 울타리",
          "original_form": "다른 짐승들 못들어오고 애들 못나가게막는 울타리",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045014.1.21",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045014.1.22",
          "form": "시골은 들개도 많고 그러니까..",
          "original_form": "시골은 들개도 많고 그러니까..",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045014.1.23",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.24",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.25",
          "form": "하하하네만 봐도",
          "original_form": "하하하네만 봐도",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.26",
          "form": "너구리도 있고..",
          "original_form": "너구리도 있고..",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.27",
          "form": "그 예전에하하하",
          "original_form": "그 예전에하하하",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.28",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.29",
          "form": "어미냥이",
          "original_form": "어미냥이",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.30",
          "form": "들개한테 물려죽은거아닐까",
          "original_form": "들개한테 물려죽은거아닐까",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.31",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.32",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045014.1.33",
          "form": "어미냥이 나중에서야",
          "original_form": "어미냥이 나중에서야",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045014.1.34",
          "form": "배에 다친흔적있다그래서",
          "original_form": "배에 다친흔적있다그래서",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045014.1.35",
          "form": "혹시나",
          "original_form": "혹시나",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045014.1.36",
          "form": "ㅇㅇㅇㅇ....",
          "original_form": "ㅇㅇㅇㅇ....",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045014.1.37",
          "form": "집 뒤에서 발견된거 너무 슬퍼...",
          "original_form": "집 뒤에서 발견된거 너무 슬퍼...",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045014.1.38",
          "form": "마자 ㅠㅠㅠ",
          "original_form": "마자 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045014.1.39",
          "form": "들개나 너구리나 멧돼지.. 뭐 그런 애들이겠지",
          "original_form": "들개나 너구리나 멧돼지.. 뭐 그런 애들이겠지",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045014.1.40",
          "form": "원래고양이들이 죽을때되면",
          "original_form": "원래고양이들이 죽을때되면",
