      "utterance": [
        {
          "id": "MDRW2100001791.1.1",
          "form": "동아리 뭐 한적 있으세요?",
          "original_form": "동아리 뭐 한적 있으세요?",
          "speaker_id": "1",
          "time": "20210819 21:16"
        },
        {
          "id": "MDRW2100001791.1.2",
          "form": "오 저는 리코더부 한 적 있어요ㅎㅎ",
          "original_form": "오 저는 리코더부 한 적 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001791.1.3",
          "form": "name1님은 어떤 동이라 히셨었어요?",
          "original_form": "&name1&님은 어떤 동이라 히셨었어요?",
          "speaker_id": "2",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001791.1.4",
          "form": "저는 그냥 단순히 모두 하는",
          "original_form": "저는 그냥 단순히 모두 하는",
          "speaker_id": "1",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100001791.1.5",
          "form": "십자수부 아니면 영화감상부 뭐 이런거요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "십자수부 아니면 영화감상부 뭐 이런거요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100001791.1.6",
          "form": "우와 십자수부 신기한데요?",
          "original_form": "우와 십자수부 신기한데요?",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001791.1.7",
          "form": "영화감상부는 다들 경쟁률이 엄청난 동아리 아니었나요..ㅋㅋㅋ",
          "original_form": "영화감상부는 다들 경쟁률이 엄청난 동아리 아니었나요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001791.1.8",
          "form": "맞아요 영화만 보니까 짱이었죠",
          "original_form": "맞아요 영화만 보니까 짱이었죠",
          "speaker_id": "1",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001791.1.9",
          "form": "리코더부면 리코더만 계속 불어요?",
          "original_form": "리코더부면 리코더만 계속 불어요?",
          "speaker_id": "1",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001791.1.10",
          "form": "어 네 거의 그랬던 거 같아요",
          "original_form": "어 네 거의 그랬던 거 같아요",
          "speaker_id": "2",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001791.1.11",
          "form": "리코더 종류가 많아서 크기별로 5개로 나눴었어요!",
          "original_form": "리코더 종류가 많아서 크기별로 5개로 나눴었어요!",
          "speaker_id": "2",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001791.1.12",
          "form": "와.. 리코더 종류가 여러개라니",
          "original_form": "와.. 리코더 종류가 여러개라니",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001791.1.13",
          "form": "전 여태 하난 줄 알았어요",
          "original_form": "전 여태 하난 줄 알았어요",
          "speaker_id": "1",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001791.1.14",
          "form": "저도 알고싶지 않았는데.. 알게됐어요",
          "original_form": "저도 알고싶지 않았는데.. 알게됐어요",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001791.1.15",
          "form": "합창처럼 소프라노 알토 테너 베이스 이렇게 있어요ㅎㅎ보통 쓰는 리코더가 소프라노인데 그거보다 작은 것도 있어요!",
          "original_form": "합창처럼 소프라노 알토 테너 베이스 이렇게 있어요ㅎㅎ보통 쓰는 리코더가 소프라노인데 그거보다 작은 것도 있어요!",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001791.1.16",
          "form": "우와 진짜 깊숙하게 파고들면 많은게 있다는 걸 알게되네요!!!!",
          "original_form": "우와 진짜 깊숙하게 파고들면 많은게 있다는 걸 알게되네요!!!!",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001791.1.17",
          "form": "진짜 너무 싱기해여",
          "original_form": "진짜 너무 싱기해여",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001791.1.18",
          "form": "그쵸 저도 처음에 신세계였어요",
          "original_form": "그쵸 저도 처음에 신세계였어요",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001791.1.19",
          "form": "알토랑 베이스는 연주할 때 손가락 짚는 법도 다르더라구요",
          "original_form": "알토랑 베이스는 연주할 때 손가락 짚는 법도 다르더라구요",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001791.1.20",
          "form": "리코더 안에 소프라노 이런 음역대가 있는게 신기해요 오ㅓ우...",
          "original_form": "리코더 안에 소프라노 이런 음역대가 있는게 신기해요 오ㅓ우...",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001791.1.21",
          "form": "저도 우와 이런게 있다고?하면서 너무 신기했어요",
          "original_form": "저도 우와 이런게 있다고?하면서 너무 신기했어요",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001791.1.22",
          "form": "소프라노보다 작은건 보통 리코더 반정도 되거든요",
          "original_form": "소프라노보다 작은건 보통 리코더 반정도 되거든요",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001791.1.23",
          "form": "거기 손가락이 다 들어가는게 신기했어요ㅋㅋㅋ",
          "original_form": "거기 손가락이 다 들어가는게 신기했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001791.1.24",
          "form": "리코더에 대해 찾아보게 되네요",
