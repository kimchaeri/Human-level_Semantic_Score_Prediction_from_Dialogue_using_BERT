      "utterance": [
        {
          "id": "MDRW2100008464.1.1",
          "form": "음 반려동물 키우고싶음?",
          "original_form": "음 반려동물 키우고싶음?",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008464.1.2",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008464.1.3",
          "form": "귀여운생물",
          "original_form": "귀여운생물",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008464.1.4",
          "form": "좋아는하는데",
          "original_form": "좋아는하는데",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008464.1.5",
          "form": "잘 키울수있을런지 왠지",
          "original_form": "잘 키울수있을런지 왠지",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008464.1.6",
          "form": "자신없어",
          "original_form": "자신없어",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008464.1.7",
          "form": "강아지와 고양이중 고른다면?",
          "original_form": "강아지와 고양이중 고른다면?",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008464.1.8",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008464.1.9",
          "form": "너무 고민되네",
          "original_form": "너무 고민되네",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008464.1.10",
          "form": "둘다 좋지만",
          "original_form": "둘다 좋지만",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008464.1.11",
          "form": "내가 길러야한다! 란 상황이면",
          "original_form": "내가 길러야한다! 란 상황이면",
          "speaker_id": "1",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008464.1.12",
          "form": "나는 외출을 귀찮아해서 고양이가 맞을거같아",
          "original_form": "나는 외출을 귀찮아해서 고양이가 맞을거같아",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008464.1.13",
          "form": "근데 난 사실 뜬금없지만 마리모 길러보고픔",
          "original_form": "근데 난 사실 뜬금없지만 마리모 길러보고픔",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008464.1.14",
          "form": "마리모는 당장이라도 가능하지않아?",
          "original_form": "마리모는 당장이라도 가능하지않아?",
          "speaker_id": "2",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008464.1.15",
          "form": "아니 내방 너무좁아 ㅋㅋ",
          "original_form": "아니 내방 너무좁아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008464.1.16",
          "form": "독립하면 몰라도",
          "original_form": "독립하면 몰라도",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008464.1.17",
          "form": "그리고 애완동물이라는 말어때서",
          "original_form": "그리고 애완동물이라는 말어때서",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008464.1.18",
          "form": "왜자꾸반려동물이라고 강요하나싶음",
          "original_form": "왜자꾸반려동물이라고 강요하나싶음",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008464.1.19",
          "form": "마리모 작은거 키우면대지",
          "original_form": "마리모 작은거 키우면대지",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008464.1.20",
          "form": "마리모는 빨리 커지지도 않잖아",
          "original_form": "마리모는 빨리 커지지도 않잖아",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008464.1.21",
          "form": "은근 세심한 케어가 필요하대여",
          "original_form": "은근 세심한 케어가 필요하대여",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008464.1.22",
          "form": "죽이면 우짜지 싶기도하고 ㅋㅋ",
          "original_form": "죽이면 우짜지 싶기도하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008464.1.23",
          "form": "안키워봐서 몰랏네..",
          "original_form": "안키워봐서 몰랏네..",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008464.1.24",
          "form": "식물은 어때",
          "original_form": "식물은 어때",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008464.1.25",
          "form": "나도 그냥 여유되면 미래에 키워보고시퍼 정도 ㄱㅋ",
          "original_form": "나도 그냥 여유되면 미래에 키워보고시퍼 정도 ㄱㅋ",
          "speaker_id": "1",
          "time": "20210822 16:17"
        }
      ]
    }
