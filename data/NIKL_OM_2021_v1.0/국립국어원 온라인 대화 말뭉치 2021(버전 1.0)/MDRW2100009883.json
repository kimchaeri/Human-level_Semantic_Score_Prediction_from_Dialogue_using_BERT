      "utterance": [
        {
          "id": "MDRW2100009883.1.1",
          "form": "name2님 정모나감?",
          "original_form": "&name2&님 정모나감?",
          "speaker_id": "1",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009883.1.2",
          "form": "나는 겜정모가봄",
          "original_form": "나는 겜정모가봄",
          "speaker_id": "1",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009883.1.3",
          "form": "흔히 번개라는 건 안해봄",
          "original_form": "흔히 번개라는 건 안해봄",
          "speaker_id": "2",
          "time": "20210823 11:16"
        },
        {
          "id": "MDRW2100009883.1.4",
          "form": "좀 겁도 난달까 ㅋㅋ",
          "original_form": "좀 겁도 난달까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:16"
        },
        {
          "id": "MDRW2100009883.1.5",
          "form": "인상이 다르잖아 온 오프가",
          "original_form": "인상이 다르잖아 온 오프가",
          "speaker_id": "1",
          "time": "20210823 11:18"
        },
        {
          "id": "MDRW2100009883.1.6",
          "form": "그게 두려워",
          "original_form": "그게 두려워",
          "speaker_id": "1",
          "time": "20210823 11:18"
        },
        {
          "id": "MDRW2100009883.1.7",
          "form": "나도 그렇게 모임했다가 범죄로 이어졌다고하는 경우도 간간히 기사뜨니까",
          "original_form": "나도 그렇게 모임했다가 범죄로 이어졌다고하는 경우도 간간히 기사뜨니까",
          "speaker_id": "2",
          "time": "20210823 11:20"
        },
        {
          "id": "MDRW2100009883.1.8",
          "form": "몸사리게됨",
          "original_form": "몸사리게됨",
          "speaker_id": "2",
          "time": "20210823 11:20"
        },
        {
          "id": "MDRW2100009883.1.9",
          "form": "나는 진짜진짜 친해졌거나",
          "original_form": "나는 진짜진짜 친해졌거나",
          "speaker_id": "1",
          "time": "20210823 11:21"
        },
        {
          "id": "MDRW2100009883.1.10",
          "form": "원래아는사람아님 안감",
          "original_form": "원래아는사람아님 안감",
          "speaker_id": "1",
          "time": "20210823 11:21"
        },
        {
          "id": "MDRW2100009883.1.11",
          "form": "온라인에서 진짜진짜 친해진다는거가 알기어려움 여자인척 하는 사람들도 있다고하곡",
          "original_form": "온라인에서 진짜진짜 친해진다는거가 알기어려움 여자인척 하는 사람들도 있다고하곡",
          "speaker_id": "2",
          "time": "20210823 11:22"
        },
        {
          "id": "MDRW2100009883.1.12",
          "form": "만나면 사람인데",
          "original_form": "만나면 사람인데",
          "speaker_id": "1",
          "time": "20210823 11:23"
        },
        {
          "id": "MDRW2100009883.1.13",
          "form": "온라인은 속내가 드러나게되는곳이라",
          "original_form": "온라인은 속내가 드러나게되는곳이라",
          "speaker_id": "1",
          "time": "20210823 11:23"
        },
        {
          "id": "MDRW2100009883.1.14",
          "form": "그런가봐",
          "original_form": "그런가봐",
          "speaker_id": "1",
          "time": "20210823 11:23"
        },
        {
          "id": "MDRW2100009883.1.15",
          "form": "그런 의미에서 번개 모임 나가는 사람 대단함",
          "original_form": "그런 의미에서 번개 모임 나가는 사람 대단함",
          "speaker_id": "2",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009883.1.16",
          "form": "나는 음 나가도",
          "original_form": "나는 음 나가도",
          "speaker_id": "2",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009883.1.17",
          "form": "예를들어 언니랑 둘이 같이 간다던가 그럼 덜 무서우려나",
          "original_form": "예를들어 언니랑 둘이 같이 간다던가 그럼 덜 무서우려나",
          "speaker_id": "2",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009883.1.18",
          "form": "근데 홈트 동아리 이런거는 무난하지않아?",
          "original_form": "근데 홈트 동아리 이런거는 무난하지않아?",
          "speaker_id": "1",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009883.1.19",
          "form": "그럼든든하제",
          "original_form": "그럼든든하제",
          "speaker_id": "1",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009883.1.20",
          "form": "개인적으로 독서 동아리나",
          "original_form": "개인적으로 독서 동아리나",
          "speaker_id": "2",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009883.1.21",
          "form": "언어 배우는동아리",
          "original_form": "언어 배우는동아리",
          "speaker_id": "2",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009883.1.22",
          "form": "같은건 해보고싶기도",
          "original_form": "같은건 해보고싶기도",
          "speaker_id": "2",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009883.1.23",
          "form": "name2님 스터디했지않은가",
          "original_form": "&name2&님 스터디했지않은가",
          "speaker_id": "1",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009883.1.24",
          "form": "일종의 동아리지",
          "original_form": "일종의 동아리지",
          "speaker_id": "1",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009883.1.25",
          "form": "그건 온라인 스터디니까",
          "original_form": "그건 온라인 스터디니까",
          "speaker_id": "2",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009883.1.26",
          "form": "또 좀 다른것같기도",
          "original_form": "또 좀 다른것같기도",
          "speaker_id": "2",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009883.1.27",
          "form": "아예 얼굴볼 예정이없는",
          "original_form": "아예 얼굴볼 예정이없는",
          "speaker_id": "2",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009883.1.28",
          "form": "목적이 명료하면",
          "original_form": "목적이 명료하면",
          "speaker_id": "1",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009883.1.29",
          "form": "오프라도 편하지않을까나",
          "original_form": "오프라도 편하지않을까나",
          "speaker_id": "1",
          "time": "20210823 11:28"
        }
      ]
    }
