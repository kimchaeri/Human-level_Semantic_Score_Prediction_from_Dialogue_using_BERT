      "utterance": [
        {
          "id": "MDRW2100042553.1.1",
          "form": "그럼 난 햄버거 줄게",
          "original_form": "그럼 난 햄버거 줄게",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042553.1.2",
          "form": "너 먹어",
          "original_form": "너 먹어",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042553.1.3",
          "form": "이거 너무 밑지는 장사 아녀",
          "original_form": "이거 너무 밑지는 장사 아녀",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042553.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 우정리가 생각나지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 우정리가 생각나지",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042553.1.5",
          "form": "아니어이가없네",
          "original_form": "아니어이가없네",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042553.1.6",
          "form": "햄버거를 왜줘",
          "original_form": "햄버거를 왜줘",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042553.1.7",
          "form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋ내 사랑이야",
          "original_form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋ내 사랑이야",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042553.1.8",
          "form": "햄버거도 먹어야 살찌지",
          "original_form": "햄버거도 먹어야 살찌지",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042553.1.9",
          "form": "받아줘",
          "original_form": "받아줘",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042553.1.10",
          "form": "근데 난 음식에 철칙이 있어",
          "original_form": "근데 난 음식에 철칙이 있어",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042553.1.11",
          "form": "참았다가 먹으면",
          "original_form": "참았다가 먹으면",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042553.1.12",
          "form": "두 배로 맛있다",
          "original_form": "두 배로 맛있다",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042553.1.13",
          "form": "최상의 맛을 위해",
          "original_form": "최상의 맛을 위해",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042553.1.14",
          "form": "진짜 모르겠네",
          "original_form": "진짜 모르겠네",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042553.1.15",
          "form": "무슨 방식이여",
          "original_form": "무슨 방식이여",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042553.1.16",
          "form": "그때그때 맛있게먹고 살어",
          "original_form": "그때그때 맛있게먹고 살어",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042553.1.17",
          "form": "버텼다가 못참을 때 먹는 거야",
          "original_form": "버텼다가 못참을 때 먹는 거야",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042553.1.18",
          "form": "맨날 먹으면 맛 없잖아",
          "original_form": "맨날 먹으면 맛 없잖아",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042553.1.19",
          "form": "평소에 마시는 물 한 컵과",
          "original_form": "평소에 마시는 물 한 컵과",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042553.1.20",
          "form": "운동하고 나서 마시는 물 한컵이 다른 이치지",
          "original_form": "운동하고 나서 마시는 물 한컵이 다른 이치지",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042553.1.21",
          "form": "목 마를수록 맛있다",
          "original_form": "목 마를수록 맛있다",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042553.1.22",
          "form": "거참 웃기는도다",
          "original_form": "거참 웃기는도다",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042553.1.23",
          "form": "나 지금 포카칩도 참고 있어",
          "original_form": "나 지금 포카칩도 참고 있어",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042553.1.24",
          "form": "내가 사줄테니 그냥 먹어라",
          "original_form": "내가 사줄테니 그냥 먹어라",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042553.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042553.1.26",
          "form": "졸지에 가엾은 사람이 된 것 같아",
          "original_form": "졸지에 가엾은 사람이 된 것 같아",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042553.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ넘 웃기다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ넘 웃기다",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042553.1.28",
          "form": "넌 포카칩 좋아하니",
          "original_form": "넌 포카칩 좋아하니",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042553.1.29",
          "form": "응 엄청 좋아해",
          "original_form": "응 엄청 좋아해",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042553.1.30",
          "form": "칼로리가 짱나지만",
          "original_form": "칼로리가 짱나지만",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042553.1.31",
          "form": "오리지널이랑 양파 중에 뭐가 좋아",
          "original_form": "오리지널이랑 양파 중에 뭐가 좋아",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042553.1.32",
          "form": "난 양파",
          "original_form": "난 양파",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042553.1.33",
          "form": "나 오리",
          "original_form": "나 오리",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042553.1.34",
          "form": "꽥꽥",
          "original_form": "꽥꽥",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042553.1.35",
          "form": "이럴수가 오리지널파라니",
          "original_form": "이럴수가 오리지널파라니",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042553.1.36",
          "form": "파랑포카칩의 어디가 좋아",
          "original_form": "파랑포카칩의 어디가 좋아",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042553.1.37",
          "form": "아무 양념맛이 없잖아!",
          "original_form": "아무 양념맛이 없잖아!",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042553.1.38",
          "form": "담백그자체",
          "original_form": "담백그자체",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042553.1.39",
          "form": "그 고소함이 좋아",
          "original_form": "그 고소함이 좋아",
          "speaker_id": "2",
          "time": "20210906 22:56"
        }
      ]
    }
  ]
