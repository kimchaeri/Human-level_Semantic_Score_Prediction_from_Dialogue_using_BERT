      "utterance": [
        {
          "id": "MDRW2100008519.1.1",
          "form": "일해여?",
          "original_form": "일해여?",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008519.1.2",
          "form": "자격증 공부해요",
          "original_form": "자격증 공부해요",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008519.1.3",
          "form": "취업할려고요!",
          "original_form": "취업할려고요!",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008519.1.4",
          "form": "자격증 요즘 필수ㅈㅎ?",
          "original_form": "자격증 요즘 필수ㅈㅎ?",
          "speaker_id": "1",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008519.1.5",
          "form": "죠?",
          "original_form": "죠?",
          "speaker_id": "1",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008519.1.6",
          "form": "그럼요 종류도 참 다양하고 이력서에 줄 하나하나가 중요하니까요",
          "original_form": "그럼요 종류도 참 다양하고 이력서에 줄 하나하나가 중요하니까요",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008519.1.7",
          "form": "name1님은 취업하셨나요?",
          "original_form": "&name1&님은 취업하셨나요?",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008519.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008519.1.9",
          "form": "부러워요 일은 적성에 맞으시나요?",
          "original_form": "부러워요 일은 적성에 맞으시나요?",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008519.1.10",
          "form": "아니요 그맘하고 싶다요",
          "original_form": "아니요 그맘하고 싶다요",
          "speaker_id": "1",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100008519.1.11",
          "form": "업무 스트레스가 있으신가봐요",
          "original_form": "업무 스트레스가 있으신가봐요",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008519.1.12",
          "form": "네 사람때문에",
          "original_form": "네 사람때문에",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008519.1.13",
          "form": "직접 겪어본 건 아니지만 먼저 취업한 동생 통해서 들은 건 있어요",
          "original_form": "직접 겪어본 건 아니지만 먼저 취업한 동생 통해서 들은 건 있어요",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008519.1.14",
          "form": "사람 대하는 게 쉽진 않죠 ㅠㅠ",
          "original_form": "사람 대하는 게 쉽진 않죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008519.1.15",
          "form": "다양해사",
          "original_form": "다양해사",
          "speaker_id": "1",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008519.1.16",
          "form": "동생은 담배를 안피는데 상사가 자꾸 담배 피는데 대려가서 힘들어 했죠",
          "original_form": "동생은 담배를 안피는데 상사가 자꾸 담배 피는데 대려가서 힘들어 했죠",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008519.1.17",
          "form": "특히 그 땐 신입 때라 뱔 말도 못하고 따라다녀서 간접 흡연을..",
          "original_form": "특히 그 땐 신입 때라 뱔 말도 못하고 따라다녀서 간접 흡연을..",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
