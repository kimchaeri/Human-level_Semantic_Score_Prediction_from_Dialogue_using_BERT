        {
          "id": "MDRW2100020381.1.1",
          "form": "왜 광화문연가 안보신다고했죠?",
          "original_form": "왜 광화문연가 안보신다고했죠?",
          "speaker_id": "2",
          "time": "20210828 02:32"
        },
        {
          "id": "MDRW2100020381.1.2",
          "form": "너무 귀찮아서..",
          "original_form": "너무 귀찮아서..",
          "speaker_id": "1",
          "time": "20210828 02:32"
        },
        {
          "id": "MDRW2100020381.1.3",
          "form": "아 공연장이 너무 멀다 하셨죠 ㅎㅎ",
          "original_form": "아 공연장이 너무 멀다 하셨죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 02:32"
        },
        {
          "id": "MDRW2100020381.1.4",
          "form": "네ㅜㅜ 그게 유일한 흠 ㅜㅜ",
          "original_form": "네ㅜㅜ 그게 유일한 흠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 02:32"
        },
        {
          "id": "MDRW2100020381.1.5",
          "form": "얼마나 걸린다셨죠?",
          "original_form": "얼마나 걸린다셨죠?",
          "speaker_id": "2",
          "time": "20210828 02:32"
        },
        {
          "id": "MDRW2100020381.1.6",
          "form": "1시간 반정도요ㅠㅠㅠ",
          "original_form": "1시간 반정도요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020381.1.7",
          "form": "왕복 3시간이네요",
          "original_form": "왕복 3시간이네요",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020381.1.8",
          "form": "네ㅠㅠㅠ 근데 이대로 보내면 아쉽긴 하고..",
          "original_form": "네ㅠㅠㅠ 근데 이대로 보내면 아쉽긴 하고..",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020381.1.9",
          "form": "딜레마군여",
          "original_form": "딜레마군여",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020381.1.10",
          "form": "네ㅜㅜ 완전 고민..",
          "original_form": "네ㅜㅜ 완전 고민..",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020381.1.11",
          "form": "혼자 가세요?",
          "original_form": "혼자 가세요?",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020381.1.12",
          "form": "네 그래서 더 지루할 것 같은.",
          "original_form": "네 그래서 더 지루할 것 같은.",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020381.1.13",
          "form": "같이 가고싶네요 ㅎㅎ",
          "original_form": "같이 가고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 02:34"
        },
        {
          "id": "MDRW2100020381.1.14",
          "form": "그러게요 ㅠㅠ",
