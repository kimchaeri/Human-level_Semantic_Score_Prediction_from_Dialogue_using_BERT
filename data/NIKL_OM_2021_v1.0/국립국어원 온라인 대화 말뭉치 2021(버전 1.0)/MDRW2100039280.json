      "utterance": [
        {
          "id": "MDRW2100039280.1.1",
          "form": "퇴근하고 싶다",
          "original_form": "퇴근하고 싶다",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039280.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아주 적절한 주제네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아주 적절한 주제네",
          "speaker_id": "2",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100039280.1.3",
          "form": "나는 퇴근했지런",
          "original_form": "나는 퇴근했지런",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039280.1.4",
          "form": "렁",
          "original_form": "렁",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039280.1.5",
          "form": "지금 공가내고 백신 2차 접종을 맞았음",
          "original_form": "지금 공가내고 백신 2차 접종을 맞았음",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039280.1.6",
          "form": "원래 1시 30분에 나가야하는데",
          "original_form": "원래 1시 30분에 나가야하는데",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039280.1.7",
          "form": "미적거리다가 2시 30분에 나갔는데",
          "original_form": "미적거리다가 2시 30분에 나갔는데",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039280.1.8",
          "form": "고 사이에 일이 생겨버리더라고",
          "original_form": "고 사이에 일이 생겨버리더라고",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039280.1.9",
          "form": "빨리 퇴근할껄...일이 다시 생기는 구나",
          "original_form": "빨리 퇴근할껄...일이 다시 생기는 구나",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039280.1.10",
          "form": "했음",
          "original_form": "했음",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039280.1.11",
          "form": "백신휴가는 며칠줘?",
          "original_form": "백신휴가는 며칠줘?",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100039280.1.12",
          "form": "나는 2차 맞은 다음날만 쉰듯",
          "original_form": "나는 2차 맞은 다음날만 쉰듯",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100039280.1.13",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039280.1.14",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039280.1.15",
          "form": "그냥 심하게 아프면 병가내야지 뭐...",
          "original_form": "그냥 심하게 아프면 병가내야지 뭐...",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039280.1.16",
          "form": "학기 중이라 쉬세요~",
          "original_form": "학기 중이라 쉬세요~",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039280.1.17",
          "form": "쉬세요~ 이런 거 없음",
          "original_form": "쉬세요~ 이런 거 없음",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039280.1.18",
          "form": "그냥 눈치껏 쉬는 거지 뭐",
          "original_form": "그냥 눈치껏 쉬는 거지 뭐",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039280.1.19",
          "form": "우리도 병가야 ㅋㅋ",
          "original_form": "우리도 병가야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039280.1.20",
          "form": "증상 있으면 바로 조퇴내거나 가야지",
          "original_form": "증상 있으면 바로 조퇴내거나 가야지",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039280.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039280.1.22",
          "form": "따로 나오진 않는다",
          "original_form": "따로 나오진 않는다",
          "speaker_id": "1",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039280.1.23",
          "form": "근데 나는 병가 쫌쫌따리 많이 써서 이제 없어",
          "original_form": "근데 나는 병가 쫌쫌따리 많이 써서 이제 없어",
          "speaker_id": "1",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039280.1.24",
          "form": "후아ㅠ 뭐셔 벌써 없나",
          "original_form": "후아ㅠ 뭐셔 벌써 없나",
          "speaker_id": "2",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100039280.1.25",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100039280.1.26",
          "form": "나는 한달 병가내서",
          "original_form": "나는 한달 병가내서",
          "speaker_id": "2",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100039280.1.27",
          "form": "아직 남아있긴한데",
          "original_form": "아직 남아있긴한데",
          "speaker_id": "2",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100039280.1.28",
          "form": "무조건 의사소견서 쓰고 써야 함",
          "original_form": "무조건 의사소견서 쓰고 써야 함",
          "speaker_id": "2",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100039280.1.29",
          "form": "2학기때는 병가 안 냈으면 좋겠댱",
          "original_form": "2학기때는 병가 안 냈으면 좋겠댱",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039280.1.30",
          "form": "6일 이상의 병가는",
          "original_form": "6일 이상의 병가는",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039280.1.31",
          "form": "진단서가 필요해",
          "original_form": "진단서가 필요해",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039280.1.32",
          "form": "근데 진단서 2만원이잖아",
          "original_form": "근데 진단서 2만원이잖아",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039280.1.33",
          "form": "응응 1만원 이상 하지",
          "original_form": "응응 1만원 이상 하지",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039280.1.34",
          "form": "마쟈",
          "original_form": "마쟈",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039280.1.35",
          "form": "아 그리고 넌 코로나2차",
          "original_form": "아 그리고 넌 코로나2차",
          "speaker_id": "2",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039280.1.36",
          "form": "괜찮았니",
          "original_form": "괜찮았니",
          "speaker_id": "2",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039280.1.37",
          "form": "심장 아프거나 그런 거 없었낭",
          "original_form": "심장 아프거나 그런 거 없었낭",
          "speaker_id": "2",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039280.1.38",
          "form": "나는 그냥 보통 다른 사람들이랑 비슷하게 지나갔어",
          "original_form": "나는 그냥 보통 다른 사람들이랑 비슷하게 지나갔어",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100039280.1.39",
          "form": "퇴근하고싶다",
          "original_form": "퇴근하고싶다",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100039280.1.40",
          "form": "고렇군",
          "original_form": "고렇군",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100039280.1.41",
          "form": "다행이고만!",
          "original_form": "다행이고만!",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100039280.1.42",
          "form": "힘내! 6시 퇴근인가?",
          "original_form": "힘내! 6시 퇴근인가?",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100039280.1.43",
          "form": "아니면 6시 30분?",
          "original_form": "아니면 6시 30분?",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100039280.1.44",
          "form": "6시 퇴근",
          "original_form": "6시 퇴근",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100039280.1.45",
          "form": "나 월요일에 연차야",
          "original_form": "나 월요일에 연차야",
          "speaker_id": "1",
          "time": "20210903 16:48"
