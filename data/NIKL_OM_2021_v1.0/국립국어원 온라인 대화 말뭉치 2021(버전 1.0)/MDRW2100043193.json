      "utterance": [
        {
          "id": "MDRW2100043193.1.1",
          "form": "비오면 그렇게하고싶어",
          "original_form": "비오면 그렇게하고싶어",
          "speaker_id": "2",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100043193.1.2",
          "form": "홈트하려고",
          "original_form": "홈트하려고",
          "speaker_id": "1",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100043193.1.3",
          "form": "오 칼소폭?",
          "original_form": "오 칼소폭?",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043193.1.4",
          "form": "헬스 3개월해놓고 많이못갔네..힝",
          "original_form": "헬스 3개월해놓고 많이못갔네..힝",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043193.1.5",
          "form": "헬스비 아깝긴하다",
          "original_form": "헬스비 아깝긴하다",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043193.1.6",
          "form": "어어 그래서 이거끝나면쉴까봐",
          "original_form": "어어 그래서 이거끝나면쉴까봐",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043193.1.7",
          "form": "근데 어쩔수없지",
          "original_form": "근데 어쩔수없지",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043193.1.8",
          "form": "ㅠㅠ글루코사민이라도...",
          "original_form": "ㅠㅠ글루코사민이라도...",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043193.1.9",
          "form": "영양제살때같이사",
          "original_form": "영양제살때같이사",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043193.1.10",
          "form": "칼슘..",
          "original_form": "칼슘..",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043193.1.11",
          "form": "글루코사민이 칼슘이야?",
          "original_form": "글루코사민이 칼슘이야?",
          "speaker_id": "1",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043193.1.12",
          "form": "아니 연골에 조은 머 그런거아닌가?",
          "original_form": "아니 연골에 조은 머 그런거아닌가?",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043193.1.13",
          "form": "관절약",
          "original_form": "관절약",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043193.1.14",
          "form": "종비도잘안먹는데.",
          "original_form": "종비도잘안먹는데.",
          "speaker_id": "1",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043193.1.15",
          "form": "그럼 종비라도 먹자...",
          "original_form": "그럼 종비라도 먹자...",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043193.1.16",
          "form": "재난지원금받은걸로사려고",
          "original_form": "재난지원금받은걸로사려고",
          "speaker_id": "1",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043193.1.17",
          "form": "나도 콜레스테롤 낮추는거 먹고있음",
          "original_form": "나도 콜레스테롤 낮추는거 먹고있음",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043193.1.18",
          "form": "처방약?",
          "original_form": "처방약?",
          "speaker_id": "1",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043193.1.19",
          "form": "그린 어쩌고...약간 마음의 진정효과가있어",
          "original_form": "그린 어쩌고...약간 마음의 진정효과가있어",
