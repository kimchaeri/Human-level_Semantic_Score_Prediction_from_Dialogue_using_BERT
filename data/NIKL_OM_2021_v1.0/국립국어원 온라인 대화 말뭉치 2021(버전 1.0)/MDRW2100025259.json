      "utterance": [
        {
          "id": "MDRW2100025259.1.1",
          "form": "아 이건",
          "original_form": "아 이건",
          "speaker_id": "2",
          "time": "20210901 03:24"
        },
        {
          "id": "MDRW2100025259.1.2",
          "form": "갈비로 가겠습니다",
          "original_form": "갈비로 가겠습니다",
          "speaker_id": "2",
          "time": "20210901 03:24"
        },
        {
          "id": "MDRW2100025259.1.3",
          "form": "약간 흔들리셨나요",
          "original_form": "약간 흔들리셨나요",
          "speaker_id": "1",
          "time": "20210901 03:24"
        },
        {
          "id": "MDRW2100025259.1.4",
          "form": "둘 다 맛있을거 같은데",
          "original_form": "둘 다 맛있을거 같은데",
          "speaker_id": "2",
          "time": "20210901 03:25"
        },
        {
          "id": "MDRW2100025259.1.5",
          "form": "둘다너무맛있죠 ㅋㅋㅋㅋ",
          "original_form": "둘다너무맛있죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:25"
        },
        {
          "id": "MDRW2100025259.1.6",
          "form": "전 LA갈비쟁이기 때문에",
          "original_form": "전 LA갈비쟁이기 때문에",
          "speaker_id": "2",
          "time": "20210901 03:25"
        },
        {
          "id": "MDRW2100025259.1.7",
          "form": "둘다 좋은데 약간 끌리는 때가 다른거같아요 ㅋㅋㅋ",
          "original_form": "둘다 좋은데 약간 끌리는 때가 다른거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:25"
        },
        {
          "id": "MDRW2100025259.1.8",
          "form": "사촌인 갈비찜으로",
          "original_form": "사촌인 갈비찜으로",
          "speaker_id": "2",
          "time": "20210901 03:25"
        },
        {
          "id": "MDRW2100025259.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 전 굳이고른다면 해물찜이요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 전 굳이고른다면 해물찜이요!!",
          "speaker_id": "1",
          "time": "20210901 03:25"
        },
        {
          "id": "MDRW2100025259.1.10",
          "form": "이상형 월드컵하면 무조건 LA갈비 우승입니다",
          "original_form": "이상형 월드컵하면 무조건 LA갈비 우승입니다",
          "speaker_id": "2",
          "time": "20210901 03:25"
        },
        {
          "id": "MDRW2100025259.1.11",
          "form": "해물찜도 맛있죠",
          "original_form": "해물찜도 맛있죠",
          "speaker_id": "2",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025259.1.12",
          "form": "그 미더덕 식감 좋던데 저는",
          "original_form": "그 미더덕 식감 좋던데 저는",
          "speaker_id": "2",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025259.1.13",
          "form": "약간 이에 안좋을까봐 살살먹어요 ㅋㅋㅋ",
          "original_form": "약간 이에 안좋을까봐 살살먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025259.1.14",
          "form": "이미 치과에 돈 많이 갖다바쳐서...",
          "original_form": "이미 치과에 돈 많이 갖다바쳐서...",
          "speaker_id": "1",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025259.1.15",
          "form": "아구찜도진짜좋아요!",
          "original_form": "아구찜도진짜좋아요!",
          "speaker_id": "1",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025259.1.16",
          "form": "치과도 돈벌어야죠 ㅎㅎ",
          "original_form": "치과도 돈벌어야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025259.1.17",
          "form": "제돈을 많이 가져간... 떡볶이먹다가도 이가 나가서...",
          "original_form": "제돈을 많이 가져간... 떡볶이먹다가도 이가 나가서...",
          "speaker_id": "1",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025259.1.18",
          "form": "떡볶이는 어디가",
          "original_form": "떡볶이는 어디가",
          "speaker_id": "2",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025259.1.19",
          "form": "최애신가요",
          "original_form": "최애신가요",
          "speaker_id": "2",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025259.1.20",
          "form": "아 맵린이시지",
          "original_form": "아 맵린이시지",
          "speaker_id": "2",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025259.1.21",
          "form": "신전이요!",
          "original_form": "신전이요!",
          "speaker_id": "1",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025259.1.22",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210901 03:28"
        },
        {
          "id": "MDRW2100025259.1.23",
          "form": "맵리니가",
          "original_form": "맵리니가",
          "speaker_id": "2",
          "time": "20210901 03:28"
        },
        {
          "id": "MDRW2100025259.1.24",
          "form": "신전좋아요 순한맛ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "신전좋아요 순한맛ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:28"
        },
        {
          "id": "MDRW2100025259.1.25",
          "form": "어떻게",
          "original_form": "어떻게",
          "speaker_id": "2",
          "time": "20210901 03:28"
        },
        {
          "id": "MDRW2100025259.1.26",
          "form": "아 매운맛이죠 신전은",
          "original_form": "아 매운맛이죠 신전은",
          "speaker_id": "2",
          "time": "20210901 03:28"
        },
        {
          "id": "MDRW2100025259.1.27",
          "form": "보통맛도 맵더라구요",
          "original_form": "보통맛도 맵더라구요",
          "speaker_id": "1",
          "time": "20210901 03:28"
        },
        {
          "id": "MDRW2100025259.1.28",
          "form": "매운맛은 한입먹고 머리카락이 서는 느낌이었어요",
          "original_form": "매운맛은 한입먹고 머리카락이 서는 느낌이었어요",
          "speaker_id": "1",
          "time": "20210901 03:28"
        },
        {
          "id": "MDRW2100025259.1.29",
          "form": "신전매운맛이 그 최고단계 말하는거 맞으시죠?",
          "original_form": "신전매운맛이 그 최고단계 말하는거 맞으시죠?",
          "speaker_id": "1",
          "time": "20210901 03:29"
        }
      ]
    }
