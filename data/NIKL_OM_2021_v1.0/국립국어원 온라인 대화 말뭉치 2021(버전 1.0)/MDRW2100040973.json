      "utterance": [
        {
          "id": "MDRW2100040973.1.1",
          "form": "오케이광자매 홍은희가 발레전공역으로 나오네",
          "original_form": "오케이광자매 홍은희가 발레전공역으로 나오네",
          "speaker_id": "2",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100040973.1.2",
          "form": "애 낳는걸 싫어해서 전에 뚱뚱한 밥집 아줌마가 남편 애 낳았자나요",
          "original_form": "애 낳는걸 싫어해서 전에 뚱뚱한 밥집 아줌마가 남편 애 낳았자나요",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100040973.1.3",
          "form": "변호사와 결혼해서 딩크족이었구나",
          "original_form": "변호사와 결혼해서 딩크족이었구나",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100040973.1.4",
          "form": "근데 밥집 아줌마가 어처구니 없이 자 살 하고 하차",
          "original_form": "근데 밥집 아줌마가 어처구니 없이 자 살 하고 하차",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100040973.1.5",
          "form": "저번에 보니 임신 할려고 하던데...나쁜말",
          "original_form": "저번에 보니 임신 할려고 하던데...나쁜말",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100040973.1.6",
          "form": "나도 나뿌말ㅠ  시험관 하드라고요",
          "original_form": "나도 나뿌말ㅠ  시험관 하드라고요",
          "speaker_id": "1",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100040973.1.7",
          "form": "임 신으로 쌍둥이 맞나? 나쁜말 많이 나오네",
          "original_form": "임 신으로 쌍둥이 맞나? 나쁜말 많이 나오네",
          "speaker_id": "2",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100040973.1.8",
          "form": "그건 모르겠어요. 임신 했어요?",
          "original_form": "그건 모르겠어요. 임신 했어요?",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100040973.1.9",
          "form": "기사에 보니 그렇네",
          "original_form": "기사에 보니 그렇네",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100040973.1.10",
          "form": "결국 임신으로 끝나나 보네요",
          "original_form": "결국 임신으로 끝나나 보네요",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100040973.1.11",
          "form": "그런갑다 전혜빈은 남편이 가수야? 법적기혼상황",
          "original_form": "그런갑다 전혜빈은 남편이 가수야? 법적기혼상황",
          "speaker_id": "2",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100040973.1.12",
          "form": "어제 보니깐 노래 부르고 있드라구요ㅋ",
          "original_form": "어제 보니깐 노래 부르고 있드라구요ㅋ",
          "speaker_id": "1",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040973.1.13",
          "form": "아 가수로 잘나가기 시작했는데 벌써 바람을 피나보다",
          "original_form": "아 가수로 잘나가기 시작했는데 벌써 바람을 피나보다",
          "speaker_id": "2",
          "time": "20210905 14:20"
        }
