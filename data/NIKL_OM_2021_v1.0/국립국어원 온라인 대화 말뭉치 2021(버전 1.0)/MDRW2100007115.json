      "utterance": [
        {
          "id": "MDRW2100007115.1.1",
          "form": "그러게요 정유미님 최근 작품이 없네요?ㅠㅠ",
          "original_form": "그러게요 정유미님 최근 작품이 없네요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007115.1.2",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007115.1.3",
          "form": "정말 괜찮은 배우인데 귀엽고 ㅎㅎ",
          "original_form": "정말 괜찮은 배우인데 귀엽고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007115.1.4",
          "form": "보니까 19년이 마지막인거 같아요",
          "original_form": "보니까 19년이 마지막인거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007115.1.5",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007115.1.6",
          "form": "앗...정유미가 둘이네요 ㅋ",
          "original_form": "앗...정유미가 둘이네요 ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007115.1.7",
          "form": "아 맞아요 그랬죠ㅋㅋㅋ",
          "original_form": "아 맞아요 그랬죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007115.1.8",
          "form": "부산행 나오신 분이랑",
          "original_form": "부산행 나오신 분이랑",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007115.1.9",
          "form": "안그래도 윤식당에 나오신분도 정유미네요",
          "original_form": "안그래도 윤식당에 나오신분도 정유미네요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007115.1.10",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007115.1.11",
          "form": "검법남녀 나오신 분은 다른 분이시네요",
          "original_form": "검법남녀 나오신 분은 다른 분이시네요",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007115.1.12",
          "form": "네 귀여운 정유미 ㅎㅎ",
          "original_form": "네 귀여운 정유미 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007115.1.13",
          "form": "못본지 오래된거같아요",
          "original_form": "못본지 오래된거같아요",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007115.1.14",
          "form": "더 열일 해주시면 좋을텐데!",
          "original_form": "더 열일 해주시면 좋을텐데!",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007115.1.15",
          "form": "결혼했나...ㅎㅎ",
          "original_form": "결혼했나...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007115.1.16",
          "form": "박지윤아시죠?",
          "original_form": "박지윤아시죠?",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007115.1.17",
          "form": "네! 크라임씬 나오셨던 분이죠?",
          "original_form": "네! 크라임씬 나오셨던 분이죠?",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007115.1.18",
          "form": "아뇨 가수 박지윤이요",
          "original_form": "아뇨 가수 박지윤이요",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007115.1.19",
          "form": "그박지윤은 아나운서지싶어요",
          "original_form": "그박지윤은 아나운서지싶어요",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007115.1.20",
          "form": "앗 맞아요!!",
          "original_form": "앗 맞아요!!",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007115.1.21",
          "form": "하늘색꿈 인가? 그거 불렀어요",
          "original_form": "하늘색꿈 인가? 그거 불렀어요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007115.1.22",
          "form": "가수 박지윤님은 잘 모르는 것 같아요ㅠㅠ",
          "original_form": "가수 박지윤님은 잘 모르는 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007115.1.23",
          "form": "그분도 잊혀져가더니...어느날 뉴스에서봤어요",
          "original_form": "그분도 잊혀져가더니...어느날 뉴스에서봤어요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007115.1.24",
          "form": "카카오사장님 와이프가 되어계시더라구요",
          "original_form": "카카오사장님 와이프가 되어계시더라구요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        }
      ]
    }
