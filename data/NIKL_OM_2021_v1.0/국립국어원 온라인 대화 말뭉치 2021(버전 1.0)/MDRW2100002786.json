      "utterance": [
        {
          "id": "MDRW2100002786.1.1",
          "form": "날씨어때요?",
          "original_form": "날씨어때요?",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002786.1.2",
          "form": "날씨 진짜좋아요",
          "original_form": "날씨 진짜좋아요",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002786.1.3",
          "form": "늦여름이라 조금 덥긴하지만",
          "original_form": "늦여름이라 조금 덥긴하지만",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002786.1.4",
          "form": "미세먼지도 없고",
          "original_form": "미세먼지도 없고",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002786.1.5",
          "form": "여긴 더워요",
          "original_form": "여긴 더워요",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002786.1.6",
          "form": "그늘밑에 있으면 그래도 바람이 선선한거같아요",
          "original_form": "그늘밑에 있으면 그래도 바람이 선선한거같아요",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002786.1.7",
          "form": "습한 기운도 없고",
          "original_form": "습한 기운도 없고",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002786.1.8",
          "form": "바람만 불면 좋아요",
          "original_form": "바람만 불면 좋아요",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002786.1.9",
          "form": "저는 그래서 초가을 날씨를 제일 좋아해요",
          "original_form": "저는 그래서 초가을 날씨를 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002786.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002786.1.11",
          "form": "추석지나면 또 찬바람불고 할텐데 그 전에 이 날씨를 즐겨야죠",
          "original_form": "추석지나면 또 찬바람불고 할텐데 그 전에 이 날씨를 즐겨야죠",
          "speaker_id": "2",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002786.1.12",
          "form": "ㅎㅎ 그렇죠",
          "original_form": "ㅎㅎ 그렇죠",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002786.1.13",
          "form": "그런데 다음주에 늦은 장마 온다고 하더라고요",
          "original_form": "그런데 다음주에 늦은 장마 온다고 하더라고요",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002786.1.14",
          "form": "내내 비더라구요",
          "original_form": "내내 비더라구요",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002786.1.15",
          "form": "비내리는건 좋은데 출퇴근하면서 비오고하면 신경쓰여서 싫더라고요",
          "original_form": "비내리는건 좋은데 출퇴근하면서 비오고하면 신경쓰여서 싫더라고요",
          "speaker_id": "2",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002786.1.16",
          "form": "앞이 안보이더라구요",
          "original_form": "앞이 안보이더라구요",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002786.1.17",
          "form": "네 그래서 운전하고부터는 비오는 날 싫어졌어요 ㅎ",
          "original_form": "네 그래서 운전하고부터는 비오는 날 싫어졌어요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:10"
        }
      ]
