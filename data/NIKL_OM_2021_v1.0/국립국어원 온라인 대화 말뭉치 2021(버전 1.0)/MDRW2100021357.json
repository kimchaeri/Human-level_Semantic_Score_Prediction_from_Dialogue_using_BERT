      "utterance": [
        {
          "id": "MDRW2100021357.1.1",
          "form": "좋아하는 음식이나 음료 있으세요?",
          "original_form": "좋아하는 음식이나 음료 있으세요?",
          "speaker_id": "2",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100021357.1.2",
          "form": "저 빵좋아해요 !!",
          "original_form": "저 빵좋아해요 !!",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100021357.1.3",
          "form": "음료는 단걸끊어서 밀크티나 차 종류 좋아해요 ㅎㅎ",
          "original_form": "음료는 단걸끊어서 밀크티나 차 종류 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100021357.1.4",
          "form": "헐 제가 좋아하는 음식이랑 음료를 정확히 말씀하셨어요",
          "original_form": "헐 제가 좋아하는 음식이랑 음료를 정확히 말씀하셨어요",
          "speaker_id": "2",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100021357.1.5",
          "form": "저 방금 베이글이랑 홍차 마셨거든요",
          "original_form": "저 방금 베이글이랑 홍차 마셨거든요",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100021357.1.6",
          "form": "오 그 조합 좋네요..! 홍차 좋아해요ㅎㅎ",
          "original_form": "오 그 조합 좋네요..! 홍차 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100021357.1.7",
          "form": "저도 홍차 완전ㅜㅜ",
          "original_form": "저도 홍차 완전ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100021357.1.8",
          "form": "빵도 .... 빵 없이 못 살아요",
          "original_form": "빵도 .... 빵 없이 못 살아요",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100021357.1.9",
          "form": "어떤 종류 빵 제일 좋아하세요?",
          "original_form": "어떤 종류 빵 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100021357.1.10",
          "form": "거의 다 좋아해요.. ! 단걸 제일 좋아하는 것 같긴하지만",
          "original_form": "거의 다 좋아해요.. ! 단걸 제일 좋아하는 것 같긴하지만",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021357.1.11",
          "form": "담백하거나 고소한 맨빵도 좋아합니다 ㅎㅎ",
          "original_form": "담백하거나 고소한 맨빵도 좋아합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021357.1.12",
          "form": "오 그럼 베이글 이런 거 좋아하시겠군요!",
          "original_form": "오 그럼 베이글 이런 거 좋아하시겠군요!",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021357.1.13",
          "form": "네네 좋아해요ㅎㅎ 크림치즈도 맛있고 연어같은거 넣어도 맛있더라구요 !",
          "original_form": "네네 좋아해요ㅎㅎ 크림치즈도 맛있고 연어같은거 넣어도 맛있더라구요 !",
          "speaker_id": "1",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100021357.1.14",
          "form": "군침 도네요... ㅎㅎ 혹시 저녁 드셨나요",
          "original_form": "군침 도네요... ㅎㅎ 혹시 저녁 드셨나요",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021357.1.15",
          "form": "곡 먹으려구요 ! 닭볶음탕 먹을예정입니다",
          "original_form": "곡 먹으려구요 ! 닭볶음탕 먹을예정입니다",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021357.1.16",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021357.1.17",
          "form": "와 닭볶음탕...! 직접 요리하시나요",
          "original_form": "와 닭볶음탕...! 직접 요리하시나요",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021357.1.18",
          "form": "저는 아직이요 ㅎㅎㅎ 간식을 많이 먹어서..",
          "original_form": "저는 아직이요 ㅎㅎㅎ 간식을 많이 먹어서..",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021357.1.19",
          "form": "엄마가 해주셔요..!! 식사준비만 합니다..",
          "original_form": "엄마가 해주셔요..!! 식사준비만 합니다..",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021357.1.20",
          "form": "어떤거 드실 예정이신가요?",
          "original_form": "어떤거 드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021357.1.21",
          "form": "저는 아직 고민 중이에요",
          "original_form": "저는 아직 고민 중이에요",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100021357.1.22",
          "form": "오늘 야채를 하나도 안 먹어서 야채들어간 걸 먹어야 할 거 같긴 한데...",
          "original_form": "오늘 야채를 하나도 안 먹어서 야채들어간 걸 먹어야 할 거 같긴 한데...",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100021357.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100021357.1.24",
          "form": "야채들어간 볶음밥으로 저녁 어떠신가요 ..!",
          "original_form": "야채들어간 볶음밥으로 저녁 어떠신가요 ..!",
