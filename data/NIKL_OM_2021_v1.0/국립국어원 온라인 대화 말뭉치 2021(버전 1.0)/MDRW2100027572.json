      "utterance": [
        {
          "id": "MDRW2100027572.1.1",
          "form": "나는 고무신 샀는데",
          "original_form": "나는 고무신 샀는데",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027572.1.2",
          "form": "옥션2000원줘서",
          "original_form": "옥션2000원줘서",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027572.1.3",
          "form": "실물 못신을정도일까봐 두근거려ㅋ",
          "original_form": "실물 못신을정도일까봐 두근거려ㅋ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027572.1.4",
          "form": "고무신? 실내에서 신게?",
          "original_form": "고무신? 실내에서 신게?",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027572.1.5",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027572.1.6",
          "form": "외부용신데ㅎ",
          "original_form": "외부용신데ㅎ",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027572.1.7",
          "form": "실외용인데",
          "original_form": "실외용인데",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027572.1.8",
          "form": "밖에서 신어도 되는 디자인이야?",
          "original_form": "밖에서 신어도 되는 디자인이야?",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027572.1.9",
          "form": "응 밖에용을 파는건데",
          "original_form": "응 밖에용을 파는건데",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027572.1.10",
          "form": "도전에 가까움ㅋㅋ",
          "original_form": "도전에 가까움ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027572.1.11",
          "form": "와 디자인만 보면 괜찮은데 ㅋㅋㅋ",
          "original_form": "와 디자인만 보면 괜찮은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027572.1.12",
          "form": "그체",
          "original_form": "그체",
          "speaker_id": "2",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027572.1.13",
          "form": "가격 얼마주고샀어??",
          "original_form": "가격 얼마주고샀어??",
          "speaker_id": "1",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027572.1.14",
          "form": "주름도 있고 플랫느낌에 굽도 있고",
          "original_form": "주름도 있고 플랫느낌에 굽도 있고",
          "speaker_id": "2",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027572.1.15",
          "form": "할인받아서 만원 좀 안돼게",
          "original_form": "할인받아서 만원 좀 안돼게",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027572.1.16",
          "form": "싸다",
          "original_form": "싸다",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027572.1.17",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027572.1.18",
          "form": "디자인만 좋으면 비오는날 신으면 좋겠다",
          "original_form": "디자인만 좋으면 비오는날 신으면 좋겠다",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027572.1.19",
          "form": "응 그역할도 하려고",
          "original_form": "응 그역할도 하려고",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027572.1.20",
          "form": "맨발용도 가능한ㅋ",
          "original_form": "맨발용도 가능한ㅋ",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027572.1.21",
          "form": "리뷰같은거 읽어봤어??",
          "original_form": "리뷰같은거 읽어봤어??",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027572.1.22",
          "form": "리뷰 좋으면 괜찮을듯",
          "original_form": "리뷰 좋으면 괜찮을듯",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027572.1.23",
          "form": "리뷰가 거의 못찾",
          "original_form": "리뷰가 거의 못찾",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027572.1.24",
          "form": "한줄짜리 있던데 참고안돼더라",
          "original_form": "한줄짜리 있던데 참고안돼더라",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027572.1.25",
          "form": "쿠폰있으니까 도전했지",
          "original_form": "쿠폰있으니까 도전했지",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027572.1.26",
          "form": "내돈 더 썻지만ㅋㅋㅋ",
          "original_form": "내돈 더 썻지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027572.1.27",
          "form": "아...  싼티 안나길 기도할게",
          "original_form": "아...  싼티 안나길 기도할게",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027572.1.28",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027572.1.29",
          "form": "응ㅋ",
          "original_form": "응ㅋ",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027572.1.30",
          "form": "싼티",
          "original_form": "싼티",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027572.1.31",
          "form": "두근",
          "original_form": "두근",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027572.1.32",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027572.1.33",
          "form": "폭립 샀던거 왔어?",
          "original_form": "폭립 샀던거 왔어?",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027572.1.34",
