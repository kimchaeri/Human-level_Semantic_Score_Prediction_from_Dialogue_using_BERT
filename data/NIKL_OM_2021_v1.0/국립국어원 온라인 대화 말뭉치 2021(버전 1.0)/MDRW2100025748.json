      "utterance": [
        {
          "id": "MDRW2100025748.1.1",
          "form": "음 나는 출퇴정리 ㅠㅠ 낼진짜바쁠듯",
          "original_form": "음 나는 출퇴정리 ㅠㅠ 낼진짜바쁠듯",
          "speaker_id": "2",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100025748.1.2",
          "form": "오늘은 시 보고 세건 끝내썽..",
          "original_form": "오늘은 시 보고 세건 끝내썽..",
          "speaker_id": "2",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100025748.1.3",
          "form": "휴 루틴인데 귀찮은거알지",
          "original_form": "휴 루틴인데 귀찮은거알지",
          "speaker_id": "2",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100025748.1.4",
          "form": "아우 바쁘게ㅛ다",
          "original_form": "아우 바쁘게ㅛ다",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100025748.1.5",
          "form": "ㅎㅎ...담주 휴가잡았는데 운영서류해야해서 9월말이나 10월에 가게",
          "original_form": "ㅎㅎ...담주 휴가잡았는데 운영서류해야해서 9월말이나 10월에 가게",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025748.1.6",
          "form": "헐...너 근데 휴가하면 며칠이나 쉬냐",
          "original_form": "헐...너 근데 휴가하면 며칠이나 쉬냐",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025748.1.7",
          "form": "4일인데 연차 끼고하면 9일정도?? 너능??",
          "original_form": "4일인데 연차 끼고하면 9일정도?? 너능??",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025748.1.8",
          "form": "난 걍 이틀쉬었어",
          "original_form": "난 걍 이틀쉬었어",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025748.1.9",
          "form": "나도 그정도도 쓸 수 있긴한데",
          "original_form": "나도 그정도도 쓸 수 있긴한데",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025748.1.10",
          "form": "오오 조은데 언제쉼?",
          "original_form": "오오 조은데 언제쉼?",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025748.1.11",
          "form": "이미쉬었는데요.ㅡ.화이자맞음",
          "original_form": "이미쉬었는데요.ㅡ.화이자맞음",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025748.1.12",
          "form": "그냥쉰게..이미쓴거였어? 연차가아니고..?ㅠ 또륵",
          "original_form": "그냥쉰게..이미쓴거였어? 연차가아니고..?ㅠ 또륵",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025748.1.13",
          "form": "으응...휴가로",
          "original_form": "으응...휴가로",
          "speaker_id": "1",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100025748.1.14",
          "form": "그랴도 담주도 2일쉬고",
          "original_form": "그랴도 담주도 2일쉬고",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025748.1.15",
          "form": "그 담주는 추석이여",
          "original_form": "그 담주는 추석이여",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025748.1.16",
          "form": "8월 9월은 많이쉬어서",
          "original_form": "8월 9월은 많이쉬어서",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025748.1.17",
          "form": "금방간다",
          "original_form": "금방간다",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025748.1.18",
          "form": "ㅁㅈ 추석만세",
          "original_form": "ㅁㅈ 추석만세",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025748.1.19",
          "form": "10월도 공휴일 2일 생겨서조아",
          "original_form": "10월도 공휴일 2일 생겨서조아",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025748.1.20",
          "form": "대체휴무인가..?",
          "original_form": "대체휴무인가..?",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025748.1.21",
          "form": "야 나",
          "original_form": "야 나",
          "speaker_id": "1",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100025748.1.22",
          "form": "이번달부터 식대 천원 올랐다",
