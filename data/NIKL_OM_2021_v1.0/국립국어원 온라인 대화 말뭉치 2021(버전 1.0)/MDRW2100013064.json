      "utterance": [
        {
          "id": "MDRW2100013064.1.1",
          "form": "저는 필라테스를 등록하고 싶어요",
          "original_form": "저는 필라테스를 등록하고 싶어요",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100013064.1.2",
          "form": "저도요 내년에 할라고요",
          "original_form": "저도요 내년에 할라고요",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013064.1.3",
          "form": "초보라 개인강습알아보는데 가격이 후덜덜해요",
          "original_form": "초보라 개인강습알아보는데 가격이 후덜덜해요",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013064.1.4",
          "form": "그니깐요 ㅜ 일대일로 해야하는데",
          "original_form": "그니깐요 ㅜ 일대일로 해야하는데",
          "speaker_id": "2",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100013064.1.5",
          "form": "허리가 항상 아프던 분이 개인강습받고 다음날 멀쩡해서 바로 50회 끊으셨다길래 그룹은 좀 배우다하려고요",
          "original_form": "허리가 항상 아프던 분이 개인강습받고 다음날 멀쩡해서 바로 50회 끊으셨다길래 그룹은 좀 배우다하려고요",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013064.1.6",
          "form": "ㅎㅎ 완전효과 직빵이네요",
          "original_form": "ㅎㅎ 완전효과 직빵이네요",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013064.1.7",
          "form": "이것도 엄청 마른 분 말고 물리치료 등 전공하신 꿀벅지쌤을 따라가야한대요",
          "original_form": "이것도 엄청 마른 분 말고 물리치료 등 전공하신 꿀벅지쌤을 따라가야한대요",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013064.1.8",
          "form": "아 꿀팁이네요 다들 마르신거같던데 ㅜ",
          "original_form": "아 꿀팁이네요 다들 마르신거같던데 ㅜ",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013064.1.9",
          "form": "근육을 잘 아는 분한테 배워야 몸이 안상한다고 하더라고요",
          "original_form": "근육을 잘 아는 분한테 배워야 몸이 안상한다고 하더라고요",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013064.1.10",
          "form": "잘따져봐야겠네요",
          "original_form": "잘따져봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013064.1.11",
          "form": "배우신 운동 있으세요?",
          "original_form": "배우신 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013064.1.12",
          "form": "수영이요",
          "original_form": "수영이요",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013064.1.13",
          "form": "저는 평영까지만 하고 더 못배웠어요",
          "original_form": "저는 평영까지만 하고 더 못배웠어요",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013064.1.14",
          "form": "저는 다이빙까지했네요",
          "original_form": "저는 다이빙까지했네요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013064.1.15",
          "form": "멋져요 다이빙",
          "original_form": "멋져요 다이빙",
          "speaker_id": "1",
          "time": "20210825 15:32"
        }
      ]
