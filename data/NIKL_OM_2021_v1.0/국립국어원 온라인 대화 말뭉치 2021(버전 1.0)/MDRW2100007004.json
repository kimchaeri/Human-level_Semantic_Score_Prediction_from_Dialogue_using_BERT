      "utterance": [
        {
          "id": "MDRW2100007004.1.1",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007004.1.2",
          "form": "어디로",
          "original_form": "어디로",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007004.1.3",
          "form": "해외",
          "original_form": "해외",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007004.1.4",
          "form": "해외못감",
          "original_form": "해외못감",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007004.1.5",
          "form": "왜? 갈수있음",
          "original_form": "왜? 갈수있음",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007004.1.6",
          "form": "제주도나 다시가라",
          "original_form": "제주도나 다시가라",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007004.1.7",
          "form": "싫어 지겨워",
          "original_form": "싫어 지겨워",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007004.1.8",
          "form": "갔따오면 자가격리 2주아이가?",
          "original_form": "갔따오면 자가격리 2주아이가?",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007004.1.9",
          "form": "아님 백신 맞아서 괜찮음",
          "original_form": "아님 백신 맞아서 괜찮음",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007004.1.10",
          "form": "백신맞으면 자가격리아님?",
          "original_form": "백신맞으면 자가격리아님?",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007004.1.11",
          "form": "옹 백신 다맞으면 그럼",
          "original_form": "옹 백신 다맞으면 그럼",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007004.1.12",
          "form": "그럼이제 해외여행가도되네?",
          "original_form": "그럼이제 해외여행가도되네?",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007004.1.13",
          "form": "근데 해외에 코로나 확진자가 더 많아서 조심",
          "original_form": "근데 해외에 코로나 확진자가 더 많아서 조심",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007004.1.14",
          "form": "신혼여행도 가도되는거고 그면? 걱정했는데",
          "original_form": "신혼여행도 가도되는거고 그면? 걱정했는데",
          "speaker_id": "1",
          "time": "20210822 00:14"
        }
