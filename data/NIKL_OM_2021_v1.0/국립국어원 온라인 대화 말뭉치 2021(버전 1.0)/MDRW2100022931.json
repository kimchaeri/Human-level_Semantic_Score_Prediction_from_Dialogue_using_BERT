        {
          "id": "MDRW2100022931.1.1",
          "form": "인턴 해보셨나요??",
          "original_form": "인턴 해보셨나요??",
          "speaker_id": "1",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100022931.1.2",
          "form": "아뇽ㅠㅠ",
