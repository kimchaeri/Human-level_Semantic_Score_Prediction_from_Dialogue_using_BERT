      "utterance": [
        {
          "id": "MDRW2100004946.1.1",
          "form": "활동하시는 카페나 모임 있으세요?",
          "original_form": "활동하시는 카페나 모임 있으세요?",
          "speaker_id": "1",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004946.1.2",
          "form": "가입한건 많은데 활동은 안해요. 눈팅만~~",
          "original_form": "가입한건 많은데 활동은 안해요. 눈팅만~~",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004946.1.3",
          "form": "저도 눈팅만하는데 토스땜에 몇번글올렸더니",
          "original_form": "저도 눈팅만하는데 토스땜에 몇번글올렸더니",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004946.1.4",
          "form": "중수됐대요ㅋ",
          "original_form": "중수됐대요ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004946.1.5",
          "form": "오~~~ 전 중수는 못할듯요",
          "original_form": "오~~~ 전 중수는 못할듯요",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004946.1.6",
          "form": "맘카페도그렇고 글쓰는게 힘들어요",
          "original_form": "맘카페도그렇고 글쓰는게 힘들어요",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004946.1.7",
          "form": "저도 몇개안썼거든요",
          "original_form": "저도 몇개안썼거든요",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004946.1.8",
          "form": "중수라길래 깜놀ㅎ",
          "original_form": "중수라길래 깜놀ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004946.1.9",
          "form": "ㅎ 그냥 거래는 못하지만 글은 볼수있으니 전 그냥 천천히 되면좋고 아님 말고~",
          "original_form": "ㅎ 그냥 거래는 못하지만 글은 볼수있으니 전 그냥 천천히 되면좋고 아님 말고~",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004946.1.10",
          "form": "저도 그냥 정보만 얻는데 의의를 두고있어요",
          "original_form": "저도 그냥 정보만 얻는데 의의를 두고있어요",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100004946.1.11",
          "form": "요즘 스너글 즉당 또 하나보더라구요. 가끔들어가는데 그때마다 인증글이 넘치네요",
          "original_form": "요즘 스너글 즉당 또 하나보더라구요. 가끔들어가는데 그때마다 인증글이 넘치네요",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100004946.1.12",
          "form": "그거 맨날  꽝이던데 다들 잘되나봐요",
          "original_form": "그거 맨날  꽝이던데 다들 잘되나봐요",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004946.1.13",
          "form": "카페에 당첨글 많더라구요",
          "original_form": "카페에 당첨글 많더라구요",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004946.1.14",
          "form": "전 오늘해봤는데 역시나 꽝~",
          "original_form": "전 오늘해봤는데 역시나 꽝~",
          "speaker_id": "2",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004946.1.15",
          "form": "즉당과는 인연이 없네요",
          "original_form": "즉당과는 인연이 없네요",
          "speaker_id": "2",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004946.1.16",
          "form": "잘안되더라구요 인증도 해야돼서 가족건못하겠고",
          "original_form": "잘안되더라구요 인증도 해야돼서 가족건못하겠고",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004946.1.17",
          "form": "그러니깐요.  인증만 매일안함 할만한데 굳이 주지도않으면서 매일하라하고ㅠ",
          "original_form": "그러니깐요.  인증만 매일안함 할만한데 굳이 주지도않으면서 매일하라하고ㅠ",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004946.1.18",
          "form": "카페보면 저만 빼고 다되는 느낌인데 그렇지도 않은가보네요",
          "original_form": "카페보면 저만 빼고 다되는 느낌인데 그렇지도 않은가보네요",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100004946.1.19",
          "form": "당첨만올리니 다당첨이고 나만 꽝인 느낌이 크긴해요",
          "original_form": "당첨만올리니 다당첨이고 나만 꽝인 느낌이 크긴해요",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004946.1.20",
          "form": "스너글 말고 다른 이벤트좀 찾아봐야겠어요",
          "original_form": "스너글 말고 다른 이벤트좀 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 20:16"
        }
      ]
