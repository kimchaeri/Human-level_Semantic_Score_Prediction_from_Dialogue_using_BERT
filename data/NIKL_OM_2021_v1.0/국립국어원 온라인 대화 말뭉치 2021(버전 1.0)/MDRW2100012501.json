      "utterance": [
        {
          "id": "MDRW2100012501.1.1",
          "form": "애들은 이제 자나?",
          "original_form": "애들은 이제 자나?",
          "speaker_id": "2",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012501.1.2",
          "form": "ㅇㅇ 방금 잠들었어",
          "original_form": "ㅇㅇ 방금 잠들었어",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012501.1.3",
          "form": "오늘은 늦게 잠들었나보네",
          "original_form": "오늘은 늦게 잠들었나보네",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012501.1.4",
          "form": "잠들기 직전까지 엄청 떠들어서",
          "original_form": "잠들기 직전까지 엄청 떠들어서",
          "speaker_id": "1",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012501.1.5",
          "form": "무슨 얘기를 그렇게 해?",
          "original_form": "무슨 얘기를 그렇게 해?",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012501.1.6",
          "form": "게임, 퀴즈 이런거 내 ㅋㅋ 서로 말하고 싶어 난리",
          "original_form": "게임, 퀴즈 이런거 내 ㅋㅋ 서로 말하고 싶어 난리",
          "speaker_id": "1",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012501.1.7",
          "form": "그냥 잠이 자기 싫은 거겠지",
          "original_form": "그냥 잠이 자기 싫은 거겠지",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012501.1.8",
          "form": "방학이었으면 더 받아주는데 내일 또 수업해야 하니 자야지",
          "original_form": "방학이었으면 더 받아주는데 내일 또 수업해야 하니 자야지",
          "speaker_id": "1",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012501.1.9",
          "form": "ㅇㅇ 이젠 늦어도 10시에는 잠들어야 수업할 때 덜 피곤할껴",
          "original_form": "ㅇㅇ 이젠 늦어도 10시에는 잠들어야 수업할 때 덜 피곤할껴",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012501.1.10",
          "form": "수다쟁이들 ㅎㅎ",
          "original_form": "수다쟁이들 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012501.1.11",
          "form": "아들은 보통 말이 없는 줄 알았는데 ㅋㅋ",
          "original_form": "아들은 보통 말이 없는 줄 알았는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012501.1.12",
          "form": "우리 애들은 둘 다 아냐 ㅎㅎ",
          "original_form": "우리 애들은 둘 다 아냐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012501.1.13",
          "form": "좋지 뭐 ㅎㅎ 과묵한 것보단",
          "original_form": "좋지 뭐 ㅎㅎ 과묵한 것보단",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012501.1.14",
          "form": "맞아~ 애들이랑 얘기하면 엄청 재미있어~ 친구같고",
          "original_form": "맞아~ 애들이랑 얘기하면 엄청 재미있어~ 친구같고",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012501.1.15",
          "form": "색시는 애들 볼 때 눈에서 꿀떨어짐",
          "original_form": "색시는 애들 볼 때 눈에서 꿀떨어짐",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012501.1.16",
          "form": "ㅎㅎ 내 꿀덩어리들~ 너무 예뻐",
          "original_form": "ㅎㅎ 내 꿀덩어리들~ 너무 예뻐",
          "speaker_id": "1",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012501.1.17",
          "form": "우리 잘 때는 어떻게 해? 창문 열고 잘거야?",
          "original_form": "우리 잘 때는 어떻게 해? 창문 열고 잘거야?",
          "speaker_id": "1",
          "time": "20210824 22:45"
        }
      ]
