      "utterance": [
        {
          "id": "MDRW2100044211.1.1",
          "form": "저두요ㅠ 나름 받으니 하려는 의지도 생기고요ㅋㅋ",
          "original_form": "저두요ㅠ 나름 받으니 하려는 의지도 생기고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044211.1.2",
          "form": "맞아요 많이 받으셨나요?",
          "original_form": "맞아요 많이 받으셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044211.1.3",
          "form": "저어는 중간쯤 까지만요ㅎㅎ",
          "original_form": "저어는 중간쯤 까지만요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044211.1.4",
          "form": "저는 그래도 만원까지는 벌었어요!",
          "original_form": "저는 그래도 만원까지는 벌었어요!",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044211.1.5",
          "form": "ㅋㅋ저두여",
          "original_form": "ㅋㅋ저두여",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044211.1.6",
          "form": "가끔 신나서 음식이야기 하고나면 쑥쑥 늘어나서",
          "original_form": "가끔 신나서 음식이야기 하고나면 쑥쑥 늘어나서",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044211.1.7",
          "form": "그쳐ㅋㅋㅋ",
          "original_form": "그쳐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044211.1.8",
          "form": "그 급여?로 또 맛있는걸 사먹었습니다",
          "original_form": "그 급여?로 또 맛있는걸 사먹었습니다",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044211.1.9",
          "form": "음식이야기가 젤많음..",
          "original_form": "음식이야기가 젤많음..",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044211.1.10",
          "form": "긍정적인 선순환",
          "original_form": "긍정적인 선순환",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044211.1.11",
          "form": "저도 그걸로 피자시켜먹었는데요..ㅋㅋ",
          "original_form": "저도 그걸로 피자시켜먹었는데요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044211.1.12",
          "form": "다른사람들도 그랬을거 같아요",
          "original_form": "다른사람들도 그랬을거 같아요",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044211.1.13",
          "form": "앗 ㅋㅋㅋㅋ 다들 비슷하군요",
          "original_form": "앗 ㅋㅋㅋㅋ 다들 비슷하군요",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044211.1.14",
          "form": "저는 건강하게 그릭요거트 시켰습니다",
          "original_form": "저는 건강하게 그릭요거트 시켰습니다",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044211.1.15",
          "form": "ㅋㅋㅋ 덕분에 토핑이 좀더 풍성한 피자를..",
          "original_form": "ㅋㅋㅋ 덕분에 토핑이 좀더 풍성한 피자를..",
