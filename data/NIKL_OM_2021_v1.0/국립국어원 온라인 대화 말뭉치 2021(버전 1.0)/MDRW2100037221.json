      "utterance": [
        {
          "id": "MDRW2100037221.1.1",
          "form": "그래서 남자한테 그 선물을 고르게 된 계기를",
          "original_form": "그래서 남자한테 그 선물을 고르게 된 계기를",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037221.1.2",
          "form": "물어보니까",
          "original_form": "물어보니까",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037221.1.3",
          "form": "알고보니 엄마가 예전에 세일할 때 사놓은 귀걸이를",
          "original_form": "알고보니 엄마가 예전에 세일할 때 사놓은 귀걸이를",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037221.1.4",
          "form": "네 뭐라그랬는지 궁금하네요",
          "original_form": "네 뭐라그랬는지 궁금하네요",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037221.1.5",
          "form": "갖고온거예요",
          "original_form": "갖고온거예요",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037221.1.6",
          "form": "으악 미쳤네요 그걸 또 말해요? ㅋㅋ",
          "original_form": "으악 미쳤네요 그걸 또 말해요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037221.1.7",
          "form": "100일 선물을",
          "original_form": "100일 선물을",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037221.1.8",
          "form": "네 그냥 말했대요",
          "original_form": "네 그냥 말했대요",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037221.1.9",
          "form": "그냥 그런 개념이 무슨 예절이나 사회생활 그런게 없어요",
          "original_form": "그냥 그런 개념이 무슨 예절이나 사회생활 그런게 없어요",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037221.1.10",
          "form": "아니그니까요 근데 또",
          "original_form": "아니그니까요 근데 또",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.11",
          "form": "그냥 비싼 물건이라는 거에만 꽂혀서",
          "original_form": "그냥 비싼 물건이라는 거에만 꽂혀서",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.12",
          "form": "뭐가 불만이냐",
          "original_form": "뭐가 불만이냐",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.13",
          "form": "귀는 너가 나중에",
          "original_form": "귀는 너가 나중에",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.14",
          "form": "그래서 그뒤에 사과는 했대요? 똑같죠?",
          "original_form": "그래서 그뒤에 사과는 했대요? 똑같죠?",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.15",
          "form": "뚫으면 되는거 아니냐",
          "original_form": "뚫으면 되는거 아니냐",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.16",
          "form": "이러더래요",
          "original_form": "이러더래요",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.17",
          "form": "미친... 도라이같아요",
          "original_form": "미친... 도라이같아요",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.18",
          "form": "그니까욬ㅋㅋㅋㅋ",
          "original_form": "그니까욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.19",
          "form": "아니진짜 이상해요",
          "original_form": "아니진짜 이상해요",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.20",
          "form": "그리고 저 진짜 답답한게",
          "original_form": "그리고 저 진짜 답답한게",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037221.1.21",
          "form": "전혀 상황에도 안맞고 동성끼리 그래도 싸움나요",
          "original_form": "전혀 상황에도 안맞고 동성끼리 그래도 싸움나요",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037221.1.22",
          "form": "너무 남자들이 여자한테 돈을 안써요",
          "original_form": "너무 남자들이 여자한테 돈을 안써요",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037221.1.23",
          "form": "아니그니까요!!!!",
          "original_form": "아니그니까요!!!!",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037221.1.24",
          "form": "저 진짜 현타왔던게",
          "original_form": "저 진짜 현타왔던게",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037221.1.25",
          "form": "남자가 사준 생일선물보다",
          "original_form": "남자가 사준 생일선물보다",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037221.1.26",
          "form": "너무 그런게 안된남자는 버리는게....",
          "original_form": "너무 그런게 안된남자는 버리는게....",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037221.1.27",
          "form": "친구들이 주는 선물이 더 비싸더라구요",
          "original_form": "친구들이 주는 선물이 더 비싸더라구요",
          "speaker_id": "2",
          "time": "20210831 21:39"
        }
      ]
