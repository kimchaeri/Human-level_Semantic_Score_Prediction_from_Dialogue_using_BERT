        },
        {
          "id": "MDRW2100038261.1.2",
          "form": "쉬프트+엔터라고 하셨나요?",
          "original_form": "쉬프트+엔터라고 하셨나요?",
          "speaker_id": "2",
          "time": "20210902 04:39"
        },
        {
          "id": "MDRW2100038261.1.3",
          "form": "아까 못보셨죠?",
          "original_form": "아까 못보셨죠?",
          "speaker_id": "1",
          "time": "20210902 04:39"
        },
        {
          "id": "MDRW2100038261.1.4",
          "form": "네맞아요ㅋㅋㅋㅋ",
          "original_form": "네맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 04:39"
        },
        {
          "id": "MDRW2100038261.1.5",
          "form": "이게 카톡 설정이 꺼져있을수도 잇ㄱ어요",
          "original_form": "이게 카톡 설정이 꺼져있을수도 잇ㄱ어요",
          "speaker_id": "1",
          "time": "20210902 04:39"
        },
        {
          "id": "MDRW2100038261.1.6",
          "form": "아 그렇군요!",
          "original_form": "아 그렇군요!",
          "speaker_id": "2",
          "time": "20210902 04:40"
        },
        {
          "id": "MDRW2100038261.1.7",
          "form": "보통 켜있을테지만요ㅎ",
          "original_form": "보통 켜있을테지만요ㅎ",
          "speaker_id": "1",
          "time": "20210902 04:40"
        },
        {
          "id": "MDRW2100038261.1.8",
          "form": "키보드 쓸 때 해봐야겠어요ㅎㅎ",
          "original_form": "키보드 쓸 때 해봐야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 04:41"
        },
        {
          "id": "MDRW2100038261.1.9",
          "form": "네 만약에 그래도 안된다면 그 키보드가 안되는걸로...ㅎ",
          "original_form": "네 만약에 그래도 안된다면 그 키보드가 안되는걸로...ㅎ",
          "speaker_id": "1",
          "time": "20210902 04:41"
        },
        {
          "id": "MDRW2100038261.1.10",
          "form": "ㅋㅋㅋㅋ잘 작동했으면 좋겠네요ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋ잘 작동했으면 좋겠네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 04:42"
        },
        {
          "id": "MDRW2100038261.1.11",
          "form": "키보드 기종 + 단축키 검색해보면 유용한거 많아요ㅋㅋ",
          "original_form": "키보드 기종 + 단축키 검색해보면 유용한거 많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 04:42"
        },
        {
          "id": "MDRW2100038261.1.12",
          "form": "검색해보고 좀 익숙해져봐야겠어요",
          "original_form": "검색해보고 좀 익숙해져봐야겠어요",
          "speaker_id": "2",
          "time": "20210902 04:43"
        },
        {
          "id": "MDRW2100038261.1.13",
          "form": "전 home이랑 end키 잘 쓰거든요 뭔킨지 아세요?",
          "original_form": "전 home이랑 end키 잘 쓰거든요 뭔킨지 아세요?",
          "speaker_id": "1",
          "time": "20210902 04:43"
        },
        {
          "id": "MDRW2100038261.1.14",
          "form": "홈은 메인 화면 가고 엔드는 창 닫아지나요?",
          "original_form": "홈은 메인 화면 가고 엔드는 창 닫아지나요?",
          "speaker_id": "2",
          "time": "20210902 04:44"
        },
        {
          "id": "MDRW2100038261.1.15",
          "form": "아니여 문장 앞으로 커서가고 문장 뒤로 커서가는거요ㅎ",
