      "utterance": [
        {
          "id": "MDRW2100038853.1.1",
          "form": "다이어트 하세용?",
          "original_form": "다이어트 하세용?",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100038853.1.2",
          "form": "네 요즘 하고 있어요",
          "original_form": "네 요즘 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038853.1.3",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038853.1.4",
          "form": "전 시간도 없고 그냥 산책 다녀와요 ㅠㅠ",
          "original_form": "전 시간도 없고 그냥 산책 다녀와요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100038853.1.5",
          "form": "식단 관리도 하세요?",
          "original_form": "식단 관리도 하세요?",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100038853.1.6",
          "form": "저도 1~2시간씩 걷고 뛰고 유산소만 해요",
          "original_form": "저도 1~2시간씩 걷고 뛰고 유산소만 해요",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100038853.1.7",
          "form": "식단도 하고 있어요",
          "original_form": "식단도 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100038853.1.8",
          "form": "우아 너무 대단하세요 ㅠㅜ",
          "original_form": "우아 너무 대단하세요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038853.1.9",
          "form": "식단은 채소 위주로 드시나요??",
          "original_form": "식단은 채소 위주로 드시나요??",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038853.1.10",
          "form": "일반식인데 탄수화물 양을 원래 먹던 양의 1/3로 줄이고 그만큼 채소 먹고 있어요",
          "original_form": "일반식인데 탄수화물 양을 원래 먹던 양의 1/3로 줄이고 그만큼 채소 먹고 있어요",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038853.1.11",
          "form": "와 삼분의일 ㅠㅠ 말만 들어도 허기지네요 ㅜ",
          "original_form": "와 삼분의일 ㅠㅠ 말만 들어도 허기지네요 ㅜ",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038853.1.12",
          "form": "그만큼 채소 먹고 있어서 괜찮았어요ㅋㅋㅋ",
          "original_form": "그만큼 채소 먹고 있어서 괜찮았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038853.1.13",
          "form": "양상추 거의 한 바가지씩 먹어요ㅋㅋㅋ",
          "original_form": "양상추 거의 한 바가지씩 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038853.1.14",
          "form": "채소는 소스 뿌려서 드시나요??",
          "original_form": "채소는 소스 뿌려서 드시나요??",
          "speaker_id": "2",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038853.1.15",
          "form": "드레싱을 별로 안 좋아해서 그냥 먹어요",
          "original_form": "드레싱을 별로 안 좋아해서 그냥 먹어요",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038853.1.16",
          "form": "우아 저는 종류별로 범벅해서 먹는데 ㅠㅠ 채소 먹으면 건강에 되게 좋을거같아요 ㅠㅠ",
          "original_form": "우아 저는 종류별로 범벅해서 먹는데 ㅠㅠ 채소 먹으면 건강에 되게 좋을거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038853.1.17",
          "form": "확실히 자극적인 거 많이 먹다가",
          "original_form": "확실히 자극적인 거 많이 먹다가",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038853.1.18",
          "form": "식단하니까 뭔가 건강해진 느낌이에요",
          "original_form": "식단하니까 뭔가 건강해진 느낌이에요",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
