      "utterance": [
        {
          "id": "MDRW2100021726.1.1",
          "form": "요즘 머 보세요?",
          "original_form": "요즘 머 보세요?",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021726.1.2",
          "form": "지금은 유퀴즈보고있어요 ㅎㅎ",
          "original_form": "지금은 유퀴즈보고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021726.1.3",
          "form": "유퀴즈 오늘 하는 날 아니지 않아요?",
          "original_form": "유퀴즈 오늘 하는 날 아니지 않아요?",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021726.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021726.1.5",
          "form": "놀면뭐하니 ....",
          "original_form": "놀면뭐하니 ....",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021726.1.6",
          "form": "ㅋㅋㅋ ㅠ 헷갈렸어요",
          "original_form": "ㅋㅋㅋ ㅠ 헷갈렸어요",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021726.1.7",
          "form": "앜ㅋㅋㅋㅋ유재석이 나오니 그럴 수도 있죠",
          "original_form": "앜ㅋㅋㅋㅋ유재석이 나오니 그럴 수도 있죠",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021726.1.8",
          "form": "ㅋㅋㅋㅋㅋㅌㅋ ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021726.1.9",
          "form": "자주 실수하능거같아요",
          "original_form": "자주 실수하능거같아요",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021726.1.10",
          "form": "괜찮아요 ㅋㅋㅋ",
          "original_form": "괜찮아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021726.1.11",
          "form": "좋아하는 프로그램있으세요?",
          "original_form": "좋아하는 프로그램있으세요?",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021726.1.12",
          "form": "놀토랑 대탈출 좋아해요",
          "original_form": "놀토랑 대탈출 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021726.1.13",
          "form": "놀토!!!",
          "original_form": "놀토!!!",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021726.1.14",
          "form": "유투브로 종종봐요",
          "original_form": "유투브로 종종봐요",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021726.1.15",
          "form": "꿀잼",
          "original_form": "꿀잼",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021726.1.16",
          "form": "맞아요 봤던 것도 클립으로 보게 되요",
          "original_form": "맞아요 봤던 것도 클립으로 보게 되요",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021726.1.17",
          "form": "옛날에 혜리 있을때",
          "original_form": "옛날에 혜리 있을때",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021726.1.18",
          "form": "혜리 먹는거 구경하는것도 재밌었어요",
          "original_form": "혜리 먹는거 구경하는것도 재밌었어요",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021726.1.19",
          "form": "먹방을 좋아하지는 않는데 ㅎㅖ리는 너무 깔끔하게 잘먹어서 ㅎㅎ",
          "original_form": "먹방을 좋아하지는 않는데 ㅎㅖ리는 너무 깔끔하게 잘먹어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021726.1.20",
          "form": "입이 크더라고요 ㅋㅋㅋ 혼자 먹을 땐 몰랐는데",
          "original_form": "입이 크더라고요 ㅋㅋㅋ 혼자 먹을 땐 몰랐는데",
          "speaker_id": "1",
          "time": "20210828 22:29"
        }
      ]
