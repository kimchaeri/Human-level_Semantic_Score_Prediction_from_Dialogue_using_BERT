        {
          "id": "MDRW2100001760.1.4",
          "form": "실은..그닥 ㅎㅎ",
          "original_form": "실은..그닥 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100001760.1.5",
          "form": "저는 코로나 때문에 홈트 ㅎㅎ",
          "original_form": "저는 코로나 때문에 홈트 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001760.1.6",
          "form": "그래도 하려고 노력합니다",
          "original_form": "그래도 하려고 노력합니다",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001760.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001760.1.8",
          "form": "그래서 요즘 홈 트 제품이 되게 많이 팔렸다고 하더라고요",
          "original_form": "그래서 요즘 홈 트 제품이 되게 많이 팔렸다고 하더라고요",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001760.1.9",
          "form": "집에만 있으니 살이 너무 쪄서요",
          "original_form": "집에만 있으니 살이 너무 쪄서요",
          "speaker_id": "1",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001760.1.10",
          "form": "네 ㅎ 저도요",
          "original_form": "네 ㅎ 저도요",
          "speaker_id": "2",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001760.1.11",
          "form": "안 그래도 움직이기 싫어하는데 코로나 핑계로 더 그럽니다",
          "original_form": "안 그래도 움직이기 싫어하는데 코로나 핑계로 더 그럽니다",
          "speaker_id": "2",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001760.1.12",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210819 18:43"
        },
        {
