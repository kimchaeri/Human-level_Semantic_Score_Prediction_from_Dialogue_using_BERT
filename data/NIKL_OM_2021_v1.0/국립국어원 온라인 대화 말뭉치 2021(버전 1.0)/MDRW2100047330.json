      "utterance": [
        {
          "id": "MDRW2100047330.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100047330.1.2",
          "form": "안녕하세요 요즘 자주보는 티비있나요",
          "original_form": "안녕하세요 요즘 자주보는 티비있나요",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100047330.1.3",
          "form": "방송이요",
          "original_form": "방송이요",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100047330.1.4",
          "form": "노는 언니봐요",
          "original_form": "노는 언니봐요",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100047330.1.5",
          "form": "아.박세리 나오는거요",
          "original_form": "아.박세리 나오는거요",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100047330.1.6",
          "form": "ㅎㅎ박세리 너무 유쾌하세요",
          "original_form": "ㅎㅎ박세리 너무 유쾌하세요",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100047330.1.7",
          "form": "맞아요ㅋㅋ 재밌어요",
          "original_form": "맞아요ㅋㅋ 재밌어요",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100047330.1.8",
          "form": "전 요즘 돌싱글즈 봤는데 나름 볼만하더라구요",
          "original_form": "전 요즘 돌싱글즈 봤는데 나름 볼만하더라구요",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100047330.1.9",
          "form": "누구나오나요",
          "original_form": "누구나오나요",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100047330.1.10",
          "form": "일반인... 돌싱들...소개팅이요",
          "original_form": "일반인... 돌싱들...소개팅이요",
          "speaker_id": "2",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100047330.1.11",
          "form": "아 ㅋ ㅋ",
          "original_form": "아 ㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100047330.1.12",
          "form": "다들 개성있고 잼있게봤어요",
          "original_form": "다들 개성있고 잼있게봤어요",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100047330.1.13",
          "form": "드라마는 뭐보세요?",
          "original_form": "드라마는 뭐보세요?",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100047330.1.14",
          "form": "안봐요 님은요?",
          "original_form": "안봐요 님은요?",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100047330.1.15",
          "form": "요즘은 딱히 볼게없어서...",
          "original_form": "요즘은 딱히 볼게없어서...",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100047330.1.16",
          "form": "밥잘사주는예쁜누나 보고 있어요",
          "original_form": "밥잘사주는예쁜누나 보고 있어요",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100047330.1.17",
