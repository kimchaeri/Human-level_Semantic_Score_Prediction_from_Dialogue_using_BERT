      "utterance": [
        {
          "id": "MDRW2100035647.1.1",
          "form": "혹시 제모 받아보신적 있으세용?",
          "original_form": "혹시 제모 받아보신적 있으세용?",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100035647.1.2",
          "form": "저 레이저 제모요",
          "original_form": "저 레이저 제모요",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035647.1.3",
          "form": "지져버렸습니다",
          "original_form": "지져버렸습니다",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035647.1.4",
          "form": "저도 5년 전에",
          "original_form": "저도 5년 전에",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035647.1.5",
          "form": "겨드랑이 5회 받았는데",
          "original_form": "겨드랑이 5회 받았는데",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035647.1.6",
          "form": "아ㅜㅜ 계속 나요~~",
          "original_form": "아ㅜㅜ 계속 나요~~",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035647.1.7",
          "form": "그져ㅠ",
          "original_form": "그져ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035647.1.8",
          "form": "저도ㅠ",
          "original_form": "저도ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100035647.1.9",
          "form": "5회했는데",
          "original_form": "5회했는데",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035647.1.10",
          "form": "나더라고요",
          "original_form": "나더라고요",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035647.1.11",
          "form": "원래 10회는 받야야한다고 하더라구용",
          "original_form": "원래 10회는 받야야한다고 하더라구용",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035647.1.12",
          "form": "5회받다가 피부과 가는게 도저히 귀찮아서 관뒀는뎈ㅋㅋㅋ",
          "original_form": "5회받다가 피부과 가는게 도저히 귀찮아서 관뒀는뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035647.1.13",
          "form": "ㅋㅋ저는 지금 팔다리 7회째인가 그래요",
          "original_form": "ㅋㅋ저는 지금 팔다리 7회째인가 그래요",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035647.1.14",
          "form": "팔이랑 다리면",
          "original_form": "팔이랑 다리면",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.15",
          "form": "아주 클린은 아니지만 만족합니다",
          "original_form": "아주 클린은 아니지만 만족합니다",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.16",
          "form": "으허 어디가 더 아파요?ㅠㅠㅠ",
          "original_form": "으허 어디가 더 아파요?ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.17",
          "form": "저 겨드랑이 받다가 눈물고였는뎈ㅋㅋㅋ",
          "original_form": "저 겨드랑이 받다가 눈물고였는뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.18",
          "form": "전 안아팠어요!",
          "original_form": "전 안아팠어요!",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.19",
          "form": "둘 다요?",
          "original_form": "둘 다요?",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.20",
          "form": "그냥 으음~따끔쓰~ 이정도?",
          "original_form": "그냥 으음~따끔쓰~ 이정도?",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.21",
          "form": "아 다행이네용",
          "original_form": "아 다행이네용",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.22",
          "form": "네ㅋㅋㅋ둔한가봐요",
          "original_form": "네ㅋㅋㅋ둔한가봐요",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.23",
          "form": "두개 한번에 끊으신거에용?",
          "original_form": "두개 한번에 끊으신거에용?",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.24",
          "form": "가격은 어느정도 하나용?",
          "original_form": "가격은 어느정도 하나용?",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035647.1.25",
          "form": "네 행사하길래요",
          "original_form": "네 행사하길래요",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035647.1.26",
          "form": "아항 여름에 행사 많이하던데 저도 그때 시술권 끊을걸 그랬어요ㅠ",
          "original_form": "아항 여름에 행사 많이하던데 저도 그때 시술권 끊을걸 그랬어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035647.1.27",
          "form": "팔 다리 인중 해서 42줬던거같아요",
          "original_form": "팔 다리 인중 해서 42줬던거같아요",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035647.1.28",
          "form": "오 가격 괜찮네요",
          "original_form": "오 가격 괜찮네요",
          "speaker_id": "1",
          "time": "20210829 21:56"
        }
      ]
    }
  ]
