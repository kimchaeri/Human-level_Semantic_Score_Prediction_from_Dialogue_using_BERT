      "utterance": [
        {
          "id": "MDRW2100005463.1.1",
          "form": "미드 추천해줄래?",
          "original_form": "미드 추천해줄래?",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005463.1.2",
          "form": "와이우먼 킬 추천해",
          "original_form": "와이우먼 킬 추천해",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005463.1.3",
          "form": "무슨 내용이야?",
          "original_form": "무슨 내용이야?",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005463.1.4",
          "form": "제목 그대로 여자들이 왜 죽이는지 그ㅓ",
          "original_form": "제목 그대로 여자들이 왜 죽이는지 그ㅓ",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005463.1.5",
          "form": "ㅓㄴ ㅇ",
          "original_form": "ㅓㄴ ㅇ",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005463.1.6",
          "form": "그런 이야기인데",
          "original_form": "그런 이야기인데",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005463.1.7",
          "form": "왜 죽이는 건데?",
          "original_form": "왜 죽이는 건데?",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005463.1.8",
          "form": "이유가 있어 그 이유를 보여주는 이야기야",
          "original_form": "이유가 있어 그 이유를 보여주는 이야기야",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005463.1.9",
          "form": "무서운 거야?",
          "original_form": "무서운 거야?",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005463.1.10",
          "form": "아니 무섭지 않아 ㅋㅋ 가볍게 볼 수 있어",
          "original_form": "아니 무섭지 않아 ㅋㅋ 가볍게 볼 수 있어",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005463.1.11",
          "form": "나중에 봐봐야겠다",
          "original_form": "나중에 봐봐야겠다",
          "speaker_id": "2",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005463.1.12",
          "form": "봐봐 인기 엄청 많았어",
          "original_form": "봐봐 인기 엄청 많았어",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005463.1.13",
          "form": "오 시즌 2 지금 하고 있네",
          "original_form": "오 시즌 2 지금 하고 있네",
          "speaker_id": "2",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005463.1.14",
          "form": "응 인기 많아서 시즌2도 나왔는데 시즌1이랑 완전 다른 이야기 같더라",
          "original_form": "응 인기 많아서 시즌2도 나왔는데 시즌1이랑 완전 다른 이야기 같더라",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005463.1.15",
          "form": "시즌 2도 봤어?",
          "original_form": "시즌 2도 봤어?",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005463.1.16",
          "form": "시즌2는 1화만 봤는데 내 취향 아니라 그 후로는 안보는 중",
          "original_form": "시즌2는 1화만 봤는데 내 취향 아니라 그 후로는 안보는 중",
          "speaker_id": "1",
          "time": "20210821 22:33"
        }
