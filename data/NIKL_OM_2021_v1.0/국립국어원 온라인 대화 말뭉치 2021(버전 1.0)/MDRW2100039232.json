      "utterance": [
        {
          "id": "MDRW2100039232.1.1",
          "form": "주말에 뭐 할 거니",
          "original_form": "주말에 뭐 할 거니",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039232.1.2",
          "form": "주말에 친구 놀러온다고하던데 아직 정해지지는 않았지",
          "original_form": "주말에 친구 놀러온다고하던데 아직 정해지지는 않았지",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039232.1.3",
          "form": "언니네는 뭐해?",
          "original_form": "언니네는 뭐해?",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039232.1.4",
          "form": "뭐 친구 놀러 오나??",
          "original_form": "뭐 친구 놀러 오나??",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039232.1.5",
          "form": "대단하다 정말",
          "original_form": "대단하다 정말",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039232.1.6",
          "form": "우린 아직 뭐 할진 모르겠네",
          "original_form": "우린 아직 뭐 할진 모르겠네",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039232.1.7",
          "form": "근데 친구가  지난주에 놀러와도되냐고캐가",
          "original_form": "근데 친구가  지난주에 놀러와도되냐고캐가",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039232.1.8",
          "form": "올만이라서 오라곤햇는데",
          "original_form": "올만이라서 오라곤햇는데",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039232.1.9",
          "form": "비와서",
          "original_form": "비와서",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039232.1.10",
          "form": "취소했거든",
          "original_form": "취소했거든",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039232.1.11",
          "form": "그리고 시간대도 안맞아서ㅋㅋ",
          "original_form": "그리고 시간대도 안맞아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100039232.1.12",
          "form": "주말에 집에 오면",
          "original_form": "주말에 집에 오면",
          "speaker_id": "1",
          "time": "20210903 15:46"
        },
        {
          "id": "MDRW2100039232.1.13",
          "form": "힘들겠는데",
          "original_form": "힘들겠는데",
          "speaker_id": "1",
          "time": "20210903 15:46"
        },
        {
          "id": "MDRW2100039232.1.14",
          "form": "하루 종일 놀다 가는 거 아닌가ㅋㅋㅋ",
          "original_form": "하루 종일 놀다 가는 거 아닌가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:46"
        },
        {
          "id": "MDRW2100039232.1.15",
          "form": "응 그친구는 하루종일 놀다가서ㅋㅋㅋ",
          "original_form": "응 그친구는 하루종일 놀다가서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100039232.1.16",
          "form": "근데 시간대가 좀 안맞다ㅜㅋㅋㅋㅋ나는 오후부터놀고싶어하고ㅋㅋ 그친구는 오전부터놀고싶어하고ㅋㅋ",
          "original_form": "근데 시간대가 좀 안맞다ㅜㅋㅋㅋㅋ나는 오후부터놀고싶어하고ㅋㅋ 그친구는 오전부터놀고싶어하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100039232.1.17",
          "form": "저번에 그래가 2시에오라캤더니 ㅋㅋ 패스하더라고",
          "original_form": "저번에 그래가 2시에오라캤더니 ㅋㅋ 패스하더라고",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100039232.1.18",
          "form": "하루종일 놀기에는 내가 피곤해서 못하겠더라고. 울신랑은 더 체력안되고ㅋㅋㄲㅋㅋㅋㅋㅋㅋㅈㅋㅋ",
          "original_form": "하루종일 놀기에는 내가 피곤해서 못하겠더라고. 울신랑은 더 체력안되고ㅋㅋㄲㅋㅋㅋㅋㅋㅋㅈㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039232.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱㅋㄱㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039232.1.20",
          "form": "오전부터라니\r\nㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오전부터라니\r\nㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039232.1.21",
          "form": "패스 웃기노 ㅋㅋㅋㅋ",
          "original_form": "패스 웃기노 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039232.1.22",
          "form": "그래 나도 오전부터는 힘들다",
          "original_form": "그래 나도 오전부터는 힘들다",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039232.1.23",
          "form": "모든 약속은 점심이후",
          "original_form": "모든 약속은 점심이후",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039232.1.24",
          "form": "밥먹었나ㅋㅋ",
          "original_form": "밥먹었나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100039232.1.25",
          "form": "어 밥 먹고 애들  씻기고",
          "original_form": "어 밥 먹고 애들  씻기고",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039232.1.26",
          "form": "설거지 하고",
          "original_form": "설거지 하고",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039232.1.27",
          "form": "나도 이제 씻으려고",
          "original_form": "나도 이제 씻으려고",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039232.1.28",
          "form": "응ㅋㅋ언니 어여씻고 육퇴하자ㅋ",
          "original_form": "응ㅋㅋ언니 어여씻고 육퇴하자ㅋ",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039232.1.29",
          "form": "파이팅",
          "original_form": "파이팅",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100039232.1.30",
          "form": "육퇴했나??",
          "original_form": "육퇴했나??",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
