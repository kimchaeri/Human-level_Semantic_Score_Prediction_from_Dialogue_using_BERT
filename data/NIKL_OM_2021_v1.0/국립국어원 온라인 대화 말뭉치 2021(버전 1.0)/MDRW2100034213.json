      "utterance": [
        {
          "id": "MDRW2100034213.1.1",
          "form": "요새 날이 넘 선선해졌더라구요",
          "original_form": "요새 날이 넘 선선해졌더라구요",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100034213.1.2",
          "form": "마자요 가을옷 딱 맞춰서 잘 사셨네요",
          "original_form": "마자요 가을옷 딱 맞춰서 잘 사셨네요",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034213.1.3",
          "form": "오늘이 진짜 시원하더라구요",
          "original_form": "오늘이 진짜 시원하더라구요",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034213.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034213.1.5",
          "form": "저녁엔 완전 가을이에요",
          "original_form": "저녁엔 완전 가을이에요",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034213.1.6",
          "form": "이제 진짜 좀만더 있으면 가을되겠어요",
          "original_form": "이제 진짜 좀만더 있으면 가을되겠어요",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034213.1.7",
          "form": "맞아요 땀나는거 너무 싫은데",
          "original_form": "맞아요 땀나는거 너무 싫은데",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034213.1.8",
          "form": "저녁엔 진짜 창문열면  추울정도 더라구요",
          "original_form": "저녁엔 진짜 창문열면  추울정도 더라구요",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034213.1.9",
          "form": "얼른되면 좋겠어요",
          "original_form": "얼른되면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034213.1.10",
          "form": "저두요ㅠ",
          "original_form": "저두요ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034213.1.11",
          "form": "맞아요 ㅋㅋㅋ 원래 열고 살았거든요",
          "original_form": "맞아요 ㅋㅋㅋ 원래 열고 살았거든요",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034213.1.12",
          "form": "딱 오늘정도가 좋은거같아요",
          "original_form": "딱 오늘정도가 좋은거같아요",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034213.1.13",
          "form": "맞아요 선선하니 비도 안오구 ㅎㅎ",
          "original_form": "맞아요 선선하니 비도 안오구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034213.1.14",
          "form": "요새는 새벽에 추울까봐 못열고 자겠더라구요",
          "original_form": "요새는 새벽에 추울까봐 못열고 자겠더라구요",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034213.1.15",
          "form": "저는 괜찮은데 가족은 춥다고 꼭 닫고 자더라구요 ㅋㅋㅋ",
          "original_form": "저는 괜찮은데 가족은 춥다고 꼭 닫고 자더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:21"
        }
