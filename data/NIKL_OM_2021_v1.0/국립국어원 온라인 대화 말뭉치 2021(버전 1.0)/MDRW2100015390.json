      "utterance": [
        {
          "id": "MDRW2100015390.1.1",
          "form": "다이어트하세요?",
          "original_form": "다이어트하세요?",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015390.1.2",
          "form": "아뇨  그냥 현상유지만 합니다",
          "original_form": "아뇨  그냥 현상유지만 합니다",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015390.1.3",
          "form": "name1님은 따로 운동하시나요?",
          "original_form": "&name1&님은 따로 운동하시나요?",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015390.1.4",
          "form": "저는 그냥 동네만 걷고있어요",
          "original_form": "저는 그냥 동네만 걷고있어요",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015390.1.5",
          "form": "name2님은 운동하세요?",
          "original_form": "&name2&님은 운동하세요?",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015390.1.6",
          "form": "숨쉬기 운동은 합니다ㅎ",
          "original_form": "숨쉬기 운동은 합니다ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015390.1.7",
          "form": "하루에 얼마 걸으세요?",
          "original_form": "하루에 얼마 걸으세요?",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015390.1.8",
          "form": "ㅎㅎㅎ 한시간정도요",
          "original_form": "ㅎㅎㅎ 한시간정도요",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015390.1.9",
          "form": "요즘 코로나땜에",
          "original_form": "요즘 코로나땜에",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015390.1.10",
          "form": "헬스장은 못가겠더라구요",
          "original_form": "헬스장은 못가겠더라구요",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015390.1.11",
          "form": "헉 한시간이나 대단하네요",
          "original_form": "헉 한시간이나 대단하네요",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015390.1.12",
          "form": "헬스장도 원래는 다니셨나요?",
          "original_form": "헬스장도 원래는 다니셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015390.1.13",
          "form": "코로나전에 짧게 삼개월정도요 ᆢ",
          "original_form": "코로나전에 짧게 삼개월정도요 ᆢ",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015390.1.14",
          "form": "무서워서 못가겠더라구요",
          "original_form": "무서워서 못가겠더라구요",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015390.1.15",
          "form": "운동하실 생각은 없으신거예요?",
          "original_form": "운동하실 생각은 없으신거예요?",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015390.1.16",
          "form": "PT하셨나요?",
          "original_form": "PT하셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015390.1.17",
          "form": "체력 길러야겠다는 생각은 하는데요",
          "original_form": "체력 길러야겠다는 생각은 하는데요",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015390.1.18",
          "form": "아뇨 pt 너무 비싸서 못했어요",
          "original_form": "아뇨 pt 너무 비싸서 못했어요",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015390.1.19",
          "form": "생각만 하죠ㅜ",
          "original_form": "생각만 하죠ㅜ",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015390.1.20",
          "form": "그쵸 저도 체력이 딸려서 억지로해요ㅜ",
          "original_form": "그쵸 저도 체력이 딸려서 억지로해요ㅜ",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015390.1.21",
          "form": "아 안배우고 헬스해도 괜찮나 싶어서요",
          "original_form": "아 안배우고 헬스해도 괜찮나 싶어서요",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015390.1.22",
          "form": "전 완전 운동 알못이라",
          "original_form": "전 완전 운동 알못이라",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015390.1.23",
          "form": "저도 알못이예요",
          "original_form": "저도 알못이예요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015390.1.24",
          "form": "그냥 근력운동보다",
          "original_form": "그냥 근력운동보다",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015390.1.25",
          "form": "유산소만 대부분해서 ㅎㅎ",
          "original_form": "유산소만 대부분해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015390.1.26",
          "form": "런닝머신같은거요",
          "original_form": "런닝머신같은거요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        }
      ]
