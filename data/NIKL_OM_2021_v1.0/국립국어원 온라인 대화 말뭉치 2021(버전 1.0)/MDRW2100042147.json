      "utterance": [
        {
          "id": "MDRW2100042147.1.1",
          "form": "요즘 넷플릭스를 엄청 이용하고 있어요ㅋㅋㅋㅋ",
          "original_form": "요즘 넷플릭스를 엄청 이용하고 있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100042147.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 좋네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 좋네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100042147.1.3",
          "form": "저도 집가면 또 봐야겠어요ㅋㅋㅋㅋㅋ",
          "original_form": "저도 집가면 또 봐야겠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042147.1.4",
          "form": "오늘은 뭐보실건가요?",
          "original_form": "오늘은 뭐보실건가요?",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100042147.1.5",
          "form": "아 저는 일단 과제가 있어서 과제 다하고 유튜브나 한바퀴 돌까 합니닷",
          "original_form": "아 저는 일단 과제가 있어서 과제 다하고 유튜브나 한바퀴 돌까 합니닷",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042147.1.6",
          "form": "방금 하이킥이 보고 싶어졌어요",
          "original_form": "방금 하이킥이 보고 싶어졌어요",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042147.1.7",
          "form": "지하철 옆자리사람이",
          "original_form": "지하철 옆자리사람이",
          "speaker_id": "2",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042147.1.8",
          "form": "하이킥을 보고 있더라구요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "하이킥을 보고 있더라구요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042147.1.9",
          "form": "보고 있어요?",
          "original_form": "보고 있어요?",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042147.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042147.1.11",
          "form": "옆에서 누가 보면 보고 싶죠 맞아요",
          "original_form": "옆에서 누가 보면 보고 싶죠 맞아요",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042147.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ하이킥......도자기 깬 편을 보고 싶어졌어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ하이킥......도자기 깬 편을 보고 싶어졌어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042147.1.13",
          "form": "하이킥 좋아하셨어요? 즐겨 보셨나요?",
          "original_form": "하이킥 좋아하셨어요? 즐겨 보셨나요?",
          "speaker_id": "1",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042147.1.14",
          "form": "네!!! 너무 재밌게 봤습니다ㅎㅎ",
          "original_form": "네!!! 너무 재밌게 봤습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042147.1.15",
          "form": "하이킥..... 진짜 그런 시트콤 요즘에 안해서 넘 아쉬워요ㅠㅜㅜ",
          "original_form": "하이킥..... 진짜 그런 시트콤 요즘에 안해서 넘 아쉬워요ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042147.1.16",
          "form": "아 마자요ㅠㅜㅜㅜ 티비보는 시청자들이 줄어서 안해주는걸까요",
          "original_form": "아 마자요ㅠㅜㅜㅜ 티비보는 시청자들이 줄어서 안해주는걸까요",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042147.1.17",
          "form": "전에는 잘 해주더니 이젠 안하나봐요ㅠ",
          "original_form": "전에는 잘 해주더니 이젠 안하나봐요ㅠ",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042147.1.18",
          "form": "ㅜㅜㅠㅠㅠ시트콤보단 좀 자극적이고 강렬한 드라마가 유행인 것 같아요",
          "original_form": "ㅜㅜㅠㅠㅠ시트콤보단 좀 자극적이고 강렬한 드라마가 유행인 것 같아요",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042147.1.19",
          "form": "name1님 말씀대로 티비를 잘 안보기도 하고...ㅜㅠㅠ",
          "original_form": "&name1&님 말씀대로 티비를 잘 안보기도 하고...ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042147.1.20",
          "form": "name2님 그거 뭐야",
          "original_form": "&name2&님 그거 뭐야",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042147.1.21",
          "form": "펜트하우스!!!",
          "original_form": "펜트하우스!!!",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042147.1.22",
          "form": "보세요?",
          "original_form": "보세요?",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042147.1.23",
          "form": "아 전 그건 안봤어요!! name1님은요~??",
          "original_form": "아 전 그건 안봤어요!! &name1&님은요~??",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042147.1.24",
          "form": "전 부부의 세계만 봤습니다ㅋㅋ 넘 옛날인가요?ㅎㅎ",
          "original_form": "전 부부의 세계만 봤습니다ㅋㅋ 넘 옛날인가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042147.1.25",
          "form": "저 안보는데 얼마전에 채널 돌리다가 봤는데",
          "original_form": "저 안보는데 얼마전에 채널 돌리다가 봤는데",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042147.1.26",
          "form": "너무 자극적이라 진짜 놀랐어요",
          "original_form": "너무 자극적이라 진짜 놀랐어요",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042147.1.27",
          "form": "공포영화보고 트라우마 생기는것처럼",
          "original_form": "공포영화보고 트라우마 생기는것처럼",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042147.1.28",
          "form": "약간 그런 느낌 들정도로요",
          "original_form": "약간 그런 느낌 들정도로요",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042147.1.29",
          "form": "많이 자극적인가요??",
          "original_form": "많이 자극적인가요??",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042147.1.30",
          "form": "기사에서만 봐서",
          "original_form": "기사에서만 봐서",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042147.1.31",
          "form": "어떤지 잘 몰라요",
          "original_form": "어떤지 잘 몰라요",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042147.1.32",
          "form": "2층에서 사람 떨어지고 떨어진후에 샹들리에가 떨어진사람한테 떨어져서",
          "original_form": "2층에서 사람 떨어지고 떨어진후에 샹들리에가 떨어진사람한테 떨어져서",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042147.1.33",
          "form": "온통 피범벅이요..",
          "original_form": "온통 피범벅이요..",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042147.1.34",
          "form": "ㅇ0ㅇ",
          "original_form": "ㅇ0ㅇ",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042147.1.35",
          "form": "그냥 피만 보여주는것도아니고",
          "original_form": "그냥 피만 보여주는것도아니고",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042147.1.36",
          "form": "주인공 얼굴이고 바닥이고",
          "original_form": "주인공 얼굴이고 바닥이고",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042147.1.37",
          "form": "세상에 텍스트로봐도 충격인데요",
          "original_form": "세상에 텍스트로봐도 충격인데요",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042147.1.38",
          "form": "피 튀기고..",
          "original_form": "피 튀기고..",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042147.1.39",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042147.1.40",
          "form": "저진짜 놀래서 바로 채널 돌렸어요",
          "original_form": "저진짜 놀래서 바로 채널 돌렸어요",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042147.1.41",
          "form": "ㅇ0ㅇ",
          "original_form": "ㅇ0ㅇ",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042147.1.42",
          "form": "근데 점점 미디어가 자극적이어야 인기가 있어지는 것 같긴해요",
          "original_form": "근데 점점 미디어가 자극적이어야 인기가 있어지는 것 같긴해요",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042147.1.43",
          "form": "넷플릭스도",
          "original_form": "넷플릭스도",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042147.1.44",
          "form": "되게 자극적인 것들이 많은 것 같아요",
          "original_form": "되게 자극적인 것들이 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042147.1.45",
          "form": "근데 펜트하우스는 그정도인지 몰랐어요",
          "original_form": "근데 펜트하우스는 그정도인지 몰랐어요",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042147.1.46",
          "form": "다음날 보니까 그장면때문에 기사 나긴했더라구요",
