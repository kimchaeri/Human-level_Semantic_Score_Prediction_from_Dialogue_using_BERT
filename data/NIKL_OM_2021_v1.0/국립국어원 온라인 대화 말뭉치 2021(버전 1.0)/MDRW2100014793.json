      "utterance": [
        {
          "id": "MDRW2100014793.1.1",
          "form": "포매라니안 강아지",
          "original_form": "포매라니안 강아지",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014793.1.2",
          "form": "너무 이뻐요",
          "original_form": "너무 이뻐요",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014793.1.3",
          "form": "머가 이쁨",
          "original_form": "머가 이쁨",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014793.1.4",
          "form": "애기 사자 같아서",
          "original_form": "애기 사자 같아서",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014793.1.5",
          "form": "글쿤 ㅋ 털 미용하고싶은데 ㅠ",
          "original_form": "글쿤 ㅋ 털 미용하고싶은데 ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014793.1.6",
          "form": "사막여우 같기도 하고",
          "original_form": "사막여우 같기도 하고",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014793.1.7",
          "form": "ㅋㅋ 강아지가 강아지 안같고 다른 동물같으누",
          "original_form": "ㅋㅋ 강아지가 강아지 안같고 다른 동물같으누",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014793.1.8",
          "form": "어떤 때는 부엉이 같다",
          "original_form": "어떤 때는 부엉이 같다",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014793.1.9",
          "form": "ㅋㅋ 부엉이는 몰겠는뎅",
          "original_form": "ㅋㅋ 부엉이는 몰겠는뎅",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014793.1.10",
          "form": "털 빨이다",
          "original_form": "털 빨이다",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014793.1.11",
          "form": "털 좀 이뿌게 미용시켜주자",
          "original_form": "털 좀 이뿌게 미용시켜주자",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014793.1.12",
          "form": "자연 그대로가 좋아요",
          "original_form": "자연 그대로가 좋아요",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014793.1.13",
          "form": "됐어유 잘라도 다시 자연으로 돌아온당",
          "original_form": "됐어유 잘라도 다시 자연으로 돌아온당",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014793.1.14",
          "form": "미용해주기 싫다",
          "original_form": "미용해주기 싫다",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014793.1.15",
          "form": "그래 솔직히 돈아깝긴 함 ㅋ",
          "original_form": "그래 솔직히 돈아깝긴 함 ㅋ",
          "speaker_id": "1",
