      "utterance": [
        {
          "id": "MDRW2100037739.1.1",
          "form": "연애하고싶으세요?",
          "original_form": "연애하고싶으세요?",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100037739.1.2",
          "form": "휴.. 제친구 얘기좀 들어보실래요??",
          "original_form": "휴.. 제친구 얘기좀 들어보실래요??",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037739.1.3",
          "form": "제친구가",
          "original_form": "제친구가",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037739.1.4",
          "form": "돌싱남을 만나요",
          "original_form": "돌싱남을 만나요",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037739.1.5",
          "form": "근데 이혼이유가 궁금하잖아요",
          "original_form": "근데 이혼이유가 궁금하잖아요",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037739.1.6",
          "form": "그리고 그걸 알아야 자기도 결혼할지 말지 결정할 수 있고",
          "original_form": "그리고 그걸 알아야 자기도 결혼할지 말지 결정할 수 있고",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100037739.1.7",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037739.1.8",
          "form": "근데모르나요",
          "original_form": "근데모르나요",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037739.1.9",
          "form": "전부인이 시댁에 더이상 안갈거라고 그랬다네요",
          "original_form": "전부인이 시댁에 더이상 안갈거라고 그랬다네요",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100037739.1.10",
          "form": "근데 이걸 듣고!!!!",
          "original_form": "근데 이걸 듣고!!!!",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100037739.1.11",
          "form": "전부인이 이상해보이세요? 시댁이 이상해보이세요??",
          "original_form": "전부인이 이상해보이세요? 시댁이 이상해보이세요??",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100037739.1.12",
          "form": "시댁이요",
          "original_form": "시댁이요",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037739.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037739.1.14",
          "form": "의심가요",
          "original_form": "의심가요",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037739.1.15",
          "form": "매우매우",
          "original_form": "매우매우",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037739.1.16",
          "form": "도망쳐!!!!!",
          "original_form": "도망쳐!!!!!",
          "speaker_id": "2",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037739.1.17",
          "form": "그쵸!!! 시댁이 도대체 어땠길래...",
          "original_form": "그쵸!!! 시댁이 도대체 어땠길래...",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037739.1.18",
          "form": "그래서 친구도 좀 더 꼬치꼬치 물어봤죠",
          "original_form": "그래서 친구도 좀 더 꼬치꼬치 물어봤죠",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037739.1.19",
          "form": "전부인이 그렇게 말하게 된 이유가 뭐냐 상황이 있었을거아니냐",
          "original_form": "전부인이 그렇게 말하게 된 이유가 뭐냐 상황이 있었을거아니냐",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037739.1.20",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037739.1.21",
          "form": "그랬더니 이 남자가...걍 자기는 효도하고싶다고",
          "original_form": "그랬더니 이 남자가...걍 자기는 효도하고싶다고",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037739.1.22",
          "form": "자기가 이혼한거 자체가 너무 부모님깨 불효한거 같다고",
          "original_form": "자기가 이혼한거 자체가 너무 부모님깨 불효한거 같다고",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037739.1.23",
          "form": "은근슬쩍 말을 돌리며 눈물을 보이더래요",
          "original_form": "은근슬쩍 말을 돌리며 눈물을 보이더래요",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037739.1.24",
          "form": "이거 제친구 말려뜯어야하는거 맞죠??",
          "original_form": "이거 제친구 말려뜯어야하는거 맞죠??",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037739.1.25",
          "form": "네 완전 완전 무조건",
          "original_form": "네 완전 완전 무조건",
          "speaker_id": "2",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100037739.1.26",
          "form": "너무 시그널아닌가요",
          "original_form": "너무 시그널아닌가요",
          "speaker_id": "2",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100037739.1.27",
          "form": "그쵸?? 시댁사람들도 만만찮을것같아요",
          "original_form": "그쵸?? 시댁사람들도 만만찮을것같아요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037739.1.28",
          "form": "ㅁ지 부모 단속할 생각음 안하규",
          "original_form": "ㅁ지 부모 단속할 생각음 안하규",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037739.1.29",
          "form": "중요한 순간에 선물이 있어요 하더니",
          "original_form": "중요한 순간에 선물이 있어요 하더니",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037739.1.30",
          "form": "준게 뭔줄 알아요?",
          "original_form": "준게 뭔줄 알아요?",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037739.1.31",
          "form": "ㅋㅋㅋ그게머죠",
          "original_form": "ㅋㅋㅋ그게머죠",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100037739.1.32",
          "form": "기념품 샵에선 그립톡이었대요....",
          "original_form": "기념품 샵에선 그립톡이었대요....",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037739.1.33",
          "form": "샵에서 산",
          "original_form": "샵에서 산",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037739.1.34",
          "form": "이쁜걸로 사줘도 모자를판에 ㅎㅎ",
          "original_form": "이쁜걸로 사줘도 모자를판에 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037739.1.35",
          "form": "아앗....",
          "original_form": "아앗....",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100037739.1.36",
          "form": "끼고다니기도시른 ㅎ",
          "original_form": "끼고다니기도시른 ㅎ",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100037739.1.37",
          "form": "제가 속이 터집니다 정말~~ 이야기 들어주기도 지쳐요",
