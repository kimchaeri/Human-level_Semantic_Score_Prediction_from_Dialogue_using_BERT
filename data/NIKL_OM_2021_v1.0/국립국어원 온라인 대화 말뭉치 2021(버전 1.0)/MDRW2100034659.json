      "utterance": [
        {
          "id": "MDRW2100034659.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034659.1.2",
          "form": "안녕하세요ㅎ",
          "original_form": "안녕하세요ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034659.1.3",
          "form": "음식 뭐 좋아하세요?!",
          "original_form": "음식 뭐 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034659.1.4",
          "form": "전 매운음식요ㅎㅎ",
          "original_form": "전 매운음식요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034659.1.5",
          "form": "오 잘드시나봐요!",
          "original_form": "오 잘드시나봐요!",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034659.1.6",
          "form": "엽떡 좋아하세요?",
          "original_form": "엽떡 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100034659.1.7",
          "form": "넵ㅎ엽떡 좋아하죠",
          "original_form": "넵ㅎ엽떡 좋아하죠",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034659.1.8",
          "form": "오래전부터 먹엇어요",
          "original_form": "오래전부터 먹엇어요",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034659.1.9",
          "form": "무슨맛으로 드세요?",
          "original_form": "무슨맛으로 드세요?",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034659.1.10",
          "form": "예전엔 젤 매운맛ㅈ",
          "original_form": "예전엔 젤 매운맛ㅈ",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034659.1.11",
          "form": "지금은 중간에서 하나더 매운맛요ㅎ",
          "original_form": "지금은 중간에서 하나더 매운맛요ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034659.1.12",
          "form": "와 젤매운맛 드시는 분을 만나다니",
          "original_form": "와 젤매운맛 드시는 분을 만나다니",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100034659.1.13",
          "form": "담날에 배안아프세요??",
          "original_form": "담날에 배안아프세요??",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100034659.1.14",
          "form": "넵ㅎㅎ장은 튼튼한가봐요",
          "original_form": "넵ㅎㅎ장은 튼튼한가봐요",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034659.1.15",
          "form": "와 부러워요",
          "original_form": "와 부러워요",
          "speaker_id": "1",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034659.1.16",
          "form": "닭발 불닭 다 좋아해요",
          "original_form": "닭발 불닭 다 좋아해요",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034659.1.17",
          "form": "불닭! 맛있죠",
          "original_form": "불닭! 맛있죠",
          "speaker_id": "1",
          "time": "20210828 17:20"
        }
      ]
