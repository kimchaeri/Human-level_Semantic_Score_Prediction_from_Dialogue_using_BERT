        {
          "id": "MDRW2100014452.1.1",
          "form": "직업이다",
          "original_form": "직업이다",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014452.1.2",
          "form": "일 때문에 화가 났나?",
          "original_form": "일 때문에 화가 났나?",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014452.1.3",
          "form": "일은 언제나 하기 싫군",
          "original_form": "일은 언제나 하기 싫군",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014452.1.4",
          "form": "화났다고 하길래 직업으로 했어",
          "original_form": "화났다고 하길래 직업으로 했어",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014452.1.5",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014452.1.6",
          "form": "빨리 복권에 당첨돼야겠어",
          "original_form": "빨리 복권에 당첨돼야겠어",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014452.1.7",
          "form": "좀 더 홀가분한 마음으로 직장을 다닐 수 있을거 같다",
          "original_form": "좀 더 홀가분한 마음으로 직장을 다닐 수 있을거 같다",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014452.1.8",
          "form": "복권 사고있어?",
          "original_form": "복권 사고있어?",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014452.1.9",
          "form": "아니 안사고 있다",
          "original_form": "아니 안사고 있다",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014452.1.10",
          "form": "오늘 살까 고민 중이야",
          "original_form": "오늘 살까 고민 중이야",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014452.1.11",
          "form": "ㅋㅋㅋ 오늘 화가 많이 났나보다",
          "original_form": "ㅋㅋㅋ 오늘 화가 많이 났나보다",
          "speaker_id": "1",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014452.1.12",
          "form": "냐 완전 일을 망쳤쨔",
          "original_form": "냐 완전 일을 망쳤쨔",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014452.1.13",
          "form": "퇴근했어?",
          "original_form": "퇴근했어?",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014452.1.14",
          "form": "응 카톡에 썼었다",
          "original_form": "응 카톡에 썼었다",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014452.1.15",
          "form": "집에 도착했냐는 뜻이었다",
          "original_form": "집에 도착했냐는 뜻이었다",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
