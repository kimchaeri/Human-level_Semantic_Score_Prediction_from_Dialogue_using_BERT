      "utterance": [
        {
          "id": "MDRW2100018493.1.1",
          "form": "올림픽 보셨나요",
          "original_form": "올림픽 보셨나요",
          "speaker_id": "2",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100018493.1.2",
          "form": "클립 영상만 유튜브로 봤어용ㅋㅋ",
          "original_form": "클립 영상만 유튜브로 봤어용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100018493.1.3",
          "form": "ㅋㅋ ㅋㅋ 저도 몇 영상은 그랬어요",
          "original_form": "ㅋㅋ ㅋㅋ 저도 몇 영상은 그랬어요",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018493.1.4",
          "form": "뭐 보셨어요",
          "original_form": "뭐 보셨어요",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018493.1.5",
          "form": "수영이랑 근대5종 봤어요!",
          "original_form": "수영이랑 근대5종 봤어요!",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018493.1.6",
          "form": "아 펜싱도요ㅋㅋㅋ",
          "original_form": "아 펜싱도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018493.1.7",
          "form": "무슨 종목 재밌게 보셨어요?",
          "original_form": "무슨 종목 재밌게 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018493.1.8",
          "form": "오 저는",
          "original_form": "오 저는",
          "speaker_id": "2",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018493.1.9",
          "form": "배구랑 양궁이요 !!",
          "original_form": "배구랑 양궁이요 !!",
          "speaker_id": "2",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018493.1.10",
          "form": "가장 인기있던 종목으로 보셨네요ㅎㅎ",
          "original_form": "가장 인기있던 종목으로 보셨네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018493.1.11",
          "form": "네 ㅎㅎ 펜싱 어땠나요",
          "original_form": "네 ㅎㅎ 펜싱 어땠나요",
          "speaker_id": "2",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018493.1.12",
          "form": "펜싱도 재밌었다는데",
          "original_form": "펜싱도 재밌었다는데",
          "speaker_id": "2",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018493.1.13",
          "form": "펜싱은 짧게 짧게 봤는데",
          "original_form": "펜싱은 짧게 짧게 봤는데",
          "speaker_id": "1",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018493.1.14",
          "form": "끈기가 있어야하는 종목같아요",
          "original_form": "끈기가 있어야하는 종목같아요",
          "speaker_id": "1",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018493.1.15",
          "form": "전 보다 지쳐서ㅋㅋㅋ",
          "original_form": "전 보다 지쳐서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018493.1.16",
          "form": "ㅋㅋㅋㅋ아하 제 지인은",
          "original_form": "ㅋㅋㅋㅋ아하 제 지인은",
          "speaker_id": "2",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018493.1.17",
          "form": "재밌다고 하더라구요",
          "original_form": "재밌다고 하더라구요",
          "speaker_id": "2",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018493.1.18",
          "form": "긴장감이 계속 유지되는 부분은 있어요ㅎㅎ",
          "original_form": "긴장감이 계속 유지되는 부분은 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:20"
        },
        {
          "id": "MDRW2100018493.1.19",
          "form": "선수들 집중력이나 체력이 대단해요",
          "original_form": "선수들 집중력이나 체력이 대단해요",
          "speaker_id": "1",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018493.1.20",
          "form": "정말요ㅋㅋㅋㅋㅋ",
          "original_form": "정말요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018493.1.21",
          "form": "떨릴 거 같은데",
          "original_form": "떨릴 거 같은데",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018493.1.22",
          "form": "잘해요",
          "original_form": "잘해요",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018493.1.23",
          "form": "그쵸 0.1초 차이로도 메달이 바뀌고",
          "original_form": "그쵸 0.1초 차이로도 메달이 바뀌고",
          "speaker_id": "1",
          "time": "20210827 08:22"
        },
        {
          "id": "MDRW2100018493.1.24",
          "form": "실수하면 바로 끝이니까요ㅠ",
          "original_form": "실수하면 바로 끝이니까요ㅠ",
          "speaker_id": "1",
          "time": "20210827 08:22"
        },
        {
          "id": "MDRW2100018493.1.25",
          "form": "맞아요 ㅠㅜㅜㅜ",
          "original_form": "맞아요 ㅠㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 08:24"
        },
        {
          "id": "MDRW2100018493.1.26",
          "form": "저라며느.. 못해요..",
          "original_form": "저라며느.. 못해요..",
          "speaker_id": "2",
          "time": "20210827 08:24"
        }
      ]
