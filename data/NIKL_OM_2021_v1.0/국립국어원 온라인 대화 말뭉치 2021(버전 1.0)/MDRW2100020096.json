      "utterance": [
        {
          "id": "MDRW2100020096.1.1",
          "form": "나혼자산다 보시나요?",
          "original_form": "나혼자산다 보시나요?",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020096.1.2",
          "form": "아니요 옛날에나 재밌었는데 요즘 너무 자기 집 자랑 느낌이라",
          "original_form": "아니요 옛날에나 재밌었는데 요즘 너무 자기 집 자랑 느낌이라",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020096.1.3",
          "form": "지금 나혼자산다 보는데",
          "original_form": "지금 나혼자산다 보는데",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020096.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020096.1.5",
          "form": "예전맛이 안나요",
          "original_form": "예전맛이 안나요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020096.1.6",
          "form": "지루한감이 있어요 ㅜㅠ",
          "original_form": "지루한감이 있어요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020096.1.7",
          "form": "지난주 오상욱나와서봤는데 2부할줄알았는데",
          "original_form": "지난주 오상욱나와서봤는데 2부할줄알았는데",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020096.1.8",
          "form": "강민경? 김민경? 개그맨 나와서 잼없어요 ㅎ",
          "original_form": "강민경? 김민경? 개그맨 나와서 잼없어요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020096.1.9",
          "form": "즐겨보시는 프로 있으세요?",
          "original_form": "즐겨보시는 프로 있으세요?",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020096.1.10",
          "form": "기안84 이번사건으로 너무 불쌍해서... 더더욱 싫어지기도 했어요",
          "original_form": "기안84 이번사건으로 너무 불쌍해서... 더더욱 싫어지기도 했어요",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020096.1.11",
          "form": "펜트하우스 시즌2까지 정말 재밌게봤어요",
          "original_form": "펜트하우스 시즌2까지 정말 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020096.1.12",
          "form": "시즌3는 재미없네요",
          "original_form": "시즌3는 재미없네요",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020096.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020096.1.14",
          "form": "시즌2에서 멈췄어야하는데",
          "original_form": "시즌2에서 멈췄어야하는데",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020096.1.15",
          "form": "오늘도 보셨어요?",
          "original_form": "오늘도 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020096.1.16",
          "form": "네 그런데 이제 안보려고요",
          "original_form": "네 그런데 이제 안보려고요",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020096.1.17",
          "form": "오늘 끝난거아니예요?ㅎ",
          "original_form": "오늘 끝난거아니예요?ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020096.1.18",
          "form": "저는 이게 끝인줄알았어요",
          "original_form": "저는 이게 끝인줄알았어요",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020096.1.19",
          "form": "인기많다고 연장했데요",
          "original_form": "인기많다고 연장했데요",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020096.1.20",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020096.1.21",
          "form": "펜트하우스도 무너지드만 ㅜㅠ",
          "original_form": "펜트하우스도 무너지드만 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020096.1.22",
          "form": "저도 시즌2까지는 본방보다가",
          "original_form": "저도 시즌2까지는 본방보다가",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020096.1.23",
          "form": "시즌3는 주로 재방봤어요",
          "original_form": "시즌3는 주로 재방봤어요",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020096.1.24",
          "form": "전 본방챙겨보는데 의리로 봐요",
          "original_form": "전 본방챙겨보는데 의리로 봐요",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020096.1.25",
          "form": "의리 맞네요 ㅎ",
          "original_form": "의리 맞네요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020096.1.26",
          "form": "그럼 몇부까지하죠?",
          "original_form": "그럼 몇부까지하죠?",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020096.1.27",
          "form": "이제 정말 안볼거예요... 몇부까지인지는 모르겠네요",
          "original_form": "이제 정말 안볼거예요... 몇부까지인지는 모르겠네요",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020096.1.28",
          "form": "안타까워요",
          "original_form": "안타까워요",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020096.1.29",
          "form": "즐겨듣는 노래 있으신가요?",
          "original_form": "즐겨듣는 노래 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020096.1.30",
          "form": "아뇨 저는 딱히 듣는노래가 없네요",
          "original_form": "아뇨 저는 딱히 듣는노래가 없네요",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020096.1.31",
          "form": "머 즐겨들으시는거 있으세요?",
          "original_form": "머 즐겨들으시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020096.1.32",
          "form": "저는 팝 음악을 좋아해요",
          "original_form": "저는 팝 음악을 좋아해요",
          "speaker_id": "2",
          "time": "20210827 23:51"
        }
      ]
    }
