        },
        {
          "id": "MDRW2100044673.1.2",
          "form": "두명 안걸린게",
          "original_form": "두명 안걸린게",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044673.1.3",
          "form": "숙소의 침대가",
          "original_form": "숙소의 침대가",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044673.1.4",
          "form": "기적인데",
          "original_form": "기적인데",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044673.1.5",
          "form": "같이있대",
          "original_form": "같이있대",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044673.1.6",
          "form": "이층침대야?",
          "original_form": "이층침대야?",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044673.1.7",
          "form": "모아놓고 다같이잔대",
          "original_form": "모아놓고 다같이잔대",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.8",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.9",
          "form": "뭔지 알겠다",
          "original_form": "뭔지 알겠다",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.10",
          "form": "아마그렇겠찌만",
          "original_form": "아마그렇겠찌만",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.11",
          "form": "근데문제는",
          "original_form": "근데문제는",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.12",
          "form": "숙소니까 당연한거겟지",
          "original_form": "숙소니까 당연한거겟지",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.14",
          "form": "하이브 건물...",
          "original_form": "하이브 건물...",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.15",
          "form": "근데 애들이 맨날 회사가있거든?",
          "original_form": "근데 애들이 맨날 회사가있거든?",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.16",
          "form": "그리고 거기 공용 오피스룸이 되게많아",
          "original_form": "그리고 거기 공용 오피스룸이 되게많아",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.17",
          "form": "헉.....",
          "original_form": "헉.....",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.18",
          "form": "거기에 컴있고 책상있고",
          "original_form": "거기에 컴있고 책상있고",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.19",
          "form": "무서운데",
          "original_form": "무서운데",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.20",
          "form": "그래서 애들이 알아서 예약하고",
          "original_form": "그래서 애들이 알아서 예약하고",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.21",
          "form": "가서 원하는시간동안 있는거",
          "original_form": "가서 원하는시간동안 있는거",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044673.1.22",
          "form": "다 그런식으로 쓰나봐",
          "original_form": "다 그런식으로 쓰나봐",
