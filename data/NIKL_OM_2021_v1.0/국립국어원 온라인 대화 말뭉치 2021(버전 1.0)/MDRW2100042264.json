      "utterance": [
        {
          "id": "MDRW2100042264.1.1",
          "form": "아 이뻐요ㅠ 근데 닥마는 슬리퍼를 신어도 아프더라고요",
          "original_form": "아 이뻐요ㅠ 근데 닥마는 슬리퍼를 신어도 아프더라고요",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042264.1.2",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042264.1.3",
          "form": "아 슬리퍼도 있어요?!",
          "original_form": "아 슬리퍼도 있어요?!",
          "speaker_id": "1",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042264.1.4",
          "form": "샌달이랑 다른거죠?!",
          "original_form": "샌달이랑 다른거죠?!",
          "speaker_id": "1",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042264.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042264.1.6",
          "form": "샌달이랑 거진 같은데",
          "original_form": "샌달이랑 거진 같은데",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042264.1.7",
          "form": "뒷 축이",
          "original_form": "뒷 축이",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042264.1.8",
          "form": "없는",
          "original_form": "없는",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042264.1.9",
          "form": "아니 근데 뒤 축이 없으면",
          "original_form": "아니 근데 뒤 축이 없으면",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042264.1.10",
          "form": "더 발에 힘주게 될텐데!!!",
          "original_form": "더 발에 힘주게 될텐데!!!",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042264.1.11",
          "form": "더 아프겠어요!!!",
          "original_form": "더 아프겠어요!!!",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042264.1.12",
          "form": "정말 오로지 멋쨍이 신발",
          "original_form": "정말 오로지 멋쨍이 신발",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042264.1.13",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042264.1.14",
          "form": "아나 저 지금 지하철인데",
          "original_form": "아나 저 지금 지하철인데",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042264.1.15",
          "form": "양옆에",
          "original_form": "양옆에",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042264.1.16",
          "form": "한국 남성들",
          "original_form": "한국 남성들",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042264.1.17",
          "form": "너무 불편하네요",
          "original_form": "너무 불편하네요",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042264.1.18",
          "form": "다리에 힘 빡주세요",
          "original_form": "다리에 힘 빡주세요",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042264.1.19",
          "form": "저 같이 쩍벌하려고 운동하는거에요",
          "original_form": "저 같이 쩍벌하려고 운동하는거에요",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042264.1.20",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042264.1.21",
          "form": "저 진짜 그래야겠어요",
          "original_form": "저 진짜 그래야겠어요",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042264.1.22",
          "form": "아니근데 지금 이 경우는...",
          "original_form": "아니근데 지금 이 경우는...",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042264.1.23",
          "form": "남성들이 너무",
          "original_form": "남성들이 너무",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042264.1.24",
          "form": "살이많이 찌셨네요!",
          "original_form": "살이많이 찌셨네요!",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042264.1.25",
          "form": "아 비도오는데",
          "original_form": "아 비도오는데",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042264.1.26",
          "form": "냄새,..",
          "original_form": "냄새,..",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042264.1.27",
          "form": "나겠네요...",
          "original_form": "나겠네요...",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042264.1.28",
          "form": "와근데 옆자리 남자",
          "original_form": "와근데 옆자리 남자",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042264.1.29",
          "form": "나이도 많은데",
          "original_form": "나이도 많은데",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042264.1.30",
          "form": "지금 저랑 기싸움해요....ㅎ",
          "original_form": "지금 저랑 기싸움해요....ㅎ",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042264.1.31",
          "form": "이럴 땐 정말",
          "original_form": "이럴 땐 정말",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042264.1.32",
          "form": "하....",
          "original_form": "하....",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042264.1.33",
          "form": "그저 어린여자만만하다고",
          "original_form": "그저 어린여자만만하다고",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042264.1.34",
          "form": "그저그저",
          "original_form": "그저그저",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042264.1.35",
          "form": "우리가 마동석같았어봐요 휴",
          "original_form": "우리가 마동석같았어봐요 휴",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042264.1.36",
          "form": "제 말이 그 말입니다!!!!",
          "original_form": "제 말이 그 말입니다!!!!",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042264.1.37",
          "form": "본론으로 돌아가자면...",
          "original_form": "본론으로 돌아가자면...",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042264.1.38",
          "form": "name2님이 추천해주시는 운동화는 그러면",
          "original_form": "&name2&님이 추천해주시는 운동화는 그러면",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042264.1.39",
          "form": "뉴발인가요?!",
          "original_form": "뉴발인가요?!{emoji:♡}",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042264.1.40",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042264.1.41",
          "form": "뉴발 530 스틸그레이!",
          "original_form": "뉴발 530 스틸그레이!",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042264.1.42",
          "form": "우아아 적어놔야겠어요!!!",
          "original_form": "우아아 적어놔야겠어요!!!",
