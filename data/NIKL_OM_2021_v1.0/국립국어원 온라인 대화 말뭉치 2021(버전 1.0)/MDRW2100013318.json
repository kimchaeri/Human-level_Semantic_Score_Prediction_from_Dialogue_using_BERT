      "utterance": [
        {
          "id": "MDRW2100013318.1.1",
          "form": "여행 어디 가는거 좋아해요?",
          "original_form": "여행 어디 가는거 좋아해요?",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013318.1.2",
          "form": "바다가는거요",
          "original_form": "바다가는거요",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013318.1.3",
          "form": "산은 힘들어소. 올라가는게. 막상 올라가보면 좋긴한데.",
          "original_form": "산은 힘들어소. 올라가는게. 막상 올라가보면 좋긴한데.",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013318.1.4",
          "form": "어디 가고싶은데???",
          "original_form": "어디 가고싶은데???",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013318.1.5",
          "form": "ㅋㅋㅋㅋ그렇긴 하져",
          "original_form": "ㅋㅋㅋㅋ그렇긴 하져",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013318.1.6",
          "form": "저는 호텔이요",
          "original_form": "저는 호텔이요",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013318.1.7",
          "form": "호캉스가 최고",
          "original_form": "호캉스가 최고",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013318.1.8",
          "form": "호캉스는 재미없어요~~~",
          "original_form": "호캉스는 재미없어요~~~",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013318.1.9",
          "form": "호캉스가 최곤데",
          "original_form": "호캉스가 최곤데",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013318.1.10",
          "form": "아니면 힐링 여행이 좋아요",
          "original_form": "아니면 힐링 여행이 좋아요",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013318.1.11",
          "form": "호텔에만 있는건 재미없어요",
          "original_form": "호텔에만 있는건 재미없어요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013318.1.12",
          "form": "근처 구경도 하고 먹기도하고.",
          "original_form": "근처 구경도 하고 먹기도하고.",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013318.1.13",
          "form": "재미있는 기구도 탈게 있음 괜챦긴한데.",
          "original_form": "재미있는 기구도 탈게 있음 괜챦긴한데.",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013318.1.14",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013318.1.15",
          "form": "근데 기구 잘 안타잖아요",
          "original_form": "근데 기구 잘 안타잖아요",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013318.1.16",
          "form": "나 말고 가족들이 즐거운게 좋아요",
          "original_form": "나 말고 가족들이 즐거운게 좋아요",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013318.1.17",
          "form": "난 나이가들오서 기운이 딸료소 오래 노는게 힘들어서 ㅋ",
          "original_form": "난 나이가들오서 기운이 딸료소 오래 노는게 힘들어서 ㅋ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013318.1.18",
          "form": "에이 그정돈 아니지",
          "original_form": "에이 그정돈 아니지",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013318.1.19",
          "form": "더 나이먹음 진짜 힘들겠다 싶어 다니는거에요",
          "original_form": "더 나이먹음 진짜 힘들겠다 싶어 다니는거에요",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013318.1.20",
          "form": "그럼 요즘 놀러갔던곳 중에 어디가 좋았어요",
          "original_form": "그럼 요즘 놀러갔던곳 중에 어디가 좋았어요",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013318.1.21",
          "form": "난 다낭갔던거",
          "original_form": "난 다낭갔던거",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013318.1.22",
          "form": "맞아~~ 거기 좋았지~~~",
          "original_form": "맞아~~ 거기 좋았지~~~",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013318.1.23",
          "form": "아빠는 호주갔던게 좋았대요",
          "original_form": "아빠는 호주갔던게 좋았대요",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013318.1.24",
          "form": "호주도 재밌었죠",
          "original_form": "호주도 재밌었죠",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013318.1.25",
          "form": "또 가고싶어요 좀 어릴때라 기억이 자세히는 안나서",
          "original_form": "또 가고싶어요 좀 어릴때라 기억이 자세히는 안나서",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013318.1.26",
          "form": "난 다낭도 좋았고 일본도 좋았고.",
          "original_form": "난 다낭도 좋았고 일본도 좋았고.",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013318.1.27",
          "form": "일본도 또 가고싶어요",
          "original_form": "일본도 또 가고싶어요",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013318.1.28",
          "form": "호주는 너무 비싸서.",
          "original_form": "호주는 너무 비싸서.",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013318.1.29",
          "form": "온천여행으로ㅋㅋ",
          "original_form": "온천여행으로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013318.1.30",
          "form": "함부로 갈 여행이 아니에요~",
          "original_form": "함부로 갈 여행이 아니에요~",
          "speaker_id": "1",
          "time": "20210825 14:37"
        }
      ]
