      "utterance": [
        {
          "id": "MDRW2100027950.1.1",
          "form": "오오 미니게임 모은건가요",
          "original_form": "오오 미니게임 모은건가요",
          "speaker_id": "2",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100027950.1.2",
          "form": "예전에 Wii에서 했던 엽기토끼 시리즈 좋아했는데",
          "original_form": "예전에 Wii에서 했던 엽기토끼 시리즈 좋아했는데",
          "speaker_id": "2",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100027950.1.3",
          "form": "그것도 나와주면 좋겠어요",
          "original_form": "그것도 나와주면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100027950.1.4",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027950.1.5",
          "form": "위게임은 젤다랑 공포게임 하는거 봤는데",
          "original_form": "위게임은 젤다랑 공포게임 하는거 봤는데",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027950.1.6",
          "form": "엽끼토끼 간만이네요",
          "original_form": "엽끼토끼 간만이네요",
          "speaker_id": "1",
          "time": "20210902 16:23"
        },
        {
          "id": "MDRW2100027950.1.7",
          "form": "찾아보니 제가 생각한 토끼가아니네요 ㅋㅋㅋㅋ",
          "original_form": "찾아보니 제가 생각한 토끼가아니네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027950.1.8",
          "form": "레이맨은 어렸을때 자주한 ㅎㅎ",
          "original_form": "레이맨은 어렸을때 자주한 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100027950.1.9",
          "form": "네 이상한 소리내는 토끼친구들인데 친구들끼리하면 의 상하고 아주 재미있습니다ㅋㅋ",
          "original_form": "네 이상한 소리내는 토끼친구들인데 친구들끼리하면 의 상하고 아주 재미있습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027950.1.10",
          "form": "오 같이 하는 게임이군요",
          "original_form": "오 같이 하는 게임이군요",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027950.1.11",
          "form": "네ㅋㅋ 미니게임 류인데 범퍼카같은 종류도 있고",
          "original_form": "네ㅋㅋ 미니게임 류인데 범퍼카같은 종류도 있고",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027950.1.12",
          "form": "버거만들기나 밴드음악연주하기",
          "original_form": "버거만들기나 밴드음악연주하기",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027950.1.13",
          "form": "진짜 재미있었어요",
          "original_form": "진짜 재미있었어요",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027950.1.14",
          "form": "허 그런거 좋아해요 저도",
          "original_form": "허 그런거 좋아해요 저도",
          "speaker_id": "1",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027950.1.15",
          "form": "스위치에는",
          "original_form": "스위치에는",
          "speaker_id": "1",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027950.1.16",
          "form": "마리오 파티 있어요",
          "original_form": "마리오 파티 있어요",
          "speaker_id": "1",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027950.1.17",
          "form": "엽기토끼 안내줄것같으니 마리오 파티라도 사야겠어요ㅋㅋ",
          "original_form": "엽기토끼 안내줄것같으니 마리오 파티라도 사야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027950.1.18",
          "form": "저는 게임 좋아하는 친구가 하나라",
          "original_form": "저는 게임 좋아하는 친구가 하나라",
          "speaker_id": "1",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100027950.1.19",
          "form": "4명이면 진짜 잼날듯",
          "original_form": "4명이면 진짜 잼날듯",
          "speaker_id": "1",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100027950.1.20",
          "form": "혹시 스위치면 온라인으로 아무나 연결은 안되려나요",
          "original_form": "혹시 스위치면 온라인으로 아무나 연결은 안되려나요",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100027950.1.21",
          "form": "오 생각해보니",
          "original_form": "오 생각해보니",
          "speaker_id": "1",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100027950.1.22",
          "form": "그것도 될것같은 ?!",
          "original_form": "그것도 될것같은 ?!",
          "speaker_id": "1",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100027950.1.23",
          "form": "전에 스위치로 오버쿡드 멀티로 했었거든요ㅋㅋ",
          "original_form": "전에 스위치로 오버쿡드 멀티로 했었거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100027950.1.24",
          "form": "헉 그거 2만 온라인됐던가요?",
          "original_form": "헉 그거 2만 온라인됐던가요?",
          "speaker_id": "1",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100027950.1.25",
          "form": "저도 삿어요 그거 ㅎㅎㅎ",
          "original_form": "저도 삿어요 그거 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100027950.1.26",
          "form": "짱 재밌어요",
          "original_form": "짱 재밌어요",
          "speaker_id": "1",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100027950.1.27",
          "form": "진짜 재밌어요ㅋㅋ 넷이서 해낼때 쾌감",
          "original_form": "진짜 재밌어요ㅋㅋ 넷이서 해낼때 쾌감",
          "speaker_id": "2",
          "time": "20210902 17:17"
        }
      ]
    }
