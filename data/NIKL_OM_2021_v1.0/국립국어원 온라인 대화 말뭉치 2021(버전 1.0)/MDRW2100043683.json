      "utterance": [
        {
          "id": "MDRW2100043683.1.1",
          "form": "쇼핑하신게 있으신가용?",
          "original_form": "쇼핑하신게 있으신가용?",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043683.1.2",
          "form": "최근에는 뭐 딱히 없어요",
          "original_form": "최근에는 뭐 딱히 없어요",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043683.1.3",
          "form": "세제나 이런거 인터넷으로 주문하는 그런거..?",
          "original_form": "세제나 이런거 인터넷으로 주문하는 그런거..?",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043683.1.4",
          "form": "아 필수죠!",
          "original_form": "아 필수죠!",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043683.1.5",
          "form": "그런거밖에 없어여",
          "original_form": "그런거밖에 없어여",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043683.1.6",
          "form": "아니면 구매하시고싶으신건?",
          "original_form": "아니면 구매하시고싶으신건?",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043683.1.7",
          "form": "옷! 신발! 가방! 이제 가을이라 가을옷들사고 싶어요",
          "original_form": "옷! 신발! 가방! 이제 가을이라 가을옷들사고 싶어요",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043683.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다 사고싶으신거네요 ㅎㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다 사고싶으신거네요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043683.1.9",
          "form": "가을되면정말 사고싶죠",
          "original_form": "가을되면정말 사고싶죠",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043683.1.10",
          "form": "입고 놀러가고싶고 ㅎㅎ",
          "original_form": "입고 놀러가고싶고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043683.1.11",
          "form": "사고 싶으신거 없으세요?",
          "original_form": "사고 싶으신거 없으세요?",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043683.1.12",
          "form": "저는 요즘 막 사가지고",
          "original_form": "저는 요즘 막 사가지고",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043683.1.13",
          "form": "가장 살까말까하는건이불 ㅎㅎ",
          "original_form": "가장 살까말까하는건이불 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043683.1.14",
          "form": "체크이불이넘갖고싶은데",
          "original_form": "체크이불이넘갖고싶은데",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043683.1.15",
          "form": "맘에드는게없더라구요 ㅠ",
          "original_form": "맘에드는게없더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043683.1.16",
          "form": "아니면 넘 비싸고",
          "original_form": "아니면 넘 비싸고",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043683.1.17",
          "form": "맞아요 이불같은건 뭔가 비싸게주고 살려하면 아까워여",
          "original_form": "맞아요 이불같은건 뭔가 비싸게주고 살려하면 아까워여",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043683.1.18",
          "form": "그쵸 ㅠㅠㅠ 근데 또 넘 안주면 못쓸이불이라 ㅠ",
          "original_form": "그쵸 ㅠㅠㅠ 근데 또 넘 안주면 못쓸이불이라 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043683.1.19",
          "form": "고민은 많은데 성급하게 살숨없어서",
          "original_form": "고민은 많은데 성급하게 살숨없어서",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043683.1.20",
          "form": "어려워요",
          "original_form": "어려워요",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043683.1.21",
          "form": "특히 겨울이불같은거...",
          "original_form": "특히 겨울이불같은거...",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043683.1.22",
          "form": "싼거사면 솜 무겁구 실밥 다 터지구..",
          "original_form": "싼거사면 솜 무겁구 실밥 다 터지구..",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043683.1.23",
          "form": "10정도주면 그래도 꽤 괜찮은거 살 수 있는거 같아요",
          "original_form": "10정도주면 그래도 꽤 괜찮은거 살 수 있는거 같아요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043683.1.24",
          "form": "맞아요 ㅎㅎ 그 쭘에서 이쁜걸 사고싶은데",
          "original_form": "맞아요 ㅎㅎ 그 쭘에서 이쁜걸 사고싶은데",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043683.1.25",
          "form": "어려워요",
          "original_form": "어려워요",
          "speaker_id": "1",
          "time": "20210907 20:03"
        }
      ]
    }
