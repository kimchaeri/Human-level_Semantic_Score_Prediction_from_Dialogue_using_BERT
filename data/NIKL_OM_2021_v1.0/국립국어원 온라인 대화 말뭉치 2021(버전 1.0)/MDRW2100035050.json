      "utterance": [
        {
          "id": "MDRW2100035050.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100035050.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100035050.1.3",
          "form": "커피 말고는 마시는거 잇으세요?",
          "original_form": "커피 말고는 마시는거 잇으세요?",
          "speaker_id": "1",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100035050.1.4",
          "form": "요즘은 오미자에 물타서 가끔 먹어요",
          "original_form": "요즘은 오미자에 물타서 가끔 먹어요",
          "speaker_id": "2",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100035050.1.5",
          "form": "오 오미자 청인가요? 말린오미자인가요?",
          "original_form": "오 오미자 청인가요? 말린오미자인가요?",
          "speaker_id": "1",
          "time": "20210829 01:15"
        },
        {
          "id": "MDRW2100035050.1.6",
          "form": "청이요!",
          "original_form": "청이요!",
          "speaker_id": "2",
          "time": "20210829 01:15"
        },
        {
          "id": "MDRW2100035050.1.7",
          "form": "오... 만드신거세요?",
          "original_form": "오... 만드신거세요?",
          "speaker_id": "1",
          "time": "20210829 01:15"
        },
        {
          "id": "MDRW2100035050.1.8",
          "form": "아니요\r\n엄마가 석류랑 오미자랑 사다놓으신게 있어서\r\n석류 비우고 오미자 먹는 중인다 거의 비워지고 있어요",
          "original_form": "아니요\r\n엄마가 석류랑 오미자랑 사다놓으신게 있어서\r\n석류 비우고 오미자 먹는 중인다 거의 비워지고 있어요",
          "speaker_id": "2",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100035050.1.9",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100035050.1.10",
          "form": "오 둘 다 청이잇나보군요",
          "original_form": "오 둘 다 청이잇나보군요",
          "speaker_id": "1",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100035050.1.11",
          "form": "정확히 청은 아니고, 청이랑 비슷하긴 한데 윈액? 이런거였어요",
          "original_form": "정확히 청은 아니고, 청이랑 비슷하긴 한데 윈액? 이런거였어요",
          "speaker_id": "2",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100035050.1.12",
          "form": "오..... 그럼 안달아요?",
          "original_form": "오..... 그럼 안달아요?",
          "speaker_id": "1",
          "time": "20210829 01:27"
        },
        {
          "id": "MDRW2100035050.1.13",
          "form": "네 크게 달달한편이 아니었어요",
          "original_form": "네 크게 달달한편이 아니었어요",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100035050.1.14",
          "form": "달지 않게 나온거여서 샀다고 했었어요",
          "original_form": "달지 않게 나온거여서 샀다고 했었어요",
          "speaker_id": "2",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100035050.1.15",
          "form": "오... 신기하네여",
          "original_form": "오... 신기하네여",
          "speaker_id": "1",
          "time": "20210829 01:31"
        }
      ]
