      "utterance": [
        {
          "id": "MDRW2100008444.1.1",
          "form": "슬의생 보시나요?",
          "original_form": "슬의생 보시나요?",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008444.1.2",
          "form": "네.  잘챙겨보고있어요",
          "original_form": "네.  잘챙겨보고있어요",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008444.1.3",
          "form": "너무 잼있죠?",
          "original_form": "너무 잼있죠?",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008444.1.4",
          "form": "시즌1보단 별루인거같긴한데 재미있어요",
          "original_form": "시즌1보단 별루인거같긴한데 재미있어요",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008444.1.5",
          "form": "본방이 언제하는거예요?",
          "original_form": "본방이 언제하는거예요?",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008444.1.6",
          "form": "저는 맨날 재방만봐서요 ㅎ",
          "original_form": "저는 맨날 재방만봐서요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008444.1.7",
          "form": "목요일 9시 정도였던거같아요",
          "original_form": "목요일 9시 정도였던거같아요",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008444.1.8",
          "form": "전 재방도봐요^^",
          "original_form": "전 재방도봐요^^",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008444.1.9",
          "form": "아...9시에 애들재우러가니 맨날못봤군요",
          "original_form": "아...9시에 애들재우러가니 맨날못봤군요",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008444.1.10",
          "form": "아까도 재방하길래보니 은근 감동이더라구요",
          "original_form": "아까도 재방하길래보니 은근 감동이더라구요",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008444.1.11",
          "form": "네. 그런병원은 없겠지만 있었응 좋겠다하고봐요",
          "original_form": "네. 그런병원은 없겠지만 있었응 좋겠다하고봐요",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008444.1.12",
          "form": "의사들중 어느의사가 젤 좋으세요?",
          "original_form": "의사들중 어느의사가 젤 좋으세요?",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008444.1.13",
          "form": "거기의사들은 다 실력도좋고 인간미도좋고~",
          "original_form": "거기의사들은 다 실력도좋고 인간미도좋고~",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008444.1.14",
          "form": "저는 조정석 스타일이 좋다고 \r\n언니들이랑 이야기했더니",
          "original_form": "저는 조정석 스타일이 좋다고 \r\n언니들이랑 이야기했더니",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008444.1.15",
          "form": "그런사람 없다며 정신차리라고 하더라구요 ㅋ",
          "original_form": "그런사람 없다며 정신차리라고 하더라구요 ㅋ",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008444.1.16",
          "form": "ㅎㅎ 현실적이시네요~~ 절때 없을꺼같긴해요",
          "original_form": "ㅎㅎ 현실적이시네요~~ 절때 없을꺼같긴해요",
          "speaker_id": "2",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008444.1.17",
          "form": "거기나오는의사들중 다 그런사람 없을꺼라했어요 ㅎㅎ",
          "original_form": "거기나오는의사들중 다 그런사람 없을꺼라했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008444.1.18",
          "form": "마자요. 그래서 현실과 동떨어진 드라마~",
          "original_form": "마자요. 그래서 현실과 동떨어진 드라마~",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008444.1.19",
          "form": "글쵸 그재미에 드라마 보는거죠",
          "original_form": "글쵸 그재미에 드라마 보는거죠",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008444.1.20",
          "form": "드라마는 드라마로~~ 그냥 저랬음 좋겠다 그런맘으로",
          "original_form": "드라마는 드라마로~~ 그냥 저랬음 좋겠다 그런맘으로",
          "speaker_id": "2",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008444.1.21",
          "form": "양석현쌤이랑 추민하 쌤 커플 잘됐으면 좋겠어요",
          "original_form": "양석현쌤이랑 추민하 쌤 커플 잘됐으면 좋겠어요",
