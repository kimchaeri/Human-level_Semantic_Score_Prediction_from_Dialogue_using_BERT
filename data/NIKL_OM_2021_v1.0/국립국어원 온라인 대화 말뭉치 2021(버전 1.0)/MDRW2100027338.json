      "utterance": [
        {
          "id": "MDRW2100027338.1.1",
          "form": "솔의눈 좋아하세요?",
          "original_form": "솔의눈 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.2",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.3",
          "form": "시원함이 일품이지요",
          "original_form": "시원함이 일품이지요",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.4",
          "form": "갈증날때 얼음타서 드셔보세요",
          "original_form": "갈증날때 얼음타서 드셔보세요",
          "speaker_id": "2",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.5",
          "form": "데자와 좋아하세요?",
          "original_form": "데자와 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.6",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.7",
          "form": "환장합니다",
          "original_form": "환장합니다",
          "speaker_id": "2",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.8",
          "form": "그럼 실론티 좋아하세요.ㅣ",
          "original_form": "그럼 실론티 좋아하세요.ㅣ",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.9",
          "form": "실론티는요?",
          "original_form": "실론티는요?",
          "speaker_id": "2",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.10",
          "form": "ㅂㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅂㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027338.1.11",
          "form": "전 좋아해요",
          "original_form": "전 좋아해요",
          "speaker_id": "2",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.12",
          "form": "노래방에서 실론티 자주 시켜먹었지요",
          "original_form": "노래방에서 실론티 자주 시켜먹었지요",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.13",
          "form": "무조건이죠",
          "original_form": "무조건이죠",
          "speaker_id": "2",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.14",
          "form": "혹시그럼맥콜 좋아하세요?",
          "original_form": "혹시그럼맥콜 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.15",
          "form": "예전에는 서비스로 줬는데ㅋㅋ",
          "original_form": "예전에는 서비스로 줬는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.16",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.17",
          "form": "물 대신 마셨던 적이 있었죠",
          "original_form": "물 대신 마셨던 적이 있었죠",
          "speaker_id": "2",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.18",
          "form": "예전에 맥콜 좋아했는데",
          "original_form": "예전에 맥콜 좋아했는데",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.19",
          "form": "없을땐 굘라사이다 섞어서먹음",
          "original_form": "없을땐 굘라사이다 섞어서먹음",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.20",
          "form": "비슷한맛났어요",
          "original_form": "비슷한맛났어요",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.21",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "2",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027338.1.22",
          "form": "역시 먹는게 짱이네요",
          "original_form": "역시 먹는게 짱이네요",
          "speaker_id": "2",
          "time": "20210902 11:38"
        }
      ]
