      "utterance": [
        {
          "id": "MDRW2100019992.1.1",
          "form": "페스츄리소세지빵 샀어요",
          "original_form": "페스츄리소세지빵 샀어요",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019992.1.2",
          "form": "그거 맛있는데",
          "original_form": "그거 맛있는데",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019992.1.3",
          "form": "짠맛의 정석",
          "original_form": "짠맛의 정석",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019992.1.4",
          "form": "약간 페스츄리에서 단맛나서 단짠단짠",
          "original_form": "약간 페스츄리에서 단맛나서 단짠단짠",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019992.1.5",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019992.1.6",
          "form": "페스츄리소세지빵 좋아해요 ㅎㅎㅎ",
          "original_form": "페스츄리소세지빵 좋아해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019992.1.7",
          "form": "name1님은 또 뭐사셧어요",
          "original_form": "&name1&님은 또 뭐사셧어요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019992.1.8",
          "form": "그 소시지빵 좋아하시면",
          "original_form": "그 소시지빵 좋아하시면",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019992.1.9",
          "form": "뽀드득 그릴소시지도 한번 드셔보세요",
          "original_form": "뽀드득 그릴소시지도 한번 드셔보세요",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019992.1.10",
          "form": "이거 소시지가 진짜 맛있어요",
          "original_form": "이거 소시지가 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019992.1.11",
          "form": "오 빵마다 소세지가 다른건가요?",
          "original_form": "오 빵마다 소세지가 다른건가요?",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019992.1.12",
          "form": "그 약간 다른 빵은 얇은 소시지쓰는데",
          "original_form": "그 약간 다른 빵은 얇은 소시지쓰는데",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019992.1.13",
          "form": "뽀드득은 소시지를 좀 비싼거써서 진짜 맛있어요",
          "original_form": "뽀드득은 소시지를 좀 비싼거써서 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019992.1.14",
          "form": "가운데 무채가있는데 그게 핵심입니다",
          "original_form": "가운데 무채가있는데 그게 핵심입니다",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019992.1.15",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019992.1.16",
          "form": "다음에 뽀드득 사봐야겟어요",
          "original_form": "다음에 뽀드득 사봐야겟어요",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019992.1.17",
          "form": "아니근데 거의 빵전문가세요",
          "original_form": "아니근데 거의 빵전문가세요",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019992.1.18",
          "form": "꼭 한번 드셔보세요",
          "original_form": "꼭 한번 드셔보세요",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019992.1.19",
          "form": "저는 사실 대부분의 음식을 좋아해서ㅎ",
          "original_form": "저는 사실 대부분의 음식을 좋아해서ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019992.1.20",
          "form": "그리고 파바는 은근.. 아이스크림 맛집입니다",
          "original_form": "그리고 파바는 은근.. 아이스크림 맛집입니다",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019992.1.21",
          "form": "하드 말씀하시는건가요?",
          "original_form": "하드 말씀하시는건가요?",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019992.1.22",
          "form": "하드라니까 급 늙은이같네요 ㅋㅋㅋ",
          "original_form": "하드라니까 급 늙은이같네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019992.1.23",
          "form": "하드도맞고 콘도맞고",
          "original_form": "하드도맞고 콘도맞고",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019992.1.24",
          "form": "아니에요ㅎㅎ",
          "original_form": "아니에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019992.1.25",
          "form": "뻔할거같은데 뻔하지않은 맛",
          "original_form": "뻔할거같은데 뻔하지않은 맛",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019992.1.26",
          "form": "진짜 맛있어요 언제 한 번 금액 애매할 때 맞추려고 사보세요",
          "original_form": "진짜 맛있어요 언제 한 번 금액 애매할 때 맞추려고 사보세요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019992.1.27",
          "form": "그래봐야겟어요",
          "original_form": "그래봐야겟어요",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019992.1.28",
          "form": "뀐아망 뽀드득 아이스크림",
          "original_form": "뀐아망 뽀드득 아이스크림",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019992.1.29",
          "form": "메모",
