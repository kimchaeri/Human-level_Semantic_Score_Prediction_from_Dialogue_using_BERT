      "utterance": [
        {
          "id": "MDRW2100007140.1.1",
          "form": "바라던바다 보시나요?",
          "original_form": "바라던바다 보시나요?",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007140.1.2",
          "form": "그건 머예요?",
          "original_form": "그건 머예요?",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007140.1.3",
          "form": "연예인들이 음식만들어주고 노래불러주는건데",
          "original_form": "연예인들이 음식만들어주고 노래불러주는건데",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007140.1.4",
          "form": "신청자 받아서 자리마련하는거같아요",
          "original_form": "신청자 받아서 자리마련하는거같아요",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007140.1.5",
          "form": "예전에 그...비긴어게인과 비슷한건가요?",
          "original_form": "예전에 그...비긴어게인과 비슷한건가요?",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007140.1.6",
          "form": "네네 컨셉은 유사해요",
          "original_form": "네네 컨셉은 유사해요",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007140.1.7",
          "form": "길거리대신 바닷가에서  거리두고 앉아서 칵테일주고 음식주고",
          "original_form": "길거리대신 바닷가에서  거리두고 앉아서 칵테일주고 음식주고",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007140.1.8",
          "form": "콘서트 같은 느낌이예요",
          "original_form": "콘서트 같은 느낌이예요",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007140.1.9",
          "form": "name2님은 콘서트 가보셨어요?",
          "original_form": "&name2&님은 콘서트 가보셨어요?",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007140.1.10",
          "form": "콘서트 보러많이 가셨어요?",
          "original_form": "콘서트 보러많이 가셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007140.1.11",
          "form": "전 연극이나 뮤지컬은 봤는데",
          "original_form": "전 연극이나 뮤지컬은 봤는데",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007140.1.12",
          "form": "콘서트는 못기봤어요",
          "original_form": "콘서트는 못기봤어요",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007140.1.13",
          "form": "이승철이나 이승환 콘서트 가보고싶어요",
          "original_form": "이승철이나 이승환 콘서트 가보고싶어요",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007140.1.14",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007140.1.15",
          "form": "저는 많이는 안가봤지만 몇번가봤어요",
          "original_form": "저는 많이는 안가봤지만 몇번가봤어요",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007140.1.16",
          "form": "아는언니는 이승철콘서트 매년가더라구요",
          "original_form": "아는언니는 이승철콘서트 매년가더라구요",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007140.1.17",
          "form": "근데 뮤지컬같은게 더 정신챙기기 나아요 ㅎㅎ",
          "original_form": "근데 뮤지컬같은게 더 정신챙기기 나아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007140.1.18",
          "form": "지킬앤하이드 함 보고싶은데 조승우나오는",
          "original_form": "지킬앤하이드 함 보고싶은데 조승우나오는",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007140.1.19",
          "form": "티켓 예약이 엄청 힘든가봐요",
          "original_form": "티켓 예약이 엄청 힘든가봐요",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007140.1.20",
          "form": "저 그거봤어요",
          "original_form": "저 그거봤어요",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007140.1.21",
          "form": "진짜 대단해요",
          "original_form": "진짜 대단해요",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007140.1.22",
          "form": "오 ㅎ 조승우나오는걸로요?",
          "original_form": "오 ㅎ 조승우나오는걸로요?",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007140.1.23",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007140.1.24",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007140.1.25",
          "form": "저는 그 작품보기전까지 조승우 별로 안좋아했거든요",
          "original_form": "저는 그 작품보기전까지 조승우 별로 안좋아했거든요",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007140.1.26",
          "form": "티켓 비싸죠?",
          "original_form": "티켓 비싸죠?",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007140.1.27",
          "form": "본지 오래되서...ㅎ\r\n그냥 뮤지컬비용이였던거같아요",
          "original_form": "본지 오래되서...ㅎ\r\n그냥 뮤지컬비용이였던거같아요",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007140.1.28",
          "form": "코로나 끝남 함 도전해봐야겠어요",
          "original_form": "코로나 끝남 함 도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007140.1.29",
          "form": "요즘은 그냥 연화보는게 문화생활 다네요",
          "original_form": "요즘은 그냥 연화보는게 문화생활 다네요",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007140.1.30",
          "form": "작품보다가 소름돋는경우는 처음이였어요",
          "original_form": "작품보다가 소름돋는경우는 처음이였어요",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007140.1.31",
          "form": "연기엄청 잘하나봐요",
          "original_form": "연기엄청 잘하나봐요",
          "speaker_id": "1",
