      "utterance": [
        {
          "id": "MDRW2100027539.1.1",
          "form": "스마트 도서관 사진 봤어요?",
          "original_form": "스마트 도서관 사진 봤어요?",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027539.1.2",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027539.1.3",
          "form": "진짜 나라에서 돈 크게 쓴모양",
          "original_form": "진짜 나라에서 돈 크게 쓴모양",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027539.1.4",
          "form": "이름 머라고 책이음?",
          "original_form": "이름 머라고 책이음?",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027539.1.5",
          "form": "ㅇㅇ그냥도서관 카드 자체가 책이음카드로 이름이 변경됨 ㅋㅋ",
          "original_form": "ㅇㅇ그냥도서관 카드 자체가 책이음카드로 이름이 변경됨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027539.1.6",
          "form": "이제는 대출할때도 아예 책이음카드 있으세요 이래물어봄",
          "original_form": "이제는 대출할때도 아예 책이음카드 있으세요 이래물어봄",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027539.1.7",
          "form": "오  그래장기전으로 가려면",
          "original_form": "오  그래장기전으로 가려면",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027539.1.8",
          "form": "도서관도비대면 해야함 ㅜㅜ 사서는 다른업무주시겠징",
          "original_form": "도서관도비대면 해야함 ㅜㅜ 사서는 다른업무주시겠징",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027539.1.9",
          "form": "사서는 그거 말고도 할게 있으니까요",
          "original_form": "사서는 그거 말고도 할게 있으니까요",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027539.1.10",
          "form": "책분류관리라던가",
          "original_form": "책분류관리라던가",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027539.1.11",
          "form": "요새는 문센같은 역할도하니까 그런쪽을 더활성화시키면 좋을듯",
          "original_form": "요새는 문센같은 역할도하니까 그런쪽을 더활성화시키면 좋을듯",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027539.1.12",
          "form": "마자마자 관리 유지 보수",
          "original_form": "마자마자 관리 유지 보수",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027539.1.13",
          "form": "강좌열면 관리 총무해야함",
          "original_form": "강좌열면 관리 총무해야함",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027539.1.14",
          "form": "글고 복지센터처럼 주민들 민원도 들어옴",
          "original_form": "글고 복지센터처럼 주민들 민원도 들어옴",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027539.1.15",
          "form": "ㅇㅇ그런기능만해도 할일 많죠",
          "original_form": "ㅇㅇ그런기능만해도 할일 많죠",
          "speaker_id": "1",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027539.1.16",
          "form": "요샌 애들대상 독서교실도 인기있고",
          "original_form": "요샌 애들대상 독서교실도 인기있고",
          "speaker_id": "1",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027539.1.17",
          "form": "어르신들께는 한글교실하고",
          "original_form": "어르신들께는 한글교실하고",
          "speaker_id": "1",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027539.1.18",
          "form": "name1님은 아직도 사서하고싶나요?",
          "original_form": "&name1&님은 아직도 사서하고싶나요?",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027539.1.19",
          "form": "할게 많어",
          "original_form": "할게 많어",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027539.1.20",
          "form": "그리구 뉴스서봤는데 나쁜짓하는 남자들도 종종 있어서",
          "original_form": "그리구 뉴스서봤는데 나쁜짓하는 남자들도 종종 있어서",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100027539.1.21",
          "form": "무인으로 복지시설 개방하믄 안됨 ㅜㅜ",
          "original_form": "무인으로 복지시설 개방하믄 안됨 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100027539.1.22",
          "form": "도서관에서 나쁜짓을 한다굽쇼?ㄷㄷㄷ",
          "original_form": "도서관에서 나쁜짓을 한다굽쇼?ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100027539.1.23",
          "form": "진짜 대단한 사람들이네",
          "original_form": "진짜 대단한 사람들이네",
          "speaker_id": "1",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027539.1.24",
          "form": "몰카설치같은거하려나",
          "original_form": "몰카설치같은거하려나",
          "speaker_id": "1",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027539.1.25",
          "form": "무엇을 상상하든 범죄는 상상이상임 ㅋ",
          "original_form": "무엇을 상상하든 범죄는 상상이상임 ㅋ",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027539.1.26",
          "form": "으슥한 곳을 노리는건가",
          "original_form": "으슥한 곳을 노리는건가",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027539.1.27",
          "form": "나쁜말 걸려서 쓰기 거시기하지만ㅋ 암튼 젤 만만한게 도서관이여",
          "original_form": "나쁜말 걸려서 쓰기 거시기하지만ㅋ 암튼 젤 만만한게 도서관이여",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027539.1.28",
          "form": "하여간 범죄자들의 머리는 따라갈수없을듯",
          "original_form": "하여간 범죄자들의 머리는 따라갈수없을듯",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027539.1.29",
          "form": "개인적으로 저 스마트 도서관에",
          "original_form": "개인적으로 저 스마트 도서관에",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027539.1.30",
          "form": "ㅋㅋ스마트하게삽시다",
          "original_form": "ㅋㅋ스마트하게삽시다",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027539.1.31",
          "form": "인터넷으로 책신청하고퇴근하고 저기서 찾아가거나하는 그런 서비스 해도 좋을거같았음",
          "original_form": "인터넷으로 책신청하고퇴근하고 저기서 찾아가거나하는 그런 서비스 해도 좋을거같았음",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027539.1.32",
          "form": "똑똑해 name1님",
          "original_form": "똑똑해 &name1&님",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027539.1.33",
          "form": "사서가 쿠팡 분류작업알바 맹키로 바쁘겠지만",
          "original_form": "사서가 쿠팡 분류작업알바 맹키로 바쁘겠지만",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027539.1.34",
