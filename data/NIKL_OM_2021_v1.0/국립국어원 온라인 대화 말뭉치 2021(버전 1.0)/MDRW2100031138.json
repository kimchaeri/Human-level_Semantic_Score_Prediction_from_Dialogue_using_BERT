      "utterance": [
        {
          "id": "MDRW2100031138.1.1",
          "form": "길냥이를 한번 키워 보세요",
          "original_form": "길냥이를 한번 키워 보세요",
          "speaker_id": "2",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031138.1.2",
          "form": "어떤 종류의 고양이를 좋아하세요",
          "original_form": "어떤 종류의 고양이를 좋아하세요",
          "speaker_id": "1",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031138.1.3",
          "form": "저는 고등어를 좋아해요",
          "original_form": "저는 고등어를 좋아해요",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031138.1.4",
          "form": "고양이는 다 귀여워요",
          "original_form": "고양이는 다 귀여워요",
          "speaker_id": "1",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031138.1.5",
          "form": "가끔 애교를 부리는 고양이도 있더라고요",
          "original_form": "가끔 애교를 부리는 고양이도 있더라고요",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031138.1.6",
          "form": "그럼 고양이는 정말 엎어오고 싶죠",
          "original_form": "그럼 고양이는 정말 엎어오고 싶죠",
          "speaker_id": "1",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031138.1.7",
          "form": "고양이는 털이 많이 빠져요",
          "original_form": "고양이는 털이 많이 빠져요",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031138.1.8",
          "form": "그게 가장 큰 단점이에요",
          "original_form": "그게 가장 큰 단점이에요",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031138.1.9",
          "form": "그래도 사랑스러워서 털은 문제가 되지 않아요",
          "original_form": "그래도 사랑스러워서 털은 문제가 되지 않아요",
          "speaker_id": "2",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031138.1.10",
          "form": "당연히 그럴 거 같네요",
          "original_form": "당연히 그럴 거 같네요",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031138.1.11",
          "form": "고양이 사료는 어디서 사나요",
          "original_form": "고양이 사료는 어디서 사나요",
          "speaker_id": "2",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031138.1.12",
          "form": "인터넷 쇼핑몰에서 대량으로 구입해요",
          "original_form": "인터넷 쇼핑몰에서 대량으로 구입해요",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031138.1.13",
          "form": "연어 맛이 인기 좋아요",
          "original_form": "연어 맛이 인기 좋아요",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031138.1.14",
          "form": "고양이는 생선을 좋아하죠",
          "original_form": "고양이는 생선을 좋아하죠",
          "speaker_id": "2",
          "time": "20210904 17:24"
        }
