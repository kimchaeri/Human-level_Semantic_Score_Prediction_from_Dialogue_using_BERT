      "utterance": [
        {
          "id": "MDRW2100022716.1.1",
          "form": "배고팠으묜",
          "original_form": "배고팠으묜",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022716.1.2",
          "form": "더맛있는거먹으면되는ㄷ0",
          "original_form": "더맛있는거먹으면되는ㄷ0",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022716.1.3",
          "form": "어케 또 배가 부르다",
          "original_form": "어케 또 배가 부르다",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022716.1.4",
          "form": "아까 짜장면 먹은게 그대로",
          "original_form": "아까 짜장면 먹은게 그대로",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022716.1.5",
          "form": "배에잇는것만가틍ㅁ",
          "original_form": "배에잇는것만가틍ㅁ",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022716.1.6",
          "form": "글고 아까 저 배탈낫어요",
          "original_form": "글고 아까 저 배탈낫어요",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022716.1.7",
          "form": "헐랭 어쩌다",
          "original_form": "헐랭 어쩌다",
          "speaker_id": "2",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022716.1.8",
          "form": "모르겎음 갑자기",
          "original_form": "모르겎음 갑자기",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022716.1.9",
          "form": "배가 머무 다파서",
          "original_form": "배가 머무 다파서",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022716.1.10",
          "form": "화장실 지박령될번~",
          "original_form": "화장실 지박령될번~",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022716.1.11",
          "form": "짜장면때문아니여??!",
          "original_form": "짜장면때문아니여??!",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022716.1.12",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022716.1.13",
          "form": "먹자마자 배가아흘수잇나?!?!",
          "original_form": "먹자마자 배가아흘수잇나?!?!",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022716.1.14",
          "form": "진짜 짜자염ㄴ 먹자마자",
          "original_form": "진짜 짜자염ㄴ 먹자마자",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022716.1.15",
          "form": "바로 배나파서",
          "original_form": "바로 배나파서",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022716.1.16",
          "form": "화장실감9",
          "original_form": "화장실감9",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022716.1.17",
          "form": "진자 바로",
          "original_form": "진자 바로",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022716.1.18",
          "form": "깨고얼마안되서",
          "original_form": "깨고얼마안되서",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022716.1.19",
          "form": "기름진걸먹어버린거지..",
          "original_form": "기름진걸먹어버린거지..",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022716.1.20",
          "form": "아니면 어제 먹은거땜에..?",
          "original_form": "아니면 어제 먹은거땜에..?",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022716.1.21",
          "form": "어제 ..",
          "original_form": "어제 ..",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022716.1.22",
          "form": "카레랑 달걀말이먹고",
          "original_form": "카레랑 달걀말이먹고",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022716.1.23",
          "form": "햄버거먹고",
          "original_form": "햄버거먹고",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022716.1.24",
          "form": "토스으해먹엇다",
          "original_form": "토스으해먹엇다",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022716.1.25",
          "form": "ㅋ성마 토스트가?",
          "original_form": "ㅋ성마 토스트가?",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022716.1.26",
          "form": "토스트에 배탈날만한거있나",
          "original_form": "토스트에 배탈날만한거있나",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022716.1.27",
          "form": "나 씻고",
          "original_form": "나 씻고",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022716.1.28",
          "form": "고링~~~~",
          "original_form": "고링~~~~",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022716.1.29",
          "form": "ㄴㅇㅅ",
          "original_form": "ㄴㅇㅅ",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022716.1.30",
          "form": "나 다구웟다 맛잇다",
          "original_form": "나 다구웟다 맛잇다",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022716.1.31",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022716.1.32",
          "form": "저녘엔 냉동실에있던 양념갈비를 먹어야지",
          "original_form": "저녘엔 냉동실에있던 양념갈비를 먹어야지",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022716.1.33",
          "form": "밥먹으면서 이거하셈",
          "original_form": "밥먹으면서 이거하셈",
