        {
          "id": "MDRW2100033920.1.1",
          "form": "로제 뭐좋아하세요?",
          "original_form": "로제 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100033920.1.2",
          "form": "브랜드브랜드",
          "original_form": "브랜드브랜드",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100033920.1.3",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100033920.1.4",
          "form": "저 떡군이네요",
          "original_form": "저 떡군이네요",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100033920.1.5",
          "form": "이유가있죠",
          "original_form": "이유가있죠",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100033920.1.6",
          "form": "헉 원조라는 이유?",
          "original_form": "헉 원조라는 이유?",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100033920.1.7",
          "form": "하 저희집 근처에는 떡군이네가 없어요",
          "original_form": "하 저희집 근처에는 떡군이네가 없어요",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100033920.1.8",
          "form": "어떤가요 떡군이",
          "original_form": "어떤가요 떡군이",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100033920.1.9",
          "form": "스파게티면 추가가",
          "original_form": "스파게티면 추가가",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100033920.1.10",
          "form": "있습니다!!!!!!",
          "original_form": "있습니다!!!!!!",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100033920.1.11",
          "form": "배떡엔 없는것이죠",
          "original_form": "배떡엔 없는것이죠",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100033920.1.12",
          "form": "헐..!!!!!!!",
          "original_form": "헐..!!!!!!!",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100033920.1.13",
          "form": "떡군이네 천재다",
          "original_form": "떡군이네 천재다",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100033920.1.14",
          "form": "저는 명랑 로제를 좋아했거즌요",
          "original_form": "저는 명랑 로제를 좋아했거즌요",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100033920.1.15",
          "form": "명랑에언제부터",
          "original_form": "명랑에언제부터",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100033920.1.16",
          "form": "로제떡볶이를 팔았죠?!!",
          "original_form": "로제떡볶이를 팔았죠?!!",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100033920.1.17",
          "form": "?!?!?!?!",
          "original_form": "?!?!?!?!",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033920.1.18",
          "form": "쓰읍 name2님 한번만 드셔보십쇼",
          "original_form": "쓰읍 &name2&님 한번만 드셔보십쇼",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033920.1.19",
          "form": "따악 한번 드셔보시면",
          "original_form": "따악 한번 드셔보시면",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100033920.1.20",
          "form": "핫도그 놈들이 꽤 하네 이러실 겁니다",
          "original_form": "핫도그 놈들이 꽤 하네 이러실 겁니다",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100033920.1.21",
          "form": "와우ㅋㅋㅋㅋ",
          "original_form": "와우ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033920.1.22",
          "form": "로제거의다먹어봤는데",
          "original_form": "로제거의다먹어봤는데",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033920.1.23",
          "form": "명랑은 새롭군요",
          "original_form": "명랑은 새롭군요",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033920.1.24",
          "form": "아주 혼내줘야겠어요",
          "original_form": "아주 혼내줘야겠어요",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033920.1.25",
          "form": "한번만..드셔보십쇼 추천추천",
          "original_form": "한번만..드셔보십쇼 추천추천",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100033920.1.26",
          "form": "핫도그 집에서 이런맛이?!",
          "original_form": "핫도그 집에서 이런맛이?!",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100033920.1.27",
          "form": "또 좋아하는 음식.있으신가요",
          "original_form": "또 좋아하는 음식.있으신가요",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100033920.1.28",
          "form": "ㅋㅋㅋ좋네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ좋네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100033920.1.29",
          "form": "전 사실",
          "original_form": "전 사실",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100033920.1.30",
          "form": "원픽은 초밥이예요",
          "original_form": "원픽은 초밥이예요",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100033920.1.31",
          "form": "헉 만약 점심때 선배가 밥사준다하면 회전초밥vs 판초밥",
          "original_form": "헉 만약 점심때 선배가 밥사준다하면 회전초밥vs 판초밥",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100033920.1.32",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100033920.1.33",
          "form": "판초밥이죠",
          "original_form": "판초밥이죠",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100033920.1.34",
          "form": "당 연 히",
          "original_form": "당 연 히",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100033920.1.35",
          "form": "쓰읍... 제가 뭘 몰랐다",
          "original_form": "쓰읍... 제가 뭘 몰랐다",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100033920.1.36",
          "form": "밸붕이었군요",
          "original_form": "밸붕이었군요",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100033920.1.37",
          "form": "그럼 락교 vs 단무지",
          "original_form": "그럼 락교 vs 단무지",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100033920.1.38",
          "form": "초밥에요??",
          "original_form": "초밥에요??",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100033920.1.39",
          "form": "초밥에 단무지를 먹나요??!!!!",
          "original_form": "초밥에 단무지를 먹나요??!!!!",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100033920.1.40",
          "form": "이거이거 초밥알못이시군요",
          "original_form": "이거이거 초밥알못이시군요",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100033920.1.41",
          "form": "!!!!!!!!!!",
          "original_form": "!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100033920.1.42",
          "form": "찐은 다르군요",
          "original_form": "찐은 다르군요",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100033920.1.43",
          "form": "그럼 회드실 때 초밥 자기가 싸먹는 사람 vs 초밥먹을 때 위만 건져먹는 사람",
          "original_form": "그럼 회드실 때 초밥 자기가 싸먹는 사람 vs 초밥먹을 때 위만 건져먹는 사람",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100033920.1.44",
          "form": "누가 더 싫나요",
          "original_form": "누가 더 싫나요",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100033920.1.45",
          "form": "근데 뭐사실",
          "original_form": "근데 뭐사실",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100033920.1.46",
          "form": "위만건져먹는건 제가 회전초밥집사장도아니고 개인이그런다면 식성이니 상관할바는아니죠ㅋㅋㅋ",
          "original_form": "위만건져먹는건 제가 회전초밥집사장도아니고 개인이그런다면 식성이니 상관할바는아니죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100033920.1.47",
          "form": "물론뷔페에서 저런다면진상이지만요",
          "original_form": "물론뷔페에서 저런다면진상이지만요",
          "speaker_id": "2",
