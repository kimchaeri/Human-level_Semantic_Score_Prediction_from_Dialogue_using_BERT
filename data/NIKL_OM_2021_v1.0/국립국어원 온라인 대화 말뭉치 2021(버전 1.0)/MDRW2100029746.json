      "utterance": [
        {
          "id": "MDRW2100029746.1.1",
          "form": "넷플릭스 재밌는거 뭐 있나요?",
          "original_form": "넷플릭스 재밌는거 뭐 있나요?",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100029746.1.2",
          "form": "최근엔 DP재밋게 봤어요",
          "original_form": "최근엔 DP재밋게 봤어요",
          "speaker_id": "1",
          "time": "20210905 23:56"
        },
        {
          "id": "MDRW2100029746.1.3",
          "form": "아 맞아요 예전에 웹툰으로 보고 까먹고있었는데",
          "original_form": "아 맞아요 예전에 웹툰으로 보고 까먹고있었는데",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100029746.1.4",
          "form": "광고 보고 바로 생각나더라구요ㅋㅋ",
          "original_form": "광고 보고 바로 생각나더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100029746.1.5",
          "form": "저두 하루만에 다봤어용",
          "original_form": "저두 하루만에 다봤어용",
          "speaker_id": "2",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100029746.1.6",
          "form": "브리저드 이후에 재밋게본거같어요",
          "original_form": "브리저드 이후에 재밋게본거같어요",
          "speaker_id": "1",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100029746.1.7",
          "form": "브리저드는 뭐예용?",
          "original_form": "브리저드는 뭐예용?",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100029746.1.8",
          "form": "영화예요?",
          "original_form": "영화예요?",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100029746.1.9",
          "form": "외국 드라마에요~",
          "original_form": "외국 드라마에요~",
          "speaker_id": "1",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100029746.1.10",
          "form": "나름재밋었어요",
          "original_form": "나름재밋었어요",
          "speaker_id": "1",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100029746.1.11",
          "form": "오 봐바야겠어용",
          "original_form": "오 봐바야겠어용",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100029746.1.12",
          "form": "오늘부터 킹덤 보고있는데",
          "original_form": "오늘부터 킹덤 보고있는데",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100029746.1.13",
          "form": "어두운곳 볼때마다 좀비튀어나올것같아요ㅋㅋㅋ",
          "original_form": "어두운곳 볼때마다 좀비튀어나올것같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100029746.1.14",
          "form": "ㅋㅋㅋ킹덤 재밋죠~ 시즌3기다리고 잇어요",
          "original_form": "ㅋㅋㅋ킹덤 재밋죠~ 시즌3기다리고 잇어요",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100029746.1.15",
          "form": "아신전 보는데 엄청 재밌더라구요",
          "original_form": "아신전 보는데 엄청 재밌더라구요",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100029746.1.16",
          "form": "아신전은 호불호갈리던데 재밋게 보셨군요",
          "original_form": "아신전은 호불호갈리던데 재밋게 보셨군요",
          "speaker_id": "1",
