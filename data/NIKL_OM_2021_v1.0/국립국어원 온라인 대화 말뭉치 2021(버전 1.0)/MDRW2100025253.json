      "utterance": [
        {
          "id": "MDRW2100025253.1.1",
          "form": "코로나가 끝나야 직관도 갈텐데 ㅎㅎ",
          "original_form": "코로나가 끝나야 직관도 갈텐데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 03:05"
        },
        {
          "id": "MDRW2100025253.1.2",
          "form": "요새 무관중 하니까 중계도 좀 심심하더라구요",
          "original_form": "요새 무관중 하니까 중계도 좀 심심하더라구요",
          "speaker_id": "1",
          "time": "20210901 03:07"
        },
        {
          "id": "MDRW2100025253.1.3",
          "form": "맞아요 심심해요ㅋㅋ",
          "original_form": "맞아요 심심해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:09"
        },
        {
          "id": "MDRW2100025253.1.4",
          "form": "막 소리나고 해야하는데",
          "original_form": "막 소리나고 해야하는데",
          "speaker_id": "2",
          "time": "20210901 03:09"
        },
        {
          "id": "MDRW2100025253.1.5",
          "form": "카메라도 자꾸 달 잡고 광고판 잡고 ㅋㅋ",
          "original_form": "카메라도 자꾸 달 잡고 광고판 잡고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:10"
        },
        {
          "id": "MDRW2100025253.1.6",
          "form": "ㅋㅋ 스포츠업계도",
          "original_form": "ㅋㅋ 스포츠업계도",
          "speaker_id": "2",
          "time": "20210901 03:11"
        },
        {
          "id": "MDRW2100025253.1.7",
          "form": "죽을맛일거 같아요",
          "original_form": "죽을맛일거 같아요",
          "speaker_id": "2",
          "time": "20210901 03:11"
        },
        {
          "id": "MDRW2100025253.1.8",
          "form": "관중 조금 받는 것 보다는 아예 무관중이 돈은 덜 든다고 하더라구요",
          "original_form": "관중 조금 받는 것 보다는 아예 무관중이 돈은 덜 든다고 하더라구요",
          "speaker_id": "1",
          "time": "20210901 03:12"
        },
        {
          "id": "MDRW2100025253.1.9",
          "form": "그죠 운영비용이 있어서 ㅠㅠ",
          "original_form": "그죠 운영비용이 있어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 03:16"
        },
        {
          "id": "MDRW2100025253.1.10",
          "form": "고척돔에는 딱 2번 가봤는데 확실히 표값이 비싸더라구요",
          "original_form": "고척돔에는 딱 2번 가봤는데 확실히 표값이 비싸더라구요",
          "speaker_id": "1",
          "time": "20210901 03:17"
        },
        {
          "id": "MDRW2100025253.1.11",
          "form": "고척 비싸쥬 ㅠㅠ",
          "original_form": "고척 비싸쥬 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 03:18"
        },
        {
          "id": "MDRW2100025253.1.12",
          "form": "그래도 집 가까워서 ㅋㅋㅋ",
          "original_form": "그래도 집 가까워서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:18"
        },
        {
          "id": "MDRW2100025253.1.13",
          "form": "근데 코로나...",
          "original_form": "근데 코로나...",
          "speaker_id": "2",
          "time": "20210901 03:18"
        },
        {
          "id": "MDRW2100025253.1.14",
          "form": "아무래도 추석때까진 무관중 하지 않을까요?",
          "original_form": "아무래도 추석때까진 무관중 하지 않을까요?",
          "speaker_id": "1",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025253.1.15",
          "form": "올 시즌 끝날때까지.....",
          "original_form": "올 시즌 끝날때까지.....",
          "speaker_id": "2",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025253.1.16",
          "form": "같은데요 ㅠ",
          "original_form": "같은데요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025253.1.17",
          "form": "그럴지도요ㅋㅋ",
          "original_form": "그럴지도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025253.1.18",
          "form": "언제 끝나나요~~~",
          "original_form": "언제 끝나나요~~~",
          "speaker_id": "2",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025253.1.19",
          "form": "올 시즌 무사히 마쳤으면 좋겠어요ㅠ",
          "original_form": "올 시즌 무사히 마쳤으면 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025253.1.20",
          "form": "올해 유독 말도 많고 탈도 많고ㅋㅋ",
          "original_form": "올해 유독 말도 많고 탈도 많고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025253.1.21",
          "form": "올시즌 두산 가을야구",
          "original_form": "올시즌 두산 가을야구",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100025253.1.22",
