          "original_form": "팝콘 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007148.1.13",
          "form": "샀어요",
          "original_form": "샀어요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007148.1.14",
