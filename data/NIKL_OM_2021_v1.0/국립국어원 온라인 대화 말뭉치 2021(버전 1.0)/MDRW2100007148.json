      "utterance": [
        {
          "id": "MDRW2100007148.1.1",
          "form": "전 왓챠 공짜 이용하는데",
          "original_form": "전 왓챠 공짜 이용하는데",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007148.1.2",
          "form": "볼게 없네요",
          "original_form": "볼게 없네요",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007148.1.3",
          "form": "넷플과 같은 상황이예요",
          "original_form": "넷플과 같은 상황이예요",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007148.1.4",
          "form": "네 맞아요 ㅋㅋㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007148.1.5",
          "form": "저도 왓챠 찜해놓고 안 보게 되는..",
          "original_form": "저도 왓챠 찜해놓고 안 보게 되는..",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007148.1.6",
          "form": "최근에는 그거 보고 있었어요",
          "original_form": "최근에는 그거 보고 있었어요",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007148.1.7",
          "form": "액션 영화 한편보고 안봐요",
          "original_form": "액션 영화 한편보고 안봐요",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007148.1.8",
          "form": "시티오브갓",
          "original_form": "시티오브갓",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007148.1.9",
          "form": "오 봤었는데요",
          "original_form": "오 봤었는데요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007148.1.10",
          "form": "ㅋㅋㅋㅋ 그래도 무료라 다행이네요..",
          "original_form": "ㅋㅋㅋㅋ 그래도 무료라 다행이네요..",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007148.1.11",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007148.1.12",
