      "utterance": [
        {
          "id": "MDRW2100010809.1.1",
          "form": "최근 뭐 사셨어요",
          "original_form": "최근 뭐 사셨어요",
          "speaker_id": "2",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010809.1.2",
          "form": "제가 신일 선풍기 이야기했던가요 ㅋㅋ",
          "original_form": "제가 신일 선풍기 이야기했던가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010809.1.3",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010809.1.4",
          "form": "선풍기 에피소드 있나요?",
          "original_form": "선풍기 에피소드 있나요?",
          "speaker_id": "1",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010809.1.5",
          "form": "좀 긴데 ㅋ적당히 맞장구부탁드려요ㅋsk 가족결합 이 10년간 자동갱신이었는데",
          "original_form": "좀 긴데 ㅋ적당히 맞장구부탁드려요ㅋsk 가족결합 이 10년간 자동갱신이었는데",
          "speaker_id": "2",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100010809.1.6",
          "form": "알고보니 그게 자동갱신말고 전화로 문의하면 장기이용거래혜택 으로",
          "original_form": "알고보니 그게 자동갱신말고 전화로 문의하면 장기이용거래혜택 으로",
          "speaker_id": "2",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010809.1.7",
          "form": "상품권을 준다더라고요",
          "original_form": "상품권을 준다더라고요",
          "speaker_id": "2",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010809.1.8",
          "form": "어휴 10년간 그것도 모르고 호구였다가 이번에 9만원 받앟어요",
          "original_form": "어휴 10년간 그것도 모르고 호구였다가 이번에 9만원 받앟어요",
          "speaker_id": "2",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010809.1.9",
          "form": "아 그러시구나 저는 이번에 하이마트에서 생돈주고하나샀어요",
          "original_form": "아 그러시구나 저는 이번에 하이마트에서 생돈주고하나샀어요",
          "speaker_id": "1",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010809.1.10",
          "form": "디자인이 예뻐서 와이프가 좋아했어요",
          "original_form": "디자인이 예뻐서 와이프가 좋아했어요",
          "speaker_id": "1",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010809.1.11",
          "form": "저희는 르젠꺼샀어요",
          "original_form": "저희는 르젠꺼샀어요",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010809.1.12",
          "form": "하이마트에서 머지포인트로 구입했는데",
          "original_form": "하이마트에서 머지포인트로 구입했는데",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010809.1.13",
          "form": "할인율 좋아서 머지포인트 더 구입했다가 낚였어요",
          "original_form": "할인율 좋아서 머지포인트 더 구입했다가 낚였어요",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010809.1.14",
          "form": "저런 그거 완전 휴지된건가요?",
          "original_form": "저런 그거 완전 휴지된건가요?",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010809.1.15",
          "form": "차라리 복구되길 바랬는데ㅜ",
          "original_form": "차라리 복구되길 바랬는데ㅜ",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010809.1.16",
          "form": "환불이 진짜 이뤄질지 된다면 언제 될지도 모르겠어요",
          "original_form": "환불이 진짜 이뤄질지 된다면 언제 될지도 모르겠어요",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010809.1.17",
          "form": "머지사용하세요",
          "original_form": "머지사용하세요",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010809.1.18",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010809.1.19",
          "form": "저는 예전에 헌혈하고",
          "original_form": "저는 예전에 헌혈하고",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010809.1.20",
          "form": "사천원받았던기억이나요",
          "original_form": "사천원받았던기억이나요",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010809.1.21",
          "form": "헌헐하면 영화티켓이나",
          "original_form": "헌헐하면 영화티켓이나",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010809.1.22",
          "form": "문화상품권주지 않나요?",
          "original_form": "문화상품권주지 않나요?",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010809.1.23",
          "form": "그거말고도 또 줬어요",
          "original_form": "그거말고도 또 줬어요",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010809.1.24",
          "form": "진심 머지 포인트 여기저기에 제휴했었네요",
          "original_form": "진심 머지 포인트 여기저기에 제휴했었네요",
          "speaker_id": "2",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010809.1.25",
          "form": "네 사용처 많아서 좋았었는데",
          "original_form": "네 사용처 많아서 좋았었는데",
          "speaker_id": "1",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010809.1.26",
          "form": "이렇게 문닫을줄이야",
          "original_form": "이렇게 문닫을줄이야",
          "speaker_id": "1",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010809.1.27",
          "form": "제 사천원은 진짜 푼돈이라 잊었어요",
          "original_form": "제 사천원은 진짜 푼돈이라 잊었어요",
          "speaker_id": "2",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100010809.1.28",
          "form": "천단위 넣은 사람들도 있다는데 ㄷㄷ",
          "original_form": "천단위 넣은 사람들도 있다는데 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100010809.1.29",
          "form": "그런사람들은 속상하겠다 진짜",
          "original_form": "그런사람들은 속상하겠다 진짜",
          "speaker_id": "1",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100010809.1.30",
          "form": "폭탄돌리기로 당한 자영업자들 사연듣고",
          "original_form": "폭탄돌리기로 당한 자영업자들 사연듣고",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100010809.1.31",
          "form": "차라리 복구되었으면 싶더라고요",
          "original_form": "차라리 복구되었으면 싶더라고요",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100010809.1.32",
          "form": "그분들 돈 받게",
          "original_form": "그분들 돈 받게",
          "speaker_id": "2",
          "time": "20210823 22:48"
        }
      ]
