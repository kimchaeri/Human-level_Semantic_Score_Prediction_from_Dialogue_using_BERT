      "utterance": [
        {
          "id": "MDRW2100012210.1.1",
          "form": "혹시 싸이월드 세대신가요?",
          "original_form": "혹시 싸이월드 세대신가요?",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012210.1.2",
          "form": "아 어릴때 썼었어요 name1은요ㅋㅋ",
          "original_form": "아 어릴때 썼었어요 &name1&은요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100012210.1.3",
          "form": "저도요 ㅋㅋㅋ 추억의 싸이월드..",
          "original_form": "저도요 ㅋㅋㅋ 추억의 싸이월드..",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100012210.1.4",
          "form": "저번에 부활한다고 뭐 신청받길래 아이디조회 해봤었어요 ㅋㅋ",
          "original_form": "저번에 부활한다고 뭐 신청받길래 아이디조회 해봤었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100012210.1.5",
          "form": "으아 저는 못쳐다보겠던데 용자시다",
          "original_form": "으아 저는 못쳐다보겠던데 용자시다",
          "speaker_id": "2",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100012210.1.6",
          "form": "ㅋㅋㅋ 그땐 아이디조회만 하는 거라 사진은 안 나와요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 그땐 아이디조회만 하는 거라 사진은 안 나와요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100012210.1.7",
          "form": "없애려면 아이디 찾아야죠 ㅋㅋㅋ",
          "original_form": "없애려면 아이디 찾아야죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100012210.1.8",
          "form": "ㅋㅈㅋ ㅋ아이디기억하세요?",
          "original_form": "ㅋㅈㅋ ㅋ아이디기억하세요?",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012210.1.9",
          "form": "다까먹었는데",
          "original_form": "다까먹었는데",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012210.1.10",
          "form": "네 저는 아이디로 쓰는 메일이 딱 하나뿐이라 ㅋㅋ",
          "original_form": "네 저는 아이디로 쓰는 메일이 딱 하나뿐이라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012210.1.11",
          "form": "근데 노래들은 그냥 다 날라갔더라고요 당연한 거지만 ㅎㅎ",
          "original_form": "근데 노래들은 그냥 다 날라갔더라고요 당연한 거지만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012210.1.12",
          "form": "아 저는 이거저거 만들었는데 일편단심이시군요",
          "original_form": "아 저는 이거저거 만들었는데 일편단심이시군요",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012210.1.13",
          "form": "네 싸이 거의 망하기 전까지 붙들고 있었던 거 같아요 ㅋㅋㅋ",
          "original_form": "네 싸이 거의 망하기 전까지 붙들고 있었던 거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012210.1.14",
          "form": "오오 대단하시다",
          "original_form": "오오 대단하시다",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012210.1.15",
          "form": "싸이 흑역사 같은거 있으신가요 ㅎㅎ",
          "original_form": "싸이 흑역사 같은거 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012210.1.16",
          "form": "열심히하셨군요",
          "original_form": "열심히하셨군요",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012210.1.17",
          "form": "전..어릴때사진 있을거같네요ㅋㅋㅋㅋ",
          "original_form": "전..어릴때사진 있을거같네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012210.1.18",
          "form": "사진은 그래도 괜찮지 않나요 ㅋㅋㅋ 추억도 되고 ㅎㅎ",
          "original_form": "사진은 그래도 괜찮지 않나요 ㅋㅋㅋ 추억도 되고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012210.1.19",
          "form": "음 그럴거 같네요ㅋㅋ젊은 내사진ㅋㅋ",
          "original_form": "음 그럴거 같네요ㅋㅋ젊은 내사진ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012210.1.20",
          "form": "name1도 많겠네요",
          "original_form": "&name1&도 많겠네요",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012210.1.21",
          "form": "ㅋㅋㅋ 네 다시 열리면 골라서 저장하려고요 ㅎㅎ",
