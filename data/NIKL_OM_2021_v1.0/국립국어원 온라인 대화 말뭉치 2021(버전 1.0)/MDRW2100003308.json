        },
        {
          "id": "MDRW2100003308.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 21:56"
        },
        {
          "id": "MDRW2100003308.1.3",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003308.1.4",
          "form": "저는엄청좋아해요",
          "original_form": "저는엄청좋아해요",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003308.1.5",
          "form": "네 저도 여행 좋아해요",
          "original_form": "네 저도 여행 좋아해요",
          "speaker_id": "2",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003308.1.6",
          "form": "어디어디 다녀봤어요?",
          "original_form": "어디어디 다녀봤어요?",
          "speaker_id": "2",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003308.1.7",
          "form": "저는 세계여행 1년정도 다녔어요!!!",
          "original_form": "저는 세계여행 1년정도 다녔어요!!!",
          "speaker_id": "1",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003308.1.8",
          "form": "국내는 거의다 가본것같아요. 직업이 출장이 많아서요",
          "original_form": "국내는 거의다 가본것같아요. 직업이 출장이 많아서요",
          "speaker_id": "1",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003308.1.9",
          "form": "오오 저는 아직 해외는 안가봤는데 함 가보고싶어요ㅠㅠ",
          "original_form": "오오 저는 아직 해외는 안가봤는데 함 가보고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 22:03"
        },
        {
          "id": "MDRW2100003308.1.10",
          "form": "코로나때문에 ㅠㅠㅠ나이가어떻게되세요?",
          "original_form": "코로나때문에 ㅠㅠㅠ나이가어떻게되세요?",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003308.1.11",
          "form": "20대 초반이에요",
          "original_form": "20대 초반이에요",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003308.1.12",
          "form": "아아 저도 27살때 처음해외나가봤어요",
          "original_form": "아아 저도 27살때 처음해외나가봤어요",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003308.1.13",
          "form": "그뒤로 계속나가게되요",
          "original_form": "그뒤로 계속나가게되요",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003308.1.14",
          "form": "언젠가 코로나없어지고 맘편히 해외여행 갈수있으면 좋겠어요ㅎㅎ",
          "original_form": "언젠가 코로나없어지고 맘편히 해외여행 갈수있으면 좋겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 22:12"
        },
        {
          "id": "MDRW2100003308.1.15",
          "form": "가장 좋았거나 멋있었던??데가 어딘지 알려주세요!",
          "original_form": "가장 좋았거나 멋있었던??데가 어딘지 알려주세요!",
          "speaker_id": "2",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003308.1.16",
          "form": "저는 조지아가 가장좋아요",
          "original_form": "저는 조지아가 가장좋아요",
          "speaker_id": "1",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100003308.1.17",
          "form": "1년에 360일 무비자에요",
          "original_form": "1년에 360일 무비자에요",
          "speaker_id": "1",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100003308.1.18",
          "form": "우와ㅋㅋㅋㅋㅋ1년이나 무비자네요",
          "original_form": "우와ㅋㅋㅋㅋㅋ1년이나 무비자네요",
          "speaker_id": "2",
          "time": "20210820 22:18"
        },
        {
          "id": "MDRW2100003308.1.19",
          "form": "네네 진짜좋아요",
          "original_form": "네네 진짜좋아요",
          "speaker_id": "1",
          "time": "20210820 22:26"
        },
        {
          "id": "MDRW2100003308.1.20",
          "form": "나중에가보세요",
