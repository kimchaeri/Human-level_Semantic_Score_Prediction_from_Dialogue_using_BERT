      "utterance": [
        {
          "id": "MDRW2100026321.1.1",
          "form": "오늘 영화 봤다ㅎ",
          "original_form": "오늘 영화 봤다ㅎ",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026321.1.2",
          "form": "오 진짜? 영화 몇편이었는데 이렇게 금방 봤어?",
          "original_form": "오 진짜? 영화 몇편이었는데 이렇게 금방 봤어?",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026321.1.3",
          "form": "나, 다니엘 브레이크",
          "original_form": "나, 다니엘 브레이크",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026321.1.4",
          "form": "예전에봤던거라 훅훅 넘기며 봤어",
          "original_form": "예전에봤던거라 훅훅 넘기며 봤어",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026321.1.5",
          "form": "소장격으로 둔건데\n지금 삭제하긴했는데\n옮길걸 그랬나 생각드네",
          "original_form": "소장격으로 둔건데\n지금 삭제하긴했는데\n옮길걸 그랬나 생각드네",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026321.1.6",
          "form": "영화괜찮아",
          "original_form": "영화괜찮아",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026321.1.7",
          "form": "한편밖에없었는데 용량 엄청 차지했나보다",
          "original_form": "한편밖에없었는데 용량 엄청 차지했나보다",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026321.1.8",
          "form": "아나 내가 오늘 본게 저 한편이고",
          "original_form": "아나 내가 오늘 본게 저 한편이고",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026321.1.9",
          "form": "영화폰에 쫌 많아ㅎ",
          "original_form": "영화폰에 쫌 많아ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026321.1.10",
          "form": "맞나 뭐뭐있어? 나 본것도 있으려나 ㅋㅋ",
          "original_form": "맞나 뭐뭐있어? 나 본것도 있으려나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026321.1.11",
          "form": "부르고뉴, 와인에서 찾은 인생",
          "original_form": "부르고뉴, 와인에서 찾은 인생",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026321.1.12",
          "form": "조제호랑이그리고물고기들",
          "original_form": "조제호랑이그리고물고기들",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026321.1.13",
          "form": "클림트",
          "original_form": "클림트",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026321.1.14",
          "form": "선생님과길고양이",
          "original_form": "선생님과길고양이",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026321.1.15",
          "form": "킬유어달링",
          "original_form": "킬유어달링",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026321.1.16",
          "form": "늑대아이",
          "original_form": "늑대아이",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026321.1.17",
          "form": "부산행",
          "original_form": "부산행",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026321.1.18",
          "form": "녹차의맛",
          "original_form": "녹차의맛",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026321.1.19",
          "form": "파리에듀케이션",
          "original_form": "파리에듀케이션",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026321.1.20",
          "form": "강아지똥",
          "original_form": "강아지똥",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026321.1.21",
          "form": "우아한세계",
          "original_form": "우아한세계",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026321.1.22",
          "form": "근데 공유해주고싶어도 어캐 가려나 모르겠다ㅎ",
          "original_form": "근데 공유해주고싶어도 어캐 가려나 모르겠다ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026321.1.23",
          "form": "여기서 본거 부산행밖에 없네 ㅋㅋ",
          "original_form": "여기서 본거 부산행밖에 없네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026321.1.24",
          "form": "공유안해줘도 돼 ㅋㅋ 걍 뭐 있는지 궁금해서 ㅋㅋ",
          "original_form": "공유안해줘도 돼 ㅋㅋ 걍 뭐 있는지 궁금해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026321.1.25",
          "form": "아 응ㅎ",
          "original_form": "아 응ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026321.1.26",
          "form": "강아지똥은 우리 예전에 중학교 교과서에 나왔던 그 강아지똥인가?",
          "original_form": "강아지똥은 우리 예전에 중학교 교과서에 나왔던 그 강아지똥인가?",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100026321.1.27",
          "form": "응 맥락은 같은데 뭔가 기독교에서 만든거같어\n위화감있더라\n영상 귀여워서 봤는데",
          "original_form": "응 맥락은 같은데 뭔가 기독교에서 만든거같어\n위화감있더라\n영상 귀여워서 봤는데",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100026321.1.28",
          "form": "오 진짜? 애니메이션인가?",
          "original_form": "오 진짜? 애니메이션인가?",
          "speaker_id": "1",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100026321.1.29",
          "form": "응 애니",
          "original_form": "응 애니",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100026321.1.30",
          "form": "귀여워ㅎㅎ",
          "original_form": "귀여워ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100026321.1.31",
          "form": "근데 기독교꺼인가 싶어ㅋㅋ",
          "original_form": "근데 기독교꺼인가 싶어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100026321.1.32",
          "form": "예전 내용생각하면 귀엽겠당 ㅋㅋ",
          "original_form": "예전 내용생각하면 귀엽겠당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100026321.1.33",
          "form": "기독교 교육용 애니메이션인가 ㅋㅋ",
          "original_form": "기독교 교육용 애니메이션인가 ㅋㅋ",
          "speaker_id": "1",
