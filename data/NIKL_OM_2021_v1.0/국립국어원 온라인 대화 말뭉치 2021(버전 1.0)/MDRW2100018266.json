      "utterance": [
        {
          "id": "MDRW2100018266.1.1",
          "form": "와 부러워요",
          "original_form": "와 부러워요",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018266.1.2",
          "form": "엄마가 먹고싶어해서 사왔는데",
          "original_form": "엄마가 먹고싶어해서 사왔는데",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018266.1.3",
          "form": "먹자마자 치과갔어요 ㅋㅋ",
          "original_form": "먹자마자 치과갔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018266.1.4",
          "form": "아쉽네요 ㅜ ㅎㅎ",
          "original_form": "아쉽네요 ㅜ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018266.1.5",
          "form": "네 ㅠㅠ 그래서 식은 치킨 먹었어요ㅠㅠ",
          "original_form": "네 ㅠㅠ 그래서 식은 치킨 먹었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018266.1.6",
          "form": "에궁 따뜻할 때 먹어야 제맛인데",
          "original_form": "에궁 따뜻할 때 먹어야 제맛인데",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018266.1.7",
          "form": "네 ㅠㅠㅠ 따뜻할 때 먹으면",
          "original_form": "네 ㅠㅠㅠ 따뜻할 때 먹으면",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018266.1.8",
          "form": "껍질이 엄청 바삭바삭한데요 ㅠㅠ",
          "original_form": "껍질이 엄청 바삭바삭한데요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018266.1.9",
          "form": "네 근데 양이 좀 적지 않아요?",
          "original_form": "네 근데 양이 좀 적지 않아요?",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018266.1.10",
          "form": "네 ㅋㅋㅋ 그래서 2마리 사왔어요",
          "original_form": "네 ㅋㅋㅋ 그래서 2마리 사왔어요",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018266.1.11",
          "form": "옛날통닭은 1인1닭 필수에요",
          "original_form": "옛날통닭은 1인1닭 필수에요",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018266.1.12",
          "form": "네 ㅋㅋ 맛닭꼬도 맛있는데",
          "original_form": "네 ㅋㅋ 맛닭꼬도 맛있는데",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018266.1.13",
          "form": "아세요??",
          "original_form": "아세요??",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018266.1.14",
          "form": "아 네 ㅎㅎ 맛있죠",
          "original_form": "아 네 ㅎㅎ 맛있죠",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018266.1.15",
          "form": "어제 원래 맛닭꼬 먹을랬는데",
          "original_form": "어제 원래 맛닭꼬 먹을랬는데",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018266.1.16",
          "form": "문닫아서 못먹었어요 ㅠㅠ",
          "original_form": "문닫아서 못먹었어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018266.1.17",
          "form": "요즘 다들 문 일찍 닫더라구요",
          "original_form": "요즘 다들 문 일찍 닫더라구요",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018266.1.18",
          "form": "네 ㅠㅠ 어제 옛날통닭도 문 닫고 ㅠㅠ",
          "original_form": "네 ㅠㅠ 어제 옛날통닭도 문 닫고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018266.1.19",
          "form": "그래서 오늘 드셨군요 ㅎㅎ",
          "original_form": "그래서 오늘 드셨군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018266.1.20",
          "form": "게장 드세요?",
          "original_form": "게장 드세요?",
