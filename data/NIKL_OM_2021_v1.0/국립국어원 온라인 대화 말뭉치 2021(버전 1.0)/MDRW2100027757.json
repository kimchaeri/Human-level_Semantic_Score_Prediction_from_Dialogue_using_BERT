      "utterance": [
        {
          "id": "MDRW2100027757.1.1",
          "form": "주제 식음료네 배고프다",
          "original_form": "주제 식음료네 배고프다",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027757.1.2",
          "form": "난 밥 먹었지",
          "original_form": "난 밥 먹었지",
          "speaker_id": "1",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100027757.1.3",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100027757.1.4",
          "form": "점심 먹어 이젠 점저인가 점저보단 좀 이른데",
          "original_form": "점심 먹어 이젠 점저인가 점저보단 좀 이른데",
          "speaker_id": "1",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100027757.1.5",
          "form": "강의가 한시간 반 간격으로 오전 9시부터 오후3시까지 있는데",
          "original_form": "강의가 한시간 반 간격으로 오전 9시부터 오후3시까지 있는데",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100027757.1.6",
          "form": "개강 첫날이라 전부 화상강의여서 못먹었어",
          "original_form": "개강 첫날이라 전부 화상강의여서 못먹었어",
          "speaker_id": "2",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100027757.1.7",
          "form": "끔찍하다.............",
          "original_form": "끔찍하다.............",
          "speaker_id": "1",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100027757.1.8",
          "form": "슬프다...",
          "original_form": "슬프다...",
          "speaker_id": "2",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100027757.1.9",
          "form": "뭔가 끝내주는걸 먹고싶은데 뭔지 모르겠네 빵은 아니고",
          "original_form": "뭔가 끝내주는걸 먹고싶은데 뭔지 모르겠네 빵은 아니고",
          "speaker_id": "2",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100027757.1.10",
          "form": "그래도 10분 남았네 뭐라도 줏어먹어",
          "original_form": "그래도 10분 남았네 뭐라도 줏어먹어",
          "speaker_id": "1",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100027757.1.11",
          "form": "끝내주는 거... 면류?",
          "original_form": "끝내주는 거... 면류?",
          "speaker_id": "1",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100027757.1.12",
          "form": "탈락!",
          "original_form": "탈락!",
          "speaker_id": "2",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100027757.1.13",
          "form": "덮밥류 먹고싶다",
          "original_form": "덮밥류 먹고싶다",
          "speaker_id": "2",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100027757.1.14",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "1",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100027757.1.15",
          "form": "돈가스덮밥",
          "original_form": "돈가스덮밥",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027757.1.16",
          "form": "치킨덮밥",
          "original_form": "치킨덮밥",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027757.1.17",
          "form": "흠..",
          "original_form": "흠..",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027757.1.18",
          "form": "다른덮밥 뭐있지",
          "original_form": "다른덮밥 뭐있지",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027757.1.19",
          "form": "좀 삼삼한걸로 먹고싶다",
          "original_form": "좀 삼삼한걸로 먹고싶다",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027757.1.20",
          "form": "삼...삼...?",
          "original_form": "삼...삼...?",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027757.1.21",
          "form": "연어... 사실 덮밥 잘 모름",
          "original_form": "연어... 사실 덮밥 잘 모름",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027757.1.22",
          "form": "장어?!",
          "original_form": "장어?!",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027757.1.23",
          "form": "그건 좀",
          "original_form": "그건 좀",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027757.1.24",
          "form": "그치만 장어구이는 먹고싶다",
          "original_form": "그치만 장어구이는 먹고싶다",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027757.1.25",
          "form": "맛있냐 안 먹어봤어",
          "original_form": "맛있냐 안 먹어봤어",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027757.1.26",
          "form": "어딘지 기억안나도 전에 갔던 장어구이 집에서",
          "original_form": "어딘지 기억안나도 전에 갔던 장어구이 집에서",
          "speaker_id": "2",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027757.1.27",
          "form": "응 소금구이랑 양념구이 있었는데",
          "original_form": "응 소금구이랑 양념구이 있었는데",
          "speaker_id": "2",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027757.1.28",
          "form": "양념구이가 진짜진짜 맛있었어",
          "original_form": "양념구이가 진짜진짜 맛있었어",
          "speaker_id": "2",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027757.1.29",
          "form": "어딘지 알아와",
          "original_form": "어딘지 알아와",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027757.1.30",
          "form": "대박...",
          "original_form": "대박...",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027757.1.31",
          "form": "나 근데 궁금한거 있어 장어구이... 구워서 뚝뚝 잘라 바로 먹던데 뼈 없어? 다 빼나?",
          "original_form": "나 근데 궁금한거 있어 장어구이... 구워서 뚝뚝 잘라 바로 먹던데 뼈 없어? 다 빼나?",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027757.1.32",
          "form": "유감! 나도 모릅니다",
          "original_form": "유감! 나도 모릅니다",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027757.1.33",
          "form": "뼈도 그냥 오독오독 먹었어",
          "original_form": "뼈도 그냥 오독오독 먹었어",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027757.1.34",
          "form": "우앵 글쿠나 신기하다 늘 궁금했어",
          "original_form": "우앵 글쿠나 신기하다 늘 궁금했어",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027757.1.35",
          "form": "아직 살아있는거 보니 목에 걸리지도 않거 먹을 만 했던듯",
          "original_form": "아직 살아있는거 보니 목에 걸리지도 않거 먹을 만 했던듯",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027757.1.36",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027757.1.37",
          "form": "푸딩 좋아해?",
          "original_form": "푸딩 좋아해?",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027757.1.38",
          "form": "ㅎㅎ비싸긴 해도 맛있어 꼭 먹어봐",
          "original_form": "ㅎㅎ비싸긴 해도 맛있어 꼭 먹어봐",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027757.1.39",
          "form": "아니..",
          "original_form": "아니..",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027757.1.40",
          "form": "사실 그닥 먹어본적도 없어",
          "original_form": "사실 그닥 먹어본적도 없어",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027757.1.41",
          "form": "좋아 나중에 기회되면 먹어봐야지",
          "original_form": "좋아 나중에 기회되면 먹어봐야지",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027757.1.42",
          "form": "아하 뭔가 좋아할줄",
          "original_form": "아하 뭔가 좋아할줄",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027757.1.43",
          "form": "알티돌던... 문익점처럼 훔쳐왔다던 푸딩 레시피 그거 광주래서",
          "original_form": "알티돌던... 문익점처럼 훔쳐왔다던 푸딩 레시피 그거 광주래서",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027757.1.44",
          "form": "그냥 난 물컹물컹한건 대체로 못먹는듯",
          "original_form": "그냥 난 물컹물컹한건 대체로 못먹는듯",
          "speaker_id": "2",
          "time": "20210902 15:08"
        }
      ]
    }
  ]
