      "utterance": [
        {
          "id": "MDRW2100046119.1.1",
          "form": "아재즐",
          "original_form": "아재즐",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046119.1.2",
          "form": "아재들 턱수염",
          "original_form": "아재들 턱수염",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046119.1.3",
          "form": "징그러워요ㅋㅋㅋ너무 굵음",
          "original_form": "징그러워요ㅋㅋㅋ너무 굵음",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046119.1.4",
          "form": "거뭇하게난것처럼",
          "original_form": "거뭇하게난것처럼",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046119.1.5",
          "form": "윽",
          "original_form": "윽",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046119.1.6",
          "form": "그냥 저거 개발자가 이상한 건지",
          "original_form": "그냥 저거 개발자가 이상한 건지",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046119.1.7",
          "form": "제가 이상한 건지",
          "original_form": "제가 이상한 건지",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046119.1.8",
          "form": "이젠 알수가 없어졌어요",
          "original_form": "이젠 알수가 없어졌어요",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046119.1.9",
          "form": "다시봐도이상하네요진짜생긴거",
          "original_form": "다시봐도이상하네요진짜생긴거",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046119.1.10",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046119.1.11",
          "form": "그죠 진짜",
          "original_form": "그죠 진짜",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046119.1.12",
          "form": "인기꽤있지않나요?",
          "original_form": "인기꽤있지않나요?",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046119.1.13",
          "form": "너무이해안가는..",
          "original_form": "너무이해안가는..",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046119.1.14",
          "form": "죠르디는 귀엽기라도 하지",
          "original_form": "죠르디는 귀엽기라도 하지",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046119.1.15",
          "form": "저건 글쎄요...",
          "original_form": "저건 글쎄요...",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046119.1.16",
          "form": "죠르디는귀엽죵 ㅠㅠ",
          "original_form": "죠르디는귀엽죵 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046119.1.17",
          "form": "저거 인형상품도 있던데",
          "original_form": "저거 인형상품도 있던데",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046119.1.18",
          "form": "아니저고양이기르는데 ㅋㅋㅋ",
          "original_form": "아니저고양이기르는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046119.1.19",
          "form": "팔리긴 하는 걸까요...",
          "original_form": "팔리긴 하는 걸까요...",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046119.1.20",
          "form": "저기고양이라니",
          "original_form": "저기고양이라니",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046119.1.21",
          "form": "고양이를 도대체 저렇게 표현하다니",
          "original_form": "고양이를 도대체 저렇게 표현하다니",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046119.1.22",
          "form": "충격이네요정말",
          "original_form": "충격이네요정말",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046119.1.23",
          "form": "그니까요 ㅋㅋ꼬리도없고",
          "original_form": "그니까요 ㅋㅋ꼬리도없고",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046119.1.24",
          "form": "꼬리가있네요다시보니 ㅋㅋ충격",
          "original_form": "꼬리가있네요다시보니 ㅋㅋ충격",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046119.1.25",
          "form": "정말 미스테리네요",
          "original_form": "정말 미스테리네요",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046119.1.26",
          "form": "헐 있어요?ㅋㅋㄲㅋ",
          "original_form": "헐 있어요?ㅋㅋㄲㅋ",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046119.1.27",
          "form": "그럼 입가의 뽕알은",
          "original_form": "그럼 입가의 뽕알은",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046119.1.28",
          "form": "사라진 땅콩의 은유일까요...",
          "original_form": "사라진 땅콩의 은유일까요...",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046119.1.29",
          "form": "원래고양이입뽕주머니라고하긴하는데 ㅋㅋㅋ",
          "original_form": "원래고양이입뽕주머니라고하긴하는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046119.1.30",
          "form": "고양이입은귀여운데 저건",
          "original_form": "고양이입은귀여운데 저건{emoji:…}",
          "speaker_id": "1",
