      "utterance": [
        {
          "id": "MDRW2100005146.1.1",
          "form": "이야 진짜 좋은회사네요ㅎ",
          "original_form": "이야 진짜 좋은회사네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005146.1.2",
          "form": "그런부분은 좋아요",
          "original_form": "그런부분은 좋아요",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005146.1.3",
          "form": "가고싶은회사ㅅㅅ",
          "original_form": "가고싶은회사ㅅㅅ",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005146.1.4",
          "form": "그럼 그외의 복지는 뭐뭐있어요?",
          "original_form": "그럼 그외의 복지는 뭐뭐있어요?",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005146.1.5",
          "form": "갑자기 궁금해졌음요ㅎ",
          "original_form": "갑자기 궁금해졌음요ㅎ",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005146.1.6",
          "form": "직원들이 신입이 많아서... 왜 다 안해주냐 하는직원들이 있죠",
          "original_form": "직원들이 신입이 많아서... 왜 다 안해주냐 하는직원들이 있죠",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005146.1.7",
          "form": "이직한 직원들은 여기 복지 괜찮다고 하고",
          "original_form": "이직한 직원들은 여기 복지 괜찮다고 하고",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005146.1.8",
          "form": "편의 봐주는게 건강검진 위수면내시경 지원\r\n평일에 은행 병원 가는거 말하고 가면 업무시간에도  갈수있어요.",
          "original_form": "편의 봐주는게 건강검진 위수면내시경 지원\r\n평일에 은행 병원 가는거 말하고 가면 업무시간에도  갈수있어요.",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005146.1.9",
          "form": "그럼 엄청좋은회사네요ㅎㅎ",
          "original_form": "그럼 엄청좋은회사네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005146.1.10",
          "form": "직원들 사정잘 봐줘요.",
          "original_form": "직원들 사정잘 봐줘요.",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005146.1.11",
          "form": "급여도 괜찮고 복지도 좋고ㅎㅎ 신입도 많은거보면 규모도 크겟네요ㅎ",
          "original_form": "급여도 괜찮고 복지도 좋고ㅎㅎ 신입도 많은거보면 규모도 크겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005146.1.12",
          "form": "부럽습니다ㅎㅎ",
          "original_form": "부럽습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005146.1.13",
          "form": "인원 20명쯤 되고 업무 스트레스로 .... 그만두거나 이직으로 빠져들 나가요",
          "original_form": "인원 20명쯤 되고 업무 스트레스로 .... 그만두거나 이직으로 빠져들 나가요",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005146.1.14",
          "form": "회사 차리거나",
          "original_form": "회사 차리거나",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005146.1.15",
          "form": "에고 업무 스트레스는 어딜가나있네요",
          "original_form": "에고 업무 스트레스는 어딜가나있네요",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005146.1.16",
          "form": "회사를 차릴수가있어요?",
          "original_form": "회사를 차릴수가있어요?",
          "speaker_id": "2",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005146.1.17",
          "form": "왜 그럼 name1님은 도전안해보셨어요?",
          "original_form": "왜 그럼 &name1&님은 도전안해보셨어요?",
          "speaker_id": "2",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005146.1.18",
          "form": "아 차린분은 한팀 팀장급이었던분 이었어요 ㅋㅋㅋ",
          "original_form": "아 차린분은 한팀 팀장급이었던분 이었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005146.1.19",
          "form": "개인회사 차려서 나가던데요",
          "original_form": "개인회사 차려서 나가던데요",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005146.1.20",
          "form": "ㅎㅎ 열심히노력해서 해보셔요ㄹ",
          "original_form": "ㅎㅎ 열심히노력해서 해보셔요ㄹ",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005146.1.21",
          "form": "요즘에는 월급쟁이로 부자 못되잖아요ㅎ",
          "original_form": "요즘에는 월급쟁이로 부자 못되잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005146.1.22",
          "form": "ㅋㅋㅋ 저는 업무하다가 한계가 와서 더 관리팀 으로 온거에요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 저는 업무하다가 한계가 와서 더 관리팀 으로 온거에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005146.1.23",
          "form": "한계요?ㅎ",
          "original_form": "한계요?ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005146.1.24",
          "form": "힘드셨나보네요ㅎ",
          "original_form": "힘드셨나보네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005146.1.25",
          "form": "근데 진짜 복지는 짱이네요ㅎ 혹시그럼 복지카드는?",
          "original_form": "근데 진짜 복지는 짱이네요ㅎ 혹시그럼 복지카드는?",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005146.1.26",
          "form": "그런거없죠 아 이번에 중소기업 근로지원 사업 신청하거 있어요",
          "original_form": "그런거없죠 아 이번에 중소기업 근로지원 사업 신청하거 있어요",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005146.1.27",
          "form": "제가 담당자니까 ㅋㅋㅋ 제가 할려고",
          "original_form": "제가 담당자니까 ㅋㅋㅋ 제가 할려고",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005146.1.28",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005146.1.29",
          "form": "그거 저희는 안해준대요",
          "original_form": "그거 저희는 안해준대요",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005146.1.30",
          "form": "직원들 동의 얻고 상무님에게 신청하게 해달라고 보고하고 ㅋㅋ 신청해요",
          "original_form": "직원들 동의 얻고 상무님에게 신청하게 해달라고 보고하고 ㅋㅋ 신청해요",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005146.1.31",
          "form": "우와 부럽습니다 ㅠ",
          "original_form": "우와 부럽습니다 ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005146.1.32",
          "form": "저희는 꼴랑 복지카드하나있고 다른건 아에없으니",
          "original_form": "저희는 꼴랑 복지카드하나있고 다른건 아에없으니",
          "speaker_id": "2",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005146.1.33",
          "form": "서류처리 할게 많아요",
          "original_form": "서류처리 할게 많아요",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005146.1.34",
          "form": "그래서 더 안해줄려고 하는거일수도 있어요",
          "original_form": "그래서 더 안해줄려고 하는거일수도 있어요",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005146.1.35",
          "form": "해줬으면 좋겟는데... 올해가 저는 마지막이에요ㅠ",
          "original_form": "해줬으면 좋겟는데... 올해가 저는 마지막이에요ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005146.1.36",
          "form": "전에 이야기를 했는데 회사주인바뀌고나서 흐지부지 됬어요ㅎ",
          "original_form": "전에 이야기를 했는데 회사주인바뀌고나서 흐지부지 됬어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005146.1.37",
          "form": "응 근로자 휴가지원은 나이제한이 없는 거일텐데",
          "original_form": "응 근로자 휴가지원은 나이제한이 없는 거일텐데",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005146.1.38",
          "form": "내일채움곤제에서 하는 근로자휴가지원 사업 인가요?  그거면 제한있을수 있겟네요",
