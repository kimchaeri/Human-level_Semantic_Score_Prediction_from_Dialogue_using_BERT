      "utterance": [
        {
          "id": "MDRW2100030757.1.1",
          "form": "오늘 날씨가 어때요? 아직 안나가봐서",
          "original_form": "오늘 날씨가 어때요? 아직 안나가봐서",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030757.1.2",
          "form": "오늘 날씨 아주아주 좋습니다!!!",
          "original_form": "오늘 날씨 아주아주 좋습니다!!!",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030757.1.3",
          "form": "햇살이 내리쬐네요~~~!",
          "original_form": "햇살이 내리쬐네요~~~!",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030757.1.4",
          "form": "ㅋㅋㅋ 요즘은 미세먼지 괜찮죠?",
          "original_form": "ㅋㅋㅋ 요즘은 미세먼지 괜찮죠?",
          "speaker_id": "1",
          "time": "20210904 12:59"
        },
        {
          "id": "MDRW2100030757.1.5",
          "form": "맞아요 ㅎㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030757.1.6",
          "form": "저도 아직 집이긴 하지만...",
          "original_form": "저도 아직 집이긴 하지만...",
          "speaker_id": "2",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030757.1.7",
          "form": "엌",
          "original_form": "엌",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030757.1.8",
          "form": "헉근데 이번 가을 뭔가 추울거같지않나요",
          "original_form": "헉근데 이번 가을 뭔가 추울거같지않나요",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030757.1.9",
          "form": "오늘도 꽤나 쌀쌀해보이던데...",
          "original_form": "오늘도 꽤나 쌀쌀해보이던데...",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030757.1.10",
          "form": "아침 기온이 16도던데요",
          "original_form": "아침 기온이 16도던데요",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030757.1.11",
          "form": "앗 맞아요 그리고",
          "original_form": "앗 맞아요 그리고",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030757.1.12",
          "form": "여름이 더웠던만큼",
          "original_form": "여름이 더웠던만큼",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030757.1.13",
          "form": "겨울이 엄청 추울거라는 이야기가 있더라구요.....?",
          "original_form": "겨울이 엄청 추울거라는 이야기가 있더라구요.....?",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030757.1.14",
          "form": "세상에... 지구가 많이 아픈가봐요..",
          "original_form": "세상에... 지구가 많이 아픈가봐요..",
          "speaker_id": "1",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030757.1.15",
          "form": "그러게말이에요...",
          "original_form": "그러게말이에요...",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030757.1.16",
          "form": "작년 여름엔 그렇게 안더웠던 것 같은데ㅠ",
          "original_form": "작년 여름엔 그렇게 안더웠던 것 같은데ㅠ",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030757.1.17",
          "form": "근데이번여름도 더운기간은 좀 짧았지않았어요?",
          "original_form": "근데이번여름도 더운기간은 좀 짧았지않았어요?",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030757.1.18",
          "form": "저만그런가",
          "original_form": "저만그런가",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030757.1.19",
          "form": "밖을안나가서그런가",
          "original_form": "밖을안나가서그런가",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030757.1.20",
          "form": "앗 그러신가보다",
          "original_form": "앗 그러신가보다",
          "speaker_id": "2",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030757.1.21",
          "form": "저는 이번 여름이 진짜",
          "original_form": "저는 이번 여름이 진짜",
          "speaker_id": "2",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030757.1.22",
          "form": "이러다 사람 죽겠다 생각했었어욬ㅋㅋㅋㅋㅋ ㅠㅠㅠ",
          "original_form": "이러다 사람 죽겠다 생각했었어욬ㅋㅋㅋㅋㅋ ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030757.1.23",
          "form": "엌ㅋㅋㅋㅋ....ㅠㅠ",
          "original_form": "엌ㅋㅋㅋㅋ....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030757.1.24",
          "form": "근데 도시속에서 열돔현상은",
          "original_form": "근데 도시속에서 열돔현상은",
          "speaker_id": "1",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030757.1.25",
          "form": "진짜 너무 심각하게더웠던거같아요",
          "original_form": "진짜 너무 심각하게더웠던거같아요",
          "speaker_id": "1",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100030757.1.26",
          "form": "오오 열돔현상  !!!!",
          "original_form": "오오 열돔현상  !!!!",
          "speaker_id": "2",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100030757.1.27",
          "form": "전문적인 용어네요 ",
          "original_form": "전문적인 용어네요 ",
          "speaker_id": "2",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100030757.1.28",
          "form": "맞아요 시멘트 바닥때문에 그런가....!",
          "original_form": "맞아요 시멘트 바닥때문에 그런가....!",
          "speaker_id": "2",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100030757.1.29",
          "form": "맞아요ㅠ 도시에 숲같은게 있다면좋을텐데...역시 나무가 많아야하려나요",
          "original_form": "맞아요ㅠ 도시에 숲같은게 있다면좋을텐데...역시 나무가 많아야하려나요",
          "speaker_id": "1",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030757.1.30",
          "form": "맞아요 나무!!!!",
          "original_form": "맞아요 나무!!!!",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030757.1.31",
          "form": "그리고 산을 그만 깎아야할텐데 말이에요 ",
          "original_form": "그리고 산을 그만 깎아야할텐데 말이에요 ",
          "speaker_id": "2",
