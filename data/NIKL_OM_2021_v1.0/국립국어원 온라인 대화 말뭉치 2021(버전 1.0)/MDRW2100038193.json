      "utterance": [
        {
          "id": "MDRW2100038193.1.1",
          "form": "평화요정이시군요 ㅎㅎㅎ",
          "original_form": "평화요정이시군요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100038193.1.2",
          "form": "식탁에서 동생이랑 많이 싸워봤죠",
          "original_form": "식탁에서 동생이랑 많이 싸워봤죠",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038193.1.3",
          "form": "평화롭게 밥 먹는게 얼마나 소중한지 안답니다ㅎㅎ",
          "original_form": "평화롭게 밥 먹는게 얼마나 소중한지 안답니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038193.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ 하 동생 있으시군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 하 동생 있으시군요",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100038193.1.5",
          "form": "전 언니 있는데 좋아하는 음식이 덜 겹쳐서 그나마 다행이랄까...",
          "original_form": "전 언니 있는데 좋아하는 음식이 덜 겹쳐서 그나마 다행이랄까...",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100038193.1.6",
          "form": "전 남동생인데",
          "original_form": "전 남동생인데",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038193.1.7",
          "form": "네네 헐",
          "original_form": "네네 헐",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038193.1.8",
          "form": "제가 케찹을 안좋아하거든요",
          "original_form": "제가 케찹을 안좋아하거든요",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038193.1.9",
          "form": "남동생이라니..",
          "original_form": "남동생이라니..",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038193.1.10",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038193.1.11",
          "form": "감튀도 안찍어먹어요",
          "original_form": "감튀도 안찍어먹어요",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038193.1.12",
          "form": "근데 엄마가 소시지나 스팸을 부쳐주거나",
          "original_form": "근데 엄마가 소시지나 스팸을 부쳐주거나",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038193.1.13",
          "form": "네네네 !",
          "original_form": "네네네 !",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038193.1.14",
          "form": "계란후라이를 해주면 거기다 케찹을 잔뜩! !",
          "original_form": "계란후라이를 해주면 거기다 케찹을 잔뜩! !",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038193.1.15",
          "form": "그럴때마다 화냈는데 그때 걔가 한창 식탐부릴때라",
          "original_form": "그럴때마다 화냈는데 그때 걔가 한창 식탐부릴때라",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038193.1.16",
          "form": "헐 ????!!!!",
          "original_form": "헐 ????!!!!",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038193.1.17",
          "form": "아 너무 싫은데요",
          "original_form": "아 너무 싫은데요",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038193.1.18",
          "form": "한 2년은 짜증내고 살았던거 같아요",
          "original_form": "한 2년은 짜증내고 살았던거 같아요",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038193.1.19",
          "form": "전 식탐부리는 사람이 젤 싫어요...",
          "original_form": "전 식탐부리는 사람이 젤 싫어요...",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038193.1.20",
          "form": "그래도 그 시기 지나니까 정신이 좀 든건지 식욕이 준건지",
          "original_form": "그래도 그 시기 지나니까 정신이 좀 든건지 식욕이 준건지",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100038193.1.21",
          "form": "심지어 싫어하는 소스를 위에 다 뿌리다니 자기 혼자 먹으려고 ㅡㅡ",
          "original_form": "심지어 싫어하는 소스를 위에 다 뿌리다니 자기 혼자 먹으려고 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100038193.1.22",
          "form": "네네 !",
          "original_form": "네네 !",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100038193.1.23",
          "form": "그짓거리는 안하더라구요",
          "original_form": "그짓거리는 안하더라구요",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100038193.1.24",
          "form": "이젠 나름 양보도 합디다",
          "original_form": "이젠 나름 양보도 합디다",
