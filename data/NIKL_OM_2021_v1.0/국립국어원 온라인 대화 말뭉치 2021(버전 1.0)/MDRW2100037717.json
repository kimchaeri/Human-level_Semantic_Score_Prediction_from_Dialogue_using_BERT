      "utterance": [
        {
          "id": "MDRW2100037717.1.1",
          "form": "추석보너스 타면 아이패드를 사겠다",
          "original_form": "추석보너스 타면 아이패드를 사겠다",
          "speaker_id": "2",
          "time": "20210905 11:29"
        },
        {
          "id": "MDRW2100037717.1.2",
          "form": "근데 종류 넘 다양한거",
          "original_form": "근데 종류 넘 다양한거",
          "speaker_id": "2",
          "time": "20210905 11:29"
        },
        {
          "id": "MDRW2100037717.1.3",
          "form": "미니 프로 에어 그냥",
          "original_form": "미니 프로 에어 그냥",
          "speaker_id": "2",
          "time": "20210905 11:30"
        },
        {
          "id": "MDRW2100037717.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:30"
        },
        {
          "id": "MDRW2100037717.1.5",
          "form": "용량도 골라야하고 색상도 골라야 하고",
          "original_form": "용량도 골라야하고 색상도 골라야 하고",
          "speaker_id": "1",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100037717.1.6",
          "form": "어렵다구",
          "original_form": "어렵다구",
          "speaker_id": "1",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100037717.1.7",
          "form": "응 아주 스무고개도 아니고",
          "original_form": "응 아주 스무고개도 아니고",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100037717.1.8",
          "form": "언니는 아이패드프로?",
          "original_form": "언니는 아이패드프로?",
          "speaker_id": "2",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100037717.1.9",
          "form": "128GB",
          "original_form": "128GB",
          "speaker_id": "2",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100037717.1.10",
          "form": "어 쇼핑몰에서도 완전 하나 고르면 그다음꺼 뜨면서 이건 뭘로 하실랴요 그런다고",
          "original_form": "어 쇼핑몰에서도 완전 하나 고르면 그다음꺼 뜨면서 이건 뭘로 하실랴요 그런다고",
          "speaker_id": "1",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100037717.1.11",
          "form": "실버?",
          "original_form": "실버?",
          "speaker_id": "2",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100037717.1.12",
          "form": "난 프로 12.9 256 실버",
          "original_form": "난 프로 12.9 256 실버",
          "speaker_id": "1",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100037717.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100037717.1.14",
          "form": "거기에 애플펜슬 추가",
          "original_form": "거기에 애플펜슬 추가",
          "speaker_id": "2",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100037717.1.15",
          "form": "만족하시쥬?",
          "original_form": "만족하시쥬?",
          "speaker_id": "2",
          "time": "20210905 11:34"
        },
        {
          "id": "MDRW2100037717.1.16",
          "form": "네 좋아유",
          "original_form": "네 좋아유",
          "speaker_id": "1",
          "time": "20210905 11:35"
        },
        {
          "id": "MDRW2100037717.1.17",
          "form": "전 액정필름은 안함",
          "original_form": "전 액정필름은 안함",
          "speaker_id": "1",
          "time": "20210905 11:35"
        },
        {
          "id": "MDRW2100037717.1.18",
          "form": "케이스도 그냥 자석형이고",
          "original_form": "케이스도 그냥 자석형이고",
          "speaker_id": "1",
          "time": "20210905 11:35"
        },
        {
          "id": "MDRW2100037717.1.19",
          "form": "구매만족도가 높으시니까",
          "original_form": "구매만족도가 높으시니까",
          "speaker_id": "2",
          "time": "20210905 11:35"
        },
        {
          "id": "MDRW2100037717.1.20",
          "form": "케이스는 별도구매?",
          "original_form": "케이스는 별도구매?",
          "speaker_id": "2",
          "time": "20210905 11:35"
        },
        {
          "id": "MDRW2100037717.1.21",
          "form": "근데 사실 뭘사도 빠르게 만족함",
          "original_form": "근데 사실 뭘사도 빠르게 만족함",
          "speaker_id": "1",
          "time": "20210905 11:35"
        },
        {
          "id": "MDRW2100037717.1.22",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 11:36"
        },
        {
          "id": "MDRW2100037717.1.23",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 11:36"
        },
        {
          "id": "MDRW2100037717.1.24",
          "form": "스마트워치는 필요없슈?",
          "original_form": "스마트워치는 필요없슈?",
          "speaker_id": "2",
          "time": "20210905 11:36"
        },
        {
          "id": "MDRW2100037717.1.25",
          "form": "내년 생일선물로 사드릴까?",
          "original_form": "내년 생일선물로 사드릴까?",
          "speaker_id": "2",
          "time": "20210905 11:36"
        },
        {
          "id": "MDRW2100037717.1.26",
          "form": "싫음말구",
          "original_form": "싫음말구",
          "speaker_id": "2",
          "time": "20210905 11:36"
        },
        {
          "id": "MDRW2100037717.1.27",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100037717.1.28",
          "form": "ㅋㅋㅋㅋ사주시면 받죠",
          "original_form": "ㅋㅋㅋㅋ사주시면 받죠",
          "speaker_id": "1",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100037717.1.29",
          "form": "함 보겠습니다",
          "original_form": "함 보겠습니다",
          "speaker_id": "1",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100037717.1.30",
          "form": "자전거 그 부착하는 건 안쓰슈?",
          "original_form": "자전거 그 부착하는 건 안쓰슈?",
          "speaker_id": "2",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100037717.1.31",
          "form": "아직 항체형성 전이라",
          "original_form": "아직 항체형성 전이라",
          "speaker_id": "1",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100037717.1.32",
          "form": "심장을 위해 보류중",
          "original_form": "심장을 위해 보류중",
          "speaker_id": "1",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100037717.1.33",
          "form": "아니 무슨 핑계람",
          "original_form": "아니 무슨 핑계람",
          "speaker_id": "2",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100037717.1.34",
          "form": "9일 지났잖수",
          "original_form": "9일 지났잖수",
          "speaker_id": "2",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100037717.1.35",
          "form": "에어팟은 충전 문제였어",
          "original_form": "에어팟은 충전 문제였어",
          "speaker_id": "2",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100037717.1.36",
          "form": "어제 풀충전 했더니",
          "original_form": "어제 풀충전 했더니",
          "speaker_id": "2",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100037717.1.37",
          "form": "출근길에 잘 작동중!",
          "original_form": "출근길에 잘 작동중!",
          "speaker_id": "2",
          "time": "20210907 08:07"
        }
      ]
