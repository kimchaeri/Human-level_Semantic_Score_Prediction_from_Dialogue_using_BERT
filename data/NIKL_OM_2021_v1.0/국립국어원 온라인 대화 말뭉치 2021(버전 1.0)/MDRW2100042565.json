          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042565.1.25",
          "form": "나도...",
          "original_form": "나도...",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042565.1.26",
          "form": "난 항상 엉덩이가 안맞음",
          "original_form": "난 항상 엉덩이가 안맞음",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042565.1.27",
          "form": "난 진짜 어릴때부터",
          "original_form": "난 진짜 어릴때부터",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042565.1.28",
          "form": "다리 컴플렉스였어서",
          "original_form": "다리 컴플렉스였어서",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042565.1.29",
          "form": "허벅지...ㅡ",
          "original_form": "허벅지...ㅡ",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042565.1.30",
          "form": "나도 지금은 좀 괜찮아졌는데",
          "original_form": "나도 지금은 좀 괜찮아졌는데",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042565.1.31",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042565.1.32",
          "form": "어릴때 진짜 스트레스엿음",
          "original_form": "어릴때 진짜 스트레스엿음",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042565.1.33",
          "form": "근데 어릴때는",
          "original_form": "근데 어릴때는",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042565.1.34",
          "form": "남한테 보여지는 거 때문에",
          "original_form": "남한테 보여지는 거 때문에",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042565.1.35",
          "form": "그치..",
          "original_form": "그치..",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042565.1.36",
          "form": "스트레스였는데 이제는 여름에 다리가 너무 더운느낌 뭔지 알지",
          "original_form": "스트레스였는데 이제는 여름에 다리가 너무 더운느낌 뭔지 알지",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042565.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄹㅇ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄹㅇ",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042565.1.38",
          "form": "그것땜에 더 스트레스임",
          "original_form": "그것땜에 더 스트레스임",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042565.1.39",
          "form": "의자에 맨살로 앉으면",
          "original_form": "의자에 맨살로 앉으면",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042565.1.40",
          "form": "막 살 달라붙는느낌나고",
          "original_form": "막 살 달라붙는느낌나고",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042565.1.41",
          "form": "으 맞음 ㅠㅠㅠㅠ",
          "original_form": "으 맞음 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042565.1.42",
          "form": "허벅지밑에 땀차",
          "original_form": "허벅지밑에 땀차",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
