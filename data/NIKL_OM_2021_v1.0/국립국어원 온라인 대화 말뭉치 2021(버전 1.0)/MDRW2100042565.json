          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042565.1.14",
          "form": "40키로댄데 맨날 살빼고싶대",
          "original_form": "40키로댄데 맨날 살빼고싶대",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042565.1.15",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042565.1.16",
          "form": "근데 난 좀 빼야함",
          "original_form": "근데 난 좀 빼야함",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042565.1.17",
          "form": "확실히 살 찌면",
          "original_form": "확실히 살 찌면",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042565.1.18",
          "form": "불편하더라",
          "original_form": "불편하더라",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042565.1.19",
          "form": "움직이는것도 그렇고",
          "original_form": "움직이는것도 그렇고",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042565.1.20",
          "form": "나.도.빼.야.함",
          "original_form": "나.도.빼.야.함",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042565.1.21",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042565.1.22",
          "form": "한 10키로 빼고싶다",
          "original_form": "한 10키로 빼고싶다",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042565.1.23",
          "form": "암튼 살 빼서",
          "original_form": "암튼 살 빼서",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042565.1.24",
          "form": "바지좀 사고 싶음",
          "original_form": "바지좀 사고 싶음",
