      "utterance": [
        {
          "id": "MDRW2100042565.1.1",
          "form": "우리 고딩때 애들도",
          "original_form": "우리 고딩때 애들도",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042565.1.2",
          "form": "거의다 50~60키로대였는데",
          "original_form": "거의다 50~60키로대였는데",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042565.1.3",
          "form": "애들 솔직히 다 말랐었음",
          "original_form": "애들 솔직히 다 말랐었음",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042565.1.4",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042565.1.5",
          "form": "그런데도 맨날 다이어트 한다고",
          "original_form": "그런데도 맨날 다이어트 한다고",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
