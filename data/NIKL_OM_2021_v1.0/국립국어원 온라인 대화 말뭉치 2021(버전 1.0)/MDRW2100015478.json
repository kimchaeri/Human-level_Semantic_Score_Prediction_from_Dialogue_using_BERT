      "utterance": [
        {
          "id": "MDRW2100015478.1.1",
          "form": "군대 이야기...음",
          "original_form": "군대 이야기...음",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015478.1.2",
          "form": "제페토 아이디 알려줘요",
          "original_form": "제페토 아이디 알려줘요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.3",
          "form": "군대?",
          "original_form": "군대?",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.4",
          "form": "잠깐 군대더라구요",
          "original_form": "잠깐 군대더라구요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.6",
          "form": "제페토 맞다 깔아야해요",
          "original_form": "제페토 맞다 깔아야해요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.7",
          "form": "아핫 다음에 가시죠 군대",
          "original_form": "아핫 다음에 가시죠 군대",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.8",
          "form": "저도 이벤트때문에 가입했는데",
          "original_form": "저도 이벤트때문에 가입했는데",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.9",
          "form": "제페토 옷꾸미기로 온통 명품 입어보고",
          "original_form": "제페토 옷꾸미기로 온통 명품 입어보고",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.10",
          "form": "다음주에 맵이 생겨서 대회 참가흐래요",
          "original_form": "다음주에 맵이 생겨서 대회 참가흐래요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.11",
          "form": "나름 재밌었어요",
          "original_form": "나름 재밌었어요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.12",
          "form": "우와 명품도 사지나요?",
          "original_form": "우와 명품도 사지나요?",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.13",
          "form": "맵이 생기는군요",
          "original_form": "맵이 생기는군요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015478.1.14",
          "form": "그냥 공짜명품 있었던 거 같은데",
          "original_form": "그냥 공짜명품 있었던 거 같은데",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.15",
          "form": "사실 꽤 오래전에 접속했어서..",
          "original_form": "사실 꽤 오래전에 접속했어서..",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.16",
          "form": "오 공짜명품",
          "original_form": "오 공짜명품",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.17",
          "form": "항만맵이 생겨서",
          "original_form": "항만맵이 생겨서",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.18",
          "form": "컨테이너 뛰어넘기 달리기 대회한대여",
          "original_form": "컨테이너 뛰어넘기 달리기 대회한대여",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.19",
          "form": "ㅋㅋㅋ웃기죠",
          "original_form": "ㅋㅋㅋ웃기죠",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.21",
          "form": "근데 조작 잘 못해소 걱정이에요",
          "original_form": "근데 조작 잘 못해소 걱정이에요",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.22",
          "form": "가상이지만 왠지 체력소모가 되네요",
          "original_form": "가상이지만 왠지 체력소모가 되네요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.23",
          "form": "상상만으로도",
          "original_form": "상상만으로도",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015478.1.24",
          "form": "이기면 우승 상품주나요",
          "original_form": "이기면 우승 상품주나요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015478.1.25",
          "form": "그쵸 잘 조작안돼서 스투레스일듯",
          "original_form": "그쵸 잘 조작안돼서 스투레스일듯",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015478.1.26",
          "form": "네 상품 없얶으면 안하죠 저는ㅋㅋ",
          "original_form": "네 상품 없얶으면 안하죠 저는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:13"
        }
      ]
    }
