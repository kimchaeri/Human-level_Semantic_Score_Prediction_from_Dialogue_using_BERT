      "utterance": [
        {
          "id": "MDRW2100036597.1.1",
          "form": "글쎄 평소 자주 착용하는 스타일이  아닐까",
          "original_form": "글쎄 평소 자주 착용하는 스타일이  아닐까",
          "speaker_id": "2",
          "time": "20210831 01:33"
        },
        {
          "id": "MDRW2100036597.1.2",
          "form": "엉 맞아 그리고 클래식한 아이템",
          "original_form": "엉 맞아 그리고 클래식한 아이템",
          "speaker_id": "1",
          "time": "20210831 01:33"
        },
        {
          "id": "MDRW2100036597.1.3",
          "form": "유행을 안따르니",
          "original_form": "유행을 안따르니",
          "speaker_id": "2",
          "time": "20210831 01:33"
        },
        {
          "id": "MDRW2100036597.1.4",
          "form": "맞아 그리고 가방",
          "original_form": "맞아 그리고 가방",
          "speaker_id": "1",
          "time": "20210831 01:33"
        },
        {
          "id": "MDRW2100036597.1.5",
          "form": "그래 가방은 고가",
          "original_form": "그래 가방은 고가",
          "speaker_id": "2",
          "time": "20210831 01:34"
        },
        {
          "id": "MDRW2100036597.1.6",
          "form": "신발도",
          "original_form": "신발도",
          "speaker_id": "1",
          "time": "20210831 01:34"
        },
        {
          "id": "MDRW2100036597.1.7",
          "form": "착용이 편안해야하니까",
          "original_form": "착용이 편안해야하니까",
          "speaker_id": "2",
          "time": "20210831 01:34"
        },
        {
          "id": "MDRW2100036597.1.8",
          "form": "응 그리고 가을 겨울에 입는 고트나 점퍼",
          "original_form": "응 그리고 가을 겨울에 입는 고트나 점퍼",
          "speaker_id": "1",
          "time": "20210831 01:34"
        },
        {
          "id": "MDRW2100036597.1.9",
          "form": "코트는 저렴한거 사면 오래 못입어",
          "original_form": "코트는 저렴한거 사면 오래 못입어",
          "speaker_id": "2",
          "time": "20210831 01:35"
        },
        {
          "id": "MDRW2100036597.1.10",
          "form": "엉 그렇지 그리고 심플한 디자인들",
          "original_form": "엉 그렇지 그리고 심플한 디자인들",
          "speaker_id": "1",
          "time": "20210831 01:35"
        },
        {
          "id": "MDRW2100036597.1.11",
          "form": "이것도 유행을 안타니까 오래 입을수 있으니",
          "original_form": "이것도 유행을 안타니까 오래 입을수 있으니",
          "speaker_id": "2",
          "time": "20210831 01:36"
        },
        {
          "id": "MDRW2100036597.1.12",
          "form": "엉 예산이 충분하면 고가 저가들 나눌 필요가 없지만 우린 아니니까",
          "original_form": "엉 예산이 충분하면 고가 저가들 나눌 필요가 없지만 우린 아니니까",
          "speaker_id": "1",
          "time": "20210831 01:36"
        },
        {
          "id": "MDRW2100036597.1.13",
          "form": "응 슬프다 그래서",
          "original_form": "응 슬프다 그래서",
