      "utterance": [
        {
          "id": "MDRW2100011550.1.1",
          "form": "왜 이렇게 끝이없나몰러 ㅋㅋㅋㅋ",
          "original_form": "왜 이렇게 끝이없나몰러 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011550.1.2",
          "form": "진짜 내 가족이니 하지..",
          "original_form": "진짜 내 가족이니 하지..",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011550.1.3",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011550.1.4",
          "form": "남 이면 못한다..안한다 ㅋㅋㅋㅋㅋ",
          "original_form": "남 이면 못한다..안한다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011550.1.5",
          "form": "매일 반복 ㅜㅜ",
          "original_form": "매일 반복 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011550.1.6",
          "form": "해도 티도 안나~안하면 또 얼마나 밀리는지 ㅋㅋㅋㅋ",
          "original_form": "해도 티도 안나~안하면 또 얼마나 밀리는지 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011550.1.7",
          "form": "그러니깐 싫다 ㅋㅋ",
          "original_form": "그러니깐 싫다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011550.1.8",
          "form": "설거지 산더미다",
          "original_form": "설거지 산더미다",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011550.1.9",
          "form": "한번 손놓음 뭐 겉잡을수 없다 ㅋㅋㅋㅋ",
          "original_form": "한번 손놓음 뭐 겉잡을수 없다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011550.1.10",
          "form": "하기싫은데 해야지뭐ㅜ",
          "original_form": "하기싫은데 해야지뭐ㅜ",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011550.1.11",
          "form": "걷이가?? 맞춤법 모르겠너 ㅋㅋㅋ",
          "original_form": "걷이가?? 맞춤법 모르겠너 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011550.1.12",
          "form": "설거지래",
          "original_form": "설거지래",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011550.1.13",
          "form": "웅~거지 ㅋㅋㅋ",
          "original_form": "웅~거지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011550.1.14",
          "form": "우리때는 설걷이였잖아 바뀌었대ㅋ",
          "original_form": "우리때는 설걷이였잖아 바뀌었대ㅋ",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011550.1.15",
          "form": "아니 같잡을수~",
          "original_form": "아니 같잡을수~",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011550.1.16",
          "form": "충격받았다. 설걷이라고 계속 썼었는데ㅋ",
          "original_form": "충격받았다. 설걷이라고 계속 썼었는데ㅋ",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011550.1.17",
          "form": "우리때도 거지 아니였나??",
          "original_form": "우리때도 거지 아니였나??",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011550.1.18",
          "form": "그랬나?ㅋㅋㅋㄱㅋ",
          "original_form": "그랬나?ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011550.1.19",
          "form": "우리때 과도기여서 ㅋㅋㅋㅋ",
          "original_form": "우리때 과도기여서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
