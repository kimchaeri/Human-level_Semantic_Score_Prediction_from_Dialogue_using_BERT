      "utterance": [
        {
          "id": "MDRW2100017505.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017505.1.2",
          "form": "안녕하세요 커피좋아하세요?",
          "original_form": "안녕하세요 커피좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017505.1.3",
          "form": "네 달달한 커피 좋아해요 ㅎㅎ",
          "original_form": "네 달달한 커피 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017505.1.4",
          "form": "저는 달달한 음료 말고 아메리카노만 마셔욥~",
          "original_form": "저는 달달한 음료 말고 아메리카노만 마셔욥~",
          "speaker_id": "2",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017505.1.5",
          "form": "아메리카노는 여름에 딱 좋더라구여",
          "original_form": "아메리카노는 여름에 딱 좋더라구여",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017505.1.6",
          "form": "깔끔하고 시원해서 ㅋㅋㅋ",
          "original_form": "깔끔하고 시원해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017505.1.7",
          "form": "맞어요 여름엔 아아죠ㅋㅋㅋ 얼죽아이신가요?",
          "original_form": "맞어요 여름엔 아아죠ㅋㅋㅋ 얼죽아이신가요?",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017505.1.8",
          "form": "전 커피는 따뜻하게 마시는걸 좋아해요 ㅋㅋㅋ",
          "original_form": "전 커피는 따뜻하게 마시는걸 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017505.1.9",
          "form": "아하 저도 겨울엔 따뜻하게 마셔용~ 아아는 너무 추워서요ㅠ",
          "original_form": "아하 저도 겨울엔 따뜻하게 마셔용~ 아아는 너무 추워서요ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017505.1.10",
          "form": "맞아요 ㅋㅋㅋ 전 왠지 따뜻해야 더 맛있는거같더라구요",
          "original_form": "맞아요 ㅋㅋㅋ 전 왠지 따뜻해야 더 맛있는거같더라구요",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017505.1.11",
          "form": "오호 여름에도 따뜻하게드시나요?",
          "original_form": "오호 여름에도 따뜻하게드시나요?",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017505.1.12",
          "form": "음 사실 커피마시면 잠을 아예 못 자서 안 마시는 편이에요 ㅠㅠ",
          "original_form": "음 사실 커피마시면 잠을 아예 못 자서 안 마시는 편이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017505.1.13",
          "form": "아하 못드시는분도 많더라구요! 제 친구도 쉬는날전날에만 마시더라구요",
          "original_form": "아하 못드시는분도 많더라구요! 제 친구도 쉬는날전날에만 마시더라구요",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100017505.1.14",
          "form": "저는 그런건 없는데 예전에 2잔이상마셔도 괜찮았는데 요새는 심장이 두근거려서 1잔이상은 안마셔요",
          "original_form": "저는 그런건 없는데 예전에 2잔이상마셔도 괜찮았는데 요새는 심장이 두근거려서 1잔이상은 안마셔요",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100017505.1.15",
          "form": "오 저도 늦잠자도 되는날 전에만 마셔요",
          "original_form": "오 저도 늦잠자도 되는날 전에만 마셔요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        }
      ]
