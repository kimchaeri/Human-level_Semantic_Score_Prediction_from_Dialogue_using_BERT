      "utterance": [
        {
          "id": "MDRW2100034328.1.1",
          "form": "아무튼 대면은 하기 싫다",
          "original_form": "아무튼 대면은 하기 싫다",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034328.1.2",
          "form": "학교 욕 하루종일 말할 수 있음..ㅋ",
          "original_form": "학교 욕 하루종일 말할 수 있음..ㅋ",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034328.1.3",
          "form": "등록금 10만원이 뭐야",
          "original_form": "등록금 10만원이 뭐야",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034328.1.4",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034328.1.5",
          "form": "뭔가 체계도 없고 그냥 대면 섞겠다는 거 넘 짜증남",
          "original_form": "뭔가 체계도 없고 그냥 대면 섞겠다는 거 넘 짜증남",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034328.1.6",
          "form": "거리두기마다 인원수 제한하고 비대면으로 돌리면 뭐함",
          "original_form": "거리두기마다 인원수 제한하고 비대면으로 돌리면 뭐함",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100034328.1.7",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034328.1.8",
          "form": "애초에 수강신청 인원을 그 기준보다 낮게 잡는데",
          "original_form": "애초에 수강신청 인원을 그 기준보다 낮게 잡는데",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034328.1.9",
          "form": "어이없어",
          "original_form": "어이없어",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034328.1.10",
          "form": "얼탱쓰 그냥 대면 하라는 거잖아ㅋㅋㅋ",
          "original_form": "얼탱쓰 그냥 대면 하라는 거잖아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034328.1.11",
          "form": "매번 소독만 하고 계속 확진자 나오는데ㅋㅋㅋ",
          "original_form": "매번 소독만 하고 계속 확진자 나오는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034328.1.12",
          "form": "해결되는 건 없음",
          "original_form": "해결되는 건 없음",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034328.1.13",
          "form": "발열체크도 겁나 대충 하면서",
          "original_form": "발열체크도 겁나 대충 하면서",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034328.1.14",
          "form": "물론 그걸 믿는 것도 아니지만",
          "original_form": "물론 그걸 믿는 것도 아니지만",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034328.1.15",
          "form": "맞아ㅋㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034328.1.16",
          "form": "별로 학생들을 보호하려는 의지가 없어보임",
          "original_form": "별로 학생들을 보호하려는 의지가 없어보임",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034328.1.17",
          "form": "인정ㅋㅋㅋ 그냥 자본주의",
          "original_form": "인정ㅋㅋㅋ 그냥 자본주의",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034328.1.18",
          "form": "확진자 나오면 대충 소독했다 다음날 학교 오면 된다ㅋ",
