      "utterance": [
        {
          "id": "MDRW2100044545.1.1",
          "form": "당연히 보고있지!",
          "original_form": "당연히 보고있지!",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044545.1.2",
          "form": "ㅋㅋㅋ 요즘에 괜탆은애들있어?",
          "original_form": "ㅋㅋㅋ 요즘에 괜탆은애들있어?",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044545.1.3",
          "form": "나 진짜 거의 두세달마다 입덕하는듯ㅋㅋㅋ",
          "original_form": "나 진짜 거의 두세달마다 입덕하는듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044545.1.4",
          "form": "아진짜?ㅋㅋㅋㅋ 누군데? 어디서 보는거야?",
          "original_form": "아진짜?ㅋㅋㅋㅋ 누군데? 어디서 보는거야?",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044545.1.5",
          "form": "유튜브 보죠!",
          "original_form": "유튜브 보죠!",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044545.1.6",
          "form": "검색하려면 이름을 알아야하는거아니야? 이름을 어디서 안거야?",
          "original_form": "검색하려면 이름을 알아야하는거아니야? 이름을 어디서 안거야?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044545.1.7",
          "form": "그러게 처음에 어떻게 봤는지 기억이 안나네",
          "original_form": "그러게 처음에 어떻게 봤는지 기억이 안나네",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044545.1.8",
          "form": "드림캐쳐는 문별 라디오 보다가 그랬는데",
          "original_form": "드림캐쳐는 문별 라디오 보다가 그랬는데",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044545.1.9",
          "form": "나머지는 어디였을까ㅋㅋㅋ",
          "original_form": "나머지는 어디였을까ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044545.1.10",
          "form": "ㅋㅋㅋ 라디오 들어? 주기적으로?",
          "original_form": "ㅋㅋㅋ 라디오 들어? 주기적으로?",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044545.1.11",
          "form": "아니ㅋㅋ라디오같은거는 주기적으로 안듣지ㅋㅋ",
          "original_form": "아니ㅋㅋ라디오같은거는 주기적으로 안듣지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044545.1.12",
          "form": "편집해놓은거 봤겠지? 아마?",
          "original_form": "편집해놓은거 봤겠지? 아마?",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044545.1.13",
          "form": "그럼 라디오도 유툽으로 검색하다가? 와 검색하게 됐는데?",
          "original_form": "그럼 라디오도 유툽으로 검색하다가? 와 검색하게 됐는데?",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044545.1.14",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044545.1.15",
          "form": "아니 라디오는 문별나오니까 본거지",
          "original_form": "아니 라디오는 문별나오니까 본거지",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044545.1.16",
          "form": "온앤오프도 왜 입덕했었는지 기억이 안나네",
          "original_form": "온앤오프도 왜 입덕했었는지 기억이 안나네",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044545.1.17",
          "form": "아 문별좋아해?\r\n온앤오프도 좋지! 그때 킹덤말고 한거 뭐였지? 거기서 봤을때 괜찮았는데",
          "original_form": "아 문별좋아해?\r\n온앤오프도 좋지! 그때 킹덤말고 한거 뭐였지? 거기서 봤을때 괜찮았는데",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044545.1.18",
          "form": "마마무 좋아한다 하지않았나요? 제가?",
          "original_form": "마마무 좋아한다 하지않았나요? 제가?",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044545.1.19",
          "form": "다들 관심이 없구만",
          "original_form": "다들 관심이 없구만",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044545.1.20",
          "form": "킹덤 말고 또 있어?",
          "original_form": "킹덤 말고 또 있어?",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044545.1.21",
          "form": "로드투킹덤이었다ㅋㅋ\r\n마마무좋아했구나ㅋㅋㅋ",
          "original_form": "로드투킹덤이었다ㅋㅋ\r\n마마무좋아했구나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:18"
        }
      ]
    }
