          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025507.1.21",
          "form": "히든밴딩도 있고",
          "original_form": "히든밴딩도 있고",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025507.1.22",
          "form": "뒷주머니도 있고",
          "original_form": "뒷주머니도 있고",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025507.1.23",
          "form": "마감도 꼼꼼한데",
          "original_form": "마감도 꼼꼼한데",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025507.1.24",
          "form": "여자거슬랙스는 밴딩같는거 없고",
          "original_form": "여자거슬랙스는 밴딩같는거 없고",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025507.1.25",
          "form": "뒷주머니는 페이크고",
          "original_form": "뒷주머니는 페이크고",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025507.1.26",
          "form": "마감 더 별론데",
          "original_form": "마감 더 별론데",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025507.1.27",
          "form": "으엥",
          "original_form": "으엥",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100025507.1.28",
          "form": "가격은 똑같아",
          "original_form": "가격은 똑같아",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100025507.1.29",
          "form": "그게 달라?",
          "original_form": "그게 달라?",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100025507.1.30",
          "form": "퀄리티가?",
          "original_form": "퀄리티가?",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100025507.1.31",
          "form": "몰랐지",
          "original_form": "몰랐지",
