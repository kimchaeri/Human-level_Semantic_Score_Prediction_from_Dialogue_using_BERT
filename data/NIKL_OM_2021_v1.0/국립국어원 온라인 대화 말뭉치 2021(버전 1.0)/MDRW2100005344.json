      "utterance": [
        {
          "id": "MDRW2100005344.1.1",
          "form": "난 그 고등학생때",
          "original_form": "난 그 고등학생때",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005344.1.2",
          "form": "영어가 참 어려웠어...",
          "original_form": "영어가 참 어려웠어...",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005344.1.3",
          "form": "실제로 수능에서도 젤 망햇고....",
          "original_form": "실제로 수능에서도 젤 망햇고....",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005344.1.4",
          "form": "영어...",
          "original_form": "영어...",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005344.1.5",
          "form": "아니 명박이는",
          "original_form": "아니 명박이는",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005344.1.6",
          "form": "왜 유리때",
          "original_form": "왜 유리때",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005344.1.7",
          "form": "우리때",
          "original_form": "우리때",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.8",
          "form": "난 영포 수포잖아...",
          "original_form": "난 영포 수포잖아...",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.9",
          "form": "에이 비를 나눠서",
          "original_form": "에이 비를 나눠서",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.10",
          "form": "억울하게ㅡㅡ",
          "original_form": "억울하게ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아.. ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.12",
          "form": "ㅋㅋㅋㅋ너 근데 수학 막 엄청 못티고",
          "original_form": "ㅋㅋㅋㅋ너 근데 수학 막 엄청 못티고",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.13",
          "form": "그러진않았던걸로 기억해",
          "original_form": "그러진않았던걸로 기억해",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.14",
          "form": "내...수학...?",
          "original_form": "내...수학...?",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.15",
          "form": "난...",
          "original_form": "난...",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.16",
          "form": "잘 찍었지 ㅎ",
          "original_form": "잘 찍었지 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005344.1.17",
          "form": "서술형 1번문제",
          "original_form": "서술형 1번문제",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005344.1.18",
          "form": "노가다로 계산하는게 국룰이었지...",
          "original_form": "노가다로 계산하는게 국룰이었지...",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005344.1.19",
          "form": "그리고 모르겠으면 1, 0, -1중에",
          "original_form": "그리고 모르겠으면 1, 0, -1중에",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005344.1.20",
          "form": "찍기...",
          "original_form": "찍기...",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005344.1.21",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005344.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005344.1.23",
          "form": "수학 잘알ㅋㅋㅋㅋㅋ",
          "original_form": "수학 잘알ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005344.1.24",
          "form": "서술형이지만 뭔가",
          "original_form": "서술형이지만 뭔가",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005344.1.25",
          "form": "서술형 1,2는",
          "original_form": "서술형 1,2는",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005344.1.26",
          "form": "어느정도 노가다가 되긴했어..",
          "original_form": "어느정도 노가다가 되긴했어..",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005344.1.27",
          "form": "그리고 수열 파트도",
          "original_form": "그리고 수열 파트도",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005344.1.28",
          "form": "진짜 미친(?)사람들은",
          "original_form": "진짜 미친(?)사람들은",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005344.1.29",
          "form": "노가다 하곤 하더라...",
          "original_form": "노가다 하곤 하더라...",
          "speaker_id": "1",
          "time": "20210821 21:58"
