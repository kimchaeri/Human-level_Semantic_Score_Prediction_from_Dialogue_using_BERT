      "utterance": [
        {
          "id": "MDRW2100023318.1.1",
          "form": "흰지부분 잘모르겠어요?",
          "original_form": "흰지부분 잘모르겠어요?",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100023318.1.2",
          "form": "어떤 점을 말씀하시는걸까욤?ㅎㅎ",
          "original_form": "어떤 점을 말씀하시는걸까욤?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100023318.1.3",
          "form": "그그 접히는부분요",
          "original_form": "그그 접히는부분요",
          "speaker_id": "2",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100023318.1.4",
          "form": "네네 흰지부분 저는 불편함을 모르겠어요??ㅎㅎ",
          "original_form": "네네 흰지부분 저는 불편함을 모르겠어요??ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100023318.1.5",
          "form": "오호 좀 거슬린다해서 쓰다보면 안그렇다던데 ㅋㅋㅋ 저도 우선 ㅋㅋㅋ 지금 요금제랑 공시2년이 안끝나서 ㅋㅋㅋ",
          "original_form": "오호 좀 거슬린다해서 쓰다보면 안그렇다던데 ㅋㅋㅋ 저도 우선 ㅋㅋㅋ 지금 요금제랑 공시2년이 안끝나서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100023318.1.6",
          "form": "견적좀 뽑아 달라했어요 ㅋㅋ",
          "original_form": "견적좀 뽑아 달라했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100023318.1.7",
          "form": "ㅎㅎㅎ오옷 그러시군용^^  배터리 빨리닳고 액정약하고 발열있고..",
          "original_form": "ㅎㅎㅎ오옷 그러시군용^^  배터리 빨리닳고 액정약하고 발열있고..",
          "speaker_id": "1",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100023318.1.8",
          "form": "주문해놓고 걱정이긴했는데",
          "original_form": "주문해놓고 걱정이긴했는데",
          "speaker_id": "1",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100023318.1.9",
          "form": "막상 구매해서 받으니까  너무예뻐서 다잊었어요 ㅋㄱ",
          "original_form": "막상 구매해서 받으니까  너무예뻐서 다잊었어요 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100023318.1.10",
          "form": "맞아요 너무 이뻐서 진짜 ㅠㅠ",
          "original_form": "맞아요 너무 이뻐서 진짜 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:12"
        },
        {
          "id": "MDRW2100023318.1.11",
          "form": "다 용서 된다고 ㅋㅋㅋ",
          "original_form": "다 용서 된다고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:12"
        },
        {
          "id": "MDRW2100023318.1.12",
          "form": "그래서 지금 제일 사고픈 물건이",
          "original_form": "그래서 지금 제일 사고픈 물건이",
          "speaker_id": "1",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100023318.1.13",
          "form": "폰케이스에요ㅠㅠㅋㅋㅋ",
          "original_form": "폰케이스에요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100023318.1.14",
          "form": "ㅋㅋㅋ 제발투명한 거 사주세요",
          "original_form": "ㅋㅋㅋ 제발투명한 거 사주세요",
