      "utterance": [
        {
          "id": "MDRW2100022121.1.1",
          "form": "아빤 집안일 잘 도와주시죠",
          "original_form": "아빤 집안일 잘 도와주시죠",
          "speaker_id": "2",
          "time": "20210829 08:29"
        },
        {
          "id": "MDRW2100022121.1.2",
          "form": "다른 집은 이정도도 안한다고",
          "original_form": "다른 집은 이정도도 안한다고",
          "speaker_id": "1",
          "time": "20210829 08:29"
        },
        {
          "id": "MDRW2100022121.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 08:29"
        },
        {
          "id": "MDRW2100022121.1.4",
          "form": "스트레스를 푸는 것도 필요한데 말이야",
          "original_form": "스트레스를 푸는 것도 필요한데 말이야",
          "speaker_id": "1",
          "time": "20210829 08:30"
        },
        {
          "id": "MDRW2100022121.1.5",
          "form": "근데 엄마 스트레스도 신경써줘야져",
          "original_form": "근데 엄마 스트레스도 신경써줘야져",
          "speaker_id": "2",
          "time": "20210829 08:30"
        },
        {
          "id": "MDRW2100022121.1.6",
          "form": "그런 그렇다.. 그래서 같이 가사일도 하고 하는건데",
          "original_form": "그런 그렇다.. 그래서 같이 가사일도 하고 하는건데",
          "speaker_id": "1",
          "time": "20210829 08:30"
        },
        {
          "id": "MDRW2100022121.1.7",
          "form": "보기 좋아요",
          "original_form": "보기 좋아요",
          "speaker_id": "2",
          "time": "20210829 08:30"
        },
        {
          "id": "MDRW2100022121.1.8",
          "form": "난 남자라도 집안일을 할 줄 알아야 한다고 생각한다",
          "original_form": "난 남자라도 집안일을 할 줄 알아야 한다고 생각한다",
          "speaker_id": "1",
          "time": "20210829 08:30"
        },
        {
          "id": "MDRW2100022121.1.9",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210829 08:30"
        },
        {
          "id": "MDRW2100022121.1.10",
          "form": "너도 집안일 할줄아는 남자 만나거라",
          "original_form": "너도 집안일 할줄아는 남자 만나거라",
          "speaker_id": "1",
          "time": "20210829 08:31"
        },
        {
          "id": "MDRW2100022121.1.11",
          "form": "있을까요?",
          "original_form": "있을까요?",
          "speaker_id": "2",
          "time": "20210829 08:31"
        },
        {
          "id": "MDRW2100022121.1.12",
          "form": "요즘은 다들 교육을 잘 받아서 옛날같지 않아",
          "original_form": "요즘은 다들 교육을 잘 받아서 옛날같지 않아",
          "speaker_id": "1",
          "time": "20210829 08:31"
        },
        {
          "id": "MDRW2100022121.1.13",
          "form": "그렇긴하겠ㅈㅛ?",
          "original_form": "그렇긴하겠ㅈㅛ?",
          "speaker_id": "2",
          "time": "20210829 08:31"
        },
        {
          "id": "MDRW2100022121.1.14",
          "form": "그래.. 난 왠만한 밥과 반찬도 할 수 있잖니",
          "original_form": "그래.. 난 왠만한 밥과 반찬도 할 수 있잖니",
          "speaker_id": "1",
          "time": "20210829 08:32"
        },
        {
          "id": "MDRW2100022121.1.15",
          "form": "그래서 대단해요",
          "original_form": "그래서 대단해요",
          "speaker_id": "2",
          "time": "20210829 08:32"
        },
        {
          "id": "MDRW2100022121.1.16",
          "form": "너희 엄마가 워낙 요리 잘해서 내가 굳이 많이 할 기회는 없지만 또 옆에서 시키는 것은 잘하니까",
          "original_form": "너희 엄마가 워낙 요리 잘해서 내가 굳이 많이 할 기회는 없지만 또 옆에서 시키는 것은 잘하니까",
          "speaker_id": "1",
          "time": "20210829 08:32"
        },
        {
          "id": "MDRW2100022121.1.17",
          "form": "그게 어디에요",
          "original_form": "그게 어디에요",
          "speaker_id": "2",
          "time": "20210829 08:32"
        }
      ]
