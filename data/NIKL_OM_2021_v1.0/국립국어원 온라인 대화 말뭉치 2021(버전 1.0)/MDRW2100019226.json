      "utterance": [
        {
          "id": "MDRW2100019226.1.1",
          "form": "오늘 조퇴?",
          "original_form": "오늘 조퇴?",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019226.1.2",
          "form": "아니 백신휴가",
          "original_form": "아니 백신휴가",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019226.1.3",
          "form": "회사는 좀 어떻노?",
          "original_form": "회사는 좀 어떻노?",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019226.1.4",
          "form": "그냥 정신없음",
          "original_form": "그냥 정신없음",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019226.1.5",
          "form": "백신휴가도 주나",
          "original_form": "백신휴가도 주나",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019226.1.6",
          "form": "없었는데 이제 주나봐",
          "original_form": "없었는데 이제 주나봐",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019226.1.7",
          "form": "개인 연차쓰고 따로 신청해서 받아야됨",
          "original_form": "개인 연차쓰고 따로 신청해서 받아야됨",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100019226.1.8",
          "form": "아 그럼 연차 안깍이고?",
          "original_form": "아 그럼 연차 안깍이고?",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019226.1.9",
          "form": "엉 개꿀이지",
          "original_form": "엉 개꿀이지",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019226.1.10",
          "form": "너네는??",
          "original_form": "너네는??",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019226.1.11",
          "form": "우리는 딱히없어",
          "original_form": "우리는 딱히없어",
          "speaker_id": "1",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019226.1.12",
          "form": "개인이 다 소화해야하니께",
          "original_form": "개인이 다 소화해야하니께",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019226.1.13",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019226.1.14",
          "form": "일찍 퇴근시켜주는 정도..",
          "original_form": "일찍 퇴근시켜주는 정도..",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019226.1.15",
          "form": "연차 부럽다",
          "original_form": "연차 부럽다",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019226.1.16",
          "form": "그래도 너네는 아플때 자유롭게 쉴수있으니까",
          "original_form": "그래도 너네는 아플때 자유롭게 쉴수있으니까",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019226.1.17",
          "form": "그게더 좋지않나",
          "original_form": "그게더 좋지않나",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019226.1.18",
          "form": "자유롭게 아니고",
          "original_form": "자유롭게 아니고",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019226.1.19",
          "form": "일많으면 그것도 힘들징",
          "original_form": "일많으면 그것도 힘들징",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019226.1.20",
          "form": "장단점이 있구만 ㅜ",
          "original_form": "장단점이 있구만 ㅜ",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019226.1.21",
          "form": "아무래도 개인회사니깐",
          "original_form": "아무래도 개인회사니깐",
          "speaker_id": "1",
          "time": "20210827 16:34"
        }
      ]
