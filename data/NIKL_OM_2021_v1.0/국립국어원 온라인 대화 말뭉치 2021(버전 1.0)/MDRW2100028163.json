      "utterance": [
        {
          "id": "MDRW2100028163.1.1",
          "form": "맨날 칼퇴해요?",
          "original_form": "맨날 칼퇴해요?",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028163.1.2",
          "form": "야근 없음???",
          "original_form": "야근 없음???",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100028163.1.3",
          "form": "맨날칼퇴",
          "original_form": "맨날칼퇴",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028163.1.4",
          "form": "야근없음",
          "original_form": "야근없음",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028163.1.5",
          "form": "일잇을때 야근하면 다른날 대체로빼쥼",
          "original_form": "일잇을때 야근하면 다른날 대체로빼쥼",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028163.1.6",
          "form": "부럽네",
          "original_form": "부럽네",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028163.1.7",
          "form": "몸을 팔아 월급을 받습니다",
          "original_form": "몸을 팔아 월급을 받습니다",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028163.1.8",
          "form": "젊을 때 벌어야지 ㅇㅇ...",
          "original_form": "젊을 때 벌어야지 ㅇㅇ...",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028163.1.9",
          "form": "야근수당준다면서요 ㅋㅋㅋㅋ",
          "original_form": "야근수당준다면서요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028163.1.10",
          "form": "우린안줌 ㅠ",
          "original_form": "우린안줌 ㅠ",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028163.1.11",
          "form": "어우 선넘네요",
          "original_form": "어우 선넘네요",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028163.1.12",
          "form": "그럼 가야죠",
          "original_form": "그럼 가야죠",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028163.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028163.1.14",
          "form": "지금완전",
          "original_form": "지금완전",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028163.1.15",
          "form": "박쳐요",
          "original_form": "박쳐요",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028163.1.16",
          "form": "학부모전화와서 자기애가 내일학교갈가방 두고갓다고 다시오래요 ㅋㅋㅋㅋㅋㅋ택시비줄것도아니면서",
          "original_form": "학부모전화와서 자기애가 내일학교갈가방 두고갓다고 다시오래요 ㅋㅋㅋㅋㅋㅋ택시비줄것도아니면서",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028163.1.17",
          "form": "인성무엇",
          "original_form": "인성무엇",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028163.1.18",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028163.1.19",
          "form": "제정신아니네",
          "original_form": "제정신아니네",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028163.1.20",
          "form": "어케됐어요?",
          "original_form": "어케됐어요?",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028163.1.21",
          "form": "내일일찍출근하는걸로",
          "original_form": "내일일찍출근하는걸로",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028163.1.22",
          "form": "ㅡㅡ진짜",
          "original_form": "ㅡㅡ진짜",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028163.1.23",
          "form": "ㅂㅂ",
          "original_form": "ㅂㅂ",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028163.1.24",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.25",
          "form": "어휴 진상이네",
          "original_form": "어휴 진상이네",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.26",
          "form": "빨리가 몇신데여?",
          "original_form": "빨리가 몇신데여?",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.27",
          "form": "8시반이요 ㅋㅋㅋㅋ",
          "original_form": "8시반이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.28",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.29",
          "form": "아니 8시반에 가방들고 학교가야하니꺄",
          "original_form": "아니 8시반에 가방들고 학교가야하니꺄",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.30",
          "form": "8시10붖까지가야댐",
          "original_form": "8시10붖까지가야댐",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.31",
          "form": "8시쯤 ?",
          "original_form": "8시쯤 ?",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.32",
          "form": "학교까지가져다달래요",
          "original_form": "학교까지가져다달래요",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.34",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028163.1.35",
          "form": "멍게소리를",
          "original_form": "멍게소리를",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028163.1.36",
          "form": "참하게 하시네",
          "original_form": "참하게 하시네",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028163.1.37",
          "form": "종인줄아나",
          "original_form": "종인줄아나",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028163.1.38",
          "form": "집도착",
          "original_form": "집도착",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028163.1.39",
          "form": "오늘은 15분걸렬네",
          "original_form": "오늘은 15분걸렬네",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028163.1.40",
          "form": "역시 차가 최곱니다",
          "original_form": "역시 차가 최곱니다",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028163.1.41",
          "form": "애 오면 뒤집오지게혼낼예정",
          "original_form": "애 오면 뒤집오지게혼낼예정",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028163.1.42",
          "form": "맛잇는저녁드세요,, 저는",
          "original_form": "맛잇는저녁드세요,, 저는",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028163.1.43",
          "form": "필테갑니다ㅠ",
          "original_form": "필테갑니다ㅠ",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028163.1.44",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028163.1.45",
          "form": "야식 드시게요?",
          "original_form": "야식 드시게요?",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028163.1.46",
          "form": "점심 풀때기에",
          "original_form": "점심 풀때기에",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028163.1.47",
          "form": "저녁 운동이면 백프론데",
          "original_form": "저녁 운동이면 백프론데",
          "speaker_id": "2",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100028163.1.48",
          "form": "집와서 아몬드브리즈먹음ㅋㅋㅋㅋ",
          "original_form": "집와서 아몬드브리즈먹음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028163.1.49",
          "form": "하두배고파서 배안고픈지경에 다다름",
          "original_form": "하두배고파서 배안고픈지경에 다다름",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028163.1.50",
          "form": "오 드디어",
          "original_form": "오 드디어",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028163.1.51",
          "form": "현자가 되셨군요",
          "original_form": "현자가 되셨군요",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028163.1.52",
          "form": "아배곱파 ㅠ",
          "original_form": "아배곱파 ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028163.1.53",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028163.1.54",
          "form": "아.. 저 구운감자(과자) 좋아하는데요",
          "original_form": "아.. 저 구운감자(과자) 좋아하는데요",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028163.1.55",
          "form": "구운고구마는 별로네요",
          "original_form": "구운고구마는 별로네요",
          "speaker_id": "2",
