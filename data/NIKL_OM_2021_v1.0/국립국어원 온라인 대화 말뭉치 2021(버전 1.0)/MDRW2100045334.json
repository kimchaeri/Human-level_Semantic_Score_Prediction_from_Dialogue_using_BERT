      "utterance": [
        {
          "id": "MDRW2100045334.1.1",
          "form": "좋아하는 음식이 뭐에요?",
          "original_form": "좋아하는 음식이 뭐에요?",
          "speaker_id": "1",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045334.1.2",
          "form": "오 저는 아무거나 다 잘먹어요!",
          "original_form": "오 저는 아무거나 다 잘먹어요!",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045334.1.3",
          "form": "해산물을 특히 좋아해요 ㅎㅎ",
          "original_form": "해산물을 특히 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045334.1.4",
          "form": "오 그렇군요!!",
          "original_form": "오 그렇군요!!",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045334.1.5",
          "form": "그럼 못드시는 음식 잇으세요?",
          "original_form": "그럼 못드시는 음식 잇으세요?",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045334.1.6",
          "form": "못 먹는 음식은 딱히 없는데 샐러드는 잘 안먹는거같아요 ㅎㅎ",
          "original_form": "못 먹는 음식은 딱히 없는데 샐러드는 잘 안먹는거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045334.1.7",
          "form": "아 샐러드 맛 없죠ㅠㅠ",
          "original_form": "아 샐러드 맛 없죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045334.1.8",
          "form": "사실 왜 먹는지 모르겠어요 ㅠㅠㅋㅋㅋ",
          "original_form": "사실 왜 먹는지 모르겠어요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045334.1.9",
          "form": "막 연어나 가슴살같이 맛있는 것만 골라먹구싶어요",
          "original_form": "막 연어나 가슴살같이 맛있는 것만 골라먹구싶어요",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045334.1.10",
          "form": "그러니까요 야채 너무 싫어요 ㅋㅋㅋ",
          "original_form": "그러니까요 야채 너무 싫어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045334.1.11",
          "form": "혹시 그럼 무슨 음식 좋아하세요??",
          "original_form": "혹시 그럼 무슨 음식 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045334.1.12",
          "form": "전 마라탕 좋아해요!",
          "original_form": "전 마라탕 좋아해요!",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045334.1.13",
          "form": "와!! 마라탕 저도 좋아해요",
          "original_form": "와!! 마라탕 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045334.1.14",
          "form": "양고기파에요 소고기파에요?! ㅎㅎ",
          "original_form": "양고기파에요 소고기파에요?! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045334.1.15",
          "form": "전 소요!!",
          "original_form": "전 소요!!",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045334.1.16",
          "form": "근데 양도 먹어봣는데 사실 차이점을 모르겟어요 ㅋㅋㅋ",
          "original_form": "근데 양도 먹어봣는데 사실 차이점을 모르겟어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045334.1.17",
          "form": "그쵸 ㅋㅋㅋ 저도 같이 먹는 친구한테 맞춰요 ㅋㅋ 다 좋아해서",
          "original_form": "그쵸 ㅋㅋㅋ 저도 같이 먹는 친구한테 맞춰요 ㅋㅋ 다 좋아해서",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045334.1.18",
          "form": "저도 그런거 같아요 ㅋㅋㅋㅋ",
          "original_form": "저도 그런거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045334.1.19",
          "form": "처음 먹을때 두려웟는데 먹고나니 똑같더라구요 ㅋㅋ",
          "original_form": "처음 먹을때 두려웟는데 먹고나니 똑같더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045334.1.20",
          "form": "그쵸 ㅋㅋㅋ 예민한 친구들은 양고기 향이 난다던데",
          "original_form": "그쵸 ㅋㅋㅋ 예민한 친구들은 양고기 향이 난다던데",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045334.1.21",
          "form": "사실 저는 잘 모르겠어요",
          "original_form": "사실 저는 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045334.1.22",
          "form": "전 특히 비염잇어서 더 냄새 구별못해요 ㅋㅋ",
          "original_form": "전 특히 비염잇어서 더 냄새 구별못해요 ㅋㅋ",
