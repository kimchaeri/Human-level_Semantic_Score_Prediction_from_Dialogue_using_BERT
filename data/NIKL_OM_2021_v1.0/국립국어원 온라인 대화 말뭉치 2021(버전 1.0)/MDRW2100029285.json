      "utterance": [
        {
          "id": "MDRW2100029285.1.1",
          "form": "안녕하세요 좋아하는 과일있으신가용?",
          "original_form": "안녕하세요 좋아하는 과일있으신가용?",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029285.1.2",
          "form": "넵 안녕하세요 ㅎ",
          "original_form": "넵 안녕하세요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029285.1.3",
          "form": "전 여룸과일은 대부분 다 좋아해요",
          "original_form": "전 여룸과일은 대부분 다 좋아해요",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029285.1.4",
          "form": "특히 수박이요 ㅎ",
          "original_form": "특히 수박이요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029285.1.5",
          "form": "수박 완전맛있죠!",
          "original_form": "수박 완전맛있죠!",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029285.1.6",
          "form": "어느 과일 좋아하세요?",
          "original_form": "어느 과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029285.1.7",
          "form": "저는 포도요!",
          "original_form": "저는 포도요!",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029285.1.8",
          "form": "앗 포도도 좋죠 ㅎㅎㅎ",
          "original_form": "앗 포도도 좋죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029285.1.9",
          "form": "저도 포도 좋아하는데 ㅎㅎㅎㅎ",
          "original_form": "저도 포도 좋아하는데 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029285.1.10",
          "form": "특히 씨없는 포도가 좋더라구요 ㅋ",
          "original_form": "특히 씨없는 포도가 좋더라구요 ㅋ",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029285.1.11",
          "form": "먹기도 편하고요!",
          "original_form": "먹기도 편하고요!",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029285.1.12",
          "form": "넵 ㅎㅎㅎ 어렀을땐 씨때문에 포도 별로 안좋아했는데 지금은 없어서 ㅜㅡ ...",
          "original_form": "넵 ㅎㅎㅎ 어렀을땐 씨때문에 포도 별로 안좋아했는데 지금은 없어서 ㅜㅡ ...",
          "speaker_id": "2",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029285.1.13",
          "form": "저는 귀찮아서 씨도 삼켜요ㅋㅋㅋ",
          "original_form": "저는 귀찮아서 씨도 삼켜요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029285.1.14",
          "form": "ㅋㅋㅋㅋㅋ 저는 껍질도 삼켜요 ㅎㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 저는 껍질도 삼켜요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029285.1.15",
          "form": "헉 한수 위시네요",
          "original_form": "헉 한수 위시네요",
          "speaker_id": "1",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029285.1.16",
          "form": "ㅋㅋㅋㅋ  전 수박은 화채때문에 좋아해요 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ  전 수박은 화채때문에 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029285.1.17",
          "form": "여름에 화채면 ㅎㅎㅎㅎ",
          "original_form": "여름에 화채면 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029285.1.18",
          "form": "오옹 화채에 사이다 넣으시나요?",
          "original_form": "오옹 화채에 사이다 넣으시나요?",
          "speaker_id": "1",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029285.1.19",
          "form": "아뇽 ㅎ 암바사요 ㅎㅎㅎ",
          "original_form": "아뇽 ㅎ 암바사요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029285.1.20",
          "form": "암바사가 더 맛있더라구요",
          "original_form": "암바사가 더 맛있더라구요",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029285.1.21",
          "form": "밀키스랑은 또 다른가요?",
          "original_form": "밀키스랑은 또 다른가요?",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029285.1.22",
          "form": "밀키스랑은 조금더 다른 ㅎ",
          "original_form": "밀키스랑은 조금더 다른 ㅎ",
          "speaker_id": "2",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029285.1.23",
          "form": "둘다 해봤는데 암바사가 더 맛있더라구요",
          "original_form": "둘다 해봤는데 암바사가 더 맛있더라구요",
          "speaker_id": "2",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029285.1.24",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029285.1.25",
          "form": "암바사를 못먹어봐서",
          "original_form": "암바사를 못먹어봐서",
          "speaker_id": "1",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029285.1.26",
          "form": "어떻게 다른가용",
          "original_form": "어떻게 다른가용",
          "speaker_id": "1",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029285.1.27",
          "form": "한번 드셔보세요 ㅎ 뭐라 말하긴 그런데 놀러가서 둘다 블라인드 테스트 해보면 다들 암바사더라구요",
          "original_form": "한번 드셔보세요 ㅎ 뭐라 말하긴 그런데 놀러가서 둘다 블라인드 테스트 해보면 다들 암바사더라구요",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029285.1.28",
          "form": "과일 예기하니 또 조금 뭐가 땡기네요 ㅎㅎㅎ",
          "original_form": "과일 예기하니 또 조금 뭐가 땡기네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029285.1.29",
          "form": "세상에 임상실험까지ㅋㅋㅋㅋ",
          "original_form": "세상에 임상실험까지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029285.1.30",
          "form": "새벽이라 야식땡겨요..",
          "original_form": "새벽이라 야식땡겨요..",
          "speaker_id": "1",
          "time": "20210903 01:12"
        }
      ]
