      "utterance": [
        {
          "id": "MDRW2100042951.1.1",
          "form": "name2님 마늘쫑 좋아하세요?",
          "original_form": "&name2&님 마늘쫑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 11:57"
        },
        {
          "id": "MDRW2100042951.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아니 이런질문을",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아니 이런질문을",
          "speaker_id": "2",
          "time": "20210907 11:58"
        },
        {
          "id": "MDRW2100042951.1.3",
          "form": "간장에 절인건 좋아해요",
          "original_form": "간장에 절인건 좋아해요",
          "speaker_id": "2",
          "time": "20210907 11:58"
        },
        {
          "id": "MDRW2100042951.1.4",
          "form": "ㅋㅋㅋㅋㅋ 닉네임보고 계속 궁금했어요",
          "original_form": "ㅋㅋㅋㅋㅋ 닉네임보고 계속 궁금했어요",
          "speaker_id": "1",
          "time": "20210907 11:58"
        },
        {
          "id": "MDRW2100042951.1.5",
          "form": "절인거요? 아니면 졸인거요?",
          "original_form": "절인거요? 아니면 졸인거요?",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042951.1.6",
          "form": "저는 절인거 좋아요!!",
          "original_form": "저는 절인거 좋아요!!",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042951.1.7",
          "form": "고추장으로 무친것도 좋아요",
          "original_form": "고추장으로 무친것도 좋아요",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042951.1.8",
          "form": "저두요ㅋㅋ근데 건새우랑 같이볶은거?",
          "original_form": "저두요ㅋㅋ근데 건새우랑 같이볶은거?",
          "speaker_id": "2",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100042951.1.9",
          "form": "그건싫어해요ㅜ",
          "original_form": "그건싫어해요ㅜ",
          "speaker_id": "2",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100042951.1.10",
          "form": "악 저도 그거 싫어요",
          "original_form": "악 저도 그거 싫어요",
          "speaker_id": "1",
          "time": "20210907 12:04"
        },
        {
          "id": "MDRW2100042951.1.11",
          "form": "건새우도 싫어요 ㅠ",
          "original_form": "건새우도 싫어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:04"
        },
        {
          "id": "MDRW2100042951.1.12",
          "form": "name1님 무슨음식좋아하세요?",
          "original_form": "&name1&님 무슨음식좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 12:51"
        },
        {
          "id": "MDRW2100042951.1.13",
          "form": "사실 저는 가리는게 많아 없어서 다 잘 먹긴하는데ㅋㅋㅋ",
          "original_form": "사실 저는 가리는게 많아 없어서 다 잘 먹긴하는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042951.1.14",
          "form": "초밥이랑 쌀국수 먹고 싶네요",
          "original_form": "초밥이랑 쌀국수 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100042951.1.15",
          "form": "비와서 뜨신게 땡겨요",
          "original_form": "비와서 뜨신게 땡겨요",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100042951.1.16",
          "form": "헐 저도...",
          "original_form": "헐 저도...",
          "speaker_id": "2",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100042951.1.17",
          "form": "전 칼국수 먹고파요",
          "original_form": "전 칼국수 먹고파요",
          "speaker_id": "2",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100042951.1.18",
          "form": "헐 칼국수도 엄청 좋아요",
          "original_form": "헐 칼국수도 엄청 좋아요",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100042951.1.19",
          "form": "얘기만해도 몸 뜨끈해지는 기분이네요",
          "original_form": "얘기만해도 몸 뜨끈해지는 기분이네요",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100042951.1.20",
          "form": "시장 바지락칼국수... 못먹은지 꽤 됐는데 먹고싶네요",
          "original_form": "시장 바지락칼국수... 못먹은지 꽤 됐는데 먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100042951.1.21",
          "form": "뜨신게 땡겼지만 배달음식 참았어요...흑 분명 돈을 아꼈는데 슬퍼요",
          "original_form": "뜨신게 땡겼지만 배달음식 참았어요...흑 분명 돈을 아꼈는데 슬퍼요",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100042951.1.22",
          "form": "name2님 저녁 드셨나요?",
          "original_form": "&name2&님 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100042951.1.23",
          "form": "전 아직이요ㅋㅋㅋㅠ 학원이 안끝났어요",
          "original_form": "전 아직이요ㅋㅋㅋㅠ 학원이 안끝났어요",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100042951.1.24",
          "form": "아이고 ㅠㅠ 늦게까지 있네요",
          "original_form": "아이고 ㅠㅠ 늦게까지 있네요",
