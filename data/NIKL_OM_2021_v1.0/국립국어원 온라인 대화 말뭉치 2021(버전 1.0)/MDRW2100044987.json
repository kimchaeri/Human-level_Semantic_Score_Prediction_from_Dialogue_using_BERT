      "utterance": [
        {
          "id": "MDRW2100044987.1.1",
          "form": "아이유 캐릭터 좋고 눈이 화려함",
          "original_form": "아이유 캐릭터 좋고 눈이 화려함",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044987.1.2",
          "form": "알았옹 ㅋㅋ",
          "original_form": "알았옹 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044987.1.3",
          "form": "아이유 어쩜 그리 연기도하고 노래도하고",
          "original_form": "아이유 어쩜 그리 연기도하고 노래도하고",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044987.1.4",
          "form": "가주가 주인공이면 보통 OST하던데 안했더라",
          "original_form": "가주가 주인공이면 보통 OST하던데 안했더라",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044987.1.5",
          "form": "자기가 오스트하면 몰입깨진다고",
          "original_form": "자기가 오스트하면 몰입깨진다고",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044987.1.6",
          "form": "지금 OST도 좋긴하지만 한편 아쉽더라",
          "original_form": "지금 OST도 좋긴하지만 한편 아쉽더라",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044987.1.7",
          "form": "안했대",
          "original_form": "안했대",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044987.1.8",
          "form": "맞나 더 잘맞게 할수있는거아닌가",
          "original_form": "맞나 더 잘맞게 할수있는거아닌가",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044987.1.9",
          "form": "난 아이유 목소리가 내취향 아닌듯",
          "original_form": "난 아이유 목소리가 내취향 아닌듯",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044987.1.10",
          "form": "몰랐었는데 이해안돼네ㅎ",
          "original_form": "몰랐었는데 이해안돼네ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044987.1.11",
          "form": "그렇구나 난 그냥 들어ㅎ",
          "original_form": "그렇구나 난 그냥 들어ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044987.1.12",
          "form": "난 공기반소리반 이느낌보다",
          "original_form": "난 공기반소리반 이느낌보다",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044987.1.13",
          "form": "진성뙇이좋아",
          "original_form": "진성뙇이좋아",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044987.1.14",
          "form": "가주 중 내 취향이다싶은 목소리 모르겠어ㅎ",
          "original_form": "가주 중 내 취향이다싶은 목소리 모르겠어ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044987.1.15",
          "form": "아하ㅎ",
          "original_form": "아하ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044987.1.16",
          "form": "같은 의미에서 성시경도 내취향아님",
          "original_form": "같은 의미에서 성시경도 내취향아님",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044987.1.17",
          "form": "아~ ㅎㅎㅎ",
          "original_form": "아~ ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044987.1.18",
          "form": "공기반소리반 박진영 생각남ㅋ",
          "original_form": "공기반소리반 박진영 생각남ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044987.1.19",
          "form": "난 조성모같이 청아하면서 목소리가 또렷하게 나는게 좋더라",
          "original_form": "난 조성모같이 청아하면서 목소리가 또렷하게 나는게 좋더라",
