      "utterance": [
        {
          "id": "MDRW2100034697.1.1",
          "form": "구입하시고 싶으신거 있으세요?",
          "original_form": "구입하시고 싶으신거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100034697.1.2",
          "form": "구입하고 싶은 물건은 항상 많죠ㅎㅎ\r\n주로 아이쇼핑이지만요",
          "original_form": "구입하고 싶은 물건은 항상 많죠ㅎㅎ\r\n주로 아이쇼핑이지만요",
          "speaker_id": "2",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100034697.1.3",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100034697.1.4",
          "form": "보통 오프라인으로 사시나요?",
          "original_form": "보통 오프라인으로 사시나요?",
          "speaker_id": "1",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100034697.1.5",
          "form": "전 온라인 쇼핑을 더 즐겨요ㅎㅎ",
          "original_form": "전 온라인 쇼핑을 더 즐겨요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034697.1.6",
          "form": "오 저두요 ㅋㅋㅋ",
          "original_form": "오 저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034697.1.7",
          "form": "저는 장도 온라인으로 봐요",
          "original_form": "저는 장도 온라인으로 봐요",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100034697.1.8",
          "form": "전 장볼때는 아직까지 오프 이용해요\r\n온라인은 가끔요\r\n식재료 제외는 주로 온라인이지만요",
          "original_form": "전 장볼때는 아직까지 오프 이용해요\r\n온라인은 가끔요\r\n식재료 제외는 주로 온라인이지만요",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100034697.1.9",
          "form": "아무래도 신선식품은 오프가 좋긴하죠!",
          "original_form": "아무래도 신선식품은 오프가 좋긴하죠!",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100034697.1.10",
          "form": "전 무거운게 너무 싫어서 온라인으로 사게 되더라구요",
          "original_form": "전 무거운게 너무 싫어서 온라인으로 사게 되더라구요",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100034697.1.11",
          "form": "맞아요 오프로 사면 무겁긴 해요ㅜㅜ",
          "original_form": "맞아요 오프로 사면 무겁긴 해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034697.1.12",
          "form": "그나마 구매 후 배달서비스가 있어서 다행인거 같아요",
          "original_form": "그나마 구매 후 배달서비스가 있어서 다행인거 같아요",
          "speaker_id": "2",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034697.1.13",
          "form": "맞아요 여름엔 더 힘들어요",
          "original_form": "맞아요 여름엔 더 힘들어요",
          "speaker_id": "1",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100034697.1.14",
          "form": "아! 마트에서 배달도 해주더라구요",
          "original_form": "아! 마트에서 배달도 해주더라구요",
          "speaker_id": "1",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100034697.1.15",
          "form": "보통 3만원 이상이면 배달해주더라구요",
          "original_form": "보통 3만원 이상이면 배달해주더라구요",
          "speaker_id": "2",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100034697.1.16",
          "form": "여름은 정말 덥죠ㅠㅠ",
          "original_form": "여름은 정말 덥죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100034697.1.17",
          "form": "그래도 장보는 시간 만큼은 마트 안이 시원하니 그건 좋아요ㅋㅋㅋ",
          "original_form": "그래도 장보는 시간 만큼은 마트 안이 시원하니 그건 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100034697.1.18",
          "form": "오3만원이면 괜찮네요",
          "original_form": "오3만원이면 괜찮네요",
          "speaker_id": "1",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100034697.1.19",
          "form": "그것도 그래요 ㅋㅋㅋ 나가기싫죠",
          "original_form": "그것도 그래요 ㅋㅋㅋ 나가기싫죠",
          "speaker_id": "1",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100034697.1.20",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100034697.1.21",
          "form": "날이 곧 선선해지면",
          "original_form": "날이 곧 선선해지면",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100034697.1.22",
          "form": "산책삼아가도 괜찮겠어요",
          "original_form": "산책삼아가도 괜찮겠어요",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100034697.1.23",
          "form": "근데 좀 늦은시간에 가면 마감이라 배달 안 되서 들고와야해요",
          "original_form": "근데 좀 늦은시간에 가면 마감이라 배달 안 되서 들고와야해요",
          "speaker_id": "2",
          "time": "20210828 17:49"
        }
      ]
    }
