      "utterance": [
        {
          "id": "MDRW2100003226.1.1",
          "form": "신혼집은 어디?",
          "original_form": "신혼집은 어디?",
          "speaker_id": "1",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100003226.1.2",
          "form": "대구나 구미",
          "original_form": "대구나 구미",
          "speaker_id": "2",
          "time": "20210820 22:35"
        },
        {
          "id": "MDRW2100003226.1.3",
          "form": "대구 살았으면 좋겠다",
          "original_form": "대구 살았으면 좋겠다",
          "speaker_id": "1",
          "time": "20210820 22:39"
        },
        {
          "id": "MDRW2100003226.1.4",
          "form": "나도 그랬으면 좋겠네",
          "original_form": "나도 그랬으면 좋겠네",
          "speaker_id": "2",
          "time": "20210820 22:43"
        },
        {
          "id": "MDRW2100003226.1.5",
          "form": "안녕 name2",
          "original_form": "안녕 &name2&",
          "speaker_id": "1",
          "time": "20210821 08:42"
        },
        {
          "id": "MDRW2100003226.1.6",
          "form": "좋은 아침이야",
          "original_form": "좋은 아침이야",
          "speaker_id": "1",
          "time": "20210821 08:42"
        },
        {
          "id": "MDRW2100003226.1.7",
          "form": "name2",
          "original_form": "&name2&",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100003226.1.8",
          "form": "일하니?",
          "original_form": "일하니?",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100003226.1.9",
          "form": "잤지 지금은 일하고",
          "original_form": "잤지 지금은 일하고",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100003226.1.10",
          "form": "연애",
          "original_form": "연애",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100003226.1.11",
          "form": "흠 바쁘겠구나",
          "original_form": "흠 바쁘겠구나",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100003226.1.12",
          "form": "배달하는거야?",
          "original_form": "배달하는거야?",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100003226.1.13",
          "form": "아니 배달안해 그냥 홀봐주는거지",
          "original_form": "아니 배달안해 그냥 홀봐주는거지",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100003226.1.14",
          "form": "결혼 준비를 위해 열심히 일하는구나",
          "original_form": "결혼 준비를 위해 열심히 일하는구나",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100003226.1.15",
          "form": "그것까진 잘 모르겠지만",
          "original_form": "그것까진 잘 모르겠지만",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100003226.1.16",
          "form": "우리 지금 결혼 연애 얘기하고있잖아",
          "original_form": "우리 지금 결혼 연애 얘기하고있잖아",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100003226.1.17",
          "form": "결혼은 ㅁㅣ친 짓이야",
          "original_form": "결혼은 ㅁㅣ친 짓이야",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100003226.1.18",
          "form": "결혼은 미쳤ㅈㅣ",
          "original_form": "결혼은 미쳤ㅈㅣ",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
