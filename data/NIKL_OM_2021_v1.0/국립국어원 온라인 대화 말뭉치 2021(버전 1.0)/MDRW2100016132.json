        {
          "id": "MDRW2100016132.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 10:03"
        },
        {
          "id": "MDRW2100016132.1.2",
          "form": "네 날이 선선하네요",
          "original_form": "네 날이 선선하네요",
          "speaker_id": "2",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016132.1.3",
          "form": "맞아요 어제까지 비가 계속오더니",
          "original_form": "맞아요 어제까지 비가 계속오더니",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016132.1.4",
          "form": "해가 좀 떴어요",
          "original_form": "해가 좀 떴어요",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016132.1.5",
          "form": "name2지역은 어떠신가요",
          "original_form": "&name2&지역은 어떠신가요",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016132.1.6",
          "form": "인천이요 거긴요",
          "original_form": "인천이요 거긴요",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016132.1.7",
          "form": "아.. 인천은 해가 뜨셨구나",
          "original_form": "아.. 인천은 해가 뜨셨구나",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016132.1.8",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016132.1.9",
          "form": "봄가을요",
          "original_form": "봄가을요",
          "speaker_id": "2",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100016132.1.10",
          "form": "좋아하는 이유가 있으세요?",
          "original_form": "좋아하는 이유가 있으세요?",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016132.1.11",
          "form": "놀고 생활하기 좋아서요",
          "original_form": "놀고 생활하기 좋아서요",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016132.1.12",
          "form": "아.. 네..",
          "original_form": "아.. 네..",
          "speaker_id": "1",
          "time": "20210826 12:59"
        },
        {
          "id": "MDRW2100016132.1.13",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016132.1.14",
          "form": "저는 겨울좋아해요",
          "original_form": "저는 겨울좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016132.1.15",
          "form": "안더워서요..",
          "original_form": "안더워서요..",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016132.1.16",
          "form": "아 땀보닼 저도 추위가 나아요",
          "original_form": "아 땀보닼 저도 추위가 나아요",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016132.1.17",
          "form": "저두요 더운건 사람을 지치게하더라구요..",
          "original_form": "저두요 더운건 사람을 지치게하더라구요..",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
