      "utterance": [
        {
          "id": "MDRW2100003170.1.1",
          "form": "중고거래 잘 하시나요?",
          "original_form": "중고거래 잘 하시나요?",
          "speaker_id": "1",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003170.1.2",
          "form": "당근 종종 이용하고 있어요:)",
          "original_form": "당근 종종 이용하고 있어요:)",
          "speaker_id": "2",
          "time": "20210820 20:09"
        },
        {
          "id": "MDRW2100003170.1.3",
          "form": "중고거래 해보신적 있으세요?",
          "original_form": "중고거래 해보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210820 20:10"
        },
        {
          "id": "MDRW2100003170.1.4",
          "form": "네 한 때 당근마켓에 빠져서 팔기도 많이 팔고 사기도 많이 샀죠ㅎㅎ",
          "original_form": "네 한 때 당근마켓에 빠져서 팔기도 많이 팔고 사기도 많이 샀죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003170.1.5",
          "form": "뭐가 필요하면 항상 당근마켓 먼저 확인해봐요",
          "original_form": "뭐가 필요하면 항상 당근마켓 먼저 확인해봐요",
          "speaker_id": "1",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003170.1.6",
          "form": "지금도 이용하세요?",
          "original_form": "지금도 이용하세요?",
          "speaker_id": "2",
          "time": "20210820 22:06"
        },
        {
          "id": "MDRW2100003170.1.7",
          "form": "처음에는 당근 매너 좋은 분들이 많아서\r\n좋았는데 요샌 비매너인분들이 많아졌어요ㅠ",
          "original_form": "처음에는 당근 매너 좋은 분들이 많아서\r\n좋았는데 요샌 비매너인분들이 많아졌어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003170.1.8",
          "form": "요즘엔 미니멀리스트에 빠져가지고 잘 안 사게 돼요",
          "original_form": "요즘엔 미니멀리스트에 빠져가지고 잘 안 사게 돼요",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003170.1.9",
          "form": "맞아요 다짜고짜 가격을 터무니없이 깎는분들 보면 기분이 안 좋아져요",
          "original_form": "맞아요 다짜고짜 가격을 터무니없이 깎는분들 보면 기분이 안 좋아져요",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003170.1.10",
          "form": "미니멀라이프 좋죠!",
          "original_form": "미니멀라이프 좋죠!",
          "speaker_id": "2",
          "time": "20210820 23:21"
        },
        {
          "id": "MDRW2100003170.1.11",
          "form": "차라리 거래전 네고는 그렇다치는데",
          "original_form": "차라리 거래전 네고는 그렇다치는데",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003170.1.12",
          "form": "거래하러 오셔서 그러는 분들은 쫌...",
          "original_form": "거래하러 오셔서 그러는 분들은 쫌...",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003170.1.13",
          "form": "헐 거래하러 오셔서 그러는 분들이 계세요??",
          "original_form": "헐 거래하러 오셔서 그러는 분들이 계세요??",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003170.1.14",
          "form": "와 이미 나와서 안 하기도 뭐하고",
          "original_form": "와 이미 나와서 안 하기도 뭐하고",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003170.1.15",
          "form": "매너가 진짜 별로네요",
          "original_form": "매너가 진짜 별로네요",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003170.1.16",
          "form": "저도 직접 겪은건 아닌데",
          "original_form": "저도 직접 겪은건 아닌데",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003170.1.17",
          "form": "주변분들 당근썰 들어보면 \r\n그런 분들도 계시고",
          "original_form": "주변분들 당근썰 들어보면 \r\n그런 분들도 계시고",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003170.1.18",
          "form": "요샌 외국인분들도\r\n당근 많이 하시나봐요",
          "original_form": "요샌 외국인분들도\r\n당근 많이 하시나봐요",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003170.1.19",
          "form": "당근 거래오셔서\r\n자꾸 못알아듣는척하고\r\n돈 조금주고 가려하는분도 계셨데요ㅋ",
          "original_form": "당근 거래오셔서\r\n자꾸 못알아듣는척하고\r\n돈 조금주고 가려하는분도 계셨데요ㅋ",
          "speaker_id": "2",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003170.1.20",
          "form": "그걸 그런식으로 이용하다니..본인들이 인식 다 깎아 먹는 건 알까요",
          "original_form": "그걸 그런식으로 이용하다니..본인들이 인식 다 깎아 먹는 건 알까요",
          "speaker_id": "1",
          "time": "20210820 23:45"
        },
        {
          "id": "MDRW2100003170.1.21",
          "form": "좋은 분들도 많은데 몇몇이 물 흐리는 거 같아요",
          "original_form": "좋은 분들도 많은데 몇몇이 물 흐리는 거 같아요",
          "speaker_id": "1",
          "time": "20210820 23:45"
        },
        {
          "id": "MDRW2100003170.1.22",
          "form": "아마 그런 분들은\r\n그런 생각조차도 없이 사실거 같아오ㅡ",
          "original_form": "아마 그런 분들은\r\n그런 생각조차도 없이 사실거 같아오ㅡ",
          "speaker_id": "2",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003170.1.23",
          "form": "비가 오니까 레인부츠가 사고싶어져요",
          "original_form": "비가 오니까 레인부츠가 사고싶어져요",
          "speaker_id": "1",
          "time": "20210821 10:48"
        },
        {
          "id": "MDRW2100003170.1.24",
          "form": "요즘 젊은분들은 어디꺼 많이 신어요?",
          "original_form": "요즘 젊은분들은 어디꺼 많이 신어요?",
          "speaker_id": "2",
          "time": "20210821 11:03"
        },
        {
          "id": "MDRW2100003170.1.25",
          "form": "저때는 헌터였는데ㅋㅋ",
          "original_form": "저때는 헌터였는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:03"
        },
        {
          "id": "MDRW2100003170.1.26",
          "form": "요즘도 헌터 많이신어요!",
          "original_form": "요즘도 헌터 많이신어요!",
          "speaker_id": "1",
          "time": "20210821 12:11"
        },
        {
          "id": "MDRW2100003170.1.27",
          "form": "헌터 구하기도 힘들고 가격도 비싸니까 락피쉬도 많이 신어요",
          "original_form": "헌터 구하기도 힘들고 가격도 비싸니까 락피쉬도 많이 신어요",
          "speaker_id": "1",
          "time": "20210821 12:12"
        },
        {
          "id": "MDRW2100003170.1.28",
          "form": "오 요즘도 헌터 신구요!",
          "original_form": "오 요즘도 헌터 신구요!",
          "speaker_id": "2",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003170.1.29",
          "form": "아 락피쉬도 신었었어요ㅋㅋ",
          "original_form": "아 락피쉬도 신었었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003170.1.30",
          "form": "요즘도 비슷한가봐요",
          "original_form": "요즘도 비슷한가봐요",
          "speaker_id": "2",
          "time": "20210821 12:48"
        }
      ]
    }
