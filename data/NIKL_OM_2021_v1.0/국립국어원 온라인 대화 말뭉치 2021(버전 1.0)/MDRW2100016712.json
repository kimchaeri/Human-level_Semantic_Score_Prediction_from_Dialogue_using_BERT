      "utterance": [
        {
          "id": "MDRW2100016712.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016712.1.2",
          "form": "오늘은 모처럼 비가 내리지 않네요",
          "original_form": "오늘은 모처럼 비가 내리지 않네요",
          "speaker_id": "1",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016712.1.3",
          "form": "네 안녕하세요 정말 오랜만에 비가 안오네요",
          "original_form": "네 안녕하세요 정말 오랜만에 비가 안오네요",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016712.1.4",
          "form": "그래도 좀 흐리긴하지만요",
          "original_form": "그래도 좀 흐리긴하지만요",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016712.1.5",
          "form": "네 요즘 매일 비가 내렸는데",
          "original_form": "네 요즘 매일 비가 내렸는데",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016712.1.6",
          "form": "그러게요. 살짝 흐리네요",
          "original_form": "그러게요. 살짝 흐리네요",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016712.1.7",
          "form": "오늘 비 온다는 예보는 없었던 것 같은데",
          "original_form": "오늘 비 온다는 예보는 없었던 것 같은데",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016712.1.8",
          "form": "네 근데 내일은 비 예보가 있더라고요",
          "original_form": "네 근데 내일은 비 예보가 있더라고요",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016712.1.9",
          "form": "맑은 날이 그립네요 ㅠㅠ",
          "original_form": "맑은 날이 그립네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016712.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016712.1.11",
          "form": "맑은 날이 언제였던가",
          "original_form": "맑은 날이 언제였던가",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016712.1.12",
          "form": "올해는 특히 비가 많은거 같아요",
          "original_form": "올해는 특히 비가 많은거 같아요",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016712.1.13",
          "form": "일기예보보니 다음주까지는 계속 흐릴예정이래요",
          "original_form": "일기예보보니 다음주까지는 계속 흐릴예정이래요",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016712.1.14",
          "form": "아 가을장마라는 얘기 얼핏 들은 것 같아요",
          "original_form": "아 가을장마라는 얘기 얼핏 들은 것 같아요",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016712.1.15",
          "form": "원래 가을 이미지는 맑고 화창한 날씨인데ㅎㅎ",
          "original_form": "원래 가을 이미지는 맑고 화창한 날씨인데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016712.1.16",
          "form": "네 하늘이 높고 푸른",
          "original_form": "네 하늘이 높고 푸른",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016712.1.17",
          "form": "요새는 맑은 날이 드물긴해요",
          "original_form": "요새는 맑은 날이 드물긴해요",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016712.1.18",
          "form": "봄에는 미세먼지도 많고",
          "original_form": "봄에는 미세먼지도 많고",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016712.1.19",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016712.1.20",
          "form": "그래도 며칠 사이에 공기가 선선해져서",
          "original_form": "그래도 며칠 사이에 공기가 선선해져서",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016712.1.21",
          "form": "비 안오면 밖에 걷기에 좋더라고요",
          "original_form": "비 안오면 밖에 걷기에 좋더라고요",
          "speaker_id": "1",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016712.1.22",
          "form": "비만 안오면 딱 좋은 날씨긴해요 ㅎㅎ",
          "original_form": "비만 안오면 딱 좋은 날씨긴해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016712.1.23",
          "form": "덥지도 춥지도 않어서",
          "original_form": "덥지도 춥지도 않어서",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016712.1.24",
          "form": "네 이런 날이 흔치 않으니까",
          "original_form": "네 이런 날이 흔치 않으니까",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016712.1.25",
          "form": "가을이네 싶음 또 금세 추워지잖아요",
          "original_form": "가을이네 싶음 또 금세 추워지잖아요",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016712.1.26",
          "form": "맞아요 좋은 날씨는 짧죠 ㅠㅠ 아쉬워요",
          "original_form": "맞아요 좋은 날씨는 짧죠 ㅠㅠ 아쉬워요",
          "speaker_id": "2",
          "time": "20210826 15:31"
        }
      ]
