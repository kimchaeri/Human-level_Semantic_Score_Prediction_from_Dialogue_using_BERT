      "utterance": [
        {
          "id": "MDRW2100037935.1.1",
          "form": "언닌 집안 일 끝났어요?",
          "original_form": "언닌 집안 일 끝났어요?",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037935.1.2",
          "form": "놀다가 자면 됩니까?",
          "original_form": "놀다가 자면 됩니까?",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037935.1.3",
          "form": "아니 설거지ㅡㅡ",
          "original_form": "아니 설거지ㅡㅡ",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100037935.1.4",
          "form": "너무싫네",
          "original_form": "너무싫네",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100037935.1.5",
          "form": "식세기갖고싶다",
          "original_form": "식세기갖고싶다",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100037935.1.6",
          "form": "저도 요즘 식 세기에 의존하고 있습니다",
          "original_form": "저도 요즘 식 세기에 의존하고 있습니다",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037935.1.7",
          "form": "아무것도 하기 싫어요ㅋㅋㅋ",
          "original_form": "아무것도 하기 싫어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037935.1.8",
          "form": "부럽구나",
          "original_form": "부럽구나",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037935.1.9",
          "form": "시간은 많아졌는데 난 뭔가귀찮노",
          "original_form": "시간은 많아졌는데 난 뭔가귀찮노",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037935.1.10",
          "form": "한가한데 귀찮음",
          "original_form": "한가한데 귀찮음",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037935.1.11",
          "form": "원래 언니한가할 때가 더 귀찮은 것 같아요",
          "original_form": "원래 언니한가할 때가 더 귀찮은 것 같아요",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037935.1.12",
          "form": "바쁘고 시간 없으면 막 후다닥 하는데",
          "original_form": "바쁘고 시간 없으면 막 후다닥 하는데",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037935.1.13",
          "form": "그래 여유있으니 ㅋㅋ",
          "original_form": "그래 여유있으니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037935.1.14",
          "form": "나도 쉬는 날 집안일을 더 안 해요",
          "original_form": "나도 쉬는 날 집안일을 더 안 해요",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037935.1.15",
          "form": "ㅋ크ㅡ",
          "original_form": "ㅋ크ㅡ",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037935.1.16",
          "form": "오히려 일 가기 전에 더 열심히 함",
          "original_form": "오히려 일 가기 전에 더 열심히 함",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037935.1.17",
          "form": "쿠크는 뭐예요",
          "original_form": "쿠크는 뭐예요",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037935.1.18",
          "form": "잘못쳤다ㅋㅋ",
          "original_form": "잘못쳤다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100037935.1.19",
          "form": "울신랑은 영화보네ㅋ",
          "original_form": "울신랑은 영화보네ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100037935.1.20",
          "form": "언니 집중해 줘요ㅋㅋㅋㅋ",
          "original_form": "언니 집중해 줘요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100037935.1.21",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100037935.1.22",
          "form": "언니는 나와 함께 해요ㅋㅋㅋ",
          "original_form": "언니는 나와 함께 해요ㅋㅋㅋ",
