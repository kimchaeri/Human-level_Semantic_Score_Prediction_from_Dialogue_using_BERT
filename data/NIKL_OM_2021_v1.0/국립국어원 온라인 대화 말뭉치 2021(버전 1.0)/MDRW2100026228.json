      "utterance": [
        {
          "id": "MDRW2100026228.1.1",
          "form": "언니!!",
          "original_form": "언니!!",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026228.1.2",
          "form": "저예용",
          "original_form": "저예용",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026228.1.3",
          "form": "오늘도 완전 바쁘셨죠ㅜ",
          "original_form": "오늘도 완전 바쁘셨죠ㅜ",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026228.1.4",
          "form": "옹!",
          "original_form": "옹!",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026228.1.5",
          "form": "퇴근했어?",
          "original_form": "퇴근했어?",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026228.1.6",
          "form": "오늘도 고생 많았어~",
          "original_form": "오늘도 고생 많았어~",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026228.1.7",
          "form": "아직요!",
          "original_form": "아직요!",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026228.1.8",
          "form": "아직이야??ㅜㅜ",
          "original_form": "아직이야??ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026228.1.9",
          "form": "언니는 오늘 업무 너무 많아서 힘드셨을 것 같아요",
          "original_form": "언니는 오늘 업무 너무 많아서 힘드셨을 것 같아요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026228.1.10",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026228.1.11",
          "form": "아휴 말도마...ㅋㅋ 하나하나 말해줄게..ㅋㅋ",
          "original_form": "아휴 말도마...ㅋㅋ 하나하나 말해줄게..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026228.1.12",
          "form": "저는 오늘 박스까서 와인놓고\r\n몸이 너무 삭아서 힘들었어요",
          "original_form": "저는 오늘 박스까서 와인놓고\r\n몸이 너무 삭아서 힘들었어요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026228.1.13",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026228.1.14",
          "form": "고생이네 고생이야",
          "original_form": "고생이네 고생이야",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026228.1.15",
          "form": "나는 오늘 팀장 오는 날이었단말이지",
          "original_form": "나는 오늘 팀장 오는 날이었단말이지",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026228.1.16",
          "form": "아침부터 업무 시작도 안했는데 다짜고짜 자재마감 서류 달라고 하더라고",
          "original_form": "아침부터 업무 시작도 안했는데 다짜고짜 자재마감 서류 달라고 하더라고",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026228.1.17",
          "form": "근데 그 자재마감 하는건 내 업무가 아니거든",
          "original_form": "근데 그 자재마감 하는건 내 업무가 아니거든",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026228.1.18",
          "form": "그러니까 마감서류 챙기는것도 내 업무는 아닌거지",
          "original_form": "그러니까 마감서류 챙기는것도 내 업무는 아닌거지",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026228.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026228.1.20",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026228.1.21",
          "form": "언니 업무가아닌거죠",
          "original_form": "언니 업무가아닌거죠",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026228.1.22",
          "form": "그리고 그거 마감도 금요일까지거든",
          "original_form": "그리고 그거 마감도 금요일까지거든",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026228.1.23",
          "form": "시간 여유도 꽤있는데 오늘 오자마자 달라고 그러니까",
          "original_form": "시간 여유도 꽤있는데 오늘 오자마자 달라고 그러니까",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026228.1.24",
          "form": "나도 준비도 안되어있는데 아침부터 기운이 빠지는거징",
          "original_form": "나도 준비도 안되어있는데 아침부터 기운이 빠지는거징",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026228.1.25",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026228.1.26",
          "form": "그래도 어찌어찌해서 그 업무부터 해서 전달하고",
          "original_form": "그래도 어찌어찌해서 그 업무부터 해서 전달하고",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026228.1.27",
          "form": "아니. 언니한테 업무 맡겨놨대요?",
          "original_form": "아니. 언니한테 업무 맡겨놨대요?",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026228.1.28",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026228.1.29",
          "form": "그니까~~~~ 나한테 맡겨놧어 아주",
          "original_form": "그니까~~~~ 나한테 맡겨놧어 아주",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026228.1.30",
          "form": "그러고 내 업무 하고잇는데",
          "original_form": "그러고 내 업무 하고잇는데",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026228.1.31",
          "form": "네세",
          "original_form": "네세",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026228.1.32",
          "form": "점심시간 좀 남겨놧는데 밥먹으러 가자고 그러더니",
          "original_form": "점심시간 좀 남겨놧는데 밥먹으러 가자고 그러더니",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026228.1.33",
          "form": "준비하고 나가려는데 다른 직원하고 얘기하느라 안나오더라고",
          "original_form": "준비하고 나가려는데 다른 직원하고 얘기하느라 안나오더라고",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026228.1.34",
          "form": "그래서 그냥 그런가보다하고 대기하고 있다가 밥먹으러 가는데",
          "original_form": "그래서 그냥 그런가보다하고 대기하고 있다가 밥먹으러 가는데",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026228.1.35",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026228.1.36",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026228.1.37",
          "form": "뭐먹고싶냐고 물어보길래 보쌈이랫는대",
          "original_form": "뭐먹고싶냐고 물어보길래 보쌈이랫는대",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026228.1.38",
          "form": "가볍게 씹더니 찜닭을 먹으러가네?",
          "original_form": "가볍게 씹더니 찜닭을 먹으러가네?",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026228.1.39",
          "form": "찜닭..",
          "original_form": "찜닭..",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026228.1.40",
          "form": "차라리 물어보지를 말지",
          "original_form": "차라리 물어보지를 말지",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.41",
          "form": "첨부터 찜닭 먹으러가자 하면",
          "original_form": "첨부터 찜닭 먹으러가자 하면",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.42",
          "form": "완전 직장상사가 무시한거잖아요",
          "original_form": "완전 직장상사가 무시한거잖아요",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.43",
          "form": "그냥 좋게 끝날것을",
          "original_form": "그냥 좋게 끝날것을",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.44",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.45",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.46",
          "form": "그러더니",
          "original_form": "그러더니",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.47",
          "form": "물어보지나말지",
          "original_form": "물어보지나말지",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.48",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.49",
          "form": "오늘 그만두는 분이 잇엇거든",
          "original_form": "오늘 그만두는 분이 잇엇거든",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.50",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.51",
          "form": "그 분 핑계랑 내가 얼굴이 노랗게 떠서 달달한거 먹어야 될거같다며",
          "original_form": "그 분 핑계랑 내가 얼굴이 노랗게 떠서 달달한거 먹어야 될거같다며",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.52",
          "form": "ㅋㅋㅋㄱㄱㄲ",
          "original_form": "ㅋㅋㅋㄱㄱㄲ",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026228.1.53",
          "form": "마카롱을 사러가자는거야",
          "original_form": "마카롱을 사러가자는거야",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026228.1.54",
          "form": "완전 핑계죠 진짜",
          "original_form": "완전 핑계죠 진짜",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026228.1.55",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026228.1.56",
          "form": "그래서 사러갓는데 고르는거도 첨에 나보고 고르라더니",
          "original_form": "그래서 사러갓는데 고르는거도 첨에 나보고 고르라더니",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026228.1.57",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026228.1.58",
          "form": "마지막엔 다 팀장 맘대로 ㅋ",
          "original_form": "마지막엔 다 팀장 맘대로 ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026228.1.59",
          "form": "ㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026228.1.60",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026228.1.61",
          "form": "할말이없네요",
          "original_form": "할말이없네요",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026228.1.62",
