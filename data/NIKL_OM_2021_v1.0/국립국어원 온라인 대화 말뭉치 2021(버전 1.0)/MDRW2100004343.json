      "utterance": [
        {
          "id": "MDRW2100004343.1.1",
          "form": "최근 영화 뭐보셨어요?",
          "original_form": "최근 영화 뭐보셨어요?",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004343.1.2",
          "form": "저는 소울??봤었어요",
          "original_form": "저는 소울??봤었어요",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004343.1.3",
          "form": "소울 애니매이션이죠?",
          "original_form": "소울 애니매이션이죠?",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004343.1.4",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004343.1.5",
          "form": "마카롱님은 최근에 보신 영화가 무엇이었나요?",
          "original_form": "마카롱님은 최근에 보신 영화가 무엇이었나요?",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004343.1.6",
          "form": "일욜날 모가디슈봤어요",
          "original_form": "일욜날 모가디슈봤어요",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004343.1.7",
          "form": "아앗 검색해보니 실화네요!",
          "original_form": "아앗 검색해보니 실화네요!",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004343.1.8",
          "form": "네 재밌어요 감동도 있고",
          "original_form": "네 재밌어요 감동도 있고",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004343.1.9",
          "form": "나중에 봐야겠네요",
          "original_form": "나중에 봐야겠네요",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004343.1.10",
          "form": "극장에서 관람하셨나요?",
          "original_form": "극장에서 관람하셨나요?",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004343.1.11",
          "form": "네 영화관 주말에가니 사람 많더라구요",
          "original_form": "네 영화관 주말에가니 사람 많더라구요",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004343.1.12",
          "form": "저는 코로나 전에도 영화관 자주 안가서 요즘 영화관  어떤지 궁금했어요",
          "original_form": "저는 코로나 전에도 영화관 자주 안가서 요즘 영화관  어떤지 궁금했어요",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004343.1.13",
          "form": "저도 잘안가는데 예매권있어서 보고왔어요",
          "original_form": "저도 잘안가는데 예매권있어서 보고왔어요",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004343.1.14",
          "form": "요즘 비싸더라구요",
          "original_form": "요즘 비싸더라구요",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004343.1.15",
          "form": "오 예매권있으면 무조건 가야죠",
          "original_form": "오 예매권있으면 무조건 가야죠",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004343.1.16",
          "form": "아는분이  2인 7만원하는걸 주셔서  누워서보고 왔어요ㅎ",
          "original_form": "아는분이  2인 7만원하는걸 주셔서  누워서보고 왔어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004343.1.17",
          "form": "비지니스석같은 의자에서",
          "original_form": "비지니스석같은 의자에서",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004343.1.18",
          "form": "2인 7만원!! 우와 호화롭네요",
          "original_form": "2인 7만원!! 우와 호화롭네요",
          "speaker_id": "2",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004343.1.19",
          "form": "저도 첨가봤어요 물도 에비앙으로 주고",
          "original_form": "저도 첨가봤어요 물도 에비앙으로 주고",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004343.1.20",
          "form": "음료도 갖다주고",
          "original_form": "음료도 갖다주고",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004343.1.21",
          "form": "우와....",
          "original_form": "우와....",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004343.1.22",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        }
