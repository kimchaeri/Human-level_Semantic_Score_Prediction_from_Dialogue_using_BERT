      "utterance": [
        {
          "id": "MDRW2100003043.1.1",
          "form": "게임을 하시나요?",
          "original_form": "게임을 하시나요?",
          "speaker_id": "1",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003043.1.2",
          "form": "요즘은 안합니다",
          "original_form": "요즘은 안합니다",
          "speaker_id": "2",
          "time": "20210820 17:56"
        },
        {
          "id": "MDRW2100003043.1.3",
          "form": "전에는 어떤거 하셨나요?",
          "original_form": "전에는 어떤거 하셨나요?",
          "speaker_id": "1",
          "time": "20210820 17:56"
        },
        {
          "id": "MDRW2100003043.1.4",
          "form": "오버워치요",
          "original_form": "오버워치요",
          "speaker_id": "2",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100003043.1.5",
          "form": "무슨게임하시나요",
          "original_form": "무슨게임하시나요",
          "speaker_id": "2",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100003043.1.6",
          "form": "저는 심즈요",
          "original_form": "저는 심즈요",
          "speaker_id": "1",
          "time": "20210820 18:05"
        },
        {
          "id": "MDRW2100003043.1.7",
          "form": "심즈 해본 적 있으세요??",
          "original_form": "심즈 해본 적 있으세요??",
          "speaker_id": "1",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100003043.1.8",
          "form": "아니요 짤은 많이 봤어요",
          "original_form": "아니요 짤은 많이 봤어요",
          "speaker_id": "2",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100003043.1.9",
          "form": "그렇군요 오버워치는 왜 더 안 하시나요",
          "original_form": "그렇군요 오버워치는 왜 더 안 하시나요",
          "speaker_id": "1",
          "time": "20210820 18:08"
        },
        {
          "id": "MDRW2100003043.1.10",
          "form": "나이가드니 멀미나서 못하겠더라구요ㅋㅋ",
          "original_form": "나이가드니 멀미나서 못하겠더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003043.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003043.1.12",
          "form": "요새도 사람들이 많이 하나요???",
          "original_form": "요새도 사람들이 많이 하나요???",
          "speaker_id": "1",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003043.1.13",
          "form": "ㅋㅋ 화면돌리는데 어지럽더라구요",
          "original_form": "ㅋㅋ 화면돌리는데 어지럽더라구요",
          "speaker_id": "2",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003043.1.14",
          "form": "하는사람은 하는것같아요",
          "original_form": "하는사람은 하는것같아요",
          "speaker_id": "2",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003043.1.15",
          "form": "ㅋㅋㅋ여전히 인기 많나보네요",
          "original_form": "ㅋㅋㅋ여전히 인기 많나보네요",
          "speaker_id": "1",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003043.1.16",
          "form": "그런가봐요ㅎ",
          "original_form": "그런가봐요ㅎ",
          "speaker_id": "2",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003043.1.17",
          "form": "저는 컴이 느려서 잘 안 하게 돼요",
          "original_form": "저는 컴이 느려서 잘 안 하게 돼요",
          "speaker_id": "1",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003043.1.18",
          "form": "네 그리고 컴앞에 앉을시간이 없네요",
          "original_form": "네 그리고 컴앞에 앉을시간이 없네요",
          "speaker_id": "2",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003043.1.19",
          "form": "거의 폰하고",
          "original_form": "거의 폰하고",
          "speaker_id": "2",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003043.1.20",
          "form": "네 요샌 걍 누워서 폰해요",
          "original_form": "네 요샌 걍 누워서 폰해요",
          "speaker_id": "1",
          "time": "20210820 18:15"
        },
        {
          "id": "MDRW2100003043.1.21",
          "form": "컴 켜기도 귀찮음..",
          "original_form": "컴 켜기도 귀찮음..",
          "speaker_id": "1",
          "time": "20210820 18:15"
        },
        {
          "id": "MDRW2100003043.1.22",
          "form": "ㅋㅋ맞아요",
          "original_form": "ㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003043.1.23",
          "form": "그리고 컴퓨터 게임들 너무 고사양이라 못하겠어요",
          "original_form": "그리고 컴퓨터 게임들 너무 고사양이라 못하겠어요",
          "speaker_id": "1",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100003043.1.24",
          "form": "뿌요뿌요나 하던 시절이 그립네요..",
          "original_form": "뿌요뿌요나 하던 시절이 그립네요..",
          "speaker_id": "1",
          "time": "20210820 18:17"
