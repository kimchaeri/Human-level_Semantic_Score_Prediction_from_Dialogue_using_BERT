      "utterance": [
        {
          "id": "MDRW2100008104.1.1",
          "form": "쇼핑은 뭐 주로해",
          "original_form": "쇼핑은 뭐 주로해",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008104.1.2",
          "form": "뭐 식자재랑 애들꺼",
          "original_form": "뭐 식자재랑 애들꺼",
          "speaker_id": "2",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008104.1.3",
          "form": "난 요새 취미 끊었더니 사는게 없어",
          "original_form": "난 요새 취미 끊었더니 사는게 없어",
          "speaker_id": "2",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008104.1.4",
          "form": "낙이 없네",
          "original_form": "낙이 없네",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008104.1.5",
          "form": "예전엔 장난감 샀거든",
          "original_form": "예전엔 장난감 샀거든",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008104.1.6",
          "form": "식자재를 사는데 취미를 들여야하나...",
          "original_form": "식자재를 사는데 취미를 들여야하나...",
          "speaker_id": "1",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008104.1.7",
          "form": "넌 뭐 쇼핑해?",
          "original_form": "넌 뭐 쇼핑해?",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008104.1.8",
          "form": "어떤 장난감 샀어ㅋㅋㅋㅋ",
          "original_form": "어떤 장난감 샀어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008104.1.9",
          "form": "나는 식재료ㅋㅋㅋ",
          "original_form": "나는 식재료ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008104.1.10",
          "form": "헉 ㅜㅜ 그게 취미야 이제",
          "original_form": "헉 ㅜㅜ 그게 취미야 이제",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.11",
          "form": "내가 집에서 먹고 싶은 애들ㅋㅋㅋㅋ사는게 취미야ㅋㅋㅋㅋ",
          "original_form": "내가 집에서 먹고 싶은 애들ㅋㅋㅋㅋ사는게 취미야ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.12",
          "form": "쿠폰 잘써서ㅋㅋㅋ",
          "original_form": "쿠폰 잘써서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.13",
          "form": "나도 쿠폰 쏠쏠해",
          "original_form": "나도 쿠폰 쏠쏠해",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.14",
          "form": "식완이라고",
          "original_form": "식완이라고",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.15",
          "form": "오 뭐써 응응",
          "original_form": "오 뭐써 응응",
          "speaker_id": "1",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.16",
          "form": "미니어처",
          "original_form": "미니어처",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.17",
          "form": "이마트에도 팔아",
          "original_form": "이마트에도 팔아",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.18",
          "form": "리멘트 제품",
          "original_form": "리멘트 제품",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.19",
          "form": "식완? 장난감이야 식재료야",
          "original_form": "식완? 장난감이야 식재료야",
          "speaker_id": "1",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.20",
          "form": "조만간 하나 지를려고",
          "original_form": "조만간 하나 지를려고",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ혼동이 온닼ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ혼동이 온닼ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008104.1.22",
          "form": "장난감",
          "original_form": "장난감",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008104.1.23",
          "form": "식품완구 ㅋㅋ껌들어있어서",
          "original_form": "식품완구 ㅋㅋ껌들어있어서",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008104.1.24",
          "form": "오홍ㅋㅋㅋㅋㅋ그래 취미생활 꾸준히 하면좋지ㅋㅋㅋ",
          "original_form": "오홍ㅋㅋㅋㅋㅋ그래 취미생활 꾸준히 하면좋지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008104.1.25",
          "form": "다찰았는데 다시 사게",
          "original_form": "다찰았는데 다시 사게",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008104.1.26",
          "form": "앜ㅋㅋㅋㅋㅋ비싸서 자제중인거야?",
          "original_form": "앜ㅋㅋㅋㅋㅋ비싸서 자제중인거야?",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008104.1.27",
          "form": "여새 가입시 공짜 쿠폰 쏠쏠해",
          "original_form": "여새 가입시 공짜 쿠폰 쏠쏠해",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008104.1.28",
          "form": "비마트 이렁데?",
          "original_form": "비마트 이렁데?",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008104.1.29",
          "form": "아니 식었다 다시 좀 흥미가 생겨",
          "original_form": "아니 식었다 다시 좀 흥미가 생겨",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008104.1.30",
          "form": "응 ㅋㅋ",
          "original_form": "응 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008104.1.31",
          "form": "미안해서 하나 더 시킴",
          "original_form": "미안해서 하나 더 시킴",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008104.1.32",
          "form": "ㅋㅋㅋㅋ마자 그동안은 왜 몰랐지ㅋㅋㅋ싶은게 많넼ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ마자 그동안은 왜 몰랐지ㅋㅋㅋ싶은게 많넼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008104.1.33",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008104.1.34",
          "form": "세상이 달리보임",
          "original_form": "세상이 달리보임",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008104.1.35",
          "form": "싸게사서 기분도 좋아",
          "original_form": "싸게사서 기분도 좋아",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008104.1.36",
          "form": "한개 더 구매해도ㅋㅋㅋ",
          "original_form": "한개 더 구매해도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008104.1.37",
          "form": "쿠폰 못쓰면 이불킥",
          "original_form": "쿠폰 못쓰면 이불킥",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008104.1.38",
          "form": "응 이익",
          "original_form": "응 이익",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008104.1.39",
          "form": "세상에 싸게 못사면 바보돼",
          "original_form": "세상에 싸게 못사면 바보돼",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008104.1.40",
          "form": "나 이불킥ㅋㅋㅋ",
          "original_form": "나 이불킥ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008104.1.41",
          "form": "비마트 첫구매 0워딜 2개 날렸거든",
          "original_form": "비마트 첫구매 0워딜 2개 날렸거든",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008104.1.42",
          "form": "1.5만원 최소 손해다ㅋㅋㅋ",
          "original_form": "1.5만원 최소 손해다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008104.1.43",
          "form": "맞아 그런게 자꾸 실수해서 날려",
          "original_form": "맞아 그런게 자꾸 실수해서 날려",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008104.1.44",
