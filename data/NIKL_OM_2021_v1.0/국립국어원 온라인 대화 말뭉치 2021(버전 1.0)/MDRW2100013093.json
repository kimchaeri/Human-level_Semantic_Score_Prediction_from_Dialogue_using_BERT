        {
          "id": "MDRW2100013093.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 12:49"
        },
        {
          "id": "MDRW2100013093.1.2",
          "form": "요샌 못갔지만 좋아해요",
          "original_form": "요샌 못갔지만 좋아해요",
          "speaker_id": "2",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100013093.1.3",
          "form": "바다 주로 가는것 같네요",
          "original_form": "바다 주로 가는것 같네요",
          "speaker_id": "2",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100013093.1.4",
          "form": "오 바다 좋죠",
          "original_form": "오 바다 좋죠",
          "speaker_id": "1",
          "time": "20210825 12:51"
        },
        {
          "id": "MDRW2100013093.1.5",
          "form": "여행 어디 가보셨나요?",
          "original_form": "여행 어디 가보셨나요?",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100013093.1.6",
          "form": "대만이요!",
          "original_form": "대만이요!",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100013093.1.7",
          "form": "안가봤는데 어때요?",
          "original_form": "안가봤는데 어때요?",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100013093.1.8",
          "form": "넘 좋아요ㅠ",
          "original_form": "넘 좋아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100013093.1.9",
          "form": "한번 가보고 싶네요.",
          "original_form": "한번 가보고 싶네요.",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100013093.1.10",
          "form": "코로나가 끝나야ㅠㅠ",
          "original_form": "코로나가 끝나야ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100013093.1.11",
          "form": "그러니까요ㅠㅠ 뭘 할 수가 없네요",
          "original_form": "그러니까요ㅠㅠ 뭘 할 수가 없네요",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100013093.1.12",
          "form": "세부도 가고싶네요",
          "original_form": "세부도 가고싶네요",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100013093.1.13",
          "form": "유럽여행도 가보고싶어요",
          "original_form": "유럽여행도 가보고싶어요",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100013093.1.14",
          "form": "오 유럽도 버킷리스트ㅠ",
          "original_form": "오 유럽도 버킷리스트ㅠ",
          "speaker_id": "1",
