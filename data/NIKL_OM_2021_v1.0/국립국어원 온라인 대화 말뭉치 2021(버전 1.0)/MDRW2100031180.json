      "utterance": [
        {
          "id": "MDRW2100031180.1.1",
          "form": "저는 학교 졸업한지 너무 오래돼서",
          "original_form": "저는 학교 졸업한지 너무 오래돼서",
          "speaker_id": "2",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031180.1.2",
          "form": "초등학생으로 돌아가고 싶어요",
          "original_form": "초등학생으로 돌아가고 싶어요",
          "speaker_id": "1",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031180.1.3",
          "form": "왜 초등학생이에요",
          "original_form": "왜 초등학생이에요",
          "speaker_id": "2",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031180.1.4",
          "form": "아무 걱정 없이 놀고 싶어요",
          "original_form": "아무 걱정 없이 놀고 싶어요",
          "speaker_id": "1",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031180.1.5",
          "form": "그렇지는 않을 거예요",
          "original_form": "그렇지는 않을 거예요",
          "speaker_id": "2",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031180.1.6",
          "form": "학교 앞 문방구에서 군것질하고 싶어요",
          "original_form": "학교 앞 문방구에서 군것질하고 싶어요",
          "speaker_id": "1",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031180.1.7",
          "form": "뭐가 맛있었나요",
          "original_form": "뭐가 맛있었나요",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031180.1.8",
          "form": "물만두요",
          "original_form": "물만두요",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031180.1.9",
          "form": "맛있었겠다네요",
          "original_form": "맛있었겠다네요",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031180.1.10",
          "form": "내 식초가 많이 들어갔었는데도 달달하고 좋았어요",
          "original_form": "내 식초가 많이 들어갔었는데도 달달하고 좋았어요",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031180.1.11",
          "form": "저도 먹어보고 싶네요",
          "original_form": "저도 먹어보고 싶네요",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031180.1.12",
          "form": "근데 지금은 안 팔더라구요",
          "original_form": "근데 지금은 안 팔더라구요",
