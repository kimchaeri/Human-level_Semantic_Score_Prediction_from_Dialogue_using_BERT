      "utterance": [
        {
          "id": "MDRW2100003098.1.1",
          "form": "아 그래 경북궁에도 하는데",
          "original_form": "아 그래 경북궁에도 하는데",
          "speaker_id": "2",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003098.1.2",
          "form": "경기도 이천에 별빛정원이라고 있다",
          "original_form": "경기도 이천에 별빛정원이라고 있다",
          "speaker_id": "1",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003098.1.3",
          "form": "평범할듯",
          "original_form": "평범할듯",
          "speaker_id": "2",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003098.1.4",
          "form": "빛과 자연이 어울리는 신개념이라는데",
          "original_form": "빛과 자연이 어울리는 신개념이라는데",
          "speaker_id": "1",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003098.1.5",
          "form": "그래? 자세히",
          "original_form": "그래? 자세히",
          "speaker_id": "2",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003098.1.6",
          "form": "미디어아트로 조명이 좀 틀린갑다",
          "original_form": "미디어아트로 조명이 좀 틀린갑다",
          "speaker_id": "1",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003098.1.7",
          "form": "특이하네 그럼",
          "original_form": "특이하네 그럼",
          "speaker_id": "2",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003098.1.8",
          "form": "미디어 아트 체험시설도 있고 라이팅쇼도 있다야",
          "original_form": "미디어 아트 체험시설도 있고 라이팅쇼도 있다야",
          "speaker_id": "1",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003098.1.9",
          "form": "시간은 언제인데",
          "original_form": "시간은 언제인데",
          "speaker_id": "2",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003098.1.10",
          "form": "점등이후 15분마다 한다는데",
          "original_form": "점등이후 15분마다 한다는데",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003098.1.11",
          "form": "흥 15분마다면 길게 안하겠다 무료야?",
          "original_form": "흥 15분마다면 길게 안하겠다 무료야?",
          "speaker_id": "2",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003098.1.12",
          "form": "아니  유료다",
          "original_form": "아니  유료다",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003098.1.13",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003098.1.14",
          "form": "버스킹도 있다는데 그럼 괜찮지",
          "original_form": "버스킹도 있다는데 그럼 괜찮지",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003098.1.15",
          "form": "버스킹까지",
          "original_form": "버스킹까지",
          "speaker_id": "2",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003098.1.16",
          "form": "응 거기다 산책로도 있고",
          "original_form": "응 거기다 산책로도 있고",
          "speaker_id": "1",
          "time": "20210820 18:46"
        },
        {
          "id": "MDRW2100003098.1.17",
          "form": "제주도 허브동산 가봤지",
          "original_form": "제주도 허브동산 가봤지",
