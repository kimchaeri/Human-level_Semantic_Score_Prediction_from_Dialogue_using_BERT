      "utterance": [
        {
          "id": "MDRW2100033547.1.1",
          "form": "카페는 어디 가세요?",
          "original_form": "카페는 어디 가세요?",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100033547.1.2",
          "form": "여기저기 가요",
          "original_form": "여기저기 가요",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100033547.1.3",
          "form": "혼자가면 조용한 집앞 이디야",
          "original_form": "혼자가면 조용한 집앞 이디야",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100033547.1.4",
          "form": "차타고 가다가 픽업은 스벅 ㅎ",
          "original_form": "차타고 가다가 픽업은 스벅 ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100033547.1.5",
          "form": "일리 라테가 땡기면 일리 ㅎㅎㅎ",
          "original_form": "일리 라테가 땡기면 일리 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100033547.1.6",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100033547.1.7",
          "form": "똑같아요",
          "original_form": "똑같아요",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100033547.1.8",
          "form": "자주가시는데 있으세요?",
          "original_form": "자주가시는데 있으세요?",
          "speaker_id": "1",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100033547.1.9",
          "form": "혼자는 이디야",
          "original_form": "혼자는 이디야",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100033547.1.10",
          "form": "스타벅스는 같이 가요",
          "original_form": "스타벅스는 같이 가요",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100033547.1.11",
          "form": "두군데죠 뭐",
          "original_form": "두군데죠 뭐",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100033547.1.12",
          "form": "컴포는 와플 먹으러가요",
          "original_form": "컴포는 와플 먹으러가요",
          "speaker_id": "2",
          "time": "20210824 13:11"
        },
        {
          "id": "MDRW2100033547.1.13",
          "form": "친구는 할리스? 골드인가? 회원이라고 ㅎㅎ",
          "original_form": "친구는 할리스? 골드인가? 회원이라고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100033547.1.14",
          "form": "커피랑 세트 4000원 정말 넘싸요",
          "original_form": "커피랑 세트 4000원 정말 넘싸요",
          "speaker_id": "2",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100033547.1.15",
          "form": "그친구만나면 할리스가요",
          "original_form": "그친구만나면 할리스가요",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100033547.1.16",
          "form": "오 골드 뭔가 좋아보여요",
          "original_form": "오 골드 뭔가 좋아보여요",
          "speaker_id": "2",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100033547.1.17",
          "form": "골드는 머든 우와하게되죠 ㅎㅎ",
          "original_form": "골드는 머든 우와하게되죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100033547.1.18",
          "form": "컴포는 어디예요?",
          "original_form": "컴포는 어디예요?",
          "speaker_id": "1",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100033547.1.19",
          "form": "할리스도 가끔가요 근데 저희는 흡연구역에 아저씨 먾아서 ㅜㅜ자주 안가요",
          "original_form": "할리스도 가끔가요 근데 저희는 흡연구역에 아저씨 먾아서 ㅜㅜ자주 안가요",
          "speaker_id": "2",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100033547.1.20",
          "form": "커피가 1500원 조그마한 체인이요",
          "original_form": "커피가 1500원 조그마한 체인이요",
          "speaker_id": "2",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100033547.1.21",
          "form": "노란간판",
          "original_form": "노란간판",
          "speaker_id": "2",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100033547.1.22",
          "form": "여긴 와플이 2500이라 혼자 점심 때울때 좋아요",
          "original_form": "여긴 와플이 2500이라 혼자 점심 때울때 좋아요",
          "speaker_id": "2",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100033547.1.23",
          "form": "와 싸네요",
          "original_form": "와 싸네요",
          "speaker_id": "1",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100033547.1.24",
          "form": "여긴아직 안생겼나봐요",
          "original_form": "여긴아직 안생겼나봐요",
          "speaker_id": "1",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100033547.1.25",
          "form": "낯설어요",
          "original_form": "낯설어요",
          "speaker_id": "1",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100033547.1.26",
          "form": "네 ㅎㅎ진짜 넘사벽 가성비요",
          "original_form": "네 ㅎㅎ진짜 넘사벽 가성비요",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100033547.1.27",
          "form": "잘 없어요 ㅋㅋ",
          "original_form": "잘 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100033547.1.28",
          "form": "빽다방 이미지네요",
          "original_form": "빽다방 이미지네요",
          "speaker_id": "1",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100033547.1.29",
          "form": "저희동네만 유독 2개예요",
          "original_form": "저희동네만 유독 2개예요",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100033547.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100033547.1.31",
          "form": "저희 시골이라 교외에 카페도 많아요",
          "original_form": "저희 시골이라 교외에 카페도 많아요",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100033547.1.32",
          "form": "진짜 카페는 여기저기 많아요",
          "original_form": "진짜 카페는 여기저기 많아요",
          "speaker_id": "1",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100033547.1.33",
          "form": "개인이 운영해서 분위기랑 맛 풍경도 괜찮아요",
          "original_form": "개인이 운영해서 분위기랑 맛 풍경도 괜찮아요",
          "speaker_id": "2",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100033547.1.34",
          "form": "네 요새 온통 카페죠",
          "original_form": "네 요새 온통 카페죠",
          "speaker_id": "2",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100033547.1.35",
          "form": "식당은 없어도 카페는 꼭 하나씩은 있는거같아요",
          "original_form": "식당은 없어도 카페는 꼭 하나씩은 있는거같아요",
          "speaker_id": "1",
          "time": "20210824 13:15"
        },
        {
          "id": "MDRW2100033547.1.36",
          "form": "맞아요 허허벌판에도 카페는 있어요",
          "original_form": "맞아요 허허벌판에도 카페는 있어요",
          "speaker_id": "2",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100033547.1.37",
          "form": "커피맛에 풍경까지더하면 끝내주죠",
          "original_form": "커피맛에 풍경까지더하면 끝내주죠",
          "speaker_id": "1",
          "time": "20210824 13:16"
        }
      ]
