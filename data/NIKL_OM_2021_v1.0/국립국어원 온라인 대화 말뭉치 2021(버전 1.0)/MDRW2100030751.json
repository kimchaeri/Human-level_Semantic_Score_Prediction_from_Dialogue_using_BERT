      "utterance": [
        {
          "id": "MDRW2100030751.1.1",
          "form": "나 이제 점심먹어",
          "original_form": "나 이제 점심먹어",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100030751.1.2",
          "form": "먹고 온다",
          "original_form": "먹고 온다",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100030751.1.3",
          "form": "난 아점먹었어",
          "original_form": "난 아점먹었어",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030751.1.4",
          "form": "뭐먹을예정??",
          "original_form": "뭐먹을예정??",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030751.1.5",
          "form": "참치미역국 먹는 중",
          "original_form": "참치미역국 먹는 중",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030751.1.6",
          "form": "참치미역국 맛있겠다~",
          "original_form": "참치미역국 맛있겠다~",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030751.1.7",
          "form": "나는 엄마밥먹었어",
          "original_form": "나는 엄마밥먹었어",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030751.1.8",
          "form": "엄마손맛만한게 없지",
          "original_form": "엄마손맛만한게 없지",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030751.1.9",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030751.1.10",
          "form": "그리고 오징어장조림도",
          "original_form": "그리고 오징어장조림도",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030751.1.11",
          "form": "먹는중",
          "original_form": "먹는중",
          "speaker_id": "1",
          "time": "20210904 12:17"
        },
        {
          "id": "MDRW2100030751.1.12",
          "form": "오징어장조림??",
          "original_form": "오징어장조림??",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030751.1.13",
          "form": "오오 신기",
          "original_form": "오오 신기",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030751.1.14",
          "form": "생각보다",
          "original_form": "생각보다",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030751.1.15",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030751.1.16",
          "form": "얌얌이 테이스티굿~",
          "original_form": "얌얌이 테이스티굿~",
          "speaker_id": "2",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100030751.1.17",
          "form": "와플먹고싶다나는",
          "original_form": "와플먹고싶다나는",
          "speaker_id": "2",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100030751.1.18",
          "form": "와플조 좋다",
          "original_form": "와플조 좋다",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030751.1.19",
          "form": "난 크로플",
          "original_form": "난 크로플",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030751.1.20",
          "form": "너 그러고보니 어제 크로플먹는담서",
          "original_form": "너 그러고보니 어제 크로플먹는담서",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030751.1.21",
          "form": "먹엇니",
          "original_form": "먹엇니",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030751.1.22",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030751.1.23",
          "form": "와클 사먹었어",
          "original_form": "와클 사먹었어",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030751.1.24",
          "form": "와클 맛있었어?",
          "original_form": "와클 맛있었어?",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030751.1.25",
          "form": "짱 맛있지",
          "original_form": "짱 맛있지",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030751.1.26",
          "form": "편의점에 1500원에 팔더라",
          "original_form": "편의점에 1500원에 팔더라",
          "speaker_id": "1",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100030751.1.27",
          "form": "편의점은 정말 최고의 간식카페야",
          "original_form": "편의점은 정말 최고의 간식카페야",
