      "utterance": [
        {
          "id": "MDRW2100019788.1.1",
          "form": "요즘은 배달료가",
          "original_form": "요즘은 배달료가",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019788.1.2",
          "form": "더올라서 너무 부담스러워요",
          "original_form": "더올라서 너무 부담스러워요",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019788.1.3",
          "form": "맞아요.. 해먹는거도 재료가 그만큼 필요하지 않은데 많이 사야하고..",
          "original_form": "맞아요.. 해먹는거도 재료가 그만큼 필요하지 않은데 많이 사야하고..",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019788.1.4",
          "form": "이래저래 다 부담스러워요ㅠㅠ",
          "original_form": "이래저래 다 부담스러워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019788.1.5",
          "form": "그나마 밀키트 사면 양이 딱 적당해서 좋아요",
          "original_form": "그나마 밀키트 사면 양이 딱 적당해서 좋아요",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019788.1.6",
          "form": "저두 그래서 밀키트 가끔사먹어요",
          "original_form": "저두 그래서 밀키트 가끔사먹어요",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019788.1.7",
          "form": "마트에서 세일할때 사놓으면 든든하죠",
          "original_form": "마트에서 세일할때 사놓으면 든든하죠",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019788.1.8",
          "form": "가끔 티몬이나 위메프에서 싸게 팔때 있는데 그럴때 사면 좋아요ㅎㅎ",
          "original_form": "가끔 티몬이나 위메프에서 싸게 팔때 있는데 그럴때 사면 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019788.1.9",
          "form": "아하 팁감사해요",
          "original_form": "아하 팁감사해요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019788.1.10",
          "form": "과일은 좋아하시나요?",
          "original_form": "과일은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019788.1.11",
          "form": "네!! 완전 좋아해요ㅎㅎ",
          "original_form": "네!! 완전 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019788.1.12",
          "form": "근데 사실 과일도 비싸고 사면 한번에 많이 사야해서 먹기가 힘들죠ㅠㅠ",
          "original_form": "근데 사실 과일도 비싸고 사면 한번에 많이 사야해서 먹기가 힘들죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019788.1.13",
          "form": "그죠 참 1인가구는 장보기힘들어요",
          "original_form": "그죠 참 1인가구는 장보기힘들어요",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019788.1.14",
          "form": "저는 친구랑 둘이사는데도 식재료관리가 힘들더라구요",
          "original_form": "저는 친구랑 둘이사는데도 식재료관리가 힘들더라구요",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019788.1.15",
          "form": "과일도 둘이서 매일먹어야 안썩어요 ...",
          "original_form": "과일도 둘이서 매일먹어야 안썩어요 ...",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019788.1.16",
          "form": "그래서 남는과일 청을 만드는편이예요",
          "original_form": "그래서 남는과일 청을 만드는편이예요",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019788.1.17",
          "form": "그쵸ㅠㅠ 1인분 식료품점 이런거 생기면 좋겠네요",
          "original_form": "그쵸ㅠㅠ 1인분 식료품점 이런거 생기면 좋겠네요",
          "speaker_id": "2",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019788.1.18",
          "form": "과일청 만드는거 좋네요!!ㅋㅋㅋㅋ 보관하기도 좋구",
          "original_form": "과일청 만드는거 좋네요!!ㅋㅋㅋㅋ 보관하기도 좋구",
          "speaker_id": "2",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019788.1.19",
          "form": "아니면 친구들 하고 같은 동네 살면 좋겠어요ㅠ",
          "original_form": "아니면 친구들 하고 같은 동네 살면 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019788.1.20",
          "form": "식재료 나누고 배달음식 나눠먹게요",
          "original_form": "식재료 나누고 배달음식 나눠먹게요",
          "speaker_id": "1",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019788.1.21",
          "form": "맞아요!! 저도 맨날 그런 생각해요ㅋㅋㅋ",
          "original_form": "맞아요!! 저도 맨날 그런 생각해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019788.1.22",
          "form": "돌아가면서 요리해서 만들어먹고ㅋㅋㅋ",
          "original_form": "돌아가면서 요리해서 만들어먹고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019788.1.23",
          "form": "한빌라를 다같이 몰래 점령하자는 이야기 많이해요ㅋㅋㅋㅋ",
          "original_form": "한빌라를 다같이 몰래 점령하자는 이야기 많이해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019788.1.24",
          "form": "과일청 괜찮아요 추천드려요",
          "original_form": "과일청 괜찮아요 추천드려요",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019788.1.25",
          "form": "같은 건물에 친구들 같이 사는거 좋을거같아요ㅋㅋㅋ",
          "original_form": "같은 건물에 친구들 같이 사는거 좋을거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019788.1.26",
          "form": "다음에 과일 남으면 청 한번 만들어봐야겠네요",
          "original_form": "다음에 과일 남으면 청 한번 만들어봐야겠네요",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019788.1.27",
          "form": "감사합니달ㅋㅋㅋ",
          "original_form": "감사합니달ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019788.1.28",
          "form": "디저트는 좋아하세요?",
          "original_form": "디저트는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019788.1.29",
          "form": "네!! 완전 좋아해요",
          "original_form": "네!! 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019788.1.30",
