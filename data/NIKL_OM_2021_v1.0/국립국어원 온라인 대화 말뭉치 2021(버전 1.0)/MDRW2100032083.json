      "utterance": [
        {
          "id": "MDRW2100032083.1.1",
          "form": "연돈 돈까스 집인가요",
          "original_form": "연돈 돈까스 집인가요",
          "speaker_id": "2",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032083.1.2",
          "form": "골목식당에 나왔어요",
          "original_form": "골목식당에 나왔어요",
          "speaker_id": "1",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032083.1.3",
          "form": "혹시 가보셨나요",
          "original_form": "혹시 가보셨나요",
          "speaker_id": "2",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032083.1.4",
          "form": "아마 몇 년 됐을 건데 지금도 난리예요",
          "original_form": "아마 몇 년 됐을 건데 지금도 난리예요",
          "speaker_id": "1",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032083.1.5",
          "form": "엄청 유명하잖아요 사람들 줄서서 먹는다던데",
          "original_form": "엄청 유명하잖아요 사람들 줄서서 먹는다던데",
          "speaker_id": "2",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032083.1.6",
          "form": "전에 친구가 왔는데 새벽 3시부터 줄 잤대요",
          "original_form": "전에 친구가 왔는데 새벽 3시부터 줄 잤대요",
          "speaker_id": "1",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032083.1.7",
          "form": "아니 돈까스 맛이 그 정도인가요",
          "original_form": "아니 돈까스 맛이 그 정도인가요",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.8",
          "form": "옆에서 텐트 파는 사람도 있고",
          "original_form": "옆에서 텐트 파는 사람도 있고",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.9",
          "form": "새벽 3시에 어떻게 줄을서 있을 수가 있지요",
          "original_form": "새벽 3시에 어떻게 줄을서 있을 수가 있지요",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.10",
          "form": "휴대폰 배터리 대여하는 사람도 있대요",
          "original_form": "휴대폰 배터리 대여하는 사람도 있대요",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.11",
          "form": "그 시간에 찾아 갔더니 너무 신기하네요",
          "original_form": "그 시간에 찾아 갔더니 너무 신기하네요",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.12",
          "form": "그래야 9시에 번호표를 받을 수 있어요",
          "original_form": "그래야 9시에 번호표를 받을 수 있어요",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.13",
          "form": "부가적인 경제 활동이 많이 활발하게 이루어지고 있네요",
          "original_form": "부가적인 경제 활동이 많이 활발하게 이루어지고 있네요",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.14",
          "form": "그러게요 그렇게 몇 년 동안 운영하다가",
          "original_form": "그러게요 그렇게 몇 년 동안 운영하다가",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.15",
          "form": "지금은 앱으로 예약제로 바뀌었어요",
          "original_form": "지금은 앱으로 예약제로 바뀌었어요",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.16",
          "form": "그렇게 해서 줄서서 먹었을 때 만족도는 있었다합니까",
          "original_form": "그렇게 해서 줄서서 먹었을 때 만족도는 있었다합니까",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.17",
          "form": "맛있었다고 하네요",
          "original_form": "맛있었다고 하네요",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032083.1.18",
          "form": "그 정도 줄서서 먹을 만큼의 가치가 있을까요",
          "original_form": "그 정도 줄서서 먹을 만큼의 가치가 있을까요",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032083.1.19",
          "form": "여행자들이 앱으로 예약하려고 치열하게 경쟁하고 있는데",
          "original_form": "여행자들이 앱으로 예약하려고 치열하게 경쟁하고 있는데",
          "speaker_id": "1",
          "time": "20210905 12:13"
        }
      ]
