      "utterance": [
        {
          "id": "MDRW2100029818.1.1",
          "form": "전 방금 파운드케이크 먹었어요",
          "original_form": "전 방금 파운드케이크 먹었어요",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029818.1.2",
          "form": "점심 드셨어요??",
          "original_form": "점심 드셨어요??",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029818.1.3",
          "form": "버거요",
          "original_form": "버거요",
          "speaker_id": "1",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100029818.1.4",
          "form": "오 어떤 버거예요??",
          "original_form": "오 어떤 버거예요??",
          "speaker_id": "2",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029818.1.5",
          "form": "맥도날드요 ㅋ",
          "original_form": "맥도날드요 ㅋ",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029818.1.6",
          "form": "오 요즘 맥도날드 뭐가 맛있나요??",
          "original_form": "오 요즘 맥도날드 뭐가 맛있나요??",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029818.1.7",
          "form": "상하이여",
          "original_form": "상하이여",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029818.1.8",
          "form": "역시 상하이가 독보적이네요",
          "original_form": "역시 상하이가 독보적이네요",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100029818.1.9",
          "form": "사이드는 뭐 드세요?",
          "original_form": "사이드는 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100029818.1.10",
          "form": "맥너겟이요",
          "original_form": "맥너겟이요",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029818.1.11",
          "form": "오 맛있겠네요",
          "original_form": "오 맛있겠네요",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029818.1.12",
          "form": "매장에서 드신건가요???",
          "original_form": "매장에서 드신건가요???",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029818.1.13",
          "form": "포장이여",
          "original_form": "포장이여",
          "speaker_id": "1",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100029818.1.14",
          "form": "앗 포장이면 드시고 뒤처리 귀찮지 않으신가요?",
          "original_form": "앗 포장이면 드시고 뒤처리 귀찮지 않으신가요?",
          "speaker_id": "2",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100029818.1.15",
          "form": "그래도 혼자 안전하게요",
          "original_form": "그래도 혼자 안전하게요",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100029818.1.16",
          "form": "하긴 코시국이죠ㅠ",
          "original_form": "하긴 코시국이죠ㅠ",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100029818.1.17",
          "form": "배달 자주 시키세요?",
          "original_form": "배달 자주 시키세요?",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100029818.1.18",
          "form": "가끔 밥하기 귀찮을 때만 시켜요",
          "original_form": "가끔 밥하기 귀찮을 때만 시켜요",
          "speaker_id": "1",
          "time": "20210903 21:29"
        }
