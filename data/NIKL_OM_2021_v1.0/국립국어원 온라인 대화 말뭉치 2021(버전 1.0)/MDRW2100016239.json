        {
          "id": "MDRW2100016239.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016239.1.3",
          "form": "혹시 요즘에 관심있는 동호회같은거 있으시나요?",
          "original_form": "혹시 요즘에 관심있는 동호회같은거 있으시나요?",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016239.1.4",
          "form": "음 동호회는 따로 하고있지않아요",
          "original_form": "음 동호회는 따로 하고있지않아요",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016239.1.5",
          "form": "name1님은 있으신가요",
          "original_form": "&name1&님은 있으신가요",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016239.1.6",
          "form": "너도 하고있지는 않아요!",
          "original_form": "너도 하고있지는 않아요!",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016239.1.7",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016239.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016239.1.9",
          "form": "관심있는 동호회 분야는 있으세요?",
          "original_form": "관심있는 동호회 분야는 있으세요?",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016239.1.10",
          "form": "저 예전에 사물놀이했었어요",
          "original_form": "저 예전에 사물놀이했었어요",
          "speaker_id": "2",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016239.1.11",
          "form": "오 사물놀이 신기하네요!",
          "original_form": "오 사물놀이 신기하네요!",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016239.1.12",
          "form": "학교에서 배우신건가요?",
          "original_form": "학교에서 배우신건가요?",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016239.1.13",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016239.1.14",
          "form": "아는 분 따라서 갔다가 배우게된건데",
          "original_form": "아는 분 따라서 갔다가 배우게된건데",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016239.1.15",
          "form": "저는 거기서 징을 쳤습니다",
          "original_form": "저는 거기서 징을 쳤습니다",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016239.1.16",
          "form": "아하 그러셨군요!",
          "original_form": "아하 그러셨군요!",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016239.1.17",
          "form": "그런거 배우는 모임같은게 있었나봐요!?",
          "original_form": "그런거 배우는 모임같은게 있었나봐요!?",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016239.1.18",
          "form": "그건 아닌데 재밌는거 있다고 따라갔다가",
          "original_form": "그건 아닌데 재밌는거 있다고 따라갔다가",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016239.1.19",
          "form": "징을 잡게 되었습니다",
          "original_form": "징을 잡게 되었습니다",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016239.1.20",
          "form": "아하 그러셨균요! 재미는 있으셨나요?",
          "original_form": "아하 그러셨균요! 재미는 있으셨나요?",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016239.1.21",
          "form": "아무래도 새로운 걸 배우는다는건",
          "original_form": "아무래도 새로운 걸 배우는다는건",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016239.1.22",
          "form": "꽤 흥미있고 재밌더라구요",
          "original_form": "꽤 흥미있고 재밌더라구요",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016239.1.23",
          "form": "name1님은 관심있는 분야 있으신가요",
          "original_form": "&name1&님은 관심있는 분야 있으신가요",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016239.1.24",
          "form": "그럴거같아요!!ㅎㅎ",
          "original_form": "그럴거같아요!!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016239.1.25",
          "form": "저는 농구동호회같은건 생각이 있어요!",
          "original_form": "저는 농구동호회같은건 생각이 있어요!",
          "speaker_id": "1",
          "time": "20210826 11:29"
