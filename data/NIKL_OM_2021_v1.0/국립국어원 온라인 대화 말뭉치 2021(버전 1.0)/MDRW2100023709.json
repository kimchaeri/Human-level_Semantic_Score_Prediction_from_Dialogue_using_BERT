      "utterance": [
        {
          "id": "MDRW2100023709.1.1",
          "form": "name2 월요팅입니다~일하고 계세요~?",
          "original_form": "&name2& 월요팅입니다~일하고 계세요~?",
          "speaker_id": "1",
          "time": "20210830 15:10"
        },
        {
          "id": "MDRW2100023709.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100023709.1.3",
          "form": "아직학생이라",
          "original_form": "아직학생이라",
          "speaker_id": "2",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100023709.1.4",
          "form": "개강 기다리고있습니다",
          "original_form": "개강 기다리고있습니다",
          "speaker_id": "2",
          "time": "20210830 15:41"
        },
        {
          "id": "MDRW2100023709.1.5",
          "form": "아하!!",
          "original_form": "아하!!",
          "speaker_id": "1",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100023709.1.6",
          "form": "개강 제일 기다려지지않는 일이겠어요",
          "original_form": "개강 제일 기다려지지않는 일이겠어요",
          "speaker_id": "1",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100023709.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023709.1.8",
          "form": "벌써 시작된 친구도 있는데",
          "original_form": "벌써 시작된 친구도 있는데",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023709.1.9",
          "form": "최대한 뒤로 미루고싶네요",
          "original_form": "최대한 뒤로 미루고싶네요",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023709.1.10",
          "form": "휴학고민도 많았어요",
          "original_form": "휴학고민도 많았어요",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023709.1.11",
          "form": "아무래도 코로나가 크더라구여",
          "original_form": "아무래도 코로나가 크더라구여",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023709.1.12",
          "form": "그렇겠어요~",
          "original_form": "그렇겠어요~",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100023709.1.13",
          "form": "아직 대부분 싸강인가요?",
          "original_form": "아직 대부분 싸강인가요?",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100023709.1.14",
          "form": "네 4단계 이후로는 싸강인데",
          "original_form": "네 4단계 이후로는 싸강인데",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023709.1.15",
          "form": "실습이 있는 수업은 대면하기도하더라구요",
          "original_form": "실습이 있는 수업은 대면하기도하더라구요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023709.1.16",
          "form": "그렇군요 ㅠ",
          "original_form": "그렇군요 ㅠ",
          "speaker_id": "1",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023709.1.17",
          "form": "네 전 실습이있아서",
          "original_form": "네 전 실습이있아서",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023709.1.18",
          "form": "학교를 가야합니다",
          "original_form": "학교를 가야합니다",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023709.1.19",
          "form": "온라인 수업 경우에 따라 지루할것 같아요..",
          "original_form": "온라인 수업 경우에 따라 지루할것 같아요..",
          "speaker_id": "1",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023709.1.20",
          "form": "맞아요 질이 떨어지기도하구요",
          "original_form": "맞아요 질이 떨어지기도하구요",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023709.1.21",
          "form": "근데 직접가는건 매우 불안해서",
          "original_form": "근데 직접가는건 매우 불안해서",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023709.1.22",
          "form": "빨리 백신맞고싶네요",
          "original_form": "빨리 백신맞고싶네요",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023709.1.23",
          "form": "얼른 맞으실수 있기를 바랄게요!!",
          "original_form": "얼른 맞으실수 있기를 바랄게요!!",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023709.1.24",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023709.1.25",
          "form": "name1도 백신 맞으시나요?",
          "original_form": "&name1&도 백신 맞으시나요?",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023709.1.26",
          "form": "네 저 곧 맞아요!",
          "original_form": "네 저 곧 맞아요!",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023709.1.27",
          "form": "오 백신 부작용없으시길바라요",
          "original_form": "오 백신 부작용없으시길바라요",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023709.1.28",
          "form": "감사합니다!!",
          "original_form": "감사합니다!!",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023709.1.29",
          "form": "일때매 필수로 맞는건데",
          "original_form": "일때매 필수로 맞는건데",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023709.1.30",
          "form": "안 아프길 ㅠ",
          "original_form": "안 아프길 ㅠ",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023709.1.31",
          "form": "언제맞으세요?",
          "original_form": "언제맞으세요?",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023709.1.32",
          "form": "전 일부러 추석전에 예약했어요",
          "original_form": "전 일부러 추석전에 예약했어요",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023709.1.33",
          "form": "며칠뒤오!",
          "original_form": "며칠뒤오!",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023709.1.34",
          "form": "괜찮으시길 바라요!!",
          "original_form": "괜찮으시길 바라요!!",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023709.1.35",
          "form": "우와 며칠뒤군요",
          "original_form": "우와 며칠뒤군요",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023709.1.36",
          "form": "좀 떨리겠어요",
          "original_form": "좀 떨리겠어요",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100023709.1.37",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023709.1.38",
          "form": "맞고 좀 쉬고픈데 계속 바로 일하네요..",
          "original_form": "맞고 좀 쉬고픈데 계속 바로 일하네요..",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023709.1.39",
          "form": "헐 너무 빡세네요",
          "original_form": "헐 너무 빡세네요",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100023709.1.40",
          "form": "좀 쉬게해주지",
          "original_form": "좀 쉬게해주지",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100023709.1.41",
          "form": "일이 많이 바쁘시군요ㅜ",
          "original_form": "일이 많이 바쁘시군요ㅜ",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100023709.1.42",
          "form": "그니깐요:)",
          "original_form": "그니깐요:)",
          "speaker_id": "1",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100023709.1.43",
          "form": "곧 개강이겠어요!",
          "original_form": "곧 개강이겠어요!",
          "speaker_id": "1",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100023709.1.44",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100023709.1.45",
          "form": "내일이네요",
          "original_form": "내일이네요",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100023709.1.46",
          "form": "으아 시간이 너무 빨라요",
          "original_form": "으아 시간이 너무 빨라요",
          "speaker_id": "2",
          "time": "20210831 22:07"
        }
      ]
    }
