      "utterance": [
        {
          "id": "MDRW2100031835.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210905 02:12"
        },
        {
          "id": "MDRW2100031835.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210905 10:12"
        },
        {
          "id": "MDRW2100031835.1.3",
          "form": "여긴 하늘이 흐리네요 name2님네는 어떠신가요",
          "original_form": "여긴 하늘이 흐리네요 &name2&님네는 어떠신가요",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031835.1.4",
          "form": "여기도 많이 흐리네요ㅜ",
          "original_form": "여기도 많이 흐리네요ㅜ",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031835.1.5",
          "form": "바람도 꽤 불고 있어요",
          "original_form": "바람도 꽤 불고 있어요",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031835.1.6",
          "form": "다시보니 비가 오고 있어요...",
          "original_form": "다시보니 비가 오고 있어요...",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031835.1.7",
          "form": "아이고 저런..",
          "original_form": "아이고 저런..",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100031835.1.8",
          "form": "여긴 흐리가만 하고 바람은 안 불어요 그런데도 제법 쌀쌀하네요",
          "original_form": "여긴 흐리가만 하고 바람은 안 불어요 그런데도 제법 쌀쌀하네요",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100031835.1.9",
          "form": "벌써 여름이 다 지나간 느낌이에요~",
          "original_form": "벌써 여름이 다 지나간 느낌이에요~",
          "speaker_id": "2",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100031835.1.10",
          "form": "맞아요 이젠 선풍기 틀면 춥더라구요",
          "original_form": "맞아요 이젠 선풍기 틀면 춥더라구요",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100031835.1.11",
          "form": "낮에도 에어컨이나 선풍기보단 그냥 창문 여는게 나은것 같아요",
          "original_form": "낮에도 에어컨이나 선풍기보단 그냥 창문 여는게 나은것 같아요",
          "speaker_id": "2",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100031835.1.12",
          "form": "바람도 선선하게 부는 편이기도 해서",
          "original_form": "바람도 선선하게 부는 편이기도 해서",
          "speaker_id": "2",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100031835.1.13",
          "form": "맞아요 저도 이젠 창문만 열어요ㅋㅋ",
          "original_form": "맞아요 저도 이젠 창문만 열어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:55"
        },
        {
          "id": "MDRW2100031835.1.14",
          "form": "그마저도 밤엔 추워지는 걍우도 있고 말이죠",
          "original_form": "그마저도 밤엔 추워지는 걍우도 있고 말이죠",
          "speaker_id": "1",
          "time": "20210905 11:56"
        },
        {
          "id": "MDRW2100031835.1.15",
          "form": "이제 아침저녁으로는 기온이 많이 떨어진 게 느껴지더라구요 ㅋㅋ",
          "original_form": "이제 아침저녁으로는 기온이 많이 떨어진 게 느껴지더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:34"
        },
        {
          "id": "MDRW2100031835.1.16",
          "form": "맞아요ㅋㅋ 지금 기온이 딱인 거 같아요 너무 덥지도 춥지도 않으니깐요",
          "original_form": "맞아요ㅋㅋ 지금 기온이 딱인 거 같아요 너무 덥지도 춥지도 않으니깐요",
          "speaker_id": "1",
          "time": "20210905 13:11"
        },
        {
          "id": "MDRW2100031835.1.17",
          "form": "그쵸 ㅎㅎ 일년 내내 이런 날만 있었으면 좋겠네요 ㅎㅎ",
          "original_form": "그쵸 ㅎㅎ 일년 내내 이런 날만 있었으면 좋겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:26"
        },
        {
          "id": "MDRW2100031835.1.18",
          "form": "마자요 곧 있으면 이런 날씨를 그리워하겠죠..ㅠ 덜덜 떨면서..",
          "original_form": "마자요 곧 있으면 이런 날씨를 그리워하겠죠..ㅠ 덜덜 떨면서..",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100031835.1.19",
          "form": "기온도 떨어지기 시작했으니 금방 추워질텐데 겨울이 늦게 왔으면 좋겠어요..ㅜ",
          "original_form": "기온도 떨어지기 시작했으니 금방 추워질텐데 겨울이 늦게 왔으면 좋겠어요..ㅜ",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100031835.1.20",
          "form": "아무리 껴입어도 추운 것...ㅜㅜ",
          "original_form": "아무리 껴입어도 추운 것...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100031835.1.21",
          "form": "맞아요ㅠㅠ 전 추위 많이 타는 편이라 두렵네요 작년에는 나갈 일 없어서 집에서 담요두르고 살았지만 이젠 밖에 나갈 일이 많아서..",
          "original_form": "맞아요ㅠㅠ 전 추위 많이 타는 편이라 두렵네요 작년에는 나갈 일 없어서 집에서 담요두르고 살았지만 이젠 밖에 나갈 일이 많아서..",
          "speaker_id": "1",
          "time": "20210905 14:04"
        }
      ]
    }
