      "utterance": [
        {
          "id": "MDRW2100019300.1.1",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019300.1.2",
          "form": "건강하게 너희랑 놀러도 가고",
          "original_form": "건강하게 너희랑 놀러도 가고",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019300.1.3",
          "form": "맛있는 것도 먹어야지...ㅠㅠ",
          "original_form": "맛있는 것도 먹어야지...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019300.1.4",
          "form": "그러자!!",
          "original_form": "그러자!!",
          "speaker_id": "2",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019300.1.5",
          "form": "우리가게 조지러 가자ㅏㅏ!!!!!",
          "original_form": "우리가게 조지러 가자ㅏㅏ!!!!!",
          "speaker_id": "2",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019300.1.6",
          "form": "으아ㅏㅏㄱ",
          "original_form": "으아ㅏㅏㄱ",
          "speaker_id": "2",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019300.1.7",
          "form": "피자도 먹고 다른 안주도 먹고",
          "original_form": "피자도 먹고 다른 안주도 먹고",
          "speaker_id": "2",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019300.1.8",
          "form": "술도 마시고!",
          "original_form": "술도 마시고!",
          "speaker_id": "2",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019300.1.9",
          "form": "응 조와..ㅠ",
          "original_form": "응 조와..ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019300.1.10",
          "form": "나도 살 빼야지...",
          "original_form": "나도 살 빼야지...",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019300.1.11",
          "form": "빠지고 있다며...",
          "original_form": "빠지고 있다며...",
          "speaker_id": "1",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019300.1.12",
          "form": "단지 굶어서..?",
          "original_form": "단지 굶어서..?",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019300.1.13",
          "form": "밥먹는거 귀찮아서",
          "original_form": "밥먹는거 귀찮아서",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019300.1.14",
          "form": "안먹다가 빠진거지...",
          "original_form": "안먹다가 빠진거지...",
          "speaker_id": "2",
          "time": "20210827 17:06"
        },
        {
          "id": "MDRW2100019300.1.15",
          "form": "안 먹다가 먹으면 소화도 잘 안 되지 않어?ㅠㅠ",
          "original_form": "안 먹다가 먹으면 소화도 잘 안 되지 않어?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100019300.1.16",
          "form": "약간...?",
          "original_form": "약간...?",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100019300.1.17",
          "form": "아이고ㅠㅜ",
          "original_form": "아이고ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019300.1.18",
          "form": "그래도 한 끼 씩은 꼭 챙겨 먹어!",
          "original_form": "그래도 한 끼 씩은 꼭 챙겨 먹어!",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019300.1.19",
          "form": "입맛 없으면 맛있는 거라도 먹어!",
          "original_form": "입맛 없으면 맛있는 거라도 먹어!",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019300.1.20",
          "form": "한끼는 꼭 먹지!",
          "original_form": "한끼는 꼭 먹지!",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019300.1.21",
          "form": "한끼는 먹어!",
          "original_form": "한끼는 먹어!",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019300.1.22",
          "form": "히히히",
          "original_form": "히히히",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019300.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019300.1.24",
          "form": "그래 잘했어!",
          "original_form": "그래 잘했어!",
          "speaker_id": "1",
          "time": "20210827 17:14"
        },
