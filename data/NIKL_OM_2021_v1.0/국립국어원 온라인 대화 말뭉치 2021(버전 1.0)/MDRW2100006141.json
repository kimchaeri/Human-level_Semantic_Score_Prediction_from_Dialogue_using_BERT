        },
        {
          "id": "MDRW2100006141.1.2",
          "form": "네 낮잠 자야 ㅎㅎ",
          "original_form": "네 낮잠 자야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100006141.1.3",
          "form": "ㅎㅎㅎ한산한가요지금",
          "original_form": "ㅎㅎㅎ한산한가요지금",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006141.1.4",
          "form": "네 막차 전까지는",
          "original_form": "네 막차 전까지는",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006141.1.5",
          "form": "괜찮아요 ㅎㅎ",
          "original_form": "괜찮아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006141.1.6",
          "form": "병원은요?",
          "original_form": "병원은요?",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006141.1.7",
          "form": "음..나름괜차나용",
          "original_form": "음..나름괜차나용",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006141.1.8",
          "form": "새벽에는 안바쁜가요",
          "original_form": "새벽에는 안바쁜가요",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006141.1.9",
          "form": "케바케에요",
          "original_form": "케바케에요",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006141.1.10",
          "form": "오늘무사하길ㅎㅎ",
          "original_form": "오늘무사하길ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006141.1.11",
          "form": "무사하길 빕니다!",
          "original_form": "무사하길 빕니다!",
