        },
        {
          "id": "MDRW2100044901.1.2",
          "form": "제티비씨야 정신차려",
          "original_form": "제티비씨야 정신차려",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.3",
          "form": "일을 심각하게 못한건 맞지만",
          "original_form": "일을 심각하게 못한건 맞지만",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.4",
          "form": "저거 또 국제소송 걸면",
          "original_form": "저거 또 국제소송 걸면",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.5",
          "form": "엄청 힘들고 오래걸려서",
          "original_form": "엄청 힘들고 오래걸려서",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.6",
          "form": "그치....",
          "original_form": "그치....",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.7",
          "form": "걸기도 좀 그런거같더라고여",
          "original_form": "걸기도 좀 그런거같더라고여",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.8",
          "form": "그래도 걸긴 해야지 ㅠㅠㅠ",
          "original_form": "그래도 걸긴 해야지 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.9",
          "form": "멍청이들아 ㅠㅠㅠ",
          "original_form": "멍청이들아 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.10",
          "form": "어휴진짜",
          "original_form": "어휴진짜",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.11",
          "form": "아니근데",
          "original_form": "아니근데",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.12",
          "form": "너무했다 진짜",
          "original_form": "너무했다 진짜",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.13",
          "form": "정신나갔나 진짜",
          "original_form": "정신나갔나 진짜",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.14",
          "form": "정말 팬텀싱어 만들어준거밖에 잘한게 없는 회사",
          "original_form": "정말 팬텀싱어 만들어준거밖에 잘한게 없는 회사",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044901.1.15",
          "form": "아니 그냥 베낀것도 아니고",
          "original_form": "아니 그냥 베낀것도 아니고",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044901.1.16",
          "form": "기술전수 다 받아놓고",
          "original_form": "기술전수 다 받아놓고",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044901.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ기술을",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ기술을",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044901.1.18",
          "form": "통수를 친다는게",
          "original_form": "통수를 친다는게",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044901.1.19",
          "form": "그렇게 가지고 도망가다니",
          "original_form": "그렇게 가지고 도망가다니",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044901.1.20",
          "form": "어디 듣보 방송사도 아니고",
          "original_form": "어디 듣보 방송사도 아니고",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044901.1.21",
          "form": "후난티비가ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "후난티비가ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044901.1.22",
          "form": "와우 얼마나 멍청하게...",
          "original_form": "와우 얼마나 멍청하게...",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044901.1.23",
          "form": "진짜 모든 나라가",
          "original_form": "진짜 모든 나라가",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044901.1.24",
