      "utterance": [
        {
          "id": "MDRW2100027268.1.1",
          "form": "오 합창부 !",
          "original_form": "오 합창부 !",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027268.1.2",
          "form": "철봉에 매달리기 하고 귀가하던 루틴이 있어서",
          "original_form": "철봉에 매달리기 하고 귀가하던 루틴이 있어서",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027268.1.3",
          "form": "넹넹 이상하게 그게 제 습관이었어요",
          "original_form": "넹넹 이상하게 그게 제 습관이었어요",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027268.1.4",
          "form": "그걸 3년 하니 팔힘은 최상위급입니다ㅎㅎ",
          "original_form": "그걸 3년 하니 팔힘은 최상위급입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027268.1.5",
          "form": "사람들이 저랑 팔씨름하면 다 놀라더라구용ㅎ",
          "original_form": "사람들이 저랑 팔씨름하면 다 놀라더라구용ㅎ",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027268.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027268.1.7",
          "form": "팔씨름 대장이셧네요",
          "original_form": "팔씨름 대장이셧네요",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027268.1.8",
          "form": "근데 오직 매달리기만 가능ㅎ 돌기 안돼요",
          "original_form": "근데 오직 매달리기만 가능ㅎ 돌기 안돼요",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027268.1.9",
          "form": "한번 잡아보고싶네요",
          "original_form": "한번 잡아보고싶네요",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027268.1.10",
          "form": "매달리기 얼마나 하심?",
          "original_form": "매달리기 얼마나 하심?",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027268.1.11",
          "form": "대학생 때까지 그랬죠ㅎ",
          "original_form": "대학생 때까지 그랬죠ㅎ",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027268.1.12",
          "form": "철봉 체력장은 매년 만점이었어요!",
          "original_form": "철봉 체력장은 매년 만점이었어요!",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027268.1.13",
          "form": "아닠 잠깐만요",
          "original_form": "아닠 잠깐만요",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027268.1.14",
          "form": "자아, 오늘 점심은 뭐 드실건가요ㅋㅋ",
          "original_form": "자아, 오늘 점심은 뭐 드실건가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027268.1.15",
          "form": "아 식음료",
          "original_form": "아 식음료",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027268.1.16",
          "form": "음 오늘은 물회를 먹어보겠습니다",
          "original_form": "음 오늘은 물회를 먹어보겠습니다",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027268.1.17",
          "form": "건강 찾다 노란거보고 깜짝",
          "original_form": "건강 찾다 노란거보고 깜짝",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027268.1.18",
          "form": "헉! JMT!!!",
          "original_form": "헉! JMT!!!",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027268.1.19",
          "form": "창원에 귀산동이라고 있거든요 거기 물회맛집이 있습니다",
          "original_form": "창원에 귀산동이라고 있거든요 거기 물회맛집이 있습니다",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027268.1.20",
          "form": "저도 회 먹을 거예요!",
          "original_form": "저도 회 먹을 거예요!",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027268.1.21",
          "form": "선배님들 모시고 가려구요",
          "original_form": "선배님들 모시고 가려구요",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027268.1.22",
          "form": "오오 뭐뭐 들어가나요",
          "original_form": "오오 뭐뭐 들어가나요",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027268.1.23",
          "form": "저도 잘 모릅니다 ㅋㅋ",
          "original_form": "저도 잘 모릅니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027268.1.24",
          "form": "선배들 추천이라",
          "original_form": "선배들 추천이라",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027268.1.25",
          "form": "갔다와서",
          "original_form": "갔다와서",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027268.1.26",
          "form": "말씀드릴게요",
          "original_form": "말씀드릴게요",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027268.1.27",
          "form": "님처럼 스패셜은 아니고 그냥 일반 물회일겁니다",
          "original_form": "님처럼 스패셜은 아니고 그냥 일반 물회일겁니다",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027268.1.28",
          "form": "다금바리 노노",
          "original_form": "다금바리 노노",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027268.1.29",
          "form": "ㅎㅎ아닠 다금바리 들어가면 가격 얼마인가요ㅎ",
          "original_form": "ㅎㅎ아닠 다금바리 들어가면 가격 얼마인가요ㅎ",
          "speaker_id": "1",
          "time": "20210902 11:06"
        }
      ]
    }
