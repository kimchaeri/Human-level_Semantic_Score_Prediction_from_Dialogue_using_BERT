      "utterance": [
        {
          "id": "MDRW2100039635.1.1",
          "form": "더많이 놀고싶어요",
          "original_form": "더많이 놀고싶어요",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039635.1.2",
          "form": "아이랑요??",
          "original_form": "아이랑요??",
          "speaker_id": "1",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039635.1.3",
          "form": "3학년만 되도 주말에 엄마아빠랑 놀려고 안하나요??",
          "original_form": "3학년만 되도 주말에 엄마아빠랑 놀려고 안하나요??",
          "speaker_id": "1",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039635.1.4",
          "form": "아니요..그렇지는않아요",
          "original_form": "아니요..그렇지는않아요",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039635.1.5",
          "form": "부모가 하기 나름인듯해요",
          "original_form": "부모가 하기 나름인듯해요",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039635.1.6",
          "form": "그럼 전 지금 더 많이 예뻐해주고 같이 많이 놀아주면 될까요?",
          "original_form": "그럼 전 지금 더 많이 예뻐해주고 같이 많이 놀아주면 될까요?",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039635.1.7",
          "form": "네..많이 놀아주면 지금도 같이 잘다녀요",
          "original_form": "네..많이 놀아주면 지금도 같이 잘다녀요",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100039635.1.8",
          "form": "많이 놀아주셨군요. 뭐 하고 놀아줘야 할까요??",
          "original_form": "많이 놀아주셨군요. 뭐 하고 놀아줘야 할까요??",
          "speaker_id": "1",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100039635.1.9",
          "form": "캠핑다니고 여행다니고..집앞에서도 놀고..운동장도 가고했어요",
          "original_form": "캠핑다니고 여행다니고..집앞에서도 놀고..운동장도 가고했어요",
          "speaker_id": "2",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100039635.1.10",
          "form": "저 이번에 텐트 샀는데 가까운 공원이라도 가서 텐트 치고 놀아줘야겠네요",
          "original_form": "저 이번에 텐트 샀는데 가까운 공원이라도 가서 텐트 치고 놀아줘야겠네요",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039635.1.11",
          "form": "이 시기에 공부는 아직인가요?",
          "original_form": "이 시기에 공부는 아직인가요?",
          "speaker_id": "1",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100039635.1.12",
          "form": "그래도 학교가야하니 안시킬수는 없죠",
          "original_form": "그래도 학교가야하니 안시킬수는 없죠",
          "speaker_id": "2",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100039635.1.13",
          "form": "뭐 정도 시키면 좋나요??",
          "original_form": "뭐 정도 시키면 좋나요??",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039635.1.14",
          "form": "저대로 학교 보내면 대역죄인이 될것 같아요 ㅠ",
          "original_form": "저대로 학교 보내면 대역죄인이 될것 같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039635.1.15",
          "form": "7살이라고하셨죠?",
          "original_form": "7살이라고하셨죠?",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039635.1.16",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039635.1.17",
          "form": "둘째는 5살이라 그냥 놀고 있고요",
          "original_form": "둘째는 5살이라 그냥 놀고 있고요",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039635.1.18",
          "form": "한글정도는..",
          "original_form": "한글정도는..",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039635.1.19",
          "form": "유창하게 읽을정도요?",
          "original_form": "유창하게 읽을정도요?",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039635.1.20",
          "form": "아님 유창하게 쓸정도??",
          "original_form": "아님 유창하게 쓸정도??",
          "speaker_id": "1",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039635.1.21",
          "form": "아니요..어느정도는 다알고 들어오더군요",
          "original_form": "아니요..어느정도는 다알고 들어오더군요",
