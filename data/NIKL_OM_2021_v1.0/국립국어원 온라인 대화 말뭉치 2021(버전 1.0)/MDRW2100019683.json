      "utterance": [
        {
          "id": "MDRW2100019683.1.1",
          "form": "미국인도 사망했다고하네",
          "original_form": "미국인도 사망했다고하네",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019683.1.2",
          "form": "진짜 전쟁 날지도 모르겠어요",
          "original_form": "진짜 전쟁 날지도 모르겠어요",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019683.1.3",
          "form": "보복한다고 하니까 어떻게 될지 모르겠네",
          "original_form": "보복한다고 하니까 어떻게 될지 모르겠네",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019683.1.4",
          "form": "아프간 입소자 얘기도 나오고 있어요",
          "original_form": "아프간 입소자 얘기도 나오고 있어요",
          "speaker_id": "1",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019683.1.5",
          "form": "어린애들이 반이더라",
          "original_form": "어린애들이 반이더라",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019683.1.6",
          "form": "걔들이 무슨 죄가 있겠어요 어른들이 문제지",
          "original_form": "걔들이 무슨 죄가 있겠어요 어른들이 문제지",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019683.1.7",
          "form": "애들만보면불쌍하지",
          "original_form": "애들만보면불쌍하지",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019683.1.8",
          "form": "뉴스보니까 좀 짠하기도 하고 그러네요",
          "original_form": "뉴스보니까 좀 짠하기도 하고 그러네요",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019683.1.9",
          "form": "코로나확진자 천팔백명이래",
          "original_form": "코로나확진자 천팔백명이래",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019683.1.10",
          "form": "진짜 많네요",
          "original_form": "진짜 많네요",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019683.1.11",
          "form": "추석때가 앞으로 길어서 안좋아",
          "original_form": "추석때가 앞으로 길어서 안좋아",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019683.1.12",
          "form": "뉴스보니까 더 늘거 같긴 해요",
          "original_form": "뉴스보니까 더 늘거 같긴 해요",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019683.1.13",
          "form": "델타변이가",
          "original_form": "델타변이가",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019683.1.14",
          "form": "추석에 어떻게 될지 모르니까 조심해야겠어요",
          "original_form": "추석에 어떻게 될지 모르니까 조심해야겠어요",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019683.1.15",
          "form": "내일 주사맞으시니까 잘 쉬세요",
          "original_form": "내일 주사맞으시니까 잘 쉬세요",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019683.1.16",
          "form": "응 맞으러가야지",
          "original_form": "응 맞으러가야지",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019683.1.17",
          "form": "요즘 허재가 방송에 많이 나와",
          "original_form": "요즘 허재가 방송에 많이 나와",
