      "utterance": [
        {
          "id": "MDRW2100037451.1.1",
          "form": "엉 오늘부티  새직장으로 출근",
          "original_form": "엉 오늘부티  새직장으로 출근",
          "speaker_id": "2",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100037451.1.2",
          "form": "직장도 참 잘 구한다",
          "original_form": "직장도 참 잘 구한다",
          "speaker_id": "1",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100037451.1.3",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100037451.1.4",
          "form": "이번에도 같은 직종으로 구한거지",
          "original_form": "이번에도 같은 직종으로 구한거지",
          "speaker_id": "1",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100037451.1.5",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100037451.1.6",
          "form": "그래도 기술직이라서 금방 구하네",
          "original_form": "그래도 기술직이라서 금방 구하네",
          "speaker_id": "1",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100037451.1.7",
          "form": "다행이지뭐",
          "original_form": "다행이지뭐",
          "speaker_id": "2",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100037451.1.8",
          "form": "그러게 코르나때문에 경기도 안좋은데 다행이네",
          "original_form": "그러게 코르나때문에 경기도 안좋은데 다행이네",
          "speaker_id": "1",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100037451.1.9",
          "form": "오늘 출근해서 잘 할지 모르겠다",
          "original_form": "오늘 출근해서 잘 할지 모르겠다",
          "speaker_id": "2",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100037451.1.10",
          "form": "왜 잘 하겠지 나이도 있는데",
          "original_form": "왜 잘 하겠지 나이도 있는데",
          "speaker_id": "1",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100037451.1.11",
          "form": "글쎄다",
          "original_form": "글쎄다",
          "speaker_id": "2",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100037451.1.12",
          "form": "새직장이니 이제 마음 든든하게 먹고 잘 할꺼야",
          "original_form": "새직장이니 이제 마음 든든하게 먹고 잘 할꺼야",
          "speaker_id": "1",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100037451.1.13",
          "form": "그랬으면 좋겠다",
          "original_form": "그랬으면 좋겠다",
