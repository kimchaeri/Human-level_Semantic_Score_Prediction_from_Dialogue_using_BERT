      "utterance": [
        {
          "id": "MDRW2100025497.1.1",
          "form": "와이드는 막",
          "original_form": "와이드는 막",
          "speaker_id": "2",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100025497.1.2",
          "form": "펄럭일까봐 걱정됨",
          "original_form": "펄럭일까봐 걱정됨",
          "speaker_id": "2",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100025497.1.3",
          "form": "청바지는 그래도",
          "original_form": "청바지는 그래도",
          "speaker_id": "1",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100025497.1.4",
          "form": "펄럭펄럭하는게 덜하지않나?",
          "original_form": "펄럭펄럭하는게 덜하지않나?",
          "speaker_id": "1",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025497.1.5",
          "form": "그러긴 하겠네?",
          "original_form": "그러긴 하겠네?",
          "speaker_id": "2",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025497.1.6",
          "form": "내가 휴가때",
          "original_form": "내가 휴가때",
          "speaker_id": "2",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025497.1.7",
          "form": "좀 넉넉한 슬랙스를 샀는데",
          "original_form": "좀 넉넉한 슬랙스를 샀는데",
          "speaker_id": "2",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025497.1.8",
          "form": "펄럭펄럭해?",
          "original_form": "펄럭펄럭해?",
          "speaker_id": "1",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025497.1.9",
          "form": "막 다리에 휘감겨가지고",
          "original_form": "막 다리에 휘감겨가지고",
          "speaker_id": "2",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025497.1.10",
          "form": "이상해서 환불했어",
          "original_form": "이상해서 환불했어",
          "speaker_id": "2",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025497.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025497.1.12",
          "form": "남자는 완전 와이드까진 아니고",
          "original_form": "남자는 완전 와이드까진 아니고",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025497.1.13",
          "form": "적당한 일자핏까지를 많이입는거같기도",
          "original_form": "적당한 일자핏까지를 많이입는거같기도",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025497.1.14",
          "form": "여자랑 패션흐름이 다른 것 같아",
          "original_form": "여자랑 패션흐름이 다른 것 같아",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025497.1.15",
          "form": "맞아 ㅋㅋㅋㅋ",
          "original_form": "맞아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025497.1.16",
          "form": "여자가 옷종류도",
          "original_form": "여자가 옷종류도",
          "speaker_id": "2",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025497.1.17",
          "form": "훨씬 많은거같아",
          "original_form": "훨씬 많은거같아",
          "speaker_id": "2",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025497.1.18",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025497.1.19",
          "form": "남자건 옷 종류는",
          "original_form": "남자건 옷 종류는",
          "speaker_id": "2",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025497.1.20",
          "form": "남자는 플라워패턴 원피스 이런게 없잖아",
          "original_form": "남자는 플라워패턴 원피스 이런게 없잖아",
          "speaker_id": "1",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025497.1.21",
          "form": "몇개 없음",
          "original_form": "몇개 없음",
          "speaker_id": "2",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025497.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025497.1.23",
          "form": "입어봤자",
          "original_form": "입어봤자",
          "speaker_id": "1",
          "time": "20210901 12:16"
        },
        {
