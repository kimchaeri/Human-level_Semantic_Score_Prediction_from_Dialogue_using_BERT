          "original_form": "...?",
          "speaker_id": "2",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100025497.1.51",
          "form": "(감 안옴)",
          "original_form": "(감 안옴)",
          "speaker_id": "2",
          "time": "20210901 12:20"
        },
        {
          "id": "MDRW2100025497.1.52",
          "form": "답답",
          "original_form": "답답",
          "speaker_id": "1",
          "time": "20210901 12:20"
        },
        {
          "id": "MDRW2100025497.1.53",
          "form": "암튼 난 조거팬츠하면 그오빠가 생각나",
          "original_form": "암튼 난 조거팬츠하면 그오빠가 생각나",
          "speaker_id": "1",
          "time": "20210901 12:20"
        },
        {
          "id": "MDRW2100025497.1.54",
          "form": "아임 고구마",
          "original_form": "아임 고구마",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025497.1.55",
          "form": "너무 편하게 맨날입고다녔어",
          "original_form": "너무 편하게 맨날입고다녔어",
          "speaker_id": "1",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025497.1.56",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025497.1.57",
          "form": "나 이번에",
          "original_form": "나 이번에",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025497.1.58",
          "form": "8월 쿠폰으로",
          "original_form": "8월 쿠폰으로",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025497.1.59",
          "form": "슬랙스샀어",
          "original_form": "슬랙스샀어",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025497.1.60",
          "form": "청바지를 탈피해보겠어",
          "original_form": "청바지를 탈피해보겠어",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025497.1.61",
          "form": "슬랙스도 종류가",
          "original_form": "슬랙스도 종류가",
