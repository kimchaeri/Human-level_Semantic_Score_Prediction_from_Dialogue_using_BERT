      "utterance": [
        {
          "id": "MDRW2100017824.1.1",
          "form": "name2은 연애 중이신가요?",
          "original_form": "&name2&은 연애 중이신가요?",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017824.1.2",
          "form": "결혼햇어요 ㅎ 님은요?",
          "original_form": "결혼햇어요 ㅎ 님은요?",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017824.1.3",
          "form": "전 미혼이요ㅎㅎ",
          "original_form": "전 미혼이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100017824.1.4",
          "form": "어떻게 결혼하셨는지 여쭤봐도 될까영?",
          "original_form": "어떻게 결혼하셨는지 여쭤봐도 될까영?",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100017824.1.5",
          "form": "어떻게가 어떤의미일까용 ~",
          "original_form": "어떻게가 어떤의미일까용 ~",
          "speaker_id": "2",
          "time": "20210827 10:20"
        },
        {
          "id": "MDRW2100017824.1.6",
          "form": "앗 말이 너무 두루뭉술했나오ㅎㅎ",
          "original_form": "앗 말이 너무 두루뭉술했나오ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100017824.1.7",
          "form": "주변에 연애결혼인 사람두 있고 소개팅인 사람도 있구 다양하더라구요ㅎㅎ",
          "original_form": "주변에 연애결혼인 사람두 있고 소개팅인 사람도 있구 다양하더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100017824.1.8",
          "form": "아아ㅋㅋㅋ 소개팅해서 연애하다가",
          "original_form": "아아ㅋㅋㅋ 소개팅해서 연애하다가",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100017824.1.9",
          "form": "연애2년만에 결혼이용 ~",
          "original_form": "연애2년만에 결혼이용 ~",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100017824.1.10",
          "form": "우왕 저는 결혼하지 않아서 모르는데",
          "original_form": "우왕 저는 결혼하지 않아서 모르는데",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100017824.1.11",
          "form": "저절루 결혼 얘기가 나오는건가요?",
          "original_form": "저절루 결혼 얘기가 나오는건가요?",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100017824.1.12",
          "form": "확실히 확신이있으니깐",
          "original_form": "확실히 확신이있으니깐",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100017824.1.13",
          "form": "자연스럽게 얘기가 나오고",
          "original_form": "자연스럽게 얘기가 나오고",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100017824.1.14",
          "form": "말하게되더라구요 부모님께 ㅎㅎ",
          "original_form": "말하게되더라구요 부모님께 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100017824.1.15",
          "form": "오왕 그렇구나",
          "original_form": "오왕 그렇구나",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100017824.1.16",
          "form": "남편 분이랑 이상형이 매치가 잘 되시나요?ㅎㅎ",
