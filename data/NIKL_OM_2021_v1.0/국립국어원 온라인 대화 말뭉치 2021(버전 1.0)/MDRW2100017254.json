      "utterance": [
        {
          "id": "MDRW2100017254.1.1",
          "form": "3층강아지때문레 스트레스",
          "original_form": "3층강아지때문레 스트레스",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017254.1.2",
          "form": "강아지키워?",
          "original_form": "강아지키워?",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017254.1.3",
          "form": "3층강아지 키우는데",
          "original_form": "3층강아지 키우는데",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017254.1.4",
          "form": "자꾸 풀어놔 ㅜ",
          "original_form": "자꾸 풀어놔 ㅜ",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017254.1.5",
          "form": "우리옆집은 고냥이",
          "original_form": "우리옆집은 고냥이",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017254.1.6",
          "form": "집밖에 나갈수있게?",
          "original_form": "집밖에 나갈수있게?",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017254.1.7",
          "form": "우린 통로식이잖아",
          "original_form": "우린 통로식이잖아",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017254.1.8",
          "form": "거기에 풀어둠 ㅎ",
          "original_form": "거기에 풀어둠 ㅎ",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017254.1.9",
          "form": "아 다니기 불편하겠다",
          "original_form": "아 다니기 불편하겠다",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017254.1.10",
          "form": "어어 방금도 막 친한척함 ㅜ",
          "original_form": "어어 방금도 막 친한척함 ㅜ",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017254.1.11",
          "form": "난 강아지는 무섭더라",
          "original_form": "난 강아지는 무섭더라",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017254.1.12",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017254.1.13",
          "form": "나도 무서움 ㅜ 좀 너무 달려들어 ㅋㅋ",
          "original_form": "나도 무서움 ㅜ 좀 너무 달려들어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017254.1.14",
          "form": "강아지들 막 냄새맡고 헥헥거리는거",
          "original_form": "강아지들 막 냄새맡고 헥헥거리는거",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017254.1.15",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017254.1.16",
          "form": "나오늘 고양이 봤는데",
          "original_form": "나오늘 고양이 봤는데",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017254.1.17",
          "form": "널부러져있어서",
          "original_form": "널부러져있어서",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017254.1.18",
          "form": "죽은줄",
          "original_form": "죽은줄",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017254.1.19",
          "form": "더운가 ㅋㅋㅋㅋㅋ 요즘 애들 말라잇더라",
          "original_form": "더운가 ㅋㅋㅋㅋㅋ 요즘 애들 말라잇더라",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017254.1.20",
          "form": "깜짝놀래서다시봤더니 고양이 세수하더라",
          "original_form": "깜짝놀래서다시봤더니 고양이 세수하더라",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017254.1.21",
          "form": "우리옆집 고양이 두바리 키운댘ㅋ",
          "original_form": "우리옆집 고양이 두바리 키운댘ㅋ",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017254.1.22",
          "form": "혹시 고양이간식 줘도 될까ㅋㅋ",
