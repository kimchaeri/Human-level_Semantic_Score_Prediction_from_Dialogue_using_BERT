      "utterance": [
        {
          "id": "MDRW2100044842.1.1",
          "form": "여러맛 시켜서 나눠먹는게 최고죠ㅠㅠㅠ 무슨뮤슨맛 있어요??",
          "original_form": "여러맛 시켜서 나눠먹는게 최고죠ㅠㅠㅠ 무슨뮤슨맛 있어요??",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044842.1.2",
          "form": "바사칸현미랑 간장 이랑 양념 이렇게",
          "original_form": "바사칸현미랑 간장 이랑 양념 이렇게",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044842.1.3",
          "form": "이럼 삼만원대 나와욪ㅋㅋ",
          "original_form": "이럼 삼만원대 나와욪ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044842.1.4",
          "form": "헐 완전 저렴한디요?!?!?!",
          "original_form": "헐 완전 저렴한디요?!?!?!",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044842.1.5",
          "form": "가격 대박!! ㅋㅋㅋ",
          "original_form": "가격 대박!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044842.1.6",
          "form": "가성비로 먹고 들어갑니다ㅋㅋ",
          "original_form": "가성비로 먹고 들어갑니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044842.1.7",
          "form": "바사칸현미는 이름에서부터 맛있을것같아요",
          "original_form": "바사칸현미는 이름에서부터 맛있을것같아요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044842.1.8",
          "form": "후라이드로 그거시켱ㆍ",
          "original_form": "후라이드로 그거시켱ㆍ",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044842.1.9",
          "form": "허니맛도 있는데 그건 담주에!!",
          "original_form": "허니맛도 있는데 그건 담주에!!",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044842.1.10",
          "form": "오 ㅋㅋ 아 저희동네에는 치킨마루없네요ㅠㅠㅠ힝",
          "original_form": "오 ㅋㅋ 아 저희동네에는 치킨마루없네요ㅠㅠㅠ힝",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044842.1.11",
          "form": "아ㅜㅜ",
          "original_form": "아ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044842.1.12",
          "form": "저흰 2개있는데!!",
          "original_form": "저흰 2개있는데!!",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044842.1.13",
          "form": "비슷하게 부어치킨도 없나요?",
          "original_form": "비슷하게 부어치킨도 없나요?",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044842.1.14",
          "form": "허얼 ㅠㅠㅠㅠ칰마세권 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "허얼 ㅠㅠㅠㅠ칰마세권 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044842.1.15",
          "form": "오 검색해볼게요",
          "original_form": "오 검색해볼게요",
