      "utterance": [
        {
          "id": "MDRW2100044693.1.1",
          "form": "해산물 잘드세요?",
          "original_form": "해산물 잘드세요?",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044693.1.2",
          "form": "네 해산물 엄청 좋아해요",
          "original_form": "네 해산물 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044693.1.3",
          "form": "쿠크다스님은요?",
          "original_form": "쿠크다스님은요?",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044693.1.4",
          "form": "앗 저는 날거 비링거 못먹어욬ㅋㅋㅋ",
          "original_form": "앗 저는 날거 비링거 못먹어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044693.1.5",
          "form": "육류랑 해산물중에 머가 더 조으세요",
          "original_form": "육류랑 해산물중에 머가 더 조으세요",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044693.1.6",
          "form": "육류도 너무 좋아하지만 해산물을 더 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "육류도 너무 좋아하지만 해산물을 더 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044693.1.7",
          "form": "저는 날거 비린거 구운거 삶은거 안가려요....ㅋㅋㅋㅋㅋ",
          "original_form": "저는 날거 비린거 구운거 삶은거 안가려요....ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044693.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ다잘드시는군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ다잘드시는군요",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044693.1.9",
          "form": "저도 다 잘먹고싳어요 ㅠㅠ",
          "original_form": "저도 다 잘먹고싳어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044693.1.10",
          "form": "잘먹으면 맛잇는거 더 많이머글수잇슬텐데..",
          "original_form": "잘먹으면 맛잇는거 더 많이머글수잇슬텐데..",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044693.1.11",
          "form": "너무 잘먹어서 괴로워욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 잘먹어서 괴로워욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044693.1.12",
          "form": "조금만 방심하면 무럭무럭 살찜 ㅠㅠ",
          "original_form": "조금만 방심하면 무럭무럭 살찜 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044693.1.13",
          "form": "저두요...",
          "original_form": "저두요...",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044693.1.14",
          "form": "다이어트생각해서 저칼로리 뭐 드시능거잇스에여?",
          "original_form": "다이어트생각해서 저칼로리 뭐 드시능거잇스에여?",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044693.1.15",
          "form": "한끼는 샐러드먹어요",
          "original_form": "한끼는 샐러드먹어요",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044693.1.16",
          "form": "헉 매일 그렇게 드세요?",
          "original_form": "헉 매일 그렇게 드세요?",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044693.1.17",
          "form": "매일매일은 아니지만",
          "original_form": "매일매일은 아니지만",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044693.1.18",
          "form": "일주일에 5일정도는 하루에 한끼는 샐러드먹어요",
          "original_form": "일주일에 5일정도는 하루에 한끼는 샐러드먹어요",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044693.1.19",
          "form": "자기관리 철저한분이시군요..",
          "original_form": "자기관리 철저한분이시군요..",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044693.1.20",
          "form": "나머지 두끼를 거하게 먹거든욬ㅋㅋㅋㅋㅋ",
          "original_form": "나머지 두끼를 거하게 먹거든욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044693.1.21",
          "form": "갑자기 멀어진느낌..",
          "original_form": "갑자기 멀어진느낌..",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044693.1.22",
          "form": "고삐풀어놓고 먹다가",
          "original_form": "고삐풀어놓고 먹다가",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044693.1.23",
          "form": "이러다 큰일나겟다 싶어섴ㅋㅋㅋㅋㅋ",
          "original_form": "이러다 큰일나겟다 싶어섴ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044693.1.24",
          "form": "저 진짜 확찐자 됏엇거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "저 진짜 확찐자 됏엇거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044693.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ뭔지알거같아요 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ뭔지알거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044693.1.26",
          "form": "샐러드 만들어드세요?",
          "original_form": "샐러드 만들어드세요?",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044693.1.27",
          "form": "네 그냥 이것저것 사다가 대충 먹어요",
          "original_form": "네 그냥 이것저것 사다가 대충 먹어요",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044693.1.28",
          "form": "삶은계란이랑",
          "original_form": "삶은계란이랑",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044693.1.29",
          "form": "오 채소는 양상추만드세여?",
          "original_form": "오 채소는 양상추만드세여?",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044693.1.30",
          "form": "야채마저 잘먹어서... 한접시 가득ㅋㅋㅋㅋ.....",
          "original_form": "야채마저 잘먹어서... 한접시 가득ㅋㅋㅋㅋ.....",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044693.1.31",
          "form": "양상추랑 저 쌈채소들 좋아해서 쌈채소들도 듬성듬성썰어넣고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "양상추랑 저 쌈채소들 좋아해서 쌈채소들도 듬성듬성썰어넣고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044693.1.32",
          "form": "오이랑 당근도 넣곸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오이랑 당근도 넣곸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044693.1.33",
          "form": "오 그렇군요 드레싱은여?",
          "original_form": "오 그렇군요 드레싱은여?",
          "speaker_id": "1",
