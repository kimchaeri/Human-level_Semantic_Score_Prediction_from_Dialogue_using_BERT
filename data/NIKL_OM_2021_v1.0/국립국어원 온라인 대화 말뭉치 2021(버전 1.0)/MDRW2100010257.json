      "utterance": [
        {
          "id": "MDRW2100010257.1.1",
          "form": "여행가고싶어요~~",
          "original_form": "여행가고싶어요~~",
          "speaker_id": "1",
          "time": "20210823 16:08"
        },
        {
          "id": "MDRW2100010257.1.2",
          "form": "네 정말 여행가고 싶네요ㅠㅠ",
          "original_form": "네 정말 여행가고 싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010257.1.3",
          "form": "최근에 다녀오신 곳 있으세요?",
          "original_form": "최근에 다녀오신 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010257.1.4",
          "form": "저는 속초 짧게 다녀왔어요ㅎㅎ다녀온신데 있으세요?",
          "original_form": "저는 속초 짧게 다녀왔어요ㅎㅎ다녀온신데 있으세요?",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010257.1.5",
          "form": "저는 얼마전에 본가인 여수에 다녀왔어요",
          "original_form": "저는 얼마전에 본가인 여수에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100010257.1.6",
          "form": "여행이라고하긴 뭐하지만ㅋㅋ 콧바람 쐬고 왔답니다",
          "original_form": "여행이라고하긴 뭐하지만ㅋㅋ 콧바람 쐬고 왔답니다",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010257.1.7",
          "form": "아 여수밤바다 한번 가보고 싶었는데ㅠㅠ",
          "original_form": "아 여수밤바다 한번 가보고 싶었는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010257.1.8",
          "form": "한번 다녀오세용!",
          "original_form": "한번 다녀오세용!",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010257.1.9",
          "form": "가고싶은데 요즘은 좀 그렇네요ㅠㅠ여행지에 오래있는게 좋은데ㅠㅜ",
          "original_form": "가고싶은데 요즘은 좀 그렇네요ㅠㅠ여행지에 오래있는게 좋은데ㅠㅜ",
          "speaker_id": "2",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010257.1.10",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010257.1.11",
          "form": "제주도도 가고싶은데",
          "original_form": "제주도도 가고싶은데",
          "speaker_id": "1",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010257.1.12",
          "form": "아무래도 불안하고 그르네요",
          "original_form": "아무래도 불안하고 그르네요",
          "speaker_id": "1",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010257.1.13",
          "form": "제주도..정말 가구싶은데ㅠㅠ태풍인걸보니 시기를 놓친거같구ㅠㅠ",
          "original_form": "제주도..정말 가구싶은데ㅠㅠ태풍인걸보니 시기를 놓친거같구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010257.1.14",
          "form": "그쵸 내일 태풍때문에 비 엄청 많이 온다고 하더라구요",
          "original_form": "그쵸 내일 태풍때문에 비 엄청 많이 온다고 하더라구요",
          "speaker_id": "1",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100010257.1.15",
          "form": "비오는 여행도 분위기는 좋은데 막상 하긴 힘들구ㅎㅎ",
          "original_form": "비오는 여행도 분위기는 좋은데 막상 하긴 힘들구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010257.1.16",
          "form": "맞아요 비 맞고 돌아다니는 것두 한계가 있죠",
          "original_form": "맞아요 비 맞고 돌아다니는 것두 한계가 있죠",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010257.1.17",
          "form": "저는 일본여행 가는 걸 좋아하는데 코로나 끝나더라도 해외여행은 불안해서 한동안 못갈 것만 같아요",
          "original_form": "저는 일본여행 가는 걸 좋아하는데 코로나 끝나더라도 해외여행은 불안해서 한동안 못갈 것만 같아요",
          "speaker_id": "1",
          "time": "20210823 16:16"
        }
      ]
