      "utterance": [
        {
          "id": "MDRW2100029530.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100029530.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029530.1.3",
          "form": "독서 좋아하시나요?",
          "original_form": "독서 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029530.1.4",
          "form": "네 좋아해요 ㅎㅎ",
          "original_form": "네 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029530.1.5",
          "form": "윤 님은요?",
          "original_form": "윤 님은요?",
          "speaker_id": "1",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029530.1.6",
          "form": "요즘엔 여유가 없어 못읽었지만 좋아해요",
          "original_form": "요즘엔 여유가 없어 못읽었지만 좋아해요",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029530.1.7",
          "form": "특히 소설을 많이 읽었어요",
          "original_form": "특히 소설을 많이 읽었어요",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029530.1.8",
          "form": "오 ㅎㅎ 좋아요",
          "original_form": "오 ㅎㅎ 좋아요",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029530.1.9",
          "form": "어떤 류의 소설 좋아하시나요?",
          "original_form": "어떤 류의 소설 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100029530.1.10",
          "form": "보통 가리지않고 제목보고 재밌겠다 싶으면 읽었어요!",
          "original_form": "보통 가리지않고 제목보고 재밌겠다 싶으면 읽었어요!",
          "speaker_id": "2",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100029530.1.11",
          "form": "음 그래도 특히 환상소설을 선호하는 것 같아요",
          "original_form": "음 그래도 특히 환상소설을 선호하는 것 같아요",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029530.1.12",
          "form": "요즘엔 잘 없는 것 같지만요 ㅎㅎ...",
          "original_form": "요즘엔 잘 없는 것 같지만요 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029530.1.13",
          "form": "환상이면 판타지 같은 거 좋아하시나요?",
          "original_form": "환상이면 판타지 같은 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 11:53"
        },
        {
          "id": "MDRW2100029530.1.14",
          "form": "둘이 다른 장르로써 각각 좋아해요~",
          "original_form": "둘이 다른 장르로써 각각 좋아해요~",
          "speaker_id": "2",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029530.1.15",
          "form": "아하 그럼 어떤 작가 좋아하시나요?",
          "original_form": "아하 그럼 어떤 작가 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 12:19"
        },
        {
          "id": "MDRW2100029530.1.16",
          "form": "딱히 좋아하는 작가는 없지만 마고 래너건의 책을 좋아하는 편이예요",
          "original_form": "딱히 좋아하는 작가는 없지만 마고 래너건의 책을 좋아하는 편이예요",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029530.1.17",
          "form": "잠님은 어떤 책을 좋아하시나요?",
          "original_form": "잠님은 어떤 책을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029530.1.18",
          "form": "저는 한국소설 좋아해요~",
          "original_form": "저는 한국소설 좋아해요~",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029530.1.19",
          "form": "고전 소설들도 좋아하구요!",
          "original_form": "고전 소설들도 좋아하구요!",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029530.1.20",
          "form": "최근에는 조지 오웰 몇 권 읽어봤어요",
          "original_form": "최근에는 조지 오웰 몇 권 읽어봤어요",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029530.1.21",
          "form": "한국 소설은 취향을 넘어서 그냥 자주 읽는게 확실히 좋은 것 같아요",
          "original_form": "한국 소설은 취향을 넘어서 그냥 자주 읽는게 확실히 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100029530.1.22",
          "form": "한동안 안읽으면 어휘가 부족해지는 게 느껴지더라구요 ㅠㅠ",
          "original_form": "한동안 안읽으면 어휘가 부족해지는 게 느껴지더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100029530.1.23",
          "form": "아하 그렇죠 ㅠㅠ",
          "original_form": "아하 그렇죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100029530.1.24",
          "form": "저는 사회나 문화 종류 책도 좋아해요!",
          "original_form": "저는 사회나 문화 종류 책도 좋아해요!",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100029530.1.25",
          "form": "확실히 흐름을 읽을 수 있어서 지식도 쌓이고 좋더라구요",
          "original_form": "확실히 흐름을 읽을 수 있어서 지식도 쌓이고 좋더라구요",
          "speaker_id": "1",
          "time": "20210903 22:34"
        }
      ]
    }
