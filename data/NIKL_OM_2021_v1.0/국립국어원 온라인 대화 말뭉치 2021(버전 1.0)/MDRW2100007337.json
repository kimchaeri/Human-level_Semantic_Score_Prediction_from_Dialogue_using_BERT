      "utterance": [
        {
          "id": "MDRW2100007337.1.1",
          "form": "화내시던가요?",
          "original_form": "화내시던가요?",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.3",
          "form": "학점으로",
          "original_form": "학점으로",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.4",
          "form": "화내긴하셨는데",
          "original_form": "화내긴하셨는데",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.5",
          "form": "그건 제알빠 아니니까요",
          "original_form": "그건 제알빠 아니니까요",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.6",
          "form": "그쵸 어차피 못 받을 학점이라고 생각하면",
          "original_form": "그쵸 어차피 못 받을 학점이라고 생각하면",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.7",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.8",
          "form": "정신승리가 가능해지죠",
          "original_form": "정신승리가 가능해지죠",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.9",
          "form": "안내쫓았다고",
          "original_form": "안내쫓았다고",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.10",
          "form": "씨쁠이 아닐리가",
          "original_form": "씨쁠이 아닐리가",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007337.1.11",
          "form": "왜냐면 그 수업 전에 공강이 한시간 잇어서",
          "original_form": "왜냐면 그 수업 전에 공강이 한시간 잇어서",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007337.1.12",
          "form": "햄버거 사먹으러 가느라고",
          "original_form": "햄버거 사먹으러 가느라고",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007337.1.13",
          "form": "수업을 잘 안갓거든요",
          "original_form": "수업을 잘 안갓거든요",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007337.1.14",
          "form": "아 햄버거! 듣고 나니 궁금한데",
          "original_form": "아 햄버거! 듣고 나니 궁금한데",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007337.1.15",
          "form": "본인이 강의하는 입장이라면",
          "original_form": "본인이 강의하는 입장이라면",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007337.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007337.1.17",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007337.1.18",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007337.1.19",
          "form": "강의실 내 음식물 섭취 어디까지 가능하세요?",
          "original_form": "강의실 내 음식물 섭취 어디까지 가능하세요?",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007337.1.20",
          "form": "허용? 눈감아줌?",
          "original_form": "허용? 눈감아줌?",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007337.1.21",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007337.1.22",
          "form": "물은 허용",
          "original_form": "물은 허용",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007337.1.23",
          "form": "햄버거는 못참아요",
          "original_form": "햄버거는 못참아요",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007337.1.24",
          "form": "샌드위치는요?",
          "original_form": "샌드위치는요?",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007337.1.25",
          "form": "쉬는시간에만",
          "original_form": "쉬는시간에만",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007337.1.26",
          "form": "허용이요",
          "original_form": "허용이요",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007337.1.27",
          "form": "수업시간에",
          "original_form": "수업시간에",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007337.1.28",
          "form": "음식섭취",
          "original_form": "음식섭취",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007337.1.29",
          "form": "즐기세요?",
          "original_form": "즐기세요?",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007337.1.30",
          "form": "저... 네 저는 자주 먹었어요",
          "original_form": "저... 네 저는 자주 먹었어요",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007337.1.31",
          "form": "시리얼도 먹다가 혼난 적 있어요",
          "original_form": "시리얼도 먹다가 혼난 적 있어요",
          "speaker_id": "2",
          "time": "20210822 01:30"
        }
      ]
    }
  ]
