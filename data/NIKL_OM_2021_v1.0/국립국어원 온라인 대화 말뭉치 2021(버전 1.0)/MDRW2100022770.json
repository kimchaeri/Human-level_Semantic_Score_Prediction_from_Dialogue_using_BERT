      "utterance": [
        {
          "id": "MDRW2100022770.1.1",
          "form": "옷도 좋아하시구여?",
          "original_form": "옷도 좋아하시구여?",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022770.1.2",
          "form": "네 옷도 좋아해요 ㅎㅎ 편한 스탈이요 ㅎㅎ",
          "original_form": "네 옷도 좋아해요 ㅎㅎ 편한 스탈이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022770.1.3",
          "form": "헤어도 지지고 볶는거 자주 하시고요? ㅎㅎ",
          "original_form": "헤어도 지지고 볶는거 자주 하시고요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022770.1.4",
          "form": "헤어는 예전에 엄청했는데 ㅋㅋㅋ 요즘은 쉬고있어요ㅋㅋ",
          "original_form": "헤어는 예전에 엄청했는데 ㅋㅋㅋ 요즘은 쉬고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022770.1.5",
          "form": "머리가 감당을 못해서 ㅎㅎ",
          "original_form": "머리가 감당을 못해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022770.1.6",
          "form": "특인한거도 많이 하시나요? 보라색이런거?ㅋㅋㅋ",
          "original_form": "특인한거도 많이 하시나요? 보라색이런거?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022770.1.7",
          "form": "보라색은 안해봤는데 ㅋㅋ 파란색은 했어요 ㅋㅋㅋ",
          "original_form": "보라색은 안해봤는데 ㅋㅋ 파란색은 했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022770.1.8",
          "form": "와.. 마치.. 크레파스 같은? ㅋㅋㅋ",
          "original_form": "와.. 마치.. 크레파스 같은? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022770.1.9",
          "form": "지금은 그냥 긴머리구여?",
          "original_form": "지금은 그냥 긴머리구여?",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
