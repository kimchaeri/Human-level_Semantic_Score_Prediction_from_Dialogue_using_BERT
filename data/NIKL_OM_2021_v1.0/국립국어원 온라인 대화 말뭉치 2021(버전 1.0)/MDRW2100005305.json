      "utterance": [
        {
          "id": "MDRW2100005305.1.1",
          "form": "쭈.꾸.미.",
          "original_form": "쭈.꾸.미.",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005305.1.2",
          "form": "저 내일 닭어쩌고먹으러가요",
          "original_form": "저 내일 닭어쩌고먹으러가요",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005305.1.3",
          "form": "왜 해산물파인척 하시죠?",
          "original_form": "왜 해산물파인척 하시죠?",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005305.1.4",
          "form": "사실 육지동물 먹으면서요",
          "original_form": "사실 육지동물 먹으면서요",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005305.1.5",
          "form": "전 오리고기 먹고싶음..",
          "original_form": "전 오리고기 먹고싶음..",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005305.1.6",
          "form": "오리는 물고기인가요 땅고기인가요",
          "original_form": "오리는 물고기인가요 땅고기인가요",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005305.1.7",
          "form": "오리는",
          "original_form": "오리는",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005305.1.8",
          "form": "귀엽습니다...",
          "original_form": "귀엽습니다...",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005305.1.9",
          "form": "맛있습니다...",
          "original_form": "맛있습니다...",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005305.1.10",
          "form": "집근처에오리고기집이름이",
          "original_form": "집근처에오리고기집이름이",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005305.1.11",
          "form": "행복한오리에요",
          "original_form": "행복한오리에요",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005305.1.12",
          "form": "오리는 행복하지않을텐디...",
          "original_form": "오리는 행복하지않을텐디...",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005305.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 너무웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 너무웃겨",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005305.1.14",
          "form": "고깃집 간판에 왜 희생물이 웃으며 자기 살을 들고있을까요",
          "original_form": "고깃집 간판에 왜 희생물이 웃으며 자기 살을 들고있을까요",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005305.1.15",
          "form": "인간의추악한이기심",
          "original_form": "인간의추악한이기심",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.16",
          "form": "하지만 맛있죠...",
          "original_form": "하지만 맛있죠...",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.17",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.18",
          "form": "하..오라고기가 진짜맛있는데",
          "original_form": "하..오라고기가 진짜맛있는데",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.19",
          "form": "거기 맛동산오리라고",
          "original_form": "거기 맛동산오리라고",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.20",
          "form": "ㅁ버섯이랑 숙주랑 오리랑",
          "original_form": "ㅁ버섯이랑 숙주랑 오리랑",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.21",
          "form": "어떡하지",
          "original_form": "어떡하지",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.22",
          "form": "이제 맛동산하면",
          "original_form": "이제 맛동산하면",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.23",
          "form": "양념에절여서굽는게진짜맛있어요",
          "original_form": "양념에절여서굽는게진짜맛있어요",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.24",
          "form": "고영똥밖에",
          "original_form": "고영똥밖에",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.25",
          "form": "고영똥에비빈오리고기",
          "original_form": "고영똥에비빈오리고기",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005305.1.26",
          "form": "악ㅊ역겨워",
          "original_form": "악ㅊ역겨워",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.27",
          "form": "역겨워~",
          "original_form": "역겨워~",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.28",
          "form": "버섯숙주버무리 오리고기",
          "original_form": "버섯숙주버무리 오리고기",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.29",
          "form": "맛잇겟다",
          "original_form": "맛잇겟다",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.30",
          "form": "아무튼 그 오리고기애 맥주한잔이면",
          "original_form": "아무튼 그 오리고기애 맥주한잔이면",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.31",
          "form": "천상의맛",
          "original_form": "천상의맛",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.32",
          "form": "극락보내주는오리",
          "original_form": "극락보내주는오리",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.33",
          "form": "상추왁팍팍",
          "original_form": "상추왁팍팍",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.34",
          "form": "극락오리",
          "original_form": "극락오리",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.35",
          "form": "우리 창업하자",
          "original_form": "우리 창업하자",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.36",
          "form": "극락ㅇㅇ으로",
          "original_form": "극락ㅇㅇ으로",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.37",
          "form": "가게이름으로딱이네요",
          "original_form": "가게이름으로딱이네요",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005305.1.38",
          "form": "극락 보내 드립니다.",
          "original_form": "극락 보내 드립니다.",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005305.1.39",
          "form": "둘이먹다 하나 죽는 집",
          "original_form": "둘이먹다 하나 죽는 집",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005305.1.40",
          "form": "반드시 하나가 죽는집",
          "original_form": "반드시 하나가 죽는집",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005305.1.41",
          "form": "평생컨텐츠",
          "original_form": "평생컨텐츠",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005305.1.42",
          "form": "인생이 컨텐츠네요",
          "original_form": "인생이 컨텐츠네요",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005305.1.43",
          "form": "그것도 남의 인생으로 해먹는 컨텐츠",
          "original_form": "그것도 남의 인생으로 해먹는 컨텐츠",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005305.1.44",
          "form": "개꿀",
          "original_form": "개꿀",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005305.1.45",
          "form": "맛집",
          "original_form": "맛집",
