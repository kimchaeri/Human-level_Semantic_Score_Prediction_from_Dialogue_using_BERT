      "utterance": [
        {
          "id": "MDRW2100011793.1.1",
          "form": "혹시 중고거래해보셨나요?",
          "original_form": "혹시 중고거래해보셨나요?",
          "speaker_id": "1",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011793.1.2",
          "form": "저는 당근이용해요",
          "original_form": "저는 당근이용해요",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011793.1.3",
          "form": "저도 당근이용해요ㅎㅎ",
          "original_form": "저도 당근이용해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011793.1.4",
          "form": "많이 팔리던가요?",
          "original_form": "많이 팔리던가요?",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011793.1.5",
          "form": "저는 무료나눔이나 새거 위주로 팔아서 잘팔려요",
          "original_form": "저는 무료나눔이나 새거 위주로 팔아서 잘팔려요",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011793.1.6",
          "form": "최근에 당근에서 무료나눔어떤거 하셨어요?",
          "original_form": "최근에 당근에서 무료나눔어떤거 하셨어요?",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011793.1.7",
          "form": "독서실책상이요ㅎㅎ",
          "original_form": "독서실책상이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011793.1.8",
          "form": "와 멋지네요 학생분이 오셨던가요",
          "original_form": "와 멋지네요 학생분이 오셨던가요",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011793.1.9",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011793.1.10",
          "form": "네 시험준비한다고 용달차 불러 왔더라고요ㅎㅎ",
          "original_form": "네 시험준비한다고 용달차 불러 왔더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011793.1.11",
          "form": "주는이도 받는이도 마음따뜻하겠어요",
          "original_form": "주는이도 받는이도 마음따뜻하겠어요",
          "speaker_id": "2",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011793.1.12",
          "form": "네 당근후기에 세상은 아직 따뜻하다고 남겨주셨더라고요",
          "original_form": "네 당근후기에 세상은 아직 따뜻하다고 남겨주셨더라고요",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011793.1.13",
          "form": "뿌듯했어요",
          "original_form": "뿌듯했어요",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011793.1.14",
          "form": "그럼 당근으로 물건 구입한적도 있나요?",
          "original_form": "그럼 당근으로 물건 구입한적도 있나요?",
          "speaker_id": "2",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011793.1.15",
          "form": "아뇨ㅠ구매해본적은 없어요",
          "original_form": "아뇨ㅠ구매해본적은 없어요",
