      "utterance": [
        {
          "id": "MDRW2100047563.1.1",
          "form": "내일은 더 쨍쨍했으면 좋겠어요ㅠ",
          "original_form": "내일은 더 쨍쨍했으면 좋겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100047563.1.2",
          "form": "맞아요 ㅠㅠ 근데",
          "original_form": "맞아요 ㅠㅠ 근데",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100047563.1.3",
          "form": "비 온다는 소식이..",
          "original_form": "비 온다는 소식이..",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100047563.1.4",
          "form": "비 온다는 소식은 틀렸으면 하고 바라게 돼요...",
          "original_form": "비 온다는 소식은 틀렸으면 하고 바라게 돼요...",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100047563.1.5",
          "form": "ㅠㅠ 제발요 흑",
          "original_form": "ㅠㅠ 제발요 흑",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047563.1.6",
          "form": "잠깐 오다 그치면 너무 습해서 힘들어여ㅠ",
          "original_form": "잠깐 오다 그치면 너무 습해서 힘들어여ㅠ",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100047563.1.7",
          "form": "ㅠㅠ 맞아요 요새",
          "original_form": "ㅠㅠ 맞아요 요새",
          "speaker_id": "1",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047563.1.8",
          "form": "습기가 어마무시..",
          "original_form": "습기가 어마무시..",
          "speaker_id": "1",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047563.1.9",
          "form": "어제 자는데 이불이 눅눅...ㅠㅠ",
          "original_form": "어제 자는데 이불이 눅눅...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 09:30"
        },
        {
          "id": "MDRW2100047563.1.10",
          "form": "ㅋㅋㅎ 전 그정도까진 아니었는데 정말 싫었어요...",
          "original_form": "ㅋㅋㅎ 전 그정도까진 아니었는데 정말 싫었어요...",
          "speaker_id": "1",
          "time": "20210827 09:45"
        },
        {
          "id": "MDRW2100047563.1.11",
          "form": "오늘 일어나서 밖을 보는데 흐리더라구요...오늘도 오겠어요ㅠ",
          "original_form": "오늘 일어나서 밖을 보는데 흐리더라구요...오늘도 오겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100047563.1.12",
          "form": "ㅠㅠㅠ 계속 비 온대요",
          "original_form": "ㅠㅠㅠ 계속 비 온대요",
          "speaker_id": "1",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100047563.1.13",
          "form": "비 이제 싫어요ㅠㅠㅠㅠ",
          "original_form": "비 이제 싫어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 09:47"
        },
        {
          "id": "MDRW2100047563.1.14",
          "form": "아 정말요 ㅠㅠㅠㅠ",
          "original_form": "아 정말요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100047563.1.15",
          "form": "담주도 온다네요",
          "original_form": "담주도 온다네요",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100047563.1.16",
          "form": "지겹네요 이제는ㅠ",
          "original_form": "지겹네요 이제는ㅠ",
          "speaker_id": "2",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100047563.1.17",
          "form": "오늘 아직은 비가 안 오네요",
          "original_form": "오늘 아직은 비가 안 오네요",
          "speaker_id": "2",
          "time": "20210829 12:32"
        }
      ]
