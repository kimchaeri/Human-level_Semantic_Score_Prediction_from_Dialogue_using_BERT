      "utterance": [
        {
          "id": "MDRW2100016894.1.1",
          "form": "지금 학교 다니시나요??",
          "original_form": "지금 학교 다니시나요??",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100016894.1.2",
          "form": "아뇨ㅋㅋ졸업한지 꽤 되었어요",
          "original_form": "아뇨ㅋㅋ졸업한지 꽤 되었어요",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100016894.1.3",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100016894.1.4",
          "form": "저도 이제 곧 졸업이에요ㅎㅎ",
          "original_form": "저도 이제 곧 졸업이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100016894.1.5",
          "form": "곧 졸업이시면 바쁘시겠어요...!",
          "original_form": "곧 졸업이시면 바쁘시겠어요...!",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100016894.1.6",
          "form": "준비해야할 게 많죠ㅜ?",
          "original_form": "준비해야할 게 많죠ㅜ?",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100016894.1.7",
          "form": "바빠야되는데 너무 태평해서 문제에요ㅋㅋㅋㅋ",
          "original_form": "바빠야되는데 너무 태평해서 문제에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100016894.1.8",
          "form": "ㅋㅋㅋㅋ요즘에도 비대면수업인가요?",
          "original_form": "ㅋㅋㅋㅋ요즘에도 비대면수업인가요?",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100016894.1.9",
          "form": "중고등학생애들은 학교나가는 것 같더라구요ㅜ 대학생들은 어떤지 모르겠네요",
          "original_form": "중고등학생애들은 학교나가는 것 같더라구요ㅜ 대학생들은 어떤지 모르겠네요",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100016894.1.10",
          "form": "네네 요즘 확진자 늘어서 다시 전면 비대면 됐어요",
          "original_form": "네네 요즘 확진자 늘어서 다시 전면 비대면 됐어요",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100016894.1.11",
          "form": "전 좋지만요ㅋㅋ",
          "original_form": "전 좋지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100016894.1.12",
          "form": "ㅋㅋㅋ전 비대면강의 상상이 안돼요...!",
          "original_form": "ㅋㅋㅋ전 비대면강의 상상이 안돼요...!",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100016894.1.13",
          "form": "ㅋㅋㅋ재미없는 인강 같아요..ㅋㅋ",
          "original_form": "ㅋㅋㅋ재미없는 인강 같아요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100016894.1.14",
          "form": "맨날 배속해놓고 봐요ㅋㅋ",
          "original_form": "맨날 배속해놓고 봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100016894.1.15",
          "form": "실시간이 아니에요...?",
          "original_form": "실시간이 아니에요...?",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100016894.1.16",
          "form": "실시간인 수업도 있고 아닌 수업도 있고 그래요ㅎㅎ",
          "original_form": "실시간인 수업도 있고 아닌 수업도 있고 그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100016894.1.17",
          "form": "아닌 수업은 무조건 배속으로..ㅎㅎ",
          "original_form": "아닌 수업은 무조건 배속으로..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100016894.1.18",
          "form": "ㅋㅋㅋ시험은 어떻게 보나요?",
          "original_form": "ㅋㅋㅋ시험은 어떻게 보나요?",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100016894.1.19",
          "form": "전 복전중인데 통계학 수업은 오픈북으로 문제풀고 찍어서 올리게 하구요",
          "original_form": "전 복전중인데 통계학 수업은 오픈북으로 문제풀고 찍어서 올리게 하구요",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100016894.1.20",
          "form": "국문과 수업은 대부분 보고서 제출하는거에요!",
          "original_form": "국문과 수업은 대부분 보고서 제출하는거에요!",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100016894.1.21",
          "form": "오...신기신기하네요",
          "original_form": "오...신기신기하네요",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100016894.1.22",
          "form": "뭔가 옛날사람된 기분이 들어요ㅜ",
          "original_form": "뭔가 옛날사람된 기분이 들어요ㅜ",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100016894.1.23",
          "form": "앜ㅋㅋㅋ 아니에요",
          "original_form": "앜ㅋㅋㅋ 아니에요",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100016894.1.24",
          "form": "저도 곧 졸업이어서 이제 옛날사람 될 일밖에 안 남았어요ㅋㅋㅋㅋ",
          "original_form": "저도 곧 졸업이어서 이제 옛날사람 될 일밖에 안 남았어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100016894.1.25",
          "form": "복전 힘들진않아요?",
          "original_form": "복전 힘들진않아요?",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100016894.1.26",
          "form": "본전공이 잘 안맞았어서 그런지 더 나은거같아요ㅎㅎ",
          "original_form": "본전공이 잘 안맞았어서 그런지 더 나은거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100016894.1.27",
          "form": "오...저도 복전하고 싶긴했는데 하나만 하는거로도 벅차서 도저히 못하겠더라구요ㅜ",
          "original_form": "오...저도 복전하고 싶긴했는데 하나만 하는거로도 벅차서 도저히 못하겠더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210827 11:22"
        },
        {
          "id": "MDRW2100016894.1.28",
          "form": "수강해야하는 학점이 좀 많이 늘긴 하더라구요ㅠㅠ",
          "original_form": "수강해야하는 학점이 좀 많이 늘긴 하더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 13:10"
        }
      ]
    }
