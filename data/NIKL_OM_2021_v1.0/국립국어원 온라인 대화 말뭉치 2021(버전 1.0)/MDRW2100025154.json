      "utterance": [
        {
          "id": "MDRW2100025154.1.1",
          "form": "반려동물 키워보신 적 있으세요?",
          "original_form": "반려동물 키워보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100025154.1.2",
          "form": "엄청 어릴때요 키워보신적 있으신가요?",
          "original_form": "엄청 어릴때요 키워보신적 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100025154.1.3",
          "form": "네 저도 어릴때 청거북이랑 금붕어 키워봤어요",
          "original_form": "네 저도 어릴때 청거북이랑 금붕어 키워봤어요",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100025154.1.4",
          "form": "와 청거북이라니! 어디서 키우나요?",
          "original_form": "와 청거북이라니! 어디서 키우나요?",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100025154.1.5",
          "form": "물고기 어항에 큰 돌이랑 작은 돌 깔고 키웠어요",
          "original_form": "물고기 어항에 큰 돌이랑 작은 돌 깔고 키웠어요",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100025154.1.6",
          "form": "물에서 키우는군요ㅎㅎ대단해요!",
          "original_form": "물에서 키우는군요ㅎㅎ대단해요!",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100025154.1.7",
          "form": "먹이는 어떻게 하나요?",
          "original_form": "먹이는 어떻게 하나요?",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100025154.1.8",
          "form": "사료 주고 가끔 돼지고기 줬어요",
          "original_form": "사료 주고 가끔 돼지고기 줬어요",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100025154.1.9",
          "form": "돼지고기도 먹나봐요!",
          "original_form": "돼지고기도 먹나봐요!",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100025154.1.10",
          "form": "처음 알았어요",
          "original_form": "처음 알았어요",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100025154.1.11",
          "form": "지금 생각해보면 아무 고기나 먹는거 같아요",
          "original_form": "지금 생각해보면 아무 고기나 먹는거 같아요",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100025154.1.12",
          "form": "그때는 돼지고기말곤 줄 생각을 못했어요",
          "original_form": "그때는 돼지고기말곤 줄 생각을 못했어요",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100025154.1.13",
          "form": "와 신기하네요 몇년 키우셨어요?",
          "original_form": "와 신기하네요 몇년 키우셨어요?",
          "speaker_id": "2",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100025154.1.14",
          "form": "6-7년 키웠어요",
          "original_form": "6-7년 키웠어요",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100025154.1.15",
          "form": "오래 키우셨군요",
          "original_form": "오래 키우셨군요",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100025154.1.16",
          "form": "정이 엄청 들었겠어요",
          "original_form": "정이 엄청 들었겠어요",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100025154.1.17",
          "form": "네 제가 고3때 죽었어요 ㅠㅠㅠ",
          "original_form": "네 제가 고3때 죽었어요 ㅠㅠㅠ",
