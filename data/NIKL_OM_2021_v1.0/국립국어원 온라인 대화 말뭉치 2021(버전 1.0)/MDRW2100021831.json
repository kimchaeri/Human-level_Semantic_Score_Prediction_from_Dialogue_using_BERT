      "utterance": [
        {
          "id": "MDRW2100021831.1.1",
          "form": "좋아하는 피자 브랜드 있어요?",
          "original_form": "좋아하는 피자 브랜드 있어요?",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021831.1.2",
          "form": "좋기는 도미노가 젤 좋은데",
          "original_form": "좋기는 도미노가 젤 좋은데",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021831.1.3",
          "form": "비싸니깐",
          "original_form": "비싸니깐",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021831.1.4",
          "form": "ㄷ오 도미노 피자!",
          "original_form": "ㄷ오 도미노 피자!",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021831.1.5",
          "form": "도미노 피자는 행사 자주해서 할인해야 원래 그 가격 같아요..",
          "original_form": "도미노 피자는 행사 자주해서 할인해야 원래 그 가격 같아요..",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021831.1.6",
          "form": "네 행사 반드시 이용해야해요",
          "original_form": "네 행사 반드시 이용해야해요",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021831.1.7",
          "form": "원래 집앞에 도미노 있어서",
          "original_form": "원래 집앞에 도미노 있어서",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021831.1.8",
          "form": "방문 포장이 쉬웠는데",
          "original_form": "방문 포장이 쉬웠는데",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021831.1.9",
          "form": "너무 멀리 이사가서...",
          "original_form": "너무 멀리 이사가서...",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021831.1.10",
          "form": "ㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021831.1.11",
          "form": "전 다행히 포장세권에 있어요",
          "original_form": "전 다행히 포장세권에 있어요",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021831.1.12",
          "form": "큽 부럽따...",
          "original_form": "큽 부럽따...",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021831.1.13",
          "form": "3분거리 안에 교촌치킨, 치킨사냥, 피자헛, 롯데리아, 파리바게트, 스타벅스, 메가커피, 이디야, 투썸 등등 있고..",
          "original_form": "3분거리 안에 교촌치킨, 치킨사냥, 피자헛, 롯데리아, 파리바게트, 스타벅스, 메가커피, 이디야, 투썸 등등 있고..",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021831.1.14",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021831.1.15",
          "form": "지하철역에 사세요?",
          "original_form": "지하철역에 사세요?",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021831.1.16",
          "form": "ㅌㅋㅌㅌㅋㅋ",
          "original_form": "ㅌㅋㅌㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021831.1.17",
          "form": "5분 거리로는 피자스쿨, 난타피자, 비비큐 등등..",
          "original_form": "5분 거리로는 피자스쿨, 난타피자, 비비큐 등등..",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021831.1.18",
          "form": "어머 저 지하철역 3분거리에요 ㅎㅎ",
          "original_form": "어머 저 지하철역 3분거리에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021831.1.19",
          "form": "저는 지하철역 7분거리라 좀 머네요 흑ㅎㄱ",
          "original_form": "저는 지하철역 7분거리라 좀 머네요 흑ㅎㄱ",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021831.1.20",
          "form": "그래서 지하철이 전역에 있으면 그때 뛰어가면 탈 수 있어요 ㅋㅋ",
          "original_form": "그래서 지하철이 전역에 있으면 그때 뛰어가면 탈 수 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021831.1.21",
          "form": "완전 짱이다",
          "original_form": "완전 짱이다",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021831.1.22",
          "form": "비싸겠어요",
          "original_form": "비싸겠어요",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021831.1.23",
          "form": "근데 지하철역 안에 있던 던킨도너츠는 사라졌네요...",
          "original_form": "근데 지하철역 안에 있던 던킨도너츠는 사라졌네요...",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021831.1.24",
          "form": "요즘 행사도 자주하던데.. 폐점하지 말아주지 ㅠㅠ",
          "original_form": "요즘 행사도 자주하던데.. 폐점하지 말아주지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021831.1.25",
          "form": "앗 저 퇴근길에도 역사에 던킨 사라졌어요",
          "original_form": "앗 저 퇴근길에도 역사에 던킨 사라졌어요",
          "speaker_id": "2",
          "time": "20210828 22:56"
        }
      ]
    }
