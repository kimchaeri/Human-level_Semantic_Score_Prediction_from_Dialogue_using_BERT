      "utterance": [
        {
          "id": "MDRW2100002524.1.1",
          "form": "집안일을 미루는 편인가요 그때 그때 하는 편인가요?",
          "original_form": "집안일을 미루는 편인가요 그때 그때 하는 편인가요?",
          "speaker_id": "1",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002524.1.2",
          "form": "저는 약간 미루는 편이에요",
          "original_form": "저는 약간 미루는 편이에요",
          "speaker_id": "1",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002524.1.3",
          "form": "음 저는 집안일마다 다른데",
          "original_form": "음 저는 집안일마다 다른데",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002524.1.4",
          "form": "설거지는 그때그때하고",
          "original_form": "설거지는 그때그때하고",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002524.1.5",
          "form": "청소는 미루는 편이에요",
          "original_form": "청소는 미루는 편이에요",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002524.1.6",
          "form": "세상에 저 그거 제일 미뤄요 설거지",
          "original_form": "세상에 저 그거 제일 미뤄요 설거지",
          "speaker_id": "1",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002524.1.7",
          "form": "뭔가 해야하는건 아는데 귀찮아서",
          "original_form": "뭔가 해야하는건 아는데 귀찮아서",
          "speaker_id": "1",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002524.1.8",
          "form": "저는 설거지는 미루면",
          "original_form": "저는 설거지는 미루면",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002524.1.9",
          "form": "자꾸 쌓이는게 바로바로보여서",
          "original_form": "자꾸 쌓이는게 바로바로보여서",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002524.1.10",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002524.1.11",
          "form": "바로하게 되더라구요",
          "original_form": "바로하게 되더라구요",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002524.1.12",
          "form": "좋은 습관을 가지고 계시는군요!",
          "original_form": "좋은 습관을 가지고 계시는군요!",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002524.1.13",
          "form": "그렇다고 안귀찮은건 아닙니다ㅋㅋㅋ",
          "original_form": "그렇다고 안귀찮은건 아닙니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002524.1.14",
          "form": "청소중에서도 이거 제일 귀찮다 하는 거 있나요?",
          "original_form": "청소중에서도 이거 제일 귀찮다 하는 거 있나요?",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002524.1.15",
          "form": "엄청귀찮은데",
          "original_form": "엄청귀찮은데",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002524.1.16",
          "form": "화장실청소??",
          "original_form": "화장실청소??",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002524.1.17",
          "form": "다른건 다하는데",
          "original_form": "다른건 다하는데",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002524.1.18",
          "form": "화장실청소는 제일 싫어요",
          "original_form": "화장실청소는 제일 싫어요",
          "speaker_id": "2",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002524.1.19",
          "form": "저도 화장실",
          "original_form": "저도 화장실",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002524.1.20",
          "form": "벽이랑 바닥닦는거",
          "original_form": "벽이랑 바닥닦는거",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002524.1.21",
          "form": "너무 귀찮아요",
          "original_form": "너무 귀찮아요",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002524.1.22",
          "form": "팔아프고 힘들어서",
          "original_form": "팔아프고 힘들어서",
          "speaker_id": "2",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002524.1.23",
          "form": "근데 또 안하면 물 때 생기니까",
          "original_form": "근데 또 안하면 물 때 생기니까",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002524.1.24",
          "form": "차라리 빨래두번할래요ㅠ",
          "original_form": "차라리 빨래두번할래요ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002524.1.25",
          "form": "진짜 청소요정 있었으면 좋겠어요ㅠ",
          "original_form": "진짜 청소요정 있었으면 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002524.1.26",
          "form": "맨날 부려먹을텐데...",
          "original_form": "맨날 부려먹을텐데...",
          "speaker_id": "2",
          "time": "20210820 13:12"
        }
      ]
    }
