      "utterance": [
        {
          "id": "MDRW2100037564.1.1",
          "form": "덕분에 맛있는 전어회 먹었습니다",
          "original_form": "덕분에 맛있는 전어회 먹었습니다",
          "speaker_id": "1",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100037564.1.2",
          "form": "진짜 제철이라 맛있더라구요ㅋㅋ",
          "original_form": "진짜 제철이라 맛있더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100037564.1.3",
          "form": "ㅎㅎ 제가 다 기분 좋네요ㅎㅎ",
          "original_form": "ㅎㅎ 제가 다 기분 좋네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100037564.1.4",
          "form": "저도 어제 또 시켜 먹었어요!",
          "original_form": "저도 어제 또 시켜 먹었어요!",
          "speaker_id": "2",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100037564.1.5",
          "form": "간만에 진짜 맛있게 먹었어요",
          "original_form": "간만에 진짜 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100037564.1.6",
          "form": "name2님이 얘기해주신 파인애플 피자만 남았네요ㅋㅋㅋㅋ",
          "original_form": "&name2&님이 얘기해주신 파인애플 피자만 남았네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100037564.1.7",
          "form": "ㅎㅎㅎ그거 드시고 저 차단하는 거 아녜요?ㅎㅎ",
          "original_form": "ㅎㅎㅎ그거 드시고 저 차단하는 거 아녜요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100037564.1.8",
          "form": "헉ㅋㅋㅋㅋㄱㅋ",
          "original_form": "헉ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100037564.1.9",
          "form": "그정도인가요?!",
          "original_form": "그정도인가요?!",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100037564.1.10",
          "form": "저는 좋아합니다ㅎㅎㅎ",
          "original_form": "저는 좋아합니다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100037564.1.11",
          "form": "공차 민트 초코칩 추천해주신 분이 계셔서 오늘 먹었는데 약간 밍밍하긴 했지만 그래도 맛있더라구요",
          "original_form": "공차 민트 초코칩 추천해주신 분이 계셔서 오늘 먹었는데 약간 밍밍하긴 했지만 그래도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100037564.1.12",
          "form": "공차 민초 괜찮나요?",
          "original_form": "공차 민초 괜찮나요?",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100037564.1.13",
          "form": "왠지 파인애플 피자도  따뜻한 파인애플만 극복한다면 괜찮을 거 같아요",
          "original_form": "왠지 파인애플 피자도  따뜻한 파인애플만 극복한다면 괜찮을 거 같아요",
          "speaker_id": "1",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100037564.1.14",
          "form": "다른데 민초보면 너무 달기만 한 경우가 있어서ㅜ",
          "original_form": "다른데 민초보면 너무 달기만 한 경우가 있어서ㅜ",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100037564.1.15",
          "form": "ㅋㅋ 그게 관건이죠!",
          "original_form": "ㅋㅋ 그게 관건이죠!",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100037564.1.16",
          "form": "민트 맛보다는  그냥 달달한 초코 가루가 바삭바삭 씹히더라구요",
          "original_form": "민트 맛보다는  그냥 달달한 초코 가루가 바삭바삭 씹히더라구요",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100037564.1.17",
          "form": "시럽 펌핑을 두 번만 해서 30프로로 낮추니까 괜찮았어요",
          "original_form": "시럽 펌핑을 두 번만 해서 30프로로 낮추니까 괜찮았어요",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100037564.1.18",
          "form": "진짜 민초 잘못 먹으면 너무 달아서",
          "original_form": "진짜 민초 잘못 먹으면 너무 달아서",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100037564.1.19",
          "form": "시럽 둘 당도 30  기억하겠어요!",
          "original_form": "시럽 둘 당도 30  기억하겠어요!",
          "speaker_id": "2",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100037564.1.20",
          "form": "엄청 추천하지는 않습니다",
          "original_form": "엄청 추천하지는 않습니다",
          "speaker_id": "1",
          "time": "20210904 16:22"
        }
      ]
    }
