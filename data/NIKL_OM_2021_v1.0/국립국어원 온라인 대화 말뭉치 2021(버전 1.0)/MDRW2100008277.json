      "utterance": [
        {
          "id": "MDRW2100008277.1.1",
          "form": "점심은 드셨어요?",
          "original_form": "점심은 드셨어요?",
          "speaker_id": "1",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008277.1.2",
          "form": "방금 먹고 왔어요",
          "original_form": "방금 먹고 왔어요",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008277.1.3",
          "form": "조금 늦은 점심이었네요 벌써 3시가 다 되어간다니",
          "original_form": "조금 늦은 점심이었네요 벌써 3시가 다 되어간다니",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008277.1.4",
          "form": "늦게드셨네요.  어떤거 드셨어요?",
          "original_form": "늦게드셨네요.  어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008277.1.5",
          "form": "전 올만에 밥해서 먹었어요",
          "original_form": "전 올만에 밥해서 먹었어요",
          "speaker_id": "1",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008277.1.6",
          "form": "고등어굽고 계란찜하고",
          "original_form": "고등어굽고 계란찜하고",
          "speaker_id": "1",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008277.1.7",
          "form": "저는 김밥 만들어 먹었어요",
          "original_form": "저는 김밥 만들어 먹었어요",
          "speaker_id": "2",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008277.1.8",
          "form": "손이 많이 가긴하는데 좋아해서ㅎㅎ",
          "original_form": "손이 많이 가긴하는데 좋아해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008277.1.9",
          "form": "와~~~김밥맛있겠어요",
          "original_form": "와~~~김밥맛있겠어요",
          "speaker_id": "1",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008277.1.10",
          "form": "저도 집에서 계란. 햄. 단무지만넣고 자주해먹어요.",
          "original_form": "저도 집에서 계란. 햄. 단무지만넣고 자주해먹어요.",
          "speaker_id": "1",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008277.1.11",
          "form": "저도 계란 햄 단무지 넣고 오늘은 특별히 어묵 넣었어요",
          "original_form": "저도 계란 햄 단무지 넣고 오늘은 특별히 어묵 넣었어요",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008277.1.12",
          "form": "어묵 살짝 간장넣고 볶아서넣음 좋더라구요",
          "original_form": "어묵 살짝 간장넣고 볶아서넣음 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008277.1.13",
          "form": "맞아요 어묵볶음 밥도둑이죠",
          "original_form": "맞아요 어묵볶음 밥도둑이죠",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008277.1.14",
          "form": "이따 저녁때 해먹어야겠어요~~~ 저녁 해결했네요^^",
          "original_form": "이따 저녁때 해먹어야겠어요~~~ 저녁 해결했네요^^",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008277.1.15",
          "form": "좋은 소식이네요",
          "original_form": "좋은 소식이네요",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008277.1.16",
          "form": "저는 매번 메뉴 정하는게 고역이라",
          "original_form": "저는 매번 메뉴 정하는게 고역이라",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008277.1.17",
          "form": "마자요. 밥먹고나면 그다음 밥 몰할지 고민하니깐요",
          "original_form": "마자요. 밥먹고나면 그다음 밥 몰할지 고민하니깐요",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008277.1.18",
          "form": "그걸 하루에 3번 하니까 지쳐요",
          "original_form": "그걸 하루에 3번 하니까 지쳐요",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008277.1.19",
          "form": "힘 나라고 밥먹는건데 오히려 힘이 빠져요",
          "original_form": "힘 나라고 밥먹는건데 오히려 힘이 빠져요",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008277.1.20",
          "form": "ㅎㅎ 맞아요",
          "original_form": "ㅎㅎ 맞아요",
          "speaker_id": "1",
          "time": "20210822 14:50"
        }
