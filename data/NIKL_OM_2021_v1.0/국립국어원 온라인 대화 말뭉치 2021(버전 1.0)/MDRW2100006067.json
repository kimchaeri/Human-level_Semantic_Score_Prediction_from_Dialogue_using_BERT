      "utterance": [
        {
          "id": "MDRW2100006067.1.1",
          "form": "백신은 맞으셧어요?",
          "original_form": "백신은 맞으셧어요?",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100006067.1.2",
          "form": "의료인이라서 진작 맞앗어욤",
          "original_form": "의료인이라서 진작 맞앗어욤",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100006067.1.3",
          "form": "그러셧군요",
          "original_form": "그러셧군요",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100006067.1.4",
          "form": "코로나때문에 잃은게 너무 많아요 ㅠㅠ",
          "original_form": "코로나때문에 잃은게 너무 많아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100006067.1.5",
          "form": "맞으셧나요?",
          "original_form": "맞으셧나요?",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100006067.1.6",
          "form": "1차만 맞앗어요",
          "original_form": "1차만 맞앗어요",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100006067.1.7",
          "form": "화이자맞으셧나욤?",
          "original_form": "화이자맞으셧나욤?",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100006067.1.8",
          "form": "모더나맞앗어요",
          "original_form": "모더나맞앗어요",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100006067.1.9",
          "form": "오 저는 화이자!!",
          "original_form": "오 저는 화이자!!",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100006067.1.10",
          "form": "어땟어요?",
          "original_form": "어땟어요?",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100006067.1.11",
          "form": "저는 하나도안아팟는데",
          "original_form": "저는 하나도안아팟는데",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100006067.1.12",
          "form": "주변에서 다 아파햇어요",
          "original_form": "주변에서 다 아파햇어요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100006067.1.13",
          "form": "수액도 맞고",
          "original_form": "수액도 맞고",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100006067.1.14",
          "form": "수액맞을정도로요? ㄷㄷ",
          "original_form": "수액맞을정도로요? ㄷㄷ",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100006067.1.15",
          "form": "화이자2차맞고 아픈사람들 잇엇어여",
          "original_form": "화이자2차맞고 아픈사람들 잇엇어여",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100006067.1.16",
          "form": "ㅠㅠ무섭네요",
          "original_form": "ㅠㅠ무섭네요",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006067.1.17",
          "form": "모더나 2차 맞은 분은 주변에 잇나요?",
          "original_form": "모더나 2차 맞은 분은 주변에 잇나요?",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006067.1.18",
          "form": "엄마 오늘 2차맞앗어요",
          "original_form": "엄마 오늘 2차맞앗어요",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006067.1.19",
          "form": "모더나맞고 아프다는소리 주변에서 못들엇어요!!",
          "original_form": "모더나맞고 아프다는소리 주변에서 못들엇어요!!",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006067.1.20",
          "form": "아 정말요? 다행이네요",
          "original_form": "아 정말요? 다행이네요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        }
      ]
