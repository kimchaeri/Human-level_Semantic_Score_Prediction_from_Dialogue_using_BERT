      "utterance": [
        {
          "id": "MDRW2100014403.1.1",
          "form": "군대 이야기 들어보셨나요",
          "original_form": "군대 이야기 들어보셨나요",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014403.1.2",
          "form": "ㅋㅋ그럼요\r\n군대 이야기 들어봤죠",
          "original_form": "ㅋㅋ그럼요\r\n군대 이야기 들어봤죠",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014403.1.3",
          "form": "군대에서 축구한 이야기도\r\n들어보구여ㅋㅋㅋ",
          "original_form": "군대에서 축구한 이야기도\r\n들어보구여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014403.1.4",
          "form": "저도 연애할때 남편에게 들어봤는데 요즘은 들을일이 없네요",
          "original_form": "저도 연애할때 남편에게 들어봤는데 요즘은 들을일이 없네요",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014403.1.5",
          "form": "결혼 9년차라고 하셨던거 같은데",
          "original_form": "결혼 9년차라고 하셨던거 같은데",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014403.1.6",
          "form": "지금까지 군대 얘기하면\r\n넘 재미없을거 같아요ㅋㅋ",
          "original_form": "지금까지 군대 얘기하면\r\n넘 재미없을거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014403.1.7",
          "form": "ㅋㅋㅋ맞아요 이제 제가 안듣죠...",
          "original_form": "ㅋㅋㅋ맞아요 이제 제가 안듣죠...",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014403.1.8",
          "form": "아들 있으신가요? 군대보낼?",
          "original_form": "아들 있으신가요? 군대보낼?",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014403.1.9",
          "form": "요샌 군대 복무기간도 많이 줄었더라구요ㅋ",
          "original_form": "요샌 군대 복무기간도 많이 줄었더라구요ㅋ",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014403.1.10",
          "form": "저희 남편때만해도 2년이었는데\r\n지금은 1년반이더라구용",
          "original_form": "저희 남편때만해도 2년이었는데\r\n지금은 1년반이더라구용",
          "speaker_id": "2",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014403.1.11",
          "form": "군대 보낼 아들 둘이나 있어요ㅋㅋ",
          "original_form": "군대 보낼 아들 둘이나 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014403.1.12",
          "form": "우리 아이들 갈때면 더 줄어들을까요?",
          "original_form": "우리 아이들 갈때면 더 줄어들을까요?",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014403.1.13",
          "form": "아마 그렇지 않을까요?",
          "original_form": "아마 그렇지 않을까요?",
          "speaker_id": "2",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014403.1.14",
          "form": "요샌 핸드폰 사용도 가능하고\r\n확실히 이전과는 많이 다르더라구요",
          "original_form": "요샌 핸드폰 사용도 가능하고\r\n확실히 이전과는 많이 다르더라구요",
          "speaker_id": "2",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100014403.1.15",
          "form": "그러게요..그래도 막상 보내려면 걱정이 되지 싶어요.. 어떠시나요^^",
          "original_form": "그러게요..그래도 막상 보내려면 걱정이 되지 싶어요.. 어떠시나요^^",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014403.1.16",
          "form": "그쵸ㅠㅠ\r\n그때되면 안가면 좋겠어여ㅋㅋ",
          "original_form": "그쵸ㅠㅠ\r\n그때되면 안가면 좋겠어여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014403.1.17",
          "form": "강철부대 보셨어요?",
          "original_form": "강철부대 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014403.1.18",
          "form": "다 보진 못하고\r\n몇 번 보긴 했었어요",
          "original_form": "다 보진 못하고\r\n몇 번 보긴 했었어요",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014403.1.19",
          "form": "저도 잠깐만 봤는데..보면 군대는 체력이 중요해보이더라구요",
          "original_form": "저도 잠깐만 봤는데..보면 군대는 체력이 중요해보이더라구요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        }
