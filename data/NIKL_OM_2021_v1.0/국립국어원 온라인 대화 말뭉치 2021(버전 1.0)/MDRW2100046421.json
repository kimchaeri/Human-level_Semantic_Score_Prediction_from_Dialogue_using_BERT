      "utterance": [
        {
          "id": "MDRW2100046421.1.1",
          "form": "안녕하세유",
          "original_form": "안녕하세유",
          "speaker_id": "1",
          "time": "20211020 20:16"
        },
        {
          "id": "MDRW2100046421.1.2",
          "form": "안뇽하세요!",
          "original_form": "안뇽하세요!",
          "speaker_id": "2",
          "time": "20211020 20:19"
        },
        {
          "id": "MDRW2100046421.1.3",
          "form": "저녁 드셨어요~?",
          "original_form": "저녁 드셨어요~?",
          "speaker_id": "2",
          "time": "20211020 20:20"
        },
        {
          "id": "MDRW2100046421.1.4",
          "form": "그럼요~",
          "original_form": "그럼요~",
          "speaker_id": "1",
          "time": "20211020 20:20"
        },
        {
          "id": "MDRW2100046421.1.5",
          "form": "식사는하셨나요?",
          "original_form": "식사는하셨나요?",
          "speaker_id": "1",
          "time": "20211020 20:20"
        },
        {
          "id": "MDRW2100046421.1.6",
          "form": "저는 야채곱창 기다리는 중이에요 ㅎㅎ",
          "original_form": "저는 야채곱창 기다리는 중이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 20:20"
        },
        {
          "id": "MDRW2100046421.1.7",
          "form": "뭐 드셨어요~?",
          "original_form": "뭐 드셨어요~?",
          "speaker_id": "2",
          "time": "20211020 20:20"
        },
        {
          "id": "MDRW2100046421.1.8",
          "form": "저는 식단조절중이라 사과랑 닭가슴살먹었어요",
          "original_form": "저는 식단조절중이라 사과랑 닭가슴살먹었어요",
          "speaker_id": "1",
          "time": "20211020 20:20"
        },
        {
          "id": "MDRW2100046421.1.9",
          "form": "야채곱창ㅜㅜㅜ",
          "original_form": "야채곱창ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20211020 20:20"
        },
        {
          "id": "MDRW2100046421.1.10",
          "form": "멋지네요",
          "original_form": "멋지네요",
          "speaker_id": "1",
          "time": "20211020 20:20"
        },
        {
          "id": "MDRW2100046421.1.11",
          "form": "헐 다이어트 중이시군요",
          "original_form": "헐 다이어트 중이시군요",
          "speaker_id": "2",
          "time": "20211020 20:21"
        },
        {
          "id": "MDRW2100046421.1.12",
          "form": "더 멋지시네요!!",
          "original_form": "더 멋지시네요!!",
          "speaker_id": "2",
          "time": "20211020 20:21"
        },
        {
          "id": "MDRW2100046421.1.13",
          "form": "전 닭가슴살은 진짜 못먹겠더라고요 ㅠㅠ",
          "original_form": "전 닭가슴살은 진짜 못먹겠더라고요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 20:21"
        },
        {
          "id": "MDRW2100046421.1.14",
          "form": "감사합니다ㅎㅎ",
          "original_form": "감사합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20211020 20:21"
        },
        {
          "id": "MDRW2100046421.1.15",
          "form": "끔찍하죠",
          "original_form": "끔찍하죠",
          "speaker_id": "1",
          "time": "20211020 20:21"
        },
        {
          "id": "MDRW2100046421.1.16",
          "form": "어떻게 맛있어지지가 않더라구요",
          "original_form": "어떻게 맛있어지지가 않더라구요",
          "speaker_id": "1",
          "time": "20211020 20:21"
        },
        {
          "id": "MDRW2100046421.1.17",
          "form": "맞아요.. 나혼자산다에서 박나래에 미역국수에 올려먹던데",
          "original_form": "맞아요.. 나혼자산다에서 박나래에 미역국수에 올려먹던데",
          "speaker_id": "2",
          "time": "20211020 20:22"
        },
        {
          "id": "MDRW2100046421.1.18",
          "form": "그거 한번 해보세요...!",
          "original_form": "그거 한번 해보세요...!",
          "speaker_id": "2",
          "time": "20211020 20:22"
        },
        {
          "id": "MDRW2100046421.1.19",
          "form": "귀찮겠지만....ㅋㅋ",
          "original_form": "귀찮겠지만....ㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 20:22"
        },
        {
          "id": "MDRW2100046421.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ좋네요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ좋네요!",
          "speaker_id": "1",
          "time": "20211020 20:23"
        },
        {
          "id": "MDRW2100046421.1.21",
          "form": "야채곱창 맛있겠어요",
          "original_form": "야채곱창 맛있겠어요",
          "speaker_id": "1",
          "time": "20211020 20:24"
        },
        {
          "id": "MDRW2100046421.1.22",
          "form": "배달시키셨나요?",
          "original_form": "배달시키셨나요?",
          "speaker_id": "1",
          "time": "20211020 20:24"
        },
        {
          "id": "MDRW2100046421.1.23",
          "form": "네!! 넘 맛있어요 ㅠㅠ",
          "original_form": "네!! 넘 맛있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 20:50"
        },
        {
          "id": "MDRW2100046421.1.24",
          "form": "제일 먹고싶은 음식이 뭐예요?",
          "original_form": "제일 먹고싶은 음식이 뭐예요?",
          "speaker_id": "2",
          "time": "20211020 20:50"
        },
        {
          "id": "MDRW2100046421.1.25",
          "form": "저는 치킨이요ㅜㅜㅜㅜ",
          "original_form": "저는 치킨이요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20211020 20:50"
        },
        {
          "id": "MDRW2100046421.1.26",
          "form": "치킨 진리죠....",
          "original_form": "치킨 진리죠....",
          "speaker_id": "2",
          "time": "20211020 20:50"
        },
        {
          "id": "MDRW2100046421.1.27",
