      "utterance": [
        {
          "id": "MDRW2100013324.1.1",
          "form": "악뮤 낙하 너무 좋아해요",
          "original_form": "악뮤 낙하 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013324.1.2",
          "form": "저도 낙하좋아해요",
          "original_form": "저도 낙하좋아해요",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013324.1.3",
          "form": "아이유가 한몫했죠",
          "original_form": "아이유가 한몫했죠",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013324.1.4",
          "form": "이번에 유스케 보셨어요?",
          "original_form": "이번에 유스케 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013324.1.5",
          "form": "봤죠ㅋㅋ 찬혁이 대박이던데요?",
          "original_form": "봤죠ㅋㅋ 찬혁이 대박이던데요?",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013324.1.6",
          "form": "그니까요ㄲㄲㅋ",
          "original_form": "그니까요ㄲㄲㅋ",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013324.1.7",
          "form": "퍼포먼스가 귀엽더라고요",
          "original_form": "퍼포먼스가 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013324.1.8",
          "form": "비티에스 좋아하세요?",
          "original_form": "비티에스 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013324.1.9",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013324.1.10",
          "form": "곧 정국이 생일",
          "original_form": "곧 정국이 생일",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013324.1.11",
          "form": "뮤비가 항상 트렌디해서 좋아해요",
          "original_form": "뮤비가 항상 트렌디해서 좋아해요",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013324.1.12",
          "form": "전 음악이 너무 좋아요",
          "original_form": "전 음악이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013324.1.13",
          "form": "요즘 듣는 노래는 뭐예요?",
          "original_form": "요즘 듣는 노래는 뭐예요?",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013324.1.14",
          "form": "뮤직이즈 마이라이프랄까요?",
          "original_form": "뮤직이즈 마이라이프랄까요?",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013324.1.15",
          "form": "전 노동요들어요",
          "original_form": "전 노동요들어요",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013324.1.16",
          "form": "전 오늘 넷플릭스 보다 잘거에요",
          "original_form": "전 오늘 넷플릭스 보다 잘거에요",
