      "utterance": [
        {
          "id": "MDRW2100037745.1.1",
          "form": "저녁뭐드실예정인가요?",
          "original_form": "저녁뭐드실예정인가요?",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100037745.1.2",
          "form": "오징어국을 끓여볼까해요",
          "original_form": "오징어국을 끓여볼까해요",
          "speaker_id": "1",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100037745.1.3",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "2",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100037745.1.4",
          "form": "장금이시네욤",
          "original_form": "장금이시네욤",
          "speaker_id": "2",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100037745.1.5",
          "form": "아니요ㅋㅋㅋ 그냥 시늉만\r\n조미료가 필요한데 없네요ㅠ",
          "original_form": "아니요ㅋㅋㅋ 그냥 시늉만\r\n조미료가 필요한데 없네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037745.1.6",
          "form": "오늘날씨랑 딱이네요",
          "original_form": "오늘날씨랑 딱이네요",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100037745.1.7",
          "form": "저는 점심을 늦게먹어서 애매해서",
          "original_form": "저는 점심을 늦게먹어서 애매해서",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100037745.1.8",
          "form": "저녁은 패쓰할려구욧",
          "original_form": "저녁은 패쓰할려구욧",
          "speaker_id": "2",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100037745.1.9",
          "form": "늦엇어도 한루에 세번안먹으면 허전해요ㅋㅋ",
          "original_form": "늦엇어도 한루에 세번안먹으면 허전해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100037745.1.10",
          "form": "ㅋㅋ야무지게챙겨드시는군요",
          "original_form": "ㅋㅋ야무지게챙겨드시는군요",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037745.1.11",
          "form": "다 먹고살자고 하는짓ㅋㅋ\r\n과하게 먹어서 문제지만요",
          "original_form": "다 먹고살자고 하는짓ㅋㅋ\r\n과하게 먹어서 문제지만요",
          "speaker_id": "1",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037745.1.12",
          "form": "저두요ㅜㅜ",
          "original_form": "저두요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037745.1.13",
          "form": "먹는낙으로살아유",
          "original_form": "먹는낙으로살아유",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037745.1.14",
          "form": "오늘도 오마논어치나 장봣는더 모가 없네요ㅋ",
          "original_form": "오늘도 오마논어치나 장봣는더 모가 없네요ㅋ",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100037745.1.15",
          "form": "크 저는 쓱배송 칠만원치시켯는데",
          "original_form": "크 저는 쓱배송 칠만원치시켯는데",
          "speaker_id": "2",
          "time": "20210901 17:58"
        },
        {
          "id": "MDRW2100037745.1.16",
          "form": "먹을게없어요",
          "original_form": "먹을게없어요",
          "speaker_id": "2",
          "time": "20210901 17:58"
        },
        {
          "id": "MDRW2100037745.1.17",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:58"
        },
        {
          "id": "MDRW2100037745.1.18",
          "form": "진짜 왜 그런걸까요ㅋㅋ\r\n그렇게나 삿는데",
          "original_form": "진짜 왜 그런걸까요ㅋㅋ\r\n그렇게나 삿는데",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100037745.1.19",
          "form": "흑흑ㅠㅠ 더담고싶엇는데 뺏던것들이",
          "original_form": "흑흑ㅠㅠ 더담고싶엇는데 뺏던것들이",
          "speaker_id": "2",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100037745.1.20",
          "form": "아른거려욥ㅠㅠ",
          "original_form": "아른거려욥ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:59"
        }
      ]
