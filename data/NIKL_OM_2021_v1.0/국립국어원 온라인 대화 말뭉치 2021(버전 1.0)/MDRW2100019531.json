      "utterance": [
        {
          "id": "MDRW2100019531.1.1",
          "form": "복숭아도 종류가 다양하던데",
          "original_form": "복숭아도 종류가 다양하던데",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019531.1.2",
          "form": "선호하는 종류 있으세요?",
          "original_form": "선호하는 종류 있으세요?",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019531.1.3",
          "form": "전 노란색 물복을 조아해요",
          "original_form": "전 노란색 물복을 조아해요",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019531.1.4",
          "form": "황도요?",
          "original_form": "황도요?",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019531.1.5",
          "form": "황도품종이라고 해야되나 앨버트종이 맛이어요",
          "original_form": "황도품종이라고 해야되나 앨버트종이 맛이어요",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019531.1.6",
          "form": "요즘 철인가요?",
          "original_form": "요즘 철인가요?",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019531.1.7",
          "form": "지금보다 살짝 전이 철인데 지금도 있을거에요 좀 가을스러울때 나와요",
          "original_form": "지금보다 살짝 전이 철인데 지금도 있을거에요 좀 가을스러울때 나와요",
          "speaker_id": "2",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019531.1.8",
          "form": "아 며칠전에 마트에서 노란 복숭아 사서 맛있게 먹었는데",
          "original_form": "아 며칠전에 마트에서 노란 복숭아 사서 맛있게 먹었는데",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019531.1.9",
          "form": "그 품종일수도 있겠네요",
          "original_form": "그 품종일수도 있겠네요",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019531.1.10",
          "form": "전 마도카 좋아해요",
          "original_form": "전 마도카 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019531.1.11",
          "form": "그럴 수 있겠어요 rㅘ즙이 많고 달아요",
          "original_form": "그럴 수 있겠어요 rㅘ즙이 많고 달아요",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019531.1.12",
          "form": "맞아요 당도가 높았어요",
          "original_form": "맞아요 당도가 높았어요",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019531.1.13",
          "form": "아 마도카 복숭아 알아요",
          "original_form": "아 마도카 복숭아 알아요",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019531.1.14",
          "form": "이게 딱 적당히 딱딱하고 단거같아요",
          "original_form": "이게 딱 적당히 딱딱하고 단거같아요",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019531.1.15",
          "form": "전 약간 아삭거리는 것도 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "전 약간 아삭거리는 것도 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019531.1.16",
          "form": "맞아요 마도카는 달아요  저도 좋아해요",
          "original_form": "맞아요 마도카는 달아요  저도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019531.1.17",
          "form": "따로 주문해먹는 곳 있으신가요?",
          "original_form": "따로 주문해먹는 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019531.1.18",
          "form": "마도카가 앨버트보다 좀 더 일찍 나와요",
          "original_form": "마도카가 앨버트보다 좀 더 일찍 나와요",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019531.1.19",
          "form": "아뇨 전 많이 살땐 청과물시장가서 보고사요",
          "original_form": "아뇨 전 많이 살땐 청과물시장가서 보고사요",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019531.1.20",
          "form": "한번에 많이 사세요?",
          "original_form": "한번에 많이 사세요?",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019531.1.21",
          "form": "많이는 안사는데 한박스 단위로 살땐 자전거를 타고 근처 청과시장으로 가서 싣고와요",
          "original_form": "많이는 안사는데 한박스 단위로 살땐 자전거를 타고 근처 청과시장으로 가서 싣고와요",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019531.1.22",
          "form": "차대긴 좀 불편해요",
          "original_form": "차대긴 좀 불편해요",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019531.1.23",
          "form": "아하 근처에 시장 있으면 좋죠",
          "original_form": "아하 근처에 시장 있으면 좋죠",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019531.1.24",
          "form": "요즘 딱복 자체가 거의 끝난거 같아서 좀 아쉬워요",
          "original_form": "요즘 딱복 자체가 거의 끝난거 같아서 좀 아쉬워요",
          "speaker_id": "1",
          "time": "20210827 20:15"
        }
      ]
    }
