      "utterance": [
        {
          "id": "MDRW2100014706.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014706.1.2",
          "form": "네 여행좋아합니다 최근엔 코로나로 못갔네요",
          "original_form": "네 여행좋아합니다 최근엔 코로나로 못갔네요",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014706.1.3",
          "form": "제일 좋아하시는 여행장소가 있으신가요?",
          "original_form": "제일 좋아하시는 여행장소가 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014706.1.4",
          "form": "경주 좋아합니다",
          "original_form": "경주 좋아합니다",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014706.1.5",
          "form": "저도 경주 좋아해요 ㅋㅋㅋㅋㅋ 추억의 장소가 많아요",
          "original_form": "저도 경주 좋아해요 ㅋㅋㅋㅋㅋ 추억의 장소가 많아요",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014706.1.6",
          "form": "아 정말요 저두요 정말 자주 가는편인데",
          "original_form": "아 정말요 저두요 정말 자주 가는편인데",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014706.1.7",
          "form": "코로나이후론 못가네요",
          "original_form": "코로나이후론 못가네요",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014706.1.8",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014706.1.9",
          "form": "저는 소풍으로 경주를 많이 갔어서 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 소풍으로 경주를 많이 갔어서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014706.1.10",
          "form": "박물관 앞에서 수건돌리기나 보물찾기 하고 그랫던 기억이 어렴풋하게 많이 남아있어요 ㅋㅋㅋㅋ",
          "original_form": "박물관 앞에서 수건돌리기나 보물찾기 하고 그랫던 기억이 어렴풋하게 많이 남아있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014706.1.11",
          "form": "아 정말요 ㅋㅋㅋ",
          "original_form": "아 정말요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014706.1.12",
          "form": "저는 대릉원 월정교 좋아해요",
          "original_form": "저는 대릉원 월정교 좋아해요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014706.1.13",
          "form": "대릉원 월정교 밤에 너무 예쁘죠 ㅎㅎ",
          "original_form": "대릉원 월정교 밤에 너무 예쁘죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014706.1.14",
          "form": "맞아요 역시 아시는군요",
          "original_form": "맞아요 역시 아시는군요",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014706.1.15",
          "form": "교리김밥은 좀 실망했어요 ㅋㅋㅋㅋㅋㅋㅋ너무기대했나봅니다",
          "original_form": "교리김밥은 좀 실망했어요 ㅋㅋㅋㅋㅋㅋㅋ너무기대했나봅니다",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014706.1.16",
          "form": "경주는 그리고 봄에가면 은근 곳곳에 벚꽃이 너무 예뻐요",
          "original_form": "경주는 그리고 봄에가면 은근 곳곳에 벚꽃이 너무 예뻐요",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014706.1.17",
          "form": "안압지? 동궁과월지? 야경투어좋았어요",
          "original_form": "안압지? 동궁과월지? 야경투어좋았어요",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014706.1.18",
          "form": "맞아요 경주는 맛집은 크게 없는...ㅋㅋㅋㅋ",
          "original_form": "맞아요 경주는 맛집은 크게 없는...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014706.1.19",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014706.1.20",
          "form": "벚꽃 최고죠 자전거 라이딩 하기좋은",
          "original_form": "벚꽃 최고죠 자전거 라이딩 하기좋은",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014706.1.21",
          "form": "가을되면 꼭 가보고 싶어요 다시ㅠ",
          "original_form": "가을되면 꼭 가보고 싶어요 다시ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014706.1.22",
          "form": "맞아요 ㅠㅠ 코로나만 아니면 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 코로나만 아니면 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014706.1.23",
          "form": "해외는 좋아하는 나라 있으세요?",
          "original_form": "해외는 좋아하는 나라 있으세요?",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014706.1.24",
          "form": "해외는 후쿠오카 여행 좋았어요",
          "original_form": "해외는 후쿠오카 여행 좋았어요",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014706.1.25",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014706.1.26",
          "form": "베트남이랑",
          "original_form": "베트남이랑",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014706.1.27",
          "form": "저는 라오스가 그렇게 좋았어요",
          "original_form": "저는 라오스가 그렇게 좋았어요",
          "speaker_id": "1",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014706.1.28",
          "form": "우와 라오스도 다녀온 분들 정말 좋다하더라구요",
          "original_form": "우와 라오스도 다녀온 분들 정말 좋다하더라구요",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014706.1.29",
          "form": "방... 방카..? 뭐랬지",
          "original_form": "방... 방카..? 뭐랬지",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014706.1.30",
          "form": "방비엥?",
          "original_form": "방비엥?",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014706.1.31",
          "form": "헐 맞아요 ㅋㅋ",
          "original_form": "헐 맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014706.1.32",
          "form": "기억이 안났어요 역시 안가보니 무지하네요ㅠ",
          "original_form": "기억이 안났어요 역시 안가보니 무지하네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014706.1.33",
          "form": "방비엥에 푸른 호수에 그네타는곳 있지않아요?",
          "original_form": "방비엥에 푸른 호수에 그네타는곳 있지않아요?",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014706.1.34",
          "form": "거기 또 다른덴가",
          "original_form": "거기 또 다른덴가",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014706.1.35",
          "form": "사실 저는 정작 방비엥은 안갔어서 ㅋㅋㅋ",
          "original_form": "사실 저는 정작 방비엥은 안갔어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014706.1.36",
          "form": "저는 루앙프라방에 오래있었어요",
          "original_form": "저는 루앙프라방에 오래있었어요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014706.1.37",
          "form": "아 그렇균요 루앙프라방엔 뭐가 유명해요?",
          "original_form": "아 그렇균요 루앙프라방엔 뭐가 유명해요?",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014706.1.38",
          "form": "폭포도 많구요",
          "original_form": "폭포도 많구요",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014706.1.39",
          "form": "야시장 구경하는 재미도 쏠쏠하고",
          "original_form": "야시장 구경하는 재미도 쏠쏠하고",
