        {
          "id": "MDRW2100007647.1.1",
          "form": "모임해여?",
          "original_form": "모임해여?",
          "speaker_id": "1",
          "time": "20210822 08:58"
        },
        {
          "id": "MDRW2100007647.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007647.1.3",
          "form": "하시는거 있나요?",
          "original_form": "하시는거 있나요?",
          "speaker_id": "2",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007647.1.4",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "1",
          "time": "20210822 08:59"
        },
        {
          "id": "MDRW2100007647.1.5",
          "form": "사람못만난지도 오래됐네요",
          "original_form": "사람못만난지도 오래됐네요",
          "speaker_id": "2",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007647.1.6",
          "form": "거리두기가 참",
          "original_form": "거리두기가 참",
          "speaker_id": "1",
          "time": "20210822 09:00"
        },
        {
          "id": "MDRW2100007647.1.7",
          "form": "보고싶어요",
          "original_form": "보고싶어요",
          "speaker_id": "1",
          "time": "20210822 09:27"
        },
        {
          "id": "MDRW2100007647.1.8",
          "form": "결혼식 뷔페도 먹고싶고 ㅋ",
          "original_form": "결혼식 뷔페도 먹고싶고 ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:27"
        },
        {
          "id": "MDRW2100007647.1.9",
          "form": "만나서 이야기도ㅠ하고",
          "original_form": "만나서 이야기도ㅠ하고",
          "speaker_id": "1",
          "time": "20210822 09:29"
        },
        {
          "id": "MDRW2100007647.1.10",
          "form": "예전엔 무슨 동호회하셨나요?",
          "original_form": "예전엔 무슨 동호회하셨나요?",
          "speaker_id": "2",
          "time": "20210822 09:30"
        },
        {
          "id": "MDRW2100007647.1.11",
          "form": "친구 계모임요",
          "original_form": "친구 계모임요",
          "speaker_id": "1",
          "time": "20210822 09:31"
        },
        {
          "id": "MDRW2100007647.1.12",
          "form": "ㅋㅋ이자가 쎄나요?",
          "original_form": "ㅋㅋ이자가 쎄나요?",
          "speaker_id": "2",
          "time": "20210822 09:31"
        },
        {
          "id": "MDRW2100007647.1.13",
          "form": "걍 모으느 거죠 ㅋㅋ",
          "original_form": "걍 모으느 거죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007647.1.14",
          "form": "전 산악동호회 한번갔었어요",
          "original_form": "전 산악동호회 한번갔었어요",
          "speaker_id": "2",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007647.1.15",
          "form": "펀드 동호회랑",
          "original_form": "펀드 동호회랑",
          "speaker_id": "2",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007647.1.16",
          "form": "재테크동호회",
          "original_form": "재테크동호회",
          "speaker_id": "2",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007647.1.17",
          "form": "알고보니 다단계더라고요",
          "original_form": "알고보니 다단계더라고요",
          "speaker_id": "2",
          "time": "20210822 09:34"
        },
