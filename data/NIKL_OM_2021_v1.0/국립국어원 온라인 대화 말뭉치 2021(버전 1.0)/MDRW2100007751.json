      "utterance": [
        {
          "id": "MDRW2100007751.1.1",
          "form": "name1 님은 제일 기억남는 여행지가 어디였어요?",
          "original_form": "&name1& 님은 제일 기억남는 여행지가 어디였어요?",
          "speaker_id": "2",
          "time": "20210822 10:14"
        },
        {
          "id": "MDRW2100007751.1.2",
          "form": "저는 처음간 해외여행인 일본이요",
          "original_form": "저는 처음간 해외여행인 일본이요",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007751.1.3",
          "form": "와 전 해외여행 한 번도 안 가봤어요",
          "original_form": "와 전 해외여행 한 번도 안 가봤어요",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007751.1.4",
          "form": "할로윈기간에가서 구경할게 너무 많았어요",
          "original_form": "할로윈기간에가서 구경할게 너무 많았어요",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007751.1.5",
          "form": "어느 지역 가보셨어요?",
          "original_form": "어느 지역 가보셨어요?",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007751.1.6",
          "form": "처음간건 오사카요",
          "original_form": "처음간건 오사카요",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007751.1.7",
          "form": "후쿠오카랑 도쿄는 그 뒤에갔었어요",
          "original_form": "후쿠오카랑 도쿄는 그 뒤에갔었어요",
          "speaker_id": "1",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007751.1.8",
          "form": "할로윈이었다니 엄청 볼만 했겠네요",
          "original_form": "할로윈이었다니 엄청 볼만 했겠네요",
          "speaker_id": "2",
          "time": "20210822 10:16"
        },
        {
          "id": "MDRW2100007751.1.9",
          "form": "그땐 아직 우리나라에서 할로윈을 딱히 즐기지않던때라서",
          "original_form": "그땐 아직 우리나라에서 할로윈을 딱히 즐기지않던때라서",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007751.1.10",
          "form": "일본에서 막 코스프레하고 돌아다니는분들 신기했어요",
          "original_form": "일본에서 막 코스프레하고 돌아다니는분들 신기했어요",
          "speaker_id": "1",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007751.1.11",
          "form": "재밌었겠어요",
          "original_form": "재밌었겠어요",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007751.1.12",
          "form": "저도 코로나 끝나면 꼭 가보고 싶어요",
          "original_form": "저도 코로나 끝나면 꼭 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007751.1.13",
          "form": "코로나 끝나면 꼭 갈수 있을거예요!",
          "original_form": "코로나 끝나면 꼭 갈수 있을거예요!",
          "speaker_id": "1",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007751.1.14",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007751.1.15",
          "form": "그런 편이죠",
          "original_form": "그런 편이죠",
          "speaker_id": "2",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007751.1.16",
          "form": "name1 님은 국내여행 많이 가보셨나요?",
          "original_form": "&name1& 님은 국내여행 많이 가보셨나요?",
          "speaker_id": "2",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007751.1.17",
          "form": "코로나 터지기전에는 많이갔어요",
          "original_form": "코로나 터지기전에는 많이갔어요",
          "speaker_id": "1",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007751.1.18",
          "form": "요즘은ㅠㅠ",
          "original_form": "요즘은ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007751.1.19",
          "form": "어디가 제일 좋았나요?",
          "original_form": "어디가 제일 좋았나요?",
          "speaker_id": "2",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007751.1.20",
          "form": "요즘은 집 밖에서 외식하기도 힘들죠ㅠ",
          "original_form": "요즘은 집 밖에서 외식하기도 힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007751.1.21",
          "form": "그쵸ㅠ 요즘은 힘들어요",
          "original_form": "그쵸ㅠ 요즘은 힘들어요",
          "speaker_id": "1",
          "time": "20210822 10:33"
        },
        {
          "id": "MDRW2100007751.1.22",
          "form": "저는 경주가 제일 좋았어요",
          "original_form": "저는 경주가 제일 좋았어요",
          "speaker_id": "1",
          "time": "20210822 10:33"
        },
        {
          "id": "MDRW2100007751.1.23",
          "form": "자주가도 갈때마다 좋은곳이예요",
          "original_form": "자주가도 갈때마다 좋은곳이예요",
          "speaker_id": "1",
          "time": "20210822 10:33"
        },
        {
          "id": "MDRW2100007751.1.24",
          "form": "경주는 안 가봤네요 경주에서 뭐 하셨어요?",
          "original_form": "경주는 안 가봤네요 경주에서 뭐 하셨어요?",
          "speaker_id": "2",
          "time": "20210822 10:34"
        },
        {
          "id": "MDRW2100007751.1.25",
          "form": "경주에서 안압지 야경도 구경하고 자전거타고 왕릉구경하면서 느긋하게 다녔어요",
          "original_form": "경주에서 안압지 야경도 구경하고 자전거타고 왕릉구경하면서 느긋하게 다녔어요",
          "speaker_id": "1",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007751.1.26",
          "form": "아 듣기만 해도 너무 좋네요",
          "original_form": "아 듣기만 해도 너무 좋네요",
          "speaker_id": "2",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007751.1.27",
          "form": "국내여행 많이 다니시나요?",
          "original_form": "국내여행 많이 다니시나요?",
          "speaker_id": "1",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007751.1.28",
          "form": "코로나 전에는 꽤 다녔어요",
          "original_form": "코로나 전에는 꽤 다녔어요",
          "speaker_id": "2",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007751.1.29",
          "form": "부산 또 가보고 싶어요",
          "original_form": "부산 또 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007751.1.30",
          "form": "부산! 부산에 어디가 좋으셨나요",
          "original_form": "부산! 부산에 어디가 좋으셨나요",
          "speaker_id": "1",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007751.1.31",
          "form": "어디라기보단 물떡을 맛있게 먹어서ㅋㅋㅋ",
          "original_form": "어디라기보단 물떡을 맛있게 먹어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007751.1.32",
          "form": "그거 먹고 싶어요",
          "original_form": "그거 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007751.1.33",
          "form": "물떡 좋죠ㅠㅠ",
          "original_form": "물떡 좋죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007751.1.34",
          "form": "저부산사람이라 물떡 어디가도. 다있는줄 알았잖아요",
          "original_form": "저부산사람이라 물떡 어디가도. 다있는줄 알았잖아요",
          "speaker_id": "1",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007751.1.35",
          "form": "부러워요!!",
          "original_form": "부러워요!!",
          "speaker_id": "2",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007751.1.36",
          "form": "왜 물떡은 전국에서 안 파는 거죠",
          "original_form": "왜 물떡은 전국에서 안 파는 거죠",
          "speaker_id": "2",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007751.1.37",
          "form": "그러니까요 간단하고 맛있는데 분식집마다 팔아주셨으면",
          "original_form": "그러니까요 간단하고 맛있는데 분식집마다 팔아주셨으면",
