      "utterance": [
        {
          "id": "MDRW2100007175.1.1",
          "form": "그 현대미술관갓을때 기획전시 했엇거든",
          "original_form": "그 현대미술관갓을때 기획전시 했엇거든",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007175.1.2",
          "form": "어디서?",
          "original_form": "어디서?",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007175.1.3",
          "form": "그 그림중에 벌거벗은5명이 둥글레둥글레하는거 작가이름머고",
          "original_form": "그 그림중에 벌거벗은5명이 둥글레둥글레하는거 작가이름머고",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007175.1.4",
          "form": "뉴욕에서",
          "original_form": "뉴욕에서",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007175.1.5",
          "form": "음 김흥도 그림인가",
          "original_form": "음 김흥도 그림인가",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007175.1.6",
          "form": "대단한 발상이네",
          "original_form": "대단한 발상이네",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007175.1.7",
          "form": "아님? ㅋㅋ",
          "original_form": "아님? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007175.1.8",
          "form": "그거모르나",
          "original_form": "그거모르나",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007175.1.9",
          "form": "어휴 미대출신맞나",
          "original_form": "어휴 미대출신맞나",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007175.1.10",
          "form": "모르겠다 피카소인가?",
          "original_form": "모르겠다 피카소인가?",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007175.1.11",
          "form": "그게 왜",
          "original_form": "그게 왜",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007175.1.12",
          "form": "아무튼 그거 기획전시도봤음 쩔제",
          "original_form": "아무튼 그거 기획전시도봤음 쩔제",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007175.1.13",
          "form": "뉴욕에서?",
          "original_form": "뉴욕에서?",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007175.1.14",
          "form": "무튼 거기 신기한그림들많더라 별헤는밤도잇고",
          "original_form": "무튼 거기 신기한그림들많더라 별헤는밤도잇고",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007175.1.15",
          "form": "아 별해는밤은 타자검정이가? 별이쏟아지는밤인가?",
          "original_form": "아 별해는밤은 타자검정이가? 별이쏟아지는밤인가?",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007175.1.16",
          "form": "오 그 그림 좋지 나도 가보고싶군",
          "original_form": "오 그 그림 좋지 나도 가보고싶군",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007175.1.17",
          "form": "가봐라",
          "original_form": "가봐라",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007175.1.18",
          "form": "별이 빛나는 밤이다",
          "original_form": "별이 빛나는 밤이다",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007175.1.19",
          "form": "그래 뉴욕 또 가고싶네",
          "original_form": "그래 뉴욕 또 가고싶네",
          "speaker_id": "1",
          "time": "20210822 00:47"
        }
      ]
