        {
          "id": "MDRW2100015887.1.1",
          "form": "혹시 학창시절에 동아리 해보셨나요?",
          "original_form": "혹시 학창시절에 동아리 해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 02:25"
        },
        {
          "id": "MDRW2100015887.1.2",
          "form": "십자수동아리요",
          "original_form": "십자수동아리요",
          "speaker_id": "2",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015887.1.3",
          "form": "오 언제 하셨던 거에요?",
          "original_form": "오 언제 하셨던 거에요?",
          "speaker_id": "1",
          "time": "20210826 02:27"
        },
        {
          "id": "MDRW2100015887.1.4",
          "form": "중딩때요",
          "original_form": "중딩때요",
          "speaker_id": "2",
          "time": "20210826 02:28"
        },
        {
          "id": "MDRW2100015887.1.5",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210826 02:28"
        },
        {
          "id": "MDRW2100015887.1.6",
          "form": "저는 중딩때 비즈공예했었어요ㅋㅋ",
          "original_form": "저는 중딩때 비즈공예했었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:28"
        },
        {
          "id": "MDRW2100015887.1.7",
          "form": "현재 학생이세요?",
          "original_form": "현재 학생이세요?",
          "speaker_id": "2",
          "time": "20210826 02:29"
        },
        {
          "id": "MDRW2100015887.1.8",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210826 02:29"
        },
        {
          "id": "MDRW2100015887.1.9",
          "form": "어떤 학생이셨나요?",
          "original_form": "어떤 학생이셨나요?",
          "speaker_id": "2",
          "time": "20210826 02:29"
        },
        {
          "id": "MDRW2100015887.1.10",
          "form": "저는 조용하고 게으른 학생이었어요",
          "original_form": "저는 조용하고 게으른 학생이었어요",
          "speaker_id": "1",
          "time": "20210826 02:30"
        },
        {
          "id": "MDRW2100015887.1.11",
          "form": "학생때 가장 스트레스 받는게 뭐였어요?",
          "original_form": "학생때 가장 스트레스 받는게 뭐였어요?",
          "speaker_id": "1",
          "time": "20210826 02:31"
        },
        {
          "id": "MDRW2100015887.1.12",
          "form": "등교하는거요ㅋㅋ",
          "original_form": "등교하는거요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:31"
        },
        {
          "id": "MDRW2100015887.1.13",
          "form": "아  ㅋㅋㅋㅋ",
          "original_form": "아  ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:32"
        },
        {
          "id": "MDRW2100015887.1.14",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 02:33"
        },
        {
          "id": "MDRW2100015887.1.15",
