        {
          "id": "MDRW2100010471.1.1",
          "form": "아직뇨",
          "original_form": "아직뇨",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010471.1.2",
          "form": "키우고 싶은 동물은 있나요",
          "original_form": "키우고 싶은 동물은 있나요",
          "speaker_id": "1",
          "time": "20210824 06:35"
        },
        {
          "id": "MDRW2100010471.1.3",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100010471.1.4",
          "form": "저는 알러지 땜에 좀 힘드네요",
          "original_form": "저는 알러지 땜에 좀 힘드네요",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100010471.1.5",
          "form": "저도 털이싫어서",
          "original_form": "저도 털이싫어서",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100010471.1.6",
          "form": "그럼 열대어 키우는건 어떨까요",
          "original_form": "그럼 열대어 키우는건 어떨까요",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100010471.1.7",
          "form": "물관리가 힘들죠",
          "original_form": "물관리가 힘들죠",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100010471.1.8",
          "form": "뭐든 키운다는건 힘들긴 하죠",
          "original_form": "뭐든 키운다는건 힘들긴 하죠",
          "speaker_id": "1",
          "time": "20210825 06:58"
        },
        {
          "id": "MDRW2100010471.1.9",
          "form": "네 케어가 장난아님",
          "original_form": "네 케어가 장난아님",
          "speaker_id": "2",
          "time": "20210825 07:11"
        },
        {
          "id": "MDRW2100010471.1.10",
          "form": "식물을 키우는건 어떨까요?",
          "original_form": "식물을 키우는건 어떨까요?",
          "speaker_id": "1",
          "time": "20210825 07:28"
        },
        {
          "id": "MDRW2100010471.1.11",
          "form": "아.것도 좋네요",
          "original_form": "아.것도 좋네요",
          "speaker_id": "2",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100010471.1.12",
          "form": "식물도 사랑과 정성으로 키워야 잘큰대요",
          "original_form": "식물도 사랑과 정성으로 키워야 잘큰대요",
          "speaker_id": "1",
          "time": "20210825 07:50"
        },
