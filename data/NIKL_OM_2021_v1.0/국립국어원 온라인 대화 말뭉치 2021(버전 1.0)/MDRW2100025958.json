      "utterance": [
        {
          "id": "MDRW2100025958.1.1",
          "form": "오 하시는김에 엘지로 싹다 ㅎㅎ",
          "original_form": "오 하시는김에 엘지로 싹다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025958.1.2",
          "form": "ㅋㅋㅋ그런까여?ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그런까여?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025958.1.3",
          "form": "공청은 어디꺼세요?",
          "original_form": "공청은 어디꺼세요?",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025958.1.4",
          "form": "저는 다 삼성으로 샀어욬ㅋㅋㅋㅋ",
          "original_form": "저는 다 삼성으로 샀어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025958.1.5",
          "form": "어쩌다보니",
          "original_form": "어쩌다보니",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025958.1.6",
          "form": "오ㅋㅋㄱㄱ다삼성꺼구나",
          "original_form": "오ㅋㅋㄱㄱ다삼성꺼구나",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025958.1.7",
          "form": "큐브?",
          "original_form": "큐브?",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025958.1.8",
          "form": "그거예요?",
          "original_form": "그거예요?",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025958.1.9",
          "form": "저희집도 삼성도 있어요ㅋ",
          "original_form": "저희집도 삼성도 있어요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025958.1.10",
          "form": "한대있어서 추가로 더 사려는거ㅋㅋ",
          "original_form": "한대있어서 추가로 더 사려는거ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025958.1.11",
          "form": "네네 큐브예용ㅋㅋㅋㅋㅋ",
          "original_form": "네네 큐브예용ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025958.1.12",
          "form": "긴큐브...?ㅋㅋㅋ",
          "original_form": "긴큐브...?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025958.1.13",
          "form": "오ㅋㅋㅋ큐브가 좋자나요",
          "original_form": "오ㅋㅋㅋ큐브가 좋자나요",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025958.1.14",
          "form": "저는 이번에 지마켓 빅스마일데이인줄알고",
          "original_form": "저는 이번에 지마켓 빅스마일데이인줄알고",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025958.1.15",
          "form": "기다렸는데 아니여서 속상ㅋ",
          "original_form": "기다렸는데 아니여서 속상ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025958.1.16",
          "form": "할인팍팍 들어갈줄알았거든요",
          "original_form": "할인팍팍 들어갈줄알았거든요",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025958.1.17",
          "form": "ㅠㅠㅠ할인 얼마나 됐나요 ㅠㅠ?",
          "original_form": "ㅠㅠㅠ할인 얼마나 됐나요 ㅠㅠ?",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025958.1.18",
          "form": "최대할인이 10만원ㅠㅠ",
          "original_form": "최대할인이 10만원ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025958.1.19",
          "form": "빅스마일데이는30만원이거든여",
          "original_form": "빅스마일데이는30만원이거든여",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025958.1.20",
          "form": "아 10만원이면 적기늨 하네요 ㅠㅠㅠ",
          "original_form": "아 10만원이면 적기늨 하네요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025958.1.21",
          "form": "네ㅋ공청 2단짜리는 거의 100정도라서요",
          "original_form": "네ㅋ공청 2단짜리는 거의 100정도라서요",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025958.1.22",
          "form": "생각보다 할인율이 적어요",
          "original_form": "생각보다 할인율이 적어요",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025958.1.23",
          "form": "맞아요 2단은 100 넘더라두요 ㅠㅠㅠ",
          "original_form": "맞아요 2단은 100 넘더라두요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025958.1.24",
          "form": "비싸서꼭할인할때사려구요ㅋ",
          "original_form": "비싸서꼭할인할때사려구요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100025958.1.25",
          "form": "늦어서죄송해욬ㅋㅋㅋ 꼭 할인해서 싸게싸게 사세용 ㅎㅎ",
