      "utterance": [
        {
          "id": "MDRW2100022458.1.1",
          "form": "고양이도 키웠어요?",
          "original_form": "고양이도 키웠어요?",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022458.1.2",
          "form": "친척누나가 고양이 키웠어요",
          "original_form": "친척누나가 고양이 키웠어요",
          "speaker_id": "2",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022458.1.3",
          "form": "근데 주워왔대요",
          "original_form": "근데 주워왔대요",
          "speaker_id": "2",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022458.1.4",
          "form": "아~  저희 누나도 기르거든요",
          "original_form": "아~  저희 누나도 기르거든요",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022458.1.5",
          "form": "걔도 주워왔어요ㅋㅋ",
          "original_form": "걔도 주워왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022458.1.6",
          "form": "아 ㅋㅋ 누나랑 같이 사는거 아니였어요?",
          "original_form": "아 ㅋㅋ 누나랑 같이 사는거 아니였어요?",
          "speaker_id": "2",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022458.1.7",
          "form": "고양이는 보통 주워오나봐오ㅡ",
          "original_form": "고양이는 보통 주워오나봐오ㅡ",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022458.1.8",
          "form": "따로살아요ㅋㅋ",
          "original_form": "따로살아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022458.1.9",
          "form": "길고양이들이 집사 간택하잖아요",
          "original_form": "길고양이들이 집사 간택하잖아요",
          "speaker_id": "2",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022458.1.10",
          "form": "저도 간택당해봤어요..",
          "original_form": "저도 간택당해봤어요..",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022458.1.11",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022458.1.12",
          "form": "네 학교 마치고 집 가는데 따라오더라구요",
          "original_form": "네 학교 마치고 집 가는데 따라오더라구요",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022458.1.13",
          "form": "막 야옹~거리면서",
          "original_form": "막 야옹~거리면서",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022458.1.14",
          "form": "간택을 승낙했나요",
          "original_form": "간택을 승낙했나요",
          "speaker_id": "1",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022458.1.15",
          "form": "승낙하고 싶었는데 자취집이라서 ㅠㅠ",
          "original_form": "승낙하고 싶었는데 자취집이라서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022458.1.16",
          "form": "못데리고 왔어요",
          "original_form": "못데리고 왔어요",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022458.1.17",
          "form": "아.. 아쉬웠겠다",
          "original_form": "아.. 아쉬웠겠다",
          "speaker_id": "1",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022458.1.18",
          "form": "근데 그 고양이가 학교 안에 살고 있어서",
          "original_form": "근데 그 고양이가 학교 안에 살고 있어서",
          "speaker_id": "2",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022458.1.19",
          "form": "볼 때마다 간식 사줬어요",
          "original_form": "볼 때마다 간식 사줬어요",
          "speaker_id": "2",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022458.1.20",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210829 14:25"
        }
      ]
    }
