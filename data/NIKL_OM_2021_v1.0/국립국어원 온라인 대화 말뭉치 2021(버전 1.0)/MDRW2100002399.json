      "utterance": [
        {
          "id": "MDRW2100002399.1.1",
          "form": "요즘 코로나가 심해서",
          "original_form": "요즘 코로나가 심해서",
          "speaker_id": "1",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002399.1.2",
          "form": "집에서 일하고 싶더라",
          "original_form": "집에서 일하고 싶더라",
          "speaker_id": "1",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002399.1.3",
          "form": "맞다 요즘 코로나 걸릴까봐 밖에서 일하는 것도 무섭더라",
          "original_form": "맞다 요즘 코로나 걸릴까봐 밖에서 일하는 것도 무섭더라",
          "speaker_id": "2",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002399.1.4",
          "form": "집에서 일할게 있나?",
          "original_form": "집에서 일할게 있나?",
          "speaker_id": "2",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002399.1.5",
          "form": "요즘 찾아봤는데",
          "original_form": "요즘 찾아봤는데",
          "speaker_id": "1",
          "time": "20210820 11:10"
        },
        {
          "id": "MDRW2100002399.1.6",
          "form": "데이터 라벨링 같은거 있더라",
          "original_form": "데이터 라벨링 같은거 있더라",
          "speaker_id": "1",
          "time": "20210820 11:10"
        },
        {
          "id": "MDRW2100002399.1.7",
          "form": "엥? 그게 뭐야?",
          "original_form": "엥? 그게 뭐야?",
          "speaker_id": "2",
          "time": "20210820 11:10"
        },
        {
          "id": "MDRW2100002399.1.8",
          "form": "ai 학습용으로",
          "original_form": "ai 학습용으로",
          "speaker_id": "1",
          "time": "20210820 11:10"
        },
        {
          "id": "MDRW2100002399.1.9",
          "form": "사진이나 음성, 영상을 인간이 분류해서",
          "original_form": "사진이나 음성, 영상을 인간이 분류해서",
          "speaker_id": "1",
          "time": "20210820 11:10"
        },
        {
          "id": "MDRW2100002399.1.10",
          "form": "인공지능 학습하는 일자리래",
          "original_form": "인공지능 학습하는 일자리래",
          "speaker_id": "1",
          "time": "20210820 11:10"
        },
        {
          "id": "MDRW2100002399.1.11",
          "form": "그런 일은 돈 많이 줘??",
          "original_form": "그런 일은 돈 많이 줘??",
          "speaker_id": "2",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002399.1.12",
          "form": "돈은 별로 안주는것 같아",
          "original_form": "돈은 별로 안주는것 같아",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002399.1.13",
          "form": "하는만큼 벌어가는 일이라서",
          "original_form": "하는만큼 벌어가는 일이라서",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002399.1.14",
          "form": "최저시급보다 안나오는 경우도...",
          "original_form": "최저시급보다 안나오는 경우도...",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002399.1.15",
          "form": "정직하지만 별로 좋진 못하네",
          "original_form": "정직하지만 별로 좋진 못하네",
          "speaker_id": "2",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002399.1.16",
          "form": "응 신세대 노가다야",
          "original_form": "응 신세대 노가다야",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002399.1.17",
          "form": "하지만 집에서 할 일 없으면",
          "original_form": "하지만 집에서 할 일 없으면",
          "speaker_id": "1",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002399.1.18",
          "form": "할만한듯",
          "original_form": "할만한듯",
          "speaker_id": "1",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002399.1.19",
          "form": "너는 그런거 할 생각 있어?",
          "original_form": "너는 그런거 할 생각 있어?",
          "speaker_id": "2",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002399.1.20",
          "form": "관심은 가는데",
          "original_form": "관심은 가는데",
          "speaker_id": "1",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002399.1.21",
          "form": "예전에 조금 알아봤다가 돈을 진짜 안주는거 보고 접었어",
          "original_form": "예전에 조금 알아봤다가 돈을 진짜 안주는거 보고 접었어",
          "speaker_id": "1",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002399.1.22",
          "form": "너는 관심가?",
          "original_form": "너는 관심가?",
          "speaker_id": "1",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002399.1.23",
          "form": "일단 관심은 가긴 해",
          "original_form": "일단 관심은 가긴 해",
          "speaker_id": "2",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002399.1.24",
          "form": "한번 해볼까 싶어",
          "original_form": "한번 해볼까 싶어",
          "speaker_id": "2",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002399.1.25",
          "form": "그래 그럼 잘 찾아보고 해",
          "original_form": "그래 그럼 잘 찾아보고 해",
          "speaker_id": "1",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002399.1.26",
          "form": "사기도 있는것 같아",
          "original_form": "사기도 있는것 같아",
          "speaker_id": "1",
          "time": "20210820 11:13"
        }
      ]
    }
  ]
