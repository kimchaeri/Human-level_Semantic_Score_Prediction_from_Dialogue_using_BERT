      "utterance": [
        {
          "id": "MDRW2100031645.1.1",
          "form": "그러게 ㄷㄷ",
          "original_form": "그러게 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031645.1.2",
          "form": "아 나 진짜",
          "original_form": "아 나 진짜",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031645.1.3",
          "form": "내일 사장이 한번만 더 건드리면",
          "original_form": "내일 사장이 한번만 더 건드리면",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.5",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.6",
          "form": "진짜 당일 통보 퇴사할꺼임",
          "original_form": "진짜 당일 통보 퇴사할꺼임",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.7",
          "form": "내가 하삼동 있을때도",
          "original_form": "내가 하삼동 있을때도",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.8",
          "form": "사장땜에 개개개개개개",
          "original_form": "사장땜에 개개개개개개",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.9",
          "form": "ㅇㅋㅇㅋ 가자",
          "original_form": "ㅇㅋㅇㅋ 가자",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.10",
          "form": "사장땜에",
          "original_form": "사장땜에",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.11",
          "form": "진짜 화난적은 많아도",
          "original_form": "진짜 화난적은 많아도",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.12",
          "form": "이렇게 울것 같은 기분을 느낀적은 없거든",
          "original_form": "이렇게 울것 같은 기분을 느낀적은 없거든",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031645.1.13",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031645.1.14",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031645.1.15",
          "form": "진짜 너무 짜증남",
          "original_form": "진짜 너무 짜증남",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031645.1.16",
          "form": "그런사장은 당해봐야함",
          "original_form": "그런사장은 당해봐야함",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031645.1.17",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031645.1.18",
          "form": "근데 새로 오는 사람들도",
          "original_form": "근데 새로 오는 사람들도",
          "speaker_id": "2",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031645.1.19",
          "form": "다 하루 나오고 안나오고",
          "original_form": "다 하루 나오고 안나오고",
          "speaker_id": "2",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031645.1.20",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031645.1.21",
          "form": "그정도면",
          "original_form": "그정도면",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031645.1.22",
          "form": "있던 사람도 내일부터 못나가요~",
          "original_form": "있던 사람도 내일부터 못나가요~",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031645.1.23",
          "form": "이러거든",
          "original_form": "이러거든",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031645.1.24",
          "form": "내가 망하라고 고사안지내도",
          "original_form": "내가 망하라고 고사안지내도",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031645.1.25",
          "form": "알아서 망하겠네",
          "original_form": "알아서 망하겠네",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031645.1.26",
          "form": "너무좋다 ㅎㅎ",
          "original_form": "너무좋다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031645.1.27",
          "form": "진짜 사장 자기 문제인거 알아야함",
          "original_form": "진짜 사장 자기 문제인거 알아야함",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031645.1.28",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
