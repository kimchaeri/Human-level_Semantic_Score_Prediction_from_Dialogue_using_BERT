      "utterance": [
        {
          "id": "MDRW2100011071.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20210824 00:33"
        },
        {
          "id": "MDRW2100011071.1.2",
          "form": "나 요즘 알바 구하고 잇잖아.",
          "original_form": "나 요즘 알바 구하고 잇잖아.",
          "speaker_id": "2",
          "time": "20210824 00:33"
        },
        {
          "id": "MDRW2100011071.1.3",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "2",
          "time": "20210824 00:33"
        },
        {
          "id": "MDRW2100011071.1.4",
          "form": "알아??",
          "original_form": "알아??",
          "speaker_id": "2",
          "time": "20210824 00:33"
        },
        {
          "id": "MDRW2100011071.1.5",
          "form": "응응 알지 건너건너 들었어",
          "original_form": "응응 알지 건너건너 들었어",
          "speaker_id": "1",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.6",
          "form": "너 알바 많이 해봤어?",
          "original_form": "너 알바 많이 해봤어?",
          "speaker_id": "2",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.7",
          "form": "너의 경험이 듣고싶어",
          "original_form": "너의 경험이 듣고싶어",
          "speaker_id": "2",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.8",
          "form": "음 조금해본것 같아",
          "original_form": "음 조금해본것 같아",
          "speaker_id": "1",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.9",
          "form": "오 어떤 걸 해봤어?",
          "original_form": "오 어떤 걸 해봤어?",
          "speaker_id": "2",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.10",
          "form": "나는 학원 재시쌤 알바도 해봤어",
          "original_form": "나는 학원 재시쌤 알바도 해봤어",
          "speaker_id": "1",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.11",
          "form": "어떻게해야 뽑힐 수 잇는걸까?",
          "original_form": "어떻게해야 뽑힐 수 잇는걸까?",
          "speaker_id": "2",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.12",
          "form": "우와 신기하다.",
          "original_form": "우와 신기하다.",
          "speaker_id": "2",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.13",
          "form": "그 알바 재밌어?",
          "original_form": "그 알바 재밌어?",
          "speaker_id": "2",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.14",
          "form": "흠 아무래도 공고를 빨리 봐야하는 것 같아",
          "original_form": "흠 아무래도 공고를 빨리 봐야하는 것 같아",
          "speaker_id": "1",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.15",
          "form": "응 나름 앉아서 하는거고",
          "original_form": "응 나름 앉아서 하는거고",
          "speaker_id": "1",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.16",
          "form": "부지런했구나 너는.",
          "original_form": "부지런했구나 너는.",
          "speaker_id": "2",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.17",
          "form": "내 시간도 많아",
          "original_form": "내 시간도 많아",
          "speaker_id": "1",
          "time": "20210824 00:34"
        },
        {
          "id": "MDRW2100011071.1.18",
          "form": "그리고 프린터를 무료로 쓸 수 있어",
          "original_form": "그리고 프린터를 무료로 쓸 수 있어",
          "speaker_id": "1",
          "time": "20210824 00:35"
        },
        {
          "id": "MDRW2100011071.1.19",
          "form": "급료는 만족스러웠니?",
          "original_form": "급료는 만족스러웠니?",
          "speaker_id": "2",
          "time": "20210824 00:35"
        },
        {
          "id": "MDRW2100011071.1.20",
          "form": "그것 참 좋구나.",
          "original_form": "그것 참 좋구나.",
          "speaker_id": "2",
          "time": "20210824 00:35"
        },
        {
          "id": "MDRW2100011071.1.21",
          "form": "응 그때 최저보다 조금 더 줬거든",
          "original_form": "응 그때 최저보다 조금 더 줬거든",
          "speaker_id": "1",
          "time": "20210824 00:35"
        },
        {
          "id": "MDRW2100011071.1.22",
          "form": "하지만 모자를 쓰고가서 짤렸러",
          "original_form": "하지만 모자를 쓰고가서 짤렸러",
          "speaker_id": "1",
          "time": "20210824 00:35"
        }
      ]
