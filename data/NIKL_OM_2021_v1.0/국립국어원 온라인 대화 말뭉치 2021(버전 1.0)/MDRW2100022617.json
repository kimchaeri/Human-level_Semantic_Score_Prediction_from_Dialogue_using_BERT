      "utterance": [
        {
          "id": "MDRW2100022617.1.1",
          "form": "사케 같은가요?",
          "original_form": "사케 같은가요?",
          "speaker_id": "2",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022617.1.2",
          "form": "어 맞아요 ㅎㅎ",
          "original_form": "어 맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.3",
          "form": "이름이 생각 안났는데 딱 말씀해주셨네요",
          "original_form": "이름이 생각 안났는데 딱 말씀해주셨네요",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.4",
          "form": "한번맛보긴햇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "한번맛보긴햇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.5",
          "form": "저 그거 처음에 우유인 줄 알았어요 용기가 독특해서",
          "original_form": "저 그거 처음에 우유인 줄 알았어요 용기가 독특해서",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.6",
          "form": "근대 도수 넘쎄고 그렇더라구요",
          "original_form": "근대 도수 넘쎄고 그렇더라구요",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.7",
          "form": "소주도 못먹는타입이라 ㅋㅋㅋㅋㅋ",
          "original_form": "소주도 못먹는타입이라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.8",
          "form": "따뜻하게 먹었어요 아니면 차갑게 먹었어요?",
          "original_form": "따뜻하게 먹었어요 아니면 차갑게 먹었어요?",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.9",
          "form": "앗 잘 기억이안나여 ㅋㅋㅋㅋ",
          "original_form": "앗 잘 기억이안나여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.10",
          "form": "선택 할 수 있더라구요 가게에서는 ㅎㅎ",
          "original_form": "선택 할 수 있더라구요 가게에서는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.11",
          "form": "신기하게 따뜩하게도 먹더라구요",
          "original_form": "신기하게 따뜩하게도 먹더라구요",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.12",
          "form": "전 따뜻하게도 먹어봤는데 되게 신기했어요",
          "original_form": "전 따뜻하게도 먹어봤는데 되게 신기했어요",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022617.1.13",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022617.1.14",
          "form": "뭔가 술맛이 덜나더라구요",
          "original_form": "뭔가 술맛이 덜나더라구요",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022617.1.15",
          "form": "그리고 그 온도 그대로 따뜻하게 위를 감싸줘요",
          "original_form": "그리고 그 온도 그대로 따뜻하게 위를 감싸줘요",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022617.1.16",
          "form": "일본술도 섞어마시는 종류잇던데",
          "original_form": "일본술도 섞어마시는 종류잇던데",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022617.1.17",
          "form": "그건 맛 괜찮앗던 기억나요 ㅋㅋㅋㅋ",
          "original_form": "그건 맛 괜찮앗던 기억나요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022617.1.18",
          "form": "허걱 폭탄주처럼요?ㅋㅋㅋ",
          "original_form": "허걱 폭탄주처럼요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022617.1.19",
          "form": "궁금하네요 뭐랑 뭐를 섞는지",
          "original_form": "궁금하네요 뭐랑 뭐를 섞는지",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022617.1.20",
          "form": "아뇨아뇨 뭐지 그냥과일소주 느낌",
          "original_form": "아뇨아뇨 뭐지 그냥과일소주 느낌",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022617.1.21",
          "form": "오~ 그런거는 저도 좋아할 것 같아요",
          "original_form": "오~ 그런거는 저도 좋아할 것 같아요",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022617.1.22",
          "form": "네 ㅠㅠ 최애 이자카야잇엇는데",
          "original_form": "네 ㅠㅠ 최애 이자카야잇엇는데",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022617.1.23",
          "form": "혹시 그런거에 인위적으로 당 많이 넣진 않겠죠ㅠ?",
          "original_form": "혹시 그런거에 인위적으로 당 많이 넣진 않겠죠ㅠ?",
          "speaker_id": "1",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022617.1.24",
          "form": "사라져서 아쉬웟던기억나네요",
          "original_form": "사라져서 아쉬웟던기억나네요",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022617.1.25",
          "form": "에이 그럼 티나지않을까요 ㅠ",
          "original_form": "에이 그럼 티나지않을까요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:26"
        }
      ]
    }
