      "utterance": [
        {
          "id": "MDRW2100019200.1.1",
          "form": "고양이들은 가끔 신기해",
          "original_form": "고양이들은 가끔 신기해",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019200.1.2",
          "form": "어떤 면이?",
          "original_form": "어떤 면이?",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019200.1.3",
          "form": "무슨 생각하는지 모르겠어",
          "original_form": "무슨 생각하는지 모르겠어",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019200.1.4",
          "form": "맞아 뚱한 표정으로",
          "original_form": "맞아 뚱한 표정으로",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019200.1.5",
          "form": "가끔은 우스운 표정을 짓기도 해",
          "original_form": "가끔은 우스운 표정을 짓기도 해",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019200.1.6",
          "form": "고양이들은 고집이 센 것 같아",
          "original_form": "고양이들은 고집이 센 것 같아",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019200.1.7",
          "form": "호불호가 뚜렷한ㅁ것 같아",
          "original_form": "호불호가 뚜렷한ㅁ것 같아",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019200.1.8",
          "form": "애교가 많은 고양이도 있어",
          "original_form": "애교가 많은 고양이도 있어",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019200.1.9",
          "form": "그런 고양이들은 참 사랑스럽지",
          "original_form": "그런 고양이들은 참 사랑스럽지",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019200.1.10",
          "form": "주인을 경계하는ㅁ고양이도 있더라",
          "original_form": "주인을 경계하는ㅁ고양이도 있더라",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019200.1.11",
          "form": "정말? 그건 좀 서러운데",
          "original_form": "정말? 그건 좀 서러운데",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019200.1.12",
          "form": "그치만 그게 고양이의 매력인걸",
          "original_form": "그치만 그게 고양이의 매력인걸",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019200.1.13",
          "form": "귀엽고 사랑스럽지",
          "original_form": "귀엽고 사랑스럽지",
          "speaker_id": "2",
          "time": "20210827 20:25"
        }
