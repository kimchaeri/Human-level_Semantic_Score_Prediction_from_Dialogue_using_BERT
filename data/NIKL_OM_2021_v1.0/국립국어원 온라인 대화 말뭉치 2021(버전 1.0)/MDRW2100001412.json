      "utterance": [
        {
          "id": "MDRW2100001412.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 14:05"
        },
        {
          "id": "MDRW2100001412.1.2",
          "form": "안녕하세요~ 요즘 날씨가 많이 쌀쌀해졌네요",
          "original_form": "안녕하세요~ 요즘 날씨가 많이 쌀쌀해졌네요",
          "speaker_id": "2",
          "time": "20211015 14:06"
        },
        {
          "id": "MDRW2100001412.1.3",
          "form": "저는 추위가 좋아 반갑네요",
          "original_form": "저는 추위가 좋아 반갑네요",
          "speaker_id": "1",
          "time": "20211015 14:06"
        },
        {
          "id": "MDRW2100001412.1.4",
          "form": "저도 더운것 보다는 추운걸 좋아해요",
          "original_form": "저도 더운것 보다는 추운걸 좋아해요",
          "speaker_id": "2",
          "time": "20211015 14:07"
        },
        {
          "id": "MDRW2100001412.1.5",
          "form": "주말에 옷장정리를 해야겠어요",
          "original_form": "주말에 옷장정리를 해야겠어요",
          "speaker_id": "2",
          "time": "20211015 14:07"
        },
        {
          "id": "MDRW2100001412.1.6",
          "form": "아하 ㅡㅡ고마워요",
          "original_form": "아하 ㅡㅡ고마워요",
          "speaker_id": "1",
          "time": "20211015 14:09"
        },
        {
          "id": "MDRW2100001412.1.7",
          "form": "가을 옷들은 꺼내셨나요?",
          "original_form": "가을 옷들은 꺼내셨나요?",
          "speaker_id": "2",
          "time": "20211015 14:10"
        },
        {
          "id": "MDRW2100001412.1.8",
          "form": "주말에 한파가 온대요",
          "original_form": "주말에 한파가 온대요",
          "speaker_id": "1",
          "time": "20211015 14:10"
        },
        {
          "id": "MDRW2100001412.1.9",
          "form": "그러게요 이러다 롱패딩만 입고다니겠어요",
          "original_form": "그러게요 이러다 롱패딩만 입고다니겠어요",
          "speaker_id": "2",
          "time": "20211015 14:10"
        },
        {
          "id": "MDRW2100001412.1.10",
          "form": "겨울을 대비해야겠어요",
          "original_form": "겨울을 대비해야겠어요",
          "speaker_id": "1",
          "time": "20211015 14:10"
        },
        {
          "id": "MDRW2100001412.1.11",
          "form": "겨울 옷은 다 준비되신건가요",
          "original_form": "겨울 옷은 다 준비되신건가요",
          "speaker_id": "2",
          "time": "20211015 14:11"
        },
        {
          "id": "MDRW2100001412.1.12",
          "form": "마침 대대장 급 사놓은게 있어 안심이에요",
          "original_form": "마침 대대장 급 사놓은게 있어 안심이에요",
          "speaker_id": "1",
          "time": "20211015 14:11"
        },
        {
          "id": "MDRW2100001412.1.13",
          "form": "저는 준비가 안되어서 걱정입니다..",
          "original_form": "저는 준비가 안되어서 걱정입니다..",
          "speaker_id": "2",
          "time": "20211015 14:12"
        },
        {
          "id": "MDRW2100001412.1.14",
          "form": "이렇게 갑자기 추워질 줄 몰랐거든요",
          "original_form": "이렇게 갑자기 추워질 줄 몰랐거든요",
          "speaker_id": "2",
          "time": "20211015 14:12"
        },
        {
          "id": "MDRW2100001412.1.15",
          "form": "새거로 하나 보내드리고 싶군요",
          "original_form": "새거로 하나 보내드리고 싶군요",
          "speaker_id": "1",
          "time": "20211015 14:12"
        },
        {
          "id": "MDRW2100001412.1.16",
          "form": "저는 이제그만 물러갈게요",
          "original_form": "저는 이제그만 물러갈게요",
          "speaker_id": "1",
          "time": "20211015 14:13"
        },
        {
          "id": "MDRW2100001412.1.17",
          "form": "좋은시간 되세요",
          "original_form": "좋은시간 되세요",
          "speaker_id": "1",
          "time": "20211015 14:13"
        },
        {
          "id": "MDRW2100001412.1.18",
          "form": "좋은하루 보내세요~!",
          "original_form": "좋은하루 보내세요~!",
          "speaker_id": "2",
          "time": "20211015 14:14"
        }
      ]
