      "utterance": [
        {
          "id": "MDRW2100041995.1.1",
          "form": "요즘 너무 비만 오는 것 같아",
          "original_form": "요즘 너무 비만 오는 것 같아",
          "speaker_id": "1",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041995.1.2",
          "form": "거기도 비와??",
          "original_form": "거기도 비와??",
          "speaker_id": "1",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041995.1.3",
          "form": "여기도 요즘 자주 옴",
          "original_form": "여기도 요즘 자주 옴",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041995.1.4",
          "form": "지금은 안오는데",
          "original_form": "지금은 안오는데",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041995.1.5",
          "form": "아까 낮에 잠깐 왔었음",
          "original_form": "아까 낮에 잠깐 왔었음",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041995.1.6",
          "form": "거기는? 지금 비와?",
          "original_form": "거기는? 지금 비와?",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100041995.1.7",
          "form": "아까까지 오다가 지금은 우중충하기만 해",
          "original_form": "아까까지 오다가 지금은 우중충하기만 해",
          "speaker_id": "1",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041995.1.8",
          "form": "앞머리 자르고 싶은데 요즘 너무 습해서 못 자르겠어",
          "original_form": "앞머리 자르고 싶은데 요즘 너무 습해서 못 자르겠어",
          "speaker_id": "1",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041995.1.9",
          "form": "너무 습하니까 머리 볼륨이 금방 죽어버려",
          "original_form": "너무 습하니까 머리 볼륨이 금방 죽어버려",
          "speaker_id": "1",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041995.1.10",
          "form": "아 맞아.... 습한 날엔 또 그렇지",
          "original_form": "아 맞아.... 습한 날엔 또 그렇지",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041995.1.11",
          "form": "그래도 요즘 비와서 좀 시원해진듯",
          "original_form": "그래도 요즘 비와서 좀 시원해진듯",
          "speaker_id": "2",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041995.1.12",
          "form": "맞아 나 오늘 드디어 긴팔 입었잖아",
          "original_form": "맞아 나 오늘 드디어 긴팔 입었잖아",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041995.1.13",
          "form": "덥지 않아서 좋아",
          "original_form": "덥지 않아서 좋아",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041995.1.14",
          "form": "맞음맞음",
          "original_form": "맞음맞음",
          "speaker_id": "2",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041995.1.15",
          "form": "난 오늘 반팔에 체크남방 레이어드 해서 입었음",
          "original_form": "난 오늘 반팔에 체크남방 레이어드 해서 입었음",
          "speaker_id": "2",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100041995.1.16",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041995.1.17",
          "form": "나는 그냥 청바지에 까만 긴팔 입었어",
          "original_form": "나는 그냥 청바지에 까만 긴팔 입었어",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041995.1.18",
          "form": "근데 비는 그만 왔으면 좋겠다",
          "original_form": "근데 비는 그만 왔으면 좋겠다",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041995.1.19",
          "form": "우린 비가 엄청 내라는 시기는 지난것같아",
          "original_form": "우린 비가 엄청 내라는 시기는 지난것같아",
          "speaker_id": "2",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041995.1.20",
          "form": "이제 조금조금 내려",
          "original_form": "이제 조금조금 내려",
          "speaker_id": "2",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041995.1.21",
          "form": "그게 더 싫어...",
          "original_form": "그게 더 싫어...",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041995.1.22",
          "form": "우산 들고다니기 귀찮은데",
          "original_form": "우산 들고다니기 귀찮은데",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041995.1.23",
          "form": "그냥 비 맞기엔 뭔가 찝찝해",
          "original_form": "그냥 비 맞기엔 뭔가 찝찝해",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041995.1.24",
          "form": "맞아 맞아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아 맞아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041995.1.25",
          "form": "그래도 수요일엔 비 안온대",
          "original_form": "그래도 수요일엔 비 안온대",
          "speaker_id": "1",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100041995.1.26",
          "form": "오.... 잘됐당",
          "original_form": "오.... 잘됐당",
          "speaker_id": "2",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100041995.1.27",
          "form": "거기는 어때??",
          "original_form": "거기는 어때??",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100041995.1.28",
          "form": "그때 비 안 온대?",
          "original_form": "그때 비 안 온대?",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100041995.1.29",
          "form": "당분간 비소식이 없긴한데...",
          "original_form": "당분간 비소식이 없긴한데...",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100041995.1.30",
          "form": "비소식 없을때도 잠깐 비온적있어서",
          "original_form": "비소식 없을때도 잠깐 비온적있어서",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100041995.1.31",
          "form": "잘 모르겠엉 비올지 안올지...",
          "original_form": "잘 모르겠엉 비올지 안올지...",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100041995.1.32",
          "form": "맞아...요즘 비 안 온다고 해도 오더라",
          "original_form": "맞아...요즘 비 안 온다고 해도 오더라",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100041995.1.33",
          "form": "비 온다고 해도 안오고",
          "original_form": "비 온다고 해도 안오고",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100041995.1.34",
          "form": "맞아.... 짱 너무함",
          "original_form": "맞아.... 짱 너무함",
          "speaker_id": "2",
          "time": "20210906 18:45"
        }
      ]
    }
