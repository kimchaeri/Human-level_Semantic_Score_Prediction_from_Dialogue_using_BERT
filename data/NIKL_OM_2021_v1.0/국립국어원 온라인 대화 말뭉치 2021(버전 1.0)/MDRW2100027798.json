      "utterance": [
        {
          "id": "MDRW2100027798.1.1",
          "form": "웅 우리엄마 감기 걸려서",
          "original_form": "웅 우리엄마 감기 걸려서",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027798.1.2",
          "form": "이번에 대추고랑 생강청으로 차끓여먹었어",
          "original_form": "이번에 대추고랑 생강청으로 차끓여먹었어",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027798.1.3",
          "form": "이제 거의 나았다는데",
          "original_form": "이제 거의 나았다는데",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027798.1.4",
          "form": "에구 고생하셨네",
          "original_form": "에구 고생하셨네",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027798.1.5",
          "form": "감기걸리면 보통 죽도 먹지않나",
          "original_form": "감기걸리면 보통 죽도 먹지않나",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027798.1.6",
          "form": "요즘은 죽도 배달되더라고",
          "original_form": "요즘은 죽도 배달되더라고",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027798.1.7",
          "form": "우리집에 죽이 남어있었어",
          "original_form": "우리집에 죽이 남어있었어",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027798.1.8",
          "form": "옺닭 알아?",
          "original_form": "옺닭 알아?",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027798.1.9",
          "form": "아니 처음 들어봐",
          "original_form": "아니 처음 들어봐",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027798.1.10",
          "form": "닭죽이야?",
          "original_form": "닭죽이야?",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027798.1.11",
          "form": "옺이라는 나무 달여서 만든건데 이거 잘못 먹으면",
          "original_form": "옺이라는 나무 달여서 만든건데 이거 잘못 먹으면",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027798.1.12",
          "form": "독올라서 먹을 수 있는 사람만 먹을 수 있음",
          "original_form": "독올라서 먹을 수 있는 사람만 먹을 수 있음",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027798.1.13",
          "form": "우리집은 초중말복에 옺물로 닭끓여서 먹어",
          "original_form": "우리집은 초중말복에 옺물로 닭끓여서 먹어",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027798.1.14",
          "form": "오!!신기~~",
          "original_form": "오!!신기~~",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027798.1.15",
          "form": "우리집도 옺을 넣나 모르겠네",
          "original_form": "우리집도 옺을 넣나 모르겠네",
          "speaker_id": "2",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027798.1.16",
          "form": "다음에 물어봐야지",
          "original_form": "다음에 물어봐야지",
          "speaker_id": "2",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027798.1.17",
          "form": "죽하니까 참치죽먹고싶어 흑흑",
          "original_form": "죽하니까 참치죽먹고싶어 흑흑",
          "speaker_id": "2",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027798.1.18",
          "form": "본죽 맛있는데",
          "original_form": "본죽 맛있는데",
          "speaker_id": "2",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027798.1.19",
          "form": "난 어릴때부터 이렇게 먹어서 문제된적은 없는데",
          "original_form": "난 어릴때부터 이렇게 먹어서 문제된적은 없는데",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027798.1.20",
          "form": "심하게 오르는 사람은 진물나고 두드러기 쩐다더라 알레르기성 반응 심해서",
          "original_form": "심하게 오르는 사람은 진물나고 두드러기 쩐다더라 알레르기성 반응 심해서",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027798.1.21",
          "form": "본죽 조아",
          "original_form": "본죽 조아",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027798.1.22",
          "form": "아 되게 조심해야되네",
          "original_form": "아 되게 조심해야되네",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027798.1.23",
          "form": "맞아 글서 함부로 먹으면 안돼",
          "original_form": "맞아 글서 함부로 먹으면 안돼",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027798.1.24",
          "form": "참치죽도 맛있겠다",
          "original_form": "참치죽도 맛있겠다",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027798.1.25",
          "form": "떡볶이 먹고싶다. 신전떡볶이~",
          "original_form": "떡볶이 먹고싶다. 신전떡볶이~",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027798.1.26",
          "form": "튀김찍어먹으면 짱맛",
          "original_form": "튀김찍어먹으면 짱맛",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027798.1.27",
          "form": "난 크로플 먹고 싶어",
          "original_form": "난 크로플 먹고 싶어",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027798.1.28",
          "form": "내일 백신 맞고 내일 아래에 있는 카페에서 주문할거야",
          "original_form": "내일 백신 맞고 내일 아래에 있는 카페에서 주문할거야",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027798.1.29",
          "form": "난 닭고기 안된대서 지금 개슬프게 집가는중",
          "original_form": "난 닭고기 안된대서 지금 개슬프게 집가는중",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027798.1.30",
          "form": "뚜붝...",
          "original_form": "뚜붝...",
          "speaker_id": "2",
          "time": "20210902 16:37"
        }
      ]
    }
