      "utterance": [
        {
          "id": "MDRW2100007197.1.1",
          "form": "내일 점심 뭐먹을까?",
          "original_form": "내일 점심 뭐먹을까?",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007197.1.2",
          "form": "저는 나가서 먹어요 ㅎㅎ",
          "original_form": "저는 나가서 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.4",
          "form": "뭐먹을껀데",
          "original_form": "뭐먹을껀데",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.5",
          "form": "그럼 내 점심 골라주고 나가",
          "original_form": "그럼 내 점심 골라주고 나가",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.6",
          "form": "안해먹을 거 같으니 그냥 배달음식 먹어",
          "original_form": "안해먹을 거 같으니 그냥 배달음식 먹어",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.7",
          "form": "아무도 없으면 비빔면이지 뭐...",
          "original_form": "아무도 없으면 비빔면이지 뭐...",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.8",
          "form": "피자...",
          "original_form": "피자...",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.9",
          "form": "배달음식 먹을 것도 없어",
          "original_form": "배달음식 먹을 것도 없어",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.10",
          "form": "피자 나 대신 먹어줘",
          "original_form": "피자 나 대신 먹어줘",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.11",
          "form": "피자 맛있겠네",
          "original_form": "피자 맛있겠네",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007197.1.12",
          "form": "금요일에 먹어야돼",
          "original_form": "금요일에 먹어야돼",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.13",
          "form": "다이어트하는 나 대신 피자먹어줘..",
          "original_form": "다이어트하는 나 대신 피자먹어줘..",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.14",
          "form": "오ㅑ",
          "original_form": "오ㅑ",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.15",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.16",
          "form": "파파존스 방문포장하면 1+1",
          "original_form": "파파존스 방문포장하면 1+1",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.18",
          "form": "근처에 파파존스 없어요",
          "original_form": "근처에 파파존스 없어요",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.19",
          "form": "존스페이버릿 맛나요",
          "original_form": "존스페이버릿 맛나요",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.20",
          "form": "회사에서 먹을껀데 ㅋㅋㅋㅋ",
          "original_form": "회사에서 먹을껀데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.21",
          "form": "내일 뭐 드심??",
          "original_form": "내일 뭐 드심??",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007197.1.22",
          "form": "결정함???",
          "original_form": "결정함???",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007197.1.23",
          "form": "존스 페이버릿이 뭔데",
          "original_form": "존스 페이버릿이 뭔데",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007197.1.24",
          "form": "피자 이름 ㅋㅋ",
          "original_form": "피자 이름 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007197.1.25",
          "form": "나 파파존스 먹어본 기억이 없어....",
          "original_form": "나 파파존스 먹어본 기억이 없어....",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007197.1.26",
          "form": "파파존스 피자 메뉴",
          "original_form": "파파존스 피자 메뉴",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007197.1.27",
          "form": "나 버리고 맛있는거 먹으러 다니는구나",
          "original_form": "나 버리고 맛있는거 먹으러 다니는구나",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007197.1.28",
          "form": "내 파파존스 최애",
          "original_form": "내 파파존스 최애",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007197.1.29",
          "form": "동생은 너무 슬프다",
          "original_form": "동생은 너무 슬프다",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007197.1.30",
          "form": "회사에서 맛난거라도 먹어야 버티지",
          "original_form": "회사에서 맛난거라도 먹어야 버티지",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007197.1.31",
          "form": "하지만 맛있는거 먹으면 점심값 너무 많이 나와",
          "original_form": "하지만 맛있는거 먹으면 점심값 너무 많이 나와",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.32",
          "form": "진심",
          "original_form": "진심",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.33",
          "form": "친구 스콘 좋아하니???",
          "original_form": "친구 스콘 좋아하니???",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.34",
          "form": "요즘 밖에서 못먹어서 소비 초과임",
          "original_form": "요즘 밖에서 못먹어서 소비 초과임",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.35",
          "form": "내가 좋아해 ^^",
          "original_form": "내가 좋아해 ^^",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.36",
          "form": "배달 음식 점심값 장난아님",
          "original_form": "배달 음식 점심값 장난아님",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.37",
          "form": "다이어트 스콘은 비추...",
          "original_form": "다이어트 스콘은 비추...",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.38",
          "form": "더현대에 레이어드라고 스콘집있음",
          "original_form": "더현대에 레이어드라고 스콘집있음",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.39",
          "form": "너무 달고... 퍽퍽하고....",
          "original_form": "너무 달고... 퍽퍽하고....",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.40",
          "form": "내일 사올까?",
          "original_form": "내일 사올까?",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.41",
          "form": "내가 저번에 가져오기는 함 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "내가 저번에 가져오기는 함 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.42",
          "form": "앗 구럼 다른거...",
          "original_form": "앗 구럼 다른거...",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007197.1.43",
          "form": "그 마늘크림 들어갔던거",
          "original_form": "그 마늘크림 들어갔던거",
