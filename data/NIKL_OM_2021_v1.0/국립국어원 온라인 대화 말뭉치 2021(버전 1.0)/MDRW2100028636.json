      "utterance": [
        {
          "id": "MDRW2100028636.1.1",
          "form": "넹 씁쓸해하다가",
          "original_form": "넹 씁쓸해하다가",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028636.1.2",
          "form": "저두 이제 남친 생겻으니",
          "original_form": "저두 이제 남친 생겻으니",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028636.1.3",
          "form": "저는 예전에 솔로일때 소개팅받았던 사람이",
          "original_form": "저는 예전에 솔로일때 소개팅받았던 사람이",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028636.1.4",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028636.1.5",
          "form": "반년만인가?",
          "original_form": "반년만인가?",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028636.1.6",
          "form": "아이생겨서 결혼했던데",
          "original_form": "아이생겨서 결혼했던데",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028636.1.7",
          "form": "앜ㅋ",
          "original_form": "앜ㅋ",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028636.1.8",
          "form": "엄청 놀랐어요",
          "original_form": "엄청 놀랐어요",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028636.1.9",
          "form": "이럴수가 있나",
          "original_form": "이럴수가 있나",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028636.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028636.1.11",
          "form": "소개팅 한 사람의 결혼소식도 깜놀하게 되자나요",
          "original_form": "소개팅 한 사람의 결혼소식도 깜놀하게 되자나요",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028636.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028636.1.13",
          "form": "보통은 모르는데",
          "original_form": "보통은 모르는데",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028636.1.14",
          "form": "번호정리하다가",
          "original_form": "번호정리하다가",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028636.1.15",
          "form": "저도 첫 소개팅 했던 사람 대화목록이 pc 카톡에 남아잇길래 한번 근황봤는데 결혼했더라구욬ㅋㅋㅋㅋ",
          "original_form": "저도 첫 소개팅 했던 사람 대화목록이 pc 카톡에 남아잇길래 한번 근황봤는데 결혼했더라구욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028636.1.16",
          "form": "카톡으로 알게된...",
          "original_form": "카톡으로 알게된...",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028636.1.17",
          "form": "오 비슷 ㅋㅋㅋㅋ",
          "original_form": "오 비슷 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028636.1.18",
          "form": "ㅋㅋㅋㅋㅋ그런 경우가 많나봐요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그런 경우가 많나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028636.1.19",
          "form": "진짜 결혼이 급한 사람이었구나..",
          "original_form": "진짜 결혼이 급한 사람이었구나..",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028636.1.20",
          "form": "많죠 ㅋㅋ 나이가 있다보니",
          "original_form": "많죠 ㅋㅋ 나이가 있다보니",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028636.1.21",
          "form": "네 그러니 6년 사귄 남친의 결혼소식은",
          "original_form": "네 그러니 6년 사귄 남친의 결혼소식은",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028636.1.22",
          "form": "아무리 미련이 없더라도",
          "original_form": "아무리 미련이 없더라도",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028636.1.23",
          "form": "놀라운 소식 ㅋㅋㅋㅋ",
          "original_form": "놀라운 소식 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028636.1.24",
          "form": "기분 이상하지 않아요?",
          "original_form": "기분 이상하지 않아요?",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028636.1.25",
          "form": "이상했죠",
          "original_form": "이상했죠",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028636.1.26",
          "form": "왜냐면 결혼생각이 없다 그러고 헤어졌거든요!!",
          "original_form": "왜냐면 결혼생각이 없다 그러고 헤어졌거든요!!",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028636.1.27",
          "form": "저 전여친이랑 결혼직전에 헤어졌는데",
