      "utterance": [
        {
          "id": "MDRW2100007840.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 11:16"
        },
        {
          "id": "MDRW2100007840.1.2",
          "form": "안녕하세요~~ 요즘 친구들 이상형은 어떻게 돼요?",
          "original_form": "안녕하세요~~ 요즘 친구들 이상형은 어떻게 돼요?",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007840.1.3",
          "form": "요즘은 이상형보다",
          "original_form": "요즘은 이상형보다",
          "speaker_id": "1",
          "time": "20210822 11:23"
        },
        {
          "id": "MDRW2100007840.1.4",
          "form": "사상이 이상하지 않은 사람을 찾는거 같아요",
          "original_form": "사상이 이상하지 않은 사람을 찾는거 같아요",
          "speaker_id": "1",
          "time": "20210822 11:23"
        },
        {
          "id": "MDRW2100007840.1.5",
          "form": "호~ 중요하죠 ㅎㅎ",
          "original_form": "호~ 중요하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007840.1.6",
          "form": "연예인으로 치면 어떤 외모가 좋아요?",
          "original_form": "연예인으로 치면 어떤 외모가 좋아요?",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007840.1.7",
          "form": "저는 귀엽게 생긴 사람 좋아해요",
          "original_form": "저는 귀엽게 생긴 사람 좋아해요",
          "speaker_id": "1",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007840.1.8",
          "form": "주변 친구들도 그렇고",
          "original_form": "주변 친구들도 그렇고",
          "speaker_id": "1",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100007840.1.9",
          "form": "아아 막 그러자나요 ㅋㅋㅋ 사람이 귀여워보이면 답이 없는거라고 ㅋㅋㅋ",
          "original_form": "아아 막 그러자나요 ㅋㅋㅋ 사람이 귀여워보이면 답이 없는거라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100007840.1.10",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007840.1.11",
          "form": "멋진걸 넘어서 귀여워보이면",
          "original_form": "멋진걸 넘어서 귀여워보이면",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007840.1.12",
          "form": "모든 순간이 귀여워 보인다고ㅋㅋ",
          "original_form": "모든 순간이 귀여워 보인다고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007840.1.13",
          "form": "저는 좀 맛있어보이는 사람의 빈틈이 보일때 좋더라구요 ㅋㅋㅋ",
          "original_form": "저는 좀 맛있어보이는 사람의 빈틈이 보일때 좋더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007840.1.14",
          "form": "갭차이 좋죠",
          "original_form": "갭차이 좋죠",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007840.1.15",
          "form": "양면성이 있어야 더 매력적이기도 하구요",
          "original_form": "양면성이 있어야 더 매력적이기도 하구요",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007840.1.16",
          "form": "맞아요 너무 헐랭하기만 하면 별로..",
          "original_form": "맞아요 너무 헐랭하기만 하면 별로..",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007840.1.17",
          "form": "빈틈없어보여도 부담...",
          "original_form": "빈틈없어보여도 부담...",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007840.1.18",
          "form": "ㅋㅋ저희 취향이 비슷한가봐요",
          "original_form": "ㅋㅋ저희 취향이 비슷한가봐요",
          "speaker_id": "1",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007840.1.19",
          "form": "이성적이 아니고 인간적으로 생각해도 사실 비슷한거 같아요 ㅎㅎ",
          "original_form": "이성적이 아니고 인간적으로 생각해도 사실 비슷한거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100007840.1.20",
          "form": "혹시 성격면 말고",
          "original_form": "혹시 성격면 말고",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007840.1.21",
          "form": "외모에 대한 취향 있으세요?",
          "original_form": "외모에 대한 취향 있으세요?",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007840.1.22",
          "form": "저는 사실 대중없는거 같은데 잘생긴사람 좋아해욬ㅋㅋㅋ",
          "original_form": "저는 사실 대중없는거 같은데 잘생긴사람 좋아해욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007840.1.23",
          "form": "아니면 자주봐서 익숙해진사람..... 익숙해지면 친근감느낀다잖아요 ㅎㅎ",
          "original_form": "아니면 자주봐서 익숙해진사람..... 익숙해지면 친근감느낀다잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100007840.1.24",
          "form": "원래 키도 엄청봅니당...",
          "original_form": "원래 키도 엄청봅니당...",
          "speaker_id": "2",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100007840.1.25",
          "form": "저도 키 많이 봐요",
          "original_form": "저도 키 많이 봐요",
          "speaker_id": "1",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100007840.1.26",
          "form": "잘생겨도 키가 작으면 좀...ㅋㅋ",
          "original_form": "잘생겨도 키가 작으면 좀...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100007840.1.27",
          "form": "맞아여.. 갑자기 허경환이 생각나네요..",
          "original_form": "맞아여.. 갑자기 허경환이 생각나네요..",
          "speaker_id": "2",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007840.1.28",
          "form": "허경환님... ㅋㅋ 죄송합니다",
          "original_form": "허경환님... ㅋㅋ 죄송합니다",
          "speaker_id": "1",
