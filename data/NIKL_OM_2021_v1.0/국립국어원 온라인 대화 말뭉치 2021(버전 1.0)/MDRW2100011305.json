      "utterance": [
        {
          "id": "MDRW2100011305.1.1",
          "form": "식사 하셨나요",
          "original_form": "식사 하셨나요",
          "speaker_id": "1",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100011305.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210824 10:54"
        },
        {
          "id": "MDRW2100011305.1.3",
          "form": "점심메뉴는 정하셨나요?",
          "original_form": "점심메뉴는 정하셨나요?",
          "speaker_id": "2",
          "time": "20210824 10:54"
        },
        {
          "id": "MDRW2100011305.1.4",
          "form": "아직 정하지 못했어요",
          "original_form": "아직 정하지 못했어요",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100011305.1.5",
          "form": "밤새님은요?",
          "original_form": "밤새님은요?",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100011305.1.6",
          "form": "저는 백반을 먹게 될 거 같아요.",
          "original_form": "저는 백반을 먹게 될 거 같아요.",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100011305.1.7",
          "form": "백반 맛있겠다",
          "original_form": "백반 맛있겠다",
          "speaker_id": "1",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100011305.1.8",
          "form": "반찬 중 좋아하는거 있나요?",
          "original_form": "반찬 중 좋아하는거 있나요?",
          "speaker_id": "1",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100011305.1.9",
          "form": "한식은 너무 많잖아요?",
          "original_form": "한식은 너무 많잖아요?",
          "speaker_id": "2",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100011305.1.10",
          "form": "자주 가는 백반집은 깻잎장아찌가 맛있어요",
          "original_form": "자주 가는 백반집은 깻잎장아찌가 맛있어요",
          "speaker_id": "2",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100011305.1.11",
          "form": "깻잎장아찌 맛있어요",
          "original_form": "깻잎장아찌 맛있어요",
          "speaker_id": "1",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100011305.1.12",
          "form": "완전 밥도둑",
          "original_form": "완전 밥도둑",
          "speaker_id": "1",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100011305.1.13",
          "form": "맞아요. 향도 좋고 짭조름해서ㅋㅋ",
          "original_form": "맞아요. 향도 좋고 짭조름해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011305.1.14",
          "form": "고기반찬이 제일 좋긴 하지만요",
          "original_form": "고기반찬이 제일 좋긴 하지만요",
          "speaker_id": "2",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011305.1.15",
          "form": "어떤 고기 좋아하세요",
          "original_form": "어떤 고기 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011305.1.16",
          "form": "저는 돼지고기 좋아해요",
          "original_form": "저는 돼지고기 좋아해요",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011305.1.17",
          "form": "저도요. 삼겹살을 제일 좋아해요.",
          "original_form": "저도요. 삼겹살을 제일 좋아해요.",
          "speaker_id": "2",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011305.1.18",
          "form": "밥이랑 먹을땐 제육볶음이 좋네요",
          "original_form": "밥이랑 먹을땐 제육볶음이 좋네요",
          "speaker_id": "2",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100011305.1.19",
          "form": "삼겹살도 제육볶음도 너무 맛있는 반찬이에요",
          "original_form": "삼겹살도 제육볶음도 너무 맛있는 반찬이에요",
          "speaker_id": "1",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100011305.1.20",
          "form": "혹시 양념갈비도 좋아하시나요",
          "original_form": "혹시 양념갈비도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100011305.1.21",
          "form": "너무 좋아하죠ㅋㅋ 굽기가 어렵긴 하지만요",
          "original_form": "너무 좋아하죠ㅋㅋ 굽기가 어렵긴 하지만요",
          "speaker_id": "2",
          "time": "20210824 11:02"
        },
        {
          "id": "MDRW2100011305.1.22",
          "form": "맞아요 잘못하면 타버리니까",
          "original_form": "맞아요 잘못하면 타버리니까",
          "speaker_id": "1",
          "time": "20210824 11:02"
        }
      ]
