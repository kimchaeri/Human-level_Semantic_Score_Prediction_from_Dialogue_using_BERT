      "utterance": [
        {
          "id": "MDRW2100000734.1.1",
          "form": "캣타워사둬봤자 또 안써요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "캣타워사둬봤자 또 안써요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000734.1.2",
          "form": "비싼거 맘먹고 사줬는데 처음몇번쓰더니",
          "original_form": "비싼거 맘먹고 사줬는데 처음몇번쓰더니",
          "speaker_id": "1",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000734.1.3",
          "form": "장롱에만올라갑니다 ㅋㅋㅋㅋ",
          "original_form": "장롱에만올라갑니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000734.1.4",
          "form": "저녁먹고 설거지까지 하고 오느라 늦었어요",
          "original_form": "저녁먹고 설거지까지 하고 오느라 늦었어요",
          "speaker_id": "2",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000734.1.5",
          "form": "저녁  드셨나요?",
          "original_form": "저녁  드셨나요?",
          "speaker_id": "2",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000734.1.6",
          "form": "네 저도 이제 치우고있어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 저도 이제 치우고있어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000734.1.7",
          "form": "오늘 넘 뭔가 바쁘네요 ㅋㅋㅋㅋㅋ",
          "original_form": "오늘 넘 뭔가 바쁘네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000734.1.8",
          "form": "치우고 만나봅시다 ㅋㅋ",
          "original_form": "치우고 만나봅시다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000734.1.9",
          "form": "숙제같은데 재밌네요 수다떠니까",
          "original_form": "숙제같은데 재밌네요 수다떠니까",
          "speaker_id": "2",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000734.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그니가여 오랜만의 수다타임",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그니가여 오랜만의 수다타임",
          "speaker_id": "1",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000734.1.11",
          "form": "어릴때 키워보셨던 동물있나여",
          "original_form": "어릴때 키워보셨던 동물있나여",
          "speaker_id": "1",
          "time": "20210819 19:46"
        },
        {
          "id": "MDRW2100000734.1.12",
          "form": "물고기나 할머니집에서 닭이나 ㅎㄹ",
          "original_form": "물고기나 할머니집에서 닭이나 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210819 19:46"
        },
        {
          "id": "MDRW2100000734.1.13",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 19:46"
        },
        {
          "id": "MDRW2100000734.1.14",
          "form": "전 집 마당에서 개도많이 키웠고",
          "original_form": "전 집 마당에서 개도많이 키웠고",
          "speaker_id": "2",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000734.1.15",
          "form": "방에서 토끼도 키웟고",
          "original_form": "방에서 토끼도 키웟고",
          "speaker_id": "2",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000734.1.16",
          "form": "거북이 키우다 방생해줬고요",
          "original_form": "거북이 키우다 방생해줬고요",
          "speaker_id": "2",
          "time": "20210819 19:48"
        },
        {
          "id": "MDRW2100000734.1.17",
          "form": "닭이랑 염소 소는 예삿일이죠 ㅋㅋㅋ",
          "original_form": "닭이랑 염소 소는 예삿일이죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000734.1.18",
          "form": "여름님은요?",
          "original_form": "여름님은요?",
          "speaker_id": "2",
          "time": "20210819 19:49"
        },
        {
          "id": "MDRW2100000734.1.19",
          "form": "와 엄청많이키우셨네요",
          "original_form": "와 엄청많이키우셨네요",
          "speaker_id": "1",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000734.1.20",
          "form": "전 어릴때 햄스터 병아리",
          "original_form": "전 어릴때 햄스터 병아리",
          "speaker_id": "1",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000734.1.21",
          "form": "시골집에선 닭들 ㅋㅋㅋㅋ",
          "original_form": "시골집에선 닭들 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000734.1.22",
          "form": "더 제가키웠다기보단 같이 자랐던것같아요",
          "original_form": "더 제가키웠다기보단 같이 자랐던것같아요",
          "speaker_id": "1",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000734.1.23",
          "form": "아! 햄스터 깜빡했어요",
          "original_form": "아! 햄스터 깜빡했어요",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000734.1.24",
          "form": "태교한다며ㅡ..ㅡ",
          "original_form": "태교한다며ㅡ..ㅡ",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000734.1.25",
          "form": "ㅋㅋㅋㅋㅋ그러나 이미 하늘나라로....",
          "original_form": "ㅋㅋㅋㅋㅋ그러나 이미 하늘나라로....",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000734.1.26",
          "form": "ㅎㅎ..애들수명이 넘짧아요",
          "original_form": "ㅎㅎ..애들수명이 넘짧아요",
          "speaker_id": "1",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000734.1.27",
          "form": "어렸을땐 좀 철이없어가지고 학교앞에 파는 병아리에 홀린적도 몇번있었네요",
          "original_form": "어렸을땐 좀 철이없어가지고 학교앞에 파는 병아리에 홀린적도 몇번있었네요",
          "speaker_id": "1",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000734.1.28",
          "form": "병아리 키워서 닭키운 사람도 봣어요 ㅎ",
          "original_form": "병아리 키워서 닭키운 사람도 봣어요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000734.1.29",
          "form": "학교 앞 병아리는 금방 죽는건줄 알았는데 꼭 그렇지만은 않나봐요 대단해요",
          "original_form": "학교 앞 병아리는 금방 죽는건줄 알았는데 꼭 그렇지만은 않나봐요 대단해요",
          "speaker_id": "2",
          "time": "20210819 19:55"
        },
        {
          "id": "MDRW2100000734.1.30",
          "form": "맞아요 전아니었지만 잘키우는애들은 닭으로 키우더라구요",
          "original_form": "맞아요 전아니었지만 잘키우는애들은 닭으로 키우더라구요",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000734.1.31",
          "form": "요즘 유튜브로 앵무새 키우는 집도 보고 있어요",
          "original_form": "요즘 유튜브로 앵무새 키우는 집도 보고 있어요",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000734.1.32",
          "form": "음악이랑 자막이 더해진 탓도 있겠지만 너무 재미있게 살더라고요",
          "original_form": "음악이랑 자막이 더해진 탓도 있겠지만 너무 재미있게 살더라고요",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000734.1.33",
          "form": "반려동물이 하늘나라 가서 슬픈 영상 보고 따라울고",
          "original_form": "반려동물이 하늘나라 가서 슬픈 영상 보고 따라울고",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000734.1.34",
          "form": "웃긴거 보고는 따라 웃고 ㅋㅋㅋ",
          "original_form": "웃긴거 보고는 따라 웃고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000734.1.35",
          "form": "마자요 앵무새 진짜 신기하더라구요",
          "original_form": "마자요 앵무새 진짜 신기하더라구요",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000734.1.36",
          "form": "어떻게 말을 그렇게하고 똑똑하고",
          "original_form": "어떻게 말을 그렇게하고 똑똑하고",
          "speaker_id": "1",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000734.1.37",
          "form": "맞아요 랜선으로 키워요ㅜ",
          "original_form": "맞아요 랜선으로 키워요ㅜ",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000734.1.38",
          "form": "남편은 옆에서 갸들 냄새 많이 나 하며 감동파괴하고 ㅎ",
          "original_form": "남편은 옆에서 갸들 냄새 많이 나 하며 감동파괴하고 ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000734.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ감동파괴",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ감동파괴",
          "speaker_id": "1",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000734.1.40",
          "form": "옆에서 한번씩 깐족대기는 남편들 스킬인가봐여",
          "original_form": "옆에서 한번씩 깐족대기는 남편들 스킬인가봐여",
          "speaker_id": "1",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000734.1.41",
          "form": "제 남편 좀 심한 편이에요",
          "original_form": "제 남편 좀 심한 편이에요",
          "speaker_id": "2",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000734.1.42",
          "form": "그래서 저 성격 유하게 만든다고 고양이 데려온다 한거에요",
          "original_form": "그래서 저 성격 유하게 만든다고 고양이 데려온다 한거에요",
          "speaker_id": "2",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000734.1.43",
          "form": "제성격이 날카로워진게 누구때문인데",
          "original_form": "제성격이 날카로워진게 누구때문인데",
          "speaker_id": "2",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000734.1.44",
          "form": "우다다 같이할것같은",
          "original_form": "우다다 같이할것같은",
          "speaker_id": "1",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000734.1.45",
          "form": "ㅋㅋㅋㅋ맞아요 우리는 레슬링 해버려요",
          "original_form": "ㅋㅋㅋㅋ맞아요 우리는 레슬링 해버려요",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000734.1.46",
          "form": "데리고 살기 힘들다네요",
          "original_form": "데리고 살기 힘들다네요",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000734.1.47",
          "form": "그래서 고양이를 데리고 와야할것 같대요 결론이 그렇네요 ㅋㅋ 저를 위한거라며",
          "original_form": "그래서 고양이를 데리고 와야할것 같대요 결론이 그렇네요 ㅋㅋ 저를 위한거라며",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000734.1.48",
          "form": "반려동물 있으면 제가 착해질것 같고, 뭐 그렇다나",
          "original_form": "반려동물 있으면 제가 착해질것 같고, 뭐 그렇다나",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000734.1.49",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ이제 4명의자식이되는",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ이제 4명의자식이되는",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000734.1.50",
          "form": "냥이가 키우면 행복을 주긴하는데 말썽도 배로 부려서요 ㅋㅋㅋㅋ언제나 애기니까",
          "original_form": "냥이가 키우면 행복을 주긴하는데 말썽도 배로 부려서요 ㅋㅋㅋㅋ언제나 애기니까",
          "speaker_id": "1",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000734.1.51",
          "form": "도도한 맛이 있다던데 ㅎ",
          "original_form": "도도한 맛이 있다던데 ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000734.1.52",
          "form": "울언니가 키우는 냥이는 아직도 언니를 할퀴어요",
          "original_form": "울언니가 키우는 냥이는 아직도 언니를 할퀴어요",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000734.1.53",
          "form": "그걸 보는 저는 못마땅...ㅋㅋㅋ",
          "original_form": "그걸 보는 저는 못마땅...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000734.1.54",
