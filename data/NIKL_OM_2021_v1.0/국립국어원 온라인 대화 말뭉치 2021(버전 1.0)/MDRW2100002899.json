      "utterance": [
        {
          "id": "MDRW2100002899.1.1",
          "form": "게임처럼 영상도 많이 보시나요?",
          "original_form": "게임처럼 영상도 많이 보시나요?",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002899.1.2",
          "form": "아 뮤직비디오 보는거 좋아해요~",
          "original_form": "아 뮤직비디오 보는거 좋아해요~",
          "speaker_id": "2",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002899.1.3",
          "form": "오 좋아하는 가수가 있으신가요?",
          "original_form": "오 좋아하는 가수가 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002899.1.4",
          "form": "네 에스엠,와이지 가수들 노래 좋아해요~",
          "original_form": "네 에스엠,와이지 가수들 노래 좋아해요~",
          "speaker_id": "2",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002899.1.5",
          "form": "생태씨님은 어떤 가수 좋아하시나요?",
          "original_form": "생태씨님은 어떤 가수 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002899.1.6",
          "form": "저도 yg요ㅋㅋ",
          "original_form": "저도 yg요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002899.1.7",
          "form": "소미 요즘 너무 귀여워요",
          "original_form": "소미 요즘 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002899.1.8",
          "form": "아 제가 자세하겐 몰라서.. 전소미 말씀하시는건가요~",
          "original_form": "아 제가 자세하겐 몰라서.. 전소미 말씀하시는건가요~",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002899.1.9",
          "form": "네ㅎㅎ 전소미 yg아니던가ㅎ",
          "original_form": "네ㅎㅎ 전소미 yg아니던가ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002899.1.10",
          "form": "그 아이돌 오디션 일등한 아이돌 말이죠> 와이지 갔구나~",
          "original_form": "그 아이돌 오디션 일등한 아이돌 말이죠> 와이지 갔구나~",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002899.1.11",
          "form": "네 최근에 전참시 봤거든요",
          "original_form": "네 최근에 전참시 봤거든요",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002899.1.12",
          "form": "강다니엘 아시나요?",
          "original_form": "강다니엘 아시나요?",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002899.1.13",
          "form": "네 그럼요ㅎㅎ",
          "original_form": "네 그럼요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100002899.1.14",
          "form": "강다니엘 좋아하세요? 하리보 좋아한다던가요?",
          "original_form": "강다니엘 좋아하세요? 하리보 좋아한다던가요?",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002899.1.15",
          "form": "아 저는 음악위주! 사람보다는",
          "original_form": "아 저는 음악위주! 사람보다는",
          "speaker_id": "2",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002899.1.16",
          "form": "강다니엘 요즘 잘 안보이네요~ 잘은 모르는 친구",
          "original_form": "강다니엘 요즘 잘 안보이네요~ 잘은 모르는 친구",
          "speaker_id": "2",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002899.1.17",
          "form": "아하 음악!",
          "original_form": "아하 음악!",
          "speaker_id": "1",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100002899.1.18",
          "form": "요즘엔 뭘 많이 들으세요?",
          "original_form": "요즘엔 뭘 많이 들으세요?",
          "speaker_id": "1",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100002899.1.19",
          "form": "요즘 아이돌음악도 듣고, 오늘은 코다쿠미 음악 들었습니다!",
          "original_form": "요즘 아이돌음악도 듣고, 오늘은 코다쿠미 음악 들었습니다!",
