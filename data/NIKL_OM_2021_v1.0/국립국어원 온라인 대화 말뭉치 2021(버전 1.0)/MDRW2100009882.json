      "utterance": [
        {
          "id": "MDRW2100009882.1.1",
          "form": "집안일은 끝이 없어요ㅜㄴ",
          "original_form": "집안일은 끝이 없어요ㅜㄴ",
          "speaker_id": "1",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100009882.1.2",
          "form": "요새 세제를 잘못샀나 빨래가 영 시원찮아여",
          "original_form": "요새 세제를 잘못샀나 빨래가 영 시원찮아여",
          "speaker_id": "2",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009882.1.3",
          "form": "맞아요 젤슬플건",
          "original_form": "맞아요 젤슬플건",
          "speaker_id": "2",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009882.1.4",
          "form": "주말에도 해야 한단거죠",
          "original_form": "주말에도 해야 한단거죠",
          "speaker_id": "2",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009882.1.5",
          "form": "다쉬는데 집안일 하는 사람은 해야하니 좀 그럴땐 그래요",
          "original_form": "다쉬는데 집안일 하는 사람은 해야하니 좀 그럴땐 그래요",
          "speaker_id": "2",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009882.1.6",
          "form": "뭔가를 같이 하는 분위기여야 합니다!!",
          "original_form": "뭔가를 같이 하는 분위기여야 합니다!!",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009882.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009882.1.8",
          "form": "혼자만 해요",
          "original_form": "혼자만 해요",
          "speaker_id": "2",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009882.1.9",
          "form": "자녀분들은 용돈으로 시키지 않나요?",
          "original_form": "자녀분들은 용돈으로 시키지 않나요?",
          "speaker_id": "1",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009882.1.10",
          "form": "큰애만 공짜로 해줘요",
          "original_form": "큰애만 공짜로 해줘요",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009882.1.11",
          "form": "설거지 가끔해줘요",
          "original_form": "설거지 가끔해줘요",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009882.1.12",
          "form": "그래도 큰 아이라 의젓한가봐요ㅎㅎ",
          "original_form": "그래도 큰 아이라 의젓한가봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009882.1.13",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009882.1.14",
          "form": "작은애는 커피를 1000원 받고  해줘요",
          "original_form": "작은애는 커피를 1000원 받고  해줘요",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009882.1.15",
          "form": "갑부 되겠어요 ㅋㅋ",
          "original_form": "갑부 되겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009882.1.16",
          "form": "헉 커피 타는거요?",
          "original_form": "헉 커피 타는거요?",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009882.1.17",
          "form": "네 바리스타 2급이예요 ㅋㅋ",
          "original_form": "네 바리스타 2급이예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009882.1.18",
          "form": "딸이",
          "original_form": "딸이",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009882.1.19",
          "form": "초5에 땄어요",
          "original_form": "초5에 땄어요",
          "speaker_id": "2",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009882.1.20",
          "form": "와 대견하시겠어요.",
          "original_form": "와 대견하시겠어요.",
          "speaker_id": "1",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009882.1.21",
          "form": "네 ㅎㅎ 거기서 최연소 였어요",
          "original_form": "네 ㅎㅎ 거기서 최연소 였어요",
          "speaker_id": "2",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009882.1.22",
