          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100041348.1.11",
          "form": "저 버섯 엄청좋아해요",
          "original_form": "저 버섯 엄청좋아해요",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100041348.1.12",
          "form": "상추",
          "original_form": "상추",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100041348.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100041348.1.14",
          "form": "저는 피자랑 통닭에 쏘맥했어여",
          "original_form": "저는 피자랑 통닭에 쏘맥했어여",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100041348.1.15",
          "form": "저도 버섯좋아해요",
          "original_form": "저도 버섯좋아해요",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100041348.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
