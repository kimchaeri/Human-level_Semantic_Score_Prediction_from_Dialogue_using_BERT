      "utterance": [
        {
          "id": "MDRW2100012826.1.1",
          "form": "라면 좋아하세요?",
          "original_form": "라면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 02:03"
        },
        {
          "id": "MDRW2100012826.1.2",
          "form": "좋아하지요\r\n라면은 스낵면을 좋아해요",
          "original_form": "좋아하지요\r\n라면은 스낵면을 좋아해요",
          "speaker_id": "1",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012826.1.3",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 02:08"
        },
        {
          "id": "MDRW2100012826.1.4",
          "form": "특이하시네요",
          "original_form": "특이하시네요",
          "speaker_id": "2",
          "time": "20210825 02:08"
        },
        {
          "id": "MDRW2100012826.1.5",
          "form": "진라면vs신라면은요?",
          "original_form": "진라면vs신라면은요?",
          "speaker_id": "2",
          "time": "20210825 02:08"
        },
        {
          "id": "MDRW2100012826.1.6",
          "form": "고르라면 진라면요 매운걸못먹어서 ㅋ",
          "original_form": "고르라면 진라면요 매운걸못먹어서 ㅋ",
          "speaker_id": "1",
          "time": "20210825 02:09"
        },
        {
          "id": "MDRW2100012826.1.7",
          "form": "너구리vs오동통면은요?",
          "original_form": "너구리vs오동통면은요?",
          "speaker_id": "2",
          "time": "20210825 02:10"
        },
        {
          "id": "MDRW2100012826.1.8",
          "form": "오동이요 더 저렴해서",
          "original_form": "오동이요 더 저렴해서",
          "speaker_id": "1",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100012826.1.9",
          "form": "ㅋㅋㅋ\r\n비빔면vs짜파게티?",
          "original_form": "ㅋㅋㅋ\r\n비빔면vs짜파게티?",
          "speaker_id": "2",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012826.1.10",
          "form": "짜파게티죠 짜장사랑",
          "original_form": "짜파게티죠 짜장사랑",
          "speaker_id": "1",
          "time": "20210825 02:13"
        },
        {
          "id": "MDRW2100012826.1.11",
          "form": "육개장사발면vs 김치사발면?",
          "original_form": "육개장사발면vs 김치사발면?",
          "speaker_id": "2",
          "time": "20210825 02:14"
        },
        {
          "id": "MDRW2100012826.1.12",
          "form": "육개장사발면이요 익숙해서",
          "original_form": "육개장사발면이요 익숙해서",
          "speaker_id": "1",
          "time": "20210825 02:16"
        },
        {
          "id": "MDRW2100012826.1.13",
          "form": "ㅋㅋㅋ저도요ㅋㅋㅋ 국민컵라면",
          "original_form": "ㅋㅋㅋ저도요ㅋㅋㅋ 국민컵라면",
          "speaker_id": "2",
          "time": "20210825 02:17"
        },
        {
          "id": "MDRW2100012826.1.14",
          "form": "라면에 겨란 넣vs말?",
          "original_form": "라면에 겨란 넣vs말?",
          "speaker_id": "2",
          "time": "20210825 02:18"
        },
        {
          "id": "MDRW2100012826.1.15",
          "form": "무조건 넣어요. 계란을 넣으면 좀 건강하게 먹는다는 생각이 들어서요",
          "original_form": "무조건 넣어요. 계란을 넣으면 좀 건강하게 먹는다는 생각이 들어서요",
          "speaker_id": "1",
          "time": "20210825 02:19"
        },
        {
          "id": "MDRW2100012826.1.16",
          "form": "계란은",
          "original_form": "계란은",
          "speaker_id": "2",
          "time": "20210825 02:22"
        },
        {
          "id": "MDRW2100012826.1.17",
          "form": "1개인가요 두개인가요?",
          "original_form": "1개인가요 두개인가요?",
          "speaker_id": "2",
          "time": "20210825 02:23"
        },
        {
          "id": "MDRW2100012826.1.18",
          "form": "한개요 ㅋ 두개는 과한느낌이라",
          "original_form": "한개요 ㅋ 두개는 과한느낌이라",
