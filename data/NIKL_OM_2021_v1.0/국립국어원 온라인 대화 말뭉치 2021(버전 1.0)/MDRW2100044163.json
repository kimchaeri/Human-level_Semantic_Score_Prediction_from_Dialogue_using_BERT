        },
        {
          "id": "MDRW2100044163.1.2",
          "form": "해산물말고 전 고기도 좋아해여",
          "original_form": "해산물말고 전 고기도 좋아해여",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044163.1.3",
          "form": "우아 고기 너무 좋죠",
          "original_form": "우아 고기 너무 좋죠",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044163.1.4",
          "form": "저는 턱이 약해서 얇은고기 너무 좋아해욬ㅋㅋ",
          "original_form": "저는 턱이 약해서 얇은고기 너무 좋아해욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044163.1.5",
          "form": "얇은거 두꺼운거 가리지 않습니다ㅋㅋ",
          "original_form": "얇은거 두꺼운거 가리지 않습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044163.1.6",
          "form": "오오 소고기 돼지고기 둘중 고르라면요?? ㅋㅋㅋ",
          "original_form": "오오 소고기 돼지고기 둘중 고르라면요?? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044163.1.7",
          "form": "악 힘든데요",
          "original_form": "악 힘든데요",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044163.1.8",
          "form": "돼지고기 다양한요리해먹기 좋아서 자주먹어요",
          "original_form": "돼지고기 다양한요리해먹기 좋아서 자주먹어요",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044163.1.9",
          "form": "두부 좋아하세요?",
          "original_form": "두부 좋아하세요?",
