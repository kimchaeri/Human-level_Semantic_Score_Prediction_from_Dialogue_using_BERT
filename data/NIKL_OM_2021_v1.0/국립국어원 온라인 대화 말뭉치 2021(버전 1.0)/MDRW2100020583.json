      "utterance": [
        {
          "id": "MDRW2100020583.1.1",
          "form": "안녕하세요 아침 드셨어요?",
          "original_form": "안녕하세요 아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:07"
        },
        {
          "id": "MDRW2100020583.1.2",
          "form": "네! 식빵 2조각먹었습니다",
          "original_form": "네! 식빵 2조각먹었습니다",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020583.1.3",
          "form": "오 빵!",
          "original_form": "오 빵!",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020583.1.4",
          "form": "name1님도 아침 드셨나요?",
          "original_form": "&name1&님도 아침 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020583.1.5",
          "form": "저는 아직 안먹었는데",
          "original_form": "저는 아직 안먹었는데",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020583.1.6",
          "form": "저도 이따 요기요 파리바게트 오픈하면",
          "original_form": "저도 이따 요기요 파리바게트 오픈하면",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020583.1.7",
          "form": "식빵 어떠세요??ㅋㅋㅋㅋㅋ",
          "original_form": "식빵 어떠세요??ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020583.1.8",
          "form": "빵 사다 먹으려고 기다리고있어요",
          "original_form": "빵 사다 먹으려고 기다리고있어요",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.9",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.10",
          "form": "그거 할인 엄청 해주더라구요",
          "original_form": "그거 할인 엄청 해주더라구요",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.11",
          "form": "좋죠!ㅋㅋㅋ 꿀식빵 맛있어요",
          "original_form": "좋죠!ㅋㅋㅋ 꿀식빵 맛있어요",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.12",
          "form": "맞아용 내일까지라ㅋㅋㅋ",
          "original_form": "맞아용 내일까지라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.13",
          "form": "name1님 지점에도 파는지 모르겠는데 뀐아망이라는 빵 맛있어요!",
          "original_form": "&name1&님 지점에도 파는지 모르겠는데 뀐아망이라는 빵 맛있어요!",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.14",
          "form": "아아 이거 맛있다고",
          "original_form": "아아 이거 맛있다고",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.15",
          "form": "얘기 많이 들었어용",
          "original_form": "얘기 많이 들었어용",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.16",
          "form": "버터버터한 빵인데",
          "original_form": "버터버터한 빵인데",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.17",
          "form": "바삭촉촉해요",
          "original_form": "바삭촉촉해요",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.18",
          "form": "겉바삭속촉촉이군요",
          "original_form": "겉바삭속촉촉이군요",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.19",
          "form": "오늘 장바구니에 담을때",
          "original_form": "오늘 장바구니에 담을때",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.20",
          "form": "1개 다 먹으면 살찔것 같지만 맛있습니다 ㅎㅎ",
          "original_form": "1개 다 먹으면 살찔것 같지만 맛있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.21",
          "form": "같이 담아볼게용ㅎㅎ",
          "original_form": "같이 담아볼게용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020583.1.22",
          "form": "와! 영업성공!! ㅋㅋㅋㅋㅋ",
          "original_form": "와! 영업성공!! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020583.1.23",
          "form": "빵은 원래 살찌는 맛일수록",
          "original_form": "빵은 원래 살찌는 맛일수록",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020583.1.24",
          "form": "맛있자나용ㅎㅎ",
          "original_form": "맛있자나용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020583.1.25",
          "form": "혹시 파바에서 다른거 뭐 사시나요?",
          "original_form": "혹시 파바에서 다른거 뭐 사시나요?",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020583.1.26",
          "form": "전 항상 같은것만 사게되더라구요 ㅠ",
          "original_form": "전 항상 같은것만 사게되더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020583.1.27",
          "form": "그 토종효모빵",
          "original_form": "그 토종효모빵",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020583.1.28",
          "form": "이거랑",
          "original_form": "이거랑",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020583.1.29",
          "form": "바게트 사서 버터에 구워먹어용",
          "original_form": "바게트 사서 버터에 구워먹어용",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020583.1.30",
          "form": "오리덕님 미식가시네",
          "original_form": "오리덕님 미식가시네",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020583.1.31",
          "form": "토종효모빵 맛있죠!",
          "original_form": "토종효모빵 맛있죠!",
          "speaker_id": "2",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020583.1.32",
          "form": "ㅋㅋㅋ 미식가 알아주시니 기쁘네요",
          "original_form": "ㅋㅋㅋ 미식가 알아주시니 기쁘네요",
