      "utterance": [
        {
          "id": "MDRW2100007238.1.1",
          "form": "줌으로 피티받으면 어떨 거 같아?",
          "original_form": "줌으로 피티받으면 어떨 거 같아?",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007238.1.2",
          "form": "피티가 운동이지?",
          "original_form": "피티가 운동이지?",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007238.1.3",
          "form": "응 일대일로 운동강습받는 거",
          "original_form": "응 일대일로 운동강습받는 거",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007238.1.4",
          "form": "난 헬스 별로 안좋아해",
          "original_form": "난 헬스 별로 안좋아해",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007238.1.5",
          "form": "아 맞다 그럼 관심있는 운동 없어?",
          "original_form": "아 맞다 그럼 관심있는 운동 없어?",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007238.1.6",
          "form": "하는거?",
          "original_form": "하는거?",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007238.1.7",
          "form": "응 보는 거 말고",
          "original_form": "응 보는 거 말고",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007238.1.8",
          "form": "축구",
          "original_form": "축구",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007238.1.9",
          "form": "축구 하는 것도 좋아해?",
          "original_form": "축구 하는 것도 좋아해?",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007238.1.10",
          "form": "응 좋아해",
          "original_form": "응 좋아해",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007238.1.11",
          "form": "근데 축구강습은 온라인으로는 힘들겠다",
          "original_form": "근데 축구강습은 온라인으로는 힘들겠다",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007238.1.12",
          "form": "아무래도 야외에서 하는 운동이다 보니까",
          "original_form": "아무래도 야외에서 하는 운동이다 보니까",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007238.1.13",
          "form": "온라인으로 시도해보면 웃기긴 할 거 같네",
          "original_form": "온라인으로 시도해보면 웃기긴 할 거 같네",
          "speaker_id": "1",
