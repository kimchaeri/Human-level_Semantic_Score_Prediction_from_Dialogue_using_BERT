      "utterance": [
        {
          "id": "MDRW2100005614.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100005614.1.2",
          "form": "안녕하세요 학교 다니시나요?",
          "original_form": "안녕하세요 학교 다니시나요?",
          "speaker_id": "1",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100005614.1.3",
          "form": "대학교 다녀요",
          "original_form": "대학교 다녀요",
          "speaker_id": "2",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100005614.1.4",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100005614.1.5",
          "form": "저는 고등학교 다녀요",
          "original_form": "저는 고등학교 다녀요",
          "speaker_id": "1",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100005614.1.6",
          "form": "대학교면 엠티 같은것도 가셨나요?",
          "original_form": "대학교면 엠티 같은것도 가셨나요?",
          "speaker_id": "1",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100005614.1.7",
          "form": "코로나 때문에 다 취소됐어요ㅠㅠ",
          "original_form": "코로나 때문에 다 취소됐어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 10:56"
        },
        {
          "id": "MDRW2100005614.1.8",
          "form": "고등학교도 수련회랑 수학여행 못 가죠?",
          "original_form": "고등학교도 수련회랑 수학여행 못 가죠?",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100005614.1.9",
          "form": "네 다 못갔네요 ㅠ ㅠ ㅋㅋ",
          "original_form": "네 다 못갔네요 ㅠ ㅠ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100005614.1.10",
          "form": "시험공부만 하려니 죽을맛이에요",
          "original_form": "시험공부만 하려니 죽을맛이에요",
          "speaker_id": "1",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100005614.1.11",
          "form": "아이고ㅠㅠ",
          "original_form": "아이고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100005614.1.12",
          "form": "개학하셨나요?",
          "original_form": "개학하셨나요?",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100005614.1.13",
          "form": "네 개학하는데 지금은 온라인이랑 대면 섞어서 하다가",
          "original_form": "네 개학하는데 지금은 온라인이랑 대면 섞어서 하다가",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100005614.1.14",
          "form": "이제 나중가면 전면등교한다더라구요",
          "original_form": "이제 나중가면 전면등교한다더라구요",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100005614.1.15",
          "form": "아예 대면 하는 학교도 있다더라고요",
          "original_form": "아예 대면 하는 학교도 있다더라고요",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100005614.1.16",
          "form": "대면이랑 비대면 중에 어떤 걸 선호하세요?",
          "original_form": "대면이랑 비대면 중에 어떤 걸 선호하세요?",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100005614.1.17",
          "form": "저는 대면이 나은 것 같아요 물론 힘들지만 온라인은 수업 효율이 떨어지는 것 같아서..",
          "original_form": "저는 대면이 나은 것 같아요 물론 힘들지만 온라인은 수업 효율이 떨어지는 것 같아서..",
          "speaker_id": "1",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100005614.1.18",
          "form": "맞아요 소통하기도 힘들고 집중도 안 되죠",
          "original_form": "맞아요 소통하기도 힘들고 집중도 안 되죠",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100005614.1.19",
          "form": "비대면은 녹화 강의인가요?",
          "original_form": "비대면은 녹화 강의인가요?",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100005614.1.20",
          "form": "일단 쌍방향으로 출첵 다하고 녹화강의를 보여주던가 선생님이 직접 수업을 하시던가 하세요",
          "original_form": "일단 쌍방향으로 출첵 다하고 녹화강의를 보여주던가 선생님이 직접 수업을 하시던가 하세요",
          "speaker_id": "1",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100005614.1.21",
          "form": "얼른 코로나가 종식되면 좋겠네요",
          "original_form": "얼른 코로나가 종식되면 좋겠네요",
          "speaker_id": "2",
          "time": "20210822 11:01"
        }
      ]
