      "utterance": [
        {
          "id": "MDRW2100011370.1.1",
          "form": "최근에 사신거있나요?",
          "original_form": "최근에 사신거있나요?",
          "speaker_id": "1",
          "time": "20210824 11:40"
        },
        {
          "id": "MDRW2100011370.1.2",
          "form": "아 최근에 장난감정리함 구입했어요ㅋㅋ",
          "original_form": "아 최근에 장난감정리함 구입했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:43"
        },
        {
          "id": "MDRW2100011370.1.3",
          "form": "누누님은요?",
          "original_form": "누누님은요?",
          "speaker_id": "2",
          "time": "20210824 11:43"
        },
        {
          "id": "MDRW2100011370.1.4",
          "form": "와우 장난감이 많으신가요?",
          "original_form": "와우 장난감이 많으신가요?",
          "speaker_id": "1",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011370.1.5",
          "form": "저는 샌들을 산거같네요",
          "original_form": "저는 샌들을 산거같네요",
          "speaker_id": "1",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011370.1.6",
          "form": "아이들 장난감이 많아서요:)",
          "original_form": "아이들 장난감이 많아서요:)",
          "speaker_id": "2",
          "time": "20210824 11:59"
        },
        {
          "id": "MDRW2100011370.1.7",
          "form": "샌들은 어디서 구입하셨어요?",
          "original_form": "샌들은 어디서 구입하셨어요?",
          "speaker_id": "2",
          "time": "20210824 12:00"
        },
        {
          "id": "MDRW2100011370.1.8",
          "form": "아 아이가있으시군요!",
          "original_form": "아 아이가있으시군요!",
          "speaker_id": "1",
          "time": "20210824 12:20"
        },
        {
          "id": "MDRW2100011370.1.9",
          "form": "그냥 보세샌들이요ㅋㅋㅋ",
          "original_form": "그냥 보세샌들이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:20"
        },
        {
          "id": "MDRW2100011370.1.10",
          "form": "구매하시고싶으신거 있으신가요?",
          "original_form": "구매하시고싶으신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 12:20"
        },
        {
          "id": "MDRW2100011370.1.11",
          "form": "직접 신어보고 구매하셨는지",
          "original_form": "직접 신어보고 구매하셨는지",
          "speaker_id": "2",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100011370.1.12",
          "form": "아님 인쇼로 구매하신건지해서요:(",
          "original_form": "아님 인쇼로 구매하신건지해서요:(",
          "speaker_id": "2",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100011370.1.13",
          "form": "저는 신혼때 집들이 선물로 받았던\r\n에어프라이어가 용량이 넘 작아서\r\n요새 대용량 에어프라이어를\r\n들이고 싶어요ㅋㅋ",
          "original_form": "저는 신혼때 집들이 선물로 받았던\r\n에어프라이어가 용량이 넘 작아서\r\n요새 대용량 에어프라이어를\r\n들이고 싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:22"
        },
        {
          "id": "MDRW2100011370.1.14",
          "form": "저는 230사면보통맞아서 안신어보고 구매했답니다ㅋㅋㅋ",
          "original_form": "저는 230사면보통맞아서 안신어보고 구매했답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100011370.1.15",
          "form": "에어프라이어좋죠~ 통삼겹요리",
          "original_form": "에어프라이어좋죠~ 통삼겹요리",
          "speaker_id": "1",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100011370.1.16",
          "form": "아하 그러셨군용ㅋㅋ",
          "original_form": "아하 그러셨군용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011370.1.17",
          "form": "에프 진짜 유용하죠:)",
          "original_form": "에프 진짜 유용하죠:)",
          "speaker_id": "2",
          "time": "20210824 12:30"
        },
        {
          "id": "MDRW2100011370.1.18",
          "form": "요샌 오븐형으로도 나오네요ㅋㅋ",
          "original_form": "요샌 오븐형으로도 나오네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:31"
        },
        {
          "id": "MDRW2100011370.1.19",
          "form": "오븐형으로도 나오나요?ㅋㅋㅋ",
          "original_form": "오븐형으로도 나오나요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100011370.1.20",
          "form": "저도 한창 구매하고나서 만두엄청튀겨먹었었네요ㅋㅋㅋ",
          "original_form": "저도 한창 구매하고나서 만두엄청튀겨먹었었네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100011370.1.21",
          "form": "네ㅋㅋ 오븐형도 있더라구요",
          "original_form": "네ㅋㅋ 오븐형도 있더라구요",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011370.1.22",
          "form": "저도 맨날 치킨 해먹고ㅋㅋ",
          "original_form": "저도 맨날 치킨 해먹고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011370.1.23",
          "form": "돈까스 해먹고ㅋㅋ",
          "original_form": "돈까스 해먹고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011370.1.24",
          "form": "튀김종류 엄청 먹었었어요ㅋ",
          "original_form": "튀김종류 엄청 먹었었어요ㅋ",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011370.1.25",
          "form": "ㅋㅋㅋ저도요 한창삘받아서",
          "original_form": "ㅋㅋㅋ저도요 한창삘받아서",
          "speaker_id": "1",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100011370.1.26",
          "form": "이거저거해보다가 요즘은 좀뜸하네요",
          "original_form": "이거저거해보다가 요즘은 좀뜸하네요",
          "speaker_id": "1",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100011370.1.27",
          "form": "에프돌리기 시작함\r\n살찌는 소리가ㅋㅋ",
          "original_form": "에프돌리기 시작함\r\n살찌는 소리가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011370.1.28",
          "form": "누누님은 구매하고 싶은\r\n제품 있으세요?",
          "original_form": "누누님은 구매하고 싶은\r\n제품 있으세요?",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011370.1.29",
          "form": "저는 가을옷이요!",
          "original_form": "저는 가을옷이요!",
          "speaker_id": "1",
          "time": "20210824 12:57"
        }
      ]
    }
