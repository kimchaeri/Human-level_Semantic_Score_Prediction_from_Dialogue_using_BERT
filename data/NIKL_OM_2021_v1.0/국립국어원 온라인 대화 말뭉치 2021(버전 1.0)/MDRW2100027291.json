      "utterance": [
        {
          "id": "MDRW2100027291.1.1",
          "form": "곰표 맥주 드셔보셨나요",
          "original_form": "곰표 맥주 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100027291.1.2",
          "form": "아니여 근데 디자인이 넘 예뻐서 멉고싶어요",
          "original_form": "아니여 근데 디자인이 넘 예뻐서 멉고싶어요",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027291.1.3",
          "form": "저도 사놨는데 안마셔지네요 ㅋㅋ",
          "original_form": "저도 사놨는데 안마셔지네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027291.1.4",
          "form": "그냥 보기만ㅋ",
          "original_form": "그냥 보기만ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100027291.1.5",
          "form": "네네 4캔 만원해서 호기롭게사왔는데 ㅋㅋ",
          "original_form": "네네 4캔 만원해서 호기롭게사왔는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027291.1.6",
          "form": "곰표 나쵸는 다 먹었어요",
          "original_form": "곰표 나쵸는 다 먹었어요",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027291.1.7",
          "form": "마케팅을 너무잘하는것같아요",
          "original_form": "마케팅을 너무잘하는것같아요",
          "speaker_id": "1",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100027291.1.8",
          "form": "그쵸 ㅋㅋ 사놓고 안먹어ㅠㅠㅋㅋ",
          "original_form": "그쵸 ㅋㅋ 사놓고 안먹어ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100027291.1.9",
          "form": "사고싶게만들어요",
          "original_form": "사고싶게만들어요",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027291.1.10",
          "form": "맞아요 인스타구램도 들어가보명 살게참많아요",
          "original_form": "맞아요 인스타구램도 들어가보명 살게참많아요",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027291.1.11",
          "form": "지나고보면 돈아깝고ㅠ",
          "original_form": "지나고보면 돈아깝고ㅠ",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027291.1.12",
          "form": "알면서 계속사요",
          "original_form": "알면서 계속사요",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027291.1.13",
          "form": "맞어요 ㅋㅋㅋㅋ",
          "original_form": "맞어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027291.1.14",
          "form": "돈을 어디다가 묶어놔여지ㅠㅠ",
          "original_form": "돈을 어디다가 묶어놔여지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027291.1.15",
          "form": "사면서 행복해요",
          "original_form": "사면서 행복해요",
          "speaker_id": "1",
          "time": "20210902 15:08"
        }
