      "utterance": [
        {
          "id": "MDRW2100039308.1.1",
          "form": "아뇨 코로난데 ㅎㅎ",
          "original_form": "아뇨 코로난데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039308.1.2",
          "form": "어디가시나요?",
          "original_form": "어디가시나요?",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039308.1.3",
          "form": "아뇨 부모님댁도 못가요",
          "original_form": "아뇨 부모님댁도 못가요",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039308.1.4",
          "form": "시골은 더 경계라더라고요",
          "original_form": "시골은 더 경계라더라고요",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039308.1.5",
          "form": "누가 돌아다니는걸까요? 아주 ㅋㅋ",
          "original_form": "누가 돌아다니는걸까요? 아주 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039308.1.6",
          "form": "저는 여행가는건 이해해도 사진찍는다고여행지에서 마스크 벗는게 너무 싫어요",
          "original_form": "저는 여행가는건 이해해도 사진찍는다고여행지에서 마스크 벗는게 너무 싫어요",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039308.1.7",
          "form": "제주도 가는사람들도 얄밉고 ㅋ",
          "original_form": "제주도 가는사람들도 얄밉고 ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039308.1.8",
          "form": "아 제주도 많이 가더라고요 진짜",
          "original_form": "아 제주도 많이 가더라고요 진짜",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039308.1.9",
          "form": "여행이 참 소중해졌어요 일상에서..",
          "original_form": "여행이 참 소중해졌어요 일상에서..",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039308.1.10",
          "form": "맞아용 근처 음식점가기도 얼마나 좋았는지",
          "original_form": "맞아용 근처 음식점가기도 얼마나 좋았는지",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039308.1.11",
          "form": "그러게요 동생이 해외에 있어서 내년엔 여행가고싶은데 갈 수 있을지 모르겠어요",
          "original_form": "그러게요 동생이 해외에 있어서 내년엔 여행가고싶은데 갈 수 있을지 모르겠어요",
          "speaker_id": "1",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039308.1.12",
          "form": "자가격리만 안귀찮으면 가긴하더라고요",
          "original_form": "자가격리만 안귀찮으면 가긴하더라고요",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039308.1.13",
          "form": "안전해져야 가려고요 ㅎㅎ",
          "original_form": "안전해져야 가려고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039308.1.14",
          "form": "동생이 있는 여행지는 어디신가요?",
          "original_form": "동생이 있는 여행지는 어디신가요?",
          "speaker_id": "2",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039308.1.15",
          "form": "두바이에요 일하러 3년정도 가있어요",
          "original_form": "두바이에요 일하러 3년정도 가있어요",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039308.1.16",
          "form": "오 경유로 가봤었는데 럭셔리하더라고요",
          "original_form": "오 경유로 가봤었는데 럭셔리하더라고요",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039308.1.17",
          "form": "저는 근처도 안가봐서...ㅎㅎ",
          "original_form": "저는 근처도 안가봐서...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:19"
        }
