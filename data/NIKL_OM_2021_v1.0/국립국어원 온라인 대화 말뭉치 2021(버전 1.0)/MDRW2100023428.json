      "utterance": [
        {
          "id": "MDRW2100023428.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210830 02:11"
        },
        {
          "id": "MDRW2100023428.1.2",
          "form": "주말에 맛있는거 많이 드셨어요~?",
          "original_form": "주말에 맛있는거 많이 드셨어요~?",
          "speaker_id": "1",
          "time": "20210830 02:11"
        },
        {
          "id": "MDRW2100023428.1.3",
          "form": "네~주말에 오리고기도 먹고 그랬어요",
          "original_form": "네~주말에 오리고기도 먹고 그랬어요",
          "speaker_id": "2",
          "time": "20210830 02:12"
        },
        {
          "id": "MDRW2100023428.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210830 02:12"
        },
        {
          "id": "MDRW2100023428.1.5",
          "form": "아항ㅋㅋㅋ",
          "original_form": "아항ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:12"
        },
        {
          "id": "MDRW2100023428.1.6",
          "form": "부대찌개랑",
          "original_form": "부대찌개랑",
          "speaker_id": "1",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023428.1.7",
          "form": "샌드위치 먹었어용!",
          "original_form": "샌드위치 먹었어용!",
          "speaker_id": "1",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023428.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023428.1.9",
          "form": "부대찌개 좋아하세요?",
          "original_form": "부대찌개 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023428.1.10",
          "form": "오 맛있었겠네요",
          "original_form": "오 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023428.1.11",
          "form": "네 전 완전 좋아해요",
          "original_form": "네 전 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023428.1.12",
          "form": "아항ㅋㅋㅋ 부대찌개 안좋아하는 분들은 흔치 않더라구여",
          "original_form": "아항ㅋㅋㅋ 부대찌개 안좋아하는 분들은 흔치 않더라구여",
          "speaker_id": "1",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023428.1.13",
          "form": "배달해서 드신건가요?",
          "original_form": "배달해서 드신건가요?",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023428.1.14",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023428.1.15",
          "form": "직접 해먹었어용",
          "original_form": "직접 해먹었어용",
          "speaker_id": "1",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023428.1.16",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023428.1.17",
          "form": "사골설렁탕 육수로 넣고",
          "original_form": "사골설렁탕 육수로 넣고",
          "speaker_id": "1",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023428.1.18",
          "form": "스팸이랑 소세지랑 마카로니랑",
          "original_form": "스팸이랑 소세지랑 마카로니랑",
          "speaker_id": "1",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023428.1.19",
          "form": "등등ㅋㅋㅋ",
          "original_form": "등등ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023428.1.20",
          "form": "오 마카로니도 넣으시는구나",
          "original_form": "오 마카로니도 넣으시는구나",
          "speaker_id": "2",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023428.1.21",
          "form": "넹넹 부대찌개에 들어가는 콩도 좋아해요",
          "original_form": "넹넹 부대찌개에 들어가는 콩도 좋아해요",
          "speaker_id": "1",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100023428.1.22",
          "form": "좋아하시는 라면 브랜드 있으세요?",
          "original_form": "좋아하시는 라면 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210830 02:15"
        }
      ]
