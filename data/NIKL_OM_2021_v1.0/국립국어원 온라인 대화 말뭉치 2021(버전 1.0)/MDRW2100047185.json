      "utterance": [
        {
          "id": "MDRW2100047185.1.1",
          "form": "name2님 당근하세요",
          "original_form": "&name2&님 당근하세요",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100047185.1.2",
          "form": "아뇻 당근은 뭔가 직접 나가기 귀찮아서",
          "original_form": "아뇻 당근은 뭔가 직접 나가기 귀찮아서",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100047185.1.3",
          "form": "해본적 없어요 ㅋㅋㅋㅋ",
          "original_form": "해본적 없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100047185.1.4",
          "form": "아하 집토끼 시군요",
          "original_form": "아하 집토끼 시군요",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100047185.1.5",
          "form": "당근 자주 하세요??",
          "original_form": "당근 자주 하세요??",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100047185.1.6",
          "form": "가격이 싸서 좋아요",
          "original_form": "가격이 싸서 좋아요",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100047185.1.7",
          "form": "ㅇㅎㅇㅎ",
          "original_form": "ㅇㅎㅇㅎ",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100047185.1.8",
          "form": "당근 깔아서 구경한 적은 있는데",
          "original_form": "당근 깔아서 구경한 적은 있는데",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100047185.1.9",
          "form": "뭔가 음...",
          "original_form": "뭔가 음...",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100047185.1.10",
          "form": "전살게 있으면 검색해요",
          "original_form": "전살게 있으면 검색해요",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100047185.1.11",
          "form": "구매하고 싶은? 그런건 잘 못봐서 ㅋㅋㅋㅋㅋ",
          "original_form": "구매하고 싶은? 그런건 잘 못봐서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100047185.1.12",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100047185.1.13",
          "form": "버티컬마우스 사서 잘쓰고있어요",
          "original_form": "버티컬마우스 사서 잘쓰고있어요",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100047185.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100047185.1.15",
          "form": "그거 편한가요??",
          "original_form": "그거 편한가요??",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100047185.1.16",
          "form": "오 컴터많이 쓰시는분들 마우스때문에",
          "original_form": "오 컴터많이 쓰시는분들 마우스때문에",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047185.1.17",
          "form": "터널증후군오는데",
          "original_form": "터널증후군오는데",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047185.1.18",
          "form": "버티컬마우스가 예방해줘요",
          "original_form": "버티컬마우스가 예방해줘요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047185.1.19",
          "form": "안그래도 손목안좋아서 그거 고민해봤었는데",
          "original_form": "안그래도 손목안좋아서 그거 고민해봤었는데",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047185.1.20",
          "form": "뭔가 익숙하지않게 생기기도 했고...",
          "original_form": "뭔가 익숙하지않게 생기기도 했고...",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047185.1.21",
          "form": "컴퓨터 게임할때 그걸론 못하게 생겨서 그냥 포기했었어요 ㅠㅠㅋㅋㅋ",
          "original_form": "컴퓨터 게임할때 그걸론 못하게 생겨서 그냥 포기했었어요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047185.1.22",
          "form": "습관은 만들어나가야죠",
          "original_form": "습관은 만들어나가야죠",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047185.1.23",
          "form": "손목이 안좋아지면 늦습니당!",
          "original_form": "손목이 안좋아지면 늦습니당!",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047185.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047185.1.25",
          "form": "저도 한 번 알아봐야겠어요",
          "original_form": "저도 한 번 알아봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047185.1.26",
          "form": "키보드도 막 손목건강에 좋은 쪼개진 키보드는",
          "original_form": "키보드도 막 손목건강에 좋은 쪼개진 키보드는",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047185.1.27",
          "form": "엄청 비싸더라구요 ㅠㅠ",
          "original_form": "엄청 비싸더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:28"
        }
      ]
    }
  ]
