      "utterance": [
        {
          "id": "MDRW2100002561.1.1",
          "form": "최근에 구매하신거있으신가요?",
          "original_form": "최근에 구매하신거있으신가요?",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002561.1.2",
          "form": "최근에 책상 구매했어요!",
          "original_form": "최근에 책상 구매했어요!",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002561.1.3",
          "form": "오 책상은 왜요??",
          "original_form": "오 책상은 왜요??",
          "speaker_id": "1",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002561.1.4",
          "form": "책상이 좀 낡아서",
          "original_form": "책상이 좀 낡아서",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002561.1.5",
          "form": "이번에 새롭게 사봤어요!",
          "original_form": "이번에 새롭게 사봤어요!",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002561.1.6",
          "form": "좋네요ㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002561.1.7",
          "form": "저는 갤워치사고싶더라고요",
          "original_form": "저는 갤워치사고싶더라고요",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002561.1.8",
          "form": "오 맞아요 이번에",
          "original_form": "오 맞아요 이번에",
          "speaker_id": "2",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002561.1.9",
          "form": "갤워치4 새로 나온다고 들었어요!",
          "original_form": "갤워치4 새로 나온다고 들었어요!",
          "speaker_id": "2",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002561.1.10",
          "form": "맞아요 저 갤럭시액티브2가지고있는데",
          "original_form": "맞아요 저 갤럭시액티브2가지고있는데",
          "speaker_id": "1",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002561.1.11",
          "form": "훨씬싸게나오더라고요ㅠ",
          "original_form": "훨씬싸게나오더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002561.1.12",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "2",
          "time": "20210820 14:21"
        },
        {
          "id": "MDRW2100002561.1.13",
          "form": "안타깝네요..ㅠㅠ",
          "original_form": "안타깝네요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:21"
        },
        {
          "id": "MDRW2100002561.1.14",
          "form": "4가 더 싸게 나온다니..",
          "original_form": "4가 더 싸게 나온다니..",
          "speaker_id": "2",
          "time": "20210820 14:21"
        },
        {
          "id": "MDRW2100002561.1.15",
          "form": "액티브2 41만원",
          "original_form": "액티브2 41만원",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002561.1.16",
          "form": "인가주고샀는데",
          "original_form": "인가주고샀는데",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002561.1.17",
          "form": "이번신제품이 20만원대인가",
          "original_form": "이번신제품이 20만원대인가",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002561.1.18",
          "form": "그러더라고요...ㅋㅋㅋㅋ",
          "original_form": "그러더라고요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002561.1.19",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "2",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002561.1.20",
          "form": "이번에 4 성능도 좋은걸로 알고있는데",
          "original_form": "이번에 4 성능도 좋은걸로 알고있는데",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002561.1.21",
          "form": "엄청 싸게나오네요",
          "original_form": "엄청 싸게나오네요",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002561.1.22",
          "form": "그니까요ㅋㅋㅋ어이가없어서",
          "original_form": "그니까요ㅋㅋㅋ어이가없어서",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002561.1.23",
          "form": "인바디도된대요",
          "original_form": "인바디도된대요",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002561.1.24",
          "form": "요즘 버즈도그렇고 갤럭시가",
          "original_form": "요즘 버즈도그렇고 갤럭시가",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002561.1.25",
          "form": "신제품을 더싸게내놓나봐요",
          "original_form": "신제품을 더싸게내놓나봐요",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002561.1.26",
          "form": "버즈도 싸게 나왔나요?!",
          "original_form": "버즈도 싸게 나왔나요?!",
          "speaker_id": "2",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002561.1.27",
          "form": "버즈 이번에 새롭게 바꿀려고 하는데",
          "original_form": "버즈 이번에 새롭게 바꿀려고 하는데",
          "speaker_id": "2",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002561.1.28",
          "form": "버즈 소식은 못들었네요 이런",
          "original_form": "버즈 소식은 못들었네요 이런",
          "speaker_id": "2",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002561.1.29",
          "form": "이번에버즈",
          "original_form": "이번에버즈",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002561.1.30",
          "form": "2",
          "original_form": "2",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002561.1.31",
          "form": "13마넌인가밖에안한대요ㅋㅋㅋㅋ",
          "original_form": "13마넌인가밖에안한대요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002561.1.32",
          "form": "사전예약",
          "original_form": "사전예약",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002561.1.33",
          "form": "어서 고고하세요 정가가14만인가그런데",
          "original_form": "어서 고고하세요 정가가14만인가그런데",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002561.1.34",
          "form": "사전예약하면 13인가? 요즘은 신제품더싸게내놓더라고요ㅠ",
          "original_form": "사전예약하면 13인가? 요즘은 신제품더싸게내놓더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002561.1.35",
          "form": "14?????헐",
          "original_form": "14?????헐",
          "speaker_id": "2",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002561.1.36",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002561.1.37",
          "form": "이번주나 다음주 안으로 사야겠어요 대박 ㅠㅠ",
          "original_form": "이번주나 다음주 안으로 사야겠어요 대박 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002561.1.38",
          "form": "네이버검색하봐요ㅋㅋ대박임ㅋㅋㅋ색도이쁜데",
          "original_form": "네이버검색하봐요ㅋㅋ대박임ㅋㅋㅋ색도이쁜데",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002561.1.39",
          "form": "정가가14.9이고",
          "original_form": "정가가14.9이고",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002561.1.40",
          "form": "예약가가 13?",
          "original_form": "예약가가 13?",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002561.1.41",
          "form": "저렴쓰",
          "original_form": "저렴쓰",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002561.1.42",
          "form": "예약가가 훨싸네요 예약할걸 힝..ㅠㅠ",
          "original_form": "예약가가 훨싸네요 예약할걸 힝..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002561.1.43",
          "form": "그래도 정가도 싼게 확인해보고",
          "original_form": "그래도 정가도 싼게 확인해보고",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002561.1.44",
          "form": "사야겠네오 꼭~",
          "original_form": "사야겠네오 꼭~",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002561.1.45",
          "form": "예약 끝났대요???",
          "original_form": "예약 끝났대요???",
          "speaker_id": "1",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002561.1.46",
          "form": "지금받고있지않나요??",
          "original_form": "지금받고있지않나요??",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002561.1.47",
          "form": "헉 아직 하고있네요",
          "original_form": "헉 아직 하고있네요",
          "speaker_id": "2",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002561.1.48",
          "form": "예약 하러가야겠다",
          "original_form": "예약 하러가야겠다",
          "speaker_id": "2",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002561.1.49",
          "form": "마카롱커버 이거 진짜 이쁘네요 ㅠㅠㅜㅠㅠㅠ대박",
          "original_form": "마카롱커버 이거 진짜 이쁘네요 ㅠㅠㅜㅠㅠㅠ대박",
          "speaker_id": "2",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002561.1.50",
          "form": "요즘 갤럭시신제품이",
          "original_form": "요즘 갤럭시신제품이",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002561.1.51",
          "form": "다 대박인듯요ㅋㅋㅋ",
          "original_form": "다 대박인듯요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002561.1.52",
          "form": "플립도 존예예요ㅠ넘사고픔",
          "original_form": "플립도 존예예요ㅠ넘사고픔",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002561.1.53",
          "form": "맞아요 폰은 안타깝게 이미 사서",
          "original_form": "맞아요 폰은 안타깝게 이미 사서",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002561.1.54",
          "form": "플립을 놓쳤다는..ㅠㅠ",
          "original_form": "플립을 놓쳤다는..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002561.1.55",
          "form": "저도 폰산지",
          "original_form": "저도 폰산지",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002561.1.56",
          "form": "2년안되가지고 못사네요ㅠ",
          "original_form": "2년안되가지고 못사네요ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:20"
        }
