      "utterance": [
        {
          "id": "MDRW2100032333.1.1",
          "form": "경주도 또가야하는데..",
          "original_form": "경주도 또가야하는데..",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032333.1.2",
          "form": "경주도 가야지 ...",
          "original_form": "경주도 가야지 ...",
          "speaker_id": "2",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032333.1.3",
          "form": "너네랑은 한 5년된거 같은데?",
          "original_form": "너네랑은 한 5년된거 같은데?",
          "speaker_id": "2",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032333.1.4",
          "form": "그치...너 전애인이랑 갔지않나?",
          "original_form": "그치...너 전애인이랑 갔지않나?",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032333.1.5",
          "form": "나는 한 3년전에 갔지",
          "original_form": "나는 한 3년전에 갔지",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032333.1.6",
          "form": "별로 기억하고싶진않네ㅋㅋㅋㄱㄲ",
          "original_form": "별로 기억하고싶진않네ㅋㅋㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032333.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032333.1.8",
          "form": "그래 가서 뭐했음?",
          "original_form": "그래 가서 뭐했음?",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032333.1.9",
          "form": "또 안압지 가고 자전거타고 왕릉가고 했지",
          "original_form": "또 안압지 가고 자전거타고 왕릉가고 했지",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032333.1.10",
          "form": "안압지 좋지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "안압지 좋지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032333.1.11",
          "form": "거기서 name3 넘어진거 기억나?ㅋㅋㅋㅋㅋ",
          "original_form": "거기서 &name3& 넘어진거 기억나?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032333.1.12",
          "form": "잊을수 없지ㅋㄲㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "잊을수 없지ㅋㄲㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100032333.1.13",
          "form": "옆에있던애가 갑자기 사라졌다니까ㅋㅋㅋ",
          "original_form": "옆에있던애가 갑자기 사라졌다니까ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032333.1.14",
          "form": "아 그때 잔짜웃겼다고",
          "original_form": "아 그때 잔짜웃겼다고",
          "speaker_id": "1",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100032333.1.15",
          "form": "안압지 갈때마다 웃겨",
          "original_form": "안압지 갈때마다 웃겨",
          "speaker_id": "1",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100032333.1.16",
          "form": "하필 길이 언덕처럼 솟아있어서",
          "original_form": "하필 길이 언덕처럼 솟아있어서",
          "speaker_id": "2",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100032333.1.17",
          "form": "굴러 떨어진거 생각하면 웃겨",
          "original_form": "굴러 떨어진거 생각하면 웃겨",
          "speaker_id": "2",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032333.1.18",
          "form": "안다쳐서 웃을수 있는거지만!",
          "original_form": "안다쳐서 웃을수 있는거지만!",
          "speaker_id": "2",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032333.1.19",
          "form": "맞아 안다친게 진짜 다행이야",
          "original_form": "맞아 안다친게 진짜 다행이야",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032333.1.20",
          "form": "무튼 안압지 또가고싶다",
          "original_form": "무튼 안압지 또가고싶다",
          "speaker_id": "1",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100032333.1.21",
          "form": "나도 또가고싶다...",
          "original_form": "나도 또가고싶다...",
          "speaker_id": "2",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100032333.1.22",
          "form": "추석쯔음에 가면 좋은데...",
          "original_form": "추석쯔음에 가면 좋은데...",
          "speaker_id": "2",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100032333.1.23",
          "form": "딱좋지... 적당히 시원하고",
          "original_form": "딱좋지... 적당히 시원하고",
          "speaker_id": "1",
