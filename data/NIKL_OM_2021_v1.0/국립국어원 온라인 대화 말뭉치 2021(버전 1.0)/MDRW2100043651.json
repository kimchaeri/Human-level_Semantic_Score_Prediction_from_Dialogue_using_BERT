      "utterance": [
        {
          "id": "MDRW2100043651.1.1",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.2",
          "form": "숯불맛 무시못해!",
          "original_form": "숯불맛 무시못해!",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.3",
          "form": "헉 글쿠나",
          "original_form": "헉 글쿠나",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.4",
          "form": "난 그냥인줄..",
          "original_form": "난 그냥인줄..",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.5",
          "form": "아 치즈잇으면",
          "original_form": "아 치즈잇으면",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.6",
          "form": "달라져",
          "original_form": "달라져",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.7",
          "form": "그래도 숯불아니구?",
          "original_form": "그래도 숯불아니구?",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.8",
          "form": "치즈는 치즈다",
          "original_form": "치즈는 치즈다",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.9",
          "form": "명언이다",
          "original_form": "명언이다",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.10",
          "form": "치즈가 꿀맛이지",
          "original_form": "치즈가 꿀맛이지",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043651.1.11",
          "form": "고건 인정 ~",
          "original_form": "고건 인정 ~",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043651.1.12",
          "form": "치즈는 너무 맛잇어",
          "original_form": "치즈는 너무 맛잇어",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043651.1.13",
          "form": "난 숯불도 좋은데",
          "original_form": "난 숯불도 좋은데",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043651.1.14",
          "form": "치즈는 어언데?",
          "original_form": "치즈는 어언데?",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043651.1.15",
          "form": "치즈도 맛잇지만",
          "original_form": "치즈도 맛잇지만",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043651.1.16",
          "form": "숯불 불맛이좋아",
          "original_form": "숯불 불맛이좋아",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043651.1.17",
          "form": "마자 불맛",
          "original_form": "마자 불맛",
          "speaker_id": "1",
          "time": "20210907 19:52"
        }
      ]
