      "utterance": [
        {
          "id": "MDRW2100031283.1.1",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031283.1.2",
          "form": "저는 포도랑  옥수수 먹었어요",
          "original_form": "저는 포도랑  옥수수 먹었어요",
          "speaker_id": "1",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031283.1.3",
          "form": "맛있었겠다네요",
          "original_form": "맛있었겠다네요",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031283.1.4",
          "form": "포도가 캠벨 이라 너무 달았어요",
          "original_form": "포도가 캠벨 이라 너무 달았어요",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031283.1.5",
          "form": "포도는 역시 달아야죠",
          "original_form": "포도는 역시 달아야죠",
          "speaker_id": "2",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031283.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031283.1.7",
          "form": "저는 해 먹었습니다",
          "original_form": "저는 해 먹었습니다",
          "speaker_id": "2",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031283.1.8",
          "form": "뭐 요리해서 드셨나요",
          "original_form": "뭐 요리해서 드셨나요",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031283.1.9",
          "form": "미역국이 요",
          "original_form": "미역국이 요",
          "speaker_id": "2",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031283.1.10",
          "form": "조기 넣으셨나요",
          "original_form": "조기 넣으셨나요",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031283.1.11",
          "form": "아니요 소고기 요",
          "original_form": "아니요 소고기 요",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031283.1.12",
          "form": "고소했겠네요",
          "original_form": "고소했겠네요",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031283.1.13",
          "form": "감질 맛도 있고",
          "original_form": "감질 맛도 있고",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031283.1.14",
          "form": "많이 하셨어요",
          "original_form": "많이 하셨어요",
          "speaker_id": "1",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031283.1.15",
          "form": "넉넉하게 했습니다",
          "original_form": "넉넉하게 했습니다",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031283.1.16",
          "form": "포도 정말 좋아합니다",
          "original_form": "포도 정말 좋아합니다",
