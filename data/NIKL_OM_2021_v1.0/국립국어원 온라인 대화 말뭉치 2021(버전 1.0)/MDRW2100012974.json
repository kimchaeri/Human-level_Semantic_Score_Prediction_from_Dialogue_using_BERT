      "utterance": [
        {
          "id": "MDRW2100012974.1.1",
          "form": "학교 다니고 있으세요?",
          "original_form": "학교 다니고 있으세요?",
          "speaker_id": "1",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100012974.1.2",
          "form": "네! 이번 학기 재학중이에요",
          "original_form": "네! 이번 학기 재학중이에요",
          "speaker_id": "2",
          "time": "20210825 09:48"
        },
        {
          "id": "MDRW2100012974.1.3",
          "form": "지금 방학인가요 ?",
          "original_form": "지금 방학인가요 ?",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012974.1.4",
          "form": "네 아직 개학을 하지 않았어요",
          "original_form": "네 아직 개학을 하지 않았어요",
          "speaker_id": "2",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100012974.1.5",
          "form": "다음달부터 다니시나요??",
          "original_form": "다음달부터 다니시나요??",
          "speaker_id": "1",
          "time": "20210825 09:51"
        },
        {
          "id": "MDRW2100012974.1.6",
          "form": "9월 초부터 다녀요!",
          "original_form": "9월 초부터 다녀요!",
          "speaker_id": "2",
          "time": "20210825 09:51"
        },
        {
          "id": "MDRW2100012974.1.7",
          "form": "name1님은 학교를 다니고 계신가요?",
          "original_form": "&name1&님은 학교를 다니고 계신가요?",
          "speaker_id": "2",
          "time": "20210825 09:51"
        },
        {
          "id": "MDRW2100012974.1.8",
          "form": "저는 졸업을 해서 ㅜㅜ 과제가 많은 편인가요??",
          "original_form": "저는 졸업을 해서 ㅜㅜ 과제가 많은 편인가요??",
          "speaker_id": "1",
          "time": "20210825 09:52"
        },
        {
          "id": "MDRW2100012974.1.9",
          "form": "그렇군요 네 다른학과보다 과제가 많는 편이에요...",
          "original_form": "그렇군요 네 다른학과보다 과제가 많는 편이에요...",
          "speaker_id": "2",
          "time": "20210825 09:53"
        },
        {
          "id": "MDRW2100012974.1.10",
          "form": "저번 학기에는 매주 과제를 안하는 날이 없었어요",
          "original_form": "저번 학기에는 매주 과제를 안하는 날이 없었어요",
          "speaker_id": "2",
          "time": "20210825 09:53"
        },
        {
          "id": "MDRW2100012974.1.11",
          "form": "조별과제가 제일 힘들지않나요??",
          "original_form": "조별과제가 제일 힘들지않나요??",
          "speaker_id": "1",
          "time": "20210825 09:54"
        },
        {
          "id": "MDRW2100012974.1.12",
          "form": "저는 조별과제는 별로 없는 학과라서 많이 경험해보진 않았어요",
          "original_form": "저는 조별과제는 별로 없는 학과라서 많이 경험해보진 않았어요",
          "speaker_id": "2",
          "time": "20210825 09:55"
        },
        {
          "id": "MDRW2100012974.1.13",
          "form": "조별과제가 힘드셨나요?",
          "original_form": "조별과제가 힘드셨나요?",
          "speaker_id": "2",
          "time": "20210825 09:55"
        },
        {
          "id": "MDRW2100012974.1.14",
          "form": "네.. 무임승차가 많아서.. 제가 거의 하느라 ㅎㅎ",
          "original_form": "네.. 무임승차가 많아서.. 제가 거의 하느라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 10:11"
        },
        {
          "id": "MDRW2100012974.1.15",
          "form": "학업 스트레스가 많으셨겠어요 ㅠㅠ",
          "original_form": "학업 스트레스가 많으셨겠어요 ㅠㅠ",
