      "utterance": [
        {
          "id": "MDRW2100030177.1.1",
          "form": "난 짜장면이 좋아",
          "original_form": "난 짜장면이 좋아",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030177.1.2",
          "form": "간짜장이 아니고 짜장면인데도?",
          "original_form": "간짜장이 아니고 짜장면인데도?",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030177.1.3",
          "form": "응 짜장면엔 양파도 들어있잖어",
          "original_form": "응 짜장면엔 양파도 들어있잖어",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030177.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030177.1.5",
          "form": "양파 없어 짜장면에는",
          "original_form": "양파 없어 짜장면에는",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030177.1.6",
          "form": "고기랑 양파 없댔어",
          "original_form": "고기랑 양파 없댔어",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030177.1.7",
          "form": "진짜야?",
          "original_form": "진짜야?",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030177.1.8",
          "form": "나는 그냥 짜장면일거면",
          "original_form": "나는 그냥 짜장면일거면",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030177.1.9",
          "form": "간짜장에만 들어잇어?",
          "original_form": "간짜장에만 들어잇어?",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030177.1.10",
          "form": "짜장면이 좋아",
          "original_form": "짜장면이 좋아",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.11",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.12",
          "form": "짬뽕은 짬뽕 국물로 충분해",
          "original_form": "짬뽕은 짬뽕 국물로 충분해",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.13",
          "form": "헐...충격적인 사실이다..",
          "original_form": "헐...충격적인 사실이다..",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.14",
          "form": "그게 짬뽕의 정체성이야",
          "original_form": "그게 짬뽕의 정체성이야",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.15",
          "form": "일리 있네",
          "original_form": "일리 있네",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.16",
          "form": "짬뽕은 그냥 곁들이는거지",
          "original_form": "짬뽕은 그냥 곁들이는거지",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.17",
          "form": "짜장면이 메인이구 ㅎㅎ",
          "original_form": "짜장면이 메인이구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.18",
          "form": "나도 그렇게생각해",
          "original_form": "나도 그렇게생각해",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.19",
          "form": "그럼 다음엔",
          "original_form": "그럼 다음엔",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.20",
          "form": "짜장면 먹자!",
          "original_form": "짜장면 먹자!",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.21",
          "form": "그래 ㅎㅎ 맛있겠다",
          "original_form": "그래 ㅎㅎ 맛있겠다",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.22",
          "form": "그리고 탕수육도 ㅎㅎ",
          "original_form": "그리고 탕수육도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030177.1.23",
          "form": "민트초코 있자나",
          "original_form": "민트초코 있자나",
          "speaker_id": "1",
          "time": "20210903 22:48"
        }
      ]
