      "utterance": [
        {
          "id": "MDRW2100038739.1.1",
          "form": "글쎄요...몇년전 오뗄두스 이런 정도?",
          "original_form": "글쎄요...몇년전 오뗄두스 이런 정도?",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100038739.1.2",
          "form": "10년전 스놉?",
          "original_form": "10년전 스놉?",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100038739.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다 너무 과거네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다 너무 과거네요",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100038739.1.4",
          "form": "그리고 인천에선 멀고ㅠㅠ",
          "original_form": "그리고 인천에선 멀고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100038739.1.5",
          "form": "오뗄두스와 스놉을",
          "original_form": "오뗄두스와 스놉을",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100038739.1.6",
          "form": "십년전이라면 추천합니다",
          "original_form": "십년전이라면 추천합니다",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100038739.1.7",
          "form": "오뗄두스는 그래도 함 가보고 싶네요.",
          "original_form": "오뗄두스는 그래도 함 가보고 싶네요.",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100038739.1.8",
          "form": "여러 번 들어 본 가게라...",
          "original_form": "여러 번 들어 본 가게라...",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100038739.1.9",
          "form": "컬리로도 배달돼요",
          "original_form": "컬리로도 배달돼요",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100038739.1.10",
          "form": "단 좀 단단한 제품만..",
          "original_form": "단 좀 단단한 제품만..",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100038739.1.11",
          "form": "오잉또잉 배달 되는 줄은 몰랐네요",
          "original_form": "오잉또잉 배달 되는 줄은 몰랐네요",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100038739.1.12",
          "form": "까눌레는 배달될걸요",
          "original_form": "까눌레는 배달될걸요",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038739.1.13",
          "form": "다음에 컬리 배달 시킬 때 함 먹어봐야 겠어요",
          "original_form": "다음에 컬리 배달 시킬 때 함 먹어봐야 겠어요",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038739.1.14",
          "form": "바스라지는 얇은 파이크림 적층이건 안되구",
          "original_form": "바스라지는 얇은 파이크림 적층이건 안되구",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038739.1.15",
          "form": "까눌레는 추천!",
          "original_form": "까눌레는 추천!",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038739.1.16",
          "form": "ㅋㅋㅋㅋ 넹 까...눌...레... 기억해두겠습니다.",
          "original_form": "ㅋㅋㅋㅋ 넹 까...눌...레... 기억해두겠습니다.",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038739.1.17",
          "form": "겉은 좀 타구 속은 쫄깃한 빵이에요",
          "original_form": "겉은 좀 타구 속은 쫄깃한 빵이에요",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100038739.1.18",
          "form": "앗 근데 단 맛은 없나요?",
          "original_form": "앗 근데 단 맛은 없나요?",
          "speaker_id": "2",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100038739.1.19",
          "form": "좀 달아요 디저트 빵이에요",
          "original_form": "좀 달아요 디저트 빵이에요",
          "speaker_id": "1",
          "time": "20210902 20:40"
        }
      ]
