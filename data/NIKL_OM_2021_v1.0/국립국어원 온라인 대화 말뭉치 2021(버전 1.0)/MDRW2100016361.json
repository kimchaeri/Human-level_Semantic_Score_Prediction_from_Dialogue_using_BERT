      "utterance": [
        {
          "id": "MDRW2100016361.1.1",
          "form": "안녕하세요 ",
          "original_form": "안녕하세요 ",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016361.1.2",
          "form": "네 점심시간이신가요ㅋ",
          "original_form": "네 점심시간이신가요ㅋ",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016361.1.3",
          "form": "네네 식사뭐드세요?",
          "original_form": "네네 식사뭐드세요?",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016361.1.4",
          "form": "저흰 회사 밥이 그냥 나와요",
          "original_form": "저흰 회사 밥이 그냥 나와요",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016361.1.5",
          "form": "우와 저는 매번 뭐먹을지 걱정인데 걱정없으시겠네요!",
          "original_form": "우와 저는 매번 뭐먹을지 걱정인데 걱정없으시겠네요!",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016361.1.6",
          "form": "네 맞아요 맘은 편해요ㅋㅋ",
          "original_form": "네 맞아요 맘은 편해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016361.1.7",
          "form": "어떤업무하세요??",
          "original_form": "어떤업무하세요??",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016361.1.8",
          "form": "저는 연구직이에요ㅎㅎ",
          "original_form": "저는 연구직이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016361.1.9",
          "form": "어떤 업무하세요?",
          "original_form": "어떤 업무하세요?",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016361.1.10",
          "form": "아하 저는 사무직이예요! 우와 연구원 멋있네요",
          "original_form": "아하 저는 사무직이예요! 우와 연구원 멋있네요",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016361.1.11",
          "form": "현실은 ㅠㅠㅋㅋ",
          "original_form": "현실은 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016361.1.12",
          "form": "다들 고생하시죠",
          "original_form": "다들 고생하시죠",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016361.1.13",
          "form": "아아,, 연구원은 다 흰색가운입고 일하나요??ㅎㅎ",
          "original_form": "아아,, 연구원은 다 흰색가운입고 일하나요??ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016361.1.14",
          "form": "아녜요 그건 이공계 쪽이겠지요?ㅋㅋ",
          "original_form": "아녜요 그건 이공계 쪽이겠지요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016361.1.15",
          "form": "저는 문과예요ㅋㅋ",
          "original_form": "저는 문과예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016361.1.16",
          "form": "내일이 금욜이네요!!내일만 일하고 주말이라 넘좋네요 ㅎㅎ",
          "original_form": "내일이 금욜이네요!!내일만 일하고 주말이라 넘좋네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100016361.1.17",
          "form": "네 금요일은 기분좋죠ㄱㄲ",
          "original_form": "네 금요일은 기분좋죠ㄱㄲ",
          "speaker_id": "2",
          "time": "20210827 10:13"
        },
        {
          "id": "MDRW2100016361.1.18",
          "form": "빨리 나가고 싶지만ㅋ",
          "original_form": "빨리 나가고 싶지만ㅋ",
          "speaker_id": "2",
          "time": "20210827 10:13"
        },
        {
          "id": "MDRW2100016361.1.19",
          "form": "네 그러게요 점심먹으러 나왔는데,날씨가 좋네요!!",
          "original_form": "네 그러게요 점심먹으러 나왔는데,날씨가 좋네요!!",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100016361.1.20",
          "form": "아 여긴 아직 흐리네요",
          "original_form": "아 여긴 아직 흐리네요",
          "speaker_id": "2",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100016361.1.21",
          "form": "매번 사드세요?",
          "original_form": "매번 사드세요?",
          "speaker_id": "2",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100016361.1.22",
          "form": "네 ㅜㅎㅎ 급여에식비는포함이긴한데 항상초과하는것같아요 ㅎㅎ오늘은짬뽕먹었어요",
          "original_form": "네 ㅜㅎㅎ 급여에식비는포함이긴한데 항상초과하는것같아요 ㅎㅎ오늘은짬뽕먹었어요",
          "speaker_id": "1",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100016361.1.23",
          "form": "ㅎㅎ 짬뽕 좋죠",
          "original_form": "ㅎㅎ 짬뽕 좋죠",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100016361.1.24",
          "form": "동료분과 같이 드세요?",
          "original_form": "동료분과 같이 드세요?",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100016361.1.25",
          "form": "네 동료들이 나이대가 다들비슷해서 같이먹고있어요~~유일한 쉬는시간이져 ㅎㅎ",
          "original_form": "네 동료들이 나이대가 다들비슷해서 같이먹고있어요~~유일한 쉬는시간이져 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:34"
        },
        {
          "id": "MDRW2100016361.1.26",
          "form": "와 사이가 좋으신가봐요",
          "original_form": "와 사이가 좋으신가봐요",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100016361.1.27",
          "form": "복받으셨다",
          "original_form": "복받으셨다",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100016361.1.28",
          "form": "아 거기는 안좋으신가요??,,ㅎㅎ,, 지점에서 영업지원업무하는데 다른지점 총무들이랑 나이대가 비슷해서 같이잘지내고있어요 ㅎㅎ",
