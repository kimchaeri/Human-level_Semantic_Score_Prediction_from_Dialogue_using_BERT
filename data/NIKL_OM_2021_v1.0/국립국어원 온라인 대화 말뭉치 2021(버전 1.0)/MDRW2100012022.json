      "utterance": [
        {
          "id": "MDRW2100012022.1.1",
          "form": "유튜브 자주보시나요",
          "original_form": "유튜브 자주보시나요",
          "speaker_id": "1",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100012022.1.2",
          "form": "유투브보단 넷플을 많이 봐요:)",
          "original_form": "유투브보단 넷플을 많이 봐요:)",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012022.1.3",
          "form": "가가구님은요?",
          "original_form": "가가구님은요?",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012022.1.4",
          "form": "오 저도 넷플봐요!",
          "original_form": "오 저도 넷플봐요!",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012022.1.5",
          "form": "넷플 뭐 보세요?",
          "original_form": "넷플 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012022.1.6",
          "form": "킹덤좋아해요!",
          "original_form": "킹덤좋아해요!",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012022.1.7",
          "form": "대부분 킹덤으로\n넷플 입문 하시더라구요ㅋㅋ",
          "original_form": "대부분 킹덤으로\n넷플 입문 하시더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012022.1.8",
          "form": "어떤거 보시나요?",
          "original_form": "어떤거 보시나요?",
          "speaker_id": "1",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100012022.1.9",
          "form": "저는 슬의생 보고 있어요:)",
          "original_form": "저는 슬의생 보고 있어요:)",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100012022.1.10",
          "form": "알고있지만도 봤었는데\n지난주에 끝났어요ㅋ",
          "original_form": "알고있지만도 봤었는데\n지난주에 끝났어요ㅋ",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100012022.1.11",
          "form": "슬의생 시즌1부터 봐야하는데",
          "original_form": "슬의생 시즌1부터 봐야하는데",
          "speaker_id": "1",
          "time": "20210824 19:59"
        },
        {
          "id": "MDRW2100012022.1.12",
          "form": "시즌1부터 꼭 보세요",
          "original_form": "시즌1부터 꼭 보세요",
          "speaker_id": "2",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012022.1.13",
          "form": "휴지 꼭 준비하시구염ㅋㅋ",
          "original_form": "휴지 꼭 준비하시구염ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012022.1.14",
          "form": "오 좀 슬픈가요",
          "original_form": "오 좀 슬픈가요",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012022.1.15",
          "form": "매회마다 갬동의 스나미가ㅠㅠㅋ",
          "original_form": "매회마다 갬동의 스나미가ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012022.1.16",
          "form": "저는 감정이 메말라서ㅋㅋ",
          "original_form": "저는 감정이 메말라서ㅋㅋ",
          "speaker_id": "1",
