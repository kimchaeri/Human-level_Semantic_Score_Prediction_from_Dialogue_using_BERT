        },
        {
          "id": "MDRW2100031177.1.2",
          "form": "잘하네 민우씨",
          "original_form": "잘하네 민우씨",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031177.1.3",
          "form": "다시 쉬시가 됐지뭐야",
          "original_form": "다시 쉬시가 됐지뭐야",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031177.1.4",
          "form": "그치ㅠ",
          "original_form": "그치ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031177.1.5",
          "form": "굉장히 빠르게 끝내셨네",
          "original_form": "굉장히 빠르게 끝내셨네",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031177.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031177.1.7",
          "form": "크크 민우 잘하니까 점수내죠,, 십승해야하는디",
          "original_form": "크크 민우 잘하니까 점수내죠,, 십승해야하는디",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031177.1.8",
          "form": "김민우! 국대도 했었당",
          "original_form": "김민우! 국대도 했었당",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031177.1.9",
          "form": "이번 올림픽에서",
          "original_form": "이번 올림픽에서",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031177.1.10",
          "form": "아아 알아알아",
          "original_form": "아아 알아알아",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031177.1.11",
          "form": "야구 다 보지는 않았지만",
          "original_form": "야구 다 보지는 않았지만",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031177.1.12",
          "form": "대강 봤어",
          "original_form": "대강 봤어",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031177.1.13",
          "form": "꺆 정은원 안타 오웅 오랜만이야ㅠ 찐안타",
          "original_form": "꺆 정은원 안타 오웅 오랜만이야ㅠ 찐안타",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031177.1.14",
          "form": "정은원 너무 귀여으ㅝ,,,",
          "original_form": "정은원 너무 귀여으ㅝ,,,",
          "speaker_id": "1",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031177.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ찐팬이네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ찐팬이네",
          "speaker_id": "2",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031177.1.16",
          "form": "야구잘해서 계속보고싶어",
          "original_form": "야구잘해서 계속보고싶어",
          "speaker_id": "1",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031177.1.17",
          "form": "2군안갔으면,,",
          "original_form": "2군안갔으면,,",
          "speaker_id": "1",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031177.1.18",
          "form": "정은원?",
          "original_form": "정은원?",
          "speaker_id": "2",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031177.1.19",
          "form": "응응 애기야애기 한화의 1번",
          "original_form": "응응 애기야애기 한화의 1번",
          "speaker_id": "1",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031177.1.20",
          "form": "전반기?에 잘했다더라",
          "original_form": "전반기?에 잘했다더라",
          "speaker_id": "1",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031177.1.21",
          "form": "지금은 후반기ㅠ",
          "original_form": "지금은 후반기ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031177.1.22",
          "form": "2000년생 내야수네",
          "original_form": "2000년생 내야수네",
          "speaker_id": "2",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031177.1.23",
          "form": "요즘 좀 잘안되는편이래ㅠ",
          "original_form": "요즘 좀 잘안되는편이래ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031177.1.24",
          "form": "그치 이번 시즌 슬슬 마무리하는 감이지",
          "original_form": "그치 이번 시즌 슬슬 마무리하는 감이지",
          "speaker_id": "2",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031177.1.25",
          "form": "응?응?아니 정은원의 타격이?!랑 출루가",
          "original_form": "응?응?아니 정은원의 타격이?!랑 출루가",
          "speaker_id": "1",
          "time": "20210904 17:54"
        },
        {
          "id": "MDRW2100031177.1.26",
          "form": "정은원이 볼넷 야구 올해1등이래",
          "original_form": "정은원이 볼넷 야구 올해1등이래",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031177.1.27",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031177.1.28",
          "form": "ㅋㅋㅋㅋ진짜 아까두 볼넷으루 나가쏘",
          "original_form": "ㅋㅋㅋㅋ진짜 아까두 볼넷으루 나가쏘",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031177.1.29",
          "form": "근데 찐애기네",
          "original_form": "근데 찐애기네",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031177.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ공을 잘 보는구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ공을 잘 보는구나",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031177.1.31",
          "form": "응응 나도 찾아보고 놀랐다",
          "original_form": "응응 나도 찾아보고 놀랐다",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031177.1.32",
          "form": "한화 진짜 어린애들 많아!",
          "original_form": "한화 진짜 어린애들 많아!",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100031177.1.33",
          "form": "최재훈도 출루했으 볼넷 지금 한화 경기중에",
          "original_form": "최재훈도 출루했으 볼넷 지금 한화 경기중에",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100031177.1.34",
          "form": "주자 1,2루 노아웃 ^^",
          "original_form": "주자 1,2루 노아웃 ^^",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100031177.1.35",
          "form": "기회당ㅠㅠ",
          "original_form": "기회당ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100031177.1.36",
          "form": "최재훈은 포수인데 잘하더라 나홈런두봤지모야",
