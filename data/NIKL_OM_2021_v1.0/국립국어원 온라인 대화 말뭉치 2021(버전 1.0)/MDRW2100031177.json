      "utterance": [
        {
          "id": "MDRW2100031177.1.1",
          "form": "ㅋㅋㅋㅋ다시 야구군요",
          "original_form": "ㅋㅋㅋㅋ다시 야구군요",
          "speaker_id": "2",
