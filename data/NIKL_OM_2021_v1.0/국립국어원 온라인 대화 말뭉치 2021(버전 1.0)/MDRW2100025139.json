      "utterance": [
        {
          "id": "MDRW2100025139.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025139.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100025139.1.3",
          "form": "최근에 비가 많이 와서 날이 습하죠",
          "original_form": "최근에 비가 많이 와서 날이 습하죠",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100025139.1.4",
          "form": "네 지금도 비가와서 많이 습해요",
          "original_form": "네 지금도 비가와서 많이 습해요",
          "speaker_id": "1",
          "time": "20210831 23:45"
        },
        {
          "id": "MDRW2100025139.1.5",
          "form": "에어컨을 틀면 또 춥고요,,",
          "original_form": "에어컨을 틀면 또 춥고요,,",
          "speaker_id": "1",
          "time": "20210831 23:45"
        },
        {
          "id": "MDRW2100025139.1.6",
          "form": "애매한 날씨에요 심지어 낮에는 덥기까지 하네요",
          "original_form": "애매한 날씨에요 심지어 낮에는 덥기까지 하네요",
          "speaker_id": "2",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025139.1.7",
          "form": "네,, 전 여름옷을 정리하기 귀찮아서 좀 더 다뜻했음 좋겠지만",
          "original_form": "네,, 전 여름옷을 정리하기 귀찮아서 좀 더 다뜻했음 좋겠지만",
          "speaker_id": "1",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025139.1.8",
          "form": "지금의 날씨는 가을도 여름도 아닌 뭔가에요,,",
          "original_form": "지금의 날씨는 가을도 여름도 아닌 뭔가에요,,",
          "speaker_id": "1",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025139.1.9",
          "form": "내일이면 9월인데 빨리 시원해지면 좋겠어요",
          "original_form": "내일이면 9월인데 빨리 시원해지면 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025139.1.10",
          "form": "여름옷 정리하기는 귀찮지만 계속 습해서 기분이 별로에요",
          "original_form": "여름옷 정리하기는 귀찮지만 계속 습해서 기분이 별로에요",
          "speaker_id": "2",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025139.1.11",
          "form": "적당히 따뜻하며 안 습하면 좋겠어요!",
          "original_form": "적당히 따뜻하며 안 습하면 좋겠어요!",
          "speaker_id": "1",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100025139.1.12",
          "form": "태풍이 또 올라올까봐 걱정이에요",
          "original_form": "태풍이 또 올라올까봐 걱정이에요",
          "speaker_id": "1",
          "time": "20210831 23:51"
        },
        {
          "id": "MDRW2100025139.1.13",
          "form": "그러게요 비가 많이 오면 나가기도 힘든데 말이에요",
          "original_form": "그러게요 비가 많이 오면 나가기도 힘든데 말이에요",
          "speaker_id": "2",
          "time": "20210831 23:51"
        },
        {
          "id": "MDRW2100025139.1.14",
          "form": "추석전에 과일이 다 떨어질까봐 걱정이에요",
          "original_form": "추석전에 과일이 다 떨어질까봐 걱정이에요",
          "speaker_id": "1",
          "time": "20210831 23:53"
        },
        {
          "id": "MDRW2100025139.1.15",
          "form": "맞네요 태풍이 오면 과일값이 많이 오르겠네요",
          "original_form": "맞네요 태풍이 오면 과일값이 많이 오르겠네요",
          "speaker_id": "2",
          "time": "20210831 23:54"
        },
        {
          "id": "MDRW2100025139.1.16",
          "form": "태풍이 온다는 예보가 있던가요?",
          "original_form": "태풍이 온다는 예보가 있던가요?",
          "speaker_id": "2",
          "time": "20210831 23:54"
        },
        {
          "id": "MDRW2100025139.1.17",
          "form": "가을장마랑 가을 태풍 보통 이맘때즘 오니까요 ??",
          "original_form": "가을장마랑 가을 태풍 보통 이맘때즘 오니까요 ??",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025139.1.18",
          "form": "예보는 모르겠어요",
          "original_form": "예보는 모르겠어요",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025139.1.19",
          "form": "저도 예보는 모르겠네요 예보가 없었으니 이제 태풍은 그만 오지 않을까요",
          "original_form": "저도 예보는 모르겠네요 예보가 없었으니 이제 태풍은 그만 오지 않을까요",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025139.1.20",
          "form": "가을태풍이 오지 않으면 좋겠어요",
          "original_form": "가을태풍이 오지 않으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025139.1.21",
          "form": "한번 더 태풍이 오면 날이 본격적으로 쌀쌀해지겠죠",
          "original_form": "한번 더 태풍이 오면 날이 본격적으로 쌀쌀해지겠죠",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025139.1.22",
          "form": "검색해보니 당분간은 가을장마래요",
          "original_form": "검색해보니 당분간은 가을장마래요",
          "speaker_id": "1",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025139.1.23",
          "form": "장화를 꺼내놔야겠어요",
          "original_form": "장화를 꺼내놔야겠어요",
          "speaker_id": "1",
          "time": "20210901 00:00"
        }
      ]
    }
  ]
