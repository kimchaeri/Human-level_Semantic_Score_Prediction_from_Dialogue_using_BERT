      "utterance": [
        {
          "id": "MDRW2100004711.1.1",
          "form": "저녁시간인데 뭐드실 예정인가요?",
          "original_form": "저녁시간인데 뭐드실 예정인가요?",
          "speaker_id": "1",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004711.1.2",
          "form": "방금 저녁먹고 왔어요!",
          "original_form": "방금 저녁먹고 왔어요!",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004711.1.3",
          "form": "부대찌개 먹고왔어요 ㅎㅎ",
          "original_form": "부대찌개 먹고왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004711.1.4",
          "form": "오 빨리드셨네요",
          "original_form": "오 빨리드셨네요",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004711.1.5",
          "form": "부대찌개 맛있죠",
          "original_form": "부대찌개 맛있죠",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004711.1.6",
          "form": "배달이 예상외로 빨리와서",
          "original_form": "배달이 예상외로 빨리와서",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004711.1.7",
          "form": "빨리먹었네요 ㅋㅋ",
          "original_form": "빨리먹었네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004711.1.8",
          "form": "아 배달시키셨군요",
          "original_form": "아 배달시키셨군요",
          "speaker_id": "1",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004711.1.9",
          "form": "체인시키셨어요?",
          "original_form": "체인시키셨어요?",
          "speaker_id": "1",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004711.1.10",
          "form": "체인인진 잘 모르겠는데",
          "original_form": "체인인진 잘 모르겠는데",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004711.1.11",
          "form": "아무튼 맛있더라고요",
          "original_form": "아무튼 맛있더라고요",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004711.1.12",
          "form": "쿵쓰부대찌개?",
          "original_form": "쿵쓰부대찌개?",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004711.1.13",
          "form": "였던거같아요",
          "original_form": "였던거같아요",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004711.1.14",
          "form": "첨들어보네요 가족분들 다같이 드신건가봐요",
          "original_form": "첨들어보네요 가족분들 다같이 드신건가봐요",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004711.1.15",
          "form": "네 가족들하고 먹었는데",
          "original_form": "네 가족들하고 먹었는데",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004711.1.16",
          "form": "양도 많고 좋았어요!",
          "original_form": "양도 많고 좋았어요!",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004711.1.17",
          "form": "요즘 배달음식도 맛있더라구요",
          "original_form": "요즘 배달음식도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004711.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004711.1.19",
          "form": "퀄리티도 장난없어서 ㄷㄷ.",
          "original_form": "퀄리티도 장난없어서 ㄷㄷ.",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004711.1.20",
          "form": "전 찜닭이 좋던데  혼자먹기 넘 많아서 시키질 못해요",
          "original_form": "전 찜닭이 좋던데  혼자먹기 넘 많아서 시키질 못해요",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004711.1.21",
          "form": "찜닭도 1인분 용이 있으면 좋을련데",
          "original_form": "찜닭도 1인분 용이 있으면 좋을련데",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004711.1.22",
          "form": "저도 찜닭마니아인데 다 못먹고 가족들 줘요",
          "original_form": "저도 찜닭마니아인데 다 못먹고 가족들 줘요",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004711.1.23",
          "form": "그니까요 반마리도 넘비싸고",
          "original_form": "그니까요 반마리도 넘비싸고",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004711.1.24",
          "form": "ㅠㅠ 그러게요",
          "original_form": "ㅠㅠ 그러게요",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004711.1.25",
          "form": "저녁드셨으니 이제 후식드셔야죠",
          "original_form": "저녁드셨으니 이제 후식드셔야죠",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004711.1.26",
          "form": "과일~",
          "original_form": "과일~",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004711.1.27",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004711.1.28",
          "form": "청사과 있으니 그거 먹어야겠어요",
          "original_form": "청사과 있으니 그거 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004711.1.29",
          "form": "상큼하겠어요ㅎ 맛나게 드세요^^",
          "original_form": "상큼하겠어요ㅎ 맛나게 드세요^^",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004711.1.30",
          "form": "네 감사합니다!",
          "original_form": "네 감사합니다!",
          "speaker_id": "2",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004711.1.31",
          "form": "맛있네요!ㅎㅎ",
          "original_form": "맛있네요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:54"
        }
      ]
    }
