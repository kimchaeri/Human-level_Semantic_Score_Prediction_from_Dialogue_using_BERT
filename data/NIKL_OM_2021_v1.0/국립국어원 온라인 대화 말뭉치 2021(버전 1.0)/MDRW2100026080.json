      "utterance": [
        {
          "id": "MDRW2100026080.1.1",
          "form": "안녕하세요 저녁 드셨나요???",
          "original_form": "안녕하세요 저녁 드셨나요???",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026080.1.2",
          "form": "네 저녁먹었어요!",
          "original_form": "네 저녁먹었어요!",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026080.1.3",
          "form": "간단하게 라면 먹었습니다 ㅎㅎ",
          "original_form": "간단하게 라면 먹었습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026080.1.4",
          "form": "라면 좋죠 ㅎㅎ",
          "original_form": "라면 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026080.1.5",
          "form": "무슨 라면 드셨어요??",
          "original_form": "무슨 라면 드셨어요??",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026080.1.6",
          "form": "왕뚜껑이요!",
          "original_form": "왕뚜껑이요!",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026080.1.7",
          "form": "오랜만에 땡겨서 ㅎㅎ",
          "original_form": "오랜만에 땡겨서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026080.1.8",
          "form": "오 컵라면 드신 건가요??",
          "original_form": "오 컵라면 드신 건가요??",
          "speaker_id": "1",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026080.1.9",
          "form": "저는 떡볶이에 중국당면 넣어서 먹었어요 ㅎㅎ",
          "original_form": "저는 떡볶이에 중국당면 넣어서 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026080.1.10",
          "form": "네 컵라면!",
          "original_form": "네 컵라면!",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026080.1.11",
          "form": "오 맛있겠네요!",
          "original_form": "오 맛있겠네요!",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026080.1.12",
          "form": "중국당면 엄청 좋아하는데 ㅠㅠ",
          "original_form": "중국당면 엄청 좋아하는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026080.1.13",
          "form": "그쵸 중당 너무 맛있어요 ㅠㅠ",
          "original_form": "그쵸 중당 너무 맛있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026080.1.14",
          "form": "오징어튀김도 먹고 싶네요...",
          "original_form": "오징어튀김도 먹고 싶네요...",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026080.1.15",
          "form": "갠적으로 찜닭에 중당 넣는거 엄청 좋아해요",
          "original_form": "갠적으로 찜닭에 중당 넣는거 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026080.1.16",
          "form": "오징어튀김 ㅠㅠㅠ",
          "original_form": "오징어튀김 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026080.1.17",
          "form": "비올때 이상하게 땡기는 음식 중 하나죠",
          "original_form": "비올때 이상하게 땡기는 음식 중 하나죠",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026080.1.18",
          "form": "찜닭 ㅠㅠㅠㅠ 맞아요",
          "original_form": "찜닭 ㅠㅠㅠㅠ 맞아요",
          "speaker_id": "1",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026080.1.19",
          "form": "막걸리도 같이 먹으면 진짜 좋을 것 같아요",
          "original_form": "막걸리도 같이 먹으면 진짜 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026080.1.20",
          "form": "크으.. 맞아요 ㅠㅠ",
          "original_form": "크으.. 맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026080.1.21",
          "form": "비가 오니까 맛있는 게 많이 생각나네용",
          "original_form": "비가 오니까 맛있는 게 많이 생각나네용",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100026080.1.22",
          "form": "특히 파전이랑 떡볶이",
          "original_form": "특히 파전이랑 떡볶이",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100026080.1.23",
          "form": "엄청 생각나요!",
          "original_form": "엄청 생각나요!",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100026080.1.24",
          "form": "맞아요 파전 ㅠㅠ",
          "original_form": "맞아요 파전 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100026080.1.25",
          "form": "해물 많이 들어간 파전",
          "original_form": "해물 많이 들어간 파전",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026080.1.26",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026080.1.27",
          "form": "맞아요~ㅠㅠ",
          "original_form": "맞아요~ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026080.1.28",
          "form": "좋아하시는 브랜드있으신가용?",
          "original_form": "좋아하시는 브랜드있으신가용?",
