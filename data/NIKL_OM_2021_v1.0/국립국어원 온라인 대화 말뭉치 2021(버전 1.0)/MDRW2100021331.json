      "utterance": [
        {
          "id": "MDRW2100021331.1.1",
          "form": "굿모닝",
          "original_form": "굿모닝",
          "speaker_id": "1",
          "time": "20210830 07:52"
        },
        {
          "id": "MDRW2100021331.1.2",
          "form": "ㅋㅋ 애들등교하고 잠깐앉아있음",
          "original_form": "ㅋㅋ 애들등교하고 잠깐앉아있음",
          "speaker_id": "2",
          "time": "20210830 08:23"
        },
        {
          "id": "MDRW2100021331.1.3",
          "form": "ㅋㅋㅋ언니 벌써 저녁이 되었네요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ언니 벌써 저녁이 되었네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100021331.1.4",
          "form": "오늘 넘나 바빴음...ㅋㅋㅋ내일부터는 프리합니다요 ㅋㅋ",
          "original_form": "오늘 넘나 바빴음...ㅋㅋㅋ내일부터는 프리합니다요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100021331.1.5",
          "form": "ㅋㅋ 그래 바쁜하루 있지 월요일이잖아ㅎㅎ",
          "original_form": "ㅋㅋ 그래 바쁜하루 있지 월요일이잖아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:29"
        },
        {
          "id": "MDRW2100021331.1.6",
          "form": "난 이제 밥다했다",
          "original_form": "난 이제 밥다했다",
          "speaker_id": "2",
          "time": "20210830 18:29"
        },
        {
          "id": "MDRW2100021331.1.7",
          "form": "저두요 ㅋㅋ 오늘은 아빠짜장날입니다. ㅋㅋ",
          "original_form": "저두요 ㅋㅋ 오늘은 아빠짜장날입니다. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100021331.1.8",
          "form": "언니는 뭐 먹어요?",
          "original_form": "언니는 뭐 먹어요?",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100021331.1.9",
          "form": "오늘 된찌ㅋㅋ",
          "original_form": "오늘 된찌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100021331.1.10",
          "form": "설거지 어쩌지?ㅡㅡ",
          "original_form": "설거지 어쩌지?ㅡㅡ",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100021331.1.11",
          "form": "아빠짜장맛있는데 사장싫어가 패스",
          "original_form": "아빠짜장맛있는데 사장싫어가 패스",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100021331.1.12",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100021331.1.13",
          "form": "ㅋㅋ 된찌좋네요ㅋㅋ",
          "original_form": "ㅋㅋ 된찌좋네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100021331.1.14",
          "form": "아 사장이 왜 싫다했죠?ㅋㅋ",
          "original_form": "아 사장이 왜 싫다했죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100021331.1.15",
          "form": "난 성격 시원하이 좋든데 ㅋㅋ",
          "original_form": "난 성격 시원하이 좋든데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100021331.1.16",
          "form": "내한테 목소리크다해서ㅡㅡ",
          "original_form": "내한테 목소리크다해서ㅡㅡ",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100021331.1.17",
          "form": "싫다ㅋㅈㅋ",
          "original_form": "싫다ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100021331.1.18",
          "form": "물건도 바꿔서 배달해놓고 미안하단말도없었음",
          "original_form": "물건도 바꿔서 배달해놓고 미안하단말도없었음",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100021331.1.19",
          "form": "암튼 그이후로 안녕임ㅋ",
          "original_form": "암튼 그이후로 안녕임ㅋ",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100021331.1.20",
          "form": "ㅋㅋㅋㅋㅋ아 웃겨ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아 웃겨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100021331.1.21",
          "form": "언니 맘에 담아둠ㅋ",
          "original_form": "언니 맘에 담아둠ㅋ",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100021331.1.22",
          "form": "어 뒤끝 대박ㅋㅋ",
          "original_form": "어 뒤끝 대박ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100021331.1.23",
          "form": "난 그릔가봐ㅡㅋ",
          "original_form": "난 그릔가봐ㅡㅋ",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100021331.1.24",
          "form": "ㅋㅋㅋ아 웃겨ㅋㅋㅋ 그럴수있죠ㅋㅋ",
          "original_form": "ㅋㅋㅋ아 웃겨ㅋㅋㅋ 그럴수있죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:35"
        },
        {
          "id": "MDRW2100021331.1.25",
          "form": "꽂힐때있음ㅋ",
          "original_form": "꽂힐때있음ㅋ",
          "speaker_id": "1",
          "time": "20210831 08:35"
        },
        {
          "id": "MDRW2100021331.1.26",
          "form": "ㅋㅋ 짜장맛있는데 아쉽네ㅋㅋ",
          "original_form": "ㅋㅋ 짜장맛있는데 아쉽네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 08:45"
        },
        {
          "id": "MDRW2100021331.1.27",
          "form": "집엉망인 채로 출근ㅋ",
          "original_form": "집엉망인 채로 출근ㅋ",
          "speaker_id": "2",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100021331.1.28",
          "form": "네 갔다와서 치우면되죠ㅋ",
          "original_form": "네 갔다와서 치우면되죠ㅋ",
          "speaker_id": "1",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100021331.1.29",
          "form": "아침에 전쟁이구먼ㅜ",
          "original_form": "아침에 전쟁이구먼ㅜ",
          "speaker_id": "2",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100021331.1.30",
          "form": "요즘 뺀질뺀질ㅈ말을 안들억ᆢㅡㅡ",
          "original_form": "요즘 뺀질뺀질ㅈ말을 안들억ᆢㅡㅡ",
          "speaker_id": "2",
          "time": "20210831 08:47"
        },
        {
          "id": "MDRW2100021331.1.31",
          "form": "누가요?ㅋ",
          "original_form": "누가요?ㅋ",
          "speaker_id": "2",
          "time": "20210831 08:47"
        },
        {
          "id": "MDRW2100021331.1.32",
