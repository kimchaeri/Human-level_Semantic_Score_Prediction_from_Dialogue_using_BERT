      "utterance": [
        {
          "id": "MDRW2100017284.1.1",
          "form": "주로 쇼핑은 어디서 하세요?",
          "original_form": "주로 쇼핑은 어디서 하세요?",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017284.1.2",
          "form": "네이버 쇼핑이요",
          "original_form": "네이버 쇼핑이요",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017284.1.3",
          "form": "네이버 쇼핑 포인트 적립 돼서 좋은 것 같아요.",
          "original_form": "네이버 쇼핑 포인트 적립 돼서 좋은 것 같아요.",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017284.1.4",
          "form": "그럼 네이버에서 최근에 뭐 쇼핑하셨어요?",
          "original_form": "그럼 네이버에서 최근에 뭐 쇼핑하셨어요?",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017284.1.5",
          "form": "최근엔 폰케이스 샀어요",
          "original_form": "최근엔 폰케이스 샀어요",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017284.1.6",
          "form": "폰케이스 어디서 샀어요?",
          "original_form": "폰케이스 어디서 샀어요?",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017284.1.7",
          "form": "네이버 오픈마켓?",
          "original_form": "네이버 오픈마켓?",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017284.1.8",
          "form": "네 예쁜거 많더라구요",
          "original_form": "네 예쁜거 많더라구요",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017284.1.9",
          "form": "젤리 케이스요?",
          "original_form": "젤리 케이스요?",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017284.1.10",
          "form": "아뇨 카드 넣을수 있는 케이스에요",
          "original_form": "아뇨 카드 넣을수 있는 케이스에요",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017284.1.11",
          "form": "되게 실용성 있는 거 사셨네요 ㅎㅎ",
          "original_form": "되게 실용성 있는 거 사셨네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017284.1.12",
          "form": "수납형 케이스네요~ 두꺼워요?",
          "original_form": "수납형 케이스네요~ 두꺼워요?",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017284.1.13",
          "form": "아뇨 마그네틱 카드케이스라 두껍진 않네요",
          "original_form": "아뇨 마그네틱 카드케이스라 두껍진 않네요",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017284.1.14",
          "form": "다행이네요 ㅎㅎ 맘에 들어요?",
          "original_form": "다행이네요 ㅎㅎ 맘에 들어요?",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017284.1.15",
          "form": "네 디자인도 예뻐서 좋아요",
          "original_form": "네 디자인도 예뻐서 좋아요",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017284.1.16",
          "form": "다행이네요~ 얼마주고 사셨어요?",
          "original_form": "다행이네요~ 얼마주고 사셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017284.1.17",
          "form": "만오천원? 정도 했던거 같아요",
          "original_form": "만오천원? 정도 했던거 같아요",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017284.1.18",
          "form": "저렴하게 잘사셨네요 ㅎㅎㅎ",
          "original_form": "저렴하게 잘사셨네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017284.1.19",
          "form": "할인해서 사신거에요?",
          "original_form": "할인해서 사신거에요?",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017284.1.20",
          "form": "네 할인 하고있었어요",
          "original_form": "네 할인 하고있었어요",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017284.1.21",
          "form": "쿠폰도 적용했어요?",
          "original_form": "쿠폰도 적용했어요?",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017284.1.22",
          "form": "아니요ㅋㅋ 쿠폰은 없어서요",
          "original_form": "아니요ㅋㅋ 쿠폰은 없어서요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        }
      ]
