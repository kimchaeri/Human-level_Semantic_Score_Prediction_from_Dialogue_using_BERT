      "utterance": [
        {
          "id": "MDRW2100032450.1.1",
          "form": "집안일 꿀팁?",
          "original_form": "집안일 꿀팁?",
          "speaker_id": "2",
          "time": "20210905 18:02"
        },
        {
          "id": "MDRW2100032450.1.2",
          "form": "김빠진 콜라로 변기 청소라던지",
          "original_form": "김빠진 콜라로 변기 청소라던지",
          "speaker_id": "1",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100032450.1.3",
          "form": "양파는 껍질 까서 랩으로 싸서 내장보관!",
          "original_form": "양파는 껍질 까서 랩으로 싸서 내장보관!",
          "speaker_id": "2",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100032450.1.4",
          "form": "그러면 좋아?",
          "original_form": "그러면 좋아?",
          "speaker_id": "1",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100032450.1.5",
          "form": "한달은 보관 가능하더라고",
          "original_form": "한달은 보관 가능하더라고",
          "speaker_id": "2",
          "time": "20210905 18:05"
        },
        {
          "id": "MDRW2100032450.1.6",
          "form": "우리는 껍질 안까고 밖에 내놓는데",
          "original_form": "우리는 껍질 안까고 밖에 내놓는데",
          "speaker_id": "1",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100032450.1.7",
          "form": "양파는 곰팡이가 잘 생기더라고",
          "original_form": "양파는 곰팡이가 잘 생기더라고",
          "speaker_id": "2",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100032450.1.8",
          "form": "맞아 한 개 곰팡이 생기면 옮아서 안좋아",
          "original_form": "맞아 한 개 곰팡이 생기면 옮아서 안좋아",
          "speaker_id": "1",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100032450.1.9",
          "form": "아까워 그래서 냉장보관해",
          "original_form": "아까워 그래서 냉장보관해",
          "speaker_id": "2",
          "time": "20210905 18:06"
        },
        {
          "id": "MDRW2100032450.1.10",
          "form": "전에 티비에서 봤는데",
          "original_form": "전에 티비에서 봤는데",
          "speaker_id": "1",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100032450.1.11",
          "form": "양파를 못쓰는 스타킹에",
          "original_form": "양파를 못쓰는 스타킹에",
          "speaker_id": "1",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100032450.1.12",
          "form": "스타킹을?",
          "original_form": "스타킹을?",
          "speaker_id": "2",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100032450.1.13",
          "form": "하나씩 줄줄이 소세지처럼 묶어서 걸어놓으면 돟다고",
          "original_form": "하나씩 줄줄이 소세지처럼 묶어서 걸어놓으면 돟다고",
          "speaker_id": "1",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100032450.1.14",
          "form": "줄줄이 소새지 ㅋㅋㅋ",
          "original_form": "줄줄이 소새지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:08"
        },
        {
          "id": "MDRW2100032450.1.15",
          "form": "서늘하고 바람 통하면 좋은 것 같긴 하더라",
          "original_form": "서늘하고 바람 통하면 좋은 것 같긴 하더라",
          "speaker_id": "2",
          "time": "20210905 18:08"
        }
      ]
