      "utterance": [
        {
          "id": "MDRW2100037327.1.1",
          "form": "너 유산균 뭐 먹어?",
          "original_form": "너 유산균 뭐 먹어?",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037327.1.2",
          "form": "나 요즘 장상태가 말이아냐",
          "original_form": "나 요즘 장상태가 말이아냐",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100037327.1.3",
          "form": "흠 한번 알아보고있는데",
          "original_form": "흠 한번 알아보고있는데",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.4",
          "form": "우리 건강한 모습으로 만나야할텐데 걱정이네.",
          "original_form": "우리 건강한 모습으로 만나야할텐데 걱정이네.",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.5",
          "form": "질유산균을 먹을까 생각중이야",
          "original_form": "질유산균을 먹을까 생각중이야",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.6",
          "form": "내친구도 그거 강추하더라",
          "original_form": "내친구도 그거 강추하더라",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.7",
          "form": "여성필수영양제라고 하더라",
          "original_form": "여성필수영양제라고 하더라",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.8",
          "form": "근데 그거 진짜로 효과가 있을까?",
          "original_form": "근데 그거 진짜로 효과가 있을까?",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.9",
          "form": "이제 영양제를 꾝 챙겨먹어여겠어",
          "original_form": "이제 영양제를 꾝 챙겨먹어여겠어",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.10",
          "form": "난 항상 영양제들에대한 불신이 있어",
          "original_form": "난 항상 영양제들에대한 불신이 있어",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.11",
          "form": "평소 질 건강이 안좋은 애들은 체감하더라",
          "original_form": "평소 질 건강이 안좋은 애들은 체감하더라",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.12",
          "form": "흠 궁금하긴하다",
          "original_form": "흠 궁금하긴하다",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.13",
          "form": "그리고 영양제들은 챙겨먹기가 너무 귀찮아",
          "original_form": "그리고 영양제들은 챙겨먹기가 너무 귀찮아",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.14",
          "form": "일단 그 효능이 쌓이면 나중에 좋은 일이 있을거야",
          "original_form": "일단 그 효능이 쌓이면 나중에 좋은 일이 있을거야",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037327.1.15",
          "form": "우린 운동을 잘 안하니 영양제라도 먹자.",
          "original_form": "우린 운동을 잘 안하니 영양제라도 먹자.",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037327.1.16",
          "form": "그렇구나 이제 챙겨먹을 나이긴하지",
          "original_form": "그렇구나 이제 챙겨먹을 나이긴하지",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037327.1.17",
          "form": "좋아 운동보단 덜귀찮아",
          "original_form": "좋아 운동보단 덜귀찮아",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037327.1.18",
          "form": "어쨌든 좋은 거 찾으면 연락줘.",
          "original_form": "어쨌든 좋은 거 찾으면 연락줘.",
          "speaker_id": "2",
          "time": "20210831 22:57"
        }
      ]
