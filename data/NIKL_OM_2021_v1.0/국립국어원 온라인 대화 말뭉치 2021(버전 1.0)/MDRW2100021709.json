      "utterance": [
        {
          "id": "MDRW2100021709.1.1",
          "form": "좋아하는 프로그램 있으세용?",
          "original_form": "좋아하는 프로그램 있으세용?",
          "speaker_id": "1",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021709.1.2",
          "form": "저는 심야괴담회 즐겨봐용",
          "original_form": "저는 심야괴담회 즐겨봐용",
          "speaker_id": "1",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021709.1.3",
          "form": "저는 대탈출이용",
          "original_form": "저는 대탈출이용",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021709.1.4",
          "form": "슬기로운의사생활도 좋아해요!",
          "original_form": "슬기로운의사생활도 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021709.1.5",
          "form": "앗 저도 대탈출 봐요ㅋㅋㅋㅋ 슬기로운 핫하더라구요",
          "original_form": "앗 저도 대탈출 봐요ㅋㅋㅋㅋ 슬기로운 핫하더라구요",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021709.1.6",
          "form": "대탈출 저번주에 못봐가지고ㅠ",
          "original_form": "대탈출 저번주에 못봐가지고ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021709.1.7",
          "form": "내일 오후4시30분인가? 그때하는거 보려구요",
          "original_form": "내일 오후4시30분인가? 그때하는거 보려구요",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021709.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021709.1.9",
          "form": "지난주꺼 되게 재밌었어요!",
          "original_form": "지난주꺼 되게 재밌었어요!",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021709.1.10",
          "form": "이번시즌 회차중에 제일 재밌더라구요ㅋㅋㅋ",
          "original_form": "이번시즌 회차중에 제일 재밌더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021709.1.11",
          "form": "요새 계속 레전드편 찍는거 같아요",
          "original_form": "요새 계속 레전드편 찍는거 같아요",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021709.1.12",
          "form": "마자요 그리고 출연진들이",
          "original_form": "마자요 그리고 출연진들이",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021709.1.13",
          "form": "초반 시즌보다 겁도 많이 줄어들어서",
          "original_form": "초반 시즌보다 겁도 많이 줄어들어서",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021709.1.14",
          "form": "덜 답답해요ㅋㅋㅋ",
          "original_form": "덜 답답해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021709.1.15",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021709.1.16",
          "form": "여고추리반 보고",
          "original_form": "여고추리반 보고",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021709.1.17",
          "form": "답답해질까봐 걱정했는데",
          "original_form": "답답해질까봐 걱정했는데",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021709.1.18",
          "form": "단련이 된건지ㅋㅋㅋㅋㅋ",
          "original_form": "단련이 된건지ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100021709.1.19",
          "form": "여고추리반도 보셨군요ㅋㅋㅋㅋ 저도 그것때문에",
          "original_form": "여고추리반도 보셨군요ㅋㅋㅋㅋ 저도 그것때문에",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021709.1.20",
          "form": "티빙 결제해서 봤는데",
          "original_form": "티빙 결제해서 봤는데",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021709.1.21",
          "form": "알마치에 영업당하고ㅋㅋㅋ",
          "original_form": "알마치에 영업당하고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021709.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021709.1.23",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021709.1.24",
          "form": "결국 계속 시켜먹었죠",
          "original_form": "결국 계속 시켜먹었죠",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021709.1.25",
          "form": "알싸아아아ㅏ한 마늘치킨",
          "original_form": "알싸아아아ㅏ한 마늘치킨",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021709.1.26",
          "form": "요즘은 ppl도 재밌게 잘하더라구요",
          "original_form": "요즘은 ppl도 재밌게 잘하더라구요",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021709.1.27",
          "form": "예전에는 거부감 들었능데ㅋㅋㅋ",
          "original_form": "예전에는 거부감 들었능데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021709.1.28",
          "form": "대탈출 돈 많이 들어가는거 아니까",
          "original_form": "대탈출 돈 많이 들어가는거 아니까",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021709.1.29",
          "form": "ppl 계속 들어왔으면 좋겠어요",
          "original_form": "ppl 계속 들어왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021709.1.30",
          "form": "제작비가 어마무시...ㅋㅋㅋ 마자용",
          "original_form": "제작비가 어마무시...ㅋㅋㅋ 마자용",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021709.1.31",
          "form": "시즌 100까지 해주길!",
          "original_form": "시즌 100까지 해주길!",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021709.1.32",
          "form": "제발!!!",
          "original_form": "제발!!!",
          "speaker_id": "2",
          "time": "20210828 21:50"
        }
      ]
