      "utterance": [
        {
          "id": "MDRW2100010001.1.1",
          "form": "언니 오늘 저녁에 태풍지나간대요",
          "original_form": "언니 오늘 저녁에 태풍지나간대요",
          "speaker_id": "1",
          "time": "20210823 13:05"
        },
        {
          "id": "MDRW2100010001.1.2",
          "form": "그래 문단속하자ㅋㅋ",
          "original_form": "그래 문단속하자ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010001.1.3",
          "form": "태풍무섭",
          "original_form": "태풍무섭",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010001.1.4",
          "form": "요즘 그래도 미세먼지없어 살겠다이",
          "original_form": "요즘 그래도 미세먼지없어 살겠다이",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010001.1.5",
          "form": "그러니깐요. ㅜ",
          "original_form": "그러니깐요. ㅜ",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010001.1.6",
          "form": "미세먼지없어서 살겠는데",
          "original_form": "미세먼지없어서 살겠는데",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010001.1.7",
          "form": "요즘 비가 너무오는듯요ㅋㅋ",
          "original_form": "요즘 비가 너무오는듯요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010001.1.8",
          "form": "빨래 우짜노ㅡㅡ",
          "original_form": "빨래 우짜노ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010001.1.9",
          "form": "건조기는 줄어들고",
          "original_form": "건조기는 줄어들고",
          "speaker_id": "2",
          "time": "20210823 13:47"
        },
        {
          "id": "MDRW2100010001.1.10",
          "form": "뭐시 에라다",
          "original_form": "뭐시 에라다",
          "speaker_id": "2",
          "time": "20210823 13:47"
        },
        {
          "id": "MDRW2100010001.1.11",
          "form": "ㅋㅋㅋㅋ 저는 그냥 건조기 다돌려요.",
          "original_form": "ㅋㅋㅋㅋ 저는 그냥 건조기 다돌려요.",
          "speaker_id": "1",
          "time": "20210823 13:47"
        },
        {
          "id": "MDRW2100010001.1.12",
          "form": "작아질거 생각해서 큰사이즈로삼ㅋ",
          "original_form": "작아질거 생각해서 큰사이즈로삼ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010001.1.13",
          "form": "오호ㅋ",
          "original_form": "오호ㅋ",
          "speaker_id": "2",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010001.1.14",
          "form": "둘째 바지 사야되는데ㅋㅋㅋ",
          "original_form": "둘째 바지 사야되는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010001.1.15",
          "form": "이버주 내도록 비인강?",
          "original_form": "이버주 내도록 비인강?",
          "speaker_id": "2",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010001.1.16",
          "form": "아가방 옷이 ㅋㅋ 생각이남ㅋㅋ 푸핫ㅋㅋ",
          "original_form": "아가방 옷이 ㅋㅋ 생각이남ㅋㅋ 푸핫ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100010001.1.17",
          "form": "인터넷검색해봐야겠어요ㅋㄱ",
          "original_form": "인터넷검색해봐야겠어요ㅋㄱ",
          "speaker_id": "1",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100010001.1.18",
          "form": "이번주 내도록 비래요ㅜ",
          "original_form": "이번주 내도록 비래요ㅜ",
          "speaker_id": "1",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100010001.1.19",
          "form": "그래 가을옷없다ㅜ",
          "original_form": "그래 가을옷없다ㅜ",
          "speaker_id": "2",
          "time": "20210823 13:50"
        },
        {
          "id": "MDRW2100010001.1.20",
          "form": "작년 가을옷은 또 작아서",
          "original_form": "작년 가을옷은 또 작아서",
          "speaker_id": "1",
          "time": "20210823 13:50"
        },
        {
          "id": "MDRW2100010001.1.21",
          "form": "둘째 옷도 사야되요. ㅜㅋㄱ",
          "original_form": "둘째 옷도 사야되요. ㅜㅋㄱ",
          "speaker_id": "1",
          "time": "20210823 13:50"
        },
        {
          "id": "MDRW2100010001.1.22",
          "form": "돈 깨지는 소리가ㅋㅋ",
          "original_form": "돈 깨지는 소리가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:50"
        },
        {
          "id": "MDRW2100010001.1.23",
          "form": "비오니 꿉꿉해가 에어컨 틀까하다가 선풍기 3대 풀가동",
          "original_form": "비오니 꿉꿉해가 에어컨 틀까하다가 선풍기 3대 풀가동",
          "speaker_id": "2",
          "time": "20210823 14:52"
        },
        {
          "id": "MDRW2100010001.1.24",
          "form": "저희는 에어컨 틀었다가 지금껐어요. 춥네요ㅋㅋ",
          "original_form": "저희는 에어컨 틀었다가 지금껐어요. 춥네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010001.1.25",
          "form": "오늘밤은 문다닫고 자자ㅋㅋ",
          "original_form": "오늘밤은 문다닫고 자자ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010001.1.26",
          "form": "시원하다",
          "original_form": "시원하다",
          "speaker_id": "2",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010001.1.27",
          "form": "그러니깐요ㅋㅋ",
          "original_form": "그러니깐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010001.1.28",
          "form": "커피한잔의여유",
          "original_form": "커피한잔의여유",
          "speaker_id": "2",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010001.1.29",
          "form": "문닫고 자야됨ㅋㅋ",
          "original_form": "문닫고 자야됨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010001.1.30",
          "form": "둘찌 언제오",
          "original_form": "둘찌 언제오",
          "speaker_id": "2",
          "time": "20210823 15:59"
        }
      ]
    }
