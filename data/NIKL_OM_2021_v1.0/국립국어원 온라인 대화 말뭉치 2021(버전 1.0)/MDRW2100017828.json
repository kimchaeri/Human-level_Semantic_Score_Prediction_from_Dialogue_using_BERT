        {
          "id": "MDRW2100017828.1.3",
          "form": "군대..",
          "original_form": "군대..",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017828.1.4",
          "form": "군대는 역시 이등병의 노래 아닙니까",
          "original_form": "군대는 역시 이등병의 노래 아닙니까",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017828.1.5",
          "form": "저는 근래에",
          "original_form": "저는 근래에",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017828.1.6",
          "form": "군대 관련해서 익명러랑 싸움날뻔했어서",
          "original_form": "군대 관련해서 익명러랑 싸움날뻔했어서",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017828.1.7",
          "form": "약간 지긋지긋...",
          "original_form": "약간 지긋지긋...",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017828.1.8",
          "form": "어쩌다가요?",
          "original_form": "어쩌다가요?",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017828.1.9",
          "form": "제가 무슨글 올렸더니 득달같이 달려들어서...",
          "original_form": "제가 무슨글 올렸더니 득달같이 달려들어서...",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017828.1.10",
          "form": "에효...",
          "original_form": "에효...",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017828.1.11",
          "form": "아이고 득달같이 달려들다니",
          "original_form": "아이고 득달같이 달려들다니",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017828.1.12",
          "form": "못됬네요",
          "original_form": "못됬네요",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017828.1.13",
          "form": "그래서 제가 이겼어요 ㅎㅎ",
          "original_form": "그래서 제가 이겼어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017828.1.14",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017828.1.15",
          "form": "군대에 예민한사람 불편해요",
          "original_form": "군대에 예민한사람 불편해요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017828.1.16",
          "form": "저도용",
          "original_form": "저도용",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017828.1.17",
          "form": "군대에 거품무는사람 싫어요",
