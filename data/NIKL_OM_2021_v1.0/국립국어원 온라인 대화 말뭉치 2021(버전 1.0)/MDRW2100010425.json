      "utterance": [
        {
          "id": "MDRW2100010425.1.1",
          "form": "name1님은 알바 많이 해보셨나요?",
          "original_form": "&name1&님은 알바 많이 해보셨나요?",
          "speaker_id": "2",
          "time": "20210823 18:13"
        },
        {
          "id": "MDRW2100010425.1.2",
          "form": "네 카페알바 편의점 해봤어요ㅎㅎ",
          "original_form": "네 카페알바 편의점 해봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:13"
        },
        {
          "id": "MDRW2100010425.1.3",
          "form": "와 많이하셨군요",
          "original_form": "와 많이하셨군요",
          "speaker_id": "2",
          "time": "20210823 18:14"
        },
        {
          "id": "MDRW2100010425.1.4",
          "form": "전 그거 안해봤어요 ㅜㅜ",
          "original_form": "전 그거 안해봤어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 18:14"
        },
        {
          "id": "MDRW2100010425.1.5",
          "form": "저는 미술학원강사알바 해봤어요",
          "original_form": "저는 미술학원강사알바 해봤어요",
          "speaker_id": "2",
          "time": "20210823 18:14"
        },
        {
          "id": "MDRW2100010425.1.6",
          "form": "오 가르치는거 재밌을거같아요ㅎㅎ",
          "original_form": "오 가르치는거 재밌을거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:15"
        },
        {
          "id": "MDRW2100010425.1.7",
          "form": "애들수업은 그냥 봐주는 것도 크죠",
          "original_form": "애들수업은 그냥 봐주는 것도 크죠",
          "speaker_id": "2",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010425.1.8",
          "form": "카페알바도 배울게많아서 재밋을거같아요",
          "original_form": "카페알바도 배울게많아서 재밋을거같아요",
          "speaker_id": "2",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010425.1.9",
          "form": "재미는 없고 사람만 싫어졌어요ㅋㅋ",
          "original_form": "재미는 없고 사람만 싫어졌어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010425.1.10",
          "form": "앗 저런",
          "original_form": "앗 저런",
          "speaker_id": "2",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010425.1.11",
          "form": "커피메뉴 만드는거 배우셨어요?",
          "original_form": "커피메뉴 만드는거 배우셨어요?",
          "speaker_id": "2",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010425.1.12",
          "form": "네 그냥 얼추 다 해본거같아요ㅋㅋ",
          "original_form": "네 그냥 얼추 다 해본거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010425.1.13",
          "form": "와 부럽당",
          "original_form": "와 부럽당",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010425.1.14",
          "form": "저도 해보고싶네요",
          "original_form": "저도 해보고싶네요",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010425.1.15",
          "form": "남은건 레시피같아요ㅎㅎ",
          "original_form": "남은건 레시피같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010425.1.16",
          "form": "그럼 베이킹도 해보셨나요?",
          "original_form": "그럼 베이킹도 해보셨나요?",
          "speaker_id": "2",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010425.1.17",
          "form": "카페메뉴요",
          "original_form": "카페메뉴요",
          "speaker_id": "2",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010425.1.18",
          "form": "음 아뇨 쿠키만 파는 카페여서 베이킹은ㅜㅜ",
          "original_form": "음 아뇨 쿠키만 파는 카페여서 베이킹은ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 18:43"
        },
        {
          "id": "MDRW2100010425.1.19",
          "form": "글쿤요 기술이 많으셔서 부럽당",
          "original_form": "글쿤요 기술이 많으셔서 부럽당",
          "speaker_id": "2",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100010425.1.20",
          "form": "없는거같은데ㅜㅜㅋㅋ그래도 추억인거같아요",
          "original_form": "없는거같은데ㅜㅜㅋㅋ그래도 추억인거같아요",
          "speaker_id": "1",
          "time": "20210823 18:44"
        }
      ]
