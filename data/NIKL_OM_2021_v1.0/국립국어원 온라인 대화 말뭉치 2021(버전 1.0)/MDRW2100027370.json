      "utterance": [
        {
          "id": "MDRW2100027370.1.1",
          "form": "시대가 바뀌니 이런시대가오네요",
          "original_form": "시대가 바뀌니 이런시대가오네요",
          "speaker_id": "1",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027370.1.2",
          "form": "그러니까요 ㅋㅋ 온라인으로 대화를 해야하고",
          "original_form": "그러니까요 ㅋㅋ 온라인으로 대화를 해야하고",
          "speaker_id": "2",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027370.1.3",
          "form": "줌수업도그렇고",
          "original_form": "줌수업도그렇고",
          "speaker_id": "1",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027370.1.4",
          "form": "맞아요 줌 수업으러 요샌 활발히 하긴하는데",
          "original_form": "맞아요 줌 수업으러 요샌 활발히 하긴하는데",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027370.1.5",
          "form": "편하기도 한거같아요 ㅋㅋ",
          "original_form": "편하기도 한거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027370.1.6",
          "form": "공부안하는애도 많더라구요",
          "original_form": "공부안하는애도 많더라구요",
          "speaker_id": "1",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100027370.1.7",
          "form": "솔직히 하는척하고 딴짓해도 알수가 없으니",
          "original_form": "솔직히 하는척하고 딴짓해도 알수가 없으니",
          "speaker_id": "2",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100027370.1.8",
          "form": "문제긴해요..",
          "original_form": "문제긴해요..",
          "speaker_id": "1",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100027370.1.9",
          "form": "돈많은집들애들은 다 과외하고..",
          "original_form": "돈많은집들애들은 다 과외하고..",
          "speaker_id": "1",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027370.1.10",
          "form": "맞아요 제대로 애들이 공부가 되기나 하는지 걱정이네요",
          "original_form": "맞아요 제대로 애들이 공부가 되기나 하는지 걱정이네요",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027370.1.11",
          "form": "격차가 심하더라구요",
          "original_form": "격차가 심하더라구요",
          "speaker_id": "1",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027370.1.12",
          "form": "검사도 제대로 못한다고 하던데 아이들 의지로만 가능한지",
          "original_form": "검사도 제대로 못한다고 하던데 아이들 의지로만 가능한지",
          "speaker_id": "2",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027370.1.13",
          "form": "빨리 학교가서 공부해야되는데  걱정이네요",
          "original_form": "빨리 학교가서 공부해야되는데  걱정이네요",
          "speaker_id": "1",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027370.1.14",
          "form": "애들이 예전처럼 학교생활 할 수 있으면 좋겠어요",
          "original_form": "애들이 예전처럼 학교생활 할 수 있으면 좋겠어요",
          "speaker_id": "2",
