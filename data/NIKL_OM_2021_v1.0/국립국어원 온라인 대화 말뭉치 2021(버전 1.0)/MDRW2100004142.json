      "utterance": [
        {
          "id": "MDRW2100004142.1.1",
          "form": "여행 가는 거 좋아하시나요?",
          "original_form": "여행 가는 거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004142.1.2",
          "form": "네 코로나전까지 해외여행도 자주자주갔다왔어요",
          "original_form": "네 코로나전까지 해외여행도 자주자주갔다왔어요",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004142.1.3",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004142.1.4",
          "form": "오 어디 가셨어요?",
          "original_form": "오 어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004142.1.5",
          "form": "전 해외는 일본이 끝이에요 하하",
          "original_form": "전 해외는 일본이 끝이에요 하하",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004142.1.6",
          "form": "아 전 일본은 안가봤는데 ㅎㅎ\r\n저 대만 미국 남미 캐나다 베트남 요케 갔다왔어요",
          "original_form": "아 전 일본은 안가봤는데 ㅎㅎ\r\n저 대만 미국 남미 캐나다 베트남 요케 갔다왔어요",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004142.1.7",
          "form": "우와 진짜 되게 많이 가셨네요",
          "original_form": "우와 진짜 되게 많이 가셨네요",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004142.1.8",
          "form": "미국은 어땠어요?",
          "original_form": "미국은 어땠어요?",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004142.1.9",
          "form": "저 미국 가고싶어요",
          "original_form": "저 미국 가고싶어요",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004142.1.10",
          "form": "미국좋았어요 일단거리들도 넓고깨끗하고 다른나라에비해 ㅎㅎ \r\n먹을곳도넘많고",
          "original_form": "미국좋았어요 일단거리들도 넓고깨끗하고 다른나라에비해 ㅎㅎ \r\n먹을곳도넘많고",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004142.1.11",
          "form": "우와... 뭐 드셨나요...",
          "original_form": "우와... 뭐 드셨나요...",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004142.1.12",
          "form": "다음엔간다면 동부쪽 가보고싶어요 완전도시 뉴욕이런데로",
          "original_form": "다음엔간다면 동부쪽 가보고싶어요 완전도시 뉴욕이런데로",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004142.1.13",
          "form": "햄버거 스테이크 이런 육식류들 먹었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "햄버거 스테이크 이런 육식류들 먹었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004142.1.14",
          "form": "햄버거가 진짜 맛있더라구여 ㅋㅋㅋㅋ",
          "original_form": "햄버거가 진짜 맛있더라구여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004142.1.15",
          "form": "으아 저도 미국 뉴욕 꿈입니다...",
          "original_form": "으아 저도 미국 뉴욕 꿈입니다...",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004142.1.16",
          "form": "쉑쉑버거 드셨나요?",
          "original_form": "쉑쉑버거 드셨나요?",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004142.1.17",
          "form": "네! ㅋㅋㅋㅋㅋㅋ그때 한국에들어오기전이라서 꼭먹어야지하고 먹으러갔는데",
          "original_form": "네! ㅋㅋㅋㅋㅋㅋ그때 한국에들어오기전이라서 꼭먹어야지하고 먹으러갔는데",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004142.1.18",
          "form": "생각보다 양이넘적은거에요 거기에실망..",
          "original_form": "생각보다 양이넘적은거에요 거기에실망..",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004142.1.19",
          "form": "파이브가이즈버거가 더맛있고양도 엄청많았어요 ㅋㅋㅋ",
          "original_form": "파이브가이즈버거가 더맛있고양도 엄청많았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004142.1.20",
          "form": "으아 너무 맛있을 거 같아요",
          "original_form": "으아 너무 맛있을 거 같아요",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004142.1.21",
          "form": "제 로망이거든요ㅋㅋㅋㅋㅋㅋㅠ",
          "original_form": "제 로망이거든요ㅋㅋㅋㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004142.1.22",
          "form": "일단 여행가서 돈써서먹는것자체가 넘좋아요 ㅋㅋㅋ지갑은눈물나지만",
          "original_form": "일단 여행가서 돈써서먹는것자체가 넘좋아요 ㅋㅋㅋ지갑은눈물나지만",
          "speaker_id": "1",
          "time": "20210821 15:24"
        }
      ]
