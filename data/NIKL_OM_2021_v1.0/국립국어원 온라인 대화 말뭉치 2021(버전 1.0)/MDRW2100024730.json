      "utterance": [
        {
          "id": "MDRW2100024730.1.1",
          "form": "요즘 구입하고 싶은 상품이 있나요?",
          "original_form": "요즘 구입하고 싶은 상품이 있나요?",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024730.1.2",
          "form": "저는 노트북이요",
          "original_form": "저는 노트북이요",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024730.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024730.1.4",
          "form": "저는 컴퓨터용 모니터가 사고싶어요ㅎ",
          "original_form": "저는 컴퓨터용 모니터가 사고싶어요ㅎ",
          "speaker_id": "1",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024730.1.5",
          "form": "듀얼모니터같은거요??",
          "original_form": "듀얼모니터같은거요??",
          "speaker_id": "2",
          "time": "20210831 18:06"
        },
        {
          "id": "MDRW2100024730.1.6",
          "form": "듀얼 모니터까진 아니고요 지금 쓰는게 구식이라 최신으오 바꾸고 싶어서요ㅎㅎ",
          "original_form": "듀얼 모니터까진 아니고요 지금 쓰는게 구식이라 최신으오 바꾸고 싶어서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024730.1.7",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "2",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100024730.1.8",
          "form": "데스크탑 쓰시는군요",
          "original_form": "데스크탑 쓰시는군요",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100024730.1.9",
          "form": "네 모니터가 고장이 안나네요ㅠ",
          "original_form": "네 모니터가 고장이 안나네요ㅠ",
          "speaker_id": "1",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100024730.1.10",
          "form": "ㅋㅋㅋ꼭 바꾸고싶은건 고장이 안나더라구요",
          "original_form": "ㅋㅋㅋ꼭 바꾸고싶은건 고장이 안나더라구요",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024730.1.11",
          "form": "그쵸ㅋㅋ name2님은 특별히 봐두신 노트북이 있나요?",
          "original_form": "그쵸ㅋㅋ &name2&님은 특별히 봐두신 노트북이 있나요?",
          "speaker_id": "1",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100024730.1.12",
          "form": "넘 고민이에요",
          "original_form": "넘 고민이에요",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100024730.1.13",
          "form": "그램을 썼었는데",
          "original_form": "그램을 썼었는데",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100024730.1.14",
          "form": "요즘 저가형도 스팩이 잘 나오는것 같아서",
          "original_form": "요즘 저가형도 스팩이 잘 나오는것 같아서",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100024730.1.15",
          "form": "굳이 브랜드 고집해야하나싶기도하고",
          "original_form": "굳이 브랜드 고집해야하나싶기도하고",
          "speaker_id": "2",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100024730.1.16",
          "form": "as생각하면 또 역시 엘지인가 싶고그래요",
          "original_form": "as생각하면 또 역시 엘지인가 싶고그래요",
          "speaker_id": "2",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100024730.1.17",
          "form": "아 노트북 저가는 수리가 잘 안되나요??",
          "original_form": "아 노트북 저가는 수리가 잘 안되나요??",
          "speaker_id": "1",
