      "utterance": [
        {
          "id": "MDRW2100007024.1.1",
          "form": "헐 이게 제가 좀 서툴어서...",
          "original_form": "헐 이게 제가 좀 서툴어서...",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007024.1.2",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007024.1.3",
          "form": "아 아뇨... 넘 키우고 싶어서",
          "original_form": "아 아뇨... 넘 키우고 싶어서",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007024.1.4",
          "form": "랜선 애기들만 있어요ㅋㅋㅋ",
          "original_form": "랜선 애기들만 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007024.1.5",
          "form": "강아지 큰 애 키워보고싶긴 해요",
          "original_form": "강아지 큰 애 키워보고싶긴 해요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007024.1.6",
          "form": "키워본 적 있으세요ㅡ",
          "original_form": "키워본 적 있으세요ㅡ",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007024.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007024.1.8",
          "form": "네.ㅋㅋㅋ",
          "original_form": "네.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007024.1.9",
          "form": "강아지 키워봤어요",
          "original_form": "강아지 키워봤어요",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007024.1.10",
          "form": "오오 무슨 종이요?",
          "original_form": "오오 무슨 종이요?",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007024.1.11",
          "form": "사진 가능하면 사진 보내달라고 조르는건데ㅋㅋㅋ",
          "original_form": "사진 가능하면 사진 보내달라고 조르는건데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007024.1.12",
          "form": "저는 요크셔요",
          "original_form": "저는 요크셔요",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007024.1.13",
          "form": "지금은 무지개 다리 건넜어요.ㅠㅠ",
          "original_form": "지금은 무지개 다리 건넜어요.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007024.1.14",
          "form": "으앙 젤 기요미를ㅋㅋㅋㅋㅋ",
          "original_form": "으앙 젤 기요미를ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007024.1.15",
          "form": "아앙",
          "original_form": "아앙",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007024.1.16",
          "form": "15년간 가족처럼 지내던 아인데.ㅠㅠ",
          "original_form": "15년간 가족처럼 지내던 아인데.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007024.1.17",
          "form": "그게 너무 아숩더라구요 애기들 아무래도ㅠㅠ",
          "original_form": "그게 너무 아숩더라구요 애기들 아무래도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007024.1.18",
          "form": "아무래도 수명이 사람보다 짧다보니.. 이별의 아픔이 있더라고요.ㅠㅠ",
          "original_form": "아무래도 수명이 사람보다 짧다보니.. 이별의 아픔이 있더라고요.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007024.1.19",
          "form": "그니까요ㅜ",
          "original_form": "그니까요ㅜ",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007024.1.20",
          "form": "중요한 시험앞두고 무지개 다리 건너서..",
          "original_form": "중요한 시험앞두고 무지개 다리 건너서..",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007024.1.21",
          "form": "친구 강아지도 지금 할아부지라 맨날 애지중지 하고 있어요",
          "original_form": "친구 강아지도 지금 할아부지라 맨날 애지중지 하고 있어요",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007024.1.22",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007024.1.23",
          "form": "가는것도 못봤어요.ㅠㅠ",
          "original_form": "가는것도 못봤어요.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007024.1.24",
          "form": "아 맘이 도 아프셨겠다",
          "original_form": "아 맘이 도 아프셨겠다",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007024.1.25",
          "form": "그쵸 그래서 상처였는지 아직 새 반려동물은 못 들이겠더라고요.",
          "original_form": "그쵸 그래서 상처였는지 아직 새 반려동물은 못 들이겠더라고요.",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007024.1.26",
          "form": "아아 마자 그런 경우 많더라구요",
          "original_form": "아아 마자 그런 경우 많더라구요",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007024.1.27",
          "form": "친구 중엔 바로 입양한 애도 있긴 했어요",
          "original_form": "친구 중엔 바로 입양한 애도 있긴 했어요",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007024.1.28",
          "form": "히잉 애기드류ㅠㅠㅠㅠ",
          "original_form": "히잉 애기드류ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007024.1.29",
          "form": "그렇더라고요",
          "original_form": "그렇더라고요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007024.1.30",
          "form": "바로 입양하거나... 아님 저처럼 오래 새가족 못맞거나..",
          "original_form": "바로 입양하거나... 아님 저처럼 오래 새가족 못맞거나..",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007024.1.31",
          "form": "마자요 그런가바요",
          "original_form": "마자요 그런가바요",
          "speaker_id": "1",
          "time": "20210822 00:24"
        }
      ]
    }
