      "utterance": [
        {
          "id": "MDRW2100023205.1.1",
          "form": "일식인가요?",
          "original_form": "일식인가요?",
          "speaker_id": "2",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023205.1.2",
          "form": "아 최애중에 일식이 한놈있습니다",
          "original_form": "아 최애중에 일식이 한놈있습니다",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023205.1.3",
          "form": "그놈으로 가시죠",
          "original_form": "그놈으로 가시죠",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023205.1.4",
          "form": "ㅋㅋㅋㅋㅋ일식 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ일식 맞아요",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023205.1.5",
          "form": "유후 좋아요",
          "original_form": "유후 좋아요",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023205.1.6",
          "form": "튀김인가요?",
          "original_form": "튀김인가요?",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023205.1.7",
          "form": "튀기지는 않아요!",
          "original_form": "튀기지는 않아요!",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023205.1.8",
          "form": "으으으으으음",
          "original_form": "으으으으으음",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023205.1.9",
          "form": "생선이 들어가나요?",
          "original_form": "생선이 들어가나요?",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023205.1.10",
          "form": "아앗 생선이 들어갑니다",
          "original_form": "아앗 생선이 들어갑니다",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023205.1.11",
          "form": "밥이 들어가나요?",
          "original_form": "밥이 들어가나요?",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023205.1.12",
          "form": "밥도 ... 들어갑니다",
          "original_form": "밥도 ... 들어갑니다",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023205.1.13",
          "form": "알것다 스시!!!!!!!!!!!!!!!!!!!!!!!!!!",
          "original_form": "알것다 스시!!!!!!!!!!!!!!!!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023205.1.14",
          "form": "스시 정답이에요ㅋㅋㅋㅋㅋㅋ 왕빠르네요",
          "original_form": "스시 정답이에요ㅋㅋㅋㅋㅋㅋ 왕빠르네요",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023205.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023205.1.16",
          "form": "저도 진짜 좋아하거든요",
          "original_form": "저도 진짜 좋아하거든요",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023205.1.17",
          "form": "초밥 진짜 너무좋아해요",
          "original_form": "초밥 진짜 너무좋아해요",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023205.1.18",
          "form": "스시 너무 좋아요",
          "original_form": "스시 너무 좋아요",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023205.1.19",
          "form": "다음 최애 맞추기 갑니다",
          "original_form": "다음 최애 맞추기 갑니다",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023205.1.20",
          "form": "고등학교 때 친구들이랑 무한리필 이런데 진짜 자주갔었어요 ㅋㅋㅋ",
          "original_form": "고등학교 때 친구들이랑 무한리필 이런데 진짜 자주갔었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023205.1.21",
          "form": "저도요ㅋㅋㅋㅋ 존맛탱",
          "original_form": "저도요ㅋㅋㅋㅋ 존맛탱",
