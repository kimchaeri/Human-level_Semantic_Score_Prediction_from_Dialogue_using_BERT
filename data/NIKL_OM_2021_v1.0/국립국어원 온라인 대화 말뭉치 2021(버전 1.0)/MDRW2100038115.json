      "utterance": [
        {
          "id": "MDRW2100038115.1.1",
          "form": "정말 싫어해",
          "original_form": "정말 싫어해",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038115.1.2",
          "form": "난 그래서 요리도 싫어",
          "original_form": "난 그래서 요리도 싫어",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038115.1.3",
          "form": "나도 싫어",
          "original_form": "나도 싫어",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038115.1.4",
          "form": "설거짓거리가 나오겆든",
          "original_form": "설거짓거리가 나오겆든",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038115.1.5",
          "form": "근데 물컵을 자주 씻어야한대",
          "original_form": "근데 물컵을 자주 씻어야한대",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038115.1.6",
          "form": "나는 원팬요리를 좋아해",
          "original_form": "나는 원팬요리를 좋아해",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.7",
          "form": "안그러면 장염에 걸린다더라",
          "original_form": "안그러면 장염에 걸린다더라",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.8",
          "form": "귀찮구나.",
          "original_form": "귀찮구나.",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.9",
          "form": "나는 원래 받아두고 계속먹는데",
          "original_form": "나는 원래 받아두고 계속먹는데",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.10",
          "form": "이제 안그러려고 노력중이야",
          "original_form": "이제 안그러려고 노력중이야",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.11",
          "form": "너는 물을 잘 안마시지않아?",
          "original_form": "너는 물을 잘 안마시지않아?",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.12",
          "form": "음 그런데 귀찮아서 나는 그냥 마셔",
          "original_form": "음 그런데 귀찮아서 나는 그냥 마셔",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.13",
          "form": "응 맞아 그래서 더 자주안마시게되었러",
          "original_form": "응 맞아 그래서 더 자주안마시게되었러",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.14",
          "form": "귀찮음이 이겼구나",
          "original_form": "귀찮음이 이겼구나",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.15",
          "form": "상한물을 먹는거랑ㅁ",
          "original_form": "상한물을 먹는거랑ㅁ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.16",
          "form": "물먹는걸 참는거중에 머ㅓ가 더 나을까",
          "original_form": "물먹는걸 참는거중에 머ㅓ가 더 나을까",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.17",
          "form": "나는 그 싱크대 청소도 싫어해",
          "original_form": "나는 그 싱크대 청소도 싫어해",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.18",
          "form": "그래서 요리도 안해",
          "original_form": "그래서 요리도 안해",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.19",
          "form": "싱크대 청소 너무 자주해야해",
          "original_form": "싱크대 청소 너무 자주해야해",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038115.1.20",
          "form": "그럼 설거지거리도 안나오니 좋아",
          "original_form": "그럼 설거지거리도 안나오니 좋아",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038115.1.21",
          "form": "왜 모든건 가만히나두면 더러워지는거야??",
          "original_form": "왜 모든건 가만히나두면 더러워지는거야??",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038115.1.22",
          "form": "싱크대를 쓰지마",
          "original_form": "싱크대를 쓰지마",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038115.1.23",
          "form": "집안일투성이야 정말",
          "original_form": "집안일투성이야 정말",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038115.1.24",
          "form": "삶은 그런 거 같아.",
          "original_form": "삶은 그런 거 같아.",
          "speaker_id": "2",
          "time": "20210901 23:24"
        }
      ]
    }
  ]
