      "utterance": [
        {
          "id": "MDRW2100026556.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026556.1.2",
          "form": "햄을 좋아하시나요?",
          "original_form": "햄을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026556.1.3",
          "form": "햄 좋아하죠! 오늘 저녁에도 소시지들간 핫도그 먹엇어용",
          "original_form": "햄 좋아하죠! 오늘 저녁에도 소시지들간 핫도그 먹엇어용",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026556.1.4",
          "form": "name1늠 뭐좋아하세요?",
          "original_form": "&name1&늠 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026556.1.5",
          "form": "많은 음식을 좋아하는데",
          "original_form": "많은 음식을 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026556.1.6",
          "form": "햄중에선 서양식 햄? 수제햄도 좋아하고",
          "original_form": "햄중에선 서양식 햄? 수제햄도 좋아하고",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026556.1.7",
          "form": "스팸도 좋아해요",
          "original_form": "스팸도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026556.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ저두 스팸 좋아해요 밥해먹을거없을때 맨날 밥 볶아먹거나 덮밥해먹구 합니당",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저두 스팸 좋아해요 밥해먹을거없을때 맨날 밥 볶아먹거나 덮밥해먹구 합니당",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026556.1.9",
          "form": "그죠 밥도둑이자 저의 친구",
          "original_form": "그죠 밥도둑이자 저의 친구",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026556.1.10",
          "form": "하지만 다른 통조림보다 고가에요",
          "original_form": "하지만 다른 통조림보다 고가에요",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026556.1.11",
          "form": "많은 힘엔 많은 책임이 따른다더니",
          "original_form": "많은 힘엔 많은 책임이 따른다더니",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026556.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026556.1.13",
          "form": "음식얘기하니까 배고프네요 저녁 뭐드셧어요.. 저는 점심 늦게먹어서 굶었어요ㅠㅠ",
          "original_form": "음식얘기하니까 배고프네요 저녁 뭐드셧어요.. 저는 점심 늦게먹어서 굶었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026556.1.14",
          "form": "전 집밥이요,,",
          "original_form": "전 집밥이요,,",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026556.1.15",
          "form": "맛있는건 아마 주말쯤 먹을듯요,,",
          "original_form": "맛있는건 아마 주말쯤 먹을듯요,,",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026556.1.16",
          "form": "내일은 스팸이라도 궈야겠어요,,",
          "original_form": "내일은 스팸이라도 궈야겠어요,,",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026556.1.17",
          "form": "ㅋㅋㅋㅋ저두요.... 요즘 저녁을 자주 걸러서 밥 챙겨먹어야지...내일 메뉴 뭐할지 고민해봐야겟네요 ",
          "original_form": "ㅋㅋㅋㅋ저두요.... 요즘 저녁을 자주 걸러서 밥 챙겨먹어야지...내일 메뉴 뭐할지 고민해봐야겟네요 ",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026556.1.18",
          "form": "ㅠ 그럴땐 그냥 햇반에 김이라도 드세요",
          "original_form": "ㅠ 그럴땐 그냥 햇반에 김이라도 드세요",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026556.1.19",
          "form": "거르는게 젤 안좋더라구요 위에요",
          "original_form": "거르는게 젤 안좋더라구요 위에요",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026556.1.20",
          "form": "넵넵 ㅜㅜ 내일은 귀찮아도 계란이나 햄이라두 구어서 먹어야겠어요",
          "original_form": "넵넵 ㅜㅜ 내일은 귀찮아도 계란이나 햄이라두 구어서 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026556.1.21",
          "form": "같이 힘내자구요",
          "original_form": "같이 힘내자구요",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026556.1.22",
