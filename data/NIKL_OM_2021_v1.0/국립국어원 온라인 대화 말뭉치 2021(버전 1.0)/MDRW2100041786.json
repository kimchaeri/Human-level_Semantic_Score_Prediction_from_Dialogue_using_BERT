      "utterance": [
        {
          "id": "MDRW2100041786.1.1",
          "form": "근데라고 하셔서 안어울리는거 말해주시는줄 알았어욬ㅋㅋㅋ",
          "original_form": "근데라고 하셔서 안어울리는거 말해주시는줄 알았어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041786.1.2",
          "form": "저 딤섬도 한번 사볼까 맨날 보다가 말아요",
          "original_form": "저 딤섬도 한번 사볼까 맨날 보다가 말아요",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041786.1.3",
          "form": "혹시 사보신적 있으세요? 냉동딤섬",
          "original_form": "혹시 사보신적 있으세요? 냉동딤섬",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041786.1.4",
          "form": "아ㅋㅋㅋ\r\n딤섬은 안사봤어요",
          "original_form": "아ㅋㅋㅋ\r\n딤섬은 안사봤어요",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041786.1.5",
          "form": "궁금해서 사볼까 하다가 그냥 먹던거 먹자 하게되네옄ㅋㅋ",
          "original_form": "궁금해서 사볼까 하다가 그냥 먹던거 먹자 하게되네옄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041786.1.6",
          "form": "냉동식품은 도전이 어려워요 종류가 너무 많아섴ㅋㅋㅋ",
          "original_form": "냉동식품은 도전이 어려워요 종류가 너무 많아섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041786.1.7",
          "form": "맞아요 모험하기가 겁나서",
          "original_form": "맞아요 모험하기가 겁나서",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041786.1.8",
          "form": "맛있는거 계속 먹게 돼죠?ㅎㅎ",
          "original_form": "맛있는거 계속 먹게 돼죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041786.1.9",
          "form": "네 맞아요 특히나 튀김류",
          "original_form": "네 맞아요 특히나 튀김류",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041786.1.10",
          "form": "튀김 맛있는거 아세여??",
          "original_form": "튀김 맛있는거 아세여??",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041786.1.11",
          "form": "저는 김말이는 튀김공방이라고 쓰여져있는게 맛있더라고요",
          "original_form": "저는 김말이는 튀김공방이라고 쓰여져있는게 맛있더라고요",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041786.1.12",
          "form": "대부분이 바삭함을 못살리는거같아여",
          "original_form": "대부분이 바삭함을 못살리는거같아여",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041786.1.13",
          "form": "진짜 기름에 튀기는거 아니면",
          "original_form": "진짜 기름에 튀기는거 아니면",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041786.1.14",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041786.1.15",
          "form": "에어프라이어가 거기까지 힘내긴 어렵나바여",
          "original_form": "에어프라이어가 거기까지 힘내긴 어렵나바여",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041786.1.16",
          "form": "근데 저 김말이는 좀 맛있었어요 밖에서 먹는것보다도",
          "original_form": "근데 저 김말이는 좀 맛있었어요 밖에서 먹는것보다도",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041786.1.17",
          "form": "한번 먹어바야겟네여",
          "original_form": "한번 먹어바야겟네여",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041786.1.18",
          "form": "튀김공방이라고 써져있는 김말이를 네",
          "original_form": "튀김공방이라고 써져있는 김말이를 네",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041786.1.19",
          "form": "도전해보셔여",
          "original_form": "도전해보셔여",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041786.1.20",
          "form": "전 못난이좋아하는데 그건 없더라고요",
          "original_form": "전 못난이좋아하는데 그건 없더라고요",
