      "utterance": [
        {
          "id": "MDRW2100043171.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043171.1.2",
          "form": "네네 좋아해요 최근엔 잘 못갔어요",
          "original_form": "네네 좋아해요 최근엔 잘 못갔어요",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043171.1.3",
          "form": "아 특별히 좋아하는 여행지 있으신가요?",
          "original_form": "아 특별히 좋아하는 여행지 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043171.1.4",
          "form": "아 저는 경주 좋아해요 적당히 한적하면서도 볼게많아서",
          "original_form": "아 저는 경주 좋아해요 적당히 한적하면서도 볼게많아서",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043171.1.5",
          "form": "아 저는 경주 근처살긴한데",
          "original_form": "아 저는 경주 근처살긴한데",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043171.1.6",
          "form": "의외로 많이 안가봤어요",
          "original_form": "의외로 많이 안가봤어요",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043171.1.7",
          "form": "우와 정말요",
          "original_form": "우와 정말요",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043171.1.8",
          "form": "어떤곳추천하시나요",
          "original_form": "어떤곳추천하시나요",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043171.1.9",
          "form": "음 황리단길 하고",
          "original_form": "음 황리단길 하고",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043171.1.10",
          "form": "월정교요",
          "original_form": "월정교요",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043171.1.11",
          "form": "혹시 월정교는 밤에 불켜져있는 곳인가요",
          "original_form": "혹시 월정교는 밤에 불켜져있는 곳인가요",
          "speaker_id": "1",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043171.1.12",
          "form": "마자요 역시",
          "original_form": "마자요 역시",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043171.1.13",
          "form": "아 야경보기 좋겠네요",
          "original_form": "아 야경보기 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043171.1.14",
          "form": "네네마자요",
          "original_form": "네네마자요",
          "speaker_id": "2",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043171.1.15",
          "form": "대룽원도 좋고요",
          "original_form": "대룽원도 좋고요",
          "speaker_id": "2",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043171.1.16",
          "form": "왕릉 있는 곳인가요?",
          "original_form": "왕릉 있는 곳인가요?",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043171.1.17",
          "form": "황리단길 근처일걸요?",
          "original_form": "황리단길 근처일걸요?",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043171.1.18",
          "form": "역사공부하고 가면 더 좋겠네요",
          "original_form": "역사공부하고 가면 더 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 18:27"
        }
