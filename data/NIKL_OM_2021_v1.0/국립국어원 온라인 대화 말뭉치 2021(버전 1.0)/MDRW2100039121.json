      "utterance": [
        {
          "id": "MDRW2100039121.1.1",
          "form": "최근.잘 산템 있으신가요",
          "original_form": "최근.잘 산템 있으신가요",
          "speaker_id": "1",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100039121.1.2",
          "form": "최근에 할인받아서 샴푸샀어요",
          "original_form": "최근에 할인받아서 샴푸샀어요",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100039121.1.3",
          "form": "와 무슨 샴푸에용",
          "original_form": "와 무슨 샴푸에용",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100039121.1.4",
          "form": "헤드앤 숄더요. 맨솔 쓰는데 좋더라구요",
          "original_form": "헤드앤 숄더요. 맨솔 쓰는데 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100039121.1.5",
          "form": "오 멘솔 시원할거같아여",
          "original_form": "오 멘솔 시원할거같아여",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039121.1.6",
          "form": "저도 샴푸 살때가 다가왔는데",
          "original_form": "저도 샴푸 살때가 다가왔는데",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039121.1.7",
          "form": "바디워시도 잔뜩구매했어요 ㅋ",
          "original_form": "바디워시도 잔뜩구매했어요 ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039121.1.8",
          "form": "우와 어디꺼 사셨어요",
          "original_form": "우와 어디꺼 사셨어요",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039121.1.9",
          "form": "해피바스꺼요 ㅋ 찾아보면 세일해요",
          "original_form": "해피바스꺼요 ㅋ 찾아보면 세일해요",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100039121.1.10",
          "form": "와 그런가요 찾아볼게요",
          "original_form": "와 그런가요 찾아볼게요",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039121.1.11",
          "form": "최근에  어떤거 구매하셨나요?",
          "original_form": "최근에  어떤거 구매하셨나요?",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039121.1.12",
          "form": "저 방금 니트랑 바지 구매하고와싼용",
          "original_form": "저 방금 니트랑 바지 구매하고와싼용",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100039121.1.13",
          "form": "가을옷 구매하셨군요",
          "original_form": "가을옷 구매하셨군요",
          "speaker_id": "2",
          "time": "20210904 07:10"
        },
        {
          "id": "MDRW2100039121.1.14",
          "form": "네네 고민 엄청 하다가 샀어요",
          "original_form": "네네 고민 엄청 하다가 샀어요",
          "speaker_id": "1",
          "time": "20210904 07:10"
        },
        {
          "id": "MDRW2100039121.1.15",
          "form": "최근에인테리어를 바꿨는데",
          "original_form": "최근에인테리어를 바꿨는데",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100039121.1.16",
          "form": "이케아조명 맘에둘어요",
          "original_form": "이케아조명 맘에둘어요",
          "speaker_id": "1",
