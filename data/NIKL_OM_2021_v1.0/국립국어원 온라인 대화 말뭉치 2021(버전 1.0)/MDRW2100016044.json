      "utterance": [
        {
          "id": "MDRW2100016044.1.1",
          "form": "쓱데이는 과자나 골뱅이 50프로돼요",
          "original_form": "쓱데이는 과자나 골뱅이 50프로돼요",
          "speaker_id": "2",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016044.1.2",
          "form": "좋아하는 과자있어요?",
          "original_form": "좋아하는 과자있어요?",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016044.1.3",
          "form": "마미손파이",
          "original_form": "마미손파이",
          "speaker_id": "2",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016044.1.4",
          "form": "엄마손",
          "original_form": "엄마손",
          "speaker_id": "2",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016044.1.5",
          "form": "그거 완전맛있잖아요",
          "original_form": "그거 완전맛있잖아요",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016044.1.6",
          "form": "한 때 그거 엄청 먹었는데",
          "original_form": "한 때 그거 엄청 먹었는데",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016044.1.7",
          "form": "또 뭐 좋아하세요",
          "original_form": "또 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016044.1.8",
          "form": "그거 지금 원 플러스 원해요",
          "original_form": "그거 지금 원 플러스 원해요",
          "speaker_id": "2",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016044.1.9",
          "form": "이마트에서요?",
          "original_form": "이마트에서요?",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016044.1.10",
          "form": "도쿄바나나랑 맛 비슷해요",
          "original_form": "도쿄바나나랑 맛 비슷해요",
          "speaker_id": "2",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016044.1.11",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016044.1.12",
          "form": "사러가아곘네요",
          "original_form": "사러가아곘네요",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016044.1.13",
          "form": "도쿄바나나?",
          "original_form": "도쿄바나나?",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016044.1.14",
          "form": "옛날에 일본갔을 때 샀던",
          "original_form": "옛날에 일본갔을 때 샀던",
          "speaker_id": "2",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100016044.1.15",
          "form": "그것도 과자에요?",
          "original_form": "그것도 과자에요?",
          "speaker_id": "1",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100016044.1.16",
          "form": "도쿄바나나파이인데",
          "original_form": "도쿄바나나파이인데",
          "speaker_id": "2",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100016044.1.17",
          "form": "비싸기만 엄청 비쌌는데",
          "original_form": "비싸기만 엄청 비쌌는데",
          "speaker_id": "2",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100016044.1.18",
          "form": "아~ 일본과자구나",
          "original_form": "아~ 일본과자구나",
          "speaker_id": "1",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100016044.1.19",
          "form": "엄마손파이가 더 맛있어요",
          "original_form": "엄마손파이가 더 맛있어요",
          "speaker_id": "2",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100016044.1.20",
          "form": "역시 갓마손팡",
          "original_form": "역시 갓마손팡",
          "speaker_id": "1",
          "time": "20210826 08:41"
        },
