        },
        {
          "id": "MDRW2100037951.1.4",
          "form": "저는 떡볶이요\r\nname2님은요?",
          "original_form": "저는 떡볶이요\r\n&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037951.1.5",
          "form": "저도 떡볶이",
          "original_form": "저도 떡볶이",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037951.1.6",
          "form": "닭발 좋아해요~",
          "original_form": "닭발 좋아해요~",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037951.1.7",
          "form": "떡볶이 최애 브랜드 어디에요",
          "original_form": "떡볶이 최애 브랜드 어디에요",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037951.1.8",
          "form": "저는 엽기떡볶이요\r\nname2님은요?",
          "original_form": "저는 엽기떡볶이요\r\n&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037951.1.9",
          "form": "어 저도요!!",
          "original_form": "어 저도요!!",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037951.1.10",
          "form": "진짜 좋아하는데",
          "original_form": "진짜 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037951.1.11",
          "form": "주변에 매운 걸 못 먹는 사람들이 많아서",
          "original_form": "주변에 매운 걸 못 먹는 사람들이 많아서",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037951.1.12",
          "form": "혼자 이틀 동안 먹어요ㅋㅋㅋ",
          "original_form": "혼자 이틀 동안 먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037951.1.13",
          "form": "저도 저희가족이 떡볶이를 별로 안좋아해서 혼자먹어요ㅋㅋㅋㅋ\r\n닭발은 뼈 있는거랑 없는것 중에 어떤 걸 더 좋아하시나요?",
          "original_form": "저도 저희가족이 떡볶이를 별로 안좋아해서 혼자먹어요ㅋㅋㅋㅋ\r\n닭발은 뼈 있는거랑 없는것 중에 어떤 걸 더 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100037951.1.14",
          "form": "전 발라 먹는 게 귀찮아서",
          "original_form": "전 발라 먹는 게 귀찮아서",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037951.1.15",
          "form": "뼈 없는 것을 더  선호합니당^^",
          "original_form": "뼈 없는 것을 더  선호합니당^^",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100037951.1.16",
          "form": "맞아요 발라먹을 시간에 더 많이 먹어야죠!!!",
          "original_form": "맞아요 발라먹을 시간에 더 많이 먹어야죠!!!",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037951.1.17",
          "form": "역시 배운 사람!!ㅋㅋㅋㅋㅋ",
          "original_form": "역시 배운 사람!!ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037951.1.18",
          "form": "떡볶이랑 닭발 좋아하는 사람 중",
          "original_form": "떡볶이랑 닭발 좋아하는 사람 중",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037951.1.19",
          "form": "나쁜 사람은 아무도 없어요",
          "original_form": "나쁜 사람은 아무도 없어요",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100037951.1.20",
          "form": "맞습니다 그리고 떡볶이랑 닭발을 좋아하는 사람은 99.99%가 여자죠ㅋㅋㅋㅋ",
          "original_form": "맞습니다 그리고 떡볶이랑 닭발을 좋아하는 사람은 99.99%가 여자죠ㅋㅋㅋㅋ",
