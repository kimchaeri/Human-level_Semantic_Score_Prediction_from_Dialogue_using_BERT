      "utterance": [
        {
          "id": "MDRW2100022791.1.1",
          "form": "피자빵이요!",
          "original_form": "피자빵이요!",
          "speaker_id": "1",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022791.1.2",
          "form": "아 피자빵 ㅋㅋㅋ 소시지빵 고로케도 ㅋㅋ",
          "original_form": "아 피자빵 ㅋㅋㅋ 소시지빵 고로케도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022791.1.3",
          "form": "저 빵에 치즈만 들어가면 오케이 수준이에요ㅋㅋ",
          "original_form": "저 빵에 치즈만 들어가면 오케이 수준이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:28"
        },
        {
          "id": "MDRW2100022791.1.4",
          "form": "찐 치즈사랑이시군요 ㅋㅋㅋㅋ",
          "original_form": "찐 치즈사랑이시군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100022791.1.5",
          "form": "넹ㅋㅋㄲ 왜이리 좋은지 모르겠어요ㅋㅋㅋ",
          "original_form": "넹ㅋㅋㄲ 왜이리 좋은지 모르겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022791.1.6",
          "form": "이번에 체다치즈 덩어리로 샀어요ㅋㅋ",
          "original_form": "이번에 체다치즈 덩어리로 샀어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022791.1.7",
          "form": "와우 대박",
          "original_form": "와우 대박",
          "speaker_id": "2",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022791.1.8",
          "form": "맛있게따 요리해먹으면 크으",
          "original_form": "맛있게따 요리해먹으면 크으",
          "speaker_id": "2",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022791.1.9",
          "form": "생각난김에 비빔면레 치즈 올려 먹어야겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "생각난김에 비빔면레 치즈 올려 먹어야겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022791.1.10",
          "form": "크으 좋죠",
          "original_form": "크으 좋죠",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022791.1.11",
          "form": "비빔면 치즈라니 최고 아님니까?!",
          "original_form": "비빔면 치즈라니 최고 아님니까?!",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022791.1.12",
          "form": "크으 역시 최고였어요",
          "original_form": "크으 역시 최고였어요",
          "speaker_id": "2",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100022791.1.13",
          "form": "맛저 하셨나봐욥!",
          "original_form": "맛저 하셨나봐욥!",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022791.1.14",
          "form": "네 ㅌㅋㅋㅋ 대충 먹기엔 최고 ㅋㅋㅋㅋ",
          "original_form": "네 ㅌㅋㅋㅋ 대충 먹기엔 최고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022791.1.15",
          "form": "대충이라기엔 고급인걸용",
          "original_form": "대충이라기엔 고급인걸용",
          "speaker_id": "1",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100022791.1.16",
          "form": "name1님은 뭐드셨나요??",
          "original_form": "&name1&님은 뭐드셨나요??",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022791.1.17",
          "form": "저는 케이크용ㅋㅋ",
          "original_form": "저는 케이크용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022791.1.18",
          "form": "간식이자나요 그건 ㅠㅠ",
          "original_form": "간식이자나요 그건 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100022791.1.19",
          "form": "해먹는게 귀찮아서 그만..",
          "original_form": "해먹는게 귀찮아서 그만..",
          "speaker_id": "1",
