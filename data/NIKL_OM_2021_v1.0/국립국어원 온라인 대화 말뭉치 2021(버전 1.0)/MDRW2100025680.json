      "utterance": [
        {
          "id": "MDRW2100025680.1.1",
          "form": "ㅋㅋㄱㅋㅋ 콘서트랑",
          "original_form": "ㅋㅋㄱㅋㅋ 콘서트랑",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025680.1.2",
          "form": "규모가확실히 차이가 나니까",
          "original_form": "규모가확실히 차이가 나니까",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025680.1.3",
          "form": "나시카는 과하긴한듯",
          "original_form": "나시카는 과하긴한듯",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025680.1.4",
          "form": "니쿠라도좋던데....",
          "original_form": "니쿠라도좋던데....",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025680.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ1층이면 니쿠라 정도도 괜찮을거 같긴한데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ1층이면 니쿠라 정도도 괜찮을거 같긴한데",
          "speaker_id": "2",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025680.1.6",
          "form": "꼭대기석이면 체조 아니어도",
          "original_form": "꼭대기석이면 체조 아니어도",
          "speaker_id": "2",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025680.1.7",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025680.1.8",
          "form": "걍 나시카 가져가도 괜찮긴 할듯",
          "original_form": "걍 나시카 가져가도 괜찮긴 할듯",
          "speaker_id": "2",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025680.1.9",
          "form": "생각보다 잘보이더라고",
          "original_form": "생각보다 잘보이더라고",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025680.1.10",
          "form": "샤롯데꼭데기는",
          "original_form": "샤롯데꼭데기는",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025680.1.11",
          "form": "나시카 괜찮을듯ㅋㅋㅋㅋㅋ",
          "original_form": "나시카 괜찮을듯ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025680.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실 무거워서 그렇지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실 무거워서 그렇지",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025680.1.13",
          "form": "1층이어도 나시카 괜찮긴하드라",
          "original_form": "1층이어도 나시카 괜찮긴하드라",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025680.1.14",
          "form": "2열은 에바지만",
          "original_form": "2열은 에바지만",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025680.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025680.1.16",
          "form": "ㅇㅇㅇㅇ ㅋㄱㅋㄱㅋㅋㅋㄱㄱ",
          "original_form": "ㅇㅇㅇㅇ ㅋㄱㅋㄱㅋㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025680.1.17",
          "form": "그럼 눈물까지 다보일듯",
          "original_form": "그럼 눈물까지 다보일듯",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025680.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 2열이면 생눈으로 보는게 더 좋지않은가...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 2열이면 생눈으로 보는게 더 좋지않은가...",
          "speaker_id": "2",
          "time": "20210901 16:02"
        },
        {
          "id": "MDRW2100025680.1.19",
          "form": "뭐 이것도 취향차이긴 하지",
          "original_form": "뭐 이것도 취향차이긴 하지",
          "speaker_id": "2",
          "time": "20210901 16:02"
        },
        {
          "id": "MDRW2100025680.1.20",
          "form": "아 나는",
          "original_form": "아 나는",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025680.1.21",
          "form": "망원경으로 확대가되니까",
          "original_form": "망원경으로 확대가되니까",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025680.1.22",
          "form": "조명이나 분위기가 안보아ㅛ",
          "original_form": "조명이나 분위기가 안보아ㅛ",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025680.1.23",
          "form": "여서",
          "original_form": "여서",
          "speaker_id": "1",
          "time": "20210901 16:05"
        },
        {
          "id": "MDRW2100025680.1.24",
          "form": "생눈이좋더라",
          "original_form": "생눈이좋더라",
          "speaker_id": "1",
          "time": "20210901 16:05"
        },
        {
          "id": "MDRW2100025680.1.25",
          "form": "ㅇㅇ..그니깐 자리만 괜찮으면 생눈이 최고지",
          "original_form": "ㅇㅇ..그니깐 자리만 괜찮으면 생눈이 최고지",
          "speaker_id": "2",
          "time": "20210901 16:05"
        },
        {
          "id": "MDRW2100025680.1.26",
          "form": "극은 전체적으로 봐야 더 재밋엉",
          "original_form": "극은 전체적으로 봐야 더 재밋엉",
          "speaker_id": "2",
          "time": "20210901 16:05"
        },
        {
          "id": "MDRW2100025680.1.27",
          "form": "ㅇㅇㅇㅇ 특히 대극장은",
          "original_form": "ㅇㅇㅇㅇ 특히 대극장은",
          "speaker_id": "1",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100025680.1.28",
          "form": "멀리서봐야",
          "original_form": "멀리서봐야",
          "speaker_id": "1",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100025680.1.29",
          "form": "극이 잘보여 ㅇㅇ",
          "original_form": "극이 잘보여 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100025680.1.30",
          "form": "맞엉 콘은 블레도 나오고 애들 얼굴도 보고싶어서 나시카 잘 쓰긴하지만",
          "original_form": "맞엉 콘은 블레도 나오고 애들 얼굴도 보고싶어서 나시카 잘 쓰긴하지만",
          "speaker_id": "2",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100025680.1.31",
          "form": "공연은 어지간하면 걍 생눈이 좋아",
          "original_form": "공연은 어지간하면 걍 생눈이 좋아",
          "speaker_id": "2",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100025680.1.32",
          "form": "ㅇㅇㅇㅇㅇ ㅇ9",
          "original_form": "ㅇㅇㅇㅇㅇ ㅇ9",
          "speaker_id": "1",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100025680.1.33",
          "form": "맞아 생눈으로 보면 ㄸ느낌도 다르고",
          "original_form": "맞아 생눈으로 보면 ㄸ느낌도 다르고",
          "speaker_id": "1",
          "time": "20210901 16:08"
        }
      ]
