      "utterance": [
        {
          "id": "MDRW2100019110.1.1",
          "form": "싸이 흠뻑쇼가서 스트레스 확 풀고싶어요",
          "original_form": "싸이 흠뻑쇼가서 스트레스 확 풀고싶어요",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019110.1.2",
          "form": "전 거기가 더 스트레스일 거같은데...",
          "original_form": "전 거기가 더 스트레스일 거같은데...",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019110.1.3",
          "form": "시끄러운  거 안좋아하시구나",
          "original_form": "시끄러운  거 안좋아하시구나",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100019110.1.4",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100019110.1.5",
          "form": "전시회다니는 건 좋아하세요?",
          "original_form": "전시회다니는 건 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100019110.1.6",
          "form": "네 어릴때미술을 했어서 좋아해요",
          "original_form": "네 어릴때미술을 했어서 좋아해요",
          "speaker_id": "2",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100019110.1.7",
          "form": "가장 좋았던 전시회가 뭐예요?",
          "original_form": "가장 좋았던 전시회가 뭐예요?",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100019110.1.8",
          "form": "전 피카소전이요",
          "original_form": "전 피카소전이요",
          "speaker_id": "2",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100019110.1.9",
          "form": "피카소 좋죠",
          "original_form": "피카소 좋죠",
          "speaker_id": "1",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019110.1.10",
          "form": "전시회 좋아하세요",
          "original_form": "전시회 좋아하세요",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019110.1.11",
          "form": "즐겨가진않지만 종종 가요",
          "original_form": "즐겨가진않지만 종종 가요",
          "speaker_id": "1",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019110.1.12",
          "form": "특이하시네요ㅋㅋ",
          "original_form": "특이하시네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019110.1.13",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019110.1.14",
          "form": "이번에 마블 신작 나온대서 보러가려구요",
          "original_form": "이번에 마블 신작 나온대서 보러가려구요",
          "speaker_id": "1",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019110.1.15",
          "form": "아 샹치",
          "original_form": "아 샹치",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100019110.1.16",
          "form": "맞아요 재밌을진 모른겠지만...",
          "original_form": "맞아요 재밌을진 모른겠지만...",
          "speaker_id": "1",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019110.1.17",
          "form": "저도 보러가려구요",
          "original_form": "저도 보러가려구요",
          "speaker_id": "2",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019110.1.18",
          "form": "내일 전시회가요",
