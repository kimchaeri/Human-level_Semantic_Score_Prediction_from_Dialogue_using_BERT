      "utterance": [
        {
          "id": "MDRW2100047181.1.1",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100047181.1.2",
          "form": "배달음식 자주드시나요",
          "original_form": "배달음식 자주드시나요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100047181.1.3",
          "form": "아니요 name2님은요",
          "original_form": "아니요 &name2&님은요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100047181.1.4",
          "form": "달에 2번정도 먹어요",
          "original_form": "달에 2번정도 먹어요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100047181.1.5",
          "form": "뭐 드셨나요",
          "original_form": "뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100047181.1.6",
          "form": "최근에는 해물찜이랑 피자요",
          "original_form": "최근에는 해물찜이랑 피자요",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100047181.1.7",
          "form": "배달음식 안드시는 이유가 있으신가요?",
          "original_form": "배달음식 안드시는 이유가 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100047181.1.8",
          "form": "돈 아까워서요",
          "original_form": "돈 아까워서요",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100047181.1.9",
          "form": "하긴 배달료도 그렇고 부담스럽긴하죠",
          "original_form": "하긴 배달료도 그렇고 부담스럽긴하죠",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100047181.1.10",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100047181.1.11",
          "form": "네 삼겹살먹었어요",
          "original_form": "네 삼겹살먹었어요",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100047181.1.12",
          "form": "식사 하셨죠?",
          "original_form": "식사 하셨죠?",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100047181.1.13",
          "form": "삼겹살 좋아해요?",
          "original_form": "삼겹살 좋아해요?",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100047181.1.14",
          "form": "좋아하는편이예요",
          "original_form": "좋아하는편이예요",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100047181.1.15",
          "form": "저는 목살 좋아해요",
          "original_form": "저는 목살 좋아해요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047181.1.16",
          "form": "목살도 좋죠 기름기적어서 좋아요",
          "original_form": "목살도 좋죠 기름기적어서 좋아요",
