      "utterance": [
        {
          "id": "MDRW2100042240.1.1",
          "form": "아침은 챙겨먹는 편인가요?",
          "original_form": "아침은 챙겨먹는 편인가요?",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042240.1.2",
          "form": "아침은 잘 안 먹게 되더라구요",
          "original_form": "아침은 잘 안 먹게 되더라구요",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042240.1.3",
          "form": "먹어도 그냥 간단히 속 채울 정도?",
          "original_form": "먹어도 그냥 간단히 속 채울 정도?",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042240.1.4",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042240.1.5",
          "form": "아침에 바빠서 인가요 아니면 습관이라서 인가요?",
          "original_form": "아침에 바빠서 인가요 아니면 습관이라서 인가요?",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042240.1.6",
          "form": "둘 다일까요?ㅋㅋ",
          "original_form": "둘 다일까요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042240.1.7",
          "form": "그런가요?ㅎㅎ",
          "original_form": "그런가요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042240.1.8",
          "form": "저희 가족들이 아침형 인간들이라서 꼭 아침 다들 먹거든요",
          "original_form": "저희 가족들이 아침형 인간들이라서 꼭 아침 다들 먹거든요",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042240.1.9",
          "form": "저 역시 아침은 꼬박꼬박 먹어요",
          "original_form": "저 역시 아침은 꼬박꼬박 먹어요",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042240.1.10",
          "form": "먹는게 습관이 됐어요",
          "original_form": "먹는게 습관이 됐어요",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042240.1.11",
          "form": "그럼 보통 일반 한식 드시나요?",
          "original_form": "그럼 보통 일반 한식 드시나요?",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042240.1.12",
          "form": "네 제가 밥과 국을 좋아해서 그렇게 요리하는데",
          "original_form": "네 제가 밥과 국을 좋아해서 그렇게 요리하는데",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042240.1.13",
          "form": "최근들어 살짝 간소하게 할떄도 종종 있어요",
          "original_form": "최근들어 살짝 간소하게 할떄도 종종 있어요",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042240.1.14",
          "form": "전 먹어도 간단하게 샌드위치나 그냥 바나나 하나, 요거트 정도에요ㅎㅎ",
          "original_form": "전 먹어도 간단하게 샌드위치나 그냥 바나나 하나, 요거트 정도에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042240.1.15",
          "form": "저도 요새 간단하게 요거트에 견과류 넣고 과일과 채소를 좀 썷어넣어서 먹고 있어요",
          "original_form": "저도 요새 간단하게 요거트에 견과류 넣고 과일과 채소를 좀 썷어넣어서 먹고 있어요",
          "speaker_id": "1",
          "time": "20210907 08:31"
        },
        {
          "id": "MDRW2100042240.1.16",
          "form": "간단하고 든든한 레시피네요ㅎㅎ",
          "original_form": "간단하고 든든한 레시피네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:18"
        },
        {
          "id": "MDRW2100042240.1.17",
          "form": "네 간소한 아침도 괜찮더라구요",
          "original_form": "네 간소한 아침도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100042240.1.18",
          "form": "간단하게라도 아침 먹으면 좋긴 한데 챙겨먹기가 쉽지 않네요ㅎㅎ",
          "original_form": "간단하게라도 아침 먹으면 좋긴 한데 챙겨먹기가 쉽지 않네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100042240.1.19",
          "form": "챙겨먹는게 쉽진 않죠",
          "original_form": "챙겨먹는게 쉽진 않죠",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100042240.1.20",
          "form": "그래서 저도 식구들 챙겨줘요",
          "original_form": "그래서 저도 식구들 챙겨줘요",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100042240.1.21",
          "form": "스스로 챙겨먹으라고 하면 끼니 거를까봐",
          "original_form": "스스로 챙겨먹으라고 하면 끼니 거를까봐",
          "speaker_id": "1",
          "time": "20210907 13:48"
        }
      ]
