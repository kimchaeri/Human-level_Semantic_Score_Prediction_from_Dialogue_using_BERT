      "utterance": [
        {
          "id": "MDRW2100031990.1.1",
          "form": "와 맛있겠어요",
          "original_form": "와 맛있겠어요",
          "speaker_id": "2",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031990.1.2",
          "form": "음료는 뭘드시나요?",
          "original_form": "음료는 뭘드시나요?",
          "speaker_id": "2",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031990.1.3",
          "form": "보이차마셨네요ㅋ\r\n모드셨어여?",
          "original_form": "보이차마셨네요ㅋ\r\n모드셨어여?",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031990.1.4",
          "form": "보이차 좋죠",
          "original_form": "보이차 좋죠",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031990.1.5",
          "form": "저는 유부초밥에 물마셨어욯ㅎㅎㅇ",
          "original_form": "저는 유부초밥에 물마셨어욯ㅎㅎㅇ",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031990.1.6",
          "form": "차 좋아하세요?",
          "original_form": "차 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031990.1.7",
          "form": "아 유부초밥맛잇져~\r\n사실전 맹물을 젤좋아해요ㅋㅋ",
          "original_form": "아 유부초밥맛잇져~\r\n사실전 맹물을 젤좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031990.1.8",
          "form": "그러시군요 ㅋㅋㅋ",
          "original_form": "그러시군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100031990.1.9",
          "form": "저는 마시는건 다 좋아해요!",
          "original_form": "저는 마시는건 다 좋아해요!",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100031990.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100031990.1.11",
          "form": "점심은 뭐 드실건가요??",
          "original_form": "점심은 뭐 드실건가요??",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100031990.1.12",
          "form": "꽃게된장찌개에 오징어초무침",
          "original_form": "꽃게된장찌개에 오징어초무침",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100031990.1.13",
          "form": "하려고요",
          "original_form": "하려고요",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100031990.1.14",
          "form": "와 꽃게된장찌개라니 너무 맛있겠어요",
          "original_form": "와 꽃게된장찌개라니 너무 맛있겠어요",
          "speaker_id": "2",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100031990.1.15",
          "form": "꽃게 요즘 살많이 찼나요?",
          "original_form": "꽃게 요즘 살많이 찼나요?",
          "speaker_id": "2",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100031990.1.16",
          "form": "그럴줄알고 삿는데 아직인가봐요\r\n빈것도 잇네요",
          "original_form": "그럴줄알고 삿는데 아직인가봐요\r\n빈것도 잇네요",
          "speaker_id": "1",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100031990.1.17",
          "form": "아쉽네요 ㅠㅠ",
          "original_form": "아쉽네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 16:20"
        },
        {
          "id": "MDRW2100031990.1.18",
          "form": "하지만 꽃게된장찌개는 너무 시원하겠어요",
          "original_form": "하지만 꽃게된장찌개는 너무 시원하겠어요",
          "speaker_id": "2",
          "time": "20210905 16:20"
        },
        {
          "id": "MDRW2100031990.1.19",
          "form": "고기육수를 좋아하시나요 해물육수를 좋아하시나요?",
          "original_form": "고기육수를 좋아하시나요 해물육수를 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 16:20"
        },
        {
          "id": "MDRW2100031990.1.20",
          "form": "아 고를수없는 질문이네요\r\n둘다 놓칠수없어여",
          "original_form": "아 고를수없는 질문이네요\r\n둘다 놓칠수없어여",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100031990.1.21",
          "form": "ㅋㅋㅋㅋㅋ그렇죠 둘다 다른매력이잖아요",
          "original_form": "ㅋㅋㅋㅋㅋ그렇죠 둘다 다른매력이잖아요",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100031990.1.22",
          "form": "전 해물육수를 좋아해요 시원해서",
          "original_form": "전 해물육수를 좋아해요 시원해서",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100031990.1.23",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100031990.1.24",
          "form": "샤브샤브할때 둘다 너무 좋아요",
          "original_form": "샤브샤브할때 둘다 너무 좋아요",
          "speaker_id": "1",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100031990.1.25",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210905 19:21"
        },
        {
          "id": "MDRW2100031990.1.26",
          "form": "오랜만에 샤브샤브가 먹고싶네요",
          "original_form": "오랜만에 샤브샤브가 먹고싶네요",
          "speaker_id": "2",
          "time": "20210905 19:22"
        },
        {
          "id": "MDRW2100031990.1.27",
          "form": "샤브샤브 야채잇는거 아무거나 넣고 해도\r\n맛나니까 좋아요",
          "original_form": "샤브샤브 야채잇는거 아무거나 넣고 해도\r\n맛나니까 좋아요",
          "speaker_id": "1",
          "time": "20210905 19:23"
        }
      ]
    }
