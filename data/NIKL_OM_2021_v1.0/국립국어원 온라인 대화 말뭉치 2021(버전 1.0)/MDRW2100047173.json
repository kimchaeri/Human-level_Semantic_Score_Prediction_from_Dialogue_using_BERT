        {
          "id": "MDRW2100047173.1.2",
          "form": "name2님은 공연 좋아하세요?",
          "original_form": "&name2&님은 공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100047173.1.3",
          "form": "공연 잘은 못보지만 좋아해요!",
          "original_form": "공연 잘은 못보지만 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100047173.1.4",
          "form": "어떤 작품 보셨나요?",
          "original_form": "어떤 작품 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100047173.1.5",
          "form": "예전에 봤던거라 기억이 잘 나지않네요",
          "original_form": "예전에 봤던거라 기억이 잘 나지않네요",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100047173.1.6",
          "form": "name1님은 공연 좋아하세요?",
          "original_form": "&name1&님은 공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100047173.1.7",
          "form": "콘서트 보러 다녔어요",
          "original_form": "콘서트 보러 다녔어요",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100047173.1.8",
          "form": "오 콘서트",
          "original_form": "오 콘서트",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100047173.1.9",
          "form": "누구 콘서트 다녀오셨어요?",
          "original_form": "누구 콘서트 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100047173.1.10",
          "form": "아이유랑 내한 공연이요",
          "original_form": "아이유랑 내한 공연이요",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100047173.1.11",
          "form": "아이유요?",
          "original_form": "아이유요?",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100047173.1.12",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100047173.1.13",
          "form": "너무 부러워요",
          "original_form": "너무 부러워요",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100047173.1.14",
          "form": "아이유콘서트",
          "original_form": "아이유콘서트",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100047173.1.15",
          "form": "저 진짜 가고싶었는데 코로나 터지고나서",
          "original_form": "저 진짜 가고싶었는데 코로나 터지고나서",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100047173.1.16",
          "form": "후회하고 있어요ㅜ",
          "original_form": "후회하고 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100047173.1.17",
          "form": "내한공연은 어떤 가수요?",
          "original_form": "내한공연은 어떤 가수요?",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100047173.1.18",
          "form": "아주 옛날에 갔었어요ㅋㅋ",
          "original_form": "아주 옛날에 갔었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100047173.1.19",
          "form": "디엔씨이, 케이티페리 등등",
          "original_form": "디엔씨이, 케이티페리 등등",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100047173.1.20",
          "form": "봤어용",
          "original_form": "봤어용",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100047173.1.21",
          "form": "와 진짜 재밌으셨겠어요!",
          "original_form": "와 진짜 재밌으셨겠어요!",
          "speaker_id": "2",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100047173.1.22",
          "form": "에너지가 남달랐어요",
          "original_form": "에너지가 남달랐어요",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100047173.1.23",
          "form": "전 코로나 전엠 그냥 미루기만했는데",
          "original_form": "전 코로나 전엠 그냥 미루기만했는데",
