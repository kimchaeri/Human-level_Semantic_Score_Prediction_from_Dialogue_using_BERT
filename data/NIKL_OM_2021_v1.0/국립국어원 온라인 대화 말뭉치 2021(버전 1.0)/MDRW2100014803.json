      "utterance": [
        {
          "id": "MDRW2100014803.1.1",
          "form": "다이어트 해보셨나요?",
          "original_form": "다이어트 해보셨나요?",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014803.1.2",
          "form": "많이 시도해봤어요ㅎㅎ",
          "original_form": "많이 시도해봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014803.1.3",
          "form": "정말 많이ㅎㅎ",
          "original_form": "정말 많이ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014803.1.4",
          "form": "전 운동해서 뺐다가 또 치고 다시빼고해요ㅎㅎ",
          "original_form": "전 운동해서 뺐다가 또 치고 다시빼고해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014803.1.5",
          "form": "능숙하시군요",
          "original_form": "능숙하시군요",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014803.1.6",
          "form": "운동은 뭐좋아하세요?",
          "original_form": "운동은 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014803.1.7",
          "form": "요가 매일해요",
          "original_form": "요가 매일해요",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014803.1.8",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014803.1.9",
          "form": "나마스떼~^^",
          "original_form": "나마스떼~^^",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014803.1.10",
          "form": "뭔말인지모르는데 요가하니깐 말하네요ㅎㅎㅎ",
          "original_form": "뭔말인지모르는데 요가하니깐 말하네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014803.1.11",
          "form": "감사합니다인가?",
          "original_form": "감사합니다인가?",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014803.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100014803.1.13",
          "form": "전 운동은 홈트밖에안해요ㅠ",
          "original_form": "전 운동은 홈트밖에안해요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100014803.1.14",
          "form": "예전에는 크로스핏 했었는데",
          "original_form": "예전에는 크로스핏 했었는데",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100014803.1.15",
          "form": "아무래도 코로나라 못하네요ㅠ",
          "original_form": "아무래도 코로나라 못하네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100014803.1.16",
          "form": "헬스장 가본적있으세요?",
          "original_form": "헬스장 가본적있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100014803.1.17",
          "form": "헬스장은 저랑 안맞더라구요ㅎ",
          "original_form": "헬스장은 저랑 안맞더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100014803.1.18",
          "form": "헬스장 자주가세요?",
          "original_form": "헬스장 자주가세요?",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100014803.1.19",
          "form": "가본적없네요ㅎㅎ",
          "original_form": "가본적없네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100014803.1.20",
          "form": "ㅎㅎ 운동은 그럼 요가원을 가는거에요?",
          "original_form": "ㅎㅎ 운동은 그럼 요가원을 가는거에요?",
