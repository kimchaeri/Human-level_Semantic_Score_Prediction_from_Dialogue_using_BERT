      "utterance": [
        {
          "id": "MDRW2100017574.1.1",
          "form": "다이어트 중이세요?",
          "original_form": "다이어트 중이세요?",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017574.1.2",
          "form": "어떻게 아셨어요?",
          "original_form": "어떻게 아셨어요?",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017574.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017574.1.4",
          "form": "요즘 젊은 사람들 코시국이라 다이어트 많이들 한다하더라구요",
          "original_form": "요즘 젊은 사람들 코시국이라 다이어트 많이들 한다하더라구요",
          "speaker_id": "1",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017574.1.5",
          "form": "저도 역시 하고 있습니다. ㅎㅎ",
          "original_form": "저도 역시 하고 있습니다. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017574.1.6",
          "form": "다 같은가봐요 ㅋㅋㅋ",
          "original_form": "다 같은가봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017574.1.7",
          "form": "동지네요 ㅠㅠ 눈물",
          "original_form": "동지네요 ㅠㅠ 눈물",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017574.1.8",
          "form": "어떤식으로 다이어트 중이세요?",
          "original_form": "어떤식으로 다이어트 중이세요?",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017574.1.9",
          "form": "저는 칼로리만 제한해서 하고있어요 ㅋㅋㅋ",
          "original_form": "저는 칼로리만 제한해서 하고있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017574.1.10",
          "form": "딱 그것만 하고 있어요?",
          "original_form": "딱 그것만 하고 있어요?",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017574.1.11",
          "form": "어떤식으로 제한하고 있는데요?",
          "original_form": "어떤식으로 제한하고 있는데요?",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017574.1.12",
          "form": "천칼로리 미만으로 먹고있어요 일반식으루",
          "original_form": "천칼로리 미만으로 먹고있어요 일반식으루",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017574.1.13",
          "form": "운동은 그냥 실내자전거 타요",
          "original_form": "운동은 그냥 실내자전거 타요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017574.1.14",
          "form": "오.. 전 칼로리 제한은 안하는데",
          "original_form": "오.. 전 칼로리 제한은 안하는데",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017574.1.15",
          "form": "대신 탄수화물만 제한해요 ㅎㅎ",
          "original_form": "대신 탄수화물만 제한해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017574.1.16",
          "form": "뭔가 그게더 힘들거같아요 ㅋㅋㅋ",
          "original_form": "뭔가 그게더 힘들거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017574.1.17",
          "form": "적응 되면 할만하더라구요 ㅎㅎ",
          "original_form": "적응 되면 할만하더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017574.1.18",
          "form": "빵, 밀가루, 밥, 떡 이런 것만 피하면 되는거라서..",
          "original_form": "빵, 밀가루, 밥, 떡 이런 것만 피하면 되는거라서..",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017574.1.19",
          "form": "하 맛있는건 다 피하네요...",
          "original_form": "하 맛있는건 다 피하네요...",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017574.1.20",
          "form": "그거 다 못 먹으면 뭐먹죠? ㅠㅠ",
          "original_form": "그거 다 못 먹으면 뭐먹죠? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017574.1.21",
          "form": "샐러드, 연어, 고기(삼겹살, 갈비, 치킨 등 가능) 등등..",
          "original_form": "샐러드, 연어, 고기(삼겹살, 갈비, 치킨 등 가능) 등등..",
          "speaker_id": "1",
