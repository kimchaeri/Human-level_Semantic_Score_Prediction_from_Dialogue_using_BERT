      "utterance": [
        {
          "id": "MDRW2100011248.1.1",
          "form": "둘째 어린이집 갔나??",
          "original_form": "둘째 어린이집 갔나??",
          "speaker_id": "2",
          "time": "20210824 09:57"
        },
        {
          "id": "MDRW2100011248.1.2",
          "form": "응 언니 둘째보냈어",
          "original_form": "응 언니 둘째보냈어",
          "speaker_id": "1",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011248.1.3",
          "form": "언니네는?",
          "original_form": "언니네는?",
          "speaker_id": "1",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011248.1.4",
          "form": "언니 출근했어?",
          "original_form": "언니 출근했어?",
          "speaker_id": "1",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100011248.1.5",
          "form": "엉 난 출근했지",
          "original_form": "엉 난 출근했지",
          "speaker_id": "2",
          "time": "20210824 10:05"
        },
        {
          "id": "MDRW2100011248.1.6",
          "form": "비와서 사무실에 앉아있음",
          "original_form": "비와서 사무실에 앉아있음",
          "speaker_id": "2",
          "time": "20210824 10:05"
        },
        {
          "id": "MDRW2100011248.1.7",
          "form": "우리투도 갔겠지?? ㅎㅎ",
          "original_form": "우리투도 갔겠지?? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:05"
        },
        {
          "id": "MDRW2100011248.1.8",
          "form": "응 갔을거야. 나는 아침에 못봤다만ㅋ",
          "original_form": "응 갔을거야. 나는 아침에 못봤다만ㅋ",
          "speaker_id": "1",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100011248.1.9",
          "form": "울집 둘째 계속 돌아다니면서 첨벙첨벙놀아가ㅋㅋㅋ정신없었음ㅋㅋ",
          "original_form": "울집 둘째 계속 돌아다니면서 첨벙첨벙놀아가ㅋㅋㅋ정신없었음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100011248.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 요새 차 시간바껴서 31분에 감",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 요새 차 시간바껴서 31분에 감",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100011248.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 신났구먼",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 신났구먼",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100011248.1.12",
          "form": "아 그래서 안보였구나ㅋㅋ",
          "original_form": "아 그래서 안보였구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100011248.1.13",
          "form": "요즘 안보이더라고ㅋㅋ",
          "original_form": "요즘 안보이더라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100011248.1.14",
          "form": "시간이 안맞네ㅋㅋ",
          "original_form": "시간이 안맞네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100011248.1.15",
          "form": "엉 ㅋㅋㅋㅋ",
          "original_form": "엉 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100011248.1.16",
          "form": "첫째 돌봄교실에",
          "original_form": "첫째 돌봄교실에",
          "speaker_id": "1",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100011248.1.17",
          "form": "전화했는데",
          "original_form": "전화했는데",
          "speaker_id": "1",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100011248.1.18",
          "form": "우리투도 첨벙첨벙하면서 갔겠지 ㅋㅋㅋ",
          "original_form": "우리투도 첨벙첨벙하면서 갔겠지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100011248.1.19",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100011248.1.20",
          "form": "내일까지는 안나간다했거든ㅋㅋ",
          "original_form": "내일까지는 안나간다했거든ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100011248.1.21",
          "form": "내일 장기자랑하기로 했는데 첫째가 평소엔 조용히 있다가",
          "original_form": "내일 장기자랑하기로 했는데 첫째가 평소엔 조용히 있다가",
          "speaker_id": "1",
          "time": "20210824 10:09"
        },
        {
          "id": "MDRW2100011248.1.22",
          "form": "어제는 너무 적극적이더라면서",
          "original_form": "어제는 너무 적극적이더라면서",
          "speaker_id": "1",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011248.1.23",
          "form": "장기자랑 조장까지 한다고 했다대?",
          "original_form": "장기자랑 조장까지 한다고 했다대?",
          "speaker_id": "1",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011248.1.24",
          "form": "근데 출석안하면 아....",
          "original_form": "근데 출석안하면 아....",
          "speaker_id": "1",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011248.1.25",
          "form": "근데  못 가겠다했으니ㅋㅋ",
          "original_form": "근데  못 가겠다했으니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100011248.1.26",
          "form": "선생님이 난감해하시더라ㅋㅋ",
          "original_form": "선생님이 난감해하시더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100011248.1.27",
          "form": "그래서 첫째 일어나서 한번 물어보고ㅋㅋㅋ다시 전화하기로ㅋㅋ",
          "original_form": "그래서 첫째 일어나서 한번 물어보고ㅋㅋㅋ다시 전화하기로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:12"
        },
        {
          "id": "MDRW2100011248.1.28",
          "form": "장기자랑 조장 되었는데ㅋㅋ",
          "original_form": "장기자랑 조장 되었는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011248.1.29",
          "form": "보내야될것같은ㅋㅋㅋ",
          "original_form": "보내야될것같은ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011248.1.30",
          "form": "아하....",
          "original_form": "아하....",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011248.1.31",
          "form": "조장인데 가야지 ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "조장인데 가야지 ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011248.1.32",
          "form": "대단하다 정말",
          "original_form": "대단하다 정말",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011248.1.33",
          "form": "ㅋㅋㅋㅋㄱㅋㅋ웃겨ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋ웃겨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:16"
        },
        {
          "id": "MDRW2100011248.1.34",
          "form": "첫째가 갈때 뭐라고하고 간줄아나?ㅋㅋ",
          "original_form": "첫째가 갈때 뭐라고하고 간줄아나?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:16"
        },
        {
          "id": "MDRW2100011248.1.35",
          "form": "엄마 아픈데 내까지 있으면 더 아플테니",
          "original_form": "엄마 아픈데 내까지 있으면 더 아플테니",
          "speaker_id": "1",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100011248.1.36",
          "form": "내가 할머니집갈게. 보내줘",
          "original_form": "내가 할머니집갈게. 보내줘",
          "speaker_id": "1",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100011248.1.37",
          "form": "이러고 갔다",
          "original_form": "이러고 갔다",
          "speaker_id": "1",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100011248.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋ 효녀다 정말",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋ 효녀다 정말",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011248.1.39",
          "form": "보내줘 라니 ㅋㅋㄱㅋㄱㅋㅋㄱ",
          "original_form": "보내줘 라니 ㅋㅋㄱㅋㄱㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011248.1.40",
          "form": "조장님 오시나??",
          "original_form": "조장님 오시나??",
          "speaker_id": "2",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100011248.1.41",
