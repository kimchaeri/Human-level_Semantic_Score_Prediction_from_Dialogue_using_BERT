      "utterance": [
        {
          "id": "MDRW2100020231.1.1",
          "form": "점심 드셨나요!",
          "original_form": "점심 드셨나요!",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020231.1.2",
          "form": "네 간단하게 컵라면 먹었어요",
          "original_form": "네 간단하게 컵라면 먹었어요",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020231.1.3",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020231.1.4",
          "form": "저는 이제 먹으려구요 ㅎㅎ !",
          "original_form": "저는 이제 먹으려구요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020231.1.5",
          "form": "어떤거 드시나요?",
          "original_form": "어떤거 드시나요?",
          "speaker_id": "2",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020231.1.6",
          "form": "저 오늘 밀면 먹으려구요!",
          "original_form": "저 오늘 밀면 먹으려구요!",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020231.1.7",
          "form": "와 맛있겠네요",
          "original_form": "와 맛있겠네요",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020231.1.8",
          "form": "식당에서 드시나요?",
          "original_form": "식당에서 드시나요?",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020231.1.9",
          "form": "아니요 배달 기다리는데",
          "original_form": "아니요 배달 기다리는데",
          "speaker_id": "1",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020231.1.10",
          "form": "1시간째 안오네요 ㅜㅜ",
          "original_form": "1시간째 안오네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020231.1.11",
          "form": "헉 ㅠㅠ",
          "original_form": "헉 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020231.1.12",
          "form": "점심때 배달은 진짜 오래걸리더라구요",
          "original_form": "점심때 배달은 진짜 오래걸리더라구요",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020231.1.13",
          "form": "맞죠 오늘 토요일이라 더 그런가봐요ㅜㅜ",
          "original_form": "맞죠 오늘 토요일이라 더 그런가봐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100020231.1.14",
          "form": "중식 좋아하세요?",
          "original_form": "중식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100020231.1.15",
          "form": "네네 짬뽕 좋아해요",
          "original_form": "네네 짬뽕 좋아해요",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020231.1.16",
          "form": "중식 어떤메뉴 좋아하세요?",
          "original_form": "중식 어떤메뉴 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020231.1.17",
          "form": "저는 짜장면이요 !",
          "original_form": "저는 짜장면이요 !",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020231.1.18",
          "form": "짬뽕도 좋아하는데 맛집 찾기가 어려워요 ㅜㅜ",
          "original_form": "짬뽕도 좋아하는데 맛집 찾기가 어려워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020231.1.19",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100020231.1.20",
          "form": "맵기만 해서 맛은 없는 곳도 많더라구요",
          "original_form": "맵기만 해서 맛은 없는 곳도 많더라구요",
          "speaker_id": "2",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100020231.1.21",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100020231.1.22",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210828 15:57"
