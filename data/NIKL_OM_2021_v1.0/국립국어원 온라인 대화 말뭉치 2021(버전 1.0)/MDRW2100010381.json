      "utterance": [
        {
          "id": "MDRW2100010381.1.1",
          "form": "내가 최근에 뭐 샀는지 아니?",
          "original_form": "내가 최근에 뭐 샀는지 아니?",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010381.1.2",
          "form": "어 정말? 어디서 샀어?",
          "original_form": "어 정말? 어디서 샀어?",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010381.1.3",
          "form": "쿠팡에서 샀어",
          "original_form": "쿠팡에서 샀어",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010381.1.4",
          "form": "집에 걸 쓰지 왜 또 산거야?",
          "original_form": "집에 걸 쓰지 왜 또 산거야?",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010381.1.5",
          "form": "드디어 진작살껄 그랬어",
          "original_form": "드디어 진작살껄 그랬어",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010381.1.6",
          "form": "그냥 새로운 베개가 갖고싶었단다",
          "original_form": "그냥 새로운 베개가 갖고싶었단다",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010381.1.7",
          "form": "그동안 베개없이 살았지?",
          "original_form": "그동안 베개없이 살았지?",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010381.1.8",
          "form": "어떤 베개인지 다음에 알려주렴",
          "original_form": "어떤 베개인지 다음에 알려주렴",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.9",
          "form": "응 나 담요로 만들어서 썼어",
          "original_form": "응 나 담요로 만들어서 썼어",
          "speaker_id": "1",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.10",
          "form": "이불은 안샀어?",
          "original_form": "이불은 안샀어?",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.11",
          "form": "응응 너는 뭘샀니?",
          "original_form": "응응 너는 뭘샀니?",
          "speaker_id": "1",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.12",
          "form": "아무래도 돈이 소중하니까",
          "original_form": "아무래도 돈이 소중하니까",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.13",
          "form": "나는 최근에 북트롤리를 샀어",
          "original_form": "나는 최근에 북트롤리를 샀어",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.14",
          "form": "근데 배송이 오지를 않네",
          "original_form": "근데 배송이 오지를 않네",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.15",
          "form": "감감무소식이야",
          "original_form": "감감무소식이야",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.16",
          "form": "앗 ㅠ 그래도 아직 주말은",
          "original_form": "앗 ㅠ 그래도 아직 주말은",
          "speaker_id": "1",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.17",
          "form": "무소식이 희소식인건가... 난 빨리 필요한데",
          "original_form": "무소식이 희소식인건가... 난 빨리 필요한데",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.18",
          "form": "많이 남아있으니까 조금만 기다리자!",
          "original_form": "많이 남아있으니까 조금만 기다리자!",
          "speaker_id": "1",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.19",
          "form": "난 참을성이 없어",
          "original_form": "난 참을성이 없어",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.20",
          "form": "빨리 보내줘",
          "original_form": "빨리 보내줘",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010381.1.21",
          "form": "내친구가 최근에 베개를 샀대",
          "original_form": "내친구가 최근에 베개를 샀대",
          "speaker_id": "2",
          "time": "20210823 23:26"
        }
      ]
