      "utterance": [
        {
          "id": "MDRW2100014281.1.1",
          "form": "기본에 충실한곳이 좋아",
          "original_form": "기본에 충실한곳이 좋아",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014281.1.2",
          "form": "비싸다",
          "original_form": "비싸다",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014281.1.3",
          "form": "치킨은 맥주죠",
          "original_form": "치킨은 맥주죠",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014281.1.4",
          "form": "양념은 어디가 제일 맛있나?",
          "original_form": "양념은 어디가 제일 맛있나?",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014281.1.5",
          "form": "멕시카나도맛잇고 원래 장모님치킨좋아했는데없어졌나봐",
          "original_form": "멕시카나도맛잇고 원래 장모님치킨좋아했는데없어졌나봐",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014281.1.6",
          "form": "저기 맥주는 보조일뿐이에요..",
          "original_form": "저기 맥주는 보조일뿐이에요..",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014281.1.7",
          "form": "가격도싸고 쿠폰도주고 좋았는데 ㅎ",
          "original_form": "가격도싸고 쿠폰도주고 좋았는데 ㅎ",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014281.1.8",
          "form": "헐 장모님치킨 알아? 대박",
          "original_form": "헐 장모님치킨 알아? 대박",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014281.1.9",
          "form": "여기와서알았어",
          "original_form": "여기와서알았어",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014281.1.10",
          "form": "예전에 우리 큰외삼촌 장모님치킨 했어 ㅋㅋ 진짜 맛있었는데",
          "original_form": "예전에 우리 큰외삼촌 장모님치킨 했어 ㅋㅋ 진짜 맛있었는데",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014281.1.11",
          "form": "진짜 많이 시켜먹었었움",
          "original_form": "진짜 많이 시켜먹었었움",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014281.1.12",
          "form": "암튼 치킨이 제일 만만하긴하다 배달 음식중엔",
          "original_form": "암튼 치킨이 제일 만만하긴하다 배달 음식중엔",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014281.1.13",
          "form": "브랜드가너무많아서 머리아픔? ㅋㅋ",
          "original_form": "브랜드가너무많아서 머리아픔? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014281.1.14",
          "form": "행복한고민이지",
          "original_form": "행복한고민이지",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014281.1.15",
          "form": "오늘은 또 어디껄 시켜먹나~하면서",
          "original_form": "오늘은 또 어디껄 시켜먹나~하면서",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014281.1.16",
          "form": "아직도 아까 닭개장이 ㅋㅋㅋ배터지겟당 ㅋㅋ",
          "original_form": "아직도 아까 닭개장이 ㅋㅋㅋ배터지겟당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014281.1.17",
          "form": "닭개장먹었는데 치킨 가능합니까?",
          "original_form": "닭개장먹었는데 치킨 가능합니까?",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014281.1.18",
          "form": "아..아뇨안땡깁니다",
          "original_form": "아..아뇨안땡깁니다",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014281.1.19",
          "form": "너무배불러서 ㅋㅋㅋ맥주한캔하고싶군요",
          "original_form": "너무배불러서 ㅋㅋㅋ맥주한캔하고싶군요",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014281.1.20",
          "form": "닭이지만 조리 방법이 다르니 난 가능할거같아 ㅋㅋㅋ",
          "original_form": "닭이지만 조리 방법이 다르니 난 가능할거같아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:16"
        }
