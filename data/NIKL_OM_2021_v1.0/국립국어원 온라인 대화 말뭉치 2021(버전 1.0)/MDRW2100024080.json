      "utterance": [
        {
          "id": "MDRW2100024080.1.1",
          "form": "탁주요?",
          "original_form": "탁주요?",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024080.1.2",
          "form": "탁주요??",
          "original_form": "탁주요??",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024080.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024080.1.4",
          "form": "크 맛있겠다",
          "original_form": "크 맛있겠다",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024080.1.5",
          "form": "술 좋아하세요?",
          "original_form": "술 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024080.1.6",
          "form": "네!자주는 안먹지만 먹을땐 좋아해요ㅋㅋ",
          "original_form": "네!자주는 안먹지만 먹을땐 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024080.1.7",
          "form": "전 술은 가끔만 마셔요 ㅋㅋㅋ 집에서 홀짝 정도??",
          "original_form": "전 술은 가끔만 마셔요 ㅋㅋㅋ 집에서 홀짝 정도??",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024080.1.8",
          "form": "막걸리도 가끔 마시구...",
          "original_form": "막걸리도 가끔 마시구...",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024080.1.9",
          "form": "다음날 숙취없을 정도로만요!",
          "original_form": "다음날 숙취없을 정도로만요!",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024080.1.10",
          "form": "아 종류는 어떤걸로 드세요?",
          "original_form": "아 종류는 어떤걸로 드세요?",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024080.1.11",
          "form": "거의 맥주 많이 마시구요",
          "original_form": "거의 맥주 많이 마시구요",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024080.1.12",
          "form": "보드카 사다놓고 하이볼 말아 마시거나",
          "original_form": "보드카 사다놓고 하이볼 말아 마시거나",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024080.1.13",
          "form": "가끔 아빠랑 막걸리",
          "original_form": "가끔 아빠랑 막걸리",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024080.1.14",
          "form": "엄마랑은 와인 자주 마시규",
          "original_form": "엄마랑은 와인 자주 마시규",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024080.1.15",
          "form": "스파클링 와인도 가끔이요 ㅋㅋ",
          "original_form": "스파클링 와인도 가끔이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024080.1.16",
          "form": "와우!",
          "original_form": "와우!",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024080.1.17",
          "form": "요새편의점 와인이 너무 잘나와서",
          "original_form": "요새편의점 와인이 너무 잘나와서",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024080.1.18",
          "form": "좋더라구요ㅎㅎ",
          "original_form": "좋더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024080.1.19",
          "form": "그쳐 ㅋㅋㅋㅋ",
          "original_form": "그쳐 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024080.1.20",
          "form": "예전에는 사디 힘들었는데 비싸구요",
          "original_form": "예전에는 사디 힘들었는데 비싸구요",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024080.1.21",
          "form": "요즘은 저렴한 와인 많이 나오고 맛도있더라구요",
          "original_form": "요즘은 저렴한 와인 많이 나오고 맛도있더라구요",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024080.1.22",
          "form": "이마트24에서 1년에 두번 세일하는 와인이 있는데 진짜 맛있더라구요",
          "original_form": "이마트24에서 1년에 두번 세일하는 와인이 있는데 진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024080.1.23",
          "form": "전 달달한 술 좋아해서요 ㅋㅋㅋㅋ",
          "original_form": "전 달달한 술 좋아해서요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024080.1.24",
          "form": "와 뭐예요?",
          "original_form": "와 뭐예요?",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024080.1.25",
          "form": "이름이 긴데",
          "original_form": "이름이 긴데",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024080.1.26",
          "form": "라 크라사드 까베르네 시라라고",
          "original_form": "라 크라사드 까베르네 시라라고",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024080.1.27",
          "form": "전 시라 와인이라고 불러요ㅋ",
          "original_form": "전 시라 와인이라고 불러요ㅋ",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024080.1.28",
          "form": "근데 달달하지 않아서ㅜㅜ",
          "original_form": "근데 달달하지 않아서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024080.1.29",
          "form": "마느리 님한테는  어떨지 모르겠네요",
          "original_form": "마느리 님한테는  어떨지 모르겠네요",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100024080.1.30",
          "form": "어 이거 알아야!! ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어 이거 알아야!! ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100024080.1.31",
          "form": "저희 엄마가 좋아하세여",
          "original_form": "저희 엄마가 좋아하세여",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100024080.1.32",
          "form": "같이 한 모금했다다 재 타입은 아니라",
          "original_form": "같이 한 모금했다다 재 타입은 아니라",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100024080.1.33",
          "form": "엄마한테 양보한...",
          "original_form": "엄마한테 양보한...",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100024080.1.34",
          "form": "ㅋㅋㅋㅋㅋ평소에는 너무 비싼데",
          "original_form": "ㅋㅋㅋㅋㅋ평소에는 너무 비싼데",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100024080.1.35",
          "form": "세일하면 득템이라ㅋ",
          "original_form": "세일하면 득템이라ㅋ",
          "speaker_id": "1",
          "time": "20210830 22:08"
        }
      ]
