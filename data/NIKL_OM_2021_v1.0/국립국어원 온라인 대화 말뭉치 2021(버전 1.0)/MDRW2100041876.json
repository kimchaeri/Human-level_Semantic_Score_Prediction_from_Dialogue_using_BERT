      "utterance": [
        {
          "id": "MDRW2100041876.1.1",
          "form": "점심 어떤거 드셧어여,ㅡ",
          "original_form": "점심 어떤거 드셧어여,ㅡ",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041876.1.2",
          "form": "전 즉석떡볶이 먹었어요!",
          "original_form": "전 즉석떡볶이 먹었어요!",
          "speaker_id": "2",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041876.1.3",
          "form": "점심 뭐드셨나요",
          "original_form": "점심 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041876.1.4",
          "form": "저는 김밥 먹었어요!! 간단하게",
          "original_form": "저는 김밥 먹었어요!! 간단하게",
          "speaker_id": "1",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100041876.1.5",
          "form": "이제 저녁시간이네요 ㅠ",
          "original_form": "이제 저녁시간이네요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100041876.1.6",
          "form": "김밥으로 저녁까지 너뮤 힘들지 않나여? ㅠ",
          "original_form": "김밥으로 저녁까지 너뮤 힘들지 않나여? ㅠ",
          "speaker_id": "2",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100041876.1.7",
          "form": "요즘 입맛이없어서 ㅠㅠ",
          "original_form": "요즘 입맛이없어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:04"
        },
        {
          "id": "MDRW2100041876.1.8",
          "form": "밤되면 배고파요 ㅋㅋ",
          "original_form": "밤되면 배고파요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:04"
        },
        {
          "id": "MDRW2100041876.1.9",
          "form": "떡볶이 어디로 드셧어요??",
          "original_form": "떡볶이 어디로 드셧어요??",
          "speaker_id": "1",
          "time": "20210907 09:04"
        },
        {
          "id": "MDRW2100041876.1.10",
          "form": "회사 앞에 있는 건데 프랜차이즈는 아닌것 같아요 ㅋㅋ",
          "original_form": "회사 앞에 있는 건데 프랜차이즈는 아닌것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100041876.1.11",
          "form": "근데 두끼떡볶이 같은 그런 거예요!",
          "original_form": "근데 두끼떡볶이 같은 그런 거예요!",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100041876.1.12",
          "form": "오오 튀김도 같이 드셧나요?",
          "original_form": "오오 튀김도 같이 드셧나요?",
          "speaker_id": "1",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100041876.1.13",
          "form": "네넿ㅎㅎ 갓 튀긴거여서 맛있더라규여",
          "original_form": "네넿ㅎㅎ 갓 튀긴거여서 맛있더라규여",
          "speaker_id": "2",
          "time": "20210907 09:55"
        },
        {
          "id": "MDRW2100041876.1.14",
          "form": "튀김 어떤거 좋아하세요?",
          "original_form": "튀김 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 12:24"
        },
        {
          "id": "MDRW2100041876.1.15",
          "form": "대체로 다 좋은데...단호박튀김이 요즘은 좋은 것 같아요",
          "original_form": "대체로 다 좋은데...단호박튀김이 요즘은 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100041876.1.16",
          "form": "분식 좋아하는 편이세요?",
          "original_form": "분식 좋아하는 편이세요?",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100041876.1.17",
          "form": "네네!! 분식집 너무 좋아요 ㅋㅋㅋ",
          "original_form": "네네!! 분식집 너무 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100041876.1.18",
          "form": "돈가스 쫄면 만둣국 ㅠㅜ",
          "original_form": "돈가스 쫄면 만둣국 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100041876.1.19",
          "form": "진짜 다 맛있죠ㅋㅋㅋ",
          "original_form": "진짜 다 맛있죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100041876.1.20",
          "form": "최애는 어떤 건가요?!",
          "original_form": "최애는 어떤 건가요?!",
          "speaker_id": "2",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100041876.1.21",
          "form": "저는 돈가스 먹어요!!",
          "original_form": "저는 돈가스 먹어요!!",
          "speaker_id": "1",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100041876.1.22",
          "form": "치즈 돈가스가 좋드라구요 ㅎㅎ",
          "original_form": "치즈 돈가스가 좋드라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100041876.1.23",
          "form": "와 치돈...갑자기 먹구싶네요 ㅋㅋㅋ",
          "original_form": "와 치돈...갑자기 먹구싶네요 ㅋㅋㅋ",
