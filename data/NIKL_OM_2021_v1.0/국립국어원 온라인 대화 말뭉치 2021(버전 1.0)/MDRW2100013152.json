      "utterance": [
        {
          "id": "MDRW2100013152.1.1",
          "form": "올해 후반 즈음에 드라마가 나온다는데",
          "original_form": "올해 후반 즈음에 드라마가 나온다는데",
          "speaker_id": "2",
          "time": "20210825 13:06"
        },
        {
          "id": "MDRW2100013152.1.2",
          "form": "잘 모르겠음",
          "original_form": "잘 모르겠음",
          "speaker_id": "2",
          "time": "20210825 13:06"
        },
        {
          "id": "MDRW2100013152.1.3",
          "form": "10월쯤나오려나",
          "original_form": "10월쯤나오려나",
          "speaker_id": "1",
          "time": "20210825 13:06"
        },
        {
          "id": "MDRW2100013152.1.4",
          "form": "남주는정해짐?",
          "original_form": "남주는정해짐?",
          "speaker_id": "1",
          "time": "20210825 13:06"
        },
        {
          "id": "MDRW2100013152.1.5",
          "form": "남주혁",
          "original_form": "남주혁",
          "speaker_id": "2",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100013152.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100013152.1.7",
          "form": "태리만연기하겠네",
          "original_form": "태리만연기하겠네",
          "speaker_id": "1",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100013152.1.8",
          "form": "앜ㅌㅋㅌㅌㅋㅋㅋㅋㅋㅋ 연기 못해??",
          "original_form": "앜ㅌㅋㅌㅌㅋㅋㅋㅋㅋㅋ 연기 못해??",
          "speaker_id": "2",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100013152.1.9",
          "form": "ㅇㅇㅇ남주혁드라마한번도안봄?",
          "original_form": "ㅇㅇㅇ남주혁드라마한번도안봄?",
          "speaker_id": "1",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100013152.1.10",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100013152.1.11",
          "form": "그렇게못하는애처음봤음",
          "original_form": "그렇게못하는애처음봤음",
          "speaker_id": "1",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100013152.1.12",
          "form": "송강보다더못하는거같음",
          "original_form": "송강보다더못하는거같음",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013152.1.13",
          "form": "아 진심??",
          "original_form": "아 진심??",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013152.1.14",
          "form": "그러기 쉽지 않은데",
          "original_form": "그러기 쉽지 않은데",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013152.1.15",
          "form": "표정부터가",
          "original_form": "표정부터가",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013152.1.16",
          "form": "송강보다면 이야",
          "original_form": "송강보다면 이야",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013152.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그렇게못하기쉽지않은데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그렇게못하기쉽지않은데",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013152.1.18",
          "form": "늘었을수도있음",
          "original_form": "늘었을수도있음",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013152.1.19",
          "form": "최근에는안보긴해서",
          "original_form": "최근에는안보긴해서",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100013152.1.20",
          "form": "와 큰일 났는데",
          "original_form": "와 큰일 났는데",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013152.1.21",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013152.1.22",
          "form": "본방말고다시보기로봐서",
          "original_form": "본방말고다시보기로봐서",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013152.1.23",
          "form": "태리연기부분만",
          "original_form": "태리연기부분만",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013152.1.24",
          "form": "돌려보는게날듯",
          "original_form": "돌려보는게날듯",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013152.1.25",
          "form": "..... 가능?",
          "original_form": "..... 가능?",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013152.1.26",
          "form": "더 붙어 있을 것 같은데",
          "original_form": "더 붙어 있을 것 같은데",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100013152.1.27",
          "form": "애정신",
          "original_form": "애정신",
          "speaker_id": "1",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013152.1.28",
          "form": "많이없기를",
          "original_form": "많이없기를",
          "speaker_id": "1",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100013152.1.29",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "1",
          "time": "20210825 13:10"
        }
      ]
