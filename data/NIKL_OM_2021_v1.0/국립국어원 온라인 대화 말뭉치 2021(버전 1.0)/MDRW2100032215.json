      "utterance": [
        {
          "id": "MDRW2100032215.1.1",
          "form": "아일랜드 진짜 좋아",
          "original_form": "아일랜드 진짜 좋아",
          "speaker_id": "2",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032215.1.2",
          "form": "풍경도 좋고",
          "original_form": "풍경도 좋고",
          "speaker_id": "2",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032215.1.3",
          "form": "뭣보다 유명 관광지가 아니라서",
          "original_form": "뭣보다 유명 관광지가 아니라서",
          "speaker_id": "2",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032215.1.4",
          "form": "물가도 싸고 인심도 좋아",
          "original_form": "물가도 싸고 인심도 좋아",
          "speaker_id": "2",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032215.1.5",
          "form": "따뜻한 시골 느낌",
          "original_form": "따뜻한 시골 느낌",
          "speaker_id": "2",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032215.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032215.1.7",
          "form": "올드퀘벡도 약간 그런 느낌일 거 같은데",
          "original_form": "올드퀘벡도 약간 그런 느낌일 거 같은데",
          "speaker_id": "2",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032215.1.8",
          "form": "아니야??",
          "original_form": "아니야??",
          "speaker_id": "2",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032215.1.9",
          "form": "물가....싸진않았지만...!",
          "original_form": "물가....싸진않았지만...!",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032215.1.10",
          "form": "시골같았나",
          "original_form": "시골같았나",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032215.1.11",
          "form": "근데 한적하긴했어",
          "original_form": "근데 한적하긴했어",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032215.1.12",
          "form": "엄청자그만해서 반나절만에 동네한바퀴돌았던기억...",
          "original_form": "엄청자그만해서 반나절만에 동네한바퀴돌았던기억...",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032215.1.13",
          "form": "혹시 솧씨가 갔던 곳 중에 젤 실망한 여행지는 어디야??",
          "original_form": "혹시 솧씨가 갔던 곳 중에 젤 실망한 여행지는 어디야??",
          "speaker_id": "2",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032215.1.14",
          "form": "흠흠",
          "original_form": "흠흠",
          "speaker_id": "1",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032215.1.15",
          "form": "나.. LA갔었엉",
          "original_form": "나.. LA갔었엉",
          "speaker_id": "1",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100032215.1.16",
          "form": "그냥 아무것도 없던기억....",
          "original_form": "그냥 아무것도 없던기억....",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032215.1.17",
          "form": "헐 진짜??",
          "original_form": "헐 진짜??",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032215.1.18",
          "form": "난 la도 좋았는데...",
          "original_form": "난 la도 좋았는데...",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032215.1.19",
          "form": "내가 초등학교4학년그때가서그랬는지",
          "original_form": "내가 초등학교4학년그때가서그랬는지",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032215.1.20",
          "form": "기억이안나는데",
          "original_form": "기억이안나는데",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032215.1.21",
          "form": "워너브라더스 스튜디오 투어 넘 재밌었어",
          "original_form": "워너브라더스 스튜디오 투어 넘 재밌었어",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032215.1.22",
          "form": "그냥진짜 너무 평화로웠어",
          "original_form": "그냥진짜 너무 평화로웠어",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032215.1.23",
          "form": "흐에이ㅔ엥 난 디즈니랜드를갔었지만",
          "original_form": "흐에이ㅔ엥 난 디즈니랜드를갔었지만",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032215.1.24",
          "form": "그땐 뭘몰랐기에...",
          "original_form": "그땐 뭘몰랐기에...",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032215.1.25",
          "form": "LA가 평화로워.....??",
          "original_form": "LA가 평화로워.....??",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032215.1.26",
          "form": "평화로웠던기억...",
          "original_form": "평화로웠던기억...",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032215.1.27",
          "form": "내가 갔던곳이평화로웠돈건가",
          "original_form": "내가 갔던곳이평화로웠돈건가",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032215.1.28",
          "form": "그때도 디즈니에 스타워즈 관련 있었나??",
          "original_form": "그때도 디즈니에 스타워즈 관련 있었나??",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032215.1.29",
          "form": "흐으으으음",
          "original_form": "흐으으으음",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032215.1.30",
          "form": "15년",
          "original_form": "15년",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032215.1.31",
          "form": "그쯤전이었는데",
          "original_form": "그쯤전이었는데",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032215.1.32",
          "form": "(벌써..",
          "original_form": "(벌써..",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100032215.1.33",
          "form": "옶었..지않을까..",
          "original_form": "옶었..지않을까..",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032215.1.34",
          "form": "거기서 인디아나존스 기구 탔던건 기억나",
          "original_form": "거기서 인디아나존스 기구 탔던건 기억나",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032215.1.35",
          "form": "짱 무셔웠어...",
          "original_form": "짱 무셔웠어...",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032215.1.36",
          "form": "하지만 지금 내 본진이 인디아나존스가 되어버린...",
          "original_form": "하지만 지금 내 본진이 인디아나존스가 되어버린...",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032215.1.37",
          "form": "나도 롤러코스터 진짜 못 타는데",
          "original_form": "나도 롤러코스터 진짜 못 타는데",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032215.1.38",
          "form": "헐 근데 홍콩 디즈니랜드가 가장 아무것도없었던거같아",
          "original_form": "헐 근데 홍콩 디즈니랜드가 가장 아무것도없었던거같아",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032215.1.39",
          "form": "그래도 디즈니랜드는 탈 만하던데",
          "original_form": "그래도 디즈니랜드는 탈 만하던데",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032215.1.40",
          "form": "흐아아악",
          "original_form": "흐아아악",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032215.1.41",
          "form": "홍콩은 가본 적 없다...",
          "original_form": "홍콩은 가본 적 없다...",
          "speaker_id": "2",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100032215.1.42",
          "form": "굳이안가도되는거같아...",
          "original_form": "굳이안가도되는거같아...",
          "speaker_id": "1",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100032215.1.43",
          "form": "아니물ㄹ론 가면좋다고하는데",
          "original_form": "아니물ㄹ론 가면좋다고하는데",
          "speaker_id": "1",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100032215.1.44",
          "form": "디즈니랜드는안가도될거같아",
          "original_form": "디즈니랜드는안가도될거같아",
          "speaker_id": "1",
          "time": "20210905 14:16"
        }
      ]
    }
