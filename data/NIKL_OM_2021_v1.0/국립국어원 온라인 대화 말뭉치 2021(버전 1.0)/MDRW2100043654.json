      "utterance": [
        {
          "id": "MDRW2100043654.1.1",
          "form": "화난건 이해하는데 넘 강압적",
          "original_form": "화난건 이해하는데 넘 강압적",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043654.1.2",
          "form": "헐 아니 진짜 넷플릭스 체인지데이즈 이거 진짜",
          "original_form": "헐 아니 진짜 넷플릭스 체인지데이즈 이거 진짜",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043654.1.3",
          "form": "진록이 너무하네요",
          "original_form": "진록이 너무하네요",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043654.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043654.1.5",
          "form": "그죠 무서웟어여저까지",
          "original_form": "그죠 무서웟어여저까지",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043654.1.6",
          "form": "아니진짜 왜저래요",
          "original_form": "아니진짜 왜저래요",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043654.1.7",
          "form": "112 신고요",
          "original_form": "112 신고요",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043654.1.8",
          "form": "민선 표정...",
          "original_form": "민선 표정...",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043654.1.9",
          "form": "근데진짜 너무 말투랑 성격이랑 별로에요",
          "original_form": "근데진짜 너무 말투랑 성격이랑 별로에요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043654.1.10",
          "form": "민선 불쌍..",
          "original_form": "민선 불쌍..",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043654.1.11",
          "form": "아니 진짜 이 커플은 왜 사귀는지 모르겠어요 그냥 민선 불쌍",
          "original_form": "아니 진짜 이 커플은 왜 사귀는지 모르겠어요 그냥 민선 불쌍",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043654.1.12",
          "form": "엥 벌써 짐싸네",
          "original_form": "엥 벌써 짐싸네",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043654.1.13",
          "form": "그죠 넘 빠름 진짜",
          "original_form": "그죠 넘 빠름 진짜",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043654.1.14",
          "form": "보여줄라나...?",
          "original_form": "보여줄라나...?",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043654.1.15",
          "form": "아직 30분 남았는데",
          "original_form": "아직 30분 남았는데",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043654.1.16",
          "form": "그죠 저거기꺼지만 보고 나왓어요",
          "original_form": "그죠 저거기꺼지만 보고 나왓어요",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043654.1.17",
          "form": "카카오가 맨날 질질 끄니까 ㅋㅋㅋ",
          "original_form": "카카오가 맨날 질질 끄니까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043654.1.18",
          "form": "안보여줄것같은 마음 ㅋㅋㅋㅋㅋ",
          "original_form": "안보여줄것같은 마음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043654.1.19",
          "form": "아 웃겨 ㅋㅋㅋㅋㅋㅋ엠씨들도 상호성미 미네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 웃겨 ㅋㅋㅋㅋㅋㅋ엠씨들도 상호성미 미네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043654.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ성호 정신차려라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ성호 정신차려라",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043654.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ아 진짜 다들 한마음으로 성호 상미 미니까 ㅋㅋㅋ웃겨요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 진짜 다들 한마음으로 성호 상미 미니까 ㅋㅋㅋ웃겨요",
          "speaker_id": "1",
