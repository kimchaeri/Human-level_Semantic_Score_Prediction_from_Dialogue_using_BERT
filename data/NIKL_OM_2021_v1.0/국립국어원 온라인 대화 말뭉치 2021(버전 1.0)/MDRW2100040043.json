      "utterance": [
        {
          "id": "MDRW2100040043.1.1",
          "form": "그거 하나만 100개고",
          "original_form": "그거 하나만 100개고",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100040043.1.2",
          "form": "나머지까지 합치면",
          "original_form": "나머지까지 합치면",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100040043.1.3",
          "form": "180개는 되는데",
          "original_form": "180개는 되는데",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040043.1.4",
          "form": "나 왜 훈련해야돼....보내줘....",
          "original_form": "나 왜 훈련해야돼....보내줘....",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040043.1.5",
          "form": "ㅋㅋㅋㅋㅋ 그니까 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 그니까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040043.1.6",
          "form": "이거 훈련 혹시 1단계가 끝이 아니야?",
          "original_form": "이거 훈련 혹시 1단계가 끝이 아니야?",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040043.1.7",
          "form": "어차피 전력으로 싸우지도 않잖아...",
          "original_form": "어차피 전력으로 싸우지도 않잖아...",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040043.1.8",
          "form": "ㄴㄴ 일반 훈련이랑 최종훈련중에 하나만 끝내면 돼",
          "original_form": "ㄴㄴ 일반 훈련이랑 최종훈련중에 하나만 끝내면 돼",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040043.1.9",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040043.1.10",
          "form": "최종훈련은 뭐야?",
          "original_form": "최종훈련은 뭐야?",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040043.1.11",
          "form": "두번짼 걍 몹 전투라 쉬움",
          "original_form": "두번짼 걍 몹 전투라 쉬움",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040043.1.12",
          "form": "최종은 더 오랜시간동안",
          "original_form": "최종은 더 오랜시간동안",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040043.1.13",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040043.1.14",
          "form": "귀찮아 죽겟네",
          "original_form": "귀찮아 죽겟네",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040043.1.15",
          "form": "3번만 피격되면서 버텨야함",
          "original_form": "3번만 피격되면서 버텨야함",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040043.1.16",
          "form": "일단 더 해봄",
          "original_form": "일단 더 해봄",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040043.1.17",
          "form": "둘중 뭘 깨든 상관업ㄱ어",
          "original_form": "둘중 뭘 깨든 상관업ㄱ어",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040043.1.18",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "2",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100040043.1.19",
          "form": "한번 더 실패했고",
          "original_form": "한번 더 실패했고",
          "speaker_id": "2",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100040043.1.20",
          "form": "케이아 스킬 1타는",
          "original_form": "케이아 스킬 1타는",
          "speaker_id": "2",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100040043.1.21",
          "form": "택도 없어",
          "original_form": "택도 없어",
          "speaker_id": "2",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100040043.1.22",
          "form": "베넷 궁을 채워가봐야겠어",
          "original_form": "베넷 궁을 채워가봐야겠어",
          "speaker_id": "2",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100040043.1.23",
          "form": "ㅇㅇ... 케이아는",
          "original_form": "ㅇㅇ... 케이아는",
          "speaker_id": "1",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100040043.1.24",
          "form": "원소 묻히는 용이지",
          "original_form": "원소 묻히는 용이지",
          "speaker_id": "1",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100040043.1.25",
          "form": "딜로 밀어붙이는 타입은 아닌듯",
          "original_form": "딜로 밀어붙이는 타입은 아닌듯",
          "speaker_id": "1",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100040043.1.26",
          "form": "아니 난 분명",
          "original_form": "아니 난 분명",
          "speaker_id": "2",
          "time": "20210904 01:33"
        },
        {
          "id": "MDRW2100040043.1.27",
          "form": "10초 전에 깬것같은데",
          "original_form": "10초 전에 깬것같은데",
          "speaker_id": "2",
          "time": "20210904 01:33"
        },
        {
          "id": "MDRW2100040043.1.28",
          "form": "왜 바로 도전실패 뜨지",
          "original_form": "왜 바로 도전실패 뜨지",
          "speaker_id": "2",
          "time": "20210904 01:33"
        },
        {
          "id": "MDRW2100040043.1.29",
          "form": "아니 얘네의 빈약한 스킬로는",
          "original_form": "아니 얘네의 빈약한 스킬로는",
          "speaker_id": "2",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100040043.1.30",
          "form": "절대 못부순다고........",
          "original_form": "절대 못부순다고........",
          "speaker_id": "2",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100040043.1.31",
          "form": "ㅇㅇ 이거",
          "original_form": "ㅇㅇ 이거",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100040043.1.32",
          "form": "뭔 기준인지 모르겠는데",
          "original_form": "뭔 기준인지 모르겠는데",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100040043.1.33",
          "form": "부순거같은데 실패뜨고",
          "original_form": "부순거같은데 실패뜨고",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100040043.1.34",
          "form": "혹시 내가 불에 탄것도",
          "original_form": "혹시 내가 불에 탄것도",
          "speaker_id": "2",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100040043.1.35",
          "form": "달성 못한거같은데 성공 뜸",
          "original_form": "달성 못한거같은데 성공 뜸",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100040043.1.36",
          "form": "피격판정인가??",
          "original_form": "피격판정인가??",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100040043.1.37",
          "form": "ㄴㄴ 그건 아닐걸",
          "original_form": "ㄴㄴ 그건 아닐걸",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100040043.1.38",
          "form": "피격 여유는 많이 남앗엇지?",
          "original_form": "피격 여유는 많이 남앗엇지?",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100040043.1.39",
          "form": "근데 10초 되기도 전에",
          "original_form": "근데 10초 되기도 전에",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100040043.1.40",
          "form": "실패라고 떴거든",
          "original_form": "실패라고 떴거든",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100040043.1.41",
          "form": "ㅇㅇ 나도 그랫엇어",
          "original_form": "ㅇㅇ 나도 그랫엇어",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100040043.1.42",
          "form": "아니 이유가 뭐야",
          "original_form": "아니 이유가 뭐야",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100040043.1.43",
          "form": "글케 한 세번정도 실패하고 막판에 성공햇는데",
          "original_form": "글케 한 세번정도 실패하고 막판에 성공햇는데",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100040043.1.44",
          "form": "이유를 모르겟ㄱ음",
          "original_form": "이유를 모르겟ㄱ음",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100040043.1.45",
          "form": "피격 한번만 당해서 그런가 싶기도 하고",
          "original_form": "피격 한번만 당해서 그런가 싶기도 하고",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100040043.1.46",
          "form": "막판엔 그 토템같은거 뜨기 전까지 한번만 맞앗거든",
          "original_form": "막판엔 그 토템같은거 뜨기 전까지 한번만 맞앗거든",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100040043.1.47",
          "form": "아니 방금",
          "original_form": "아니 방금",
          "speaker_id": "2",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100040043.1.48",
          "form": "피격 한번도 안됗었는데",
          "original_form": "피격 한번도 안됗었는데",
          "speaker_id": "2",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100040043.1.49",
          "form": "나 이어폰 들고와서",
          "original_form": "나 이어폰 들고와서",
          "speaker_id": "2",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100040043.1.50",
          "form": "사플까지 하고있다고.....",
          "original_form": "사플까지 하고있다고.....",
          "speaker_id": "2",
          "time": "20210904 01:36"
        }
      ]
