      "utterance": [
        {
          "id": "MDRW2100020305.1.1",
          "form": "암튼 설거지 나도 돌아가는거 좋다",
          "original_form": "암튼 설거지 나도 돌아가는거 좋다",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020305.1.2",
          "form": "한사람만하면",
          "original_form": "한사람만하면",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020305.1.3",
          "form": "조금불편한거같아 설거지는",
          "original_form": "조금불편한거같아 설거지는",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020305.1.4",
          "form": "글고 머무 만으면",
          "original_form": "글고 머무 만으면",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020305.1.5",
          "form": "그냥 같이해",
          "original_form": "그냥 같이해",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020305.1.6",
          "form": "설거지 또",
          "original_form": "설거지 또",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020305.1.7",
          "form": "자기가 안하면",
          "original_form": "자기가 안하면",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020305.1.8",
          "form": "개막쓰기되기쉬워서",
          "original_form": "개막쓰기되기쉬워서",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020305.1.9",
          "form": "ㄱㄹ같이해",
          "original_form": "ㄱㄹ같이해",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020305.1.10",
          "form": "나 설거지난 잘해 글고",
          "original_form": "나 설거지난 잘해 글고",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.11",
          "form": "꼭 그릇 물에담궈둬야함",
          "original_form": "꼭 그릇 물에담궈둬야함",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.12",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.13",
          "form": "자리녒으면",
          "original_form": "자리녒으면",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.14",
          "form": "미리 물로",
          "original_form": "미리 물로",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.15",
          "form": "왜 ㅁ1ㅈ가 욕이여",
          "original_form": "왜 ㅁ1ㅈ가 욕이여",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.16",
          "form": "음식물 좀",
          "original_form": "음식물 좀",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.17",
          "form": "씩거낸채로",
          "original_form": "씩거낸채로",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.18",
          "form": "두던가",
          "original_form": "두던가",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.19",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.21",
          "form": "또모없나",
          "original_form": "또모없나",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020305.1.22",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020305.1.23",
          "form": "나그렇게 예민하지 않다 ..",
          "original_form": "나그렇게 예민하지 않다 ..",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020305.1.24",
          "form": "긱사살때도",
          "original_form": "긱사살때도",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020305.1.25",
          "form": "좀 잘살아서",
          "original_form": "좀 잘살아서",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020305.1.26",
          "form": "사실 지금생갓하면 다른사람이 나때메 빡틴게 잇지않앗을까",
          "original_form": "사실 지금생갓하면 다른사람이 나때메 빡틴게 잇지않앗을까",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020305.1.27",
          "form": "나는....",
          "original_form": "나는....",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.28",
          "form": "나 개인공간만",
          "original_form": "나 개인공간만",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.29",
          "form": "존중해주면 괜찮",
          "original_form": "존중해주면 괜찮",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.30",
          "form": "그거는",
          "original_form": "그거는",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.31",
          "form": "너무 기본이다",
          "original_form": "너무 기본이다",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.32",
          "form": "근데 우리집은",
          "original_form": "근데 우리집은",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.33",
          "form": "그 기본이 안돼",
          "original_form": "그 기본이 안돼",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.34",
          "form": "책상 마음대로 닦고 물건 위치 바꾸기",
          "original_form": "책상 마음대로 닦고 물건 위치 바꾸기",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.35",
          "form": "그것도 그렇겟다..",
          "original_form": "그것도 그렇겟다..",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.36",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020305.1.37",
          "form": "술마시고 내 잠자레에서 자고잇기",
          "original_form": "술마시고 내 잠자레에서 자고잇기",
          "speaker_id": "2",
