      "utterance": [
        {
          "id": "MDRW2100046218.1.1",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046218.1.2",
          "form": "순한애는 어떤가요?",
          "original_form": "순한애는 어떤가요?",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046218.1.3",
          "form": "순한애는 핸들링 안해도 손타요",
          "original_form": "순한애는 핸들링 안해도 손타요",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046218.1.4",
          "form": "굳이 훈련시킬 필요가 없는거군요",
          "original_form": "굳이 훈련시킬 필요가 없는거군요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046218.1.5",
          "form": "네 그래서편하죠",
          "original_form": "네 그래서편하죠",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046218.1.6",
          "form": "순한애 키우셨나봐요",
          "original_form": "순한애 키우셨나봐요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046218.1.7",
          "form": "전에애가 순했어요",
          "original_form": "전에애가 순했어요",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046218.1.8",
          "form": "요와 좋으셨겠어요",
          "original_form": "요와 좋으셨겠어요",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046218.1.9",
          "form": "제손에서 잠도자고 그랬는데",
          "original_form": "제손에서 잠도자고 그랬는데",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046218.1.10",
          "form": "너무귀엽네요",
          "original_form": "너무귀엽네요",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046218.1.11",
          "form": "지금애는 좀 겁이많아요",
          "original_form": "지금애는 좀 겁이많아요",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046218.1.12",
          "form": "겁많으면 어떤가요?",
          "original_form": "겁많으면 어떤가요?",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046218.1.13",
          "form": "손만 다가와도 깜짝 놀래더라구요",
          "original_form": "손만 다가와도 깜짝 놀래더라구요",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046218.1.14",
          "form": "아이고 안타깝네요",
          "original_form": "아이고 안타깝네요",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046218.1.15",
          "form": "네 예민해요",
          "original_form": "네 예민해요",
          "speaker_id": "2",
          "time": "20210908 11:21"
        }
