      "utterance": [
        {
          "id": "MDRW2100035279.1.1",
          "form": "안녕하세요 오랜만이에요ㅋㅋ",
          "original_form": "안녕하세요 오랜만이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100035279.1.2",
          "form": "식사는 하셨나요?",
          "original_form": "식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100035279.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100035279.1.4",
          "form": "네 김밥 먹었어요 name1님은요?",
          "original_form": "네 김밥 먹었어요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100035279.1.5",
          "form": "와 저도 김밥이요",
          "original_form": "와 저도 김밥이요",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100035279.1.6",
          "form": "ㅈ ㅈ 뽕이에요",
          "original_form": "ㅈ ㅈ 뽕이에요",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100035279.1.7",
          "form": "사드셨나요?",
          "original_form": "사드셨나요?",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100035279.1.8",
          "form": "ㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋ네",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100035279.1.9",
          "form": "name1님은 어떤 김밥 드셨어요?",
          "original_form": "&name1&님은 어떤 김밥 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100035279.1.10",
          "form": "전 집에서 단무지랑 햄같은걸 ㅋㅋ 그냥 조미김에 싸먹어요",
          "original_form": "전 집에서 단무지랑 햄같은걸 ㅋㅋ 그냥 조미김에 싸먹어요",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100035279.1.11",
          "form": "꼬마김밥처럼 플레이팅해서 먹었어옷",
          "original_form": "꼬마김밥처럼 플레이팅해서 먹었어옷",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100035279.1.12",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100035279.1.13",
          "form": "어떤 김밥인가요?",
          "original_form": "어떤 김밥인가요?",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100035279.1.14",
          "form": "오 맛있겠네요 전 참치김밥이었어요",
          "original_form": "오 맛있겠네요 전 참치김밥이었어요",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100035279.1.15",
          "form": "전참김도 좋아해요 ㅋㅋ 참치랑 마요네즈의 조합이 좋아요",
          "original_form": "전참김도 좋아해요 ㅋㅋ 참치랑 마요네즈의 조합이 좋아요",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100035279.1.16",
          "form": "그쵸 그래서 보통 참치김밥만 먹어요ㅋㅋ",
          "original_form": "그쵸 그래서 보통 참치김밥만 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100035279.1.17",
          "form": "저녁은 드셨어요?",
          "original_form": "저녁은 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100035279.1.18",
          "form": "아니오 ㅋㅋ 그런데 낮에 싼 꼬마김밥이 남아서 저녁도 그걸 먹을거에요",
          "original_form": "아니오 ㅋㅋ 그런데 낮에 싼 꼬마김밥이 남아서 저녁도 그걸 먹을거에요",
          "speaker_id": "1",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100035279.1.19",
          "form": "name2님은 뭐러 드시나요?",
          "original_form": "&name2&님은 뭐러 드시나요?",
          "speaker_id": "1",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100035279.1.20",
          "form": "뭐를",
          "original_form": "뭐를",
          "speaker_id": "1",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100035279.1.21",
          "form": "전 방금 밥이랑 집에 있는 반찬 먹었는데 아직 덜 배불러요",
          "original_form": "전 방금 밥이랑 집에 있는 반찬 먹었는데 아직 덜 배불러요",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100035279.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100035279.1.23",
          "form": "이럴땐 과일 후식이죠 ㅋㅋㅋ",
          "original_form": "이럴땐 과일 후식이죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100035279.1.24",
          "form": "아니 라떼류가 전 좋더라구요",
          "original_form": "아니 라떼류가 전 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100035279.1.25",
          "form": "ㅋㅋㅋㅋㅋ갑자기 과일이 땡기네요",
          "original_form": "ㅋㅋㅋㅋㅋ갑자기 과일이 땡기네요",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100035279.1.26",
          "form": "어떤 라떼 좋아하세요?",
          "original_form": "어떤 라떼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100035279.1.27",
          "form": "전 기본라떼랑 카푸치노를 좋아하는데요",
          "original_form": "전 기본라떼랑 카푸치노를 좋아하는데요",
          "speaker_id": "1",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100035279.1.28",
          "form": "계절 한정인 슈크림 라떼도 가끔은 잘먹어요 아 코코넛밀크라뗴도 한두번은 먹을만해요",
          "original_form": "계절 한정인 슈크림 라떼도 가끔은 잘먹어요 아 코코넛밀크라뗴도 한두번은 먹을만해요",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100035279.1.29",
          "form": "슈크림라떼 맛있겠네요",
          "original_form": "슈크림라떼 맛있겠네요",
