      "utterance": [
        {
          "id": "MDRW2100040796.1.1",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "2",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100040796.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100040796.1.3",
          "form": "전 보통 아침은 안먹긴해요ㅜ",
          "original_form": "전 보통 아침은 안먹긴해요ㅜ",
          "speaker_id": "2",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040796.1.4",
          "form": "아침식사가 건강에 좋다고 하는데",
          "original_form": "아침식사가 건강에 좋다고 하는데",
          "speaker_id": "1",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040796.1.5",
          "form": "바쁘면 챙겨먹기 쉽진 않죠ㅜ",
          "original_form": "바쁘면 챙겨먹기 쉽진 않죠ㅜ",
          "speaker_id": "1",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040796.1.6",
          "form": "안먹다버릇하니깐",
          "original_form": "안먹다버릇하니깐",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100040796.1.7",
          "form": "명절때 가족들모여서 먹을때도 안 먹히긴해여ㅠ",
          "original_form": "명절때 가족들모여서 먹을때도 안 먹히긴해여ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100040796.1.8",
          "form": "아 아침식사 하는 것도 습관이라",
          "original_form": "아 아침식사 하는 것도 습관이라",
          "speaker_id": "1",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040796.1.9",
          "form": "안먹다보면 잘 안먹힐듯",
          "original_form": "안먹다보면 잘 안먹힐듯",
          "speaker_id": "1",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040796.1.10",
          "form": "운동은 좋아하시나요",
          "original_form": "운동은 좋아하시나요",
          "speaker_id": "1",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040796.1.11",
          "form": "아니요ㅠㅠ 몸에좋은건 다 안하네요ㅠ",
          "original_form": "아니요ㅠㅠ 몸에좋은건 다 안하네요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040796.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100040796.1.13",
          "form": "저는 코로나 이후에 움직임이 적어지니",
          "original_form": "저는 코로나 이후에 움직임이 적어지니",
          "speaker_id": "1",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100040796.1.14",
          "form": "건강이 안좋아지는 게",
          "original_form": "건강이 안좋아지는 게",
          "speaker_id": "1",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100040796.1.15",
          "form": "느껴지더라고요",
          "original_form": "느껴지더라고요",
          "speaker_id": "1",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100040796.1.16",
          "form": "맞아여ㅠ 반핑게로 살도 많이쩠네요ㅠㅠ",
          "original_form": "맞아여ㅠ 반핑게로 살도 많이쩠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100040796.1.17",
          "form": "네네 그래서 필라테스 하기 시작했어요",
          "original_form": "네네 그래서 필라테스 하기 시작했어요",
          "speaker_id": "1",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100040796.1.18",
          "form": "일주일에 두어번 스트레칭이라도 하려고",
          "original_form": "일주일에 두어번 스트레칭이라도 하려고",
          "speaker_id": "1",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100040796.1.19",
          "form": "필라테스 안해봤눈데",
          "original_form": "필라테스 안해봤눈데",
          "speaker_id": "2",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100040796.1.20",
          "form": "기구 필라테스 해보고싶어요!",
          "original_form": "기구 필라테스 해보고싶어요!",
          "speaker_id": "2",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100040796.1.21",
          "form": "네 저 기구 필라테스 하는데",
          "original_form": "네 저 기구 필라테스 하는데",
          "speaker_id": "1",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100040796.1.22",
          "form": "하다보니 몸 풀리더라고요",
          "original_form": "하다보니 몸 풀리더라고요",
          "speaker_id": "1",
          "time": "20210905 07:55"
        },
        {
          "id": "MDRW2100040796.1.23",
          "form": "좋네요ㅠㅠ~",
          "original_form": "좋네요ㅠㅠ~",
          "speaker_id": "2",
          "time": "20210905 07:57"
        },
        {
          "id": "MDRW2100040796.1.24",
          "form": "수업가면 마스크쓰고하나요? 요샌어떤가싶네요",
          "original_form": "수업가면 마스크쓰고하나요? 요샌어떤가싶네요",
          "speaker_id": "2",
          "time": "20210905 07:57"
        },
        {
          "id": "MDRW2100040796.1.25",
          "form": "네 마스크 쓰고",
          "original_form": "네 마스크 쓰고",
          "speaker_id": "1",
          "time": "20210905 07:57"
        }
      ]
    }
  ]
