      "utterance": [
        {
          "id": "MDRW2100008259.1.1",
          "form": "점심드셨어요?",
          "original_form": "점심드셨어요?",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008259.1.2",
          "form": "네. 오늘은 밥해서 먹었어요^^",
          "original_form": "네. 오늘은 밥해서 먹었어요^^",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008259.1.3",
          "form": "오 메뉴는요?",
          "original_form": "오 메뉴는요?",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008259.1.4",
          "form": "계란찜하고 생선굽고요~~",
          "original_form": "계란찜하고 생선굽고요~~",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008259.1.5",
          "form": "요리하셨군요ㅎ 생선맛나겠어요",
          "original_form": "요리하셨군요ㅎ 생선맛나겠어요",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008259.1.6",
          "form": "고등어구우신건가요?",
          "original_form": "고등어구우신건가요?",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008259.1.7",
          "form": "네.  간만에 먹었더니 좋네요",
          "original_form": "네.  간만에 먹었더니 좋네요",
          "speaker_id": "2",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008259.1.8",
          "form": "네 고등어요",
          "original_form": "네 고등어요",
          "speaker_id": "2",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008259.1.9",
          "form": "고등어 맛있죠ㅎ",
          "original_form": "고등어 맛있죠ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008259.1.10",
          "form": "묵은지 고등어 조림먹고싶네요",
          "original_form": "묵은지 고등어 조림먹고싶네요",
          "speaker_id": "1",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008259.1.11",
          "form": "네 먹음 체한거같이 안좋은데 그걸 이겨내고 먹어요",
          "original_form": "네 먹음 체한거같이 안좋은데 그걸 이겨내고 먹어요",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008259.1.12",
          "form": "와~~저두요. 전 무넣고 푹~~~익혀서",
          "original_form": "와~~저두요. 전 무넣고 푹~~~익혀서",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008259.1.13",
          "form": "체하신거같음 매실원액 물타서드셔요",
          "original_form": "체하신거같음 매실원액 물타서드셔요",
          "speaker_id": "1",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008259.1.14",
          "form": "고등어만 먹음 그렇더라구요. 입에서만좋아요",
          "original_form": "고등어만 먹음 그렇더라구요. 입에서만좋아요",
          "speaker_id": "2",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008259.1.15",
          "form": "아직 점심전인데 급 배고프네요ㅎㅎ",
          "original_form": "아직 점심전인데 급 배고프네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008259.1.16",
          "form": "아직까지 안드셨어요?",
          "original_form": "아직까지 안드셨어요?",
          "speaker_id": "2",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008259.1.17",
          "form": "얼렁드셔야겠네요",
          "original_form": "얼렁드셔야겠네요",
          "speaker_id": "2",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008259.1.18",
          "form": "제육 어제 못먹고 이제해동다된거같아서",
          "original_form": "제육 어제 못먹고 이제해동다된거같아서",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008259.1.19",
          "form": "이따가 먹게요",
          "original_form": "이따가 먹게요",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008259.1.20",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100008259.1.21",
          "form": "상추를 사왔어야했는데 좀 아쉽네요",
          "original_form": "상추를 사왔어야했는데 좀 아쉽네요",
