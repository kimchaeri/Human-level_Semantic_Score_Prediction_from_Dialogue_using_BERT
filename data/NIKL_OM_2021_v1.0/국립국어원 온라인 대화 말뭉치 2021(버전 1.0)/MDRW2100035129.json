      "utterance": [
        {
          "id": "MDRW2100035129.1.1",
          "form": "안녕하세용 아침은 드셨어용?",
          "original_form": "안녕하세용 아침은 드셨어용?",
          "speaker_id": "1",
          "time": "20210829 06:09"
        },
        {
          "id": "MDRW2100035129.1.2",
          "form": "안녕하세요 너무 일러서 아직이에요",
          "original_form": "안녕하세요 너무 일러서 아직이에요",
          "speaker_id": "2",
          "time": "20210829 06:32"
        },
        {
          "id": "MDRW2100035129.1.3",
          "form": "저도 아직이에요ㅋㅋㅋ",
          "original_form": "저도 아직이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:33"
        },
        {
          "id": "MDRW2100035129.1.4",
          "form": "아침에 카레 먹으려고 준비하고있어요",
          "original_form": "아침에 카레 먹으려고 준비하고있어요",
          "speaker_id": "1",
          "time": "20210829 06:33"
        },
        {
          "id": "MDRW2100035129.1.5",
          "form": "부지런하시네요",
          "original_form": "부지런하시네요",
          "speaker_id": "2",
          "time": "20210829 06:34"
        },
        {
          "id": "MDRW2100035129.1.6",
          "form": "카레에  주로 어떤 재료 넣으세요?",
          "original_form": "카레에  주로 어떤 재료 넣으세요?",
          "speaker_id": "2",
          "time": "20210829 06:34"
        },
        {
          "id": "MDRW2100035129.1.7",
          "form": "브로콜리랑 감자랑 돼지고기도 넣어용ㅎㅎ",
          "original_form": "브로콜리랑 감자랑 돼지고기도 넣어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 06:35"
        },
        {
          "id": "MDRW2100035129.1.8",
          "form": "당근은 안좋아해서 뺀답니당",
          "original_form": "당근은 안좋아해서 뺀답니당",
          "speaker_id": "1",
          "time": "20210829 06:35"
        },
        {
          "id": "MDRW2100035129.1.9",
          "form": "양파도 조금 넣어용!",
          "original_form": "양파도 조금 넣어용!",
          "speaker_id": "1",
          "time": "20210829 06:35"
        },
        {
          "id": "MDRW2100035129.1.10",
          "form": "우와 영양가득",
          "original_form": "우와 영양가득",
          "speaker_id": "2",
          "time": "20210829 06:35"
        },
        {
          "id": "MDRW2100035129.1.11",
          "form": "요리솜씨 좋으신듯해요",
          "original_form": "요리솜씨 좋으신듯해요",
          "speaker_id": "2",
          "time": "20210829 06:35"
        },
        {
          "id": "MDRW2100035129.1.12",
          "form": "평소에도 카레 많이 드세요?",
          "original_form": "평소에도 카레 많이 드세요?",
          "speaker_id": "2",
          "time": "20210829 06:36"
        },
        {
          "id": "MDRW2100035129.1.13",
          "form": "앗 카레는 분말이 다해줘서용....ㅋㅋㅋㅋ",
          "original_form": "앗 카레는 분말이 다해줘서용....ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:36"
        },
        {
          "id": "MDRW2100035129.1.14",
          "form": "넹넹 카레는 가리지않고 다 좋아해요",
          "original_form": "넹넹 카레는 가리지않고 다 좋아해요",
          "speaker_id": "1",
          "time": "20210829 06:36"
        },
        {
          "id": "MDRW2100035129.1.15",
          "form": "마켓컬리에서 난 사서 찍어드셔보세요!",
          "original_form": "마켓컬리에서 난 사서 찍어드셔보세요!",
          "speaker_id": "1",
          "time": "20210829 06:36"
        },
        {
          "id": "MDRW2100035129.1.16",
          "form": "맞아요 밥보다",
          "original_form": "맞아요 밥보다",
          "speaker_id": "2",
          "time": "20210829 06:37"
        },
        {
          "id": "MDRW2100035129.1.17",
          "form": "난이랑 먹을때 더 맛난거 같아여",
          "original_form": "난이랑 먹을때 더 맛난거 같아여",
          "speaker_id": "2",
          "time": "20210829 06:38"
        },
        {
          "id": "MDRW2100035129.1.18",
          "form": "분말 어디꺼 좋아하시나용?",
          "original_form": "분말 어디꺼 좋아하시나용?",
          "speaker_id": "2",
          "time": "20210829 06:38"
        },
        {
          "id": "MDRW2100035129.1.19",
          "form": "오 뭘 좀 아시는군요!ㅋㅋㅋ",
          "original_form": "오 뭘 좀 아시는군요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:38"
        },
        {
          "id": "MDRW2100035129.1.20",
          "form": "카레여왕 주로 먹고",
          "original_form": "카레여왕 주로 먹고",
          "speaker_id": "1",
          "time": "20210829 06:38"
        },
        {
          "id": "MDRW2100035129.1.21",
          "form": "바몬드도 잘 먹어용",
          "original_form": "바몬드도 잘 먹어용",
          "speaker_id": "1",
          "time": "20210829 06:38"
        },
        {
          "id": "MDRW2100035129.1.22",
          "form": "이번에 새로나온 티아시아는 아직 못먹어봤어용",
          "original_form": "이번에 새로나온 티아시아는 아직 못먹어봤어용",
          "speaker_id": "1",
          "time": "20210829 06:39"
        },
        {
          "id": "MDRW2100035129.1.23",
          "form": "카레여왕 저도 좋아해요ㅋㅋㅋ",
          "original_form": "카레여왕 저도 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:39"
        },
        {
          "id": "MDRW2100035129.1.24",
          "form": "티아시아 전지현이 선전하는거죠?",
          "original_form": "티아시아 전지현이 선전하는거죠?",
          "speaker_id": "2",
          "time": "20210829 06:39"
        },
        {
          "id": "MDRW2100035129.1.25",
          "form": "네네! 근데 이거",
          "original_form": "네네! 근데 이거",
          "speaker_id": "1",
          "time": "20210829 06:39"
        },
        {
          "id": "MDRW2100035129.1.26",
          "form": "분말형이 있고 그냥 렌지에 돌려먹는 완제품이 있어서",
          "original_form": "분말형이 있고 그냥 렌지에 돌려먹는 완제품이 있어서",
          "speaker_id": "1",
          "time": "20210829 06:39"
        },
        {
          "id": "MDRW2100035129.1.27",
          "form": "저도 아직 못 먹어밨어요 궁금해하고만 있어요",
          "original_form": "저도 아직 못 먹어밨어요 궁금해하고만 있어요",
          "speaker_id": "2",
          "time": "20210829 06:40"
        },
        {
          "id": "MDRW2100035129.1.28",
          "form": "잘보고 사셔야해용",
          "original_form": "잘보고 사셔야해용",
          "speaker_id": "1",
          "time": "20210829 06:40"
        }
      ]
    }
  ]
