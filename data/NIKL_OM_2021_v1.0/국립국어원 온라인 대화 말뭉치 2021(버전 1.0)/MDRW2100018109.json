      "utterance": [
        {
          "id": "MDRW2100018109.1.1",
          "form": "예전엔 게임 많이했었는데",
          "original_form": "예전엔 게임 많이했었는데",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018109.1.2",
          "form": "전 게임 아는 거라곤",
          "original_form": "전 게임 아는 거라곤",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018109.1.3",
          "form": "크레이지 아케이드, 카트라이더, 테일즈런너.. 이런 예전 게임 뿐이라.. ㅎㅎ",
          "original_form": "크레이지 아케이드, 카트라이더, 테일즈런너.. 이런 예전 게임 뿐이라.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018109.1.4",
          "form": "ㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋ아",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018109.1.5",
          "form": "저희 같은세대일지도 모른다는생각이 드네요",
          "original_form": "저희 같은세대일지도 모른다는생각이 드네요",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018109.1.6",
          "form": "카트라이더 ㅋㅋㅋ",
          "original_form": "카트라이더 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018109.1.7",
          "form": "크아 ㅋㅋㅋㅋ",
          "original_form": "크아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018109.1.8",
          "form": "저는 테일즈위버라는 게임도했었어요",
          "original_form": "저는 테일즈위버라는 게임도했었어요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018109.1.9",
          "form": "퀴퀴 아시나요",
          "original_form": "퀴퀴 아시나요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018109.1.10",
          "form": "퀴즈퀴즈 큐플레이",
          "original_form": "퀴즈퀴즈 큐플레이",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018109.1.11",
          "form": "알투비트....ㅋ",
          "original_form": "알투비트....ㅋ",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018109.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ  퀴퀴는 잘 모르겠고.. 테일즈 위버는 저도 했아요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ  퀴퀴는 잘 모르겠고.. 테일즈 위버는 저도 했아요!",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018109.1.13",
          "form": "헐 알투비트",
          "original_form": "헐 알투비트",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018109.1.14",
          "form": "완전 추얻이에요",
          "original_form": "완전 추얻이에요",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018109.1.15",
          "form": "그쳐그쵸 ㅋㅋㅋㅋㅋ",
          "original_form": "그쳐그쵸 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018109.1.16",
          "form": "전 그리고 플레시게임 이런거 많이 햏아요",
          "original_form": "전 그리고 플레시게임 이런거 많이 햏아요",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018109.1.17",
          "form": "테일즈위버는 길드장이였는데 ㅋㅋㅋㅋㅋ",
          "original_form": "테일즈위버는 길드장이였는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018109.1.18",
          "form": "슈게임 이런거.. ㅋㅋㅋㅋㅋ",
          "original_form": "슈게임 이런거.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018109.1.19",
          "form": "그때 대학동기가",
          "original_form": "그때 대학동기가",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018109.1.20",
          "form": "헐ㅋㅋㅋㅋㅋ 열심히 하셨네요!",
          "original_form": "헐ㅋㅋㅋㅋㅋ 열심히 하셨네요!",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018109.1.21",
          "form": "같은서버에서 게임하고있던얘라",
          "original_form": "같은서버에서 게임하고있던얘라",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018109.1.22",
          "form": "민망했어요 ㅋㅋ",
          "original_form": "민망했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018109.1.23",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 그쵸.. 아는 사람잏으면 괜히.. ㅎㅎ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 그쵸.. 아는 사람잏으면 괜히.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018109.1.24",
          "form": "생각해보니 게임 엄청했었네요...ㅋ ㅋㅋㅋ",
          "original_form": "생각해보니 게임 엄청했었네요...ㅋ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018109.1.25",
          "form": "하라는공부는안하고",
          "original_form": "하라는공부는안하고",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018109.1.26",
          "form": "슈게임은 첨들어봐요 !@",
          "original_form": "슈게임은 첨들어봐요 !@",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018109.1.27",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018109.1.28",
          "form": "검색해보니 알겠어요",
          "original_form": "검색해보니 알겠어요",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018109.1.29",
          "form": "어렸을때 많이하던게임!!!",
