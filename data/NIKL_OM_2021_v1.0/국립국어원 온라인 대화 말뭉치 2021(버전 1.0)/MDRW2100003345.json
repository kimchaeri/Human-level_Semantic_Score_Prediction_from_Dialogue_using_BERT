      "utterance": [
        {
          "id": "MDRW2100003345.1.1",
          "form": "혹시 뭐 사고싶은거 있으세요?",
          "original_form": "혹시 뭐 사고싶은거 있으세요?",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003345.1.2",
          "form": "요새 무소유네요 ㅋㅋ",
          "original_form": "요새 무소유네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:00"
        },
        {
          "id": "MDRW2100003345.1.3",
          "form": "근데 다시 장난감 모으고 싶어요",
          "original_form": "근데 다시 장난감 모으고 싶어요",
          "speaker_id": "2",
          "time": "20210820 22:00"
        },
        {
          "id": "MDRW2100003345.1.4",
          "form": "어떤 장난감이요?",
          "original_form": "어떤 장난감이요?",
          "speaker_id": "1",
          "time": "20210820 22:00"
        },
        {
          "id": "MDRW2100003345.1.5",
          "form": "식완이요 미니어처예요",
          "original_form": "식완이요 미니어처예요",
          "speaker_id": "2",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003345.1.6",
          "form": "동심이 살아나요 ㅎㅎ",
          "original_form": "동심이 살아나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003345.1.7",
          "form": "뭐 갖고 싶으세요?",
          "original_form": "뭐 갖고 싶으세요?",
          "speaker_id": "2",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003345.1.8",
          "form": "저는 게임기...",
          "original_form": "저는 게임기...",
          "speaker_id": "1",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003345.1.9",
          "form": "닌텐도 이번 겨울에 사고싶네요",
          "original_form": "닌텐도 이번 겨울에 사고싶네요",
          "speaker_id": "1",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003345.1.10",
          "form": "오 전 애들 사주고 싶네요",
          "original_form": "오 전 애들 사주고 싶네요",
          "speaker_id": "2",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003345.1.11",
          "form": "비싸더라구요",
          "original_form": "비싸더라구요",
          "speaker_id": "2",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003345.1.12",
          "form": "모동숲 다 하던데",
          "original_form": "모동숲 다 하던데",
          "speaker_id": "2",
          "time": "20210820 22:03"
        },
        {
          "id": "MDRW2100003345.1.13",
          "form": "저도 갑자기 사고 싶어지네요 ㅎㅎ",
          "original_form": "저도 갑자기 사고 싶어지네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 22:03"
        },
        {
          "id": "MDRW2100003345.1.14",
          "form": "혹시 미니어처는 어떤 미니어처 사시고싶어요?",
          "original_form": "혹시 미니어처는 어떤 미니어처 사시고싶어요?",
          "speaker_id": "1",
          "time": "20210820 22:03"
        },
        {
          "id": "MDRW2100003345.1.15",
          "form": "식완요 리멘트 제품",
          "original_form": "식완요 리멘트 제품",
          "speaker_id": "2",
          "time": "20210820 22:03"
        },
        {
          "id": "MDRW2100003345.1.16",
          "form": "특정 제품이 있어요 ㅋㅋ",
          "original_form": "특정 제품이 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003345.1.17",
          "form": "정말 아기자기해요 이마트에도 있어요",
          "original_form": "정말 아기자기해요 이마트에도 있어요",
          "speaker_id": "2",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003345.1.18",
          "form": "리멘트가 회사 이름이죠?",
          "original_form": "리멘트가 회사 이름이죠?",
          "speaker_id": "1",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003345.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003345.1.20",
          "form": "저도 포켓몬 미니어처 알아볼때",
          "original_form": "저도 포켓몬 미니어처 알아볼때",
          "speaker_id": "1",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003345.1.21",
          "form": "예쁜거 많더라구요",
          "original_form": "예쁜거 많더라구요",
          "speaker_id": "1",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003345.1.22",
          "form": "카페도 있어요 식완소",
          "original_form": "카페도 있어요 식완소",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003345.1.23",
          "form": "네 맞아요 포켓몬도 이뻐요",
          "original_form": "네 맞아요 포켓몬도 이뻐요",
          "speaker_id": "2",
          "time": "20210820 22:06"
        },
        {
          "id": "MDRW2100003345.1.24",
          "form": "근데 가격대가 1~2개면 괜찮은데 여러개사면 또 비싸더라구요",
          "original_form": "근데 가격대가 1~2개면 괜찮은데 여러개사면 또 비싸더라구요",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003345.1.25",
          "form": "네 근데 세트로 사야 후회없어요",
          "original_form": "네 근데 세트로 사야 후회없어요",
          "speaker_id": "2",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003345.1.26",
          "form": "뭐든 세트죠 ㅜㅜ",
          "original_form": "뭐든 세트죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003345.1.27",
          "form": "일본 제품이라 더 비싼거 같아요",
          "original_form": "일본 제품이라 더 비싼거 같아요",
          "speaker_id": "2",
          "time": "20210820 22:08"
        }
      ]
    }
