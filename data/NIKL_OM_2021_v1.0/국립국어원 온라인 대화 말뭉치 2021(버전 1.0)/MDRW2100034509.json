      "utterance": [
        {
          "id": "MDRW2100034509.1.1",
          "form": "수익만 8000억 아역때",
          "original_form": "수익만 8000억 아역때",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100034509.1.2",
          "form": "대박이네. 완전 부럽네",
          "original_form": "대박이네. 완전 부럽네",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100034509.1.3",
          "form": "엘르패팅 얘기 나오네 언니의 아역으로ㅋㅋ",
          "original_form": "엘르패팅 얘기 나오네 언니의 아역으로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034509.1.4",
          "form": "저 부모님은 참 좋겠다",
          "original_form": "저 부모님은 참 좋겠다",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034509.1.5",
          "form": "그러게 부모가 괜찮은지 구설수는 없더라",
          "original_form": "그러게 부모가 괜찮은지 구설수는 없더라",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034509.1.6",
          "form": "그치. 이 남자애도 얼굴이 비슷하드라",
          "original_form": "그치. 이 남자애도 얼굴이 비슷하드라",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034509.1.7",
          "form": "지금 나오는 아역은 너무 귀엽다 꼭 동화속 요정같다",
          "original_form": "지금 나오는 아역은 너무 귀엽다 꼭 동화속 요정같다",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034509.1.8",
          "form": "저때 14살 완전 어려보임",
          "original_form": "저때 14살 완전 어려보임",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100034509.1.9",
          "form": "귀모양이 특이해서 그런강 초동안",
          "original_form": "귀모양이 특이해서 그런강 초동안",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100034509.1.10",
          "form": "지금도 동안이다",
          "original_form": "지금도 동안이다",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100034509.1.11",
          "form": "너무 마른거 아니가",
          "original_form": "너무 마른거 아니가",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100034509.1.12",
          "form": "1위는 맥컬린 컬킨?",
          "original_form": "1위는 맥컬린 컬킨?",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034509.1.13",
          "form": "드디어 나왔네 아니다 패닉룸에 나온",
          "original_form": "드디어 나왔네 아니다 패닉룸에 나온",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034509.1.14",
          "form": "브레이킹던 여주인공",
          "original_form": "브레이킹던 여주인공",
