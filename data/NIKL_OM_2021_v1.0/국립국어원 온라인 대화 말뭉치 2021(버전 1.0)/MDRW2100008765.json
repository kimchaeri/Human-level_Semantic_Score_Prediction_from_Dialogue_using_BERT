      "utterance": [
        {
          "id": "MDRW2100008765.1.1",
          "form": "홈트 해본적 있으세요?",
          "original_form": "홈트 해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008765.1.2",
          "form": "홈트 가끔 하는데.. 쉽지 않네요",
          "original_form": "홈트 가끔 하는데.. 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100008765.1.3",
          "form": "저도 몇번 했었는데",
          "original_form": "저도 몇번 했었는데",
          "speaker_id": "1",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008765.1.4",
          "form": "지루해서 노선을 바꿔써요",
          "original_form": "지루해서 노선을 바꿔써요",
          "speaker_id": "1",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008765.1.5",
          "form": "의지력 부족이라서 매번 실패 ㅋㅋㅋ",
          "original_form": "의지력 부족이라서 매번 실패 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008765.1.6",
          "form": "저도 의지 부족이요ㅋㅋ",
          "original_form": "저도 의지 부족이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008765.1.7",
          "form": "트레이너쌤이 필요한 이유가 있더라구요",
          "original_form": "트레이너쌤이 필요한 이유가 있더라구요",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008765.1.8",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008765.1.9",
          "form": "그래서 pt를 하는게 확실한듯",
          "original_form": "그래서 pt를 하는게 확실한듯",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008765.1.10",
          "form": "PT해본적 있으세요?",
          "original_form": "PT해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008765.1.11",
          "form": "아니요~",
          "original_form": "아니요~",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008765.1.12",
          "form": "그냥 혼자 헬스만 해봤어요",
          "original_form": "그냥 혼자 헬스만 해봤어요",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008765.1.13",
          "form": "저는 헬스장에 가본적은 없어요",
          "original_form": "저는 헬스장에 가본적은 없어요",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008765.1.14",
          "form": "당연히 PT도 못해봤구...",
          "original_form": "당연히 PT도 못해봤구...",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008765.1.15",
          "form": "아~ 그럼 따로 운동 하는건 있으세요?",
          "original_form": "아~ 그럼 따로 운동 하는건 있으세요?",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008765.1.16",
          "form": "코로나 이전에는 탁구장 다녔었고",
          "original_form": "코로나 이전에는 탁구장 다녔었고",
          "speaker_id": "1",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008765.1.17",
          "form": "요즘에는 집에서 아이돌춤 따라추고 있어요",
          "original_form": "요즘에는 집에서 아이돌춤 따라추고 있어요",
          "speaker_id": "1",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008765.1.18",
          "form": "탁구도 운동 많이 되는거 같아요",
          "original_form": "탁구도 운동 많이 되는거 같아요",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008765.1.19",
          "form": "맞아요 한판 치고 나면 바로 탈진ㅋㅋ",
          "original_form": "맞아요 한판 치고 나면 바로 탈진ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008765.1.20",
          "form": "아이돌춤도 추시고 멋지십니다!!",
          "original_form": "아이돌춤도 추시고 멋지십니다!!",
          "speaker_id": "2",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100008765.1.21",
          "form": "아직은 많이 뻣뻣해요",
          "original_form": "아직은 많이 뻣뻣해요",
          "speaker_id": "1",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008765.1.22",
          "form": "응원합니다~!!",
          "original_form": "응원합니다~!!",
          "speaker_id": "2",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008765.1.23",
          "form": "저도 잠시 방송댄스 배운다고 문화센터 다녀봤는데 쉽지 않더라구요",
          "original_form": "저도 잠시 방송댄스 배운다고 문화센터 다녀봤는데 쉽지 않더라구요",
          "speaker_id": "2",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008765.1.24",
          "form": "처음이 제일 힘들어요 현타가 온다고 해야하나",
          "original_form": "처음이 제일 힘들어요 현타가 온다고 해야하나",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008765.1.25",
          "form": "그래서 몇번 하다 그만 하고 ㅋㅋㅋ",
          "original_form": "그래서 몇번 하다 그만 하고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008765.1.26",
          "form": "춤이라는게 힘도 필요하지만 유연성이 중요해서",
          "original_form": "춤이라는게 힘도 필요하지만 유연성이 중요해서",
          "speaker_id": "1",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008765.1.27",
          "form": "초반부터 잘할 수 없죠...ㅋㅋ",
          "original_form": "초반부터 잘할 수 없죠...ㅋㅋ",
