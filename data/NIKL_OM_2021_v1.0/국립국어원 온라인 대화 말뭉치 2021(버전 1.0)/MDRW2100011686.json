      "utterance": [
        {
          "id": "MDRW2100011686.1.1",
          "form": "반려동물 키워보신적 있으세요?",
          "original_form": "반려동물 키워보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011686.1.2",
          "form": "키운적은 없는데 강아지 좋아하긴해요",
          "original_form": "키운적은 없는데 강아지 좋아하긴해요",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011686.1.3",
          "form": "포메 볼때마다 제 심장이 아프네요",
          "original_form": "포메 볼때마다 제 심장이 아프네요",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011686.1.4",
          "form": "반려동물 좋아하시나요?",
          "original_form": "반려동물 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011686.1.5",
          "form": "포메랑 무슨 추억이 있으세요? 저도 비슷해요",
          "original_form": "포메랑 무슨 추억이 있으세요? 저도 비슷해요",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011686.1.6",
          "form": "어릴때 청거북이만 키워봤어여",
          "original_form": "어릴때 청거북이만 키워봤어여",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100011686.1.7",
          "form": "포메키우는 친구집에 가서 놀아줬던 기억이 있어서요 ㅎㅎ",
          "original_form": "포메키우는 친구집에 가서 놀아줬던 기억이 있어서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100011686.1.8",
          "form": "포메나 말티즈만 관심가고 그외는 무서워요",
          "original_form": "포메나 말티즈만 관심가고 그외는 무서워요",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100011686.1.9",
          "form": "앗 저랑 비슷하네요",
          "original_form": "앗 저랑 비슷하네요",
          "speaker_id": "1",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100011686.1.10",
          "form": "과외알바하는 학생집에 포메를키웠는데",
          "original_form": "과외알바하는 학생집에 포메를키웠는데",
          "speaker_id": "1",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100011686.1.11",
          "form": "넘귀여운거에요 제가 집에가면 막 왈왈대고 배 보여주고요",
          "original_form": "넘귀여운거에요 제가 집에가면 막 왈왈대고 배 보여주고요",
          "speaker_id": "1",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100011686.1.12",
          "form": "강아지는 작고 무섭지않으면 되는거같아요",
          "original_form": "강아지는 작고 무섭지않으면 되는거같아요",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011686.1.13",
          "form": "귀여운면도 있으면 너무 좋고요",
          "original_form": "귀여운면도 있으면 너무 좋고요",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011686.1.14",
          "form": "반려동물을 키울 생각은 있으신가요?",
          "original_form": "반려동물을 키울 생각은 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011686.1.15",
          "form": "개랑 고양이처럼 돌아다니는 동물은 힘들거같구요",
          "original_form": "개랑 고양이처럼 돌아다니는 동물은 힘들거같구요",
          "speaker_id": "1",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011686.1.16",
          "form": "거북이처럼 작은 집에서 살면 키워보고싶네요",
          "original_form": "거북이처럼 작은 집에서 살면 키워보고싶네요",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011686.1.17",
          "form": "반려동물에 대해 생각이 확고하시네요",
          "original_form": "반려동물에 대해 생각이 확고하시네요",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011686.1.18",
          "form": "남이 키우는건 쉬워보여도 제가 키우는건 어려울꺼같네요",
          "original_form": "남이 키우는건 쉬워보여도 제가 키우는건 어려울꺼같네요",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011686.1.19",
          "form": "그런가요 잘 못키울거같고 수술하는게 저는 못할짓하는거같아요",
          "original_form": "그런가요 잘 못키울거같고 수술하는게 저는 못할짓하는거같아요",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011686.1.20",
          "form": "글고 동물이 집 어지르는게 맘이 걸려요",
          "original_form": "글고 동물이 집 어지르는게 맘이 걸려요",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011686.1.21",
          "form": "밖에 키우는 거면 키워보고싶어요",
          "original_form": "밖에 키우는 거면 키워보고싶어요",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011686.1.22",
          "form": "집에 돌아오면 어지러워져 있고 청소도 두배로 해야죠",
          "original_form": "집에 돌아오면 어지러워져 있고 청소도 두배로 해야죠",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011686.1.23",
          "form": "간식, 사료값도 무시못할듯요",
          "original_form": "간식, 사료값도 무시못할듯요",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011686.1.24",
          "form": "네 아프면 병원비랑요",
          "original_form": "네 아프면 병원비랑요",
          "speaker_id": "1",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011686.1.25",
          "form": "키울땐 몰라도 늙으면 아프는건 마찬가지라 ㅎㅎ.",
          "original_form": "키울땐 몰라도 늙으면 아프는건 마찬가지라 ㅎㅎ.",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011686.1.26",
          "form": "진짜 대단하다 생각했어요",
          "original_form": "진짜 대단하다 생각했어요",
          "speaker_id": "1",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100011686.1.27",
          "form": "참사랑인듯요",
          "original_form": "참사랑인듯요",
          "speaker_id": "1",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100011686.1.28",
          "form": "네 버려진 동물 데려와서 키우는 분 봤는데",
