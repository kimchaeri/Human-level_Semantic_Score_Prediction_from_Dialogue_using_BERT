        },
        {
          "id": "MDRW2100030415.1.2",
          "form": "심야괴다뫼 봄???",
          "original_form": "심야괴다뫼 봄???",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100030415.1.3",
          "form": "심야괴담회",
          "original_form": "심야괴담회",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100030415.1.4",
          "form": "보시나오",
          "original_form": "보시나오",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100030415.1.5",
          "form": "목 늘어나는 파란 남자",
          "original_form": "목 늘어나는 파란 남자",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100030415.1.6",
          "form": "오 맞다",
          "original_form": "오 맞다",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100030415.1.7",
          "form": "그건 재연이 넘 무서웠음",
          "original_form": "그건 재연이 넘 무서웠음",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100030415.1.8",
          "form": "그래서 기억에 남아ㅋㅋ",
          "original_form": "그래서 기억에 남아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100030415.1.9",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100030415.1.10",
          "form": "상황도 무섭고",
          "original_form": "상황도 무섭고",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100030415.1.11",
          "form": "목 쭉 늘어나는 것도 무섭고",
          "original_form": "목 쭉 늘어나는 것도 무섭고",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100030415.1.12",
          "form": "그 대문 넘어서 목 늘려서 아이들 찾던거 기억남...",
          "original_form": "그 대문 넘어서 목 늘려서 아이들 찾던거 기억남...",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100030415.1.13",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100030415.1.14",
          "form": "그거 무섭",
          "original_form": "그거 무섭",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100030415.1.15",
          "form": "해석 혹시 봤어??",
          "original_form": "해석 혹시 봤어??",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100030415.1.16",
          "form": "헐 아녀",
          "original_form": "헐 아녀",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030415.1.17",
          "form": "해석한 것도 있어??",
          "original_form": "해석한 것도 있어??",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030415.1.18",
          "form": "아 유튭에 누가 댓글 단건데",
          "original_form": "아 유튭에 누가 댓글 단건데",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100030415.1.19",
          "form": "파란 얼굴 긴 목 빼문 혀 : 목매달아 죽은 사람이라고",
          "original_form": "파란 얼굴 긴 목 빼문 혀 : 목매달아 죽은 사람이라고",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100030415.1.20",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100030415.1.21",
          "form": "목매달아 죽으면 그 혀가 길게 나온다고 하던데",
          "original_form": "목매달아 죽으면 그 혀가 길게 나온다고 하던데",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100030415.1.22",
          "form": "목도 늘어나고",
          "original_form": "목도 늘어나고",
          "speaker_id": "2",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100030415.1.23",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100030415.1.24",
          "form": "새벽에 무서운 이야기 라니...",
          "original_form": "새벽에 무서운 이야기 라니...",
          "speaker_id": "1",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100030415.1.25",
          "form": "심야괴담회 유툽 복습을...",
          "original_form": "심야괴담회 유툽 복습을...",
          "speaker_id": "1",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100030415.1.26",
