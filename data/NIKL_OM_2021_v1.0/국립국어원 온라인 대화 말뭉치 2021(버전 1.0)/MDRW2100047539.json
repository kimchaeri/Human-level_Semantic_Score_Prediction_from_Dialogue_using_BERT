      "utterance": [
        {
          "id": "MDRW2100047539.1.1",
          "form": "배달 음식",
          "original_form": "배달 음식",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100047539.1.2",
          "form": "주로 뭐 시캬드세요?",
          "original_form": "주로 뭐 시캬드세요?",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100047539.1.3",
          "form": "저는 브리또 요새 또 한창 빠져있답니다 ㅋㅋ",
          "original_form": "저는 브리또 요새 또 한창 빠져있답니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100047539.1.4",
          "form": "ㅋㅋㅋㅋ 오 브리또",
          "original_form": "ㅋㅋㅋㅋ 오 브리또",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100047539.1.5",
          "form": "맛있겠네요 ㅋㅋㅋㅋㅋ",
          "original_form": "맛있겠네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047539.1.6",
          "form": "얼마인가요?",
          "original_form": "얼마인가요?",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047539.1.7",
          "form": "하나에 8천원정도에요 ㅜ좀비싸죠?ㅜㅜ",
          "original_form": "하나에 8천원정도에요 ㅜ좀비싸죠?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100047539.1.8",
          "form": "헉 진짜 비싸네오..",
          "original_form": "헉 진짜 비싸네오..",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047539.1.9",
          "form": "요새 샐러드 같은 것도",
          "original_form": "요새 샐러드 같은 것도",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047539.1.10",
          "form": "되게 비싸더라구요",
          "original_form": "되게 비싸더라구요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100047539.1.11",
          "form": "맞아요.. 그놈의 풀데기가 뭐라구 ㅠㅠ",
          "original_form": "맞아요.. 그놈의 풀데기가 뭐라구 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100047539.1.12",
          "form": "진짜요.. 물가가 아주 비싸요",
          "original_form": "진짜요.. 물가가 아주 비싸요",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047539.1.13",
          "form": "왜이렇게된건지.. 차라리 밥이 더 저렴하고 푸짐해요 ㅋㅋ",
          "original_form": "왜이렇게된건지.. 차라리 밥이 더 저렴하고 푸짐해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047539.1.14",
          "form": "진짜요ㅋㅋ 오래된 한식음식점은",
          "original_form": "진짜요ㅋㅋ 오래된 한식음식점은",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047539.1.15",
          "form": "푸짐한데 6000원 막 그래요",
          "original_form": "푸짐한데 6000원 막 그래요",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047539.1.16",
          "form": "맞죠맞죠 ㅋㅋ",
          "original_form": "맞죠맞죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100047539.1.17",
          "form": "떡볶이도 체인점은 너무 비싸서 화가나요",
          "original_form": "떡볶이도 체인점은 너무 비싸서 화가나요",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100047539.1.18",
          "form": "진짜.. 가격이 무슨....",
          "original_form": "진짜.. 가격이 무슨....",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100047539.1.19",
          "form": "어쩌다 이렇게된건지모르겠어요..",
          "original_form": "어쩌다 이렇게된건지모르겠어요..",
          "speaker_id": "1",
