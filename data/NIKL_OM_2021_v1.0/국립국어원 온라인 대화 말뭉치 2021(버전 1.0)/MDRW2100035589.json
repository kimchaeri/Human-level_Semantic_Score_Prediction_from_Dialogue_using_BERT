      "utterance": [
        {
          "id": "MDRW2100035589.1.1",
          "form": "제일 좋아하는 음식이 뭔가요??!",
          "original_form": "제일 좋아하는 음식이 뭔가요??!",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100035589.1.2",
          "form": "생선초밥이요!",
          "original_form": "생선초밥이요!",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100035589.1.3",
          "form": "오 어떤 종류던 다 좋아하세요?",
          "original_form": "오 어떤 종류던 다 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100035589.1.4",
          "form": "흠 다 먹어본게 아니라",
          "original_form": "흠 다 먹어본게 아니라",
          "speaker_id": "2",
          "time": "20210829 21:00"
        },
        {
          "id": "MDRW2100035589.1.5",
          "form": "잘모르겠어요",
          "original_form": "잘모르겠어요",
          "speaker_id": "2",
          "time": "20210829 21:00"
        },
        {
          "id": "MDRW2100035589.1.6",
          "form": "그래도 싱싱하기만 하면 별로 안가릴거같아요",
          "original_form": "그래도 싱싱하기만 하면 별로 안가릴거같아요",
          "speaker_id": "2",
          "time": "20210829 21:00"
        },
        {
          "id": "MDRW2100035589.1.7",
          "form": "아 그럼 특별히 좋아하는 맛도 없으신...?",
          "original_form": "아 그럼 특별히 좋아하는 맛도 없으신...?",
          "speaker_id": "1",
          "time": "20210829 21:01"
        },
        {
          "id": "MDRW2100035589.1.8",
          "form": "저는 뭐니뭐니해도 연어가 맛있더라구여 ㅎㅎ",
          "original_form": "저는 뭐니뭐니해도 연어가 맛있더라구여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:01"
        },
        {
          "id": "MDRW2100035589.1.9",
          "form": "아하 그말씀이었군요",
          "original_form": "아하 그말씀이었군요",
          "speaker_id": "2",
          "time": "20210829 21:02"
        },
        {
          "id": "MDRW2100035589.1.10",
          "form": "저는 흰살생선이 좋았어요",
          "original_form": "저는 흰살생선이 좋았어요",
          "speaker_id": "2",
          "time": "20210829 21:02"
        },
        {
          "id": "MDRW2100035589.1.11",
          "form": "광어도미 그런거요",
          "original_form": "광어도미 그런거요",
          "speaker_id": "2",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100035589.1.12",
          "form": "아~ 회보다 초밥 좋아하는 이유는 식사대용이라서 그런가요?",
          "original_form": "아~ 회보다 초밥 좋아하는 이유는 식사대용이라서 그런가요?",
          "speaker_id": "1",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100035589.1.13",
          "form": "저는 둘다 좋아서요! ㅎㅎ",
          "original_form": "저는 둘다 좋아서요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100035589.1.14",
          "form": "예쁘게 생겨서요",
          "original_form": "예쁘게 생겨서요",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100035589.1.15",
          "form": "저도 회 좋아하긴 해요 ㅎㅎ",
          "original_form": "저도 회 좋아하긴 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100035589.1.16",
          "form": "오 ㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100035589.1.17",
          "form": "되게 의외의 대답이네요! 예쁘게 생겨서...",
          "original_form": "되게 의외의 대답이네요! 예쁘게 생겨서...",
          "speaker_id": "1",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100035589.1.18",
          "form": "알록달록",
          "original_form": "알록달록",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100035589.1.19",
          "form": "정성많은 느낌이라 좋아요",
          "original_form": "정성많은 느낌이라 좋아요",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100035589.1.20",
          "form": "ㅋㅋㅋ 그렇긴해요! 밥을 주먹쥐고 잘돌돌말아서 회를 하나하나 올리고 정성이 많이 들어간것 같긴하네요",
          "original_form": "ㅋㅋㅋ 그렇긴해요! 밥을 주먹쥐고 잘돌돌말아서 회를 하나하나 올리고 정성이 많이 들어간것 같긴하네요",
          "speaker_id": "1",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100035589.1.21",
          "form": "다른 음식들에 비해서",
          "original_form": "다른 음식들에 비해서",
          "speaker_id": "1",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100035589.1.22",
          "form": "네 그것도 그렇고",
          "original_form": "네 그것도 그렇고",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100035589.1.23",
          "form": "음식재활용하기 어려울거 같아서 좋아요",
          "original_form": "음식재활용하기 어려울거 같아서 좋아요",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100035589.1.24",
          "form": "며칠전에 해운대31해물칼국수였나 하는 집에 가서",
          "original_form": "며칠전에 해운대31해물칼국수였나 하는 집에 가서",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100035589.1.25",
          "form": "해물칼국수 먹다가 휴지 나왔어요",
          "original_form": "해물칼국수 먹다가 휴지 나왔어요",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100035589.1.26",
          "form": "혼자 가서 그런지 재활용음식을 끓여준거같더라구요",
          "original_form": "혼자 가서 그런지 재활용음식을 끓여준거같더라구요",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100035589.1.27",
          "form": "헐???????????",
          "original_form": "헐???????????",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035589.1.28",
          "form": "칼국수안에서 휴지가요?",
          "original_form": "칼국수안에서 휴지가요?",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035589.1.29",
          "form": "어머나..",
          "original_form": "어머나..",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035589.1.30",
          "form": "어으 진짜 불쾌하셨겠어요!!!!!",
          "original_form": "어으 진짜 불쾌하셨겠어요!!!!!",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035589.1.31",
          "form": "항의하셨나요???",
          "original_form": "항의하셨나요???",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035589.1.32",
          "form": "완전 듣는입장에서도 화나요!!",
          "original_form": "완전 듣는입장에서도 화나요!!",
          "speaker_id": "1",
