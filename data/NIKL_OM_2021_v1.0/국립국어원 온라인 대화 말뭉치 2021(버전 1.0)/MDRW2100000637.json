      "utterance": [
        {
          "id": "MDRW2100000637.1.1",
          "form": "덥다덥다덥다~",
          "original_form": "덥다덥다덥다~",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000637.1.2",
          "form": "여름은 1주일만 하고 없어도 될 것 같아",
          "original_form": "여름은 1주일만 하고 없어도 될 것 같아",
          "speaker_id": "1",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000637.1.3",
          "form": "왜 아직도 더운건가~",
          "original_form": "왜 아직도 더운건가~",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000637.1.4",
          "form": "여름은 진짜 별로야 시원하다고 해도 아직도 여름날씨야",
          "original_form": "여름은 진짜 별로야 시원하다고 해도 아직도 여름날씨야",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.5",
          "form": "겨울되면 여름이 좋을껄~  ㅋㅋ",
          "original_form": "겨울되면 여름이 좋을껄~  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.6",
          "form": "겨울에는 눈오리도 만들 수 있고 얼마나 좋아",
          "original_form": "겨울에는 눈오리도 만들 수 있고 얼마나 좋아",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.7",
          "form": "낮엔 겁나 햇볕 쨍함",
          "original_form": "낮엔 겁나 햇볕 쨍함",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.8",
          "form": "추우면 옷을 더 입으면 되지만 더우면 너무 불편하다",
          "original_form": "추우면 옷을 더 입으면 되지만 더우면 너무 불편하다",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.9",
          "form": "에어컨 많이 틀면 환경이 걱정이고",
          "original_form": "에어컨 많이 틀면 환경이 걱정이고",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.10",
          "form": "눈오리? 그건 또 뭔가?",
          "original_form": "눈오리? 그건 또 뭔가?",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.11",
          "form": "겨울이 빨리 왔으면 좋겠어",
          "original_form": "겨울이 빨리 왔으면 좋겠어",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.12",
          "form": "눈사람도 아니고? ㅋㅋ",
          "original_form": "눈사람도 아니고? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.13",
          "form": "작년에 돌풍적인 인기를 끌었던 눈오리",
          "original_form": "작년에 돌풍적인 인기를 끌었던 눈오리",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000637.1.14",
          "form": "겨울와서 눈내리면 길바닥 언다",
          "original_form": "겨울와서 눈내리면 길바닥 언다",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000637.1.15",
          "form": "눈오리 틀로 눈오면 눈 모아서 눈모양 오리를 만드는거야",
          "original_form": "눈오리 틀로 눈오면 눈 모아서 눈모양 오리를 만드는거야",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000637.1.16",
          "form": "눈오리 진짜 귀여워",
          "original_form": "눈오리 진짜 귀여워",
          "speaker_id": "1",
