      "utterance": [
        {
          "id": "MDRW2100038756.1.1",
          "form": "name2님은 키워본 동물 있나요?",
          "original_form": "&name2&님은 키워본 동물 있나요?",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038756.1.2",
          "form": "어릴때 강아지요!",
          "original_form": "어릴때 강아지요!",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038756.1.3",
          "form": "저도 말티즈 키웠었어요.. 어는 종 키웠나요?",
          "original_form": "저도 말티즈 키웠었어요.. 어는 종 키웠나요?",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100038756.1.4",
          "form": "오... 저도 말티즈는 아니고",
          "original_form": "오... 저도 말티즈는 아니고",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100038756.1.5",
          "form": "그 피가 찐한 믹스 키웠어요 ㅎㅎ",
          "original_form": "그 피가 찐한 믹스 키웠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100038756.1.6",
          "form": "아~ 똥강아지가 귀엽죠..ㅎㅎㅎ",
          "original_form": "아~ 똥강아지가 귀엽죠..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100038756.1.7",
          "form": "우리 말티즈 이름은 하니였어요.. name2님네 강아지는요?",
          "original_form": "우리 말티즈 이름은 하니였어요.. &name2&님네 강아지는요?",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100038756.1.8",
          "form": "저랑 제동생 이름 하나씩 따서...",
          "original_form": "저랑 제동생 이름 하나씩 따서...",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038756.1.9",
          "form": "수천이요... ㅎㅎㅎ",
          "original_form": "수천이요... ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038756.1.10",
          "form": "그랬구나.. 우리는 달려라 하니에서 따왔어요.. ㅎㅎ",
          "original_form": "그랬구나.. 우리는 달려라 하니에서 따왔어요.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038756.1.11",
          "form": "말티즈를 키웠었기 때문인지 아직도 강아지 중에 말티즈가 젤 정이 가네요",
          "original_form": "말티즈를 키웠었기 때문인지 아직도 강아지 중에 말티즈가 젤 정이 가네요",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038756.1.12",
          "form": "저도 그래요.",
          "original_form": "저도 그래요.",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038756.1.13",
          "form": "비슷하게 하얗고 작은 개만 보면 눈이 가요",
          "original_form": "비슷하게 하얗고 작은 개만 보면 눈이 가요",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038756.1.14",
          "form": "요새는 남편이 물고기 키우자고 해서 물고기 키우고 있어요",
          "original_form": "요새는 남편이 물고기 키우자고 해서 물고기 키우고 있어요",
          "speaker_id": "1",
          "time": "20210903 06:01"
        },
        {
          "id": "MDRW2100038756.1.15",
          "form": "어떤 물고기요? 구피?",
          "original_form": "어떤 물고기요? 구피?",
          "speaker_id": "2",
          "time": "20210903 08:35"
        },
        {
          "id": "MDRW2100038756.1.16",
          "form": "네 구피요",
          "original_form": "네 구피요",
          "speaker_id": "1",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100038756.1.17",
          "form": "구피를 누가 추천해줬어요",
          "original_form": "구피를 누가 추천해줬어요",
          "speaker_id": "1",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100038756.1.18",
          "form": "지금 한 100마리쯤 있네요",
          "original_form": "지금 한 100마리쯤 있네요",
          "speaker_id": "1",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100038756.1.19",
          "form": "우와~ 잘키우셨네요",
          "original_form": "우와~ 잘키우셨네요",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100038756.1.20",
          "form": "번식을 엄청 했네요!",
          "original_form": "번식을 엄청 했네요!",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100038756.1.21",
          "form": "구피가 번식력이 좋죠..",
          "original_form": "구피가 번식력이 좋죠..",
          "speaker_id": "1",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100038756.1.22",
          "form": "처음에 10마리 정도 가져왔는데",
          "original_form": "처음에 10마리 정도 가져왔는데",
          "speaker_id": "1",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100038756.1.23",
          "form": "어항도 엄청 늘었겠어요",
          "original_form": "어항도 엄청 늘었겠어요",
          "speaker_id": "2",
          "time": "20210903 09:39"
        },
        {
          "id": "MDRW2100038756.1.24",
          "form": "어항이 점점 커지고 있어요..ㅎㅎ",
          "original_form": "어항이 점점 커지고 있어요..ㅎㅎ",
          "speaker_id": "1",
