      "utterance": [
        {
          "id": "MDRW2100020343.1.1",
          "form": "마스크써서 다행이에요",
          "original_form": "마스크써서 다행이에요",
          "speaker_id": "1",
          "time": "20210828 02:04"
        },
        {
          "id": "MDRW2100020343.1.2",
          "form": "욕만뱉어요",
          "original_form": "욕만뱉어요",
          "speaker_id": "1",
          "time": "20210828 02:04"
        },
        {
          "id": "MDRW2100020343.1.3",
          "form": "요새 날이 참 옷입기 애매한듯해요",
          "original_form": "요새 날이 참 옷입기 애매한듯해요",
          "speaker_id": "2",
          "time": "20210828 02:04"
        },
        {
          "id": "MDRW2100020343.1.4",
          "form": "좀 쌀쌀한거같아서 긴팔입으면",
          "original_form": "좀 쌀쌀한거같아서 긴팔입으면",
          "speaker_id": "1",
          "time": "20210828 02:05"
        },
        {
          "id": "MDRW2100020343.1.5",
          "form": "집도착해서 겨드랑이 캐리비안베이고,,",
          "original_form": "집도착해서 겨드랑이 캐리비안베이고,,",
          "speaker_id": "1",
          "time": "20210828 02:05"
        },
        {
          "id": "MDRW2100020343.1.6",
          "form": "헐 마자여!!",
          "original_form": "헐 마자여!!",
          "speaker_id": "2",
          "time": "20210828 02:05"
        },
        {
          "id": "MDRW2100020343.1.7",
          "form": "날은 추워졌는데 여전히 마스크에 습기는 차고 하...",
          "original_form": "날은 추워졌는데 여전히 마스크에 습기는 차고 하...",
          "speaker_id": "2",
          "time": "20210828 02:05"
        },
        {
          "id": "MDRW2100020343.1.8",
          "form": "아침마다 반팔과 긴팔사이에서 엄청 고민해요",
          "original_form": "아침마다 반팔과 긴팔사이에서 엄청 고민해요",
          "speaker_id": "2",
          "time": "20210828 02:06"
        },
        {
          "id": "MDRW2100020343.1.9",
          "form": "반팔을 입고 남방을 들고다니는게",
          "original_form": "반팔을 입고 남방을 들고다니는게",
          "speaker_id": "1",
          "time": "20210828 02:06"
        },
        {
          "id": "MDRW2100020343.1.10",
          "form": "제선택입니다,,,!",
          "original_form": "제선택입니다,,,!",
          "speaker_id": "1",
          "time": "20210828 02:06"
        },
        {
          "id": "MDRW2100020343.1.11",
          "form": "오 그것도 좋은데요?",
          "original_form": "오 그것도 좋은데요?",
          "speaker_id": "2",
          "time": "20210828 02:07"
        },
        {
          "id": "MDRW2100020343.1.12",
          "form": "근데 아직은 거의 남방을 어깨에 둘러요",
          "original_form": "근데 아직은 거의 남방을 어깨에 둘러요",
          "speaker_id": "1",
          "time": "20210828 02:08"
        },
        {
          "id": "MDRW2100020343.1.13",
          "form": "아직 전 파워워킹하면 충분히 따뜻해서 결국 반팔을 선택해버리는ㅋㅋㅋㅋ",
          "original_form": "아직 전 파워워킹하면 충분히 따뜻해서 결국 반팔을 선택해버리는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:08"
        },
        {
          "id": "MDRW2100020343.1.14",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㄱ아침에 일찍나갈때만 서늘하고",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㄱ아침에 일찍나갈때만 서늘하고",
          "speaker_id": "1",
          "time": "20210828 02:08"
        },
        {
          "id": "MDRW2100020343.1.15",
          "form": "아직여름같아요",
          "original_form": "아직여름같아요",
          "speaker_id": "1",
          "time": "20210828 02:08"
        },
        {
          "id": "MDRW2100020343.1.16",
          "form": "낮에는 아직 햇빛이 뜨겁더라구요",
          "original_form": "낮에는 아직 햇빛이 뜨겁더라구요",
          "speaker_id": "2",
          "time": "20210828 02:08"
        },
        {
          "id": "MDRW2100020343.1.17",
          "form": "아직은 냉면이 맛있는 계절ㅠ",
          "original_form": "아직은 냉면이 맛있는 계절ㅠ",
          "speaker_id": "1",
          "time": "20210828 02:10"
        },
        {
          "id": "MDRW2100020343.1.18",
          "form": "맞아요ㅋㅋㅋㅋㅋ 아이스 못잃어...",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ 아이스 못잃어...",
          "speaker_id": "2",
          "time": "20210828 02:11"
        },
