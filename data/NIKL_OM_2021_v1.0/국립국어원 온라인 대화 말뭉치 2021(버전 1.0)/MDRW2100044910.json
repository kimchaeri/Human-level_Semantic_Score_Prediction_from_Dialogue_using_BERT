      "utterance": [
        {
          "id": "MDRW2100044910.1.1",
          "form": "좋아하시는 티비 프로 있으신가요?",
          "original_form": "좋아하시는 티비 프로 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044910.1.2",
          "form": "요즘 티비를 보는것보나 유튜브 많이 봐요\r\n티비는 유퀴즈랑 1박2일요",
          "original_form": "요즘 티비를 보는것보나 유튜브 많이 봐요\r\n티비는 유퀴즈랑 1박2일요",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044910.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044910.1.4",
          "form": "저는 놀라운 토요일 좋아해요",
          "original_form": "저는 놀라운 토요일 좋아해요",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044910.1.5",
          "form": "전 유튜브 올라온 건 봤어요 요즘 키가 웃겨요 ㅋㅋㅋ",
          "original_form": "전 유튜브 올라온 건 봤어요 요즘 키가 웃겨요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044910.1.6",
          "form": "키 넘 좋아요ㅋㅋㅋㅋㅋ요즘엔 자기가 나서서 비지엠 불러주는데 노래도 넘 잘 부르고",
          "original_form": "키 넘 좋아요ㅋㅋㅋㅋㅋ요즘엔 자기가 나서서 비지엠 불러주는데 노래도 넘 잘 부르고",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044910.1.7",
          "form": "춤도 잘추고ㅋㅋㅋㅋㅋ",
          "original_form": "춤도 잘추고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044910.1.8",
          "form": "유튜브에 춤 모음 보는데 분장도 찰떡이고 본업 잘하는게 나오더라구요 센스도 있구요",
          "original_form": "유튜브에 춤 모음 보는데 분장도 찰떡이고 본업 잘하는게 나오더라구요 센스도 있구요",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044910.1.9",
          "form": "맞아요 넘 자기 끼를 알고 잘 쓰더라고요",
          "original_form": "맞아요 넘 자기 끼를 알고 잘 쓰더라고요",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044910.1.10",
          "form": "놀토는 예전보다 패널들 엄청 많더라구요",
          "original_form": "놀토는 예전보다 패널들 엄청 많더라구요",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044910.1.11",
          "form": "군대 다녀온 동안 자리 채운 사람들이 안 빠져서 그런게 아닐까요?",
          "original_form": "군대 다녀온 동안 자리 채운 사람들이 안 빠져서 그런게 아닐까요?",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044910.1.12",
          "form": "아하 어쩐지 많아보이더라 ㅋㅋㅋㅋㅋ",
          "original_form": "아하 어쩐지 많아보이더라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044910.1.13",
          "form": "피오랑 넉살? 이렇게 둘이 키랑 한해가 군대 갔을 때 자리 채웠어요",
          "original_form": "피오랑 넉살? 이렇게 둘이 키랑 한해가 군대 갔을 때 자리 채웠어요",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044910.1.14",
          "form": "그렇구나 계속 고정인가봐요",
          "original_form": "그렇구나 계속 고정인가봐요",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044910.1.15",
          "form": "네 별일 없는 한 게스트만 바꿔요",
          "original_form": "네 별일 없는 한 게스트만 바꿔요",
          "speaker_id": "1",
          "time": "20210908 00:32"
        }
      ]
