      "utterance": [
        {
          "id": "MDRW2100004239.1.1",
          "form": "좋아하는 음식 있어?",
          "original_form": "좋아하는 음식 있어?",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004239.1.2",
          "form": "나...지금은 훠궈가 먹고싶어",
          "original_form": "나...지금은 훠궈가 먹고싶어",
          "speaker_id": "2",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004239.1.3",
          "form": "아...나는 마라탕 먹고싶다",
          "original_form": "아...나는 마라탕 먹고싶다",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004239.1.4",
          "form": "멀 좀 아는구나",
          "original_form": "멀 좀 아는구나",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004239.1.5",
          "form": "마라탕에 뭐 뭐 넣어먹어?",
          "original_form": "마라탕에 뭐 뭐 넣어먹어?",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004239.1.6",
          "form": "나는 일단 분모자 숙주 청경채 소고기 새우 유부 연근 납작당면 배추",
          "original_form": "나는 일단 분모자 숙주 청경채 소고기 새우 유부 연근 납작당면 배추",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004239.1.7",
          "form": "이건 꼭 넣어",
          "original_form": "이건 꼭 넣어",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004239.1.8",
          "form": "와! 너무좋다",
          "original_form": "와! 너무좋다",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004239.1.9",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004239.1.10",
          "form": "나는 거기에 목이버섯을 꼭 넣어",
          "original_form": "나는 거기에 목이버섯을 꼭 넣어",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004239.1.11",
          "form": "아!! 버섯!!",
          "original_form": "아!! 버섯!!",
          "speaker_id": "1",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004239.1.12",
          "form": "버섯도 진짜 좋지",
          "original_form": "버섯도 진짜 좋지",
          "speaker_id": "1",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004239.1.13",
          "form": "목이버섯 팽이버섯 새송이버섯 다 좋아",
          "original_form": "목이버섯 팽이버섯 새송이버섯 다 좋아",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004239.1.14",
          "form": "방금 처음만났지만 너무잘맞는다",
          "original_form": "방금 처음만났지만 너무잘맞는다",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004239.1.15",
          "form": "마라탕 한끼 하고싶네",
          "original_form": "마라탕 한끼 하고싶네",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004239.1.16",
          "form": "마라탕 맛은 어떤걸로 먹어?",
          "original_form": "마라탕 맛은 어떤걸로 먹어?",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004239.1.17",
          "form": "매운 정도!",
          "original_form": "매운 정도!",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004239.1.18",
          "form": "중간?! 엄청 맵게는 잘 못먹어 ㅜ",
          "original_form": "중간?! 엄청 맵게는 잘 못먹어 ㅜ",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004239.1.19",
          "form": "근데 막 얼얼한 맛이랑 매운 맛 분리해서 단계 고르는 곳 있잖아?",
          "original_form": "근데 막 얼얼한 맛이랑 매운 맛 분리해서 단계 고르는 곳 있잖아?",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004239.1.20",
          "form": "얼얼한 맛은 세게 해도 괜찮아 ㅋㅋ",
          "original_form": "얼얼한 맛은 세게 해도 괜찮아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004239.1.21",
          "form": "헐 나도 엄청 매운건 잘 못먹어",
          "original_form": "헐 나도 엄청 매운건 잘 못먹어",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004239.1.22",
          "form": "마라탕은 그 얼얼한 맛으로 먹는거지",
          "original_form": "마라탕은 그 얼얼한 맛으로 먹는거지",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004239.1.23",
          "form": "너 완전 맛잘알이다",
          "original_form": "너 완전 맛잘알이다",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004239.1.24",
          "form": "우연찮게 만난",
          "original_form": "우연찮게 만난",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004239.1.25",
