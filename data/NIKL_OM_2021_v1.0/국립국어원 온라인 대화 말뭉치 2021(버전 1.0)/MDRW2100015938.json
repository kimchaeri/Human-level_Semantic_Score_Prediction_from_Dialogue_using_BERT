      "utterance": [
        {
          "id": "MDRW2100015938.1.1",
          "form": "날도 더운데 요리하기 좀 그렇네",
          "original_form": "날도 더운데 요리하기 좀 그렇네",
          "speaker_id": "2",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015938.1.2",
          "form": "그러면 뭐 시켜먹자",
          "original_form": "그러면 뭐 시켜먹자",
          "speaker_id": "1",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015938.1.3",
          "form": "그럴까?",
          "original_form": "그럴까?",
          "speaker_id": "2",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015938.1.4",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015938.1.5",
          "form": "배달의 민족으로 시킬거지?",
          "original_form": "배달의 민족으로 시킬거지?",
          "speaker_id": "2",
          "time": "20210826 03:16"
        },
        {
          "id": "MDRW2100015938.1.6",
          "form": "그래야지",
          "original_form": "그래야지",
          "speaker_id": "1",
          "time": "20210826 03:16"
        },
        {
          "id": "MDRW2100015938.1.7",
          "form": "뭐 먹고싶은거 있어?",
          "original_form": "뭐 먹고싶은거 있어?",
          "speaker_id": "2",
          "time": "20210826 03:16"
        },
        {
          "id": "MDRW2100015938.1.8",
          "form": "나는 덮밥류나 떡볶이",
          "original_form": "나는 덮밥류나 떡볶이",
          "speaker_id": "1",
          "time": "20210826 03:16"
        },
        {
          "id": "MDRW2100015938.1.9",
          "form": "떡볶이는 집에도 재료있어서 해먹을 수 있어",
          "original_form": "떡볶이는 집에도 재료있어서 해먹을 수 있어",
          "speaker_id": "2",
          "time": "20210826 03:16"
        },
        {
          "id": "MDRW2100015938.1.10",
          "form": "그래? 그럼 덮밥류 먹을까?",
          "original_form": "그래? 그럼 덮밥류 먹을까?",
          "speaker_id": "1",
          "time": "20210826 03:16"
        },
        {
          "id": "MDRW2100015938.1.11",
          "form": "어디서 시킬건데?",
          "original_form": "어디서 시킬건데?",
          "speaker_id": "2",
          "time": "20210826 03:17"
        },
        {
          "id": "MDRW2100015938.1.12",
          "form": "address에 맛있는 덮밥집있어",
          "original_form": "&address&에 맛있는 덮밥집있어",
          "speaker_id": "1",
          "time": "20210826 03:17"
        },
        {
          "id": "MDRW2100015938.1.13",
          "form": "배달팁 많이 나오지 않을까?",
          "original_form": "배달팁 많이 나오지 않을까?",
          "speaker_id": "2",
          "time": "20210826 03:17"
        },
        {
          "id": "MDRW2100015938.1.14",
          "form": "3000원이면 괜찮지?",
          "original_form": "3000원이면 괜찮지?",
          "speaker_id": "1",
          "time": "20210826 03:17"
        },
        {
          "id": "MDRW2100015938.1.15",
          "form": "요즘 뭐 다 그정도 하니까 어쩔 수 없지",
          "original_form": "요즘 뭐 다 그정도 하니까 어쩔 수 없지",
          "speaker_id": "2",
          "time": "20210826 03:17"
        },
        {
          "id": "MDRW2100015938.1.16",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210826 03:17"
        },
        {
          "id": "MDRW2100015938.1.17",
          "form": "최소주문금액은 있어?",
          "original_form": "최소주문금액은 있어?",
          "speaker_id": "2",
          "time": "20210826 03:18"
        },
        {
          "id": "MDRW2100015938.1.18",
          "form": "요즘 거의 다 있지",
          "original_form": "요즘 거의 다 있지",
          "speaker_id": "1",
          "time": "20210826 03:18"
        },
        {
          "id": "MDRW2100015938.1.19",
          "form": "얼만데?",
          "original_form": "얼만데?",
          "speaker_id": "2",
          "time": "20210826 03:18"
        },
        {
          "id": "MDRW2100015938.1.20",
          "form": "15000원이야",
          "original_form": "15000원이야",
          "speaker_id": "1",
          "time": "20210826 03:18"
        },
        {
          "id": "MDRW2100015938.1.21",
          "form": "덮밥 하나씩 시키면 되겠네",
          "original_form": "덮밥 하나씩 시키면 되겠네",
          "speaker_id": "2",
          "time": "20210826 03:18"
        },
        {
          "id": "MDRW2100015938.1.22",
          "form": "1000원 모잘란데?",
          "original_form": "1000원 모잘란데?",
          "speaker_id": "1",
          "time": "20210826 03:18"
        },
        {
          "id": "MDRW2100015938.1.23",
          "form": "음료나 사이드메뉴 시키면되지",
          "original_form": "음료나 사이드메뉴 시키면되지",
          "speaker_id": "2",
          "time": "20210826 03:18"
        },
        {
          "id": "MDRW2100015938.1.24",
          "form": "그럼 콜라로 할게",
          "original_form": "그럼 콜라로 할게",
          "speaker_id": "1",
          "time": "20210826 03:18"
        }
      ]
