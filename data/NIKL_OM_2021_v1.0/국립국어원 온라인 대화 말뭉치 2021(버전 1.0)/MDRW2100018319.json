        },
        {
          "id": "MDRW2100018319.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018319.1.4",
          "form": "오 그러셨구나",
          "original_form": "오 그러셨구나",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018319.1.5",
          "form": "저 오늘 향수ㅛㅏㅆ어요!",
          "original_form": "저 오늘 향수ㅛㅏㅆ어요!",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018319.1.6",
          "form": "자랑 해",
          "original_form": "자랑 해",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018319.1.7",
          "form": "주세요",
          "original_form": "주세요",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018319.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018319.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018319.1.10",
          "form": "저는 우디나 풀향 좋아하는데",
          "original_form": "저는 우디나 풀향 좋아하는데",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018319.1.11",
          "form": "러쉬에서 바디스프레이 샀거든요",
          "original_form": "러쉬에서 바디스프레이 샀거든요",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018319.1.12",
          "form": "꽃집 들어가면 나는 그 생생한 풀냄새가 나요!",
          "original_form": "꽃집 들어가면 나는 그 생생한 풀냄새가 나요!",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018319.1.13",
          "form": "가디언오브더 포레스트예요 두근",
          "original_form": "가디언오브더 포레스트예요 두근",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018319.1.14",
          "form": "풀향 지나가면 장미꽃잎 뜯어서 맡는 향 나요",
          "original_form": "풀향 지나가면 장미꽃잎 뜯어서 맡는 향 나요",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100018319.1.15",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018319.1.16",
          "form": "채팅만봐도 제가다좋네요",
          "original_form": "채팅만봐도 제가다좋네요",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018319.1.17",
          "form": "자연그자체",
          "original_form": "자연그자체",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018319.1.18",
          "form": "ㅜㅜㅜ",
          "original_form": "ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018319.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋname2은 어떤향 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ&name2&은 어떤향 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018319.1.20",
          "form": "저는 안따지는데",
          "original_form": "저는 안따지는데",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018319.1.21",
          "form": "젤좋았던건 더바디샵 머스크랑",
          "original_form": "젤좋았던건 더바디샵 머스크랑",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018319.1.22",
          "form": "아우 너무좋죠",
          "original_form": "아우 너무좋죠",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018319.1.23",
          "form": "지금 향초키는거 양키캔들의 바하마브리즈요",
          "original_form": "지금 향초키는거 양키캔들의 바하마브리즈요",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018319.1.24",
          "form": "달콤해요",
          "original_form": "달콤해요",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018319.1.25",
          "form": "오오오 그러셨구나!",
          "original_form": "오오오 그러셨구나!",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018319.1.26",
          "form": "바하마브리즈 담애 맡아봐야겠어요",
          "original_form": "바하마브리즈 담애 맡아봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018319.1.27",
          "form": "두근듀근",
          "original_form": "두근듀근",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018319.1.28",
          "form": "저는 단종된 터코이즈스카이랑",
          "original_form": "저는 단종된 터코이즈스카이랑",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018319.1.29",
