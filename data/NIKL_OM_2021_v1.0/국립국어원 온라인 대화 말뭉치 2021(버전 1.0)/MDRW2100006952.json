      "utterance": [
        {
          "id": "MDRW2100006952.1.1",
          "form": "다이어트 해본적 있으세요?",
          "original_form": "다이어트 해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006952.1.2",
          "form": "인생이 다이어트죠.",
          "original_form": "인생이 다이어트죠.",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006952.1.3",
          "form": "항상 생각만 해요. ㅠㅠ",
          "original_form": "항상 생각만 해요. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006952.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 전 한번 해봤었어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 전 한번 해봤었어요",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006952.1.5",
          "form": "한번 했는데 안먹으니까 빠지긴 하더라구요",
          "original_form": "한번 했는데 안먹으니까 빠지긴 하더라구요",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006952.1.6",
          "form": "얼마전에 출산해서",
          "original_form": "얼마전에 출산해서",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006952.1.7",
          "form": "이제는 진짜 다이어트 해야하는데..",
          "original_form": "이제는 진짜 다이어트 해야하는데..",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006952.1.8",
          "form": "허얼 축하드려요!!!",
          "original_form": "허얼 축하드려요!!!",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006952.1.9",
          "form": "게을러서 아직 시작을 못하고 있어요.",
          "original_form": "게을러서 아직 시작을 못하고 있어요.",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006952.1.10",
          "form": "얼마나 되셨어요? 애기 낳은 지",
          "original_form": "얼마나 되셨어요? 애기 낳은 지",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006952.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 힘들죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 힘들죠",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006952.1.12",
          "form": "아기 3월에 낳고.. 이제 170일 가까이되니까",
          "original_form": "아기 3월에 낳고.. 이제 170일 가까이되니까",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006952.1.13",
          "form": "진짜 해야하거든요.ㅠㅠㅠㅠㅠㅠ",
          "original_form": "진짜 해야하거든요.ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006952.1.14",
          "form": "아아 한참 힘들때 같아요 ㅎㅇㅌ",
          "original_form": "아아 한참 힘들때 같아요 ㅎㅇㅌ",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006952.1.15",
          "form": "천천히 하세요. 아직 힘들때 같은데",
          "original_form": "천천히 하세요. 아직 힘들때 같은데",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006952.1.16",
          "form": "그래도 지금 안하면 더 빼기 힘들텐데.. 싶어서",
          "original_form": "그래도 지금 안하면 더 빼기 힘들텐데.. 싶어서",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006952.1.17",
          "form": "하긴 그건 그렇죠",
          "original_form": "하긴 그건 그렇죠",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006952.1.18",
          "form": "그런말 하시면 자꾸 미루고 싶잖아요..ㅎㅎ",
          "original_form": "그런말 하시면 자꾸 미루고 싶잖아요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006952.1.19",
          "form": "ㅋㅋㅋㅋㅋ 아니 그랴두 무리하면 안되니까",
          "original_form": "ㅋㅋㅋㅋㅋ 아니 그랴두 무리하면 안되니까",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006952.1.20",
          "form": "사실 숨만 쉬고 있어도 무리같기는 한데..ㅋㅋㅋㅋ",
          "original_form": "사실 숨만 쉬고 있어도 무리같기는 한데..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006952.1.21",
          "form": "자꾸 다이어트의 마음은 약하지네요...",
          "original_form": "자꾸 다이어트의 마음은 약하지네요...",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006952.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ 먹는 건 넘 줄이지 마시구",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 먹는 건 넘 줄이지 마시구",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006952.1.23",
          "form": "마자요 다눁 넘 어려워요",
          "original_form": "마자요 다눁 넘 어려워요",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006952.1.24",
          "form": "운동을 해보세요!",
          "original_form": "운동을 해보세요!",
          "speaker_id": "2",
          "time": "20210822 00:08"
        }
      ]
    }
