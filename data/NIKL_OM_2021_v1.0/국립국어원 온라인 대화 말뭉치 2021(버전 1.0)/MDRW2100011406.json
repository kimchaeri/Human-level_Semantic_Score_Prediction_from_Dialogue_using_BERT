      "utterance": [
        {
          "id": "MDRW2100011406.1.1",
          "form": "식사는하셨나요",
          "original_form": "식사는하셨나요",
          "speaker_id": "1",
          "time": "20210824 12:13"
        },
        {
          "id": "MDRW2100011406.1.2",
          "form": "네 비도 오고 그래서 칼국수먹었네요",
          "original_form": "네 비도 오고 그래서 칼국수먹었네요",
          "speaker_id": "2",
          "time": "20210824 12:13"
        },
        {
          "id": "MDRW2100011406.1.3",
          "form": "저는 식사메뉴고민중이에요",
          "original_form": "저는 식사메뉴고민중이에요",
          "speaker_id": "1",
          "time": "20210824 12:14"
        },
        {
          "id": "MDRW2100011406.1.4",
          "form": "점심 식사니깐 맛난거 드셔요ㅎㅎ",
          "original_form": "점심 식사니깐 맛난거 드셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:14"
        },
        {
          "id": "MDRW2100011406.1.5",
          "form": "직접 해먹으려고요. 메뉴추천해주세요~",
          "original_form": "직접 해먹으려고요. 메뉴추천해주세요~",
          "speaker_id": "1",
          "time": "20210824 12:15"
        },
        {
          "id": "MDRW2100011406.1.6",
          "form": "비많이오니깐 국물요리는 어때요?",
          "original_form": "비많이오니깐 국물요리는 어때요?",
          "speaker_id": "2",
          "time": "20210824 12:15"
        },
        {
          "id": "MDRW2100011406.1.7",
          "form": "좋죠 칼국수 직접해서 드신건가요?",
          "original_form": "좋죠 칼국수 직접해서 드신건가요?",
          "speaker_id": "1",
          "time": "20210824 12:15"
        },
        {
          "id": "MDRW2100011406.1.8",
          "form": "네 직접요리했어요ㅎㅎ",
          "original_form": "네 직접요리했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:16"
        },
        {
          "id": "MDRW2100011406.1.9",
          "form": "조리법 알 수있을까요?안해봐서",
          "original_form": "조리법 알 수있을까요?안해봐서",
          "speaker_id": "1",
          "time": "20210824 12:17"
        },
        {
          "id": "MDRW2100011406.1.10",
          "form": "재료는 다 있나요?",
          "original_form": "재료는 다 있나요?",
          "speaker_id": "2",
          "time": "20210824 12:17"
        },
        {
          "id": "MDRW2100011406.1.11",
          "form": "칼국수면이랑 야채랑 해산물 조금요ㅎㅎ",
          "original_form": "칼국수면이랑 야채랑 해산물 조금요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:18"
        },
        {
          "id": "MDRW2100011406.1.12",
          "form": "시간이 많이 걸릴까요?",
          "original_form": "시간이 많이 걸릴까요?",
          "speaker_id": "1",
          "time": "20210824 12:18"
        },
        {
          "id": "MDRW2100011406.1.13",
          "form": "30분내외요",
          "original_form": "30분내외요",
          "speaker_id": "2",
          "time": "20210824 12:19"
        },
        {
          "id": "MDRW2100011406.1.14",
          "form": "육수내고 면넣고 야채,해산물 끓임되요",
          "original_form": "육수내고 면넣고 야채,해산물 끓임되요",
          "speaker_id": "2",
          "time": "20210824 12:20"
        },
        {
          "id": "MDRW2100011406.1.15",
          "form": "다른 양념은요?",
          "original_form": "다른 양념은요?",
          "speaker_id": "1",
          "time": "20210824 12:20"
        },
        {
          "id": "MDRW2100011406.1.16",
          "form": "저는 다른 양념은 안하고 후추만 살짝해요",
          "original_form": "저는 다른 양념은 안하고 후추만 살짝해요",
          "speaker_id": "2",
          "time": "20210824 12:21"
        }
      ]
