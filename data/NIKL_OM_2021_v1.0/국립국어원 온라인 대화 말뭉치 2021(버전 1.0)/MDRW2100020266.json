      "utterance": [
        {
          "id": "MDRW2100020266.1.1",
          "form": "아이스크림 먹고 싶다",
          "original_form": "아이스크림 먹고 싶다",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.2",
          "form": "여름엔 아이스크림이지",
          "original_form": "여름엔 아이스크림이지",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.3",
          "form": "나는 겨울에도 먹어",
          "original_form": "나는 겨울에도 먹어",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.4",
          "form": "아이스크림은 나뚜루 도 맛있어",
          "original_form": "아이스크림은 나뚜루 도 맛있어",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.5",
          "form": "맛있긴 한데 좀 비싸",
          "original_form": "맛있긴 한데 좀 비싸",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.6",
          "form": "너가 전에 준 기프티콘으로 맛있게 먹었어",
          "original_form": "너가 전에 준 기프티콘으로 맛있게 먹었어",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.7",
          "form": "생일 때 준거 썼구나",
          "original_form": "생일 때 준거 썼구나",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.8",
          "form": "아이스크림 케이크 이뻐 고 맛있더라",
          "original_form": "아이스크림 케이크 이뻐 고 맛있더라",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.9",
          "form": "나도 아직 베스킨라빈스 아이스크림 쿠폰 써야 되는 거 있는데",
          "original_form": "나도 아직 베스킨라빈스 아이스크림 쿠폰 써야 되는 거 있는데",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.10",
          "form": "이번에 나온 식 메뉴 맛이다던데 지금 쓰는 게 어때",
          "original_form": "이번에 나온 식 메뉴 맛이다던데 지금 쓰는 게 어때",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020266.1.11",
          "form": "내일 한번 가봐야겠다",
          "original_form": "내일 한번 가봐야겠다",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020266.1.12",
          "form": "가는 길의 샌드위치도 사가자",
          "original_form": "가는 길의 샌드위치도 사가자",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020266.1.13",
          "form": "샌드위치 좋지",
          "original_form": "샌드위치 좋지",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020266.1.14",
          "form": "내일 먹을 거 많네",
          "original_form": "내일 먹을 거 많네",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
