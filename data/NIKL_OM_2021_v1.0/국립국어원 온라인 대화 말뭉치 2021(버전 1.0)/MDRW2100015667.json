      "utterance": [
        {
          "id": "MDRW2100015667.1.1",
          "form": "안녕하세요! 무슨 볶음밥 좋아하세요?",
          "original_form": "안녕하세요! 무슨 볶음밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015667.1.2",
          "form": "김치볶음밥도 좋구",
          "original_form": "김치볶음밥도 좋구",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015667.1.3",
          "form": "모든 음식의 끝 볶음밥을 조아해요",
          "original_form": "모든 음식의 끝 볶음밥을 조아해요",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015667.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 볶음밥에 어울리는 국은 뭘까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 볶음밥에 어울리는 국은 뭘까요",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015667.1.5",
          "form": "급식에선 항상 계란국 나왔던 거 같은데",
          "original_form": "급식에선 항상 계란국 나왔던 거 같은데",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015667.1.6",
          "form": "좋아하는 김볶밥 레시피 있나요?",
          "original_form": "좋아하는 김볶밥 레시피 있나요?",
          "speaker_id": "1",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015667.1.7",
          "form": "저는 베이컨 넣는거 좋아해용",
          "original_form": "저는 베이컨 넣는거 좋아해용",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100015667.1.8",
          "form": "잘게 썰어서",
          "original_form": "잘게 썰어서",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100015667.1.9",
          "form": "저녁밥 볶음밥 드실건가요?",
          "original_form": "저녁밥 볶음밥 드실건가요?",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100015667.1.10",
          "form": "아뇨ㅋㅋ",
          "original_form": "아뇨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100015667.1.11",
          "form": "샌드위치 먹었어요",
          "original_form": "샌드위치 먹었어요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100015667.1.12",
          "form": "직접 만드셨어요?",
          "original_form": "직접 만드셨어요?",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100015667.1.13",
          "form": "아뇨 사먹었어용ㅋㅋㅋ",
          "original_form": "아뇨 사먹었어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100015667.1.14",
          "form": "저녁에 볶음밥 드셨나요?",
          "original_form": "저녁에 볶음밥 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100015667.1.15",
          "form": "아뇨~ 오늘은 애매해서 저녁 안 먹었어요",
          "original_form": "아뇨~ 오늘은 애매해서 저녁 안 먹었어요",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100015667.1.16",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100015667.1.17",
          "form": "저도 점심먹은게 소화 안돼서 안먹었어요!",
          "original_form": "저도 점심먹은게 소화 안돼서 안먹었어요!",
