      "utterance": [
        {
          "id": "MDRW2100013067.1.1",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100013067.1.2",
          "form": "아니 그리고 웃긴게",
          "original_form": "아니 그리고 웃긴게",
          "speaker_id": "2",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100013067.1.3",
          "form": "김춘수가 잘났냐고 하는데",
          "original_form": "김춘수가 잘났냐고 하는데",
          "speaker_id": "2",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100013067.1.4",
          "form": "이 사람 오디컴퍼니 대표임",
          "original_form": "이 사람 오디컴퍼니 대표임",
          "speaker_id": "2",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100013067.1.5",
          "form": "지앤하 같은 대형극 올리는 사람임",
          "original_form": "지앤하 같은 대형극 올리는 사람임",
          "speaker_id": "2",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100013067.1.6",
          "form": "대표한테그런식으로",
          "original_form": "대표한테그런식으로",
          "speaker_id": "1",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100013067.1.7",
          "form": "말한거임?",
          "original_form": "말한거임?",
          "speaker_id": "1",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100013067.1.8",
          "form": "예의도없고",
          "original_form": "예의도없고",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013067.1.9",
          "form": "생각으도없고",
          "original_form": "생각으도없고",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013067.1.10",
          "form": "ㅇㅇ 검선미도 거기 총괄인가 그럼",
          "original_form": "ㅇㅇ 검선미도 거기 총괄인가 그럼",
          "speaker_id": "2",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013067.1.11",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013067.1.12",
          "form": "못하면그냥연습이나더하지",
          "original_form": "못하면그냥연습이나더하지",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013067.1.13",
          "form": "맡은 배역이 예전의 아이돌인가 그런건데",
          "original_form": "맡은 배역이 예전의 아이돌인가 그런건데",
          "speaker_id": "2",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013067.1.14",
          "form": "상관없는사람저격이나하네",
          "original_form": "상관없는사람저격이나하네",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013067.1.15",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013067.1.16",
          "form": "존심이나 부리고 불호 후기 쓴 인스타 찾아가서 뭐라하고 내리라 그러고",
          "original_form": "존심이나 부리고 불호 후기 쓴 인스타 찾아가서 뭐라하고 내리라 그러고",
          "speaker_id": "2",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013067.1.17",
          "form": "팬들도 찾아가서 뭐라 그러고",
          "original_form": "팬들도 찾아가서 뭐라 그러고",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013067.1.18",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013067.1.19",
          "form": "그런사람팬이있음?",
          "original_form": "그런사람팬이있음?",
          "speaker_id": "1",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013067.1.20",
          "form": "그걸왜쉴드쳐주는거지",
          "original_form": "그걸왜쉴드쳐주는거지",
          "speaker_id": "1",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013067.1.21",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013067.1.22",
          "form": "개인감상인데왜지우라마라임",
          "original_form": "개인감상인데왜지우라마라임",
          "speaker_id": "1",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013067.1.23",
          "form": "무슨욕을한것도",
          "original_form": "무슨욕을한것도",
          "speaker_id": "1",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013067.1.24",
          "form": "아니고",
          "original_form": "아니고",
          "speaker_id": "1",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013067.1.25",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100013067.1.26",
          "form": "비난이랑 비판을 구별 못하나봄",
          "original_form": "비난이랑 비판을 구별 못하나봄",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100013067.1.27",
          "form": "평생뮤지컬안했으면",
          "original_form": "평생뮤지컬안했으면",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100013067.1.28",
          "form": "그런사람은",
          "original_form": "그런사람은",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100013067.1.29",
          "form": "ㅇㅈㅇㅈ 뮤지컬을 너무 쉽게 봐",
          "original_form": "ㅇㅈㅇㅈ 뮤지컬을 너무 쉽게 봐",
          "speaker_id": "2",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100013067.1.30",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "1",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100013067.1.31",
          "form": "아니 반값 운운한 것도 어이없음",
          "original_form": "아니 반값 운운한 것도 어이없음",
          "speaker_id": "2",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100013067.1.32",
          "form": "보러가는데가격이무슨상관임",
          "original_form": "보러가는데가격이무슨상관임",
          "speaker_id": "1",
