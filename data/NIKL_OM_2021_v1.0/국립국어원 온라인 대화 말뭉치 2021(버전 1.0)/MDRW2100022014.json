      "utterance": [
        {
          "id": "MDRW2100022014.1.1",
          "form": "저 오늘 이수역에서 타코 먹고 왔어요 ㅋㅋㅋㅋ",
          "original_form": "저 오늘 이수역에서 타코 먹고 왔어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022014.1.2",
          "form": "올라아보! 여기 제가 어제 알려드렸었나요",
          "original_form": "올라아보! 여기 제가 어제 알려드렸었나요",
          "speaker_id": "2",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022014.1.3",
          "form": "아뇨!! 올라아보는 말씀없으셨어요!",
          "original_form": "아뇨!! 올라아보는 말씀없으셨어요!",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022014.1.4",
          "form": "아 여기도 아주 맛있어요ㅋㅋㅋ",
          "original_form": "아 여기도 아주 맛있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022014.1.5",
          "form": "딱 잘 익은 아보카도를 먹고 올 수 있어요",
          "original_form": "딱 잘 익은 아보카도를 먹고 올 수 있어요",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022014.1.6",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022014.1.7",
          "form": "제가 아보카도는 ....",
          "original_form": "제가 아보카도는 ....",
          "speaker_id": "1",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022014.1.8",
          "form": "앗 혹시 아보카도 불호 이신가요",
          "original_form": "앗 혹시 아보카도 불호 이신가요",
          "speaker_id": "2",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022014.1.9",
          "form": "네엥 .....ㅠㅅ ㅠ",
          "original_form": "네엥 .....ㅠㅅ ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022014.1.10",
          "form": "그럼 혹시 과카몰리도 안 좋아하세요?",
          "original_form": "그럼 혹시 과카몰리도 안 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022014.1.11",
          "form": "앗 한번도 안먹어봤어요!",
          "original_form": "앗 한번도 안먹어봤어요!",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022014.1.12",
          "form": "아보카도 으깨서 만든 소스예욥...",
          "original_form": "아보카도 으깨서 만든 소스예욥...",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022014.1.13",
          "form": "토마토랑 양파가 들어간...!",
          "original_form": "토마토랑 양파가 들어간...!",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022014.1.14",
          "form": "아아..!",
          "original_form": "아아..!",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022014.1.15",
          "form": "토마토랑 양파는 좋아해서",
          "original_form": "토마토랑 양파는 좋아해서",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022014.1.16",
          "form": "기회가 된다면 한번 먹어볼게요!!",
          "original_form": "기회가 된다면 한번 먹어볼게요!!",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022014.1.17",
          "form": "좋아요 좋아요 이왕이면 (name2에게) 검증된 맛집에서!",
          "original_form": "좋아요 좋아요 이왕이면 (&name2&에게) 검증된 맛집에서!",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100022014.1.18",
          "form": "고수는 잘 드세요?",
          "original_form": "고수는 잘 드세요?",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100022014.1.19",
          "form": "아뇨...퓨ㅠㅠ",
          "original_form": "아뇨...퓨ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100022014.1.20",
          "form": "쌀국수 이런 류 정말 좋아하는데",
          "original_form": "쌀국수 이런 류 정말 좋아하는데",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100022014.1.21",
          "form": "고수는...헤헤",
          "original_form": "고수는...헤헤",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100022014.1.22",
          "form": "아아앗 그럼 멕시코 음식이 쉽지 않으실수도....",
          "original_form": "아아앗 그럼 멕시코 음식이 쉽지 않으실수도....",
          "speaker_id": "2",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100022014.1.23",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100022014.1.24",
          "form": "고수가 들어가나요?",
          "original_form": "고수가 들어가나요?",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100022014.1.25",
          "form": "빼달라고 하면 뺄 수는 있는데",
          "original_form": "빼달라고 하면 뺄 수는 있는데",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100022014.1.26",
          "form": "쿠차라 부리또볼은 밥에 고수가 아예 들어있어요",
          "original_form": "쿠차라 부리또볼은 밥에 고수가 아예 들어있어요",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100022014.1.27",
          "form": "아악...",
          "original_form": "아악...",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100022014.1.28",
          "form": "힝",
          "original_form": "힝",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100022014.1.29",
          "form": "쿠차라만,, 빼고",
          "original_form": "쿠차라만,, 빼고",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100022014.1.30",
          "form": "근데 저도 원래 고수 안 먹었다가 쿠차로 먹고 고수 입문했거든요ㅋㅋㅋ 그만큼 맛있는 쿠차라..",
          "original_form": "근데 저도 원래 고수 안 먹었다가 쿠차로 먹고 고수 입문했거든요ㅋㅋㅋ 그만큼 맛있는 쿠차라..",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100022014.1.31",
          "form": "타코로코를 가볼게요..",
          "original_form": "타코로코를 가볼게요..",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100022014.1.32",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100022014.1.33",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100022014.1.34",
          "form": "네 네 쿠차라 정말 엄청나요 후후",
          "original_form": "네 네 쿠차라 정말 엄청나요 후후",
          "speaker_id": "2",
          "time": "20210829 21:33"
        }
      ]
