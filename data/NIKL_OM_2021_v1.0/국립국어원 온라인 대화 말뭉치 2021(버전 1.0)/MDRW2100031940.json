      "utterance": [
        {
          "id": "MDRW2100031940.1.1",
          "form": "아침은 뭘 주로 드시나요",
          "original_form": "아침은 뭘 주로 드시나요",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100031940.1.2",
          "form": "빵 커피 드십니까",
          "original_form": "빵 커피 드십니까",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100031940.1.3",
          "form": "아니면 어 쌀밥 드시나요",
          "original_form": "아니면 어 쌀밥 드시나요",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100031940.1.4",
          "form": "밥 주로 먹어요",
          "original_form": "밥 주로 먹어요",
          "speaker_id": "1",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100031940.1.5",
          "form": "아우",
          "original_form": "아우",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100031940.1.6",
          "form": "밥이 좋지요",
          "original_form": "밥이 좋지요",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100031940.1.7",
          "form": "예전에 와이프가 솥밥",
          "original_form": "예전에 와이프가 솥밥",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100031940.1.8",
          "form": "소주 하나 샀어요 솥 솥",
          "original_form": "소주 하나 샀어요 솥 솥",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100031940.1.9",
          "form": "네 ㅎㅎㅎ",
          "original_form": "네 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031940.1.10",
          "form": "을 사서 밥을 합니다",
          "original_form": "을 사서 밥을 합니다",
          "speaker_id": "2",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031940.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031940.1.12",
          "form": "ㅋㅋ네",
          "original_form": "ㅋㅋ네",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031940.1.13",
          "form": "솥이 20만 원이었어요",
          "original_form": "솥이 20만 원이었어요",
          "speaker_id": "2",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031940.1.14",
          "form": "본전을 하면은 수무 번에 해먹어야 됩니다",
          "original_form": "본전을 하면은 수무 번에 해먹어야 됩니다",
          "speaker_id": "2",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031940.1.15",
          "form": "스무번",
          "original_form": "스무번",
          "speaker_id": "2",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031940.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031940.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031940.1.18",
          "form": "그렇게 비싼 냄비 사서",
          "original_form": "그렇게 비싼 냄비 사서",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031940.1.19",
          "form": "친구들끼리 자랑해야 되는 걸 지도요",
          "original_form": "친구들끼리 자랑해야 되는 걸 지도요",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031940.1.20",
          "form": "무슨 인 스탄 지 별 스탄 지 거기 올립니다",
          "original_form": "무슨 인 스탄 지 별 스탄 지 거기 올립니다",
          "speaker_id": "2",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100031940.1.21",
          "form": "밥해가지고 올리고",
          "original_form": "밥해가지고 올리고",
          "speaker_id": "2",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100031940.1.22",
          "form": "너무 좋아요우 좋아요 맛있어서",
          "original_form": "너무 좋아요우 좋아요 맛있어서",
          "speaker_id": "2",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031940.1.23",
          "form": "맛있어요 짱짱짱맨",
          "original_form": "맛있어요 짱짱짱맨",
          "speaker_id": "2",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031940.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031940.1.25",
          "form": "웹툰 팔이피플 추천드립니다",
          "original_form": "웹툰 팔이피플 추천드립니다",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031940.1.26",
          "form": "아 웹툰을 안 봐서",
          "original_form": "아 웹툰을 안 봐서",
          "speaker_id": "2",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100031940.1.27",
          "form": "한번 봐볼게요",
          "original_form": "한번 봐볼게요",
          "speaker_id": "2",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100031940.1.28",
          "form": "한번 찾아보세요 공감 가실 거 같아요",
          "original_form": "한번 찾아보세요 공감 가실 거 같아요",
          "speaker_id": "1",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100031940.1.29",
          "form": "오 메모",
          "original_form": "오 메모",
          "speaker_id": "2",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100031940.1.30",
          "form": "아침밥은 어떠셨어요",
          "original_form": "아침밥은 어떠셨어요",
