      "utterance": [
        {
          "id": "MDRW2100021901.1.1",
          "form": "우리 지난번에 갔다온데 어디지?",
          "original_form": "우리 지난번에 갔다온데 어디지?",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021901.1.2",
          "form": "어디?  얼만전에?",
          "original_form": "어디?  얼만전에?",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021901.1.3",
          "form": "순천?",
          "original_form": "순천?",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021901.1.4",
          "form": "순천 맞나?",
          "original_form": "순천 맞나?",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021901.1.5",
          "form": "어 맞다 거기",
          "original_form": "어 맞다 거기",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100021901.1.6",
          "form": "갑자기 왜 근데?",
          "original_form": "갑자기 왜 근데?",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100021901.1.7",
          "form": "아니 사진 좀 정리하게",
          "original_form": "아니 사진 좀 정리하게",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021901.1.8",
          "form": "우리 사진 별로 안 찍지 않았나?",
          "original_form": "우리 사진 별로 안 찍지 않았나?",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021901.1.9",
          "form": "거의 차에서만 있어서ㅠㅜ",
          "original_form": "거의 차에서만 있어서ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021901.1.10",
          "form": "그래도 몇장 찍은거 폴더에 올려놓게",
          "original_form": "그래도 몇장 찍은거 폴더에 올려놓게",
          "speaker_id": "2",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021901.1.11",
          "form": "오옹 나 순천만 국가정원 가보고싶었는데ㅠㅜ",
          "original_form": "오옹 나 순천만 국가정원 가보고싶었는데ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021901.1.12",
          "form": "다음에 코로나 좀 시들할때 다시 가자",
          "original_form": "다음에 코로나 좀 시들할때 다시 가자",
          "speaker_id": "2",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021901.1.13",
          "form": "ㅇㅇ빨리 좀 나아짐 좋겠다",
          "original_form": "ㅇㅇ빨리 좀 나아짐 좋겠다",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021901.1.14",
          "form": "담에 순천 감 도토리묵집도 가자",
          "original_form": "담에 순천 감 도토리묵집도 가자",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021901.1.15",
          "form": "그래 이번에 간 음식점은 별로더라 사람 없는데 골라 가서 그런가",
          "original_form": "그래 이번에 간 음식점은 별로더라 사람 없는데 골라 가서 그런가",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021901.1.16",
          "form": "사람들 몰리는 시간 피해서 먹고 빠지잨ㅋㅋ",
          "original_form": "사람들 몰리는 시간 피해서 먹고 빠지잨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:59"
        }
      ]
