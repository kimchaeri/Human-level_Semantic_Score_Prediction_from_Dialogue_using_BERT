        {
          "id": "MDRW2100033970.1.1",
          "form": "버거킹 특유의 패티가 넘맛나용 ㅎㅎ",
          "original_form": "버거킹 특유의 패티가 넘맛나용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033970.1.2",
          "form": "그중",
          "original_form": "그중",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033970.1.3",
          "form": "최애는 뭔가요?",
          "original_form": "최애는 뭔가요?",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033970.1.4",
          "form": "치즈와퍼용 !",
          "original_form": "치즈와퍼용 !",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033970.1.5",
          "form": "좋아하는 버거있오요?",
          "original_form": "좋아하는 버거있오요?",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033970.1.6",
          "form": "전 슈슈",
          "original_form": "전 슈슈",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100033970.1.7",
          "form": "슈비버거요ㅋㅋㅋ",
          "original_form": "슈비버거요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100033970.1.8",
          "form": "맛나용?",
          "original_form": "맛나용?",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100033970.1.9",
          "form": "네네 맛있어요!",
          "original_form": "네네 맛있어요!",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100033970.1.10",
          "form": "먹어봐야겠네요!",
          "original_form": "먹어봐야겠네요!",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100033970.1.11",
          "form": "맥주vs소주? 어떠세요?",
          "original_form": "맥주vs소주? 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100033970.1.12",
          "form": "소주요 님은 ?",
          "original_form": "소주요 님은 ?",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100033970.1.13",
          "form": "전 맥주요ㅋㅋㅋ",
          "original_form": "전 맥주요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100033970.1.14",
          "form": "낼 출근인뎅 술얘기하니 먹고싶네여 ㅠ",
          "original_form": "낼 출근인뎅 술얘기하니 먹고싶네여 ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100033970.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100033970.1.16",
