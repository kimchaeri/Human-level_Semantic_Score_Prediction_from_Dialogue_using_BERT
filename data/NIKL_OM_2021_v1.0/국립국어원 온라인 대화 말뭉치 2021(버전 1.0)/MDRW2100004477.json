      "utterance": [
        {
          "id": "MDRW2100004477.1.1",
          "form": "ㅋㅋㅋㅋ전 지금도 치킨시켜서",
          "original_form": "ㅋㅋㅋㅋ전 지금도 치킨시켜서",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004477.1.2",
          "form": "한잔중입니다",
          "original_form": "한잔중입니다",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004477.1.3",
          "form": "맥주인가요? 치맥 최고죠",
          "original_form": "맥주인가요? 치맥 최고죠",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004477.1.4",
          "form": "ㅋㅋㅋ 치쏘맥입니당",
          "original_form": "ㅋㅋㅋ 치쏘맥입니당",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004477.1.5",
          "form": "헉 술 잘 드시나봐요 치킨 어디에서 시켰어용?",
          "original_form": "헉 술 잘 드시나봐요 치킨 어디에서 시켰어용?",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004477.1.6",
          "form": "비비큐요 !",
          "original_form": "비비큐요 !",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004477.1.7",
          "form": "기프티콘이있어서 ㅎ",
          "original_form": "기프티콘이있어서 ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004477.1.8",
          "form": "저녁 뭐드셔요",
          "original_form": "저녁 뭐드셔요",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004477.1.9",
          "form": "비비큐 맛있겠네요 전 얼마전에 노랑통닭 첨 먹어봤어용",
          "original_form": "비비큐 맛있겠네요 전 얼마전에 노랑통닭 첨 먹어봤어용",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004477.1.10",
          "form": "저녁 아직 먹기 전이라 뭐 먹을지 고민 중입니다ㅋㅋㅋ",
          "original_form": "저녁 아직 먹기 전이라 뭐 먹을지 고민 중입니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004477.1.11",
          "form": "오 여기도 촌이라 최근에 노랑통닭이 생겨서",
          "original_form": "오 여기도 촌이라 최근에 노랑통닭이 생겨서",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004477.1.12",
          "form": "먹어봤는데 되게 카레맛이 강하더라구욬ㅋ",
          "original_form": "먹어봤는데 되게 카레맛이 강하더라구욬ㅋ",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004477.1.13",
          "form": "오 그렇군요 저 알싸한 마늘통닭인가 그거 시켜서 소스에 거의 담궈먹어가지구ㅋㅋㅋㅋ 마늘맛만 잔뜩 느꼈어요",
          "original_form": "오 그렇군요 저 알싸한 마늘통닭인가 그거 시켜서 소스에 거의 담궈먹어가지구ㅋㅋㅋㅋ 마늘맛만 잔뜩 느꼈어요",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004477.1.14",
          "form": "ㅋㅋㅋㅋㅋ 최애 배달음식 뭐예요?",
          "original_form": "ㅋㅋㅋㅋㅋ 최애 배달음식 뭐예요?",
          "speaker_id": "1",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004477.1.15",
          "form": "저 요즘은 초밥이요 최애  초밥집 있는데  짱 맛있어서 가끔씩 우울할 때마다 시켜요",
          "original_form": "저 요즘은 초밥이요 최애  초밥집 있는데  짱 맛있어서 가끔씩 우울할 때마다 시켜요",
          "speaker_id": "2",
          "time": "20210821 19:07"
        }
      ]
