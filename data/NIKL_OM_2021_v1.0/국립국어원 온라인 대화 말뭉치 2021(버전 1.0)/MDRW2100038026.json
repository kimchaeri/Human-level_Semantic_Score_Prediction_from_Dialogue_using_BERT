      "utterance": [
        {
          "id": "MDRW2100038026.1.1",
          "form": "블랜딩 차를 파는곳인데",
          "original_form": "블랜딩 차를 파는곳인데",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100038026.1.2",
          "form": "새로운종류 흥미로운 종류 많아요!",
          "original_form": "새로운종류 흥미로운 종류 많아요!",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100038026.1.3",
          "form": "오오 그렇군요!!!",
          "original_form": "오오 그렇군요!!!",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038026.1.4",
          "form": "이름 다시 한 번 알려주실래요?!",
          "original_form": "이름 다시 한 번 알려주실래요?!",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038026.1.5",
          "form": "알디프예요!",
          "original_form": "알디프예요!",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038026.1.6",
          "form": "오프라인 매장에서는 차코스가 있어요",
          "original_form": "오프라인 매장에서는 차코스가 있어요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038026.1.7",
          "form": "우와 검색해보고 와야겠어요!",
          "original_form": "우와 검색해보고 와야겠어요!",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038026.1.8",
          "form": "우와 홍대에 있네요?!",
          "original_form": "우와 홍대에 있네요?!",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038026.1.9",
          "form": "진짜 꼭 가봐야겠어요 예약제네요 ",
          "original_form": "진짜 꼭 가봐야겠어요 예약제네요 ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038026.1.10",
          "form": "그죠 거기 안산선수도 가셨어가지고 예약 좀 찾을거예요",
          "original_form": "그죠 거기 안산선수도 가셨어가지고 예약 좀 찾을거예요",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038026.1.11",
          "form": "아 헉",
          "original_form": "아 헉",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038026.1.12",
          "form": "거기였군요!!!!!",
          "original_form": "거기였군요!!!!!",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038026.1.13",
          "form": "안산선수가 찻집 갔다는 말 듣긴 들었는데 알디프였을줄이야",
          "original_form": "안산선수가 찻집 갔다는 말 듣긴 들었는데 알디프였을줄이야",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038026.1.14",
          "form": "거기차를 선물용으로도 자주쓰고요",
          "original_form": "거기차를 선물용으로도 자주쓰고요",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038026.1.15",
          "form": "가끔사먹는데 저는 오디차가 제일 좋았어요",
          "original_form": "가끔사먹는데 저는 오디차가 제일 좋았어요",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038026.1.16",
          "form": "선물용 패키지도 이뻐서 선물할일있을때 애용해요",
          "original_form": "선물용 패키지도 이뻐서 선물할일있을때 애용해요",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100038026.1.17",
          "form": "우와 그렇군요",
          "original_form": "우와 그렇군요",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100038026.1.18",
          "form": "완전 꿀팁 감사해요!!!",
          "original_form": "완전 꿀팁 감사해요!!!",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100038026.1.19",
          "form": "센스 있는 사람이 될 수 있겠어요 ",
          "original_form": "센스 있는 사람이 될 수 있겠어요 ",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100038026.1.20",
          "form": "저는 차라면 twg밖에 몰랐어요 ㅋㅋㅋ",
          "original_form": "저는 차라면 twg밖에 몰랐어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100038026.1.21",
          "form": "twg도 너무 좋죠!",
          "original_form": "twg도 너무 좋죠!",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100038026.1.22",
          "form": "비싸서 가끔먹긴하지만 향 너무 좋지않나요",
          "original_form": "비싸서 가끔먹긴하지만 향 너무 좋지않나요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100038026.1.23",
          "form": "맞아요 8ㅅ8",
          "original_form": "맞아요 8ㅅ8",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100038026.1.24",
          "form": "선물하기에도 좋은 것 같아요",
          "original_form": "선물하기에도 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100038026.1.25",
          "form": "맞아요 가격대가 쎄서 망설이는데 선물받으면 기분좋았어요",
          "original_form": "맞아요 가격대가 쎄서 망설이는데 선물받으면 기분좋았어요",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100038026.1.26",
          "form": "입문용으로도 무난하구요",
          "original_form": "입문용으로도 무난하구요",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100038026.1.27",
          "form": "그 뭐지 저는 최근에",
          "original_form": "그 뭐지 저는 최근에",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100038026.1.28",
          "form": "오설록 선물 받았는데 그것도 나름 괜찮더라구요,,",
          "original_form": "오설록 선물 받았는데 그것도 나름 괜찮더라구요,,",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100038026.1.29",
          "form": "좋으시겠다! 비오는날 한잔따땃하게 마시면 포근하겠어요",
          "original_form": "좋으시겠다! 비오는날 한잔따땃하게 마시면 포근하겠어요",
          "speaker_id": "2",
