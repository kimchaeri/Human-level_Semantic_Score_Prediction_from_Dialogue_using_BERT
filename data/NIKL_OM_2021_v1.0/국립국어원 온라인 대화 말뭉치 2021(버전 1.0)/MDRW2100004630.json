      "utterance": [
        {
          "id": "MDRW2100004630.1.1",
          "form": "공연, 전시회 좋지",
          "original_form": "공연, 전시회 좋지",
          "speaker_id": "2",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004630.1.2",
          "form": "최근에는 뭐 코로나 때문에 공연이 다 취소돼서ㅠㅠ",
          "original_form": "최근에는 뭐 코로나 때문에 공연이 다 취소돼서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004630.1.3",
          "form": "맞아ㅠㅜㅠ",
          "original_form": "맞아ㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004630.1.4",
          "form": "공연하면 고등학교때 본 연극이랑 뮤지컬 생각난다",
          "original_form": "공연하면 고등학교때 본 연극이랑 뮤지컬 생각난다",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004630.1.5",
          "form": "진짜 재밌었는데",
          "original_form": "진짜 재밌었는데",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004630.1.6",
          "form": "그 서면 가서 본 거?",
          "original_form": "그 서면 가서 본 거?",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004630.1.7",
          "form": "그거 재밌었지ㅜㅜ 자리도 좋았고",
          "original_form": "그거 재밌었지ㅜㅜ 자리도 좋았고",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004630.1.8",
          "form": "서면이 아니라 남포동이였어!!",
          "original_form": "서면이 아니라 남포동이였어!!",
          "speaker_id": "2",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004630.1.9",
          "form": "자리 진짜 좋았지..",
          "original_form": "자리 진짜 좋았지..",
          "speaker_id": "2",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004630.1.10",
          "form": "심지어 재미도 있어서 더 좋았다",
          "original_form": "심지어 재미도 있어서 더 좋았다",
          "speaker_id": "2",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004630.1.11",
          "form": "아 맞네",
          "original_form": "아 맞네",
          "speaker_id": "1",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004630.1.12",
          "form": "남포동ㅜㅜ",
          "original_form": "남포동ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004630.1.13",
          "form": "고2 때.. 좋았는데 흑흑",
          "original_form": "고2 때.. 좋았는데 흑흑",
          "speaker_id": "1",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004630.1.14",
          "form": "지금까지 본 연극 중에서는",
          "original_form": "지금까지 본 연극 중에서는",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004630.1.15",
          "form": "고2때 본게 제일 재밌었던거 같다",
          "original_form": "고2때 본게 제일 재밌었던거 같다",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004630.1.16",
          "form": "난 연극을 잘 안 봐서.. 고2 때 본 게 마지막인듯..ㅜ",
          "original_form": "난 연극을 잘 안 봐서.. 고2 때 본 게 마지막인듯..ㅜ",
          "speaker_id": "1",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004630.1.17",
          "form": "연극은 아니지만",
          "original_form": "연극은 아니지만",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004630.1.18",
          "form": "뮤지컬 봤었는데",
          "original_form": "뮤지컬 봤었는데",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004630.1.19",
          "form": "어릴때 보던 파워레인저 뮤지컬이랑 차원이 다르더라..",
          "original_form": "어릴때 보던 파워레인저 뮤지컬이랑 차원이 다르더라..",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004630.1.20",
          "form": "나쁜말 안 보냈어요..",
          "original_form": "나쁜말 안 보냈어요..",
          "speaker_id": "2",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004630.1.21",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004630.1.22",
          "form": "덕후",
          "original_form": "덕후",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004630.1.23",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004630.1.24",
          "form": "뭔가 문제야!!",
          "original_form": "뭔가 문제야!!",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004630.1.25",
          "form": "뮤지컬..",
          "original_form": "뮤지컬..",
          "speaker_id": "1",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100004630.1.26",
          "form": "예전에 신흥무관학교 봤었지",
          "original_form": "예전에 신흥무관학교 봤었지",
          "speaker_id": "1",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100004630.1.27",
          "form": "성규 나오는 거..ㅋㅋㅋ",
          "original_form": "성규 나오는 거..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100004630.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004630.1.29",
          "form": "성규는 인정이지",
          "original_form": "성규는 인정이지",
          "speaker_id": "2",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004630.1.30",
          "form": "맞다.. 그때가 좋았는데ㅜㅜ",
          "original_form": "맞다.. 그때가 좋았는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004630.1.31",
          "form": "코로나도 없고..",
          "original_form": "코로나도 없고..",
          "speaker_id": "1",
