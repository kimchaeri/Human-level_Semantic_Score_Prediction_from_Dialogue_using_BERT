      "utterance": [
        {
          "id": "MDRW2100003366.1.1",
          "form": "넷플릭스 자주 보시나요?",
          "original_form": "넷플릭스 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003366.1.2",
          "form": "네ㅋㅋㅋ근데 넷플보다는 티빙을 더 많이 봐요?",
          "original_form": "네ㅋㅋㅋ근데 넷플보다는 티빙을 더 많이 봐요?",
          "speaker_id": "2",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003366.1.3",
          "form": "봐요!ㅋㅋㅋ",
          "original_form": "봐요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003366.1.4",
          "form": "티빙은 예능이나 드라마인거죠??",
          "original_form": "티빙은 예능이나 드라마인거죠??",
          "speaker_id": "1",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003366.1.5",
          "form": "추천해주실만한 것 있나요",
          "original_form": "추천해주실만한 것 있나요",
          "speaker_id": "1",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003366.1.6",
          "form": "네네 맞아요ㅋㅋ",
          "original_form": "네네 맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003366.1.7",
          "form": "저 요새 대탈출이랑 아받대 보고 있어요ㅋㅋㅋㅋ",
          "original_form": "저 요새 대탈출이랑 아받대 보고 있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003366.1.8",
          "form": "아받대....",
          "original_form": "아받대....",
          "speaker_id": "1",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003366.1.9",
          "form": "가 뭘.... 줄... 인 걸까요..?",
          "original_form": "가 뭘.... 줄... 인 걸까요..?",
          "speaker_id": "1",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003366.1.10",
          "form": "아이돌 받아쓰기 대회요ㅋㅋㅋㅋ엄청 재밌어요ㅠㅜ",
          "original_form": "아이돌 받아쓰기 대회요ㅋㅋㅋㅋ엄청 재밌어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003366.1.11",
          "form": "와 재밌어 보여요 받아쓰기를 하는 것이군요",
          "original_form": "와 재밌어 보여요 받아쓰기를 하는 것이군요",
          "speaker_id": "1",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003366.1.12",
          "form": "대탈출은 방탈출 하는건가요??",
          "original_form": "대탈출은 방탈출 하는건가요??",
          "speaker_id": "1",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003366.1.13",
          "form": "맞아요 노래 듣고 가사 받아 적는거예요!",
          "original_form": "맞아요 노래 듣고 가사 받아 적는거예요!",
          "speaker_id": "2",
          "time": "20210820 22:29"
        },
        {
          "id": "MDRW2100003366.1.14",
          "form": "대탈출은 방탈출인데 규모가 엄청나요ㅋㅋㅋㅋ",
          "original_form": "대탈출은 방탈출인데 규모가 엄청나요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:29"
        },
        {
          "id": "MDRW2100003366.1.15",
          "form": "아 그렇구나 그럼 대탈출 보면 문제도 같이 풀고 그러세요??",
          "original_form": "아 그렇구나 그럼 대탈출 보면 문제도 같이 풀고 그러세요??",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003366.1.16",
          "form": "아뇨 걍 좋아할 뿐 잘 하지는 못해요ㅋㅋㅋ",
          "original_form": "아뇨 걍 좋아할 뿐 잘 하지는 못해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003366.1.17",
          "form": "ㅋㅋㅋㅋ저는 그런 추리 예능 보면 제가 못푸는게 답답해서 잘 안보게되더라구요ㅜ",
          "original_form": "ㅋㅋㅋㅋ저는 그런 추리 예능 보면 제가 못푸는게 답답해서 잘 안보게되더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003366.1.18",
          "form": "좋아하는 연예인 잇으세요?",
          "original_form": "좋아하는 연예인 잇으세요?",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003366.1.19",
          "form": "저는 이성경 좋아해요ㅋㅋㅋ",
          "original_form": "저는 이성경 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003366.1.20",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003366.1.21",
          "form": "저는 한소희가 요즘 너무 예브더라구요",
          "original_form": "저는 한소희가 요즘 너무 예브더라구요",
          "speaker_id": "1",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003366.1.22",
          "form": "그런 이미지가 너무 좋아요",
          "original_form": "그런 이미지가 너무 좋아요",
          "speaker_id": "1",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003366.1.23",
          "form": "이성경은 어떤 계기로 관심갖게되신거예요?",
          "original_form": "이성경은 어떤 계기로 관심갖게되신거예요?",
          "speaker_id": "1",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003366.1.24",
          "form": "그냥 생긴게 너무 제 취향이예요ㅠㅜ진짜 제 워너비예요ㅋㅋㅋ",
          "original_form": "그냥 생긴게 너무 제 취향이예요ㅠㅜ진짜 제 워너비예요ㅋㅋㅋ",
