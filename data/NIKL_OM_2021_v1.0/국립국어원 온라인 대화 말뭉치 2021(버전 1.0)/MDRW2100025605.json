      "utterance": [
        {
          "id": "MDRW2100025605.1.1",
          "form": "혹시 밀도씨 빵 드셔보셨나요??,",
          "original_form": "혹시 밀도씨 빵 드셔보셨나요??,",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100025605.1.2",
          "form": "꼭드셔보레요 정말 맛있어요",
          "original_form": "꼭드셔보레요 정말 맛있어요",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100025605.1.3",
          "form": "오 아니요!!! 뭐가 맛있나요?",
          "original_form": "오 아니요!!! 뭐가 맛있나요?",
          "speaker_id": "2",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025605.1.4",
          "form": "식빵파는곳인데",
          "original_form": "식빵파는곳인데",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025605.1.5",
          "form": "꼭드셔보세요",
          "original_form": "꼭드셔보세요",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025605.1.6",
          "form": "정말 맛있어요",
          "original_form": "정말 맛있어요",
          "speaker_id": "1",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100025605.1.7",
          "form": "일반식빵이제일 맛있지만",
          "original_form": "일반식빵이제일 맛있지만",
          "speaker_id": "1",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100025605.1.8",
          "form": "생크림식빵도 정말 맛있어요",
          "original_form": "생크림식빵도 정말 맛있어요",
          "speaker_id": "1",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100025605.1.9",
          "form": "여기도 메모..",
          "original_form": "여기도 메모..",
          "speaker_id": "2",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100025605.1.10",
          "form": "생크림식빵 진짜 맛있겠네요!!",
          "original_form": "생크림식빵 진짜 맛있겠네요!!",
          "speaker_id": "2",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100025605.1.11",
          "form": "name1님은 최애음식이 뭔가요",
          "original_form": "&name1&님은 최애음식이 뭔가요",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025605.1.12",
          "form": "저는 ㅋㅋㅋㄱ 이탈리아식 피자요",
          "original_form": "저는 ㅋㅋㅋㄱ 이탈리아식 피자요",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025605.1.13",
          "form": "그건 삼시세끼먹을수있을거같아요",
          "original_form": "그건 삼시세끼먹을수있을거같아요",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025605.1.14",
          "form": "도우쫄깃한거....",
          "original_form": "도우쫄깃한거....",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025605.1.15",
          "form": "ㅋㅋㄱㅋㅋ일관된취향...",
          "original_form": "ㅋㅋㄱㅋㅋ일관된취향...",
          "speaker_id": "1",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100025605.1.16",
          "form": "아 정말 식성이.. 너무 올곧으세요",
          "original_form": "아 정말 식성이.. 너무 올곧으세요",
          "speaker_id": "2",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100025605.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
