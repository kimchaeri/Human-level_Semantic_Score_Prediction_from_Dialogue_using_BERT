      "utterance": [
        {
          "id": "MDRW2100041078.1.1",
          "form": "드라마 좋아하시나요?",
          "original_form": "드라마 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100041078.1.2",
          "form": "요즘 거의 안봐요",
          "original_form": "요즘 거의 안봐요",
          "speaker_id": "2",
          "time": "20210905 16:39"
        },
        {
          "id": "MDRW2100041078.1.3",
          "form": "name1님은 좋아하세요?",
          "original_form": "&name1&님은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 16:39"
        },
        {
          "id": "MDRW2100041078.1.4",
          "form": "네 저는 자주 봐요",
          "original_form": "네 저는 자주 봐요",
          "speaker_id": "1",
          "time": "20210905 16:39"
        },
        {
          "id": "MDRW2100041078.1.5",
          "form": "그럼 예능은 좋아하시나요?",
          "original_form": "그럼 예능은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 16:39"
        },
        {
          "id": "MDRW2100041078.1.6",
          "form": "네네 예능은 좋아해요",
          "original_form": "네네 예능은 좋아해요",
          "speaker_id": "2",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100041078.1.7",
          "form": "어떤거보세요?",
          "original_form": "어떤거보세요?",
          "speaker_id": "2",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100041078.1.8",
          "form": "저는 런닝맨이나 대탈출 봐요",
          "original_form": "저는 런닝맨이나 대탈출 봐요",
          "speaker_id": "1",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100041078.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100041078.1.10",
          "form": "런닝맨이랑 유퀴즈요",
          "original_form": "런닝맨이랑 유퀴즈요",
          "speaker_id": "2",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100041078.1.11",
          "form": "좀 있으면 런닝맨 하겠네요ㅎ",
          "original_form": "좀 있으면 런닝맨 하겠네요ㅎ",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100041078.1.12",
          "form": "앗 그러게요! 시계 안 보고 있어서",
          "original_form": "앗 그러게요! 시계 안 보고 있어서",
          "speaker_id": "1",
          "time": "20210905 16:44"
        },
        {
          "id": "MDRW2100041078.1.13",
          "form": "깜빡할 뻔했어요",
          "original_form": "깜빡할 뻔했어요",
          "speaker_id": "1",
          "time": "20210905 16:44"
        },
        {
          "id": "MDRW2100041078.1.14",
          "form": "혹시 예전 예능도 좋아하세요?",
          "original_form": "혹시 예전 예능도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100041078.1.15",
          "form": "크라임씬 같은",
          "original_form": "크라임씬 같은",
          "speaker_id": "1",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100041078.1.16",
          "form": "네네 좋아해요",
          "original_form": "네네 좋아해요",
          "speaker_id": "2",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100041078.1.17",
          "form": "크라임씬 시즌1열심히 봤어요",
          "original_form": "크라임씬 시즌1열심히 봤어요",
          "speaker_id": "2",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100041078.1.18",
          "form": "무도도 열심히 보구요",
          "original_form": "무도도 열심히 보구요",
          "speaker_id": "2",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100041078.1.19",
          "form": "헐 크라임씬 진짜 재밌지 않나요?",
          "original_form": "헐 크라임씬 진짜 재밌지 않나요?",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100041078.1.20",
          "form": "제 인생 예능이거든요",
          "original_form": "제 인생 예능이거든요",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100041078.1.21",
          "form": "무도는 조금 조금 보긴 했는데 진짜 레전드 예능 같아요",
          "original_form": "무도는 조금 조금 보긴 했는데 진짜 레전드 예능 같아요",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100041078.1.22",
          "form": "맞아요 ㅜㅜㅜ 레전드였죠",
          "original_form": "맞아요 ㅜㅜㅜ 레전드였죠",
          "speaker_id": "2",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100041078.1.23",
          "form": "크라임씬 만드신분이 지금 대탈출 하시는거죠?",
          "original_form": "크라임씬 만드신분이 지금 대탈출 하시는거죠?",
          "speaker_id": "2",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100041078.1.24",
          "form": "헉 아니에요 ㅠ.^ !!!! 그 분은 요새 아무것도 안 하시더라고요",
          "original_form": "헉 아니에요 ㅠ.^ !!!! 그 분은 요새 아무것도 안 하시더라고요",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041078.1.25",
          "form": "크라임씬 더 만들어줬으면 좋겠는데",
          "original_form": "크라임씬 더 만들어줬으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041078.1.26",
          "form": "대탈출은 정종연 피디라 달라요!",
          "original_form": "대탈출은 정종연 피디라 달라요!",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041078.1.27",
          "form": "앗 그래요? 같은분인지 알았어요",
          "original_form": "앗 그래요? 같은분인지 알았어요",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041078.1.28",
          "form": "정종연피디님? 그분 그런류 잘만드셔서",
          "original_form": "정종연피디님? 그분 그런류 잘만드셔서",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041078.1.29",
          "form": "그러니까요 저도 첨에 헷갈렸어요",
          "original_form": "그러니까요 저도 첨에 헷갈렸어요",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041078.1.30",
          "form": "저도 처음에 헷갈렸어요",
          "original_form": "저도 처음에 헷갈렸어요",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041078.1.31",
          "form": "여고추리반은 혹시 보셨나요?",
          "original_form": "여고추리반은 혹시 보셨나요?",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041078.1.32",
          "form": "대탈출 피디인데",
          "original_form": "대탈출 피디인데",
          "speaker_id": "1",
          "time": "20210905 16:51"
        }
      ]
