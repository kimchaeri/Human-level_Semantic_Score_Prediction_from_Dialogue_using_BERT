      "utterance": [
        {
          "id": "MDRW2100026567.1.1",
          "form": "연어를 구워서 쪽파랑 같이 넣고 뜸들이고 비벼비벼",
          "original_form": "연어를 구워서 쪽파랑 같이 넣고 뜸들이고 비벼비벼",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026567.1.2",
          "form": "입니다",
          "original_form": "입니다",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026567.1.3",
          "form": "아 양념장이 필요합니다",
          "original_form": "아 양념장이 필요합니다",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026567.1.4",
          "form": "이미 집밥이 아닌데요",
          "original_form": "이미 집밥이 아닌데요",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026567.1.5",
          "form": "생각보다 간단한 음식입니다",
          "original_form": "생각보다 간단한 음식입니다",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026567.1.6",
          "form": "솥밥이 찐 입니다",
          "original_form": "솥밥이 찐 입니다",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026567.1.7",
          "form": "강츄",
          "original_form": "강츄",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026567.1.8",
          "form": "ㅜ 양념 내놔요",
          "original_form": "ㅜ 양념 내놔요",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026567.1.9",
          "form": "솥밥도 내놓고 그런말을 해욧!",
          "original_form": "솥밥도 내놓고 그런말을 해욧!",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026567.1.10",
          "form": "양념드릴게요",
          "original_form": "양념드릴게요",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026567.1.11",
          "form": "잼나요 ㅋㅋ",
          "original_form": "잼나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026567.1.12",
          "form": "말투갘ㅋㅋㅋㅋ",
          "original_form": "말투갘ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026567.1.13",
          "form": "부대찌개는",
          "original_form": "부대찌개는",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026567.1.14",
          "form": "어디서 난건가요 배민?",
          "original_form": "어디서 난건가요 배민?",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026567.1.15",
          "form": "제 진심이 웃기신가요ㅎ",
          "original_form": "제 진심이 웃기신가요ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026567.1.16",
          "form": "언니가 했쥬ㅎㅎ",
          "original_form": "언니가 했쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026567.1.17",
          "form": "언니가 부대찌개 좋아하고 잘해요",
          "original_form": "언니가 부대찌개 좋아하고 잘해요",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026567.1.18",
          "form": "대량으로  해놔서 제가 와도 남았네요",
          "original_form": "대량으로  해놔서 제가 와도 남았네요",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026567.1.19",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026567.1.20",
          "form": "손이 크시네요",
          "original_form": "손이 크시네요",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026567.1.21",
          "form": "맛은 어때요?",
          "original_form": "맛은 어때요?",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026567.1.22",
          "form": "맛이야 뭐ㅎㅎ",
          "original_form": "맛이야 뭐ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026567.1.23",
          "form": "스팸과 소시지가 잔뜩 들었는데 맛이 없을리가요",
          "original_form": "스팸과 소시지가 잔뜩 들었는데 맛이 없을리가요",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026567.1.24",
          "form": "언니가 백종원입니까",
          "original_form": "언니가 백종원입니까",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026567.1.25",
          "form": "구할이 햄입니다ㅎㅎ",
          "original_form": "구할이 햄입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026567.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026567.1.27",
          "form": "햄 너무 조아요",
          "original_form": "햄 너무 조아요",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026567.1.28",
          "form": "스팸은여?",
          "original_form": "스팸은여?",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026567.1.29",
          "form": "스팸7 소시지 3",
          "original_form": "스팸7 소시지 3",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026567.1.30",
          "form": "황금비율이쥬 ㅎ",
          "original_form": "황금비율이쥬 ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026567.1.31",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026567.1.32",
          "form": "부자시네요",
          "original_form": "부자시네요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026567.1.33",
          "form": "스팸 7이면 재력있네요",
          "original_form": "스팸 7이면 재력있네요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        }
      ]
    }
  ]
