      "utterance": [
        {
          "id": "MDRW2100010541.1.1",
          "form": "와!!",
          "original_form": "와!!",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010541.1.2",
          "form": "저도 거의 눈팅해요 ㅋㅋ",
          "original_form": "저도 거의 눈팅해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010541.1.3",
          "form": "방사에 그림 올리기가ㅜ좀",
          "original_form": "방사에 그림 올리기가ㅜ좀",
          "speaker_id": "1",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010541.1.4",
          "form": "거기는 다들 금손만 있는것 같아서",
          "original_form": "거기는 다들 금손만 있는것 같아서",
          "speaker_id": "1",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010541.1.5",
          "form": "ㅠㅠㅠ 그러게요",
          "original_form": "ㅠㅠㅠ 그러게요",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010541.1.6",
          "form": "트위터에는 그림은 안올리시나요?",
          "original_form": "트위터에는 그림은 안올리시나요?",
          "speaker_id": "1",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010541.1.7",
          "form": "올릴만한 그림이 별로 없어서요 ㅠ",
          "original_form": "올릴만한 그림이 별로 없어서요 ㅠ",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010541.1.8",
          "form": "좀 최근에야 열심히 그림그리는 중이에요",
          "original_form": "좀 최근에야 열심히 그림그리는 중이에요",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010541.1.9",
          "form": "트위터도 금손 많죠",
          "original_form": "트위터도 금손 많죠",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010541.1.10",
          "form": "크으으으 bbb",
          "original_form": "크으으으 bbb",
          "speaker_id": "1",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010541.1.11",
          "form": "아 트위터가 그림쟁이분들이 많이 사용하시더라고요",
          "original_form": "아 트위터가 그림쟁이분들이 많이 사용하시더라고요",
          "speaker_id": "1",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010541.1.12",
          "form": "좀 즉각적인 반응이 보이고 퍼지는 속도도 빨라서 그런것같아요",
          "original_form": "좀 즉각적인 반응이 보이고 퍼지는 속도도 빨라서 그런것같아요",
          "speaker_id": "1",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010541.1.13",
          "form": "인스타보다는 트위터가 빠른 느낌이죠 약간",
          "original_form": "인스타보다는 트위터가 빠른 느낌이죠 약간",
          "speaker_id": "1",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010541.1.14",
          "form": "맞아요!! 진짜 대단한 분들 많아요",
          "original_form": "맞아요!! 진짜 대단한 분들 많아요",
          "speaker_id": "2",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100010541.1.15",
          "form": "아 맞아요 ㅌㅋㅋㅋ 엄청 빠르죠",
          "original_form": "아 맞아요 ㅌㅋㅋㅋ 엄청 빠르죠",
          "speaker_id": "2",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100010541.1.16",
          "form": "아 저도 취미로 그림을 그리기는 하는데",
          "original_form": "아 저도 취미로 그림을 그리기는 하는데",
          "speaker_id": "1",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100010541.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100010541.1.18",
          "form": "그그 어떤 덕질할 때",
          "original_form": "그그 어떤 덕질할 때",
          "speaker_id": "1",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100010541.1.19",
          "form": "그 모여서 하는거",
          "original_form": "그 모여서 하는거",
          "speaker_id": "1",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100010541.1.20",
          "form": "교류회 한 번 가본적 있어요!!",
          "original_form": "교류회 한 번 가본적 있어요!!",
          "speaker_id": "1",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010541.1.21",
          "form": "교류회는 안가보셨나요?!",
          "original_form": "교류회는 안가보셨나요?!",
          "speaker_id": "1",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010541.1.22",
          "form": "우와 교류회 트위터에서 몇번 본거 같은데 정확히 어떤거예요??",
          "original_form": "우와 교류회 트위터에서 몇번 본거 같은데 정확히 어떤거예요??",
          "speaker_id": "2",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010541.1.23",
          "form": "그 각자 회지를 만들어서",
          "original_form": "그 각자 회지를 만들어서",
          "speaker_id": "1",
          "time": "20210823 19:28"
        },
        {
          "id": "MDRW2100010541.1.24",
          "form": "교류회 인원만큼 뽑아간 다음에 교환하는거에요",
          "original_form": "교류회 인원만큼 뽑아간 다음에 교환하는거에요",
          "speaker_id": "1",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010541.1.25",
          "form": "글고 덕질이야기하기",
          "original_form": "글고 덕질이야기하기",
          "speaker_id": "1",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010541.1.26",
          "form": "근데 어떤 교류회는 그 덕질분야(?) 금손한테 그림 받아서 굿즈 나눠주고 그러더라고요",
          "original_form": "근데 어떤 교류회는 그 덕질분야(?) 금손한테 그림 받아서 굿즈 나눠주고 그러더라고요",
          "speaker_id": "1",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010541.1.27",
          "form": "흑흡 부러웠어요",
          "original_form": "흑흡 부러웠어요",
          "speaker_id": "1",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010541.1.28",
          "form": "우와 회지라면 만화나 소설 아닌가요? 그런걸 만들줄알아야만 갈 수 있는.. 난이도가 높네요",
          "original_form": "우와 회지라면 만화나 소설 아닌가요? 그런걸 만들줄알아야만 갈 수 있는.. 난이도가 높네요",
          "speaker_id": "2",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010541.1.29",
          "form": "헉 신기 저도 그런거 해보고 싶어요 지방 살아서 슬프네요 ㅠㅠ",
          "original_form": "헉 신기 저도 그런거 해보고 싶어요 지방 살아서 슬프네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010541.1.30",
          "form": "썰북같은거 뽑아오시는 분도 있어요!!!!",
          "original_form": "썰북같은거 뽑아오시는 분도 있어요!!!!",
          "speaker_id": "1",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010541.1.31",
          "form": "지방에서 교류회를 열면 됩니다.",
          "original_form": "지방에서 교류회를 열면 됩니다.",
          "speaker_id": "1",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010541.1.32",
          "form": "(아마도)",
          "original_form": "(아마도)",
          "speaker_id": "1",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010541.1.33",
          "form": "한다면 제가 직접 하는 수 밖에 없겠군요",
          "original_form": "한다면 제가 직접 하는 수 밖에 없겠군요",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010541.1.34",
          "form": "(포기)",
          "original_form": "(포기)",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010541.1.35",
          "form": "근데 교류회같은 거 덕질 열심히 하는 열정 많은 사람들이 할 수 있는거죠",
          "original_form": "근데 교류회같은 거 덕질 열심히 하는 열정 많은 사람들이 할 수 있는거죠",
          "speaker_id": "2",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010541.1.36",
          "form": "저도 덕질 잘 하는 열정 넘치는 사람이 되고 싶어요",
          "original_form": "저도 덕질 잘 하는 열정 넘치는 사람이 되고 싶어요",
          "speaker_id": "2",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010541.1.37",
          "form": "그럼 팬아트 연성도 하면서 저절로 그림연습도 될텐데말이죠??",
          "original_form": "그럼 팬아트 연성도 하면서 저절로 그림연습도 될텐데말이죠??",
          "speaker_id": "2",
          "time": "20210823 19:37"
        },
        {
          "id": "MDRW2100010541.1.38",
          "form": "맞아요 맞아요!!!!",
          "original_form": "맞아요 맞아요!!!!",
          "speaker_id": "1",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100010541.1.39",
          "form": "그릴거 없을 때 덕질로 그리잖아요!!",
          "original_form": "그릴거 없을 때 덕질로 그리잖아요!!",
