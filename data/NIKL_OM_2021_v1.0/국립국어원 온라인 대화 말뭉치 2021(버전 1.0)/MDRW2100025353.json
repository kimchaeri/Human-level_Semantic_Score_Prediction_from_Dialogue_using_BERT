      "utterance": [
        {
          "id": "MDRW2100025353.1.1",
          "form": "책 읽으십니까",
          "original_form": "책 읽으십니까",
          "speaker_id": "1",
          "time": "20210901 10:03"
        },
        {
          "id": "MDRW2100025353.1.2",
          "form": "인소는 여럿읽어봣습니다",
          "original_form": "인소는 여럿읽어봣습니다",
          "speaker_id": "2",
          "time": "20210901 10:03"
        },
        {
          "id": "MDRW2100025353.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100025353.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ왜요 말해주면되는거자나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ왜요 말해주면되는거자나요",
          "speaker_id": "2",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100025353.1.5",
          "form": "서점 언제 마지막으로 갔어요?",
          "original_form": "서점 언제 마지막으로 갔어요?",
          "speaker_id": "1",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025353.1.6",
          "form": "바야흐로,,",
          "original_form": "바야흐로,,",
          "speaker_id": "2",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025353.1.7",
          "form": "서점은 자주가요 애들 책사줘야해서",
          "original_form": "서점은 자주가요 애들 책사줘야해서",
          "speaker_id": "2",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025353.1.8",
          "form": "아 저 소설좋아해서 빅픽쳐 3번인가봣는데",
          "original_form": "아 저 소설좋아해서 빅픽쳐 3번인가봣는데",
          "speaker_id": "2",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025353.1.9",
          "form": "더글라스케네디 좋아함",
          "original_form": "더글라스케네디 좋아함",
          "speaker_id": "2",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025353.1.10",
          "form": "그게 머지",
          "original_form": "그게 머지",
          "speaker_id": "1",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025353.1.11",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025353.1.12",
          "form": "나름유명한사람인데",
          "original_form": "나름유명한사람인데",
          "speaker_id": "2",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025353.1.13",
          "form": "이거 두꺼워요?",
          "original_form": "이거 두꺼워요?",
          "speaker_id": "1",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025353.1.14",
          "form": "넹 엄청ㅋㅋㅋㅋ",
          "original_form": "넹 엄청ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:07"
        },
        {
          "id": "MDRW2100025353.1.15",
          "form": "아 들춰보긴 한것같음",
          "original_form": "아 들춰보긴 한것같음",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100025353.1.16",
          "form": "와 이걸",
          "original_form": "와 이걸",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100025353.1.17",
          "form": "3번 보셨다니",
          "original_form": "3번 보셨다니",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100025353.1.18",
          "form": "서점가서 베스트셀러 뒤적거리다보면",
          "original_form": "서점가서 베스트셀러 뒤적거리다보면",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100025353.1.19",
          "form": "모먼트인가 그것도재밋고",
          "original_form": "모먼트인가 그것도재밋고",
          "speaker_id": "2",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100025353.1.20",
          "form": "뭐많음",
          "original_form": "뭐많음",
          "speaker_id": "2",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100025353.1.21",
          "form": "사회 흐름 어느 정도 나와서",
          "original_form": "사회 흐름 어느 정도 나와서",
          "speaker_id": "1",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100025353.1.22",
          "form": "그 맛에 가는데 최근엔 땡기는게 읍더라구요",
          "original_form": "그 맛에 가는데 최근엔 땡기는게 읍더라구요",
          "speaker_id": "1",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100025353.1.23",
          "form": "주식책을 사볼까 고민많이햇엇는데",
          "original_form": "주식책을 사볼까 고민많이햇엇는데",
          "speaker_id": "2",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100025353.1.24",
          "form": "모먼트 뭐지 들어봤는데",
          "original_form": "모먼트 뭐지 들어봤는데",
          "speaker_id": "1",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100025353.1.25",
          "form": "그것도 저사람꺼임ㅋㅋㅋㅋㅋ",
          "original_form": "그것도 저사람꺼임ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100025353.1.26",
          "form": "엄청야,함",
          "original_form": "엄청야,함",
          "speaker_id": "2",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100025353.1.27",
          "form": "와 봐야겠다",
          "original_form": "와 봐야겠다",
          "speaker_id": "1",
          "time": "20210901 10:10"
        },
        {
          "id": "MDRW2100025353.1.28",
          "form": "검색하니 안나오는데요",
          "original_form": "검색하니 안나오는데요",
          "speaker_id": "1",
          "time": "20210901 10:10"
        },
        {
          "id": "MDRW2100025353.1.29",
          "form": "모멘트네요 모멘트",
          "original_form": "모멘트네요 모멘트",
          "speaker_id": "2",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100025353.1.30",
          "form": "주식책은 음 뭐 보면 좋긴한데",
          "original_form": "주식책은 음 뭐 보면 좋긴한데",
          "speaker_id": "1",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100025353.1.31",
          "form": "모멘튼가 하고 검색해보려다 말았는데",
          "original_form": "모멘튼가 하고 검색해보려다 말았는데",
          "speaker_id": "1",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100025353.1.32",
          "form": "까비",
          "original_form": "까비",
          "speaker_id": "1",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025353.1.33",
          "form": "심심할때읽으면 재밌음 고3때 열심히읽음",
          "original_form": "심심할때읽으면 재밌음 고3때 열심히읽음",
          "speaker_id": "2",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025353.1.34",
          "form": "주식도 다 개개인이 달라서",
          "original_form": "주식도 다 개개인이 달라서",
          "speaker_id": "1",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025353.1.35",
          "form": "투자방식이나",
          "original_form": "투자방식이나",
          "speaker_id": "1",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025353.1.36",
          "form": "스타일 등등",
          "original_form": "스타일 등등",
          "speaker_id": "1",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025353.1.37",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025353.1.38",
          "form": "나만의 루틴을 만들고 그 안에서 굴려야",
          "original_form": "나만의 루틴을 만들고 그 안에서 굴려야",
          "speaker_id": "1",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025353.1.39",
          "form": "손실이 적어요",
          "original_form": "손실이 적어요",
          "speaker_id": "1",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025353.1.40",
          "form": "최근에 뭐읽으셨어요?",
          "original_form": "최근에 뭐읽으셨어요?",
          "speaker_id": "2",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025353.1.41",
          "form": "딱 떠오르는건",
          "original_form": "딱 떠오르는건",
          "speaker_id": "1",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025353.1.42",
          "form": "모든 순간이 너였다",
          "original_form": "모든 순간이 너였다",
          "speaker_id": "1",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025353.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025353.1.44",
          "form": "이거 그거자나요",
          "original_form": "이거 그거자나요",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.45",
          "form": "인스타에서 엄청광고하는책",
          "original_form": "인스타에서 엄청광고하는책",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.46",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.47",
          "form": "아 오지게 뜸",
          "original_form": "아 오지게 뜸",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.48",
          "form": "인스타 광고",
          "original_form": "인스타 광고",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.49",
          "form": "근데 좋아요 전 모르고 본건데",
          "original_form": "근데 좋아요 전 모르고 본건데",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.50",
          "form": "재밌어요?",
          "original_form": "재밌어요?",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.51",
          "form": "수필이에요",
          "original_form": "수필이에요",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.52",
          "form": "전여친생각낫을듯",
          "original_form": "전여친생각낫을듯",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.53",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.54",
          "form": "언어의 온도? 이런거도 봤는데 그닥이었고",
          "original_form": "언어의 온도? 이런거도 봤는데 그닥이었고",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025353.1.55",
          "form": "그나마 저런 류에서는 저게 젤 나았었고",
          "original_form": "그나마 저런 류에서는 저게 젤 나았었고",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025353.1.56",
          "form": "소설하니까 옛날에 본 종이여자 생각남요",
          "original_form": "소설하니까 옛날에 본 종이여자 생각남요",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025353.1.57",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "2",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025353.1.58",
          "form": "일단 ㅈㅊ",
          "original_form": "일단 ㅈㅊ",
          "speaker_id": "2",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025353.1.59",
          "form": "그거랑 또 생각 많이하게 하는거 있었는데",
          "original_form": "그거랑 또 생각 많이하게 하는거 있었는데",
          "speaker_id": "1",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025353.1.60",
          "form": "표지 색만 생각남ㅋㅋㅋ",
