      "utterance": [
        {
          "id": "MDRW2100006226.1.1",
          "form": "어렸을때요?",
          "original_form": "어렸을때요?",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100006226.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006226.1.3",
          "form": "어릴때요",
          "original_form": "어릴때요",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006226.1.4",
          "form": "고기도 안먹고",
          "original_form": "고기도 안먹고",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006226.1.5",
          "form": "밥도 안먹고",
          "original_form": "밥도 안먹고",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006226.1.6",
          "form": "언제부터 잘 드셨어요?",
          "original_form": "언제부터 잘 드셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006226.1.7",
          "form": "초등학교 고학년부터요",
          "original_form": "초등학교 고학년부터요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006226.1.8",
          "form": "5학년인가",
          "original_form": "5학년인가",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006226.1.9",
          "form": "그때부터",
          "original_form": "그때부터",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006226.1.10",
          "form": "좀 더 기다려야겠네용",
          "original_form": "좀 더 기다려야겠네용",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006226.1.11",
          "form": "첫째도 엄청 잘 먹던 아인데",
          "original_form": "첫째도 엄청 잘 먹던 아인데",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006226.1.12",
          "form": "두돌 지나더니 편식 시작되고\r\n지금은 말랐어여ㅠ",
          "original_form": "두돌 지나더니 편식 시작되고\r\n지금은 말랐어여ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006226.1.13",
          "form": "또 먹을때 되면 먹을거에여",
          "original_form": "또 먹을때 되면 먹을거에여",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006226.1.14",
          "form": "걱정마세여",
          "original_form": "걱정마세여",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006226.1.15",
          "form": "어른들은 원래 말랐던 애가 아니니",
          "original_form": "어른들은 원래 말랐던 애가 아니니",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006226.1.16",
          "form": "볼때마다 잘 좀 챙겨먹이라는데",
          "original_form": "볼때마다 잘 좀 챙겨먹이라는데",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006226.1.17",
          "form": "매.끼마다 정말 서터레스거든요ㅠ",
          "original_form": "매.끼마다 정말 서터레스거든요ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006226.1.18",
          "form": "남자애들은 클때되면 왕창먹어요",
          "original_form": "남자애들은 클때되면 왕창먹어요",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006226.1.19",
          "form": "그럼 좀 기다려야겠네요ㅠㅠ",
          "original_form": "그럼 좀 기다려야겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006226.1.20",
          "form": "이것도 다 때가 있나봐요",
          "original_form": "이것도 다 때가 있나봐요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006226.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006226.1.22",
          "form": "아직 어린데요 머",
          "original_form": "아직 어린데요 머",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006226.1.23",
          "form": "그래도 잘먹었음 좋겠는 엄마 맘 ㅠㅋ",
          "original_form": "그래도 잘먹었음 좋겠는 엄마 맘 ㅠㅋ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006226.1.24",
          "form": "둘째는 너무 먹는다고 그러고ㅋㅋ",
          "original_form": "둘째는 너무 먹는다고 그러고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006226.1.25",
          "form": "큰애는 안먹는다고 ㅋㅋ",
          "original_form": "큰애는 안먹는다고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006226.1.26",
          "form": "엄마는 항상 고민과 걱정이 많은 법이에요 ",
          "original_form": "엄마는 항상 고민과 걱정이 많은 법이에요 ",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006226.1.27",
          "form": "안먹진 않자나요",
          "original_form": "안먹진 않자나요",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006226.1.28",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006226.1.29",
          "form": "엄마가",
          "original_form": "엄마가",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006226.1.30",
          "form": "젤 문제였군요",
          "original_form": "젤 문제였군요",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100006226.1.31",
