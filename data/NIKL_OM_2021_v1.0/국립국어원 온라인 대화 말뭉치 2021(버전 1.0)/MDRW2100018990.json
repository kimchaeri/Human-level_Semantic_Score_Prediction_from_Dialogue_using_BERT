      "utterance": [
        {
          "id": "MDRW2100018990.1.1",
          "form": "그런키보드도있지않나?",
          "original_form": "그런키보드도있지않나?",
          "speaker_id": "1",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018990.1.2",
          "form": "블루투슨데",
          "original_form": "블루투슨데",
          "speaker_id": "1",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018990.1.3",
          "form": "완전바닥에다가하는거",
          "original_form": "완전바닥에다가하는거",
          "speaker_id": "1",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018990.1.4",
          "form": "저거빔프로젝터같은걸로",
          "original_form": "저거빔프로젝터같은걸로",
          "speaker_id": "1",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018990.1.5",
          "form": "그거 돼??",
          "original_form": "그거 돼??",
          "speaker_id": "2",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018990.1.6",
          "form": "그거 쓰다가 팔목 아작나는거 아니야?",
          "original_form": "그거 쓰다가 팔목 아작나는거 아니야?",
          "speaker_id": "2",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018990.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ몰라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ몰라",
          "speaker_id": "1",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018990.1.8",
          "form": "타자를 많이 쳐야 하는 사람이면 그리 높지 않은 키보드에 키 압력이 적은게 좋댜",
          "original_form": "타자를 많이 쳐야 하는 사람이면 그리 높지 않은 키보드에 키 압력이 적은게 좋댜",
          "speaker_id": "2",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018990.1.9",
          "form": "예전에본적있음",
          "original_form": "예전에본적있음",
          "speaker_id": "1",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018990.1.10",
          "form": "나 그거 볼때마다 의문이었음",
          "original_form": "나 그거 볼때마다 의문이었음",
          "speaker_id": "2",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018990.1.11",
          "form": "키압력이적은거면",
          "original_form": "키압력이적은거면",
          "speaker_id": "1",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018990.1.12",
          "form": "어떤거임?",
          "original_form": "어떤거임?",
          "speaker_id": "1",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018990.1.13",
          "form": "뭐가?",
          "original_form": "뭐가?",
          "speaker_id": "1",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018990.1.14",
          "form": "키를 뉴를땨",
          "original_form": "키를 뉴를땨",
          "speaker_id": "2",
          "time": "20210827 14:31"
        },
        {
          "id": "MDRW2100018990.1.15",
          "form": "누를 때",
          "original_form": "누를 때",
          "speaker_id": "2",
          "time": "20210827 14:31"
        },
        {
          "id": "MDRW2100018990.1.16",
          "form": "손가락이 키를 누르는 압력 차가 있대",
          "original_form": "손가락이 키를 누르는 압력 차가 있대",
          "speaker_id": "2",
          "time": "20210827 14:31"
        },
        {
          "id": "MDRW2100018990.1.17",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018990.1.18",
          "form": "그냥잘눌린다는거아님,",
          "original_form": "그냥잘눌린다는거아님,",
          "speaker_id": "1",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018990.1.19",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018990.1.20",
          "form": "보통 기계식 키보드에서는 적축 키보드가 키압이 낮다고 많이 쓰더라",
          "original_form": "보통 기계식 키보드에서는 적축 키보드가 키압이 낮다고 많이 쓰더라",
          "speaker_id": "2",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018990.1.21",
          "form": "어닌가",
          "original_form": "어닌가",
          "speaker_id": "1",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018990.1.22",
          "form": "눌리는거 이외에",
          "original_form": "눌리는거 이외에",
          "speaker_id": "2",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018990.1.23",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "1",
          "time": "20210827 14:33"
        }
      ]
