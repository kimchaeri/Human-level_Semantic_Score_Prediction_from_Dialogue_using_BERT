        },
        {
          "id": "MDRW2100046648.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211103 23:46"
        },
        {
          "id": "MDRW2100046648.1.3",
          "form": "쇼핑 좋아하시나요?",
          "original_form": "쇼핑 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211103 23:46"
        },
        {
          "id": "MDRW2100046648.1.4",
          "form": "그냥 필요할때만 봅니다",
          "original_form": "그냥 필요할때만 봅니다",
          "speaker_id": "1",
          "time": "20211103 23:47"
        },
        {
          "id": "MDRW2100046648.1.5",
          "form": "저랑 같으시네요 ㅎㅎ",
          "original_form": "저랑 같으시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211103 23:47"
        },
        {
          "id": "MDRW2100046648.1.6",
          "form": "요즘은 뭐 사고싶은 거 있으세요?",
          "original_form": "요즘은 뭐 사고싶은 거 있으세요?",
          "speaker_id": "2",
          "time": "20211103 23:47"
        },
        {
          "id": "MDRW2100046648.1.7",
          "form": "사고싶은건 있는데",
          "original_form": "사고싶은건 있는데",
          "speaker_id": "1",
          "time": "20211103 23:47"
        },
        {
          "id": "MDRW2100046648.1.8",
          "form": "그냥 큰 핸드폰이요",
          "original_form": "그냥 큰 핸드폰이요",
          "speaker_id": "1",
          "time": "20211103 23:48"
        },
        {
          "id": "MDRW2100046648.1.9",
          "form": "어떤 거예요~?",
          "original_form": "어떤 거예요~?",
          "speaker_id": "2",
          "time": "20211103 23:48"
        },
        {
          "id": "MDRW2100046648.1.10",
          "form": "갤노트나 아이폰프로 이런거요?",
          "original_form": "갤노트나 아이폰프로 이런거요?",
          "speaker_id": "2",
          "time": "20211103 23:48"
        },
        {
          "id": "MDRW2100046648.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20211103 23:48"
        },
        {
          "id": "MDRW2100046648.1.12",
          "form": "화면 큰 거 좋아하시나봐요 ㅎㅎ",
          "original_form": "화면 큰 거 좋아하시나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211103 23:48"
        },
        {
          "id": "MDRW2100046648.1.13",
          "form": "지금은 어떤 거 사용하고 계시나요",
          "original_form": "지금은 어떤 거 사용하고 계시나요",
          "speaker_id": "2",
          "time": "20211103 23:49"
        },
        {
          "id": "MDRW2100046648.1.14",
          "form": "아이폰8이요",
          "original_form": "아이폰8이요",
          "speaker_id": "1",
          "time": "20211103 23:49"
        },
        {
          "id": "MDRW2100046648.1.15",
          "form": "오 꽤 오래 쓰신 거 아니에요?",
          "original_form": "오 꽤 오래 쓰신 거 아니에요?",
          "speaker_id": "2",
          "time": "20211103 23:49"
        },
        {
          "id": "MDRW2100046648.1.16",
          "form": "4년 안된것같아요",
          "original_form": "4년 안된것같아요",
          "speaker_id": "1",
          "time": "20211103 23:50"
        },
        {
          "id": "MDRW2100046648.1.17",
          "form": "꽤 오래 쓰셨네요 ㅎㅎ",
          "original_form": "꽤 오래 쓰셨네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211103 23:50"
        },
        {
          "id": "MDRW2100046648.1.18",
          "form": "저도 아이폰 사용하는데 잔고장이 별로 없어서 오래 쓰게 되더라구요!",
          "original_form": "저도 아이폰 사용하는데 잔고장이 별로 없어서 오래 쓰게 되더라구요!",
          "speaker_id": "2",
          "time": "20211103 23:50"
        },
        {
          "id": "MDRW2100046648.1.19",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20211103 23:51"
        },
        {
          "id": "MDRW2100046648.1.20",
          "form": "저도 아이폰10 3년반 쓰고 12프로로 갈아탔어요 ㅋㅋㅋ",
          "original_form": "저도 아이폰10 3년반 쓰고 12프로로 갈아탔어요 ㅋㅋㅋ",
