      "utterance": [
        {
          "id": "MDRW2100043609.1.1",
          "form": "제주도가면 뭐할꺼야",
          "original_form": "제주도가면 뭐할꺼야",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043609.1.2",
          "form": "맛집 다니면서 맛있는거나 먹어야지",
          "original_form": "맛집 다니면서 맛있는거나 먹어야지",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043609.1.3",
          "form": "뭐가 먹고싶어용?",
          "original_form": "뭐가 먹고싶어용?",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043609.1.4",
          "form": "제주도 해산물 먹고 싶다",
          "original_form": "제주도 해산물 먹고 싶다",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043609.1.5",
          "form": "맛있겠다..",
          "original_form": "맛있겠다..",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043609.1.6",
          "form": "싱싱 하겠지",
          "original_form": "싱싱 하겠지",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043609.1.7",
          "form": "그렇겠지?ㅋㅋ 직접 낚시도 해봐용 ㅋ",
          "original_form": "그렇겠지?ㅋㅋ 직접 낚시도 해봐용 ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043609.1.8",
          "form": "낚시를 어떻게 하니",
          "original_form": "낚시를 어떻게 하니",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043609.1.9",
          "form": "그런거 체험하는거 있던데 ㅋㅋ아니면 해녀체험",
          "original_form": "그런거 체험하는거 있던데 ㅋㅋ아니면 해녀체험",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043609.1.10",
          "form": "재밌겠네",
          "original_form": "재밌겠네",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043609.1.11",
          "form": "근데 엄마 물 무서워해서 못하겠군 ㅋ",
          "original_form": "근데 엄마 물 무서워해서 못하겠군 ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043609.1.12",
          "form": "그래 그냥 사 먹어야지",
          "original_form": "그래 그냥 사 먹어야지",
