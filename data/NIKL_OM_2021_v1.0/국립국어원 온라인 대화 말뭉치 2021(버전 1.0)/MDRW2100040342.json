      "utterance": [
        {
          "id": "MDRW2100040342.1.1",
          "form": "팬뭐써?",
          "original_form": "팬뭐써?",
          "speaker_id": "1",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040342.1.2",
          "form": "모르겠어ㅜㅜ키친아트인가? 백화점에서 세일하는거 ㅋㅋㅋ",
          "original_form": "모르겠어ㅜㅜ키친아트인가? 백화점에서 세일하는거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040342.1.3",
          "form": "팬이랑 웍세트 샀음 ㅎㅎ 엄마가 팬 오래됬는데 안바꿔서 ㅜㅜ",
          "original_form": "팬이랑 웍세트 샀음 ㅎㅎ 엄마가 팬 오래됬는데 안바꿔서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040342.1.4",
          "form": "오 잘했네! 나도 웍은 사야되는데ㅠ",
          "original_form": "오 잘했네! 나도 웍은 사야되는데ㅠ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040342.1.5",
          "form": "엄마가 하드앤라이트 좋아하는데 좀 호불호갈릴느김?",
          "original_form": "엄마가 하드앤라이트 좋아하는데 좀 호불호갈릴느김?",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040342.1.6",
          "form": "우리집은 후라이펜잘안쓰고 웍 자주씀ㅋㅋ",
          "original_form": "우리집은 후라이펜잘안쓰고 웍 자주씀ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040342.1.7",
          "form": "우리집도 ㅋㅋㅋㅋㅋ",
          "original_form": "우리집도 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040342.1.8",
          "form": "만능팬이잖아 ㅋㅋㅋㅋ",
          "original_form": "만능팬이잖아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040342.1.9",
          "form": "가격되게쌈",
          "original_form": "가격되게쌈",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040342.1.10",
          "form": "후라이펜은 아직 필요없는데 싸다니까 땡기넼ㅋㅋㅋ",
          "original_form": "후라이펜은 아직 필요없는데 싸다니까 땡기넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040342.1.11",
          "form": "얼마야?",
          "original_form": "얼마야?",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040342.1.12",
          "form": "두개해서 삼만오천?",
          "original_form": "두개해서 삼만오천?",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100040342.1.13",
          "form": "오 싸네? 어디서삼?",
          "original_form": "오 싸네? 어디서삼?",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100040342.1.14",
          "form": "지마켓!",
          "original_form": "지마켓!",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100040342.1.15",
          "form": "이거쿠팡에도있긴함",
          "original_form": "이거쿠팡에도있긴함",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100040342.1.16",
          "form": "근데 지금지마켓이 쌈",
          "original_form": "근데 지금지마켓이 쌈",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100040342.1.17",
          "form": "근데 왜 호불호갈림?!!",
          "original_form": "근데 왜 호불호갈림?!!",
          "speaker_id": "2",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100040342.1.18",
          "form": "평이 좀 갈리던데?",
          "original_form": "평이 좀 갈리던데?",
          "speaker_id": "1",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100040342.1.19",
          "form": "일단 우리엄마는 가벼워서좋다했음 그리고 깊어",
          "original_form": "일단 우리엄마는 가벼워서좋다했음 그리고 깊어",
          "speaker_id": "1",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040342.1.20",
          "form": "오 가벼운거 좋아ㅋㅋ",
          "original_form": "오 가벼운거 좋아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040342.1.21",
          "form": "근데 막 세라믹코팅팬이런것처럼 맨들맨들이런건아님 ㅋㅋ",
          "original_form": "근데 막 세라믹코팅팬이런것처럼 맨들맨들이런건아님 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100040342.1.22",
          "form": "내가볼땐 후라이펜이나 웍은 싸고 비싸고 필요없이 빨리쓰고빨리바꾸는게 좋은듯ㅋㅋ",
          "original_form": "내가볼땐 후라이펜이나 웍은 싸고 비싸고 필요없이 빨리쓰고빨리바꾸는게 좋은듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040342.1.23",
          "form": "내말이 ㅋㅋㅋㅋㅋ",
          "original_form": "내말이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040342.1.24",
          "form": "나지금 고모집왔거든",
          "original_form": "나지금 고모집왔거든",
          "speaker_id": "1",
          "time": "20210904 22:49"
        }
      ]
    }
