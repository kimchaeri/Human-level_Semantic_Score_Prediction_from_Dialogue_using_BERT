      "utterance": [
        {
          "id": "MDRW2100027418.1.1",
          "form": "최근에 구매하신 물건이 뭐예용?",
          "original_form": "최근에 구매하신 물건이 뭐예용?",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027418.1.2",
          "form": "저 안대요 후후",
          "original_form": "저 안대요 후후",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027418.1.3",
          "form": "일회용안대 샀는데 삶의 질 수직상승했슴당",
          "original_form": "일회용안대 샀는데 삶의 질 수직상승했슴당",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027418.1.4",
          "form": "우아 얼마짜리 사셧어요??",
          "original_form": "우아 얼마짜리 사셧어요??",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027418.1.5",
          "form": "일회용이면 한번쓰고 버리나요?.",
          "original_form": "일회용이면 한번쓰고 버리나요?.",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027418.1.6",
          "form": "네ㅜㅜ 아쉽게도 한번 딱 써야해요",
          "original_form": "네ㅜㅜ 아쉽게도 한번 딱 써야해요",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027418.1.7",
          "form": "20개입에 몇천원 했던거같은데",
          "original_form": "20개입에 몇천원 했던거같은데",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027418.1.8",
          "form": "아주조아요",
          "original_form": "아주조아요",
          "speaker_id": "2",
          "time": "20210902 16:16"
        },
        {
          "id": "MDRW2100027418.1.9",
          "form": "우아 저도 사야겟어요 ㅋㅋ",
          "original_form": "우아 저도 사야겟어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027418.1.10",
          "form": "저는 이번에 갤럭시워치 샀는데되게",
          "original_form": "저는 이번에 갤럭시워치 샀는데되게",
          "speaker_id": "1",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027418.1.11",
          "form": "좋드라구요 ㅠ",
          "original_form": "좋드라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027418.1.12",
          "form": "헉 워치",
          "original_form": "헉 워치",
          "speaker_id": "2",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100027418.1.13",
          "form": "저도 너무 사고싶어용",
          "original_form": "저도 너무 사고싶어용",
          "speaker_id": "2",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100027418.1.14",
          "form": "운동할 때 유용하다던대",
          "original_form": "운동할 때 유용하다던대",
          "speaker_id": "2",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100027418.1.15",
          "form": "저는 일할때 쓸려고 샀어요ㅋㅋㅋ 핸드폰 사용 금지라 ㅜㅜ 시",
          "original_form": "저는 일할때 쓸려고 샀어요ㅋㅋㅋ 핸드폰 사용 금지라 ㅜㅜ 시",
          "speaker_id": "1",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100027418.1.16",
          "form": "헐 정말요?",
          "original_form": "헐 정말요?",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100027418.1.17",
          "form": "그럼 워치 진짜 필요하시갰어요",
          "original_form": "그럼 워치 진짜 필요하시갰어요",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100027418.1.18",
          "form": "워치로 연락 다 되지않나요??",
          "original_form": "워치로 연락 다 되지않나요??",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
