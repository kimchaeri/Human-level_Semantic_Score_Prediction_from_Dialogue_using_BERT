      "utterance": [
        {
          "id": "MDRW2100044764.1.1",
          "form": "불맛이 빠져도 맛있다니 찐이네요",
          "original_form": "불맛이 빠져도 맛있다니 찐이네요",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044764.1.2",
          "form": "내기준엔..넘 맛있었읔ㅋ",
          "original_form": "내기준엔..넘 맛있었읔ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044764.1.3",
          "form": "그래도 유명하니까",
          "original_form": "그래도 유명하니까",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044764.1.4",
          "form": "언니임맛에도 맞지않을깜",
          "original_form": "언니임맛에도 맞지않을깜",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044764.1.5",
          "form": "그럴거같음ㅋㅋ",
          "original_form": "그럴거같음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044764.1.6",
          "form": "근데 내가 잘 구울수 있을까",
          "original_form": "근데 내가 잘 구울수 있을까",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044764.1.7",
          "form": "...애가 엄청 부드러워서..",
          "original_form": "...애가 엄청 부드러워서..",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044764.1.8",
          "form": "거의ㅡ다 구운후에",
          "original_form": "거의ㅡ다 구운후에",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.9",
          "form": "자르는게 좋을걸...?",
          "original_form": "자르는게 좋을걸...?",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.10",
          "form": "ㅋㅋ확실합니까?",
          "original_form": "ㅋㅋ확실합니까?",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.11",
          "form": "왜 말끝을 흐리십니까",
          "original_form": "왜 말끝을 흐리십니까",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.12",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.13",
          "form": "제가 굽다가",
          "original_form": "제가 굽다가",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.14",
          "form": "못참아서",
          "original_form": "못참아서",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.15",
          "form": "중간에 잘랐는데",
          "original_form": "중간에 잘랐는데",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.16",
          "form": "좀 터짐",
          "original_form": "좀 터짐",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044764.1.18",
          "form": "아 ㅋㅋ 경험담이었군영",
          "original_form": "아 ㅋㅋ 경험담이었군영",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044764.1.19",
          "form": "넵ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "넵ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100044764.1.20",
          "form": "맛은있지만....ㅋㅋㅋ 엉망이었음",
          "original_form": "맛은있지만....ㅋㅋㅋ 엉망이었음",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100044764.1.21",
          "form": "ㅋㅋ다굽고 자르기 메모메모",
          "original_form": "ㅋㅋ다굽고 자르기 메모메모",
          "speaker_id": "1",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100044764.1.22",
          "form": "나도 추석전에 사먹어야지 ㅋㅋㅋ",
          "original_form": "나도 추석전에 사먹어야지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100044764.1.23",
          "form": "미리주문해염ㅋㅋㅋㅋㅋ",
          "original_form": "미리주문해염ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100044764.1.24",
          "form": "추석 다가옴 또 택배대란",
          "original_form": "추석 다가옴 또 택배대란",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100044764.1.25",
          "form": "그래야겠으 ㅋㅋㅋ",
          "original_form": "그래야겠으 ㅋㅋㅋ",
          "speaker_id": "2",
