      "utterance": [
        {
          "id": "MDRW2100005686.1.1",
          "form": "유튜브로도 동물 보시나요?",
          "original_form": "유튜브로도 동물 보시나요?",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005686.1.2",
          "form": "예전에 쪼금 봤었는데 지금은 잘 안 봐요",
          "original_form": "예전에 쪼금 봤었는데 지금은 잘 안 봐요",
          "speaker_id": "2",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005686.1.3",
          "form": "동물은 주로 친구들이나.. 그냥 인터넷에서 귀여운 짤로 많이 봐요 ㅋㅋ",
          "original_form": "동물은 주로 친구들이나.. 그냥 인터넷에서 귀여운 짤로 많이 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005686.1.4",
          "form": "그렇군요ㅋㅋㅋ",
          "original_form": "그렇군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005686.1.5",
          "form": "귀여운 짤 너무 좋아요ㅠ",
          "original_form": "귀여운 짤 너무 좋아요ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005686.1.6",
          "form": "저는 쿼카도 그렇게 귀엽더라고요...",
          "original_form": "저는 쿼카도 그렇게 귀엽더라고요...",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005686.1.7",
          "form": "어우 쿼카 귀엽죠",
          "original_form": "어우 쿼카 귀엽죠",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100005686.1.8",
          "form": "항상 행복해보여서 ㅋㅋㅋㅋ 너무 귀여워요",
          "original_form": "항상 행복해보여서 ㅋㅋㅋㅋ 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100005686.1.9",
          "form": "맞아요ㅋㅋㅋㅋ 쿼카 보러 호주 가고 싶을 정도예요ㅠㅠㅠ",
          "original_form": "맞아요ㅋㅋㅋㅋ 쿼카 보러 호주 가고 싶을 정도예요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100005686.1.10",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100005686.1.11",
          "form": "돌고래도 너무 귀엽지 않나요",
          "original_form": "돌고래도 너무 귀엽지 않나요",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100005686.1.12",
          "form": "너무 똑똑해요",
          "original_form": "너무 똑똑해요",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100005686.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100005686.1.14",
          "form": "매끈하구..",
          "original_form": "매끈하구..",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100005686.1.15",
          "form": "한번 만1져보고 싶다고 썼는데 자꾸 안보내져서 당황했네요",
          "original_form": "한번 만1져보고 싶다고 썼는데 자꾸 안보내져서 당황했네요",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100005686.1.16",
          "form": "아니 만1지1다가 왜 나쁜말이지",
          "original_form": "아니 만1지1다가 왜 나쁜말이지",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100005686.1.17",
          "form": "사육사랑 물놀이 하는 돌고래 영상 봤는데",
          "original_form": "사육사랑 물놀이 하는 돌고래 영상 봤는데",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100005686.1.18",
          "form": "너무 똑똑해요...",
          "original_form": "너무 똑똑해요...",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100005686.1.19",
          "form": "오호..",
          "original_form": "오호..",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100005686.1.20",
