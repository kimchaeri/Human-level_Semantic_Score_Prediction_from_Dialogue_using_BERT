      "utterance": [
        {
          "id": "MDRW2100026546.1.1",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026546.1.2",
          "form": "당근으로 책 사보셧나요?",
          "original_form": "당근으로 책 사보셧나요?",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026546.1.3",
          "form": "네네 당근으로 책 사봤어요 ㅋㅋ",
          "original_form": "네네 당근으로 책 사봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026546.1.4",
          "form": "팔아보기도 했구요 ㅋㅋ",
          "original_form": "팔아보기도 했구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026546.1.5",
          "form": "애기 책 사려고하는데",
          "original_form": "애기 책 사려고하는데",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026546.1.6",
          "form": "프뢰벨이 너무 비싸서",
          "original_form": "프뢰벨이 너무 비싸서",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026546.1.7",
          "form": "말하기 한번 구해보려고 하는데 구하기가 쉽지 않네요",
          "original_form": "말하기 한번 구해보려고 하는데 구하기가 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026546.1.8",
          "form": "아아아 프뢰벨이요 ㅋㅋ",
          "original_form": "아아아 프뢰벨이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026546.1.9",
          "form": "그거는 개똥이 이런곳이 낫지 않나요?? 아님 당근보다 중고카페?? 이런거",
          "original_form": "그거는 개똥이 이런곳이 낫지 않나요?? 아님 당근보다 중고카페?? 이런거",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026546.1.10",
          "form": "개똥이가 뭐에요?",
          "original_form": "개똥이가 뭐에요?",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026546.1.11",
          "form": "저는 개똥이네 에서 중고책 샀었어요. 동네 서점에도 갔었구요 ㅋ",
          "original_form": "저는 개똥이네 에서 중고책 샀었어요. 동네 서점에도 갔었구요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026546.1.12",
          "form": "아 그런곳이 있군요?",
          "original_form": "아 그런곳이 있군요?",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026546.1.13",
          "form": "네네 ㅋㅋ 옛날에 그렇게 샀었어요 ㅋㅋ 직접가서 보기도 했구요",
          "original_form": "네네 ㅋㅋ 옛날에 그렇게 샀었어요 ㅋㅋ 직접가서 보기도 했구요",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026546.1.14",
          "form": "아 꿀팁 감사합니다 ㅎㅎ",
          "original_form": "아 꿀팁 감사합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026546.1.15",
          "form": "아기 책사주려고 하는데 고민이 많습니다 ㅎㅎ",
          "original_form": "아기 책사주려고 하는데 고민이 많습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026546.1.16",
          "form": "네네 ㅋㅋ 저도 그랬어요 ㅋㅋ 막 신랑이랑 전집 받으러 다른 동네도 가고 ㅋㅋ",
          "original_form": "네네 ㅋㅋ 저도 그랬어요 ㅋㅋ 막 신랑이랑 전집 받으러 다른 동네도 가고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026546.1.17",
          "form": "책 사가지고 와서 하나하나 다 살펴보고 다 닦고",
          "original_form": "책 사가지고 와서 하나하나 다 살펴보고 다 닦고",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026546.1.18",
          "form": "아 ㅎㅎ좋아겠습니다",
          "original_form": "아 ㅎㅎ좋아겠습니다",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026546.1.19",
          "form": "책읽어줄수있으니까",
          "original_form": "책읽어줄수있으니까",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026546.1.20",
          "form": "어떤 전집이 괜찮았나요?",
          "original_form": "어떤 전집이 괜찮았나요?",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026546.1.21",
          "form": "네네 ㅋㅋㅋ 저희집에는 교원책이 많아서요 ㅋ",
          "original_form": "네네 ㅋㅋㅋ 저희집에는 교원책이 많아서요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026546.1.22",
          "form": "프뢰벨 책도 좋아요 제 주위에는 프뢰벨 책 많이 읽어주고 하더라구요",
          "original_form": "프뢰벨 책도 좋아요 제 주위에는 프뢰벨 책 많이 읽어주고 하더라구요",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026546.1.23",
          "form": "교원꺼는 뭔가요?이름이",
          "original_form": "교원꺼는 뭔가요?이름이",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026546.1.24",
          "form": "교원꺼는 여러가지가 있어요 ㅋㅋㅋ",
          "original_form": "교원꺼는 여러가지가 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026546.1.25",
          "form": "근데 말하기 읽기 이거는 프뢰벨이 좋다고 들었어요 ㅋ",
          "original_form": "근데 말하기 읽기 이거는 프뢰벨이 좋다고 들었어요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026546.1.26",
          "form": "프레벨아직도 고민 중입니다",
          "original_form": "프레벨아직도 고민 중입니다",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026546.1.27",
          "form": "그렇다더라궇",
          "original_form": "그렇다더라궇",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
