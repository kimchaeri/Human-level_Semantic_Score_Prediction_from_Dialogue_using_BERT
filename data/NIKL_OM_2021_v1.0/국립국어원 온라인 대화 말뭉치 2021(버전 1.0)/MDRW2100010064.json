      "utterance": [
        {
          "id": "MDRW2100010064.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100010064.1.2",
          "form": "봉사에 관심있으신가요?",
          "original_form": "봉사에 관심있으신가요?",
          "speaker_id": "1",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100010064.1.3",
          "form": "안녕하세요 저는 제로웨이스트 봉사에 관심있어요",
          "original_form": "안녕하세요 저는 제로웨이스트 봉사에 관심있어요",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010064.1.4",
          "form": "아 제로웨이스트ㅠㅠ 저도 추구하는데 참 어렵더라구요",
          "original_form": "아 제로웨이스트ㅠㅠ 저도 추구하는데 참 어렵더라구요",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010064.1.5",
          "form": "그쵸ㅠㅠ 제로웨이스트 동호회 들고나서 버려지는 쓰레기가 많다는 걸 알게됐어요",
          "original_form": "그쵸ㅠㅠ 제로웨이스트 동호회 들고나서 버려지는 쓰레기가 많다는 걸 알게됐어요",
          "speaker_id": "2",
          "time": "20210823 13:47"
        },
        {
          "id": "MDRW2100010064.1.6",
          "form": "제로웨이스트 동호회요? 어떤걸 하나요?",
          "original_form": "제로웨이스트 동호회요? 어떤걸 하나요?",
          "speaker_id": "1",
          "time": "20210823 13:47"
        },
        {
          "id": "MDRW2100010064.1.7",
          "form": "길에 버려진 걸 같이 줍는 봉사를해요",
          "original_form": "길에 버려진 걸 같이 줍는 봉사를해요",
          "speaker_id": "2",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100010064.1.8",
          "form": "줍깅이라고ㅋㅋ",
          "original_form": "줍깅이라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100010064.1.9",
          "form": "줍깅이라니ㅋㅋ 너무 귀엽네요ㅋㅋㅋ",
          "original_form": "줍깅이라니ㅋㅋ 너무 귀엽네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100010064.1.10",
          "form": "요즘도 자주 봉사나가세요?",
          "original_form": "요즘도 자주 봉사나가세요?",
          "speaker_id": "1",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100010064.1.11",
          "form": "아니요ㅠㅠ 코로나때문에 봉사도 어렵네요..",
          "original_form": "아니요ㅠㅠ 코로나때문에 봉사도 어렵네요..",
          "speaker_id": "2",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100010064.1.12",
          "form": "아이고... 저도 종이함부러 쓰는걸 자제하고있어요",
          "original_form": "아이고... 저도 종이함부러 쓰는걸 자제하고있어요",
          "speaker_id": "1",
          "time": "20210823 13:50"
        },
        {
          "id": "MDRW2100010064.1.13",
          "form": "따로 봉사단체나 모임은 없으세요?",
          "original_form": "따로 봉사단체나 모임은 없으세요?",
          "speaker_id": "2",
          "time": "20210823 13:50"
        },
        {
          "id": "MDRW2100010064.1.14",
          "form": "친구들이랑 여행모임이 있었는데 요즘은 못가고있어요",
          "original_form": "친구들이랑 여행모임이 있었는데 요즘은 못가고있어요",
          "speaker_id": "1",
          "time": "20210823 13:51"
        },
        {
          "id": "MDRW2100010064.1.15",
          "form": "여행모임이라니",
          "original_form": "여행모임이라니",
          "speaker_id": "2",
          "time": "20210823 13:51"
        },
        {
          "id": "MDRW2100010064.1.16",
          "form": "너무좋은데요?",
          "original_form": "너무좋은데요?",
          "speaker_id": "2",
          "time": "20210823 13:51"
        },
        {
          "id": "MDRW2100010064.1.17",
          "form": "어디어디 다녀오셨어요?",
          "original_form": "어디어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210823 13:51"
        },
        {
          "id": "MDRW2100010064.1.18",
          "form": "일본이랑 대만에 다녀왔어요",
          "original_form": "일본이랑 대만에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210823 13:52"
        },
        {
          "id": "MDRW2100010064.1.19",
          "form": "모임에서 여행비를 모아 가시는 건가요?",
          "original_form": "모임에서 여행비를 모아 가시는 건가요?",
          "speaker_id": "2",
          "time": "20210823 13:52"
        },
        {
          "id": "MDRW2100010064.1.20",
          "form": "네ㅋㅋ 보통 숙소비까진 여행모임비에서 지출해요",
          "original_form": "네ㅋㅋ 보통 숙소비까진 여행모임비에서 지출해요",
          "speaker_id": "1",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100010064.1.21",
          "form": "좋네요ㅋㅋ",
          "original_form": "좋네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:54"
        }
      ]
