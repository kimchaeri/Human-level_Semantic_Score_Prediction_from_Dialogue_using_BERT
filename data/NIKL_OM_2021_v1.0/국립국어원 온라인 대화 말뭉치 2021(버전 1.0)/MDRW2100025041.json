      "utterance": [
        {
          "id": "MDRW2100025041.1.1",
          "form": "일본말은 별로더라",
          "original_form": "일본말은 별로더라",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025041.1.2",
          "form": "목소리가",
          "original_form": "목소리가",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025041.1.3",
          "form": "마자 왱알왱알 김성모 앵무새",
          "original_form": "마자 왱알왱알 김성모 앵무새",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025041.1.4",
          "form": "같아",
          "original_form": "같아",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025041.1.5",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋ일본성우가 더빙하는 목소리보다",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋ일본성우가 더빙하는 목소리보다",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100025041.1.6",
          "form": "한국성우더빙이 익숙해",
          "original_form": "한국성우더빙이 익숙해",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100025041.1.7",
          "form": "그래서 슬레이어즈도 다다다도",
          "original_form": "그래서 슬레이어즈도 다다다도",
          "speaker_id": "1",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100025041.1.8",
          "form": "재밌는데 한국어 버전으로만보게되더라",
          "original_form": "재밌는데 한국어 버전으로만보게되더라",
          "speaker_id": "1",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100025041.1.9",
          "form": "맞아 다다다 와 완전 추억",
          "original_form": "맞아 다다다 와 완전 추억",
          "speaker_id": "2",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100025041.1.10",
          "form": "따아-!",
          "original_form": "따아-!",
          "speaker_id": "2",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100025041.1.11",
          "form": "다다다 우주목소리",
          "original_form": "다다다 우주목소리",
          "speaker_id": "1",
          "time": "20210901 00:54"
        },
        {
          "id": "MDRW2100025041.1.12",
          "form": "한국머보다가",
          "original_form": "한국머보다가",
          "speaker_id": "1",
          "time": "20210901 00:54"
        },
        {
          "id": "MDRW2100025041.1.13",
          "form": "일본어보면 적응안됨",
          "original_form": "일본어보면 적응안됨",
          "speaker_id": "1",
          "time": "20210901 00:54"
        },
        {
          "id": "MDRW2100025041.1.14",
          "form": "투니버스로 길들여진 나",
          "original_form": "투니버스로 길들여진 나",
          "speaker_id": "1",
          "time": "20210901 00:54"
        },
        {
          "id": "MDRW2100025041.1.15",
          "form": "옛날 만화 뭐 봤음",
          "original_form": "옛날 만화 뭐 봤음",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100025041.1.16",
          "form": "옛날만화 카캡사",
          "original_form": "옛날만화 카캡사",
          "speaker_id": "1",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100025041.1.17",
          "form": "나루토",
          "original_form": "나루토",
          "speaker_id": "1",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100025041.1.18",
          "form": "그리고 또뭐가있지",
          "original_form": "그리고 또뭐가있지",
          "speaker_id": "1",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100025041.1.19",
          "form": "이누야사 인기많았는데",
          "original_form": "이누야사 인기많았는데",
          "speaker_id": "1",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100025041.1.20",
          "form": "난이누야샤는안봄",
          "original_form": "난이누야샤는안봄",
          "speaker_id": "1",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100025041.1.21",
          "form": "이누야사 띵작인데",
          "original_form": "이누야사 띵작인데",
          "speaker_id": "2",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100025041.1.22",
          "form": "꿀잼이야",
          "original_form": "꿀잼이야",
          "speaker_id": "2",
          "time": "20210901 01:18"
        },
        {
          "id": "MDRW2100025041.1.23",
          "form": "바바",
          "original_form": "바바",
          "speaker_id": "2",
          "time": "20210901 01:18"
        },
        {
          "id": "MDRW2100025041.1.24",
          "form": "이누야샤 요새 새로하던데",
          "original_form": "이누야샤 요새 새로하던데",
          "speaker_id": "1",
          "time": "20210901 01:19"
        },
        {
          "id": "MDRW2100025041.1.25",
          "form": "그것도 봄?",
          "original_form": "그것도 봄?",
          "speaker_id": "1",
          "time": "20210901 01:19"
        },
        {
          "id": "MDRW2100025041.1.26",
          "form": "그림체 완전 이상하던데",
          "original_form": "그림체 완전 이상하던데",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025041.1.27",
          "form": "못생겼어",
          "original_form": "못생겼어",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025041.1.28",
          "form": "그림체가달라짐?",
          "original_form": "그림체가달라짐?",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025041.1.29",
          "form": "내용이상하다는건 본것같은데",
          "original_form": "내용이상하다는건 본것같은데",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025041.1.30",
          "form": "가영이가 전국시대에서 고생을 많이 했구나...",
          "original_form": "가영이가 전국시대에서 고생을 많이 했구나...",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025041.1.31",
          "form": "영화추천좀",
          "original_form": "영화추천좀",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025041.1.32",
          "form": "그게 다음세대 애들 내용이라며",
          "original_form": "그게 다음세대 애들 내용이라며",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025041.1.33",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 01:20"
        }
      ]
    }
