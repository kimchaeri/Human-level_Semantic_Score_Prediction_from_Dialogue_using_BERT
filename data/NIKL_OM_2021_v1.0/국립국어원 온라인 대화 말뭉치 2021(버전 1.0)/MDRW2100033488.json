      "utterance": [
        {
          "id": "MDRW2100033488.1.1",
          "form": "있어",
          "original_form": "있어",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100033488.1.2",
          "form": "난 주로 까만옷을 입어",
          "original_form": "난 주로 까만옷을 입어",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100033488.1.3",
          "form": "오 확실히 오염이 덜되지",
          "original_form": "오 확실히 오염이 덜되지",
          "speaker_id": "1",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100033488.1.4",
          "form": "집에서 막 입어도 좋아",
          "original_form": "집에서 막 입어도 좋아",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100033488.1.5",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100033488.1.6",
          "form": "잠옷은 따로 사서입니?",
          "original_form": "잠옷은 따로 사서입니?",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.7",
          "form": "얘전에는 사람등이 짧은 바지를 많이 입었는데",
          "original_form": "얘전에는 사람등이 짧은 바지를 많이 입었는데",
          "speaker_id": "2",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.8",
          "form": "그게 건강에 안좋다더라",
          "original_form": "그게 건강에 안좋다더라",
          "speaker_id": "2",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.9",
          "form": "아니. 사고싶어",
          "original_form": "아니. 사고싶어",
          "speaker_id": "2",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.10",
          "form": "잠옷이 따로 있는게 수면질을 향상시켜준대서.",
          "original_form": "잠옷이 따로 있는게 수면질을 향상시켜준대서.",
          "speaker_id": "2",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.11",
          "form": "짧은바지는 너무 불편해",
          "original_form": "짧은바지는 너무 불편해",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.12",
          "form": "나는 따로 없는데 잠옷이 두벌정도 있다면 좋을 거 같아.",
          "original_form": "나는 따로 없는데 잠옷이 두벌정도 있다면 좋을 거 같아.",
          "speaker_id": "2",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.13",
          "form": "맞아 친구가 입는데 기분이좋대",
          "original_form": "맞아 친구가 입는데 기분이좋대",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.14",
          "form": "맞아. 궁금하다!",
          "original_form": "맞아. 궁금하다!",
          "speaker_id": "2",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.15",
          "form": "겨울 잠옷도 따로없니?",
          "original_form": "겨울 잠옷도 따로없니?",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.16",
          "form": "수면바지라면 세벌있어",
          "original_form": "수면바지라면 세벌있어",
          "speaker_id": "2",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.17",
          "form": "꽤나 많지?",
          "original_form": "꽤나 많지?",
          "speaker_id": "2",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.18",
          "form": "그럼 이번 생일선물로는 어때!?",
          "original_form": "그럼 이번 생일선물로는 어때!?",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100033488.1.19",
          "form": "수면바지는 가끔 답답해",
          "original_form": "수면바지는 가끔 답답해",
          "speaker_id": "2",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100033488.1.20",
          "form": "싫어",
          "original_form": "싫어",
          "speaker_id": "2",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100033488.1.21",
          "form": "그렇구나 취향을 존중할ㅋ게",
          "original_form": "그렇구나 취향을 존중할ㅋ게",
          "speaker_id": "1",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100033488.1.22",
          "form": "느낌표와 물음표 떼.",
          "original_form": "느낌표와 물음표 떼.",
          "speaker_id": "2",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100033488.1.23",
          "form": "물어보길 잘했다 사줄뻔했어",
          "original_form": "물어보길 잘했다 사줄뻔했어",
          "speaker_id": "1",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100033488.1.24",
          "form": "나는 그런건 왠지 돈 아까운 거 같아.",
          "original_form": "나는 그런건 왠지 돈 아까운 거 같아.",
          "speaker_id": "2",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100033488.1.25",
          "form": "흠 그건 나도그래",
          "original_form": "흠 그건 나도그래",
          "speaker_id": "1",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100033488.1.26",
          "form": "집에서 입을 옷이 많은걸",
          "original_form": "집에서 입을 옷이 많은걸",
          "speaker_id": "1",
          "time": "20210824 00:45"
        },
        {
          "id": "MDRW2100033488.1.27",
          "form": "재치있구나ㅡ",
          "original_form": "재치있구나ㅡ",
          "speaker_id": "2",
          "time": "20210824 00:45"
        }
      ]
    }
  ]
