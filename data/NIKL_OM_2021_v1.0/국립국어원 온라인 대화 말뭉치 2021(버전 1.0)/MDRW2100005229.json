      "utterance": [
        {
          "id": "MDRW2100005229.1.1",
          "form": "연애가 어렵네요",
          "original_form": "연애가 어렵네요",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005229.1.2",
          "form": "요즘엔 더더욱 그런거 같아요..",
          "original_form": "요즘엔 더더욱 그런거 같아요..",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005229.1.3",
          "form": "만나기도 쉽지.않고...",
          "original_form": "만나기도 쉽지.않고...",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005229.1.4",
          "form": "아 아무래도 상황이 상황이다보니...",
          "original_form": "아 아무래도 상황이 상황이다보니...",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005229.1.5",
          "form": "연애나 결혼 중이신가요",
          "original_form": "연애나 결혼 중이신가요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005229.1.6",
          "form": "둘다 해당이 안 되네요..ㅎㅎ",
          "original_form": "둘다 해당이 안 되네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005229.1.7",
          "form": "가장 자유로운 영혼!",
          "original_form": "가장 자유로운 영혼!",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005229.1.8",
          "form": "동지시네요ㅋ",
          "original_form": "동지시네요ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005229.1.9",
          "form": "어머! 그런가요??ㅎ",
          "original_form": "어머! 그런가요??ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005229.1.10",
          "form": "만나고 싶어도 만날 방법이 없네요..ㅠ",
          "original_form": "만나고 싶어도 만날 방법이 없네요..ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005229.1.11",
          "form": "엇 왜요 이리저리 방법을 만들어봐야죠!",
          "original_form": "엇 왜요 이리저리 방법을 만들어봐야죠!",
          "speaker_id": "1",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005229.1.12",
          "form": "소개받기도 애매하고 모임같은 걸 나가자니 코로나로 쉽지가 않고..",
          "original_form": "소개받기도 애매하고 모임같은 걸 나가자니 코로나로 쉽지가 않고..",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005229.1.13",
          "form": "이래저래 막막하네요..ㅎ",
          "original_form": "이래저래 막막하네요..ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005229.1.14",
          "form": "ㅜㅜ 왜 막막해요 소개받으세요ㅋ",
          "original_form": "ㅜㅜ 왜 막막해요 소개받으세요ㅋ",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100005229.1.15",
          "form": "뭔가.. 소개 받아서 만나는 거에 대해서 아직은 불편함이 있어서요..ㅎ",
          "original_form": "뭔가.. 소개 받아서 만나는 거에 대해서 아직은 불편함이 있어서요..ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100005229.1.16",
          "form": "소개 받으신 적 있으세요??",
          "original_form": "소개 받으신 적 있으세요??",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100005229.1.17",
          "form": "저는 한 번 받았는데... 별로였어요ㅜㅜ",
          "original_form": "저는 한 번 받았는데... 별로였어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100005229.1.18",
          "form": "자만추!",
          "original_form": "자만추!",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100005229.1.19",
          "form": "맞아요~// 자만추",
          "original_form": "맞아요~// 자만추",
          "speaker_id": "2",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100005229.1.20",
          "form": "제일 바라는 방법이 자만추인데..ㅋㅋㅋ",
          "original_form": "제일 바라는 방법이 자만추인데..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100005229.1.21",
          "form": "나가지를 못하니..",
          "original_form": "나가지를 못하니..",
          "speaker_id": "2",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100005229.1.22",
          "form": "자만추는 어렵죠ㅜㅜ 코로나가 문제",
          "original_form": "자만추는 어렵죠ㅜㅜ 코로나가 문제",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100005229.1.23",
          "form": "그러니까요.. 코로나만 아니면 모임에 나가던지 해서 무슨 일이라도 생겼울텐데..",
          "original_form": "그러니까요.. 코로나만 아니면 모임에 나가던지 해서 무슨 일이라도 생겼울텐데..",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100005229.1.24",
          "form": "요즘엔 자만추가 쉽지 않네요..ㅠ",
          "original_form": "요즘엔 자만추가 쉽지 않네요..ㅠ",
          "speaker_id": "2",
          "time": "20210822 12:18"
        }
      ]
    }
