      "utterance": [
        {
          "id": "MDRW2100014672.1.1",
          "form": "최근에 마음에드신 쇼핑이 있으신가요?",
          "original_form": "최근에 마음에드신 쇼핑이 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014672.1.2",
          "form": "다이소에서 산 물건이요",
          "original_form": "다이소에서 산 물건이요",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014672.1.3",
          "form": "무엇을 구매하셨나요?",
          "original_form": "무엇을 구매하셨나요?",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014672.1.4",
          "form": "주방에 붙이는 알루미늄 벽지를 샀어요",
          "original_form": "주방에 붙이는 알루미늄 벽지를 샀어요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014672.1.5",
          "form": "오 무슨무늬인데요?",
          "original_form": "오 무슨무늬인데요?",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014672.1.6",
          "form": "귀여운 은색 파인애플 무늬였어요",
          "original_form": "귀여운 은색 파인애플 무늬였어요",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014672.1.7",
          "form": "귀여운 은색 파인애플이라니\r\n상상이안가네요",
          "original_form": "귀여운 은색 파인애플이라니\r\n상상이안가네요",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014672.1.8",
          "form": "인터넷 한번 찾아보세요",
          "original_form": "인터넷 한번 찾아보세요",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014672.1.9",
          "form": "네... 열심히 찾아볼게요.",
          "original_form": "네... 열심히 찾아볼게요.",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014672.1.10",
          "form": "저는 엄마가 구매하신 오렌지 접시가 제일좋아요",
          "original_form": "저는 엄마가 구매하신 오렌지 접시가 제일좋아요",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014672.1.11",
          "form": "아 그거 제가 골랐어요",
          "original_form": "아 그거 제가 골랐어요",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014672.1.12",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014672.1.13",
          "form": "고르는 센스가 굳이시네뇨",
          "original_form": "고르는 센스가 굳이시네뇨",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014672.1.14",
          "form": "알아요",
          "original_form": "알아요",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014672.1.15",
          "form": "저는 그 외에 창문에붙히는 블라인드가 좋았어요",
          "original_form": "저는 그 외에 창문에붙히는 블라인드가 좋았어요",
          "speaker_id": "1",
          "time": "20210825 20:36"
        }
