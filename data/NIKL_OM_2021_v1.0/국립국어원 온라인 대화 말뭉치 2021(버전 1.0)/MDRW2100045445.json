      "utterance": [
        {
          "id": "MDRW2100045445.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045445.1.2",
          "form": "아뇨 학생은 아니에요",
          "original_form": "아뇨 학생은 아니에요",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045445.1.3",
          "form": "학교 다니시나요?",
          "original_form": "학교 다니시나요?",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045445.1.4",
          "form": "아니요..ㅋㅋㅋㅋ 그럼 학창시절 얘기 할까요",
          "original_form": "아니요..ㅋㅋㅋㅋ 그럼 학창시절 얘기 할까요",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045445.1.5",
          "form": "ㅋㅋㅋㅋㅋ 넵...",
          "original_form": "ㅋㅋㅋㅋㅋ 넵...",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045445.1.6",
          "form": "기억에 남는 추억 잇으신가요",
          "original_form": "기억에 남는 추억 잇으신가요",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045445.1.7",
          "form": "저 야자빼고 한강 간거요 ㅋㅋㅋㅋ",
          "original_form": "저 야자빼고 한강 간거요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045445.1.8",
          "form": "앗 ㅋㅋㅋㅋㅋ 정말 추억이네요 ㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋㅋ 정말 추억이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045445.1.9",
          "form": "가서 치킨 드셧나요",
          "original_form": "가서 치킨 드셧나요",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045445.1.10",
          "form": "네!!!ㅋㅋㅋㅋㅋ 한강하면 치킨이져",
          "original_form": "네!!!ㅋㅋㅋㅋㅋ 한강하면 치킨이져",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045445.1.11",
          "form": "ㅋㅋㅋㅋ 한강을 제대로 느끼셨네요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 한강을 제대로 느끼셨네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045445.1.12",
          "form": "저흰 기숙사였어서",
          "original_form": "저흰 기숙사였어서",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045445.1.13",
          "form": "몰래 가방에 치킨담아와서 방 안에서 먹고 그랬어요 ㅋㅋㅋ",
          "original_form": "몰래 가방에 치킨담아와서 방 안에서 먹고 그랬어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045445.1.14",
          "form": "꺄 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "꺄 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:43"
        },
        {
