      "utterance": [
        {
          "id": "MDRW2100045962.1.1",
          "form": "진짜 꽤 작은 크기에요",
          "original_form": "진짜 꽤 작은 크기에요",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045962.1.2",
          "form": "근데 먹다보면 많아요",
          "original_form": "근데 먹다보면 많아요",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045962.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045962.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ앗.. 생각보다 더 빨리 물리나봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ앗.. 생각보다 더 빨리 물리나봐요",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045962.1.5",
          "form": "뭔가 주먹만한 크기라면",
          "original_form": "뭔가 주먹만한 크기라면",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045962.1.6",
          "form": "순식간에 해치울 것만 같은데",
          "original_form": "순식간에 해치울 것만 같은데",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045962.1.7",
          "form": "최대한 작디작은 걸로 입문을 해 볼게요ㅋㅋㅋㅋ",
          "original_form": "최대한 작디작은 걸로 입문을 해 볼게요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045962.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045962.1.9",
          "form": "저만 그런 걸 수도 있어서 ㅋㅋㅋ",
          "original_form": "저만 그런 걸 수도 있어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045962.1.10",
          "form": "일단은 작은거 한번 드셔보세용 ㅎㅎ",
          "original_form": "일단은 작은거 한번 드셔보세용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045962.1.11",
          "form": "단거 짠거 어떤거 좋아하세요?",
          "original_form": "단거 짠거 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045962.1.12",
          "form": "요거트면 전 단 거요! 아마도...?",
          "original_form": "요거트면 전 단 거요! 아마도...?",
          "speaker_id": "2",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045962.1.13",
          "form": "앗 아녀 그냥 빵이나 이런거 ㅋㅋㅋ",
          "original_form": "앗 아녀 그냥 빵이나 이런거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045962.1.14",
          "form": "음식취향!",
          "original_form": "음식취향!",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045962.1.15",
          "form": "저는 음음음",
          "original_form": "저는 음음음",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045962.1.16",
          "form": "그악 하나만 고르기 너무 어려워요ㅌㅋㅌㅌㅋ",
          "original_form": "그악 하나만 고르기 너무 어려워요ㅌㅋㅌㅌㅋ",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045962.1.17",
          "form": "ㅋㅋㄱㅋㅋㄱㅋㄱㅋㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㄱㅋㄱㅋㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045962.1.18",
          "form": "그래도 고르자면 짠 거요!",
          "original_form": "그래도 고르자면 짠 거요!",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045962.1.19",
          "form": "저도 짠걸 더 좋아햐요!",
          "original_form": "저도 짠걸 더 좋아햐요!",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045962.1.20",
          "form": "빵도 소세지빵.. 피자빵.. 이런걸 더 사랑하는 편이에요",
          "original_form": "빵도 소세지빵.. 피자빵.. 이런걸 더 사랑하는 편이에요",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045962.1.21",
          "form": "맞아요 짱 맛있어요",
          "original_form": "맞아요 짱 맛있어요",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045962.1.22",
          "form": "소세지 들어간 빵들 넘 좋아요 ㅋㅋㅋㅋㅋ",
          "original_form": "소세지 들어간 빵들 넘 좋아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045962.1.23",
          "form": "아 저 파리바게트 알바 한 적 있는데",
          "original_form": "아 저 파리바게트 알바 한 적 있는데",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045962.1.24",
          "form": "진짜 너무 맛있어요",
          "original_form": "진짜 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045962.1.25",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045962.1.26",
          "form": "돈 벌어서 다 빵값으로 쓴다는 그 파바요?",
          "original_form": "돈 벌어서 다 빵값으로 쓴다는 그 파바요?",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045962.1.27",
          "form": "ㅋㅋㅋ ㅋ ㅋ ㅋ ㅋㅋ",
          "original_form": "ㅋㅋㅋ ㅋ ㅋ ㅋ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045962.1.28",
          "form": "갈릭 소시지 페스츄리가 소세지가 되게 탱글탱글 좋아요",
          "original_form": "갈릭 소시지 페스츄리가 소세지가 되게 탱글탱글 좋아요",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045962.1.29",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045962.1.30",
          "form": "저 지금캡쳐해요",
          "original_form": "저 지금캡쳐해요",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045962.1.31",
          "form": "꼭 먹어볼게요",
          "original_form": "꼭 먹어볼게요",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045962.1.32",
          "form": "다행히 저는 참았습닏다..!",
          "original_form": "다행히 저는 참았습닏다..!",
          "speaker_id": "2",
