      "utterance": [
        {
          "id": "MDRW2100044979.1.1",
          "form": "최근에 어떤 물건 사셨나요",
          "original_form": "최근에 어떤 물건 사셨나요",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044979.1.2",
          "form": "저 초고속 츙전선이요!",
          "original_form": "저 초고속 츙전선이요!",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044979.1.3",
          "form": "이전 충전기는 고장났나요?",
          "original_form": "이전 충전기는 고장났나요?",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044979.1.4",
          "form": "네 ㅠㅠㅠ 고속충전으론 만족 못해요",
          "original_form": "네 ㅠㅠㅠ 고속충전으론 만족 못해요",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044979.1.5",
          "form": "초고속 짱",
          "original_form": "초고속 짱",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044979.1.6",
          "form": "두개 차이가 확실한가요?",
          "original_form": "두개 차이가 확실한가요?",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044979.1.7",
          "form": "네 엄청요",
          "original_form": "네 엄청요",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044979.1.8",
          "form": "지금 저는 고속 충전기 쓰는데 초고속으로 바꿔야겠어요 답답하거든요",
          "original_form": "지금 저는 고속 충전기 쓰는데 초고속으로 바꿔야겠어요 답답하거든요",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044979.1.9",
          "form": "그쵸 진짜 그렇다니까요",
          "original_form": "그쵸 진짜 그렇다니까요",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044979.1.10",
          "form": "고속은 충전기 연결하면 하얀 동그라미잖아요",
          "original_form": "고속은 충전기 연결하면 하얀 동그라미잖아요",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044979.1.11",
          "form": "초고속은 파란 동그라미에요",
          "original_form": "초고속은 파란 동그라미에요",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044979.1.12",
          "form": "폰 고장 안 나나요?",
          "original_form": "폰 고장 안 나나요?",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044979.1.13",
          "form": "초고속 충전긴는 정품인데용..?",
          "original_form": "초고속 충전긴는 정품인데용..?",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044979.1.14",
          "form": "정품으로도 있어요? 고속 충전만 봐서요",
          "original_form": "정품으로도 있어요? 고속 충전만 봐서요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044979.1.15",
          "form": "아 혹시 아이폰이시면 모르겠어요..ㅠ 저는 갤럭신데 초고속 충전이 있어요!",
          "original_form": "아 혹시 아이폰이시면 모르겠어요..ㅠ 저는 갤럭신데 초고속 충전이 있어요!",
          "speaker_id": "1",
          "time": "20210908 00:42"
        }
      ]
