      "utterance": [
        {
          "id": "MDRW2100017362.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017362.1.2",
          "form": "공연이나 뮤지컬 좋아해요?",
          "original_form": "공연이나 뮤지컬 좋아해요?",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017362.1.3",
          "form": "안녕하세요~ 네네 종종 챙겨갑니다ㅎㅎ",
          "original_form": "안녕하세요~ 네네 종종 챙겨갑니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017362.1.4",
          "form": "최근엔 시카고 봤어요",
          "original_form": "최근엔 시카고 봤어요",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017362.1.5",
          "form": "뭐 좋아하시나요?",
          "original_form": "뭐 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017362.1.6",
          "form": "전 최근에 영화 봤어요",
          "original_form": "전 최근에 영화 봤어요",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017362.1.7",
          "form": "인질하고 씽크홀이요 ㅎㅎ",
          "original_form": "인질하고 씽크홀이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017362.1.8",
          "form": "아하 전 영화는 생각보다 그렇게 안 땡기더라구요 그래서 자주 안 봤어요ㅋㅋㅋ",
          "original_form": "아하 전 영화는 생각보다 그렇게 안 땡기더라구요 그래서 자주 안 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017362.1.9",
          "form": "둘 다 유명한가요?",
          "original_form": "둘 다 유명한가요?",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017362.1.10",
          "form": "인질은 들어본 것 같은데",
          "original_form": "인질은 들어본 것 같은데",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017362.1.11",
          "form": "최근에 나온 영화에요 둘 다 한국영화 ㅎㅎ",
          "original_form": "최근에 나온 영화에요 둘 다 한국영화 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017362.1.12",
          "form": "인질은 황정민 배우가 주연이에요.",
          "original_form": "인질은 황정민 배우가 주연이에요.",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017362.1.13",
          "form": "아 인질 배우 짱짱하다던 영화군요? 들어봤어요 아닌가ㅋㅋㅋ",
          "original_form": "아 인질 배우 짱짱하다던 영화군요? 들어봤어요 아닌가ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017362.1.14",
          "form": "ㅋㅋ ㅋ 근데 줄거리가 좀 아쉬웠어요..",
          "original_form": "ㅋㅋ ㅋ 근데 줄거리가 좀 아쉬웠어요..",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017362.1.15",
          "form": "개연성이 너무 없어서",
          "original_form": "개연성이 너무 없어서",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017362.1.16",
          "form": "ㅋㅋㅋ본 사람도 그러더라구요 억지라던데",
          "original_form": "ㅋㅋㅋ본 사람도 그러더라구요 억지라던데",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017362.1.17",
          "form": "저는 비추천하겠습니다..",
          "original_form": "저는 비추천하겠습니다..",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017362.1.18",
          "form": "한국영화의 후퇴에요 ㅠ",
          "original_form": "한국영화의 후퇴에요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017362.1.19",
          "form": "헐 그정도구나...",
          "original_form": "헐 그정도구나...",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017362.1.20",
          "form": "기생충보고 충격받았었어요ㅎㅎ 너무 잘 만들고 계속 생각하게해서",
          "original_form": "기생충보고 충격받았었어요ㅎㅎ 너무 잘 만들고 계속 생각하게해서",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017362.1.21",
          "form": "그래도 코로나19 때문인지 영화관에 사람 없어서 좋았어요",
          "original_form": "그래도 코로나19 때문인지 영화관에 사람 없어서 좋았어요",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017362.1.22",
          "form": "저도 오히려 기생충이 좋았는데 욕하는 사람도 있더군요",
          "original_form": "저도 오히려 기생충이 좋았는데 욕하는 사람도 있더군요",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017362.1.23",
          "form": "왜 욕을;",
          "original_form": "왜 욕을;",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017362.1.24",
          "form": "진짜 사회를 잘 빗대어 만들었던데",
          "original_form": "진짜 사회를 잘 빗대어 만들었던데",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017362.1.25",
          "form": "그러게요ㅜㅜ 보는 눈이 다른가봐요",
          "original_form": "그러게요ㅜㅜ 보는 눈이 다른가봐요",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017362.1.26",
          "form": "또 어떤거 인상 깊게 보셨어요?",
          "original_form": "또 어떤거 인상 깊게 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017362.1.27",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017362.1.28",
          "form": "먼훗날우리 요",
          "original_form": "먼훗날우리 요",
          "speaker_id": "2",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017362.1.29",
          "form": "오 ㅓㅊ음 듣는 영화인데 소개해줄 수 있어요?",
          "original_form": "오 ㅓㅊ음 듣는 영화인데 소개해줄 수 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017362.1.30",
          "form": "어떤 내용이에요?",
          "original_form": "어떤 내용이에요?",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017362.1.31",
          "form": "중국영환데요 추천받고 틀었다가  아뭐야 하고 봤다가 눈물 흘린 영화에요",
          "original_form": "중국영환데요 추천받고 틀었다가  아뭐야 하고 봤다가 눈물 흘린 영화에요",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017362.1.32",
          "form": "내용 말하면 스포 같은데ㅋㅋㅋ",
          "original_form": "내용 말하면 스포 같은데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017362.1.33",
          "form": "로맨스 멜로 영화니까 이쪽 좋아하시면 함 보세요",
          "original_form": "로맨스 멜로 영화니까 이쪽 좋아하시면 함 보세요",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017362.1.34",
          "form": "헐 그렇구나 ㅜㅜ",
          "original_form": "헐 그렇구나 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017362.1.35",
          "form": "추천 해주셨으니 꼭 봐볼게요~",
          "original_form": "추천 해주셨으니 꼭 봐볼게요~",
          "speaker_id": "1",
          "time": "20210826 19:49"
        }
      ]
    }
  ]
