      "utterance": [
        {
          "id": "MDRW2100046143.1.1",
          "form": "저는 단독은 못먹고",
          "original_form": "저는 단독은 못먹고",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046143.1.2",
          "form": "민트초코는 먹어요 ㅋㅋ",
          "original_form": "민트초코는 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046143.1.3",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046143.1.4",
          "form": "민트 초코 어떤 제품 가장 좋아하세요?",
          "original_form": "민트 초코 어떤 제품 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046143.1.5",
          "form": "제품.. 베라 같은거요??",
          "original_form": "제품.. 베라 같은거요??",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046143.1.6",
          "form": "네! 요즘 뮤ㅓ 많이 나오더라구요!",
          "original_form": "네! 요즘 뮤ㅓ 많이 나오더라구요!",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046143.1.7",
          "form": "오오 민트 ㅋㅋㅋ 민트 과자도 마니 나와요",
          "original_form": "오오 민트 ㅋㅋㅋ 민트 과자도 마니 나와요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046143.1.8",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046143.1.9",
          "form": "어떤거 두셔보셨어용?",
          "original_form": "어떤거 두셔보셨어용?",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100046143.1.10",
          "form": "그.. 민트맛 초코과자랑 섞인..?",
          "original_form": "그.. 민트맛 초코과자랑 섞인..?",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046143.1.11",
          "form": "과자이름이 기억이안나네요 ㅋㅋ",
          "original_form": "과자이름이 기억이안나네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046143.1.12",
          "form": "뭘까요 궁금하네요!",
          "original_form": "뭘까요 궁금하네요!",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046143.1.13",
          "form": "그 친구가 맛보라고해서 기억이안나네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 친구가 맛보라고해서 기억이안나네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046143.1.14",
          "form": "콜라보한 제품 마니 나와서 신기해요 ㅋㅋ",
          "original_form": "콜라보한 제품 마니 나와서 신기해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046143.1.15",
          "form": "맞아요!! 저는 abc초콜릿이랑 콜라보한거 먹어보고싶어요",
          "original_form": "맞아요!! 저는 abc초콜릿이랑 콜라보한거 먹어보고싶어요",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046143.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 민트 엄청 좋아하시나봐용",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 민트 엄청 좋아하시나봐용",
          "speaker_id": "2",
