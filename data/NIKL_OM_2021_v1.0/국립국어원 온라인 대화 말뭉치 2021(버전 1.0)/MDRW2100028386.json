      "utterance": [
        {
          "id": "MDRW2100028386.1.1",
          "form": "오늘 춥지 않아요?",
          "original_form": "오늘 춥지 않아요?",
          "speaker_id": "1",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028386.1.2",
          "form": "비가 많이 와서 그래",
          "original_form": "비가 많이 와서 그래",
          "speaker_id": "2",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028386.1.3",
          "form": "비오는거 더운것보단 낫긴 한데 ㅠㅠ 찝찝해요",
          "original_form": "비오는거 더운것보단 낫긴 한데 ㅠㅠ 찝찝해요",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028386.1.4",
          "form": "맨날 그래서 에어컨 틀어달라고 하잖아",
          "original_form": "맨날 그래서 에어컨 틀어달라고 하잖아",
          "speaker_id": "2",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028386.1.5",
          "form": "그건 그런데 ㅎㅎ 비 많이 와서 창문을 못열어놓겠다구요",
          "original_form": "그건 그런데 ㅎㅎ 비 많이 와서 창문을 못열어놓겠다구요",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028386.1.6",
          "form": "비 들이치면 빨래 젖으니까 어쩔 수 없지",
          "original_form": "비 들이치면 빨래 젖으니까 어쩔 수 없지",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028386.1.7",
          "form": "밤에 창문 열어놓지 말아라",
          "original_form": "밤에 창문 열어놓지 말아라",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028386.1.8",
          "form": "목에 머플러 감고 자고 있어요",
          "original_form": "목에 머플러 감고 자고 있어요",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028386.1.9",
          "form": "감기 안걸리려고!!",
          "original_form": "감기 안걸리려고!!",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028386.1.10",
          "form": "엄마도 감기 조심하세요",
          "original_form": "엄마도 감기 조심하세요",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028386.1.11",
          "form": "그래 일교차 심해지면 감기 걸리기 쉬우니까",
          "original_form": "그래 일교차 심해지면 감기 걸리기 쉬우니까",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028386.1.12",
          "form": "엄마도 스카프 하고 다닌다",
          "original_form": "엄마도 스카프 하고 다닌다",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028386.1.13",
          "form": "그거 아직도 하세요?",
          "original_form": "그거 아직도 하세요?",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028386.1.14",
          "form": "엄마 친구가 준거야",
          "original_form": "엄마 친구가 준거야",
          "speaker_id": "2",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028386.1.15",
          "form": "좀 더워보이던데... 곧 겨울이니까 괜찮나",
          "original_form": "좀 더워보이던데... 곧 겨울이니까 괜찮나",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028386.1.16",
          "form": "ㅠㅠ 맨날 빠는거 귀찮은데요",
          "original_form": "ㅠㅠ 맨날 빠는거 귀찮은데요",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028386.1.17",
          "form": "땀 많이 나니까...",
          "original_form": "땀 많이 나니까...",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028386.1.18",
          "form": "그래도 냄새 좋잔니",
          "original_form": "그래도 냄새 좋잔니",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028386.1.19",
          "form": "그건 그렇지만요 제 체취 별로더라구요",
          "original_form": "그건 그렇지만요 제 체취 별로더라구요",
          "speaker_id": "1",
          "time": "20210902 20:14"
        }
