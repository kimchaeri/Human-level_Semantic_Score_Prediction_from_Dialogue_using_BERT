          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038478.1.3",
          "form": "과일주스가 마시고 싶당",
          "original_form": "과일주스가 마시고 싶당",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038478.1.4",
          "form": "그런 거 몸에 안 좋다",
          "original_form": "그런 거 몸에 안 좋다",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038478.1.5",
          "form": "홀 과일주스가?",
          "original_form": "홀 과일주스가?",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038478.1.6",
          "form": "그래 설탕이 너무 많아서",
          "original_form": "그래 설탕이 너무 많아서",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100038478.1.7",
          "form": "헐 그렇군..근데 무가당 100프로 이런것도 있잖아",
          "original_form": "헐 그렇군..근데 무가당 100프로 이런것도 있잖아",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100038478.1.8",
          "form": "과당이 많다",
          "original_form": "과당이 많다",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100038478.1.9",
          "form": "아 그렇네.. 과당이 있구만 ㅜ",
          "original_form": "아 그렇네.. 과당이 있구만 ㅜ",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100038478.1.10",
          "form": "직접 갈아서 먹어라",
          "original_form": "직접 갈아서 먹어라",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100038478.1.11",
          "form": "그래야겠당 ㅋㅋ",
          "original_form": "그래야겠당 ㅋㅋ",
