      "utterance": [
        {
          "id": "MDRW2100030953.1.1",
          "form": "코로나 백신 예약 하셨나요",
          "original_form": "코로나 백신 예약 하셨나요",
          "speaker_id": "1",
          "time": "20210904 14:41"
        },
        {
          "id": "MDRW2100030953.1.2",
          "form": "네 예약했어요",
          "original_form": "네 예약했어요",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030953.1.3",
          "form": "추석있는 주에 금요일에요",
          "original_form": "추석있는 주에 금요일에요",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030953.1.4",
          "form": "예약하셨어요?",
          "original_form": "예약하셨어요?",
          "speaker_id": "2",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100030953.1.5",
          "form": "전 담주 화요일 합니다",
          "original_form": "전 담주 화요일 합니다",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100030953.1.6",
          "form": "종 두렵기는해요",
          "original_form": "종 두렵기는해요",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100030953.1.7",
          "form": "얼마 안남았네요",
          "original_form": "얼마 안남았네요",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100030953.1.8",
          "form": "화이자인가요?",
          "original_form": "화이자인가요?",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100030953.1.9",
          "form": "네 화이자 이죠",
          "original_form": "네 화이자 이죠",
          "speaker_id": "1",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100030953.1.10",
          "form": "부작용 뉴스가 나오니 넘 걱정되요",
          "original_form": "부작용 뉴스가 나오니 넘 걱정되요",
          "speaker_id": "1",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100030953.1.11",
          "form": "네 뉴스가 넘 불안하게 만들긴해요",
          "original_form": "네 뉴스가 넘 불안하게 만들긴해요",
          "speaker_id": "2",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100030953.1.12",
          "form": "근데 극소수라니 넘걱정말고 맞자구요",
          "original_form": "근데 극소수라니 넘걱정말고 맞자구요",
          "speaker_id": "2",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100030953.1.13",
          "form": "막상 맞고나면 또 별거아니일꺼예요",
          "original_form": "막상 맞고나면 또 별거아니일꺼예요",
          "speaker_id": "2",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100030953.1.14",
          "form": "그렇겠죠",
          "original_form": "그렇겠죠",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100030953.1.15",
          "form": "회사 사람들도 큰 문제 없이 맞은 사람도 많이 있죠",
          "original_form": "회사 사람들도 큰 문제 없이 맞은 사람도 많이 있죠",
          "speaker_id": "1",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100030953.1.16",
          "form": "네 저희회사도",
          "original_form": "네 저희회사도",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100030953.1.17",
          "form": "큰 문제 없으시더라구요",
          "original_form": "큰 문제 없으시더라구요",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100030953.1.18",
          "form": "이번 한번으로 끝날수 있을까요 ?",
          "original_form": "이번 한번으로 끝날수 있을까요 ?",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100030953.1.19",
          "form": "근데 한번에 안끝날것같아요",
          "original_form": "근데 한번에 안끝날것같아요",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100030953.1.20",
          "form": "매년 독감맞듯이 맞아야하는거아닌가 싶어요",
          "original_form": "매년 독감맞듯이 맞아야하는거아닌가 싶어요",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100030953.1.21",
          "form": "글쎄요 그럴수도 있겠다는 생각도 들긴해요",
          "original_form": "글쎄요 그럴수도 있겠다는 생각도 들긴해요",
          "speaker_id": "1",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100030953.1.22",
          "form": "해마다 맞아야하는 주사들이 많아지네요",
          "original_form": "해마다 맞아야하는 주사들이 많아지네요",
          "speaker_id": "1",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100030953.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100030953.1.24",
          "form": "요즘 주변에서 맞고 아프다는 이야기들으니 겁나요",
          "original_form": "요즘 주변에서 맞고 아프다는 이야기들으니 겁나요",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100030953.1.25",
          "form": "오늘 드디어 맞았답니다",
          "original_form": "오늘 드디어 맞았답니다",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100030953.1.26",
          "form": "크게 아픈지는 않네요",
          "original_form": "크게 아픈지는 않네요",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100030953.1.27",
          "form": "살짝 근육통 ..... 괜찮아요",
          "original_form": "살짝 근육통 ..... 괜찮아요",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100030953.1.28",
          "form": "걱정 안하셔도 될것 같아요",
          "original_form": "걱정 안하셔도 될것 같아요",
          "speaker_id": "1",
          "time": "20210907 19:31"
        }
