      "utterance": [
        {
          "id": "MDRW2100034652.1.1",
          "form": "커피 말고 차 종류는 좋아하시나요?",
          "original_form": "커피 말고 차 종류는 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100034652.1.2",
          "form": "전 커피를 못 마시니까 그럴때 캐모마일 마셔요 밖에서는요 ㅋㅋㅋ",
          "original_form": "전 커피를 못 마시니까 그럴때 캐모마일 마셔요 밖에서는요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100034652.1.3",
          "form": "칼로리만 아니면 안 마실텐데요 사실..ㅋㅋㅋㅋ",
          "original_form": "칼로리만 아니면 안 마실텐데요 사실..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100034652.1.4",
          "form": "저는 허브 종류는 잘 안 마시게 되더라구요",
          "original_form": "저는 허브 종류는 잘 안 마시게 되더라구요",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034652.1.5",
          "form": "좀 밍밍하죠 ㅋㅋㅋ",
          "original_form": "좀 밍밍하죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034652.1.6",
          "form": "그쵸 향도 좀 쎄기도 하고..",
          "original_form": "그쵸 향도 좀 쎄기도 하고..",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034652.1.7",
          "form": "전 그냥 0칼로리라 부담없이 마셔요",
          "original_form": "전 그냥 0칼로리라 부담없이 마셔요",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034652.1.8",
          "form": "사실 디저트를 좋아해서 아메리카노가 최고긴한데요 ㅋㅋㅋ",
          "original_form": "사실 디저트를 좋아해서 아메리카노가 최고긴한데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034652.1.9",
          "form": "달달한 디저트와 아메리카노 조합 너무 좋죠!",
          "original_form": "달달한 디저트와 아메리카노 조합 너무 좋죠!",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034652.1.10",
          "form": "맞아요 단쓴단쓴",
          "original_form": "맞아요 단쓴단쓴",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034652.1.11",
          "form": "단짠에 이은 최고조합이에요 ㅋㅋ",
          "original_form": "단짠에 이은 최고조합이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034652.1.12",
          "form": "정말 최고의 조합이에요!!",
          "original_form": "정말 최고의 조합이에요!!",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034652.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034652.1.14",
          "form": "오늘도 마카롱먹었어요 ㅋㅋ",
          "original_form": "오늘도 마카롱먹었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034652.1.15",
          "form": "오 마카롱 달달하고 맛있죠",
          "original_form": "오 마카롱 달달하고 맛있죠",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034652.1.16",
          "form": "택배도 되더라구여",
          "original_form": "택배도 되더라구여",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034652.1.17",
          "form": "전 마카롱은 먹은지 좀 오래되었어요",
