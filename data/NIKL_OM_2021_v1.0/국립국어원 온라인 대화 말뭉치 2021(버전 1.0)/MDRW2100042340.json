        {
          "id": "MDRW2100042340.1.1",
          "form": "출석 비중 높게 하고 싶지 않다고 그랬음",
          "original_form": "출석 비중 높게 하고 싶지 않다고 그랬음",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042340.1.2",
          "form": "이야...",
          "original_form": "이야...",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042340.1.3",
          "form": "그래서 확실히는 모르겠는데 출석 비중이 5인듯",
          "original_form": "그래서 확실히는 모르겠는데 출석 비중이 5인듯",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042340.1.4",
          "form": "헐 5퍼???",
          "original_form": "헐 5퍼???",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042340.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042340.1.6",
          "form": "그런것 같은데 ㅋㅋㅌㅌㅌㅌㅌ",
          "original_form": "그런것 같은데 ㅋㅋㅌㅌㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042340.1.7",
          "form": "일단 모든건",
          "original_form": "일단 모든건",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042340.1.8",
          "form": "시험을 잘 쳐야 가능하다",
          "original_form": "시험을 잘 쳐야 가능하다",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042340.1.9",
          "form": "할수있다!!!!!!!",
          "original_form": "할수있다!!!!!!!",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042340.1.10",
          "form": "그러면 2박 3일 정도 놀 수 있으니까 ㅎ",
          "original_form": "그러면 2박 3일 정도 놀 수 있으니까 ㅎ",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042340.1.11",
          "form": "나 친구랑 여행가는거",
          "original_form": "나 친구랑 여행가는거",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042340.1.12",
          "form": "처음이야",
          "original_form": "처음이야",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042340.1.13",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042340.1.14",
          "form": "친구 단 둘이는 처음인듯",
          "original_form": "친구 단 둘이는 처음인듯",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042340.1.15",
          "form": "사촌언니랑은 가봤는데",
          "original_form": "사촌언니랑은 가봤는데",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042340.1.16",
          "form": "두근",
          "original_form": "두근{emoji:♡}",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042340.1.17",
          "form": "친구랑은 또 다르징",
          "original_form": "친구랑은 또 다르징",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042340.1.18",
          "form": "진짜 이거 하나보고",
          "original_form": "진짜 이거 하나보고",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042340.1.19",
          "form": "사촌들이랑 친하게지낸느거 신기하다",
          "original_form": "사촌들이랑 친하게지낸느거 신기하다",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042340.1.20",
          "form": "열심히 공부하자",
          "original_form": "열심히 공부하자",
