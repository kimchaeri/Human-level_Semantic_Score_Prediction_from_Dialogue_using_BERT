      "utterance": [
        {
          "id": "MDRW2100020561.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020561.1.2",
          "form": "혹시 게임 좋아하세요?",
          "original_form": "혹시 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 09:47"
        },
        {
          "id": "MDRW2100020561.1.3",
          "form": "네 뭐 종종 합니다",
          "original_form": "네 뭐 종종 합니다",
          "speaker_id": "2",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100020561.1.4",
          "form": "오 무슨 게임하세요?",
          "original_form": "오 무슨 게임하세요?",
          "speaker_id": "1",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020561.1.5",
          "form": "전 그냥 캐쥬얼하게ㅋㅋㅋ 동물의 숲같은거 해요ㅋㅋㅋ",
          "original_form": "전 그냥 캐쥬얼하게ㅋㅋㅋ 동물의 숲같은거 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020561.1.6",
          "form": "오 동물의 숲",
          "original_form": "오 동물의 숲",
          "speaker_id": "1",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100020561.1.7",
          "form": "저 그거 사고싶었는데",
          "original_form": "저 그거 사고싶었는데",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020561.1.8",
          "form": "그때쯤에 닌텐도가 매진? 인가 그래서 ㅠㅠ 못했어요",
          "original_form": "그때쯤에 닌텐도가 매진? 인가 그래서 ㅠㅠ 못했어요",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020561.1.9",
          "form": "재밌나요",
          "original_form": "재밌나요",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020561.1.10",
          "form": "저도 그때 대란 났을땐 못 사고 한참 후에 좀 관심 식을때 사서 ㅋㅋㅋ",
          "original_form": "저도 그때 대란 났을땐 못 사고 한참 후에 좀 관심 식을때 사서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020561.1.11",
          "form": "그냥 힐링하는 기분 들어요ㅋㅋㅋ",
          "original_form": "그냥 힐링하는 기분 들어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020561.1.12",
          "form": "마을 친구들이 예쁜말 많이 해주고 ㅋㅋㅋ",
          "original_form": "마을 친구들이 예쁜말 많이 해주고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020561.1.13",
          "form": "중고가격이 더 비싸던...",
          "original_form": "중고가격이 더 비싸던...",
          "speaker_id": "1",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020561.1.14",
          "form": "아 진짜요? ㅋㅋㅋ",
          "original_form": "아 진짜요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020561.1.15",
          "form": "인터넷 짤보니깐",
          "original_form": "인터넷 짤보니깐",
          "speaker_id": "1",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020561.1.16",
          "form": "너무 자주 오면 걱정도 해주더라구요 ㅎㅎ",
          "original_form": "너무 자주 오면 걱정도 해주더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020561.1.17",
          "form": "앜ㅋㅋㅋ 저는 너무 안와서 걱정만 들어봤는데",
          "original_form": "앜ㅋㅋㅋ 저는 너무 안와서 걱정만 들어봤는데",
          "speaker_id": "2",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020561.1.18",
          "form": "근데 너무 정적이니까 좀 질리기도 해서 한번씩 닌텐도 다른 게임 사서 하구 그래요",
          "original_form": "근데 너무 정적이니까 좀 질리기도 해서 한번씩 닌텐도 다른 게임 사서 하구 그래요",
          "speaker_id": "2",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020561.1.19",
          "form": "어떤 게임 하세요?",
          "original_form": "어떤 게임 하세요?",
          "speaker_id": "2",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020561.1.20",
          "form": "전 예전에는 진짜 자주 했는데",
          "original_form": "전 예전에는 진짜 자주 했는데",
          "speaker_id": "1",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020561.1.21",
          "form": "요즘에는 거의 안하는 거 같아요",
          "original_form": "요즘에는 거의 안하는 거 같아요",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020561.1.22",
          "form": "듀오링고 딱 하나해요",
          "original_form": "듀오링고 딱 하나해요",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020561.1.23",
          "form": "듀오링고면 외국어 배우는?",
          "original_form": "듀오링고면 외국어 배우는?",
          "speaker_id": "2",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020561.1.24",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020561.1.25",
          "form": "매일 하나씩 해요",
          "original_form": "매일 하나씩 해요",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020561.1.26",
          "form": "영어요?",
          "original_form": "영어요?",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020561.1.27",
          "form": "뭔가 의외의 게임을 말하셔서 놀랐네요ㅋㅋㅋ",
          "original_form": "뭔가 의외의 게임을 말하셔서 놀랐네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020561.1.28",
          "form": "자기관리 열심히 하시는 분이네여",
          "original_form": "자기관리 열심히 하시는 분이네여",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020561.1.29",
          "form": "아 ㅋㅋㅋ 스페인어요",
          "original_form": "아 ㅋㅋㅋ 스페인어요",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020561.1.30",
          "form": "오 스페인어! 여행가시려구여?",
          "original_form": "오 스페인어! 여행가시려구여?",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020561.1.31",
          "form": "하다보면 재밌어요",
          "original_form": "하다보면 재밌어요",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020561.1.32",
          "form": "발음이 재밌더라구요",
          "original_form": "발음이 재밌더라구요",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020561.1.33",
          "form": "네 그래서 다른 스페인어 게임앱도 있어요ㅎㅎ",
          "original_form": "네 그래서 다른 스페인어 게임앱도 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:05"
        }
      ]
    }
