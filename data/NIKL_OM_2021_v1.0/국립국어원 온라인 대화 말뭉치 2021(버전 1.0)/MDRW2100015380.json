      "utterance": [
        {
          "id": "MDRW2100015380.1.1",
          "form": "해외여행 좋지 어디 가고 싶은데?",
          "original_form": "해외여행 좋지 어디 가고 싶은데?",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015380.1.2",
          "form": "스위스",
          "original_form": "스위스",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015380.1.3",
          "form": "스위스 진짜 좋지 공기도 엄청 좋을 거 같아",
          "original_form": "스위스 진짜 좋지 공기도 엄청 좋을 거 같아",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015380.1.4",
          "form": "기차여행 하고싶어",
          "original_form": "기차여행 하고싶어",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015380.1.5",
          "form": "유럽 기차 여행?",
          "original_form": "유럽 기차 여행?",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015380.1.6",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015380.1.7",
          "form": "근데 기차 생각보다 불편해",
          "original_form": "근데 기차 생각보다 불편해",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015380.1.8",
          "form": "그러긴 한데 운치 있잖아",
          "original_form": "그러긴 한데 운치 있잖아",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015380.1.9",
          "form": "낭만은 끝장나지",
          "original_form": "낭만은 끝장나지",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015380.1.10",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015380.1.11",
          "form": "짧게 짧게 기차 타는 건 불편하지 않고 좋아",
          "original_form": "짧게 짧게 기차 타는 건 불편하지 않고 좋아",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015380.1.12",
          "form": "그럼 좋지",
          "original_form": "그럼 좋지",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015380.1.13",
          "form": "바다 밑으로 지나가는 기차도 있어",
          "original_form": "바다 밑으로 지나가는 기차도 있어",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015380.1.14",
          "form": "오 신기하다",
          "original_form": "오 신기하다",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015380.1.15",
          "form": "나도 너무 신기했어 하나도 티가 안 나",
          "original_form": "나도 너무 신기했어 하나도 티가 안 나",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015380.1.16",
          "form": "나는 남미 여행도 가보고 싶어",
          "original_form": "나는 남미 여행도 가보고 싶어",
          "speaker_id": "2",
          "time": "20210825 23:29"
        }
      ]
