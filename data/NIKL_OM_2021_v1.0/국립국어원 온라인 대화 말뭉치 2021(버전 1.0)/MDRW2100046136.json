      "utterance": [
        {
          "id": "MDRW2100046136.1.1",
          "form": "넷플릭스이용하시나요?",
          "original_form": "넷플릭스이용하시나요?",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046136.1.2",
          "form": "네! 이용즁이에여!",
          "original_form": "네! 이용즁이에여!",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046136.1.3",
          "form": "오 저두요! 어떤거보셨어요?",
          "original_form": "오 저두요! 어떤거보셨어요?",
          "speaker_id": "1",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046136.1.4",
          "form": "최근에는 바람의 검신이라고 일본 영화 보다가 말았어욬ㅋㅋㅋ",
          "original_form": "최근에는 바람의 검신이라고 일본 영화 보다가 말았어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100046136.1.5",
          "form": "오오 저는 최근에 dp봤어요",
          "original_form": "오오 저는 최근에 dp봤어요",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046136.1.6",
          "form": "그거 엄청 재밌다고 하던데 많이 재밌나요??",
          "original_form": "그거 엄청 재밌다고 하던데 많이 재밌나요??",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046136.1.7",
          "form": "저도 지금 볼까말까 고민중이거든요!",
          "original_form": "저도 지금 볼까말까 고민중이거든요!",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046136.1.8",
          "form": "오 저도 주변에서 다들재밌다길래봤는데",
          "original_form": "오 저도 주변에서 다들재밌다길래봤는데",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046136.1.9",
          "form": "꼭 보시길추천해요!",
          "original_form": "꼭 보시길추천해요!",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046136.1.10",
          "form": "드라마 끝나기전에 후다닥 봐야겠네요ㅠㅠ",
          "original_form": "드라마 끝나기전에 후다닥 봐야겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046136.1.11",
          "form": "다른 사람들과 얘기할 수 있을때에 말이에요ㅠㅠ",
          "original_form": "다른 사람들과 얘기할 수 있을때에 말이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046136.1.12",
          "form": "ㅎㅎㅎㅎ맞아요 그리고 나오는배우들이 매력적이에요",
          "original_form": "ㅎㅎㅎㅎ맞아요 그리고 나오는배우들이 매력적이에요",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046136.1.13",
          "form": "배우분들이 매력적이라면 더 보는 재미가 있겠네요ㅠㅠ",
          "original_form": "배우분들이 매력적이라면 더 보는 재미가 있겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046136.1.14",
          "form": "6회라서 짧아요",
          "original_form": "6회라서 짧아요",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046136.1.15",
          "form": "설마 6회가 끝은 아니죠?ㅠㅠ",
          "original_form": "설마 6회가 끝은 아니죠?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046136.1.16",
          "form": "ㅋㅋㅋ그게 끝이에요",
          "original_form": "ㅋㅋㅋ그게 끝이에요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        }
