      "utterance": [
        {
          "id": "MDRW2100047576.1.1",
          "form": "집 갈 땐 완전 녹초였어요.. 앵콜까지 달렸습니다",
          "original_form": "집 갈 땐 완전 녹초였어요.. 앵콜까지 달렸습니다",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100047576.1.2",
          "form": "세상에....",
          "original_form": "세상에....",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100047576.1.3",
          "form": "체력좋으시네요",
          "original_form": "체력좋으시네요",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100047576.1.4",
          "form": "학생때가셨다고하시더니 젊음이 좋네요...",
          "original_form": "학생때가셨다고하시더니 젊음이 좋네요...",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100047576.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋname2님 지금도 젊어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ&name2&님 지금도 젊어요",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047576.1.6",
          "form": "근데 저도 그때가 더 체력이 좋았던 건 인정합니다..",
          "original_form": "근데 저도 그때가 더 체력이 좋았던 건 인정합니다..",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047576.1.7",
          "form": "아니 스탠딩을 어떻거ㅣ잡으셨어요",
          "original_form": "아니 스탠딩을 어떻거ㅣ잡으셨어요",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047576.1.8",
          "form": "티켓팅용병 돈받고 하셔요",
          "original_form": "티켓팅용병 돈받고 하셔요",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047576.1.9",
          "form": "다른분들꺼 해주시면서 돈받으셔야겠는데요",
          "original_form": "다른분들꺼 해주시면서 돈받으셔야겠는데요",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047576.1.10",
          "form": "사실 운빨 같아요..",
          "original_form": "사실 운빨 같아요..",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047576.1.11",
          "form": "저도 그게 실력이었으면 좋을텐데",
          "original_form": "저도 그게 실력이었으면 좋을텐데",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100047576.1.12",
          "form": "근데 취켓팅이 그나마 가능성 있는 거 같아요",
          "original_form": "근데 취켓팅이 그나마 가능성 있는 거 같아요",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100047576.1.13",
          "form": "진짜 그냥.. 수강신청 같음",
          "original_form": "진짜 그냥.. 수강신청 같음",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100047576.1.14",
          "form": "앜ㅋㅋㅋㅋㄱㅋㄱ",
          "original_form": "앜ㅋㅋㅋㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100047576.1.15",
          "form": "자리 날 때까지 들락날락하는..",
          "original_form": "자리 날 때까지 들락날락하는..",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100047576.1.16",
          "form": "정확한 표현이세요",
          "original_form": "정확한 표현이세요",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100047576.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047576.1.18",
          "form": "name2님은 아이돌콘서트 가 본 적은 없으시겠죠",
          "original_form": "&name2&님은 아이돌콘서트 가 본 적은 없으시겠죠",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047576.1.19",
          "form": "아이돌 관심 없으신 거죠?",
          "original_form": "아이돌 관심 없으신 거죠?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047576.1.20",
          "form": "저도 지금은 없어요",
          "original_form": "저도 지금은 없어요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047576.1.21",
          "form": "네 저는 아주예전에 에프엑스라는 그룹 잠깐",
          "original_form": "네 저는 아주예전에 에프엑스라는 그룹 잠깐",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047576.1.22",
          "form": "좋아했었어요",
          "original_form": "좋아했었어요",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047576.1.23",
          "form": "그게 다예요",
          "original_form": "그게 다예요",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047576.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100047576.1.25",
          "form": "저도 에프엑스 좋아했아요 앨범까지 산 건 아니지만",
          "original_form": "저도 에프엑스 좋아했아요 앨범까지 산 건 아니지만",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100047576.1.26",
          "form": "최근에 핑크테잎을 사고 싶었어요",
          "original_form": "최근에 핑크테잎을 사고 싶었어요",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100047576.1.27",
