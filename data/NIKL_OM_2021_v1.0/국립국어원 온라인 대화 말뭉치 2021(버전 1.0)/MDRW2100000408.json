      "utterance": [
        {
          "id": "MDRW2100000408.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000408.1.2",
          "form": "안녕하세요!! 군인이신가요?",
          "original_form": "안녕하세요!! 군인이신가요?",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000408.1.3",
          "form": "아니요 제대했어요",
          "original_form": "아니요 제대했어요",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000408.1.4",
          "form": "군생활은 어떠셨어요?",
          "original_form": "군생활은 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000408.1.5",
          "form": "1번은 경험하지만 2번은 못할 짓이죠",
          "original_form": "1번은 경험하지만 2번은 못할 짓이죠",
          "speaker_id": "1",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000408.1.6",
          "form": "전 가면 적응 못할꺼 같아요 ㅠ ㅠ",
          "original_form": "전 가면 적응 못할꺼 같아요 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000408.1.7",
          "form": "닥치면 다 하게 되요",
          "original_form": "닥치면 다 하게 되요",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000408.1.8",
          "form": "시간은 가니까",
          "original_form": "시간은 가니까",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000408.1.9",
          "form": "그렇긴하죠 인간은 적응의 동물이니까 ㅎㅎ",
          "original_form": "그렇긴하죠 인간은 적응의 동물이니까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000408.1.10",
          "form": "시간이 지나면 다 추억이죠",
          "original_form": "시간이 지나면 다 추억이죠",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000408.1.11",
          "form": "제 친구중에 여잔데 군인 된 친구있어요!!",
          "original_form": "제 친구중에 여잔데 군인 된 친구있어요!!",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000408.1.12",
          "form": "우와\n부사관",
          "original_form": "우와\n부사관",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000408.1.13",
          "form": "지금은 결혼도하고 애기도 낳아서 제대한 것 같더라구요 ㅎㅎ",
          "original_form": "지금은 결혼도하고 애기도 낳아서 제대한 것 같더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000408.1.14",
          "form": "대단한 친구네여",
          "original_form": "대단한 친구네여",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000408.1.15",
          "form": "여자 부사관들 멋지던데",
          "original_form": "여자 부사관들 멋지던데",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000408.1.16",
          "form": "고등학생때 과도 전혀 상관없는 과였는데 군인된 모습 보고 깜짝 놀랐어요 ㅎㅎ",
          "original_form": "고등학생때 과도 전혀 상관없는 과였는데 군인된 모습 보고 깜짝 놀랐어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000408.1.17",
          "form": "그래도 엄마가 되어서 다시 여자여자 되셨겠네요",
          "original_form": "그래도 엄마가 되어서 다시 여자여자 되셨겠네요",
          "speaker_id": "1",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000408.1.18",
          "form": "ㅋㅋㅋ그런거같아요 머리도 다시 기르고",
          "original_form": "ㅋㅋㅋ그런거같아요 머리도 다시 기르고",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000408.1.19",
          "form": "자녀들도 조교처럼 빡시게 굴리는 거 아닌가요?? ㅋㅋ",
          "original_form": "자녀들도 조교처럼 빡시게 굴리는 거 아닌가요?? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000408.1.20",
          "form": "설마요 애기가 이제 두살인거 같아요 ㅋㅋ",
          "original_form": "설마요 애기가 이제 두살인거 같아요 ㅋㅋ",
