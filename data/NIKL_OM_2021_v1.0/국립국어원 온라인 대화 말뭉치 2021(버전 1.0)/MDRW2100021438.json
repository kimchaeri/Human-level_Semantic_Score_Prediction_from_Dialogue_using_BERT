      "utterance": [
        {
          "id": "MDRW2100021438.1.1",
          "form": "요즘 하시는 운동있으세요",
          "original_form": "요즘 하시는 운동있으세요",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021438.1.2",
          "form": "아니요 숨쉬기 운동 빼고 없네요\r\nname1은 운동 하시나요?",
          "original_form": "아니요 숨쉬기 운동 빼고 없네요\r\n&name1&은 운동 하시나요?",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021438.1.3",
          "form": "저는 강아지랑 산책다녀오구",
          "original_form": "저는 강아지랑 산책다녀오구",
          "speaker_id": "1",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021438.1.4",
          "form": "케이팝댄스 배워용",
          "original_form": "케이팝댄스 배워용",
          "speaker_id": "1",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100021438.1.5",
          "form": "강아지를 키우시나봐요",
          "original_form": "강아지를 키우시나봐요",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021438.1.6",
          "form": "산책하면 정말 운동되겠네요 얼마동안 하시나요",
          "original_form": "산책하면 정말 운동되겠네요 얼마동안 하시나요",
          "speaker_id": "2",
          "time": "20210828 19:21"
        },
        {
          "id": "MDRW2100021438.1.7",
          "form": "네네 한 30뷴은 최소 하려는것 같아요",
          "original_form": "네네 한 30뷴은 최소 하려는것 같아요",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021438.1.8",
          "form": "댄스는 1주일에 한번만 해용",
          "original_form": "댄스는 1주일에 한번만 해용",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021438.1.9",
          "form": "짧고 가볍게 하시네요",
          "original_form": "짧고 가볍게 하시네요",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021438.1.10",
          "form": "댄스 멋있네요 주로 뭐 배우나요?",
          "original_form": "댄스 멋있네요 주로 뭐 배우나요?",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021438.1.11",
          "form": "최근엔 아스트로 노래 배웠고",
          "original_form": "최근엔 아스트로 노래 배웠고",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021438.1.12",
          "form": "아이돌 신곡을 주로 해요",
          "original_form": "아이돌 신곡을 주로 해요",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021438.1.13",
          "form": "정말 다이나믹 하겠네요 땀도 많이 나겠구요 전 옛날에 춤 배웠어요",
          "original_form": "정말 다이나믹 하겠네요 땀도 많이 나겠구요 전 옛날에 춤 배웠어요",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021438.1.14",
          "form": "우와 뭐배우셨나요",
          "original_form": "우와 뭐배우셨나요",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021438.1.15",
          "form": "너무 옛날이라 기억은 안나는데 걸스힙합 배웠어요",
          "original_form": "너무 옛날이라 기억은 안나는데 걸스힙합 배웠어요",
          "speaker_id": "2",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100021438.1.16",
          "form": "우와걸스힙합 힙합걸이네요",
          "original_form": "우와걸스힙합 힙합걸이네요",
          "speaker_id": "1",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100021438.1.17",
          "form": "네 그때 정말 재밌었어요 운동도 되구요",
          "original_form": "네 그때 정말 재밌었어요 운동도 되구요",
          "speaker_id": "2",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021438.1.18",
          "form": "맞아요 신나는 운동최고",
          "original_form": "맞아요 신나는 운동최고",
