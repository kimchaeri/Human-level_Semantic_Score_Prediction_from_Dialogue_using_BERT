      "utterance": [
        {
          "id": "MDRW2100033798.1.1",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210825 02:30"
        },
        {
          "id": "MDRW2100033798.1.2",
          "form": "너무 매운 건 안 먹으려고 해요",
          "original_form": "너무 매운 건 안 먹으려고 해요",
          "speaker_id": "2",
          "time": "20210825 02:31"
        },
        {
          "id": "MDRW2100033798.1.3",
          "form": "속이 안 좋아서",
          "original_form": "속이 안 좋아서",
          "speaker_id": "2",
          "time": "20210825 02:31"
        },
        {
          "id": "MDRW2100033798.1.4",
          "form": "저늑",
          "original_form": "저늑",
          "speaker_id": "1",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100033798.1.5",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100033798.1.6",
          "form": "완전맵찔이랍니다",
          "original_form": "완전맵찔이랍니다",
          "speaker_id": "1",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100033798.1.7",
          "form": "살짝 맛있게 매운 정도가 좋은 것 같아요",
          "original_form": "살짝 맛있게 매운 정도가 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100033798.1.8",
          "form": "마라탕은",
          "original_form": "마라탕은",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100033798.1.9",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100033798.1.10",
          "form": "먹어본 적 있는데 제일 순한맛으로 시켰어요 ㅋㅋ",
          "original_form": "먹어본 적 있는데 제일 순한맛으로 시켰어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100033798.1.11",
          "form": "입맛에 맞나요?",
          "original_form": "입맛에 맞나요?",
          "speaker_id": "1",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100033798.1.12",
          "form": "괜찮더라구요. 마라탕 좋아하세요?",
          "original_form": "괜찮더라구요. 마라탕 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 03:02"
        },
        {
          "id": "MDRW2100033798.1.13",
          "form": "저",
          "original_form": "저",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100033798.1.14",
          "form": "엄청 좋아해요ㅋㅋㅋ",
          "original_form": "엄청 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100033798.1.15",
          "form": "매운 거 잘 못 드시지만 좋아는 하시는 군요 ㅋㅋ",
          "original_form": "매운 거 잘 못 드시지만 좋아는 하시는 군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100033798.1.16",
          "form": "ㅋㅋㅋ 맞아요 기본은먹는답니다",
          "original_form": "ㅋㅋㅋ 맞아요 기본은먹는답니다",
          "speaker_id": "1",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100033798.1.17",
          "form": "떡볶이는 좋아하세요?",
          "original_form": "떡볶이는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100033798.1.18",
          "form": "별로 좋아하는 편은 아니에요. 어쩌다 가끔?",
          "original_form": "별로 좋아하는 편은 아니에요. 어쩌다 가끔?",
          "speaker_id": "2",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100033798.1.19",
          "form": "와우 아쉽네요ㅠ",
          "original_form": "와우 아쉽네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 13:00"
        },
        {
          "id": "MDRW2100033798.1.20",
          "form": "저는 좋아하거든요",
          "original_form": "저는 좋아하거든요",
          "speaker_id": "1",
          "time": "20210825 13:00"
        }
      ]
