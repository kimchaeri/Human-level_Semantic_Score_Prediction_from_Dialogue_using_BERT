      "utterance": [
        {
          "id": "MDRW2100031282.1.1",
          "form": "최근에 사신거 있어요?",
          "original_form": "최근에 사신거 있어요?",
          "speaker_id": "1",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100031282.1.2",
          "form": "저!! c to c  초고속 츙전기 선이요",
          "original_form": "저!! c to c  초고속 츙전기 선이요",
          "speaker_id": "2",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100031282.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100031282.1.4",
          "form": "마침 아까 몇시간 전에 왔어요",
          "original_form": "마침 아까 몇시간 전에 왔어요",
          "speaker_id": "2",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100031282.1.5",
          "form": "충전기 중요하죠",
          "original_form": "충전기 중요하죠",
          "speaker_id": "1",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100031282.1.6",
          "form": "저도 하나 새로 사고싶어요",
          "original_form": "저도 하나 새로 사고싶어요",
          "speaker_id": "1",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100031282.1.7",
          "form": "충전기선 너무 짧아요ㅜ",
          "original_form": "충전기선 너무 짧아요ㅜ",
          "speaker_id": "1",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031282.1.8",
          "form": "저는 그래서 2m 사요",
          "original_form": "저는 그래서 2m 사요",
          "speaker_id": "2",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031282.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031282.1.10",
          "form": "아니 기본으로 주는거 너무 짧아요",
          "original_form": "아니 기본으로 주는거 너무 짧아요",
          "speaker_id": "2",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031282.1.11",
          "form": "2m는 정품없죠?",
          "original_form": "2m는 정품없죠?",
          "speaker_id": "1",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031282.1.12",
          "form": "네 아마도요..? 제가 찾을때는 없었어요",
          "original_form": "네 아마도요..? 제가 찾을때는 없었어요",
          "speaker_id": "2",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031282.1.13",
          "form": "근데 정품 아니어도 요새는 다 잘되더라구요",
          "original_form": "근데 정품 아니어도 요새는 다 잘되더라구요",
          "speaker_id": "2",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031282.1.14",
          "form": "그건 그래요 다이소것만빼고 ᆢ",
          "original_form": "그건 그래요 다이소것만빼고 ᆢ",
          "speaker_id": "1",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031282.1.15",
          "form": "다이소에서 충전선 샀는데 후회했어요",
          "original_form": "다이소에서 충전선 샀는데 후회했어요",
          "speaker_id": "1",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100031282.1.16",
          "form": "맞아요 다이소는 못 믿어요 저도 당한적 있어서 뭔가 기기 관련한거는 절대 거기서 안사요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 다이소는 못 믿어요 저도 당한적 있어서 뭔가 기기 관련한거는 절대 거기서 안사요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100031282.1.17",
          "form": "충전이 너무 안돼요ㅜ",
          "original_form": "충전이 너무 안돼요ㅜ",
          "speaker_id": "1",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100031282.1.18",
          "form": "얼마주고 사셨어요?",
          "original_form": "얼마주고 사셨어요?",
          "speaker_id": "1",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100031282.1.19",
          "form": "8900원이요! 근데 저는 하나짜리 선도막 뭐 감싸져있고 그런거라 좀 비싼거고 여러개 들은것도 있어서 보고 사시면 될거에요",
          "original_form": "8900원이요! 근데 저는 하나짜리 선도막 뭐 감싸져있고 그런거라 좀 비싼거고 여러개 들은것도 있어서 보고 사시면 될거에요",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031282.1.20",
          "form": "초고속 되는거 사세요",
          "original_form": "초고속 되는거 사세요",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031282.1.21",
          "form": "고속충전 ㄴㄴ 초고속",
          "original_form": "고속충전 ㄴㄴ 초고속",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031282.1.22",
          "form": "오 저도 찾아봐야겠어요",
          "original_form": "오 저도 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031282.1.23",
          "form": "고속충전이랑 초고속이랑 뭐가 다른가요?",
          "original_form": "고속충전이랑 초고속이랑 뭐가 다른가요?",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031282.1.24",
          "form": "아 갤럭시만 그런가? 초고속은 충전중일때 화면에 파란 동그라미가 휙휙 돌아가고 그냥 고속은 하얀 동그라미가 돌아가요",
          "original_form": "아 갤럭시만 그런가? 초고속은 충전중일때 화면에 파란 동그라미가 휙휙 돌아가고 그냥 고속은 하얀 동그라미가 돌아가요",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031282.1.25",
          "form": "속도가 훨씬 빨라요",
          "original_form": "속도가 훨씬 빨라요",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031282.1.26",
          "form": "저도 아이폰 쓰다가 갤럭시 처음 쓰는거라 ㅋㅋㅋㅋ 잘 모르는데",
          "original_form": "저도 아이폰 쓰다가 갤럭시 처음 쓰는거라 ㅋㅋㅋㅋ 잘 모르는데",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031282.1.27",
          "form": "암튼 갤럭시면 초고속입니다",
          "original_form": "암튼 갤럭시면 초고속입니다",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031282.1.28",
          "form": "감사해요 살때 초고속으로 알아봐야겠어요",
          "original_form": "감사해요 살때 초고속으로 알아봐야겠어요",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031282.1.29",
          "form": "전 갤럭시 오래쓰면서도 몰랐네요",
          "original_form": "전 갤럭시 오래쓰면서도 몰랐네요",
          "speaker_id": "1",
          "time": "20210904 21:20"
        }
      ]
