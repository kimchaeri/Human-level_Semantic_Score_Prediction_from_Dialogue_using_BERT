      "utterance": [
        {
          "id": "MDRW2100009390.1.1",
          "form": "당연히 쫀득해야죠!!",
          "original_form": "당연히 쫀득해야죠!!",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009390.1.2",
          "form": "맛있는 마카롱 드셔보셨나요?",
          "original_form": "맛있는 마카롱 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009390.1.3",
          "form": "저는 쫀득해서 씹는 맛이 있는 꼬끄가 좋아요",
          "original_form": "저는 쫀득해서 씹는 맛이 있는 꼬끄가 좋아요",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009390.1.4",
          "form": "오 좀 쫀득한건 먹어본적 있는거같아요",
          "original_form": "오 좀 쫀득한건 먹어본적 있는거같아요",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009390.1.5",
          "form": "근데 안에 필링이라 하나요? 그거 다 터져나와서 먹기 좀 불편햇어요",
          "original_form": "근데 안에 필링이라 하나요? 그거 다 터져나와서 먹기 좀 불편햇어요",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009390.1.6",
          "form": "아 저도 뚱카롱은 별로 안좋아해요",
          "original_form": "아 저도 뚱카롱은 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009390.1.7",
          "form": "모든 것에는 적절한 비율이 있어야죠",
          "original_form": "모든 것에는 적절한 비율이 있어야죠",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009390.1.8",
          "form": "그쵸 좀 적당해야 하는디",
          "original_form": "그쵸 좀 적당해야 하는디",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009390.1.9",
          "form": "저 긴꼬리초밥도 안좋아해요!",
          "original_form": "저 긴꼬리초밥도 안좋아해요!",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009390.1.10",
          "form": "밥과 회의 비율..",
          "original_form": "밥과 회의 비율..",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009390.1.11",
          "form": "오 저는 회가 엄청 긴 초밥 먹어봤었는데 그건 맛있었어요",
          "original_form": "오 저는 회가 엄청 긴 초밥 먹어봤었는데 그건 맛있었어요",
          "speaker_id": "2",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009390.1.12",
          "form": "아 회가 아니라 소고기였군요",
          "original_form": "아 회가 아니라 소고기였군요",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009390.1.13",
          "form": "그랴서 맛있었네요ㅎ",
          "original_form": "그랴서 맛있었네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009390.1.14",
          "form": "소고깈ㅋㅋㅋㅋ 소고기는 언제나 옳은거같아요",
          "original_form": "소고깈ㅋㅋㅋㅋ 소고기는 언제나 옳은거같아요",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009390.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009390.1.16",
          "form": "그 초밥은 얇은 소고기에 새큼달콤한 김치가 같이 있었는데 조합이 진짜 맛있었어요",
          "original_form": "그 초밥은 얇은 소고기에 새큼달콤한 김치가 같이 있었는데 조합이 진짜 맛있었어요",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009390.1.17",
          "form": "와 대박적..",
          "original_form": "와 대박적..",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009390.1.18",
          "form": "상상만해도 맛잇어요",
          "original_form": "상상만해도 맛잇어요",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009390.1.19",
          "form": "소고기의 담백고소함과 새콤달콤한 김치",
          "original_form": "소고기의 담백고소함과 새콤달콤한 김치",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009390.1.20",
          "form": "맛의 하모니네요",
          "original_form": "맛의 하모니네요",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009390.1.21",
          "form": "아유 출출해지네요^^",
          "original_form": "아유 출출해지네요^^",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009390.1.22",
          "form": "야식으로 뭐 선호하시나요",
          "original_form": "야식으로 뭐 선호하시나요",
          "speaker_id": "1",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009390.1.23",
          "form": "제가 학교 학기 중에는 새벽 늣게까지 깨어있는 편이라 야식도 좀 신경써서 먹으려고 했었는데",
          "original_form": "제가 학교 학기 중에는 새벽 늣게까지 깨어있는 편이라 야식도 좀 신경써서 먹으려고 했었는데",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009390.1.24",
          "form": "그때 두유 세 팩씩 먹었어요",
          "original_form": "그때 두유 세 팩씩 먹었어요",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009390.1.25",
          "form": "저 건강하게 먹은 거 맞나요",
          "original_form": "저 건강하게 먹은 거 맞나요",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009390.1.26",
          "form": "ㅋㅋㅋㅋ 단백질은 충분히 섭취하신 것 같아요",
          "original_form": "ㅋㅋㅋㅋ 단백질은 충분히 섭취하신 것 같아요",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009390.1.27",
          "form": "운동만 하시면 벌크업 되시겠는데요",
          "original_form": "운동만 하시면 벌크업 되시겠는데요",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009390.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009390.1.29",
          "form": "다음 학기에는 꼭 그래볼게요",
          "original_form": "다음 학기에는 꼭 그래볼게요",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009390.1.30",
          "form": "지난 학기에는 좀비 그자체였어서...",
          "original_form": "지난 학기에는 좀비 그자체였어서...",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009390.1.31",
          "form": "나무 님은 야식으로 뭐 드세요?",
          "original_form": "나무 님은 야식으로 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009390.1.32",
          "form": "저는 야식 잘 안먹는데 밤에 늘 라면을 마음에 품고 있어요",
          "original_form": "저는 야식 잘 안먹는데 밤에 늘 라면을 마음에 품고 있어요",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009390.1.33",
          "form": "밤만 되면 왜 짠게 땡길까요?",
          "original_form": "밤만 되면 왜 짠게 땡길까요?",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009390.1.34",
          "form": "오 전 단게 땡겨요",
          "original_form": "오 전 단게 땡겨요",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009390.1.35",
          "form": "어렸을 땐 저도 새벽에 라면 먹고 싶었어요ㅋㅋ",
          "original_form": "어렸을 땐 저도 새벽에 라면 먹고 싶었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009390.1.36",
          "form": "오 단게 땡기세요? 저는 며칠전에도 생라면 부숴먹엇어요",
          "original_form": "오 단게 땡기세요? 저는 며칠전에도 생라면 부숴먹엇어요",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009390.1.37",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009390.1.38",
          "form": "네 혈당수치를 높이고만 싶어요",
          "original_form": "네 혈당수치를 높이고만 싶어요",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009390.1.39",
          "form": "오우.. 당뇨 주의하셔야겠는데요 ㅋㅋㅋ",
          "original_form": "오우.. 당뇨 주의하셔야겠는데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009390.1.40",
          "form": "두유도 안 단걸로 드셔야겠어요",
          "original_form": "두유도 안 단걸로 드셔야겠어요",
          "speaker_id": "1",
