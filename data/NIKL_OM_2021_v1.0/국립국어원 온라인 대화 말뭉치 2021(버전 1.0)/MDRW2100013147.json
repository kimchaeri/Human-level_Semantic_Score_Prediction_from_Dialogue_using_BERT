        },
        {
          "id": "MDRW2100013147.1.3",
          "form": "여행 좋아하세욤",
          "original_form": "여행 좋아하세욤",
          "speaker_id": "2",
          "time": "20210825 13:05"
        },
        {
          "id": "MDRW2100013147.1.4",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100013147.1.5",
          "form": "오 그럼 여행 다녀오신 곳 있나여?",
          "original_form": "오 그럼 여행 다녀오신 곳 있나여?",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100013147.1.6",
          "form": "해외요?",
          "original_form": "해외요?",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100013147.1.7",
          "form": "해외도 좋고~ 국내도 좋구여",
          "original_form": "해외도 좋고~ 국내도 좋구여",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100013147.1.8",
          "form": "해외 다녀오셨나봐요??",
          "original_form": "해외 다녀오셨나봐요??",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100013147.1.9",
          "form": "네 세부대만홍콩일본이요",
          "original_form": "네 세부대만홍콩일본이요",
          "speaker_id": "1",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100013147.1.10",
          "form": "오... 많이 가셨네여...!!",
          "original_form": "오... 많이 가셨네여...!!",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100013147.1.11",
          "form": "어디 가보셨나요",
          "original_form": "어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100013147.1.12",
          "form": "저는 경주요... ㅋㅋㅋㅋㅋ",
          "original_form": "저는 경주요... ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100013147.1.13",
          "form": "황리단길 맞나요?",
          "original_form": "황리단길 맞나요?",
          "speaker_id": "1",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100013147.1.14",
          "form": "ㅋㅋㅋㅋ 어딘지는 모르겠구 가족여행으로 갔어요",
          "original_form": "ㅋㅋㅋㅋ 어딘지는 모르겠구 가족여행으로 갔어요",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100013147.1.15",
          "form": "천문대 주변이였던걸루 기억해요",
          "original_form": "천문대 주변이였던걸루 기억해요",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100013147.1.16",
          "form": "아아 첨성대맞나여",
          "original_form": "아아 첨성대맞나여",
