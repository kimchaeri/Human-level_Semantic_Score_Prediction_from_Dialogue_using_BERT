      "utterance": [
        {
          "id": "MDRW2100008895.1.1",
          "form": "저는 최근 아이스 아메리카노 싼집 찾는 재미 들렸어요 ㅋㅋ",
          "original_form": "저는 최근 아이스 아메리카노 싼집 찾는 재미 들렸어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008895.1.2",
          "form": "그 컴포트 1500요",
          "original_form": "그 컴포트 1500요",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008895.1.3",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008895.1.4",
          "form": "싼거 찾아요",
          "original_form": "싼거 찾아요",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008895.1.5",
          "form": "전 막 용량까지 따지면서",
          "original_form": "전 막 용량까지 따지면서",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008895.1.6",
          "form": "동네커피집 순회했네요 ㅋㅋ",
          "original_form": "동네커피집 순회했네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008895.1.7",
          "form": "1리터커피도 싸요",
          "original_form": "1리터커피도 싸요",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008895.1.8",
          "form": "1리터에 2천원",
          "original_form": "1리터에 2천원",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008895.1.9",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008895.1.10",
          "form": "대박이요",
          "original_form": "대박이요",
