      "utterance": [
        {
          "id": "MDRW2100019445.1.1",
          "form": "이규형이 노래 잘하던가요?",
          "original_form": "이규형이 노래 잘하던가요?",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019445.1.2",
          "form": "네 뮤지컬배우니까요.ㅎㅎ",
          "original_form": "네 뮤지컬배우니까요.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019445.1.3",
          "form": "오만석은 어떨까요?",
          "original_form": "오만석은 어떨까요?",
          "speaker_id": "2",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019445.1.4",
          "form": "오만석 하면 포도밭 그사나이 생각나요ㅋㅋ",
          "original_form": "오만석 하면 포도밭 그사나이 생각나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019445.1.5",
          "form": "윤은혜랑 나온거요?",
          "original_form": "윤은혜랑 나온거요?",
          "speaker_id": "2",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019445.1.6",
          "form": "네 드라마요ㅋㅋㅋㅋ",
          "original_form": "네 드라마요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019445.1.7",
          "form": "넵 오만석이 첨나왔을때 뮤지컬배우라곤 생각도 못했는데 ㅎㅎㅎ",
          "original_form": "넵 오만석이 첨나왔을때 뮤지컬배우라곤 생각도 못했는데 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019445.1.8",
          "form": "헤드윅 초연 멤버더라구요.",
          "original_form": "헤드윅 초연 멤버더라구요.",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019445.1.9",
          "form": "초연이 무슨 뜻인가요?",
          "original_form": "초연이 무슨 뜻인가요?",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019445.1.10",
          "form": "제일 처음 공연한거요.ㅎㅎ",
          "original_form": "제일 처음 공연한거요.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019445.1.11",
          "form": "아 ㅋㅋㅋㅋ 혹시 뉴이스트 민기도 초연했나요?",
          "original_form": "아 ㅋㅋㅋㅋ 혹시 뉴이스트 민기도 초연했나요?",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019445.1.12",
          "form": "아니요. 초연 십 몇년전에 한거 말하는거예요.ㅋㅋ",
          "original_form": "아니요. 초연 십 몇년전에 한거 말하는거예요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019445.1.13",
          "form": "아 ㅋ 맨처음요 ㅋ ㅋㅋ",
          "original_form": "아 ㅋ 맨처음요 ㅋ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019445.1.14",
          "form": "민기는 잘하던가요?",
          "original_form": "민기는 잘하던가요?",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019445.1.15",
          "form": "너무 잘해서 놀랐어요ㅠㅠ",
          "original_form": "너무 잘해서 놀랐어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019445.1.16",
          "form": "ㅋㅋㅋ팬으롯서 좋으셨겠네요",
          "original_form": "ㅋㅋㅋ팬으롯서 좋으셨겠네요",
