      "utterance": [
        {
          "id": "MDRW2100022309.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022309.1.2",
          "form": "마늘 맛있죠  약간 구우면 사기템",
          "original_form": "마늘 맛있죠  약간 구우면 사기템",
          "speaker_id": "1",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022309.1.3",
          "form": "그쵸 ㅋㅋ 마늘 뭔가 중독돼요",
          "original_form": "그쵸 ㅋㅋ 마늘 뭔가 중독돼요",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022309.1.4",
          "form": "왜 자꾸 초면인것처럼..",
          "original_form": "왜 자꾸 초면인것처럼..",
          "speaker_id": "1",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022309.1.5",
          "form": "아 그래요?ㅋㅋㅋㅋ 잘 몰라서...",
          "original_form": "아 그래요?ㅋㅋㅋㅋ 잘 몰라서...",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022309.1.6",
          "form": "마늘 구우면 라면스프맛 나더라구요",
          "original_form": "마늘 구우면 라면스프맛 나더라구요",
          "speaker_id": "1",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022309.1.7",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022309.1.8",
          "form": "마늘에서 ㄱ그런 맛이 나요?",
          "original_form": "마늘에서 ㄱ그런 맛이 나요?",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.9",
          "form": "아닌가요",
          "original_form": "아닌가요",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.10",
          "form": "편썰어서 소금쳐보세요",
          "original_form": "편썰어서 소금쳐보세요",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.11",
          "form": "뭐라 형용할 수 없는 그런 맛이던데",
          "original_form": "뭐라 형용할 수 없는 그런 맛이던데",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.12",
          "form": "라면스프맛나요",
          "original_form": "라면스프맛나요",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.13",
          "form": "전 감자맛 같은게 나던데용",
          "original_form": "전 감자맛 같은게 나던데용",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.14",
          "form": "약간 노릇할정도면",
          "original_form": "약간 노릇할정도면",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.15",
          "form": "감자맛인데",
          "original_form": "감자맛인데",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.16",
          "form": "아~ 바삭 구워야해요?",
          "original_form": "아~ 바삭 구워야해요?",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.17",
          "form": "그 좀네",
          "original_form": "그 좀네",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.18",
          "form": "오 ㅋㅋ 처음 알았어요",
          "original_form": "오 ㅋㅋ 처음 알았어요",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022309.1.19",
          "form": "바삭구우면 조미료맛 라면스프맛",
          "original_form": "바삭구우면 조미료맛 라면스프맛",
          "speaker_id": "1",
          "time": "20210829 12:25"
        }
