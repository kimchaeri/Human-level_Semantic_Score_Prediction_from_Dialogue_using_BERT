      "utterance": [
        {
          "id": "MDRW2100017472.1.1",
          "form": "간단히 먹을 수 있는 간식같은 것도 사",
          "original_form": "간단히 먹을 수 있는 간식같은 것도 사",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017472.1.2",
          "form": "어떤 거?",
          "original_form": "어떤 거?",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017472.1.3",
          "form": "고구마 말랭이나 육포 같은거 인터넷에서 많이 팔던데",
          "original_form": "고구마 말랭이나 육포 같은거 인터넷에서 많이 팔던데",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017472.1.4",
          "form": "애들 간식으로 너무 좋지 ㅎㅎ 특히 육포",
          "original_form": "애들 간식으로 너무 좋지 ㅎㅎ 특히 육포",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017472.1.5",
          "form": "좀 비싸긴 한데 고기 대신 먹이면 좋잖아",
          "original_form": "좀 비싸긴 한데 고기 대신 먹이면 좋잖아",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017472.1.6",
          "form": "애들 육포 잘 먹긴 하더라",
          "original_form": "애들 육포 잘 먹긴 하더라",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017472.1.7",
          "form": "ㅇㅇ 잘라만 놓으면 알아서들 먹으니까",
          "original_form": "ㅇㅇ 잘라만 놓으면 알아서들 먹으니까",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017472.1.8",
          "form": "대용량으로 사는게 낫지?",
          "original_form": "대용량으로 사는게 낫지?",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017472.1.9",
          "form": "당연하지~ 저번에 900g짜리도 금방 먹더만 ㅎㅎ",
          "original_form": "당연하지~ 저번에 900g짜리도 금방 먹더만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017472.1.10",
          "form": "어디꺼 살까?",
          "original_form": "어디꺼 살까?",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017472.1.11",
          "form": "코주부가 젤로 만만하잖아 ㅎㅎ",
          "original_form": "코주부가 젤로 만만하잖아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017472.1.12",
          "form": "무난한 맛이긴 하지",
          "original_form": "무난한 맛이긴 하지",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017472.1.13",
          "form": "그럼 그걸로 해~ 코주부 대용량",
          "original_form": "그럼 그걸로 해~ 코주부 대용량",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017472.1.14",
          "form": "그리고 요즘엔 떡도 온라인이 다양하고 잘 나와",
          "original_form": "그리고 요즘엔 떡도 온라인이 다양하고 잘 나와",
