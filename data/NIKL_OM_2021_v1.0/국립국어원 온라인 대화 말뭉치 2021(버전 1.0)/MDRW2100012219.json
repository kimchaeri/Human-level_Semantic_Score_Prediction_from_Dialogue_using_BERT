      "utterance": [
        {
          "id": "MDRW2100012219.1.1",
          "form": "학교...",
          "original_form": "학교...",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012219.1.2",
          "form": "추억의 단어네요",
          "original_form": "추억의 단어네요",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012219.1.3",
          "form": "학생때는 많이걸어다니잖아요",
          "original_form": "학생때는 많이걸어다니잖아요",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012219.1.4",
          "form": "춥고덥고 다 잘견뎠던거같아요",
          "original_form": "춥고덥고 다 잘견뎠던거같아요",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012219.1.5",
          "form": "지금은 불가능",
          "original_form": "지금은 불가능",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012219.1.6",
          "form": "전 그때도 못견뎠어요",
          "original_form": "전 그때도 못견뎠어요",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012219.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012219.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012219.1.9",
          "form": "등교시마다 단전에서 짐승의 소리거",
          "original_form": "등교시마다 단전에서 짐승의 소리거",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012219.1.10",
          "form": "그때의 전 천지분간을 못했었나봐요 춥고덥고도 모르고",
          "original_form": "그때의 전 천지분간을 못했었나봐요 춥고덥고도 모르고",
          "speaker_id": "1",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012219.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012219.1.12",
          "form": "건강하셨나봐요",
          "original_form": "건강하셨나봐요",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012219.1.13",
          "form": "그래도 가끔 비도 맞고 정신나간듯 낭만적인 시기도 있었어요",
          "original_form": "그래도 가끔 비도 맞고 정신나간듯 낭만적인 시기도 있었어요",
          "speaker_id": "2",
          "time": "20210824 20:29"
        },
        {
          "id": "MDRW2100012219.1.14",
          "form": "그러니까요 그때 건강을 다 끌어다 써서 지금 이런걸까요",
          "original_form": "그러니까요 그때 건강을 다 끌어다 써서 지금 이런걸까요",
          "speaker_id": "1",
          "time": "20210824 20:29"
        },
        {
          "id": "MDRW2100012219.1.15",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "2",
          "time": "20210824 20:29"
        },
        {
          "id": "MDRW2100012219.1.16",
          "form": "ㅋㅋㅋ학생때 비맞고 돌아다니는거 저만 그런게 아니었네요",
          "original_form": "ㅋㅋㅋ학생때 비맞고 돌아다니는거 저만 그런게 아니었네요",
          "speaker_id": "1",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012219.1.17",
          "form": "아닐거에요 그럼 과거 현재 미래가 모두 학교생활때 끝났단 소리라고요",
          "original_form": "아닐거에요 그럼 과거 현재 미래가 모두 학교생활때 끝났단 소리라고요",
          "speaker_id": "2",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012219.1.18",
          "form": "건강.. 남아있을거에요",
          "original_form": "건강.. 남아있을거에요",
          "speaker_id": "2",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012219.1.19",
          "form": "학교다닐땐 왠지 우산이 있어도 한두번 비를 맞았던 기억이 있어요 왜지",
          "original_form": "학교다닐땐 왠지 우산이 있어도 한두번 비를 맞았던 기억이 있어요 왜지",
          "speaker_id": "2",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012219.1.20",
          "form": "사춘기때는 이유모를 행동을 하잖아요ㅋㅋ",
          "original_form": "사춘기때는 이유모를 행동을 하잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012219.1.21",
          "form": "맞아요 수업시간에 양호실가기나 지하 과학실 창문으로 간식사러 도망가거나..",
          "original_form": "맞아요 수업시간에 양호실가기나 지하 과학실 창문으로 간식사러 도망가거나..",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012219.1.22",
          "form": "ㅋㅋㅋ야자시간에 친구랑 학교구석에서 수다떨거나",
          "original_form": "ㅋㅋㅋ야자시간에 친구랑 학교구석에서 수다떨거나",
          "speaker_id": "1",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012219.1.23",
          "form": "점심먹고 학교운동장 도셨나요!",
          "original_form": "점심먹고 학교운동장 도셨나요!",
          "speaker_id": "2",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012219.1.24",
          "form": "저녁먹고도 돌았죠 회전초밥처럼",
          "original_form": "저녁먹고도 돌았죠 회전초밥처럼",
          "speaker_id": "1",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012219.1.25",
          "form": "국룰아닌가요~",
          "original_form": "국룰아닌가요~",
          "speaker_id": "1",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012219.1.26",
          "form": "맞아요 다들 하드 하나씩 물고",
          "original_form": "맞아요 다들 하드 하나씩 물고",
          "speaker_id": "2",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012219.1.27",
          "form": "뱅글뱅글 탑돌이하기",
          "original_form": "뱅글뱅글 탑돌이하기",
          "speaker_id": "2",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012219.1.28",
          "form": "모르긴 몰라도 많은 여고생의 정신건강을 지켜주지 않았을까요",
          "original_form": "모르긴 몰라도 많은 여고생의 정신건강을 지켜주지 않았을까요",
          "speaker_id": "2",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012219.1.29",
          "form": "맞아요 그마저 안돌았으면 정신이 먼저 돌았을거에요",
          "original_form": "맞아요 그마저 안돌았으면 정신이 먼저 돌았을거에요",
          "speaker_id": "1",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012219.1.30",
          "form": "ㅋㅋㅋ에스님도 야자전에 티비로 드라마 보셨나요",
          "original_form": "ㅋㅋㅋ에스님도 야자전에 티비로 드라마 보셨나요",
          "speaker_id": "1",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100012219.1.31",
          "form": "저희는 그시간에 방송부에서 영화틀어줬어요",
          "original_form": "저희는 그시간에 방송부에서 영화틀어줬어요",
          "speaker_id": "2",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100012219.1.32",
          "form": "메멘토 이런거.. 여고생의 잔인한 욕망을 충족시키는 공포 스릴러 위주였네요",
          "original_form": "메멘토 이런거.. 여고생의 잔인한 욕망을 충족시키는 공포 스릴러 위주였네요",
          "speaker_id": "2",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012219.1.33",
          "form": "와 방송부가 영화도 틀어주다니",
          "original_form": "와 방송부가 영화도 틀어주다니",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012219.1.34",
          "form": "그러고보니 저도 텍사스 전기톱 사건을 학교에서봤던..",
          "original_form": "그러고보니 저도 텍사스 전기톱 사건을 학교에서봤던..",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012219.1.35",
          "form": "무서운 시기였어요",
          "original_form": "무서운 시기였어요",
          "speaker_id": "2",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012219.1.36",
          "form": "쏘우 전 시리즈도 학교에서봤던ㅋㅋ",
          "original_form": "쏘우 전 시리즈도 학교에서봤던ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012219.1.37",
          "form": "맞아요 시험끝나고나면 온갖 공포영화 다 보고",
          "original_form": "맞아요 시험끝나고나면 온갖 공포영화 다 보고",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012219.1.38",
          "form": "진짜 무서운게 없었구나",
          "original_form": "진짜 무서운게 없었구나",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012219.1.39",
          "form": "약간 선생님들도 연령제한 생각안하고 내버려두셨던 것 같아요",
          "original_form": "약간 선생님들도 연령제한 생각안하고 내버려두셨던 것 같아요",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012219.1.40",
          "form": "다들 조용해지니까",
          "original_form": "다들 조용해지니까",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012219.1.41",
          "form": "아 그걸위해ㅋㅋㅋ",
          "original_form": "아 그걸위해ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012219.1.42",
          "form": "이제야 스승의 마음을 이해한 못난제자...",
          "original_form": "이제야 스승의 마음을 이해한 못난제자...",
