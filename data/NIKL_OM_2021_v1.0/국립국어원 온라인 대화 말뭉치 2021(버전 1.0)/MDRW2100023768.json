      "utterance": [
        {
          "id": "MDRW2100023768.1.1",
          "form": "황토색?",
          "original_form": "황토색?",
          "speaker_id": "2",
          "time": "20210830 16:57"
        },
        {
          "id": "MDRW2100023768.1.2",
          "form": "응 그런 거 그래서 염색을 하려다가도 망설이게 되더라고",
          "original_form": "응 그런 거 그래서 염색을 하려다가도 망설이게 되더라고",
          "speaker_id": "1",
          "time": "20210830 16:57"
        },
        {
          "id": "MDRW2100023768.1.3",
          "form": "잘 안빠지는 색으로 하면 되잖아",
          "original_form": "잘 안빠지는 색으로 하면 되잖아",
          "speaker_id": "2",
          "time": "20210830 16:58"
        },
        {
          "id": "MDRW2100023768.1.4",
          "form": "근데 내가 하고 싶은 색이 이제 잘 빠지는 거지",
          "original_form": "근데 내가 하고 싶은 색이 이제 잘 빠지는 거지",
          "speaker_id": "1",
          "time": "20210830 16:58"
        },
        {
          "id": "MDRW2100023768.1.5",
          "form": "아 어쩔 수 없네",
          "original_form": "아 어쩔 수 없네",
          "speaker_id": "2",
          "time": "20210830 16:58"
        },
        {
          "id": "MDRW2100023768.1.6",
          "form": "은색 하고 싶다고 했었지?",
          "original_form": "은색 하고 싶다고 했었지?",
          "speaker_id": "1",
          "time": "20210830 16:58"
        },
        {
          "id": "MDRW2100023768.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210830 16:58"
        },
        {
          "id": "MDRW2100023768.1.8",
          "form": "은색은 색이 빠지지는 않을 거 같은데 잘 모르겠네",
          "original_form": "은색은 색이 빠지지는 않을 거 같은데 잘 모르겠네",
          "speaker_id": "1",
          "time": "20210830 16:59"
        },
        {
          "id": "MDRW2100023768.1.9",
          "form": "왠지 잘 빠질거 같은데;;",
          "original_form": "왠지 잘 빠질거 같은데;;",
          "speaker_id": "2",
          "time": "20210830 16:59"
        },
        {
          "id": "MDRW2100023768.1.10",
          "form": "근데 은색에서 색이 빠지면 무슨 색이 되는 거지?",
          "original_form": "근데 은색에서 색이 빠지면 무슨 색이 되는 거지?",
          "speaker_id": "1",
          "time": "20210830 16:59"
        },
        {
          "id": "MDRW2100023768.1.11",
          "form": "금발 아닌가?",
          "original_form": "금발 아닌가?",
          "speaker_id": "2",
          "time": "20210830 16:59"
        },
        {
          "id": "MDRW2100023768.1.12",
          "form": "아 그러겠네 탈색하고 하니까",
          "original_form": "아 그러겠네 탈색하고 하니까",
          "speaker_id": "1",
          "time": "20210830 16:59"
        },
        {
          "id": "MDRW2100023768.1.13",
          "form": "응 탈색하니까",
          "original_form": "응 탈색하니까",
          "speaker_id": "2",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100023768.1.14",
          "form": "탈색해 본 적 없잖아",
          "original_form": "탈색해 본 적 없잖아",
          "speaker_id": "1",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100023768.1.15",
          "form": "그니까 해보고 싶어서",
          "original_form": "그니까 해보고 싶어서",
          "speaker_id": "2",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100023768.1.16",
          "form": "한번 해보는 것도 좋지",
          "original_form": "한번 해보는 것도 좋지",
