      "utterance": [
        {
          "id": "MDRW2100045470.1.1",
          "form": "물어보세요 후후후",
          "original_form": "물어보세요 후후후",
          "speaker_id": "2",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045470.1.2",
          "form": "양식인가요?",
          "original_form": "양식인가요?",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045470.1.3",
          "form": "아니요! 양식으로 헷갈릴 만도 한데 중식입니다",
          "original_form": "아니요! 양식으로 헷갈릴 만도 한데 중식입니다",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045470.1.4",
          "form": "저도 오늘 처움 알았어요",
          "original_form": "저도 오늘 처움 알았어요",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045470.1.5",
          "form": "혹시 토마토 달걀볶음인가요?",
          "original_form": "혹시 토마토 달걀볶음인가요?",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045470.1.6",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045470.1.7",
          "form": "저 지금 소름돋았어요",
          "original_form": "저 지금 소름돋았어요",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045470.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045470.1.9",
          "form": "와 뭐예요 진짜 어떻게 맞추셨지????????????",
          "original_form": "와 뭐예요 진짜 어떻게 맞추셨지????????????",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045470.1.10",
          "form": "힌트 딱 하나만 가지고 맞추셨어요 지금",
          "original_form": "힌트 딱 하나만 가지고 맞추셨어요 지금",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045470.1.11",
          "form": "진짜요?ㅋㅋㅋㅋㅋ",
          "original_form": "진짜요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045470.1.12",
          "form": "네! 진짜로요 저 지금 얼떨떨해요",
          "original_form": "네! 진짜로요 저 지금 얼떨떨해요",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045470.1.13",
          "form": "저희 집에 드론 날리셨나요",
          "original_form": "저희 집에 드론 날리셨나요",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045470.1.14",
          "form": "사람들이 양식으로 생각하지만 중식인데",
          "original_form": "사람들이 양식으로 생각하지만 중식인데",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045470.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045470.1.16",
          "form": "토마토 달걀볶음이라고 들어서욬ㅋㅋㅋ",
          "original_form": "토마토 달걀볶음이라고 들어서욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045470.1.17",
          "form": "대박이다 진짜",
          "original_form": "대박이다 진짜",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045470.1.18",
          "form": "상식왕이세요 ㅋㅋㅋㅋㅋㅋ전 오늘 처음 알았건만",
          "original_form": "상식왕이세요 ㅋㅋㅋㅋㅋㅋ전 오늘 처음 알았건만",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045470.1.19",
          "form": "토달볶 정답입니다~~~짝짝",
          "original_form": "토달볶 정답입니다~~~짝짝",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045470.1.20",
          "form": "저도 그렇게 오래되지 않았어요",
          "original_form": "저도 그렇게 오래되지 않았어요",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045470.1.21",
          "form": "맞추곧ㅎ 신기하네욬ㅋㅋ",
          "original_form": "맞추곧ㅎ 신기하네욬ㅋㅋ",
          "speaker_id": "1",
