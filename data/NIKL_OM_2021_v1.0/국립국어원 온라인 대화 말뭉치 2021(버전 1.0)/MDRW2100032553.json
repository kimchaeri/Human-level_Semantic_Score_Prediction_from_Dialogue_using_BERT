      "utterance": [
        {
          "id": "MDRW2100032553.1.1",
          "form": "김밥은..?라면이랑 딱인데...",
          "original_form": "김밥은..?라면이랑 딱인데...",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032553.1.2",
          "form": "ㅋㅋㅋ김밥도 고민햇는데",
          "original_form": "ㅋㅋㅋ김밥도 고민햇는데",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032553.1.3",
          "form": "땡기는 김밥이 없더라고",
          "original_form": "땡기는 김밥이 없더라고",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032553.1.4",
          "form": "아 그럼 아쉽겟다",
          "original_form": "아 그럼 아쉽겟다",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032553.1.5",
          "form": "머야 ...님 저녁 뭐드셨어요?...",
          "original_form": "머야 ...님 저녁 뭐드셨어요?...",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032553.1.6",
          "form": "나는 토스트해먹엇심",
          "original_form": "나는 토스트해먹엇심",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032553.1.7",
          "form": "오 머넣었어",
          "original_form": "오 머넣었어",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032553.1.8",
          "form": "베이컨?...",
          "original_form": "베이컨?...",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032553.1.9",
          "form": "길거리토스트느낌으로 계란에 파넣고 쨈바름ㅋㅋㅋㅋ",
          "original_form": "길거리토스트느낌으로 계란에 파넣고 쨈바름ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032553.1.10",
          "form": "ㅋㅋㅋㅋㅋ아 뭔지 알것같음",
          "original_form": "ㅋㅋㅋㅋㅋ아 뭔지 알것같음",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032553.1.11",
          "form": "마늘햄도 깔앗나여",
          "original_form": "마늘햄도 깔앗나여",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032553.1.12",
          "form": "마늘햄 넣으면 촉촉하니 더 맛잇어...",
          "original_form": "마늘햄 넣으면 촉촉하니 더 맛잇어...",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032553.1.13",
          "form": "ㅋㅋㅋㅋㅋ재료부족으로 가볍게..",
          "original_form": "ㅋㅋㅋㅋㅋ재료부족으로 가볍게..",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032553.1.14",
          "form": "담에 마능햄 사서만들어야겟다 ㅋㅋㅋ",
          "original_form": "담에 마능햄 사서만들어야겟다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032553.1.15",
          "form": "ㅋㅋㅋ추천드림다",
          "original_form": "ㅋㅋㅋ추천드림다",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032553.1.16",
          "form": "아니드 우리 다엿트 해여해",
          "original_form": "아니드 우리 다엿트 해여해",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032553.1.17",
          "form": "닭슴살 깔자...",
          "original_form": "닭슴살 깔자...",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032553.1.18",
          "form": "내일부터니까 ...낼부터 닭가슴살먹기..",
          "original_form": "내일부터니까 ...낼부터 닭가슴살먹기..",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032553.1.19",
          "form": "닭가슴살 샌드위치 레시피 있던데",
          "original_form": "닭가슴살 샌드위치 레시피 있던데",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032553.1.20",
          "form": "나름 맛잇도라",
          "original_form": "나름 맛잇도라",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032553.1.21",
          "form": "오 맞아 레시피되게많긴하드라",
          "original_form": "오 맞아 레시피되게많긴하드라",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032553.1.22",
          "form": "유튜브함찾아바야겟다..",
          "original_form": "유튜브함찾아바야겟다..",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032553.1.23",
          "form": "ㅇㅇ난 해먹을 손재주가 모자랐지만...",
          "original_form": "ㅇㅇ난 해먹을 손재주가 모자랐지만...",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032553.1.24",
          "form": "추천해...",
          "original_form": "추천해...",
          "speaker_id": "2",
          "time": "20210905 20:44"
        }
