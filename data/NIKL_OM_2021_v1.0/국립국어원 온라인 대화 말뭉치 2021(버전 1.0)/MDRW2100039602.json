      "utterance": [
        {
          "id": "MDRW2100039602.1.1",
          "form": "한국 가고싶은  곳 잇나용",
          "original_form": "한국 가고싶은  곳 잇나용",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039602.1.2",
          "form": "음... 서울이요!!!!",
          "original_form": "음... 서울이요!!!!",
          "speaker_id": "1",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039602.1.3",
          "form": "코로나만 끝나면 가서 끝장나게 놀고싶어요!!",
          "original_form": "코로나만 끝나면 가서 끝장나게 놀고싶어요!!",
          "speaker_id": "1",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039602.1.4",
          "form": "지방사시는가봐요?",
          "original_form": "지방사시는가봐요?",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039602.1.5",
          "form": "서울에 놀거리가 많은가요?!",
          "original_form": "서울에 놀거리가 많은가요?!",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039602.1.6",
          "form": "지방러는 약간",
          "original_form": "지방러는 약간",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039602.1.7",
          "form": "서울에 환상이 많아서요ㅋㅋㅋㅋ",
          "original_form": "서울에 환상이 많아서요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039602.1.8",
          "form": "그냥 막 돌아다녀보고 싶어요!!",
          "original_form": "그냥 막 돌아다녀보고 싶어요!!",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039602.1.9",
          "form": "저도 지방중에지방 부산사라요",
          "original_form": "저도 지방중에지방 부산사라요",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039602.1.10",
          "form": "!!저도 그 근처에 살아요!!",
          "original_form": "!!저도 그 근처에 살아요!!",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039602.1.11",
          "form": "김해...라고 아시나요...?",
          "original_form": "김해...라고 아시나요...?",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039602.1.12",
          "form": "당근빠다죠",
          "original_form": "당근빠다죠",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039602.1.13",
          "form": "ㅋㅋㅋㅋ놀러갈땐 무조건 부산 갔었는데ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ놀러갈땐 무조건 부산 갔었는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039602.1.14",
          "form": "ㅋㅋㅋㅋㅋ지겹겟군요",
          "original_form": "ㅋㅋㅋㅋㅋ지겹겟군요",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039602.1.15",
          "form": "전 서면 진짜 많이 갔어요ㅋㅋㅋ",
          "original_form": "전 서면 진짜 많이 갔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039602.1.16",
          "form": "부산은 놀거리가 많아서 좋을거같아요!!",
          "original_form": "부산은 놀거리가 많아서 좋을거같아요!!",
