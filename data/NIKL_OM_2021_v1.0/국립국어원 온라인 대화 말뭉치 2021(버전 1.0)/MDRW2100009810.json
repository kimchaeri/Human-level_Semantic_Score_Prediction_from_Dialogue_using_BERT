      "utterance": [
        {
          "id": "MDRW2100009810.1.1",
          "form": "오늘은 아직 밖에 안나가봤네요",
          "original_form": "오늘은 아직 밖에 안나가봤네요",
          "speaker_id": "2",
          "time": "20210823 10:21"
        },
        {
          "id": "MDRW2100009810.1.2",
          "form": "요즘 장마같아요",
          "original_form": "요즘 장마같아요",
          "speaker_id": "1",
          "time": "20210823 10:41"
        },
        {
          "id": "MDRW2100009810.1.3",
          "form": "어제는 오랜만에 화장했는데",
          "original_form": "어제는 오랜만에 화장했는데",
          "speaker_id": "1",
          "time": "20210823 10:41"
        },
        {
          "id": "MDRW2100009810.1.4",
          "form": "다시",
          "original_form": "다시",
          "speaker_id": "1",
          "time": "20210823 10:41"
        },
        {
          "id": "MDRW2100009810.1.5",
          "form": "날씨가 우중충해졌네요",
          "original_form": "날씨가 우중충해졌네요",
          "speaker_id": "1",
          "time": "20210823 10:42"
        },
        {
          "id": "MDRW2100009810.1.6",
          "form": "헉 다시 비가 오네요 지금 나왔는데ㅜ",
          "original_form": "헉 다시 비가 오네요 지금 나왔는데ㅜ",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009810.1.7",
          "form": "날씨가 너무 변덕이 심해요",
          "original_form": "날씨가 너무 변덕이 심해요",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009810.1.8",
          "form": "혹시 지금 어떤 우산 쓰세요?",
          "original_form": "혹시 지금 어떤 우산 쓰세요?",
          "speaker_id": "2",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100009810.1.9",
          "form": "사무실인데...",
          "original_form": "사무실인데...",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009810.1.10",
          "form": "저  우산 2개가 다 시원찮아서",
          "original_form": "저  우산 2개가 다 시원찮아서",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009810.1.11",
          "form": "오늘 쓰고온거는 파랑우산이에요",
          "original_form": "오늘 쓰고온거는 파랑우산이에요",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009810.1.12",
          "form": "비가 많이오니 안으로 비가 새더라고요 ㅋㅋ",
          "original_form": "비가 많이오니 안으로 비가 새더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009810.1.13",
          "form": "우산써도 우산안으로 비가 뚝뚝",
          "original_form": "우산써도 우산안으로 비가 뚝뚝",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009810.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009810.1.15",
          "form": "어디 가는 길이세요?",
          "original_form": "어디 가는 길이세요?",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009810.1.16",
          "form": "우산도 무슨 브랜드를 사야하는건가 싶었네요",
          "original_form": "우산도 무슨 브랜드를 사야하는건가 싶었네요",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009810.1.17",
          "form": "저는 지금 학원가는 길이에요 ㅎㅎ",
          "original_form": "저는 지금 학원가는 길이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009810.1.18",
          "form": "잘 모르겠네요",
          "original_form": "잘 모르겠네요",
          "speaker_id": "1",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009810.1.19",
          "form": "저는그냥 집에 있는거 아무거나 사용해서",
          "original_form": "저는그냥 집에 있는거 아무거나 사용해서",
          "speaker_id": "1",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009810.1.20",
          "form": "하긴 저도 이전까지는 그렇긴했어요",
          "original_form": "하긴 저도 이전까지는 그렇긴했어요",
          "speaker_id": "2",
          "time": "20210823 11:05"
        },
        {
          "id": "MDRW2100009810.1.21",
          "form": "하 우산들이 왜이렇게 약하지 ㅋㅋ",
          "original_form": "하 우산들이 왜이렇게 약하지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:05"
        },
        {
          "id": "MDRW2100009810.1.22",
          "form": "아니 비가 센건가 ㅋㅋㅋ",
          "original_form": "아니 비가 센건가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:05"
        }
      ]
