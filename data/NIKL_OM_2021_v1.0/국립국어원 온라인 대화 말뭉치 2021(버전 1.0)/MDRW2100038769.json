      "utterance": [
        {
          "id": "MDRW2100038769.1.1",
          "form": "맞아요 새우는 사랑이죠",
          "original_form": "맞아요 새우는 사랑이죠",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038769.1.2",
          "form": "근데 요즘 피곤할때 새우먹으면 두드러기 나더라구요",
          "original_form": "근데 요즘 피곤할때 새우먹으면 두드러기 나더라구요",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038769.1.3",
          "form": "평상시에는 괜찮은데 말이죠",
          "original_form": "평상시에는 괜찮은데 말이죠",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038769.1.4",
          "form": "알레르기있어요?",
          "original_form": "알레르기있어요?",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038769.1.5",
          "form": "저는 새우버거 2개씩 먹거든요",
          "original_form": "저는 새우버거 2개씩 먹거든요",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100038769.1.6",
          "form": "양이적어서",
          "original_form": "양이적어서",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100038769.1.7",
          "form": "알레르기가 생겼어요",
          "original_form": "알레르기가 생겼어요",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100038769.1.8",
          "form": "원래는 없었어요ㅠ",
          "original_form": "원래는 없었어요ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100038769.1.9",
          "form": "아...면역력이떨어졌나봐요ㅜㅜ",
          "original_form": "아...면역력이떨어졌나봐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038769.1.10",
          "form": "그러면 원래 없었던게 새로생기거든요",
          "original_form": "그러면 원래 없었던게 새로생기거든요",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038769.1.11",
          "form": "저도 그래가지구....",
          "original_form": "저도 그래가지구....",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038769.1.12",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038769.1.13",
          "form": "건강 잘 챙겨야겠어요",
          "original_form": "건강 잘 챙겨야겠어요",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100038769.1.14",
          "form": "먹고싶은거 먹을려면",
          "original_form": "먹고싶은거 먹을려면",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100038769.1.15",
          "form": "그니깐요...안그래도 따로 운동을 안하고있어서",
          "original_form": "그니깐요...안그래도 따로 운동을 안하고있어서",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038769.1.16",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038769.1.17",
          "form": "저도 운동은 홈트 진짜 간단하게 하는게 끝이에요",
          "original_form": "저도 운동은 홈트 진짜 간단하게 하는게 끝이에요",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038769.1.18",
          "form": "운동은 그러면 좀있다가 얘기합시당ㅋㅋㄱ",
          "original_form": "운동은 그러면 좀있다가 얘기합시당ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038769.1.19",
          "form": "저녁은 맛있게 먹었어요?",
          "original_form": "저녁은 맛있게 먹었어요?",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038769.1.20",
          "form": "잘 먹었습니다ㅎㅎ 떡볶이 역시 맛있어요",
          "original_form": "잘 먹었습니다ㅎㅎ 떡볶이 역시 맛있어요",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038769.1.21",
          "form": "배부르게먹었나요?",
          "original_form": "배부르게먹었나요?",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038769.1.22",
          "form": "청년다방 맛있으면",
          "original_form": "청년다방 맛있으면",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038769.1.23",
          "form": "나중에 저도 함 가볼려구욤",
          "original_form": "나중에 저도 함 가볼려구욤",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038769.1.24",
          "form": "ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038769.1.25",
          "form": "배 터질거 같아요ㅋㅋ 꼭 가보세요",
          "original_form": "배 터질거 같아요ㅋㅋ 꼭 가보세요",
