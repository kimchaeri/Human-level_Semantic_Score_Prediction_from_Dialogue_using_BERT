      "utterance": [
        {
          "id": "MDRW2100031682.1.1",
          "form": "ㅎㅎ 가족이야기하가가 차이야기로 넘어가네요ㅎㅎㅎ",
          "original_form": "ㅎㅎ 가족이야기하가가 차이야기로 넘어가네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031682.1.2",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031682.1.3",
          "form": "그것더 가족이야기죠 ㅋ",
          "original_form": "그것더 가족이야기죠 ㅋ",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031682.1.4",
          "form": "원래 전차가 올란도였는데  차박으로 안성맞춤인차인데",
          "original_form": "원래 전차가 올란도였는데  차박으로 안성맞춤인차인데",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031682.1.5",
          "form": "아쉽게  고장나서리",
          "original_form": "아쉽게  고장나서리",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031682.1.6",
          "form": "아아 올란도 좋죠 ㅡㅎㅎ",
          "original_form": "아아 올란도 좋죠 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031682.1.7",
          "form": "큰차가 좋긴해요ㅎ",
          "original_form": "큰차가 좋긴해요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031682.1.8",
          "form": "가성비 좋은차인데  단종됐어요",
          "original_form": "가성비 좋은차인데  단종됐어요",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031682.1.9",
          "form": "이번에 이혼하면",
          "original_form": "이번에 이혼하면",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100031682.1.10",
          "form": "바꾸려구요ㅎㅎㅎ 아이랑 차박도하고 놀러도갈겸ㅎ",
          "original_form": "바꾸려구요ㅎㅎㅎ 아이랑 차박도하고 놀러도갈겸ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031682.1.11",
          "form": "음 차가 지금 괜찮으면 구지",
          "original_form": "음 차가 지금 괜찮으면 구지",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031682.1.12",
          "form": "그냥 타시는거 추천해요 ㅎㅎ",
          "original_form": "그냥 타시는거 추천해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031682.1.13",
          "form": "맞아요ㅎ 저도 그생각인데ㅎㅎ 한번봐보려구요ㅎ",
          "original_form": "맞아요ㅎ 저도 그생각인데ㅎㅎ 한번봐보려구요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031682.1.14",
          "form": "요즘 애가 계속 캠핑가자고해서ㅎㅎ 고민중이에요ㅎ",
          "original_form": "요즘 애가 계속 캠핑가자고해서ㅎㅎ 고민중이에요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031682.1.15",
          "form": "아이들 그나이는 차박 비추에요 특히 여아는",
          "original_form": "아이들 그나이는 차박 비추에요 특히 여아는",
          "speaker_id": "1",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031682.1.16",
          "form": "씻고 화장실.좋은 곳",
          "original_form": "씻고 화장실.좋은 곳",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031682.1.17",
          "form": "아 그렇군요ㅎ",
          "original_form": "아 그렇군요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031682.1.18",
          "form": "글램핑이나",
          "original_form": "글램핑이나",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031682.1.19",
          "form": "화장실은 간이로 하려고하는데ㅎㅎ",
          "original_form": "화장실은 간이로 하려고하는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031682.1.20",
          "form": "카라반 추천이요",
          "original_form": "카라반 추천이요",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031682.1.21",
          "form": "글램핑은 한번가봤어요 ㅡㅎ",
          "original_form": "글램핑은 한번가봤어요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031682.1.22",
          "form": "카라반ㅎㅎ",
          "original_form": "카라반ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031682.1.23",
          "form": "요즘은 카라반 캠핑장도 많아서 ㅎㅎ",
          "original_form": "요즘은 카라반 캠핑장도 많아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031682.1.24",
          "form": "차박은 좀크구요",
          "original_form": "차박은 좀크구요",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100031682.1.25",
          "form": "ㅎㅎ 그렇군요ㅎ",
          "original_form": "ㅎㅎ 그렇군요ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031682.1.26",
          "form": "아아님 캠핑많이 가보셨군요?ㅎ",
          "original_form": "아아님 캠핑많이 가보셨군요?ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100031682.1.27",
          "form": "전 캠핑까진 아니구 당일치기요",
          "original_form": "전 캠핑까진 아니구 당일치기요",
          "speaker_id": "1",
