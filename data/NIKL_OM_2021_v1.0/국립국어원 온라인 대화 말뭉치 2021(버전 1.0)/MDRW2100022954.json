      "utterance": [
        {
          "id": "MDRW2100022954.1.1",
          "form": "앞에껄 못봣어요.. \n무슨 1인자요?",
          "original_form": "앞에껄 못봣어요.. \n무슨 1인자요?",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022954.1.2",
          "form": "동물계의 1인자요 ㅋㅋ",
          "original_form": "동물계의 1인자요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022954.1.3",
          "form": "당근... 공룡이요.. ㅋㅋㅋㅋ",
          "original_form": "당근... 공룡이요.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022954.1.4",
          "form": "아 ㅋㅋ 현존하는 동물중에서요 !",
          "original_form": "아 ㅋㅋ 현존하는 동물중에서요 !",
          "speaker_id": "1",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022954.1.5",
          "form": "ㅋㅋㅋ 코끼리요..",
          "original_form": "ㅋㅋㅋ 코끼리요..",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022954.1.6",
          "form": "코끼리는 사자도 건드리지 못하는..",
          "original_form": "코끼리는 사자도 건드리지 못하는..",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022954.1.7",
          "form": "하긴 코 한방 맞으면 즉사할듯요 ..",
          "original_form": "하긴 코 한방 맞으면 즉사할듯요 ..",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022954.1.8",
          "form": "사람도 깔아뭉개는 판국에 ..",
          "original_form": "사람도 깔아뭉개는 판국에 ..",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022954.1.9",
          "form": "ㅋㅋㅋㅋ \n설마 개인적으로 코끼리는 키우는 사람 엄겠죠?",
          "original_form": "ㅋㅋㅋㅋ \n설마 개인적으로 코끼리는 키우는 사람 엄겠죠?",
          "speaker_id": "2",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022954.1.10",
          "form": "키우다가 .. 코로 맞아죽으면 어케요?",
          "original_form": "키우다가 .. 코로 맞아죽으면 어케요?",
          "speaker_id": "1",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022954.1.11",
          "form": "ㅋㅋㅋ 코에 뭔가 꽂히셨군요.. ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 코에 뭔가 꽂히셨군요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022954.1.12",
          "form": "덤보같은 애는 귀엽겠어요 ㅎ",
          "original_form": "덤보같은 애는 귀엽겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022954.1.13",
          "form": "그쵸.. 반려동물은 딱.. 사자 호랑이까지가..",
          "original_form": "그쵸.. 반려동물은 딱.. 사자 호랑이까지가..",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022954.1.14",
          "form": "호랑이 키우는건 본거같아요",
          "original_form": "호랑이 키우는건 본거같아요",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022954.1.15",
          "form": "듬직하니 ...",
          "original_form": "듬직하니 ...",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022954.1.16",
          "form": "잔인한데.. 이빨이랑 손톱 다 갈아내더라구요..",
          "original_form": "잔인한데.. 이빨이랑 손톱 다 갈아내더라구요..",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022954.1.17",
          "form": "아랍형님들이 호랑이 플렉스 많이하시죠.. ㅋㅋㅋ",
          "original_form": "아랍형님들이 호랑이 플렉스 많이하시죠.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022954.1.18",
          "form": "플렉스 ㅋㅋ 맞아요 ㅋㅋ",
          "original_form": "플렉스 ㅋㅋ 맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022954.1.19",
          "form": "아니 일단 전 제가 안먹힌다는 전제하에 키우겠어요",
