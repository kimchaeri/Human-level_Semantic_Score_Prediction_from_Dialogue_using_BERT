      "utterance": [
        {
          "id": "MDRW2100039846.1.1",
          "form": "어제까지 비오고 날씨가 좋네요",
          "original_form": "어제까지 비오고 날씨가 좋네요",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100039846.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100039846.1.3",
          "form": "비안오는 날 넘 좋아요",
          "original_form": "비안오는 날 넘 좋아요",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100039846.1.4",
          "form": "봄부터 비가 넘 자주 왔죠ㅠㅠ",
          "original_form": "봄부터 비가 넘 자주 왔죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100039846.1.5",
          "form": "이제 가을 장마는 끝인듯 해요 ㅋㅋ",
          "original_form": "이제 가을 장마는 끝인듯 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100039846.1.6",
          "form": "올해 비가 넘 많이 와서 이상해요 ㅜ",
          "original_form": "올해 비가 넘 많이 와서 이상해요 ㅜ",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100039846.1.7",
          "form": "제발!!",
          "original_form": "제발!!",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100039846.1.8",
          "form": "올해 비가 너무 와서 이상해요",
          "original_form": "올해 비가 너무 와서 이상해요",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100039846.1.9",
          "form": "이게 이상 기후겠죠?",
          "original_form": "이게 이상 기후겠죠?",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100039846.1.10",
          "form": "그런가봐요 ㅜ",
          "original_form": "그런가봐요 ㅜ",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100039846.1.11",
          "form": "플라스틱 줄여야지 정말..",
          "original_form": "플라스틱 줄여야지 정말..",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100039846.1.12",
          "form": "맞아요 환경 오염 ㅠ 나중에는 여름과 겨울만 있을 것 같아요",
          "original_form": "맞아요 환경 오염 ㅠ 나중에는 여름과 겨울만 있을 것 같아요",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100039846.1.13",
          "form": "흑 ㅜ",
          "original_form": "흑 ㅜ",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100039846.1.14",
          "form": "더 더워지고 더 추워지고...",
          "original_form": "더 더워지고 더 추워지고...",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100039846.1.15",
          "form": "ㅠ 안되는데 ㅠ",
          "original_form": "ㅠ 안되는데 ㅠ",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100039846.1.16",
          "form": "폭염이랑 한파보면 걱정이 많이 되요ㅠ",
          "original_form": "폭염이랑 한파보면 걱정이 많이 되요ㅠ",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100039846.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100039846.1.18",
          "form": "인간의 날씨를 위해서라도",
          "original_form": "인간의 날씨를 위해서라도",
          "speaker_id": "2",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100039846.1.19",
          "form": "환경 생각해야돼요",
          "original_form": "환경 생각해야돼요",
          "speaker_id": "2",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100039846.1.20",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100039846.1.21",
          "form": "네 노력해야죠 ㅠ 미세먼지는 중국만 잘하면 되는데요",
          "original_form": "네 노력해야죠 ㅠ 미세먼지는 중국만 잘하면 되는데요",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100039846.1.22",
          "form": "ㅋㅋㅋ중국 없어지면",
          "original_form": "ㅋㅋㅋ중국 없어지면",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100039846.1.23",
          "form": "지구 컨디션 100년전으로 될것같아요",
          "original_form": "지구 컨디션 100년전으로 될것같아요",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100039846.1.24",
          "form": "아 그럴지도요 황사 미세먼지 없고 날씨도 좋아지겠죠",
          "original_form": "아 그럴지도요 황사 미세먼지 없고 날씨도 좋아지겠죠",
