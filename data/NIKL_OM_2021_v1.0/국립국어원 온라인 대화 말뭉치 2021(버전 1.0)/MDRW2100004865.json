      "utterance": [
        {
          "id": "MDRW2100004865.1.1",
          "form": "오늘 뭐 잼난거하나요?",
          "original_form": "오늘 뭐 잼난거하나요?",
          "speaker_id": "1",
          "time": "20210821 18:59"
        },
        {
          "id": "MDRW2100004865.1.2",
          "form": "유튜브 볼려고요!",
          "original_form": "유튜브 볼려고요!",
          "speaker_id": "2",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004865.1.3",
          "form": "유튜브 뭐보세요?",
          "original_form": "유튜브 뭐보세요?",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004865.1.4",
          "form": "고양이같은 동물이나",
          "original_form": "고양이같은 동물이나",
          "speaker_id": "2",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004865.1.5",
          "form": "게임이요!",
          "original_form": "게임이요!",
          "speaker_id": "2",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004865.1.6",
          "form": "아 겜은 누가하는걸 그냥지켜보는거예요?",
          "original_form": "아 겜은 누가하는걸 그냥지켜보는거예요?",
          "speaker_id": "1",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004865.1.7",
          "form": "네 아니면 편집해서 하이라이트 형식으로",
          "original_form": "네 아니면 편집해서 하이라이트 형식으로",
          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004865.1.8",
          "form": "나온것도 봐요!",
          "original_form": "나온것도 봐요!",
          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004865.1.9",
          "form": "게임 좋아하시는군요",
          "original_form": "게임 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004865.1.10",
          "form": "네 게임 엄청 좋아해요 ㅎㅎ",
          "original_form": "네 게임 엄청 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004865.1.11",
          "form": "보는거 하시는거 다좋아하세요?",
          "original_form": "보는거 하시는거 다좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004865.1.12",
          "form": "둘다요!",
          "original_form": "둘다요!",
          "speaker_id": "2",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004865.1.13",
          "form": "고양이 강이지 동영상은 주로 모나와요?",
          "original_form": "고양이 강이지 동영상은 주로 모나와요?",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004865.1.14",
          "form": "그냥 고양이 키우는거랑",
          "original_form": "그냥 고양이 키우는거랑",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004865.1.15",
          "form": "뭐 놀아주는거랑 등등",
          "original_form": "뭐 놀아주는거랑 등등",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004865.1.16",
          "form": "나오더라고용",
          "original_form": "나오더라고용",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004865.1.17",
          "form": "귀엽겠어요",
          "original_form": "귀엽겠어요",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004865.1.18",
          "form": "보고있음 힐링되겠네요",
          "original_form": "보고있음 힐링되겠네요",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004865.1.19",
          "form": "맞아요! 힐링 엄청되요 ㅠㅠ",
          "original_form": "맞아요! 힐링 엄청되요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004865.1.20",
          "form": "저도 찾아봐야겠네요ㅎㅎ",
          "original_form": "저도 찾아봐야겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:46"
        }
