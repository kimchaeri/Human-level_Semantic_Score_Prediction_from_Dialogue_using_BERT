      "utterance": [
        {
          "id": "MDRW2100016883.1.1",
          "form": "싸이월드 세대야?",
          "original_form": "싸이월드 세대야?",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016883.1.2",
          "form": "아닝",
          "original_form": "아닝",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016883.1.3",
          "form": "난 안햇고",
          "original_form": "난 안햇고",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016883.1.4",
          "form": "언니가ㅎ",
          "original_form": "언니가ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016883.1.5",
          "form": "난.. 버디버디..?",
          "original_form": "난.. 버디버디..?",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016883.1.6",
          "form": "버디버디는 뭐야",
          "original_form": "버디버디는 뭐야",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.7",
          "form": "카톡같은건가",
          "original_form": "카톡같은건가",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.8",
          "form": "채팅하는건데",
          "original_form": "채팅하는건데",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.9",
          "form": "피씨로",
          "original_form": "피씨로",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.10",
          "form": "하는거",
          "original_form": "하는거",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.11",
          "form": "싸이월드 부활한다한지 일년되지않앗나 왜안해",
          "original_form": "싸이월드 부활한다한지 일년되지않앗나 왜안해",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.12",
          "form": "그전엔",
          "original_form": "그전엔",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.13",
          "form": "문자 알쓰고",
          "original_form": "문자 알쓰고",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.14",
          "form": "아는사람끼리?",
          "original_form": "아는사람끼리?",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.15",
          "form": "요금제한잇고 이래서",
          "original_form": "요금제한잇고 이래서",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.16",
          "form": "무료로 집에서 채팅하는게",
          "original_form": "무료로 집에서 채팅하는게",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016883.1.17",
          "form": "확실히 위에 언니오빠 있으면 이런 게 빠르긴 한 거 같기도",
          "original_form": "확실히 위에 언니오빠 있으면 이런 게 빠르긴 한 거 같기도",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.18",
          "form": "꿀이엇을듯",
          "original_form": "꿀이엇을듯",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.19",
          "form": "그냥 집에서 문자하는 느낌인가 흠",
          "original_form": "그냥 집에서 문자하는 느낌인가 흠",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.20",
          "form": "모르는사람이랑도 되긴하는데",
          "original_form": "모르는사람이랑도 되긴하는데",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.21",
          "form": "보통 친구들끼리",
          "original_form": "보통 친구들끼리",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.22",
          "form": "야 겜 언제 접속하냐",
          "original_form": "야 겜 언제 접속하냐",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.23",
          "form": "난 네이트온 눌러서 요금폭탄맞은 기억뿐ㅋㅋㅋㅋ",
          "original_form": "난 네이트온 눌러서 요금폭탄맞은 기억뿐ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.24",
          "form": "나 이따 저녁먹고 들감",
          "original_form": "나 이따 저녁먹고 들감",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.25",
          "form": "이런얘기햇던듯",
          "original_form": "이런얘기햇던듯",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.26",
          "form": "언니가ㅎ",
          "original_form": "언니가ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016883.1.27",
          "form": "위에 언니가 잇어서",
          "original_form": "위에 언니가 잇어서",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016883.1.28",
          "form": "아이돌 딱히 관심없엇는데",
          "original_form": "아이돌 딱히 관심없엇는데",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016883.1.29",
          "form": "최신가요 다 알고",
          "original_form": "최신가요 다 알고",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016883.1.30",
          "form": "껄껄",
          "original_form": "껄껄",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016883.1.31",
          "form": "그건 언니의 영향이 마따",
          "original_form": "그건 언니의 영향이 마따",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016883.1.32",
          "form": "ㅋㅋㅋㅋㅋ싸이월드브금",
          "original_form": "ㅋㅋㅋㅋㅋ싸이월드브금",
          "speaker_id": "1",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016883.1.33",
          "form": "으로 flower dance 이런거 맞나",
          "original_form": "으로 flower dance 이런거 맞나",
          "speaker_id": "1",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016883.1.34",
          "form": "와이스타일(안들어봄)",
          "original_form": "와이스타일(안들어봄)",
          "speaker_id": "1",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016883.1.35",
          "form": "나도 와이스타일 안들어봣는데",
          "original_form": "나도 와이스타일 안들어봣는데",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016883.1.36",
          "form": "플라워댄스는 들어본듯?",
          "original_form": "플라워댄스는 들어본듯?",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016883.1.37",
          "form": "벌써일년?이건아닌가",
          "original_form": "벌써일년?이건아닌가",
          "speaker_id": "1",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016883.1.38",
          "form": "잘 몰겟지만 노래는 좋음",
          "original_form": "잘 몰겟지만 노래는 좋음",
          "speaker_id": "2",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016883.1.39",
          "form": "뭔가 어릴때 자주들은듯 히시어쩌구조",
          "original_form": "뭔가 어릴때 자주들은듯 히시어쩌구조",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016883.1.40",
          "form": "마자 벌써일년 띵곡",
          "original_form": "마자 벌써일년 띵곡",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016883.1.41",
          "form": "부활 언제하는지 궁금하긴함",
          "original_form": "부활 언제하는지 궁금하긴함",
          "speaker_id": "2",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016883.1.42",
          "form": "히가시 조..?",
          "original_form": "히가시 조..?",
          "speaker_id": "2",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016883.1.43",
          "form": "김연아 싸이했던 거 봤는데",
          "original_form": "김연아 싸이했던 거 봤는데",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016883.1.44",
          "form": "내가 상상한거랑 조금 다르긴 한데 졸귀",
          "original_form": "내가 상상한거랑 조금 다르긴 한데 졸귀",
          "speaker_id": "1",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016883.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016883.1.46",
          "form": "피부과 3,40분 걸린다네 대기",
          "original_form": "피부과 3,40분 걸린다네 대기",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016883.1.47",
          "form": "점심은 안먹을건가바",
          "original_form": "점심은 안먹을건가바",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016883.1.48",
          "form": "희희",
          "original_form": "희희",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016883.1.49",
          "form": "머먹즹 그 근방에",
          "original_form": "머먹즹 그 근방에",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016883.1.50",
          "form": "맛난게 잇나",
          "original_form": "맛난게 잇나",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016883.1.51",
          "form": "먹고싶은 메뉴 잇으심까",
          "original_form": "먹고싶은 메뉴 잇으심까",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016883.1.52",
          "form": "사실 난.. 고기먹고싶엉",
          "original_form": "사실 난.. 고기먹고싶엉",
          "speaker_id": "2",
          "time": "20210826 16:08"
        }
