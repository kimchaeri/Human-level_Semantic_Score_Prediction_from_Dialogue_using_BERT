      "utterance": [
        {
          "id": "MDRW2100018205.1.1",
          "form": "요즘 재밌게 보신 드라마나 예능 있으신가요?",
          "original_form": "요즘 재밌게 보신 드라마나 예능 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018205.1.2",
          "form": "요즘엔 대탈출이여",
          "original_form": "요즘엔 대탈출이여",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018205.1.3",
          "form": "아하!! 저는 방금 슬의생봤어요ㅎㅎ",
          "original_form": "아하!! 저는 방금 슬의생봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018205.1.4",
          "form": "저는 드라마를 진짜 안봐서 ㅋㅋㅋㅋ",
          "original_form": "저는 드라마를 진짜 안봐서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018205.1.5",
          "form": "마지막으로 본게 시그널이거든여",
          "original_form": "마지막으로 본게 시그널이거든여",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018205.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018205.1.7",
          "form": "오ㅋㅋㅋㅋㅋ시그널은 재밌게 보셨나용?",
          "original_form": "오ㅋㅋㅋㅋㅋ시그널은 재밌게 보셨나용?",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018205.1.8",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018205.1.9",
          "form": "시그널 안 보셧어요??",
          "original_form": "시그널 안 보셧어요??",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018205.1.10",
          "form": "제가 챙겨보진 않고 가족들이 봐서 드문드문 봤네용",
          "original_form": "제가 챙겨보진 않고 가족들이 봐서 드문드문 봤네용",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018205.1.11",
          "form": "근데 다들 연기를 너무 잘하더라구요",
          "original_form": "근데 다들 연기를 너무 잘하더라구요",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018205.1.12",
          "form": "시그널 개존잼이에여 ㅠㅠㅠ",
          "original_form": "시그널 개존잼이에여 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018205.1.13",
          "form": "담에 제대로 한번 봐볼게요!!",
          "original_form": "담에 제대로 한번 봐볼게요!!",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018205.1.14",
          "form": "영화는 좋아하세요?",
          "original_form": "영화는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018205.1.15",
          "form": "영화 어제도 봤져",
          "original_form": "영화 어제도 봤져",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018205.1.16",
          "form": "오 뭐보셨어요!?",
          "original_form": "오 뭐보셨어요!?",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018205.1.17",
          "form": "일본 영화였는데..",
          "original_form": "일본 영화였는데..",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018205.1.18",
          "form": "한국 영화 리메이크 한거라고 하더라구여",
          "original_form": "한국 영화 리메이크 한거라고 하더라구여",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018205.1.19",
          "form": "오 뭘까용,,,영화관에서 보셨어요?",
          "original_form": "오 뭘까용,,,영화관에서 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018205.1.20",
          "form": "아녀 왓챠로 봤어여",
          "original_form": "아녀 왓챠로 봤어여",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018205.1.21",
          "form": "제목이 기억이 안나네여...",
          "original_form": "제목이 기억이 안나네여...",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018205.1.22",
          "form": "아하!ㅋㅋㅋㅋ왓챠로 자주 보시나봐여",
          "original_form": "아하!ㅋㅋㅋㅋ왓챠로 자주 보시나봐여",
