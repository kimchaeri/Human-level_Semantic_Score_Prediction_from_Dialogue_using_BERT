      "utterance": [
        {
          "id": "MDRW2100004500.1.1",
          "form": "여행가는 거 좋아하시나요",
          "original_form": "여행가는 거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004500.1.2",
          "form": "네 여행 좋아해요!!",
          "original_form": "네 여행 좋아해요!!",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004500.1.3",
          "form": "저두요 여행가고싶네요",
          "original_form": "저두요 여행가고싶네요",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004500.1.4",
          "form": "요즘에는 해외가기 힘들어서 국내여행은 많이 하시나요!",
          "original_form": "요즘에는 해외가기 힘들어서 국내여행은 많이 하시나요!",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004500.1.5",
          "form": "국내여행도 코로나 때문에",
          "original_form": "국내여행도 코로나 때문에",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004500.1.6",
          "form": "근처에 당일치기로 주로 다니고",
          "original_form": "근처에 당일치기로 주로 다니고",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004500.1.7",
          "form": "그나마도 자제하게 되더라고요",
          "original_form": "그나마도 자제하게 되더라고요",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004500.1.8",
          "form": "맞아요 괜히 갔다가 확진자동선이라도 겹칠까봐 조심하게 되는 거 같더라구요",
          "original_form": "맞아요 괜히 갔다가 확진자동선이라도 겹칠까봐 조심하게 되는 거 같더라구요",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004500.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004500.1.10",
          "form": "그리고 보통 여행지가 다 비슷해서 사람이 많더라구요",
          "original_form": "그리고 보통 여행지가 다 비슷해서 사람이 많더라구요",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004500.1.11",
          "form": "그리고 전 수도권에 사는데 괜히 의도치 않게 전파에 기여할까봐...",
          "original_form": "그리고 전 수도권에 사는데 괜히 의도치 않게 전파에 기여할까봐...",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004500.1.12",
          "form": "조심하게 돼요 하루에 2천명씩 생기니까,,,,",
          "original_form": "조심하게 돼요 하루에 2천명씩 생기니까,,,,",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004500.1.13",
          "form": "여행가셨던 곳 중에서 어디가 제일 좋으셨어요",
          "original_form": "여행가셨던 곳 중에서 어디가 제일 좋으셨어요",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004500.1.14",
          "form": "저는 동유럽 여행 갔던 게 좋았어요~",
          "original_form": "저는 동유럽 여행 갔던 게 좋았어요~",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004500.1.15",
          "form": "붉은 지붕들이 멋있었고",
          "original_form": "붉은 지붕들이 멋있었고",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004500.1.16",
          "form": "성벽 걷는 것도 좋았어요",
          "original_form": "성벽 걷는 것도 좋았어요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004500.1.17",
          "form": "와 상상만해도 낭만있네요",
          "original_form": "와 상상만해도 낭만있네요",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004500.1.18",
          "form": "저도 동유럽가보고싶어요",
          "original_form": "저도 동유럽가보고싶어요",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004500.1.19",
          "form": "저는 동유럽쪽은 체코만 가봤는데 오스트리아 너무 멋있어보였어요",
          "original_form": "저는 동유럽쪽은 체코만 가봤는데 오스트리아 너무 멋있어보였어요",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004500.1.20",
          "form": "저는 체코-헝가리-크로아티아 코스로 갔는데",
          "original_form": "저는 체코-헝가리-크로아티아 코스로 갔는데",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004500.1.21",
          "form": "크로아티아 추천해요~",
          "original_form": "크로아티아 추천해요~",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004500.1.22",
          "form": "해외여행 하던 시절이 너무 먼 옛날 같네요",
          "original_form": "해외여행 하던 시절이 너무 먼 옛날 같네요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004500.1.23",
          "form": "그러게요 거의 2년가까이 막히니까",
          "original_form": "그러게요 거의 2년가까이 막히니까",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004500.1.24",
          "form": "백신은 맞으셨나요",
          "original_form": "백신은 맞으셨나요",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004500.1.25",
          "form": "백신맞아도 여행은 아직 힘든 거 같어요",
          "original_form": "백신맞아도 여행은 아직 힘든 거 같어요",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004500.1.26",
          "form": "네 1차만 맞은 상태예요",
          "original_form": "네 1차만 맞은 상태예요",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004500.1.27",
          "form": "네 백신 맞아도 한동안 해외는 못갈 듯",
          "original_form": "네 백신 맞아도 한동안 해외는 못갈 듯",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004500.1.28",
          "form": "맞아요오",
          "original_form": "맞아요오",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004500.1.29",
          "form": "해외여행은 힘들 거 같아요",
          "original_form": "해외여행은 힘들 거 같아요",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004500.1.30",
          "form": "면세점가고싶어요",
          "original_form": "면세점가고싶어요",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004500.1.31",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004500.1.32",
          "form": "공항에서 느껴지는 그 기분이 그리워요",
          "original_form": "공항에서 느껴지는 그 기분이 그리워요",
          "speaker_id": "2",
          "time": "20210821 17:58"
        }
      ]
    }
