      "utterance": [
        {
          "id": "MDRW2100041420.1.1",
          "form": "점심은드셨어요?",
          "original_form": "점심은드셨어요?",
          "speaker_id": "1",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100041420.1.2",
          "form": "안먹었네요",
          "original_form": "안먹었네요",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041420.1.3",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041420.1.4",
          "form": "ㅎㅎ넵지금저녁도먹고있어요",
          "original_form": "ㅎㅎ넵지금저녁도먹고있어요",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041420.1.5",
          "form": "배안고프세요?",
          "original_form": "배안고프세요?",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041420.1.6",
          "form": "아직은 괜찮네요",
          "original_form": "아직은 괜찮네요",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041420.1.7",
          "form": "왜 저녁을 일찍 드시나요?",
          "original_form": "왜 저녁을 일찍 드시나요?",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041420.1.8",
          "form": "직장에서당직중인데",
          "original_form": "직장에서당직중인데",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041420.1.9",
          "form": "식당이6시까지에요ㅠ",
          "original_form": "식당이6시까지에요ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041420.1.10",
          "form": "그럼 오늘 야식을 드셔야할 수도 있을 만큼 늦게 퇴근하시나요?",
          "original_form": "그럼 오늘 야식을 드셔야할 수도 있을 만큼 늦게 퇴근하시나요?",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041420.1.11",
          "form": "앗ㅎㅎ오늘당직실에서자는건데",
          "original_form": "앗ㅎㅎ오늘당직실에서자는건데",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041420.1.12",
          "form": "야식은안먹으려고요!",
          "original_form": "야식은안먹으려고요!",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041420.1.13",
          "form": "요새살쪄서..",
          "original_form": "요새살쪄서..",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041420.1.14",
          "form": "고생하시네요ㅠㅠ",
          "original_form": "고생하시네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041420.1.15",
          "form": "그럼 오늘 메뉴는 무엇인가요?",
          "original_form": "그럼 오늘 메뉴는 무엇인가요?",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041420.1.16",
          "form": "오늘은 식당에서 순대볶음나왔어요",
          "original_form": "오늘은 식당에서 순대볶음나왔어요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041420.1.17",
          "form": "어떤음료좋아하세요?",
          "original_form": "어떤음료좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041420.1.18",
          "form": "저는 환타요",
          "original_form": "저는 환타요",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041420.1.19",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041420.1.20",
          "form": "ㅎㅎ저는 아아요!",
          "original_form": "ㅎㅎ저는 아아요!",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041420.1.21",
          "form": "어느 브랜드 아아 제일 좋아하세요?",
          "original_form": "어느 브랜드 아아 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041420.1.22",
          "form": "저는 다비슷해요ㅎㅎ굳이고르자면투썸이요",
          "original_form": "저는 다비슷해요ㅎㅎ굳이고르자면투썸이요",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041420.1.23",
          "form": "커피드세요?",
          "original_form": "커피드세요?",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041420.1.24",
          "form": "달달한 것만 먹어요",
          "original_form": "달달한 것만 먹어요",
          "speaker_id": "2",
          "time": "20210906 18:13"
        }
      ]
