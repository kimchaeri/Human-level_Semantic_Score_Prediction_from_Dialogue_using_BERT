      "utterance": [
        {
          "id": "MDRW2100045460.1.1",
          "form": "name3 발톱!",
          "original_form": "&name3& 발톱!",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045460.1.2",
          "form": "낮에 보니까 침대에서 자는데 햇빛이 바로 내리꽂히는데서 자고 있더라구요",
          "original_form": "낮에 보니까 침대에서 자는데 햇빛이 바로 내리꽂히는데서 자고 있더라구요",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045460.1.3",
          "form": "어떻게 하세요",
          "original_form": "어떻게 하세요",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045460.1.4",
          "form": "ㅋㅋㅋㅋ그건 따뜻해서 그랬을까욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그건 따뜻해서 그랬을까욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045460.1.5",
          "form": "앗 name3 발톱 주기적으로 깎아주고 있어요!ㅋㅋㅋ",
          "original_form": "앗 &name3& 발톱 주기적으로 깎아주고 있어요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045460.1.6",
          "form": "모르겠어욬ㅋㅋㅋ 너무 얼굴이어가지고 괜찮나 싶긴 했는데 너무 잘자고 있어서 ㅋㅋㅋㅋ",
          "original_form": "모르겠어욬ㅋㅋㅋ 너무 얼굴이어가지고 괜찮나 싶긴 했는데 너무 잘자고 있어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045460.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ잘자면 좋은거겠거니 해야겤ㅅ네요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ잘자면 좋은거겠거니 해야겤ㅅ네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045460.1.8",
          "form": "발톱 깎을때 가만히 있어요?",
          "original_form": "발톱 깎을때 가만히 있어요?",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045460.1.9",
          "form": "아뇨 절대 가만히 안있어욬ㅋㅋㅋㅋ",
          "original_form": "아뇨 절대 가만히 안있어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045460.1.10",
          "form": "안겨잇는것도 너무 싫어해서ㅜㅜ",
          "original_form": "안겨잇는것도 너무 싫어해서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045460.1.11",
          "form": "그래서 한번에 네 발 다 못 깎고 거의 앞발/뒷발 나눠서 깎아요 ㅋㅋㅋ",
          "original_form": "그래서 한번에 네 발 다 못 깎고 거의 앞발/뒷발 나눠서 깎아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045460.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ발 두개까지는 참아요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ발 두개까지는 참아요?",
          "speaker_id": "1",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045460.1.13",
          "form": "아앗.. name3 잠에서 깨더니 제 방에서 나가버렸어요...",
          "original_form": "아앗.. &name3& 잠에서 깨더니 제 방에서 나가버렸어요...",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045460.1.14",
          "form": "ㅜㅜㅜ",
          "original_form": "ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045460.1.15",
          "form": "참아주면 발 하나까지는 참아주는 편이에욬ㅋㅋㅋㅋ",
          "original_form": "참아주면 발 하나까지는 참아주는 편이에욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045460.1.16",
          "form": "어르고 달래서 두발까지 깎고 간식으로 달래주는 편이에욬ㅋㅋㅋ",
          "original_form": "어르고 달래서 두발까지 깎고 간식으로 달래주는 편이에욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045460.1.17",
          "form": "ㅠㅠㅠㅠ 주인이 계속 안자니까 화난걸까요ㅠㅠ 아님 원래 같이 안자나용?",
          "original_form": "ㅠㅠㅠㅠ 주인이 계속 안자니까 화난걸까요ㅠㅠ 아님 원래 같이 안자나용?",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045460.1.18",
          "form": "보니까 거실 맨바닥에 찰푸닥 눕는게 제 방이 더웠을까요...",
          "original_form": "보니까 거실 맨바닥에 찰푸닥 눕는게 제 방이 더웠을까요...",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045460.1.19",
          "form": "name3 맨날 사람하고 같이 자요..",
          "original_form": "&name3& 맨날 사람하고 같이 자요..",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045460.1.20",
          "form": "거의 반반인데 제 침대 아니면 안방 침대에서 자거든요 ㅋㅋㅋ",
          "original_form": "거의 반반인데 제 침대 아니면 안방 침대에서 자거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045460.1.21",
          "form": "ㅋㅋㅋㅋ name2님도 좋고 다른사람도 좋고 그런가봐요",
          "original_form": "ㅋㅋㅋㅋ &name2&님도 좋고 다른사람도 좋고 그런가봐요",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045460.1.22",
          "form": "name3 뷰르면 와요?",
          "original_form": "&name3& 뷰르면 와요?",
          "speaker_id": "1",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045460.1.23",
          "form": "대체적으로 안오는 편이에욬ㅋㅋㅋㅋ",
          "original_form": "대체적으로 안오는 편이에욬ㅋㅋㅋㅋ",
