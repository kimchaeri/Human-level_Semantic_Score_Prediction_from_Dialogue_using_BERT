      "utterance": [
        {
          "id": "MDRW2100038742.1.1",
          "form": "까눌레는 좀 단단 쫄깃한 빵이에요",
          "original_form": "까눌레는 좀 단단 쫄깃한 빵이에요",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100038742.1.2",
          "form": "어케만드는지는 제가 거까진 레시피책을 안봐서 모르고요",
          "original_form": "어케만드는지는 제가 거까진 레시피책을 안봐서 모르고요",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100038742.1.3",
          "form": "이런...ㅠㅠ",
          "original_form": "이런...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100038742.1.4",
          "form": "전 단 게 좋아요~~~",
          "original_form": "전 단 게 좋아요~~~",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100038742.1.5",
          "form": "컬리서 오뗄두스로 단 빵이 있는지 찾아보겠습니다...",
          "original_form": "컬리서 오뗄두스로 단 빵이 있는지 찾아보겠습니다...",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100038742.1.6",
          "form": "달다구요",
          "original_form": "달다구요",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100038742.1.7",
          "form": "까눌레는 달다고욬ㅋㅋ",
          "original_form": "까눌레는 달다고욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100038742.1.8",
          "form": "다만 식감이 좀 질겅찔깃?",
          "original_form": "다만 식감이 좀 질겅찔깃?",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100038742.1.9",
          "form": "한번드셔보세요 하나만 시키김 되자나엽",
          "original_form": "한번드셔보세요 하나만 시키김 되자나엽",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100038742.1.10",
          "form": "까눌레 먹어볼게요",
          "original_form": "까눌레 먹어볼게요",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100038742.1.11",
          "form": "ㅋㅋㅋㅋㅋ name1님의 추천을 믿고!",
          "original_form": "ㅋㅋㅋㅋㅋ &name1&님의 추천을 믿고!",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038742.1.12",
          "form": "아 맞다 아까 애플덤플링이랑 아이스크림이 기억난다고 하셨는데",
          "original_form": "아 맞다 아까 애플덤플링이랑 아이스크림이 기억난다고 하셨는데",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038742.1.13",
          "form": "거기서 파는거 하나씩 다 시켜보세요 ㅋㅋ",
          "original_form": "거기서 파는거 하나씩 다 시켜보세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038742.1.14",
          "form": "아이스크림도 카페서 먹은 건가요?",
          "original_form": "아이스크림도 카페서 먹은 건가요?",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038742.1.15",
          "form": "위에 얹어주는거에요 원래",
          "original_form": "위에 얹어주는거에요 원래",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038742.1.16",
          "form": "계피사과류 디저트에",
          "original_form": "계피사과류 디저트에",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038742.1.17",
          "form": "아이스크림 많이얹어요",
          "original_form": "아이스크림 많이얹어요",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038742.1.18",
          "form": "아하ㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038742.1.19",
          "form": "애플덤플링 먹고 싶읍니다...",
          "original_form": "애플덤플링 먹고 싶읍니다...",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038742.1.20",
          "form": "맛있겠다...",
          "original_form": "맛있겠다...",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038742.1.21",
          "form": "애플 크럼블이요!",
          "original_form": "애플 크럼블이요!",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038742.1.22",
          "form": "덤플링 ㄴㄴ 크럼블입니다",
          "original_form": "덤플링 ㄴㄴ 크럼블입니다",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038742.1.23",
          "form": "아 맞다 크럼블",
          "original_form": "아 맞다 크럼블",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038742.1.24",
          "form": "낮에 읽던 소설 작가이름이 파인애플덤플링이라서 헷갈렸나봐요",
          "original_form": "낮에 읽던 소설 작가이름이 파인애플덤플링이라서 헷갈렸나봐요",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038742.1.25",
          "form": "만두요?",
          "original_form": "만두요?",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038742.1.26",
          "form": "파인애플 만두라니..",
          "original_form": "파인애플 만두라니..",
          "speaker_id": "1",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100038742.1.27",
          "form": "ㅋㅋㅋㅋㅋ 이름을 그렇게 지으셨더라고욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 이름을 그렇게 지으셨더라고욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038742.1.28",
          "form": "나중에 리디에서 검색해보세요.",
          "original_form": "나중에 리디에서 검색해보세요.",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038742.1.29",
          "form": "지금 그분 신간 1권 대여이벤트 하고 있거든요 하핳",
          "original_form": "지금 그분 신간 1권 대여이벤트 하고 있거든요 하핳",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038742.1.30",
          "form": "근데 파인애플은 피자에도 올려 먹잖아요",
          "original_form": "근데 파인애플은 피자에도 올려 먹잖아요",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038742.1.31",
          "form": "만두가 있어도 크게 놀랍진 않네여...",
          "original_form": "만두가 있어도 크게 놀랍진 않네여...",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038742.1.32",
          "form": "음..좀 질감이 별롤것 같은데요 피자는 맛있지만.",
          "original_form": "음..좀 질감이 별롤것 같은데요 피자는 맛있지만.",
