      "utterance": [
        {
          "id": "MDRW2100039215.1.1",
          "form": "약간 곤란해요 못생긴 남자는..",
          "original_form": "약간 곤란해요 못생긴 남자는..",
          "speaker_id": "2",
          "time": "20210903 14:03"
        },
        {
          "id": "MDRW2100039215.1.2",
          "form": "멍게는 무슨 맛이져?",
          "original_form": "멍게는 무슨 맛이져?",
          "speaker_id": "2",
          "time": "20210903 14:03"
        },
        {
          "id": "MDRW2100039215.1.3",
          "form": "ㅎㅎ 천상의 맛입니다",
          "original_form": "ㅎㅎ 천상의 맛입니다",
          "speaker_id": "1",
          "time": "20210903 14:03"
        },
        {
          "id": "MDRW2100039215.1.4",
          "form": "근데 오배송돼서 다시 기다리는 중이에요",
          "original_form": "근데 오배송돼서 다시 기다리는 중이에요",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039215.1.5",
          "form": "배민1 라이더 연락처도 안나와서 오배송된거 즉각 알아도 시정 어려움ㅜ",
          "original_form": "배민1 라이더 연락처도 안나와서 오배송된거 즉각 알아도 시정 어려움ㅜ",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100039215.1.6",
          "form": "어머나...",
          "original_form": "어머나...",
          "speaker_id": "2",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100039215.1.7",
          "form": "서울 사시는군요",
          "original_form": "서울 사시는군요",
          "speaker_id": "2",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100039215.1.8",
          "form": "배민1이라니...",
          "original_form": "배민1이라니...",
          "speaker_id": "2",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100039215.1.9",
          "form": "배민1 전국되는 거 아닌가요?",
          "original_form": "배민1 전국되는 거 아닌가요?",
          "speaker_id": "1",
          "time": "20210903 14:05"
        },
        {
          "id": "MDRW2100039215.1.10",
          "form": "안되더라구요...?",
          "original_form": "안되더라구요...?",
          "speaker_id": "2",
          "time": "20210903 14:06"
        },
        {
          "id": "MDRW2100039215.1.11",
          "form": "그지같은 배민1 ㅜ 담부터ㄴ 그냥 일반 시킬거예요",
          "original_form": "그지같은 배민1 ㅜ 담부터ㄴ 그냥 일반 시킬거예요",
          "speaker_id": "1",
          "time": "20210903 14:06"
        },
        {
          "id": "MDRW2100039215.1.12",
          "form": "무슨 쿠폰뿌린다고 해서 호다닥 들어갔는데",
          "original_form": "무슨 쿠폰뿌린다고 해서 호다닥 들어갔는데",
          "speaker_id": "2",
          "time": "20210903 14:06"
        },
        {
          "id": "MDRW2100039215.1.13",
          "form": "안떠서 빈정상했어여",
          "original_form": "안떠서 빈정상했어여",
          "speaker_id": "2",
          "time": "20210903 14:06"
        },
        {
          "id": "MDRW2100039215.1.14",
          "form": "시간 큰 차이 안나요ㅎㅎ",
          "original_form": "시간 큰 차이 안나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 14:06"
        },
        {
          "id": "MDRW2100039215.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:06"
        },
        {
          "id": "MDRW2100039215.1.16",
          "form": "그럼 배고프시겠어여..",
          "original_form": "그럼 배고프시겠어여..",
          "speaker_id": "2",
          "time": "20210903 14:06"
        },
        {
          "id": "MDRW2100039215.1.17",
          "form": "ㅎㅅㅎ 호옥시 이 사달을 안 가게서 서비스라도 뭐 주지 않을까 기대하고 있슴다!",
          "original_form": "ㅎㅅㅎ 호옥시 이 사달을 안 가게서 서비스라도 뭐 주지 않을까 기대하고 있슴다!",
          "speaker_id": "1",
          "time": "20210903 14:07"
        },
        {
          "id": "MDRW2100039215.1.18",
          "form": "님은 뭐 드셨나요",
          "original_form": "님은 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210903 14:08"
        },
        {
          "id": "MDRW2100039215.1.19",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100039215.1.20",
          "form": "호두타르트랑 에그타르트 먹었어욜!",
          "original_form": "호두타르트랑 에그타르트 먹었어욜!",
          "speaker_id": "2",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100039215.1.21",
          "form": "후후 배부르니까 잠이 오는군요..",
          "original_form": "후후 배부르니까 잠이 오는군요..",
          "speaker_id": "2",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100039215.1.22",
          "form": "훗 이건 기성품이겠쥬!",
          "original_form": "훗 이건 기성품이겠쥬!",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100039215.1.23",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100039215.1.24",
          "form": "이것까지 직접 만든건 아닐테니",
          "original_form": "이것까지 직접 만든건 아닐테니",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100039215.1.25",
          "form": "넹?",
          "original_form": "넹?",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100039215.1.26",
          "form": "제빵왕 띠로링입니다",
          "original_form": "제빵왕 띠로링입니다",
          "speaker_id": "2",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100039215.1.27",
          "form": "뻥이에요 기성품입니다",
          "original_form": "뻥이에요 기성품입니다",
          "speaker_id": "2",
          "time": "20210903 14:11"
        }
      ]
    }
  ]
