      "utterance": [
        {
          "id": "MDRW2100047704.1.1",
          "form": "오늘 엄청 쌀쌀하네요",
          "original_form": "오늘 엄청 쌀쌀하네요",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100047704.1.2",
          "form": "네 문을 살짝만 열렀어요 다 안열고",
          "original_form": "네 문을 살짝만 열렀어요 다 안열고",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100047704.1.3",
          "form": "쌀쌀해요",
          "original_form": "쌀쌀해요",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100047704.1.4",
          "form": "ㅋㅋㅋㅋ긴팔 입어도 쌀쌀할 지경..",
          "original_form": "ㅋㅋㅋㅋ긴팔 입어도 쌀쌀할 지경..",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100047704.1.5",
          "form": "로 긴팔 입으셨어요?",
          "original_form": "로 긴팔 입으셨어요?",
          "speaker_id": "1",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100047704.1.6",
          "form": "전 그래도 아직 반팔요",
          "original_form": "전 그래도 아직 반팔요",
          "speaker_id": "1",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100047704.1.7",
          "form": "주변인들의 반대를 물리치고ㅋㅋㅋㅋㅋ",
          "original_form": "주변인들의 반대를 물리치고ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.8",
          "form": "바람이 찬데 좀 습기로 찜찜해서 반팔입어여",
          "original_form": "바람이 찬데 좀 습기로 찜찜해서 반팔입어여",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.9",
          "form": "ㅋㅋ반대하죠",
          "original_form": "ㅋㅋ반대하죠",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.10",
          "form": "너무 추운 나머지ㅋㅋㅋ반팔위에 긴팔 겹쳤어요ㅋㅋㅋ",
          "original_form": "너무 추운 나머지ㅋㅋㅋ반팔위에 긴팔 겹쳤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.11",
          "form": "추위 엄청 타시네요",
          "original_form": "추위 엄청 타시네요",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.12",
          "form": "네네 입자마자 더울거라고 ㅋㅋㅋ",
          "original_form": "네네 입자마자 더울거라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.13",
          "form": "땀도 별로 없으신가보다",
          "original_form": "땀도 별로 없으신가보다",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.14",
          "form": "하지만 입었는데도 추운걸요...?",
          "original_form": "하지만 입었는데도 추운걸요...?",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.15",
          "form": "그럼요 ㅋㅋ",
          "original_form": "그럼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.17",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100047704.1.18",
          "form": "저는ㅋㅋㅋㅋㅋ추위엄청타욬ㅋㅋㅋㅋ",
          "original_form": "저는ㅋㅋㅋㅋㅋ추위엄청타욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100047704.1.19",
          "form": "진짜 체질이 다른가봐요",
          "original_form": "진짜 체질이 다른가봐요",
          "speaker_id": "1",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100047704.1.20",
          "form": "오늘 간기 조심하세요",
          "original_form": "오늘 간기 조심하세요",
          "speaker_id": "1",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100047704.1.21",
          "form": "감기",
          "original_form": "감기",
          "speaker_id": "1",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100047704.1.22",
          "form": "혈액순환이 잘 안되서 추운거 아닐까",
          "original_form": "혈액순환이 잘 안되서 추운거 아닐까",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100047704.1.23",
          "form": "수족냉증인게 아닐까",
          "original_form": "수족냉증인게 아닐까",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100047704.1.24",
          "form": "그건 몸이 안좋을수 있는 신호죠",
          "original_form": "그건 몸이 안좋을수 있는 신호죠",
          "speaker_id": "1",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100047704.1.25",
          "form": "이렇게 생각하고 있지요",
          "original_form": "이렇게 생각하고 있지요",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100047704.1.26",
          "form": "그런거 같아요",
          "original_form": "그런거 같아요",
          "speaker_id": "1",
          "time": "20210827 15:35"
        }
      ]
    }
  ]
