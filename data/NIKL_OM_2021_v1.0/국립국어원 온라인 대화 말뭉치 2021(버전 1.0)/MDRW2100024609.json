      "utterance": [
        {
          "id": "MDRW2100024609.1.1",
          "form": "안녕하세요 오늘은 비가많이오네요",
          "original_form": "안녕하세요 오늘은 비가많이오네요",
          "speaker_id": "1",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100024609.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100024609.1.3",
          "form": "진짜 너무많이와요~ㅠㅠ",
          "original_form": "진짜 너무많이와요~ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100024609.1.4",
          "form": "쌀쌀하기도 하고요ㅠ",
          "original_form": "쌀쌀하기도 하고요ㅠ",
          "speaker_id": "1",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100024609.1.5",
          "form": "비도 많이오는데 에어컨까지 트니까 너무 춥네요",
          "original_form": "비도 많이오는데 에어컨까지 트니까 너무 춥네요",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024609.1.6",
          "form": "저도요ㅠ 얇은겉옷 챙길걸 그랬나봐요",
          "original_form": "저도요ㅠ 얇은겉옷 챙길걸 그랬나봐요",
          "speaker_id": "1",
          "time": "20210831 15:39"
        },
        {
          "id": "MDRW2100024609.1.7",
          "form": "사무실에 겉옷하나 둬야할 것 같아요",
          "original_form": "사무실에 겉옷하나 둬야할 것 같아요",
          "speaker_id": "2",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100024609.1.8",
          "form": "진짜 가디건필수가 됐어요",
          "original_form": "진짜 가디건필수가 됐어요",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024609.1.9",
          "form": "비가 그칠 생각을 안하네요",
          "original_form": "비가 그칠 생각을 안하네요",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100024609.1.10",
          "form": "퇴근길 조심하세요ㅠㅠ",
          "original_form": "퇴근길 조심하세요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024609.1.11",
          "form": "저는 학생이라 괜찮아요:)",
          "original_form": "저는 학생이라 괜찮아요:)",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024609.1.12",
          "form": "비가 얼른 멈췄으면 좋겠어요ㅠ",
          "original_form": "비가 얼른 멈췄으면 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024609.1.13",
          "form": "앗 학생이시구나",
          "original_form": "앗 학생이시구나",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024609.1.14",
          "form": "대신 과제가ㅋㅋ큐ㅠㅜ",
          "original_form": "대신 과제가ㅋㅋ큐ㅠㅜ",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024609.1.15",
          "form": "내일부터 개강이네요...",
          "original_form": "내일부터 개강이네요...",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024609.1.16",
          "form": "과제지옥...",
          "original_form": "과제지옥...",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024609.1.17",
          "form": "날씨가 저희 마음을 대변하나봐요ㅠㅋㅋ",
