        {
          "id": "MDRW2100004564.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004564.1.2",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004564.1.3",
          "form": "운동은 산책 정도만 해요!",
          "original_form": "운동은 산책 정도만 해요!",
          "speaker_id": "2",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004564.1.4",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004564.1.5",
          "form": "홈트해요",
          "original_form": "홈트해요",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004564.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004564.1.7",
          "form": "자기관리 잘 하시네요",
          "original_form": "자기관리 잘 하시네요",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004564.1.8",
          "form": "살이 너무 쪄서요",
          "original_form": "살이 너무 쪄서요",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004564.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004564.1.10",
          "form": "저는 요새 식단 관리하고 있어요",
          "original_form": "저는 요새 식단 관리하고 있어요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004564.1.11",
          "form": "적응 되면 홈트 시작하려고요",
          "original_form": "적응 되면 홈트 시작하려고요",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004564.1.12",
          "form": "유투브봐요",
          "original_form": "유투브봐요",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004564.1.13",
          "form": "유튜브 보면서 해야죠~",
          "original_form": "유튜브 보면서 해야죠~",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004564.1.14",
          "form": "그래야 잼나죠",
          "original_form": "그래야 잼나죠",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004564.1.15",
          "form": "헉..재밌다니..",
          "original_form": "헉..재밌다니..",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004564.1.16",
          "form": "전 운동하려고 하면 한숨이 먼저 나오는데...",
          "original_form": "전 운동하려고 하면 한숨이 먼저 나오는데...",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004564.1.17",
          "form": "재밌다고 하시다니.. 운동을 좋아하시는 군요",
          "original_form": "재밌다고 하시다니.. 운동을 좋아하시는 군요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004564.1.18",
          "form": "어쩔수없이 하죠 ㅋㅋ",
          "original_form": "어쩔수없이 하죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004564.1.19",
          "form": "그래도 대단하시네요",
          "original_form": "그래도 대단하시네요",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004564.1.20",
          "form": "'재밌다'는 흥미를 갖고 계신거잖아요!",
          "original_form": "'재밌다'는 흥미를 갖고 계신거잖아요!",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004564.1.21",
          "form": "건강위해서",
