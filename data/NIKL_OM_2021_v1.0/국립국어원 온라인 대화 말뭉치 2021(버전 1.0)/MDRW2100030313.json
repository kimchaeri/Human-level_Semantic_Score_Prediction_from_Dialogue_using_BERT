      "utterance": [
        {
          "id": "MDRW2100030313.1.1",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030313.1.2",
          "form": "하는건 힘들어도.. 막상하면 진짜 도움되더라",
          "original_form": "하는건 힘들어도.. 막상하면 진짜 도움되더라",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030313.1.3",
          "form": "뭔가... 폐활량도 조금 더 늘어나고",
          "original_form": "뭔가... 폐활량도 조금 더 늘어나고",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.4",
          "form": "어떤식으루??",
          "original_form": "어떤식으루??",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.5",
          "form": "땀을 덜흘리게 돼",
          "original_form": "땀을 덜흘리게 돼",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.6",
          "form": "그건 다이어트가 아니라 그냥 운동 아녀?",
          "original_form": "그건 다이어트가 아니라 그냥 운동 아녀?",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.7",
          "form": "아니 그게 아니라 살을 빼고 나면",
          "original_form": "아니 그게 아니라 살을 빼고 나면",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.8",
          "form": "그렇더라구",
          "original_form": "그렇더라구",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.9",
          "form": "그렇군 ㅎㅎ",
          "original_form": "그렇군 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.10",
          "form": "그래도 다이어트도 건강한 방법으로 해야 좋은 것 겉어",
          "original_form": "그래도 다이어트도 건강한 방법으로 해야 좋은 것 겉어",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.11",
          "form": "맞아 지나친 다이어트는 좋지않어",
          "original_form": "맞아 지나친 다이어트는 좋지않어",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.12",
          "form": "굶으려고 하지 말구",
          "original_form": "굶으려고 하지 말구",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.13",
          "form": "골고루먹구 운동으로 빼야대",
          "original_form": "골고루먹구 운동으로 빼야대",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.14",
          "form": "근데 굶는건 원래 몸에 좋은게 맞아",
          "original_form": "근데 굶는건 원래 몸에 좋은게 맞아",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030313.1.15",
          "form": "단식 수행같은거도있잖아",
          "original_form": "단식 수행같은거도있잖아",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030313.1.16",
          "form": "그렇진 않어",
          "original_form": "그렇진 않어",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030313.1.17",
          "form": "너가 너무 많이 먹어서 그런거야",
          "original_form": "너가 너무 많이 먹어서 그런거야",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030313.1.18",
          "form": "외국 무슨 나라에서 1일씩 굶는거",
          "original_form": "외국 무슨 나라에서 1일씩 굶는거",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030313.1.19",
          "form": "단식수행이 몸에좋으려구하는거냐구~!!!!!",
          "original_form": "단식수행이 몸에좋으려구하는거냐구~!!!!!",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030313.1.20",
          "form": "그거 몸에 좋은거도있대.",
          "original_form": "그거 몸에 좋은거도있대.",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030313.1.21",
          "form": "그리고 적당히 굶는건 오히려 몸에좋아",
          "original_form": "그리고 적당히 굶는건 오히려 몸에좋아",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030313.1.22",
          "form": "먹는 양을 줄이는게 중요해 다이어트는",
          "original_form": "먹는 양을 줄이는게 중요해 다이어트는",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030313.1.23",
          "form": "그대로먹으면서 운동해서 살뺄생각은 하면 안돼!!",
          "original_form": "그대로먹으면서 운동해서 살뺄생각은 하면 안돼!!",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030313.1.24",
          "form": "자기한테 맞는 양만큼 먹는게 중요하지!",
          "original_form": "자기한테 맞는 양만큼 먹는게 중요하지!",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030313.1.25",
          "form": "아니 조금 먹는 양을 줄여야한다고 생각해 난 ㅎㅎ",
          "original_form": "아니 조금 먹는 양을 줄여야한다고 생각해 난 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030313.1.26",
          "form": "그렇구나 ㅎㅎ",
          "original_form": "그렇구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:44"
        }
      ]
    }
