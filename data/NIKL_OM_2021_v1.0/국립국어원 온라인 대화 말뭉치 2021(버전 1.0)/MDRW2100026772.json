      "utterance": [
        {
          "id": "MDRW2100026772.1.1",
          "form": "사람들이 다이어트 하는데 무슨 소스를 먹냐",
          "original_form": "사람들이 다이어트 하는데 무슨 소스를 먹냐",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026772.1.2",
          "form": "응 치즈까지 올려 먹더라",
          "original_form": "응 치즈까지 올려 먹더라",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026772.1.3",
          "form": "막 감치는 먹으면 안된다 이러는데",
          "original_form": "막 감치는 먹으면 안된다 이러는데",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026772.1.4",
          "form": "맛있겟다",
          "original_form": "맛있겟다",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026772.1.5",
          "form": "우리 평소 먹는걸 생각해 보라고",
          "original_form": "우리 평소 먹는걸 생각해 보라고",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026772.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026772.1.7",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026772.1.8",
          "form": "떡볶이나 마라탕 그런거 기본으로 먹고 후식으로",
          "original_form": "떡볶이나 마라탕 그런거 기본으로 먹고 후식으로",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026772.1.9",
          "form": "단 음료나 그런거까지 먹는거하고 비교했을때",
          "original_form": "단 음료나 그런거까지 먹는거하고 비교했을때",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026772.1.10",
          "form": "이정도는 괜찮다는 말 듣고",
          "original_form": "이정도는 괜찮다는 말 듣고",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026772.1.11",
          "form": "좀 충격이였음",
          "original_form": "좀 충격이였음",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026772.1.12",
          "form": "그렇긴하지",
          "original_form": "그렇긴하지",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026772.1.13",
          "form": "사람들이 다이어트 시작하면 너무 빡빡해져서",
          "original_form": "사람들이 다이어트 시작하면 너무 빡빡해져서",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026772.1.14",
          "form": "다이어트라고해서 무조건 풀떼기만 먹는거보다",
          "original_form": "다이어트라고해서 무조건 풀떼기만 먹는거보다",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026772.1.15",
          "form": "오히려 더 못 버틴다고",
          "original_form": "오히려 더 못 버틴다고",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026772.1.16",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026772.1.17",
          "form": "응ㅇ응",
          "original_form": "응ㅇ응",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026772.1.18",
          "form": "그 광고에서",
          "original_form": "그 광고에서",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026772.1.19",
          "form": "다이어트 보조제 이런거 많이 나오잖아",
          "original_form": "다이어트 보조제 이런거 많이 나오잖아",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026772.1.20",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026772.1.21",
          "form": "그런거 나올때 마다",
          "original_form": "그런거 나올때 마다",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026772.1.22",
          "form": "좀 사보고 싶었거든",
          "original_form": "좀 사보고 싶었거든",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026772.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ쥬비슨가 쥬디슨가?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ쥬비슨가 쥬디슨가?",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026772.1.24",
          "form": "나도 해보고싶엇음",
          "original_form": "나도 해보고싶엇음",
          "speaker_id": "1",
          "time": "20210902 00:02"
        }
