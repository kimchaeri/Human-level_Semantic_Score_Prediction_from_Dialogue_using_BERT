      "utterance": [
        {
          "id": "MDRW2100027559.1.1",
          "form": "요즘 못놀러간지 한참 됏지",
          "original_form": "요즘 못놀러간지 한참 됏지",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027559.1.2",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.3",
          "form": "코로나 때문이잖아",
          "original_form": "코로나 때문이잖아",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.4",
          "form": "코로나 풀리면 어디로 놀러가구싶어?",
          "original_form": "코로나 풀리면 어디로 놀러가구싶어?",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.5",
          "form": "내가 운전면허 따면",
          "original_form": "내가 운전면허 따면",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.6",
          "form": "다 먹으러 가좌",
          "original_form": "다 먹으러 가좌",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.7",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.8",
          "form": "돈까쓰맨날먹어!!!!!!",
          "original_form": "돈까쓰맨날먹어!!!!!!",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.9",
          "form": "파스타맨날먹어!!!!!!",
          "original_form": "파스타맨날먹어!!!!!!",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.10",
          "form": "돈까쓰는 여행이랑 상관없잖아",
          "original_form": "돈까쓰는 여행이랑 상관없잖아",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.11",
          "form": "이탈리아갈꺼야?",
          "original_form": "이탈리아갈꺼야?",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.12",
          "form": "일본 돈까쓰 이탈리아 스파게티?",
          "original_form": "일본 돈까쓰 이탈리아 스파게티?",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027559.1.13",
          "form": "돈까쓰 맛집 찾아서 여행가는거지",
          "original_form": "돈까쓰 맛집 찾아서 여행가는거지",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.14",
          "form": "우리나라도 돈까스 맛집이",
          "original_form": "우리나라도 돈까스 맛집이",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.15",
          "form": "얼마나 많은데",
          "original_form": "얼마나 많은데",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.16",
          "form": "괜찮네",
          "original_form": "괜찮네",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.17",
          "form": "난 제주도 돈까쓰 먹고싶어",
          "original_form": "난 제주도 돈까쓰 먹고싶어",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.18",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.19",
          "form": "제주 흑돼지돈까쓰",
          "original_form": "제주 흑돼지돈까쓰",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.20",
          "form": "맛있을거 같지않아?",
          "original_form": "맛있을거 같지않아?",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.21",
          "form": "제주도에 가서",
          "original_form": "제주도에 가서",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.22",
          "form": "새벽에 바다를 보고",
          "original_form": "새벽에 바다를 보고",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.23",
          "form": "아침밥으로 흑돼지 돈까쓰를 먹는거야",
          "original_form": "아침밥으로 흑돼지 돈까쓰를 먹는거야",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027559.1.24",
          "form": "맛잇겟당.....",
          "original_form": "맛잇겟당.....",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.25",
          "form": "하지만 맛집은",
          "original_form": "하지만 맛집은",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.26",
          "form": "그만큼 줄을 오래 서야 돼....",
          "original_form": "그만큼 줄을 오래 서야 돼....",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.27",
          "form": "그렇긴 하지만",
          "original_form": "그렇긴 하지만",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.28",
          "form": "그것도 다 여행의 재미지 ㅎㅎ",
          "original_form": "그것도 다 여행의 재미지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.29",
          "form": "춘천에 닭갈비먹으러 갓을 때 기억나?",
          "original_form": "춘천에 닭갈비먹으러 갓을 때 기억나?",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.30",
          "form": "가는 닭갈비집마다",
          "original_form": "가는 닭갈비집마다",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.31",
          "form": "사람이 엄청 많아서",
          "original_form": "사람이 엄청 많아서",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.32",
          "form": "결국 구석진데 가서 먹엇잖아...",
          "original_form": "결국 구석진데 가서 먹엇잖아...",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.33",
          "form": "맞아 기억나 ㅎㅎ",
          "original_form": "맞아 기억나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.34",
          "form": "하지만 맛있었어",
          "original_form": "하지만 맛있었어",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027559.1.35",
          "form": "기차타고 갔었잖아",
          "original_form": "기차타고 갔었잖아",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.36",
          "form": "근데 돌아올때도 너무 신났어",
          "original_form": "근데 돌아올때도 너무 신났어",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.37",
          "form": "난 여행가서 제일 맛있던게",
          "original_form": "난 여행가서 제일 맛있던게",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.38",
          "form": "기차 타는 거 재밋지",
          "original_form": "기차 타는 거 재밋지",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.39",
          "form": "제주도 숯불흑돼지었어",
          "original_form": "제주도 숯불흑돼지었어",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.40",
          "form": "헐!!!",
          "original_form": "헐!!!",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.41",
          "form": "숯불흑돼지 맛있지",
          "original_form": "숯불흑돼지 맛있지",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.42",
          "form": "난 흑돼지 못먹겟던데",
          "original_form": "난 흑돼지 못먹겟던데",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.43",
          "form": "그래도 숯불고기는 맛잏지",
          "original_form": "그래도 숯불고기는 맛잏지",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.44",
          "form": "언니 좋아하는 돈까스집이",
          "original_form": "언니 좋아하는 돈까스집이",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.45",
          "form": "흑돼지돈까쓰인데?",
          "original_form": "흑돼지돈까쓰인데?",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027559.1.46",
          "form": "튀긴거랑 구운거랑 달라 ㅎㅎ",
          "original_form": "튀긴거랑 구운거랑 달라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027559.1.47",
          "form": "그래 알겠어 ㅎㅎ",
          "original_form": "그래 알겠어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027559.1.48",
          "form": "아무튼 꼭 여행을가자 ㅎㅎ",
          "original_form": "아무튼 꼭 여행을가자 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027559.1.49",
          "form": "흑돼지먹으러 ㅎㅎ",
          "original_form": "흑돼지먹으러 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:37"
        }
      ]
