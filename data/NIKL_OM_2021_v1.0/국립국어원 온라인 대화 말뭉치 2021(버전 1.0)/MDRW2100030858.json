        },
        {
          "id": "MDRW2100030858.1.2",
          "form": "당신 뀸이 뭔가요",
          "original_form": "당신 뀸이 뭔가요",
          "speaker_id": "1",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.3",
          "form": "넹넹 말씀하세욘",
          "original_form": "넹넹 말씀하세욘",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.4",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.5",
          "form": "저는 아직 프로작곡가가 꿈이긴 합니다",
          "original_form": "저는 아직 프로작곡가가 꿈이긴 합니다",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.6",
          "form": "미디 작곡가",
          "original_form": "미디 작곡가",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.7",
          "form": "아님 취직하고 싶은 히사라돈가",
          "original_form": "아님 취직하고 싶은 히사라돈가",
          "speaker_id": "1",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.8",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.9",
          "form": "대학가면 음악 다시 시작할 듯",
          "original_form": "대학가면 음악 다시 시작할 듯",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.10",
          "form": "이번엔 제대로",
          "original_form": "이번엔 제대로",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.11",
          "form": "작고가 되면 그 뭐지 소속사에 들어갈건가요",
          "original_form": "작고가 되면 그 뭐지 소속사에 들어갈건가요",
          "speaker_id": "1",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100030858.1.12",
          "form": "음...글쎄요",
          "original_form": "음...글쎄요",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.13",
          "form": "음악이라니 멋진듯",
          "original_form": "음악이라니 멋진듯",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.14",
          "form": "너도 멋져",
          "original_form": "너도 멋져",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.15",
          "form": "그림 그리는 예술가~~~",
          "original_form": "그림 그리는 예술가~~~",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.16",
          "form": "뿌이뿌이뿌이",
          "original_form": "뿌이뿌이뿌이",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.17",
          "form": "ㅋㅋㅋ애니메이터가 되어서...사심채우기",
          "original_form": "ㅋㅋㅋ애니메이터가 되어서...사심채우기",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.18",
          "form": "애니메이터가 되고 샆은 이유는",
          "original_form": "애니메이터가 되고 샆은 이유는",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.20",
          "form": "단순 사심채우기 입니다",
          "original_form": "단순 사심채우기 입니다",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.21",
          "form": "알고있죠",
          "original_form": "알고있죠",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.22",
          "form": "내 아이돌을 살아잇눈 아이돌로 그리기",
          "original_form": "내 아이돌을 살아잇눈 아이돌로 그리기",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.23",
          "form": "name1이의 사심쓰",
          "original_form": "&name1&이의 사심쓰",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030858.1.24",
          "form": "눈물이 나네요",
          "original_form": "눈물이 나네요",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030858.1.25",
          "form": "나는 내가 좋아하는 가수에게",
          "original_form": "나는 내가 좋아하는 가수에게",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030858.1.26",
          "form": "노래 주고 싶음",
          "original_form": "노래 주고 싶음",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030858.1.27",
          "form": "완전대박...",
          "original_form": "완전대박...",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030858.1.28",
