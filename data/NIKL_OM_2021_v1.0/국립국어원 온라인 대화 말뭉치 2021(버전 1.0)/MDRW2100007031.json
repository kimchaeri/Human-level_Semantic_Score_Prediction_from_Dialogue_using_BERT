      "utterance": [
        {
          "id": "MDRW2100007031.1.1",
          "form": "포비피엠은 가보셨나요?",
          "original_form": "포비피엠은 가보셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007031.1.2",
          "form": "당연히 가봤죠",
          "original_form": "당연히 가봤죠",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007031.1.3",
          "form": "친구랑 갔다가 정말정말 좋아서",
          "original_form": "친구랑 갔다가 정말정말 좋아서",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007031.1.4",
          "form": "역시 배우신 분이네요",
          "original_form": "역시 배우신 분이네요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007031.1.5",
          "form": "어떤 칵테일 드셨어요?",
          "original_form": "어떤 칵테일 드셨어요?",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007031.1.6",
          "form": "전..알쓰라서 ㅋㅋㅋㅋㅋ",
          "original_form": "전..알쓰라서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007031.1.7",
          "form": "콜라에 레몬 넣은거 마셨어요",
          "original_form": "콜라에 레몬 넣은거 마셨어요",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007031.1.8",
          "form": "이름은 기억 안나네요",
          "original_form": "이름은 기억 안나네요",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007031.1.9",
          "form": "술 잘 하시나요?",
          "original_form": "술 잘 하시나요?",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007031.1.10",
          "form": "저도 알쓰예요ㅋㅋ",
          "original_form": "저도 알쓰예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007031.1.11",
          "form": "그래서 도수 제일 약한 토마토마 먹었는데 반도 못 먹었어요",
          "original_form": "그래서 도수 제일 약한 토마토마 먹었는데 반도 못 먹었어요",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007031.1.12",
          "form": "전 얼그레이 들어간 거 먹고 싶었는데 도수가 너무 높더라고요",
          "original_form": "전 얼그레이 들어간 거 먹고 싶었는데 도수가 너무 높더라고요",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007031.1.13",
          "form": "다음 생에나 먹을 수 있겠죠....",
          "original_form": "다음 생에나 먹을 수 있겠죠....",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007031.1.14",
          "form": "그러겠죠.. 저도 술맛 알고 싶은데 몸이 안 따라 주네요 ㅋㅋㅋ",
          "original_form": "그러겠죠.. 저도 술맛 알고 싶은데 몸이 안 따라 주네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007031.1.15",
          "form": "다른 안주도 먹어보셨나요?",
          "original_form": "다른 안주도 먹어보셨나요?",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007031.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007031.1.17",
          "form": "네! 야채구이랑 곶감치즈말이요",
          "original_form": "네! 야채구이랑 곶감치즈말이요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007031.1.18",
          "form": "얘네 정말 제대로 된 놈들이에요",
          "original_form": "얘네 정말 제대로 된 놈들이에요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007031.1.19",
          "form": "전 저녁먹고 가서 안주는 못 먹었는데",
          "original_form": "전 저녁먹고 가서 안주는 못 먹었는데",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007031.1.20",
          "form": "다음에 가면 꼭 먹어보고 싶어요",
          "original_form": "다음에 가면 꼭 먹어보고 싶어요",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007031.1.21",
          "form": "바로 근처 곱창집에서 엄청 많이 먹고 갔었던 지라 ㅋㅋㅋ",
          "original_form": "바로 근처 곱창집에서 엄청 많이 먹고 갔었던 지라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007031.1.22",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007031.1.23",
          "form": "그 곱창집 맛있다는 얘기 들어봤어요!",
          "original_form": "그 곱창집 맛있다는 얘기 들어봤어요!",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007031.1.24",
          "form": "다들 곱창 먹고 포비피엠 가나봐요",
          "original_form": "다들 곱창 먹고 포비피엠 가나봐요",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007031.1.25",
          "form": "name2 님 저는 자야해서 낮에 다시 얘기해요! 대화 재밌었어요",
          "original_form": "&name2& 님 저는 자야해서 낮에 다시 얘기해요! 대화 재밌었어요",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007031.1.26",
          "form": "네 내일 봐요!!",
          "original_form": "네 내일 봐요!!",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007031.1.27",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 07:54"
        },
        {
          "id": "MDRW2100007031.1.28",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100007031.1.29",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100007031.1.30",
          "form": "늦게 일어나서 아점 먹었어요",
          "original_form": "늦게 일어나서 아점 먹었어요",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100007031.1.31",
          "form": "김밥 해먹으니 간편하고 좋네요 ㅋㅋ",
          "original_form": "김밥 해먹으니 간편하고 좋네요 ㅋㅋ",
