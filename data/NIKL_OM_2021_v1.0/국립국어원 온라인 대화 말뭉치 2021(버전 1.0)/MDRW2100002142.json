      "utterance": [
        {
          "id": "MDRW2100002142.1.1",
          "form": "닌 최근에 산거중애",
          "original_form": "닌 최근에 산거중애",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002142.1.2",
          "form": "뭐가 젤 맘에들어",
          "original_form": "뭐가 젤 맘에들어",
          "speaker_id": "2",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002142.1.3",
          "form": "너 옷걸이는 사지 않아도 되니?",
          "original_form": "너 옷걸이는 사지 않아도 되니?",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002142.1.4",
          "form": "나는 아무래도 플립이 마음에 드는구나.",
          "original_form": "나는 아무래도 플립이 마음에 드는구나.",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002142.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어어 그냥 집에서 가지고올려고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어어 그냥 집에서 가지고올려고",
          "speaker_id": "2",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.6",
          "form": "플립말고 좀 소소한건 없나",
          "original_form": "플립말고 좀 소소한건 없나",
          "speaker_id": "2",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.7",
          "form": "플립은 화요일에 배송이 온다고 하는데, 그 전에 올 일은 없을까?",
          "original_form": "플립은 화요일에 배송이 온다고 하는데, 그 전에 올 일은 없을까?",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.8",
          "form": "조금 소소한 것은 아무래도 오늘 먹은 과자란다.",
          "original_form": "조금 소소한 것은 아무래도 오늘 먹은 과자란다.",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.9",
          "form": "음...플립이 벌떡 일어나서 걸어오지 않는한 없을듯",
          "original_form": "음...플립이 벌떡 일어나서 걸어오지 않는한 없을듯",
          "speaker_id": "2",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ아웃기다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아웃기다",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.11",
          "form": "오늘 과자 뭐먹었어",
          "original_form": "오늘 과자 뭐먹었어",
          "speaker_id": "2",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.12",
          "form": "친구가 기프티콘 줘서 오랜만에 지에스 초코 별 과자 그거 먹었는데",
          "original_form": "친구가 기프티콘 줘서 오랜만에 지에스 초코 별 과자 그거 먹었는데",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.13",
          "form": "얼려먹었는데 맛있더라",
          "original_form": "얼려먹었는데 맛있더라",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.14",
          "form": "근데 끝쪽에 좀 질리긴 했어",
          "original_form": "근데 끝쪽에 좀 질리긴 했어",
          "speaker_id": "1",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.15",
          "form": "아 맛있겠다",
          "original_form": "아 맛있겠다",
          "speaker_id": "2",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002142.1.16",
          "form": "그럼 나 주지 그랬니",
          "original_form": "그럼 나 주지 그랬니",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002142.1.17",
          "form": "그거 먹으니까 프링글스같은 짠과자 사고싶다",
          "original_form": "그거 먹으니까 프링글스같은 짠과자 사고싶다",
          "speaker_id": "1",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002142.1.18",
          "form": "오지 그랬니.",
          "original_form": "오지 그랬니.",
          "speaker_id": "1",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002142.1.19",
          "form": "ㅠㅠ프링글스는 항상 맛있어",
          "original_form": "ㅠㅠ프링글스는 항상 맛있어",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002142.1.20",
          "form": "지금 사러 가고싶다",
          "original_form": "지금 사러 가고싶다",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002142.1.21",
          "form": "그거 말고는 향수들 쫙 사고싶어서 캡쳐해뒀다",
          "original_form": "그거 말고는 향수들 쫙 사고싶어서 캡쳐해뒀다",
          "speaker_id": "1",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002142.1.22",
          "form": "아 니 향수사는거 좋아하니까",
          "original_form": "아 니 향수사는거 좋아하니까",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002142.1.23",
          "form": "맞아 유튜버 포니가 추천을 잘해주더라",
          "original_form": "맞아 유튜버 포니가 추천을 잘해주더라",
          "speaker_id": "1",
          "time": "20210819 23:56"
        }
      ]
