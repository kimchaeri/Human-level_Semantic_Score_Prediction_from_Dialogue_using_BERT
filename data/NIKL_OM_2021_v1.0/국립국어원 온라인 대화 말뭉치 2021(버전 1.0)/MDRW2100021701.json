      "utterance": [
        {
          "id": "MDRW2100021701.1.1",
          "form": "name2님 백신 뭐맞으셨어요??",
          "original_form": "&name2&님 백신 뭐맞으셨어요??",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021701.1.2",
          "form": "저 화이자요! 1차",
          "original_form": "저 화이자요! 1차",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021701.1.3",
          "form": "오 저도 1차 화이자로 목요일에 맞았어요",
          "original_form": "오 저도 1차 화이자로 목요일에 맞았어요",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021701.1.4",
          "form": "헐 저도 목요일",
          "original_form": "헐 저도 목요일",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021701.1.5",
          "form": "오ㅋㅋㅋㅋㅋㅋ 같은 날에 맞았네여",
          "original_form": "오ㅋㅋㅋㅋㅋㅋ 같은 날에 맞았네여",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021701.1.6",
          "form": "저는 당일날 열올랐다 내리고",
          "original_form": "저는 당일날 열올랐다 내리고",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021701.1.7",
          "form": "팔 좀 많이 아픈거 빼면 괜찮았어요 ㅎㅎ",
          "original_form": "팔 좀 많이 아픈거 빼면 괜찮았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021701.1.8",
          "form": "다음날 자고 일어나서 더 아파졌단 후기 봐서 좀 걱정했는데",
          "original_form": "다음날 자고 일어나서 더 아파졌단 후기 봐서 좀 걱정했는데",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021701.1.9",
          "form": "다행히 자고 일어나니까 팔 통증 엄청 줄었더라구요",
          "original_form": "다행히 자고 일어나니까 팔 통증 엄청 줄었더라구요",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021701.1.10",
          "form": "저는 이틀차에만 팔 조금 뻐근했고",
          "original_form": "저는 이틀차에만 팔 조금 뻐근했고",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021701.1.11",
          "form": "나머지는 안 아팠어요",
          "original_form": "나머지는 안 아팠어요",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021701.1.12",
          "form": "잠만 많더라고요",
          "original_form": "잠만 많더라고요",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021701.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021701.1.14",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021701.1.15",
          "form": "맞으면 잠 많아져서 화이잠이라고 부르기도 하더라고요 ㅋㅋㅋ",
          "original_form": "맞으면 잠 많아져서 화이잠이라고 부르기도 하더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021701.1.16",
          "form": "암튼 케바케긴 하지만 백신맞고 아프단 사람들 많아서 걱정했는데",
          "original_form": "암튼 케바케긴 하지만 백신맞고 아프단 사람들 많아서 걱정했는데",
          "speaker_id": "1",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021701.1.17",
          "form": "아직까진 별 이상 없는 것 같아서 넘 다행인 것 같아요",
          "original_form": "아직까진 별 이상 없는 것 같아서 넘 다행인 것 같아요",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021701.1.18",
          "form": "화이잠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "화이잠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021701.1.19",
          "form": "맞아요 1차는 괜찮고 2차가 아프다더라고요ㅠ",
          "original_form": "맞아요 1차는 괜찮고 2차가 아프다더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021701.1.20",
          "form": "아직 많이 남았는데 걱정되네요 ㅋㅋㅋ",
          "original_form": "아직 많이 남았는데 걱정되네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021701.1.21",
          "form": "저도요ㅋㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021701.1.22",
          "form": "그때 타이레놀 다 먹을 거 같아요",
          "original_form": "그때 타이레놀 다 먹을 거 같아요",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021701.1.23",
          "form": "너무 많이 아프지만 않았으면 좋겠네요 ㅋㅋㅋㅋ ㅜ",
          "original_form": "너무 많이 아프지만 않았으면 좋겠네요 ㅋㅋㅋㅋ ㅜ",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021701.1.24",
          "form": "맞아요ㅠ 3일 내내 뭘 하지 못한다던데... 무서워요",
          "original_form": "맞아요ㅠ 3일 내내 뭘 하지 못한다던데... 무서워요",
          "speaker_id": "2",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100021701.1.25",
          "form": "헉 그정도래요? 무섭네요..",
          "original_form": "헉 그정도래요? 무섭네요..",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100021701.1.26",
          "form": "멀쩡한 사람도 있지 않을까....",
          "original_form": "멀쩡한 사람도 있지 않을까....",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100021701.1.27",
          "form": "그리고 그게 저였으면 좋겠네요 ㅋㅋㅋㅋㅋ",
          "original_form": "그리고 그게 저였으면 좋겠네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:06"
        }
      ]
