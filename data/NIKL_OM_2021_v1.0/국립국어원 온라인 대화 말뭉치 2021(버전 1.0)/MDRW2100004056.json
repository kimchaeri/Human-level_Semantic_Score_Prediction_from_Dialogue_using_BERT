        {
          "id": "MDRW2100004056.1.12",
          "form": "일단 장바구니에 담었습니다",
          "original_form": "일단 장바구니에 담었습니다",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004056.1.13",
