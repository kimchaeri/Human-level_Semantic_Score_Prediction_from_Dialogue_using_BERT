      "utterance": [
        {
          "id": "MDRW2100020978.1.1",
          "form": "여행가고싶네요 ㅠㅠ",
          "original_form": "여행가고싶네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020978.1.2",
          "form": "그죠 편안하게 다니고싶어요 ㅜㅠ",
          "original_form": "그죠 편안하게 다니고싶어요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020978.1.3",
          "form": "여행 원래 자주 가셨어요?",
          "original_form": "여행 원래 자주 가셨어요?",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020978.1.4",
          "form": "자주 다니진 않았는데",
          "original_form": "자주 다니진 않았는데",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020978.1.5",
          "form": "집에만 있으니 답답해서 기분 전환하고 싶어서요 ㅜㅜ",
          "original_form": "집에만 있으니 답답해서 기분 전환하고 싶어서요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020978.1.6",
          "form": "맞아요 ㅠㅠ저두 그래요",
          "original_form": "맞아요 ㅠㅠ저두 그래요",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020978.1.7",
          "form": "해외도 자주 못가봤는데",
          "original_form": "해외도 자주 못가봤는데",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020978.1.8",
          "form": "막상 더 못가니 괜히 아쉽도라구요",
          "original_form": "막상 더 못가니 괜히 아쉽도라구요",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020978.1.9",
          "form": "맞죠?",
          "original_form": "맞죠?",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020978.1.10",
          "form": "지금 그래도 해외보단 국내가 더 안전한 느낌이 들어요",
          "original_form": "지금 그래도 해외보단 국내가 더 안전한 느낌이 들어요",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020978.1.11",
          "form": "그래서 사람들이 제주도 여행 가나봐요 ㅋㅋ",
          "original_form": "그래서 사람들이 제주도 여행 가나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020978.1.12",
          "form": "ㅋㅋㅋㅋㅋ맞아요 주변만 봐도 제주도 엄청 가더라구요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 주변만 봐도 제주도 엄청 가더라구요",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100020978.1.13",
          "form": "국내 여행지 중에 어디 가장 좋아하세요?",
          "original_form": "국내 여행지 중에 어디 가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100020978.1.14",
          "form": "국내 여행지 중에서는 아직 제주도가 나은 것 같아요",
          "original_form": "국내 여행지 중에서는 아직 제주도가 나은 것 같아요",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100020978.1.15",
          "form": "비행기도 타니 진짜 여행가는 기분이 나거든요",
          "original_form": "비행기도 타니 진짜 여행가는 기분이 나거든요",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100020978.1.16",
          "form": "맞아요 ㅠㅠ 저도 제주도 좋아해요",
          "original_form": "맞아요 ㅠㅠ 저도 제주도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100020978.1.17",
          "form": "자연풍경 좋아해요",
          "original_form": "자연풍경 좋아해요",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100020978.1.18",
          "form": "제주도는 무슨 계절에 가보셨어요?",
          "original_form": "제주도는 무슨 계절에 가보셨어요?",
