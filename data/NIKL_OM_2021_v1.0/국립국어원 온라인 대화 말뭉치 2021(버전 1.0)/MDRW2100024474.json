      "utterance": [
        {
          "id": "MDRW2100024474.1.1",
          "form": "전공 말고 필수로 들어야하는 과목 있어요?",
          "original_form": "전공 말고 필수로 들어야하는 과목 있어요?",
          "speaker_id": "1",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100024474.1.2",
          "form": "저 학교다닐때는 있었거든요",
          "original_form": "저 학교다닐때는 있었거든요",
          "speaker_id": "1",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100024474.1.3",
          "form": "진로관련해서 교수님이랑 상담과목 몇번 듣기",
          "original_form": "진로관련해서 교수님이랑 상담과목 몇번 듣기",
          "speaker_id": "1",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100024474.1.4",
          "form": "아 있어요",
          "original_form": "아 있어요",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100024474.1.5",
          "form": "저희는 인성과목이라고",
          "original_form": "저희는 인성과목이라고",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100024474.1.6",
          "form": "와 첨들어요ㅋㅋㅋㅋㅋ",
          "original_form": "와 첨들어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100024474.1.7",
          "form": "그건 무슨 수업이에요?",
          "original_form": "그건 무슨 수업이에요?",
          "speaker_id": "1",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100024474.1.8",
          "form": "교수님이랑 자유토론하고",
          "original_form": "교수님이랑 자유토론하고",
          "speaker_id": "2",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100024474.1.9",
          "form": "딱히 어떤수업이라고",
          "original_form": "딱히 어떤수업이라고",
          "speaker_id": "2",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100024474.1.10",
          "form": "정의는 어려워요",
          "original_form": "정의는 어려워요",
          "speaker_id": "2",
          "time": "20210831 11:06"
        },
        {
          "id": "MDRW2100024474.1.11",
          "form": "이번학기는 몇학점 들으세요?",
          "original_form": "이번학기는 몇학점 들으세요?",
          "speaker_id": "1",
          "time": "20210831 11:07"
        },
        {
          "id": "MDRW2100024474.1.12",
          "form": "21학점이요.....",
          "original_form": "21학점이요.....",
          "speaker_id": "2",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100024474.1.13",
          "form": "대박.... 가능해요??",
          "original_form": "대박.... 가능해요??",
          "speaker_id": "1",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100024474.1.14",
          "form": "저때는 학점 높으면 한과목 더 듣게해주고 그랬는데",
          "original_form": "저때는 학점 높으면 한과목 더 듣게해주고 그랬는데",
          "speaker_id": "1",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100024474.1.15",
          "form": "제가 그래요",
          "original_form": "제가 그래요",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100024474.1.16",
          "form": "원래는 19학점까진데",
          "original_form": "원래는 19학점까진데",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100024474.1.17",
          "form": "이번엔 2학점 더 늘었어요",
          "original_form": "이번엔 2학점 더 늘었어요",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100024474.1.18",
          "form": "19학점도 많은데요...?",
          "original_form": "19학점도 많은데요...?",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100024474.1.19",
          "form": "전 18학점이 최대였는데 ㅋㅋㅋㅋ",
          "original_form": "전 18학점이 최대였는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100024474.1.20",
          "form": "1학기 성적이 몇이길래요?",
          "original_form": "1학기 성적이 몇이길래요?",
          "speaker_id": "1",
          "time": "20210831 11:11"
        },
        {
          "id": "MDRW2100024474.1.21",
          "form": "학점은 만점 4.5요",
          "original_form": "학점은 만점 4.5요",
          "speaker_id": "2",
          "time": "20210831 11:11"
        },
        {
          "id": "MDRW2100024474.1.22",
          "form": "백분율은 95엿나",
          "original_form": "백분율은 95엿나",
          "speaker_id": "2",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100024474.1.23",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100024474.1.24",
          "form": "4.5학점 인터넷에만 존재하던 점수 아닌가요?",
          "original_form": "4.5학점 인터넷에만 존재하던 점수 아닌가요?",
          "speaker_id": "1",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100024474.1.25",
          "form": "ㅎ..절대평가라",
          "original_form": "ㅎ..절대평가라",
          "speaker_id": "2",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100024474.1.26",
          "form": "받기 쉬웠어요",
          "original_form": "받기 쉬웠어요",
          "speaker_id": "2",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100024474.1.27",
          "form": "아뇨!!!!!",
          "original_form": "아뇨!!!!!",
          "speaker_id": "1",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100024474.1.28",
          "form": "상대평가보단 덜하겠지만",
          "original_form": "상대평가보단 덜하겠지만",
          "speaker_id": "1",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100024474.1.29",
          "form": "절대평가도 잘해야 에이플 주자나요!",
          "original_form": "절대평가도 잘해야 에이플 주자나요!",
          "speaker_id": "1",
          "time": "20210831 11:13"
        }
      ]
    }
