      "utterance": [
        {
          "id": "MDRW2100025135.1.1",
          "form": "난 최근에 육십계치킨먹었어",
          "original_form": "난 최근에 육십계치킨먹었어",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025135.1.2",
          "form": "넌 뭐 시켜먹은거 있니",
          "original_form": "넌 뭐 시켜먹은거 있니",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025135.1.3",
          "form": "헐 어떤걸로?",
          "original_form": "헐 어떤걸로?",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025135.1.4",
          "form": "간지치킨 진짜 맛있어",
          "original_form": "간지치킨 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100025135.1.5",
          "form": "누룽지치킨이야",
          "original_form": "누룽지치킨이야",
          "speaker_id": "1",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100025135.1.6",
          "form": "우리집은 비비고에서 나온 주먹밥",
          "original_form": "우리집은 비비고에서 나온 주먹밥",
          "speaker_id": "2",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100025135.1.7",
          "form": "누룽지 치킨???",
          "original_form": "누룽지 치킨???",
          "speaker_id": "2",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100025135.1.8",
          "form": "진짜 상상도 안가는 맛이다",
          "original_form": "진짜 상상도 안가는 맛이다",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025135.1.9",
          "form": "달달하고 맛있어",
          "original_form": "달달하고 맛있어",
          "speaker_id": "1",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025135.1.10",
          "form": "들어가있는 감자가 굿이야",
          "original_form": "들어가있는 감자가 굿이야",
          "speaker_id": "1",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025135.1.11",
          "form": "내일먹어봐",
          "original_form": "내일먹어봐",
          "speaker_id": "1",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025135.1.12",
          "form": "대박인데",
          "original_form": "대박인데",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025135.1.13",
          "form": "청년피자는 먹어봤니?",
          "original_form": "청년피자는 먹어봤니?",
          "speaker_id": "1",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025135.1.14",
          "form": "헐 아니",
          "original_form": "헐 아니",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025135.1.15",
          "form": "뭐 또 맛있는거 있음?",
          "original_form": "뭐 또 맛있는거 있음?",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025135.1.16",
          "form": "왜안먹어?",
          "original_form": "왜안먹어?",
          "speaker_id": "1",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100025135.1.17",
          "form": "뭔지는 알려주고 말하라고ㅋㅋㅋㅋ",
          "original_form": "뭔지는 알려주고 말하라고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025135.1.18",
          "form": "고르곤졸라 완전 맛있어",
          "original_form": "고르곤졸라 완전 맛있어",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025135.1.19",
          "form": "근데 먹을려면 포장주문해야돼",
          "original_form": "근데 먹을려면 포장주문해야돼",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025135.1.20",
          "form": "근데 우리집 근처에는 청년피자가 없어서...",
          "original_form": "근데 우리집 근처에는 청년피자가 없어서...",
          "speaker_id": "2",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100025135.1.21",
          "form": "버스타 버스!!!!한번쯤 먹어봐!!!!",
          "original_form": "버스타 버스!!!!한번쯤 먹어봐!!!!",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.22",
          "form": "그렇게 포장해서 먹을 정도로 맛있다고?",
          "original_form": "그렇게 포장해서 먹을 정도로 맛있다고?",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.23",
          "form": "진짜 난 한번쯤 먹을만하다고봐",
          "original_form": "진짜 난 한번쯤 먹을만하다고봐",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.24",
          "form": "그 고구마크림치즈스틱은",
          "original_form": "그 고구마크림치즈스틱은",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.25",
          "form": "꼭 추가해",
          "original_form": "꼭 추가해",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.26",
          "form": "너는 어디서 그렇게 맛집 정보를 알아오는 거야",
          "original_form": "너는 어디서 그렇게 맛집 정보를 알아오는 거야",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.27",
          "form": "쩝쩝박사다",
          "original_form": "쩝쩝박사다",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.28",
          "form": "원래 공부하다보면 배고파",
          "original_form": "원래 공부하다보면 배고파",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.29",
          "form": "너도 그렇게 될거야",
          "original_form": "너도 그렇게 될거야",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.30",
          "form": "그건 인정...",
          "original_form": "그건 인정...",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100025135.1.31",
          "form": "그렇게까지 추천해주니까 궁금하다",
          "original_form": "그렇게까지 추천해주니까 궁금하다",
          "speaker_id": "2",
          "time": "20210831 23:36"
        },
        {
          "id": "MDRW2100025135.1.32",
          "form": "배민에서 도미노 만원할인한다..",
          "original_form": "배민에서 도미노 만원할인한다..",
          "speaker_id": "1",
          "time": "20210831 23:36"
        },
        {
          "id": "MDRW2100025135.1.33",
          "form": "꿀팁 감사합니다",
          "original_form": "꿀팁 감사합니다",
          "speaker_id": "2",
          "time": "20210831 23:36"
        }
      ]
    }
