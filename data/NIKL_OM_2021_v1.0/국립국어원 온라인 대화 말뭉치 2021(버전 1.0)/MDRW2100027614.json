      "utterance": [
        {
          "id": "MDRW2100027614.1.1",
          "form": "그 건강식 유튜버 요즘 내 연관에 뜨더라",
          "original_form": "그 건강식 유튜버 요즘 내 연관에 뜨더라",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027614.1.2",
          "form": "집가서 공유해야겠구만",
          "original_form": "집가서 공유해야겠구만",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027614.1.3",
          "form": "나 한번은 비건햐볼러고 했는데",
          "original_form": "나 한번은 비건햐볼러고 했는데",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027614.1.4",
          "form": "금방 포기",
          "original_form": "금방 포기",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027614.1.5",
          "form": "근데 언니가 고기먹나?",
          "original_form": "근데 언니가 고기먹나?",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027614.1.6",
          "form": "고기 너무 맜있어",
          "original_form": "고기 너무 맜있어",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027614.1.7",
          "form": "집 가면 고기 짱짱",
          "original_form": "집 가면 고기 짱짱",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027614.1.8",
          "form": "아 근데 한달에 몇번은 먹어도되는거지뭐",
          "original_form": "아 근데 한달에 몇번은 먹어도되는거지뭐",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027614.1.9",
          "form": "요즘은 선택적 비건한다잖아",
          "original_form": "요즘은 선택적 비건한다잖아",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027614.1.10",
          "form": "고기먹을때는 먹고",
          "original_form": "고기먹을때는 먹고",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027614.1.11",
          "form": "으흥",
          "original_form": "으흥",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027614.1.12",
          "form": "난 없어서 못먹눈데",
          "original_form": "난 없어서 못먹눈데",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027614.1.13",
          "form": "나는 고딩때까지 집에서 거의 비건식이었어서ㅋㅋㅋ",
          "original_form": "나는 고딩때까지 집에서 거의 비건식이었어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027614.1.14",
          "form": "고기반찬이란게 없었어",
          "original_form": "고기반찬이란게 없었어",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027614.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027614.1.16",
          "form": "급식으로 고기를 먹었지",
          "original_form": "급식으로 고기를 먹었지",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027614.1.17",
          "form": "집에서는 무조건 삼겹살 구워먹고 갈비찜정도?",
          "original_form": "집에서는 무조건 삼겹살 구워먹고 갈비찜정도?",
          "speaker_id": "1",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027614.1.18",
          "form": "저희집은 아빠가 고기 홀릭이라서",
          "original_form": "저희집은 아빠가 고기 홀릭이라서",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027614.1.19",
          "form": "나 어릴때 궁굼했던거 고기반찬없으면 밥안먹는다는 소리",
          "original_form": "나 어릴때 궁굼했던거 고기반찬없으면 밥안먹는다는 소리",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027614.1.20",
          "form": "집에서 고기반찬이 늘 나온다고?",
          "original_form": "집에서 고기반찬이 늘 나온다고?",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027614.1.21",
          "form": "허허허허",
          "original_form": "허허허허",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027614.1.22",
          "form": "아버지 몸쓰는 일이라서",
          "original_form": "아버지 몸쓰는 일이라서",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027614.1.23",
          "form": "스팸이라도 있었죠",
          "original_form": "스팸이라도 있었죠",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027614.1.24",
          "form": "아그래 햄이 있구나",
          "original_form": "아그래 햄이 있구나",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027614.1.25",
          "form": "그 햄 오빠가반찬 안먹어서 할머니가 구워주셨지",
          "original_form": "그 햄 오빠가반찬 안먹어서 할머니가 구워주셨지",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027614.1.26",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027614.1.27",
          "form": "딸래미도 자식이다",
          "original_form": "딸래미도 자식이다",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027614.1.28",
          "form": "아니 나도 물론먹었지 ㅎㅎ 근데 나는 할머니 반찬이 더 맛있었음",
          "original_form": "아니 나도 물론먹었지 ㅎㅎ 근데 나는 할머니 반찬이 더 맛있었음",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027614.1.29",
          "form": "와 멸치칼국수 라면 알아?",
          "original_form": "와 멸치칼국수 라면 알아?",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027614.1.30",
          "form": "하 존맛;;;",
          "original_form": "하 존맛;;;",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027614.1.31",
          "form": "감칠맛 대박",
          "original_form": "감칠맛 대박",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027614.1.32",
          "form": "양배추샐러드랑 같이 순삭",
          "original_form": "양배추샐러드랑 같이 순삭",
