      "utterance": [
        {
          "id": "MDRW2100008355.1.1",
          "form": "오늘 날씨 좋죠?",
          "original_form": "오늘 날씨 좋죠?",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008355.1.2",
          "form": "바람도 선선하고 이불빨래널기 좋은 날씨예요 ㅎ",
          "original_form": "바람도 선선하고 이불빨래널기 좋은 날씨예요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008355.1.3",
          "form": "낼부터 가을장마가 시작된다고 하네요",
          "original_form": "낼부터 가을장마가 시작된다고 하네요",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008355.1.4",
          "form": "이불 다 세탁기에 넣고 돌렸어요",
          "original_form": "이불 다 세탁기에 넣고 돌렸어요",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008355.1.5",
          "form": "원래 오늘도 비온다고 했던거같은데 안오네요",
          "original_form": "원래 오늘도 비온다고 했던거같은데 안오네요",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008355.1.6",
          "form": "오마이스태풍 북상 중이라고 해서요",
          "original_form": "오마이스태풍 북상 중이라고 해서요",
          "speaker_id": "2",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008355.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008355.1.8",
          "form": "태풍도 온다고 하더라구요",
          "original_form": "태풍도 온다고 하더라구요",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008355.1.9",
          "form": "별일없이 지나가야할텐데요",
          "original_form": "별일없이 지나가야할텐데요",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008355.1.10",
          "form": "우리나라 말고 밤에 방향 바꿔 다른곳으로 가길 바래보아요",
          "original_form": "우리나라 말고 밤에 방향 바꿔 다른곳으로 가길 바래보아요",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008355.1.11",
          "form": "그럼 진짜 좋겠네요 ㅎㅎ",
          "original_form": "그럼 진짜 좋겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008355.1.12",
          "form": "저는 태풍오면 대기해야되서 너무싫어요",
          "original_form": "저는 태풍오면 대기해야되서 너무싫어요",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008355.1.13",
          "form": "비피해가 해마다 있는곳은 무섭고 두렵겠죠",
          "original_form": "비피해가 해마다 있는곳은 무섭고 두렵겠죠",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008355.1.14",
          "form": "일본에 천미리씩 왔다는데",
          "original_form": "일본에 천미리씩 왔다는데",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008355.1.15",
          "form": "생각만해도 소름이예요",
          "original_form": "생각만해도 소름이예요",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008355.1.16",
          "form": "지구온난화 땜에 더 그런걸까요",
          "original_form": "지구온난화 땜에 더 그런걸까요",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008355.1.17",
          "form": "아마 그런거같아요",
          "original_form": "아마 그런거같아요",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008355.1.18",
          "form": "점점 환경이 안좋아지니까요",
          "original_form": "점점 환경이 안좋아지니까요",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008355.1.19",
          "form": "그래서 기상이변도 생긴듯 하죠",
          "original_form": "그래서 기상이변도 생긴듯 하죠",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008355.1.20",
          "form": "네 저도 같은생각이예요",
          "original_form": "네 저도 같은생각이예요",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008355.1.21",
          "form": "더이상에 이변은 없길 바래 봅니다",
          "original_form": "더이상에 이변은 없길 바래 봅니다",
          "speaker_id": "2",
          "time": "20210822 16:35"
        }
