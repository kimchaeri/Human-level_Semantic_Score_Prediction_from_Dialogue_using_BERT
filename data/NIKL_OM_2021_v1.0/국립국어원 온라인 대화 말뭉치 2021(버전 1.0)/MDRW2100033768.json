      "utterance": [
        {
          "id": "MDRW2100033768.1.1",
          "form": "순대랑 들깨가루~~",
          "original_form": "순대랑 들깨가루~~",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100033768.1.2",
          "form": "지글지글 기름에 볶아서~쫄면은 바싹",
          "original_form": "지글지글 기름에 볶아서~쫄면은 바싹",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100033768.1.3",
          "form": "들깨가루가 생명 이죠ㅠㅠ",
          "original_form": "들깨가루가 생명 이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100033768.1.4",
          "form": "들깨든거는 다 맛있는거 같아요ㅠ",
          "original_form": "들깨든거는 다 맛있는거 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100033768.1.5",
          "form": "마자요 들깨수제비도 맛나죠ㅎ",
          "original_form": "마자요 들깨수제비도 맛나죠ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100033768.1.6",
          "form": "백순대는 저렇게 볶아서 양념장이랑 싸먹어요",
          "original_form": "백순대는 저렇게 볶아서 양념장이랑 싸먹어요",
          "speaker_id": "1",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100033768.1.7",
          "form": "깻잎에",
          "original_form": "깻잎에",
          "speaker_id": "1",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100033768.1.8",
          "form": "와 환상의 궁합이네요ㅠㅠ",
          "original_form": "와 환상의 궁합이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100033768.1.9",
          "form": "야밤에 넘 먹는얘기 했네요 배고프게ㅎ",
          "original_form": "야밤에 넘 먹는얘기 했네요 배고프게ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100033768.1.10",
          "form": "맞아요 ㅋㅋ 근데 배는안고픈데 항상 뭔가 먹고 싶고 그래요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ 근데 배는안고픈데 항상 뭔가 먹고 싶고 그래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100033768.1.11",
          "form": "한동안 맛있는 녀석들 새벽에보고 야식 엄청 먹었어요",
          "original_form": "한동안 맛있는 녀석들 새벽에보고 야식 엄청 먹었어요",
          "speaker_id": "1",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100033768.1.12",
          "form": "맛녀ㅠㅠㅠ 이제 김준현 진짜 안나오나요?",
          "original_form": "맛녀ㅠㅠㅠ 이제 김준현 진짜 안나오나요?",
          "speaker_id": "2",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100033768.1.13",
          "form": "안나온데요?",
          "original_form": "안나온데요?",
          "speaker_id": "1",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100033768.1.14",
          "form": "재편성해요? 김준현이 메인인데",
          "original_form": "재편성해요? 김준현이 메인인데",
          "speaker_id": "1",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100033768.1.15",
          "form": "너무 살쪄서 건강상의 이유로 쉰다고 짤을 본것 같은데 아닌가.",
          "original_form": "너무 살쪄서 건강상의 이유로 쉰다고 짤을 본것 같은데 아닌가.",
          "speaker_id": "2",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100033768.1.16",
          "form": "아 넘 아쉽~ 그럼 다들 안볼거같은데",
          "original_form": "아 넘 아쉽~ 그럼 다들 안볼거같은데",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033768.1.17",
          "form": "ㅠㅠ 근데 너무 먹어서 건강상의 이유니 이해는가요ㅠㅠㅠ 진짜 너무 ㅋㅋ ㅁ맛있게 많이..ㅋㅋ",
          "original_form": "ㅠㅠ 근데 너무 먹어서 건강상의 이유니 이해는가요ㅠㅠㅠ 진짜 너무 ㅋㅋ ㅁ맛있게 많이..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100033768.1.18",
          "form": "아주깔끔하게 잘먹어서 넘좋아했는데",
          "original_form": "아주깔끔하게 잘먹어서 넘좋아했는데",
