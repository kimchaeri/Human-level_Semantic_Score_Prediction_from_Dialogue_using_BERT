      "utterance": [
        {
          "id": "MDRW2100039727.1.1",
          "form": "여행 최근에 다녀오셨어요?",
          "original_form": "여행 최근에 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039727.1.2",
          "form": "아니요 요즘은 통 여행은 못갔네요ㅠ",
          "original_form": "아니요 요즘은 통 여행은 못갔네요ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039727.1.3",
          "form": "저두요 마지막은 어디 가셨어요?",
          "original_form": "저두요 마지막은 어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039727.1.4",
          "form": "상하이 다녀왔어요!",
          "original_form": "상하이 다녀왔어요!",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039727.1.5",
          "form": "와 상하이 좋나요?",
          "original_form": "와 상하이 좋나요?",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039727.1.6",
          "form": "19년 여름이었는데 타이밍이 좋았죠",
          "original_form": "19년 여름이었는데 타이밍이 좋았죠",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039727.1.7",
          "form": "여름에 조금 덥긴한데",
          "original_form": "여름에 조금 덥긴한데",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039727.1.8",
          "form": "음식도 맛있고 도시도 화려해서 재밌긴해요",
          "original_form": "음식도 맛있고 도시도 화려해서 재밌긴해요",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039727.1.9",
          "form": "다만 말이 잘 안통해요ㅋㅋㅋㅋ",
          "original_form": "다만 말이 잘 안통해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039727.1.10",
          "form": "오 재밌겠네요",
          "original_form": "오 재밌겠네요",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039727.1.11",
          "form": "ㅋㅋ 말은",
          "original_form": "ㅋㅋ 말은",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039727.1.12",
          "form": "저도 19년엔간 일본다녀왔어요",
          "original_form": "저도 19년엔간 일본다녀왔어요",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039727.1.13",
          "form": "오 어디 다녀오셨어요?",
          "original_form": "오 어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039727.1.14",
          "form": "교토요",
          "original_form": "교토요",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039727.1.15",
          "form": "그리고 오사카도요",
          "original_form": "그리고 오사카도요",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039727.1.16",
          "form": "교토좋죠!",
          "original_form": "교토좋죠!",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039727.1.17",
          "form": "어느 계절에 가셨나요?",
          "original_form": "어느 계절에 가셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039727.1.18",
          "form": "가장 베이직한 곳이죠 ㅎㅎ",
          "original_form": "가장 베이직한 곳이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039727.1.19",
          "form": "가을 초입에요",
          "original_form": "가을 초입에요",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039727.1.20",
          "form": "비온다고 했는데 날씨 좋았어요",
          "original_form": "비온다고 했는데 날씨 좋았어요",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039727.1.21",
          "form": "가을 딱 좋았을 것 같아요!!",
          "original_form": "가을 딱 좋았을 것 같아요!!",
          "speaker_id": "1",
          "time": "20210903 21:00"
        }
      ]
