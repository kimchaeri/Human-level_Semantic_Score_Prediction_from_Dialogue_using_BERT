      "utterance": [
        {
          "id": "MDRW2100019945.1.1",
          "form": "내일 신전 로제떡볶이",
          "original_form": "내일 신전 로제떡볶이",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019945.1.2",
          "form": "사먹으려고요!!",
          "original_form": "사먹으려고요!!",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019945.1.3",
          "form": "너무 궁금해요 ㅠ ㅋㅋㅋ",
          "original_form": "너무 궁금해요 ㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019945.1.4",
          "form": "아 ㅋㅋㅋ 꼭 드셔보세요",
          "original_form": "아 ㅋㅋㅋ 꼭 드셔보세요",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019945.1.5",
          "form": "맛있는 지점이기를!!",
          "original_form": "맛있는 지점이기를!!",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019945.1.6",
          "form": "그래도 저희동네 나름 기본은 해서 좀 기대되네요ㅎㅎㅎ",
          "original_form": "그래도 저희동네 나름 기본은 해서 좀 기대되네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019945.1.7",
          "form": "오 그럼 맛있을거같아요 !",
          "original_form": "오 그럼 맛있을거같아요 !",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019945.1.8",
          "form": "사이드는보통 뭐드세요?",
          "original_form": "사이드는보통 뭐드세요?",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019945.1.9",
          "form": "오뎅튀김은 꼭 시키고 참치마요컵밥도 자주 시켜요!",
          "original_form": "오뎅튀김은 꼭 시키고 참치마요컵밥도 자주 시켜요!",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019945.1.10",
          "form": "매운거 먹으면 잘 탈나서 신전김밥은 가끔 시키구요ㅋㅋㅋㅋ",
          "original_form": "매운거 먹으면 잘 탈나서 신전김밥은 가끔 시키구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019945.1.11",
          "form": "오뎅튀김 진리죠",
          "original_form": "오뎅튀김 진리죠",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019945.1.12",
          "form": "오 저두요 ㅜㅜ",
          "original_form": "오 저두요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019945.1.13",
          "form": "전 먹기는 먹는데 속이 안 받아서 너무 쓰리더라구요",
          "original_form": "전 먹기는 먹는데 속이 안 받아서 너무 쓰리더라구요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019945.1.14",
          "form": "신전이 좀 뭐때문인진 몰라도 되게 속쓰린거같아요ㅠ ㅋㅋㅋ",
          "original_form": "신전이 좀 뭐때문인진 몰라도 되게 속쓰린거같아요ㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019945.1.15",
          "form": "고춧가루를 많이..넣나..??",
          "original_form": "고춧가루를 많이..넣나..??",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019945.1.16",
          "form": "항상 그래서 친구랑 먹으러가면 속에 용암이 있는것같다고",
          "original_form": "항상 그래서 친구랑 먹으러가면 속에 용암이 있는것같다고",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019945.1.17",
          "form": "쿨피스 시켜서 꼭 챙겨먹구요 ㅋㅋㅋㅋ",
          "original_form": "쿨피스 시켜서 꼭 챙겨먹구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019945.1.18",
          "form": "용암ㅌㅋㅋㅋㅋ",
          "original_form": "용암ㅌㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019945.1.19",
          "form": "쿨피스 꿀조합이에요",
          "original_form": "쿨피스 꿀조합이에요",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019945.1.20",
          "form": "순한맛으로 안드세요? 전 순한맛은 괜찮던데",
          "original_form": "순한맛으로 안드세요? 전 순한맛은 괜찮던데",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019945.1.21",
          "form": "가끔 2인분 시키면 하나는 보통맛으로 시켜요!",
          "original_form": "가끔 2인분 시키면 하나는 보통맛으로 시켜요!",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019945.1.22",
          "form": "신전 순한맛이랑 보통맛 차이가 너무 나가지고",
          "original_form": "신전 순한맛이랑 보통맛 차이가 너무 나가지고",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019945.1.23",
          "form": "보통은 진짜 쓰리더라고요 ㅋㅋㅋㅋ",
          "original_form": "보통은 진짜 쓰리더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019945.1.24",
          "form": "매운맛은 어떻게 먹는건지..",
          "original_form": "매운맛은 어떻게 먹는건지..",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019945.1.25",
          "form": "오맞아요 저도 그러더라구여",
          "original_form": "오맞아요 저도 그러더라구여",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019945.1.26",
          "form": "갭이 큰거같아요 너무속쓰려요",
          "original_form": "갭이 큰거같아요 너무속쓰려요",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019945.1.27",
          "form": "신전 첨 먹을때",
          "original_form": "신전 첨 먹을때",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019945.1.28",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019945.1.29",
          "form": "그냥 보통 시켰다가 진짜 혼쭐났어요ㅋㅋㅋㅋ",
          "original_form": "그냥 보통 시켰다가 진짜 혼쭐났어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019945.1.30",
          "form": "매운맛 안시켜서 다행이지..",
          "original_form": "매운맛 안시켜서 다행이지..",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019945.1.31",
          "form": "ㅋㅋㅋㅋㅋ 너무 매워요!!",
          "original_form": "ㅋㅋㅋㅋㅋ 너무 매워요!!",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019945.1.32",
          "form": "후폭풍이 더무서워요",
          "original_form": "후폭풍이 더무서워요",
          "speaker_id": "1",
          "time": "20210827 23:19"
        }
      ]
