      "utterance": [
        {
          "id": "MDRW2100014570.1.1",
          "form": "오늘 저녁은 뭐 드셨어요?",
          "original_form": "오늘 저녁은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014570.1.2",
          "form": "식빵이랑 에너지바랑 두유요.. \r\n운동하기전 에너비 보충으로요..",
          "original_form": "식빵이랑 에너지바랑 두유요.. \r\n운동하기전 에너비 보충으로요..",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014570.1.3",
          "form": "글고 자기전에 샐러드먹고요..",
          "original_form": "글고 자기전에 샐러드먹고요..",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014570.1.4",
          "form": "식사하셧나여?",
          "original_form": "식사하셧나여?",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014570.1.5",
          "form": "오 식단조절하시나봐요?",
          "original_form": "오 식단조절하시나봐요?",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014570.1.6",
          "form": "저는 비빔국수 먹었어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 비빔국수 먹었어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014570.1.7",
          "form": "아뇽..",
          "original_form": "아뇽..",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014570.1.8",
          "form": "평소에 늘 이렇게 먹어요..",
          "original_form": "평소에 늘 이렇게 먹어요..",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014570.1.9",
          "form": "술 안마시는날은.. ㅋㅋㅋㅋ",
          "original_form": "술 안마시는날은.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014570.1.10",
          "form": "우와..",
          "original_form": "우와..",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014570.1.11",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014570.1.12",
          "form": "안 질리나요?",
          "original_form": "안 질리나요?",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014570.1.13",
          "form": "그.. 드레싱을 엄청 종류별로 먹어요..",
          "original_form": "그.. 드레싱을 엄청 종류별로 먹어요..",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014570.1.14",
          "form": "그래서 안질리는듯..",
          "original_form": "그래서 안질리는듯..",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014570.1.15",
          "form": "야채도 계속바꾸고",
          "original_form": "야채도 계속바꾸고",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014570.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ좋은 방법같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ좋은 방법같아요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014570.1.17",
          "form": "샐러드는 드레싱 맛에 먹지요",
          "original_form": "샐러드는 드레싱 맛에 먹지요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014570.1.18",
          "form": "맞아요... 근데 전 야채가 넘 맛잇어요..",
          "original_form": "맞아요... 근데 전 야채가 넘 맛잇어요..",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014570.1.19",
          "form": "막 오이랑 상추같은게...",
          "original_form": "막 오이랑 상추같은게...",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014570.1.20",
          "form": "그냥 타고난 천민인듯.. ㅋㅋㅋ",
          "original_form": "그냥 타고난 천민인듯.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014570.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ타고난 천민",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ타고난 천민",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014570.1.22",
          "form": "고기도..드시지요?",
          "original_form": "고기도..드시지요?",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014570.1.23",
          "form": "네.. 닭가슴살이랑요..",
          "original_form": "네.. 닭가슴살이랑요..",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014570.1.24",
          "form": "아님 제육볶음이나.. 치킨텐더 ㅎㅎ",
          "original_form": "아님 제육볶음이나.. 치킨텐더 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014570.1.25",
          "form": "이거도 다 술안주네요?ㅋㅋㅋ",
          "original_form": "이거도 다 술안주네요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014570.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ샐러드 아니면 술안주",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ샐러드 아니면 술안주",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014570.1.27",
          "form": "저녁 주로 뭐드세요?",
          "original_form": "저녁 주로 뭐드세요?",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014570.1.28",
          "form": "점심 많이 먹어서 배 부르면 거를 때도 많고",
          "original_form": "점심 많이 먹어서 배 부르면 거를 때도 많고",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014570.1.29",
          "form": "그냥 먹고 싶은 거 먹어요!",
          "original_form": "그냥 먹고 싶은 거 먹어요!",
          "speaker_id": "1",
          "time": "20210825 20:04"
        }
      ]
    }
  ]
