      "utterance": [
        {
          "id": "MDRW2100022233.1.1",
          "form": "저는 강된장찌개에 생선구이 먹었어요",
          "original_form": "저는 강된장찌개에 생선구이 먹었어요",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022233.1.2",
          "form": "앗 완전 제대로 드셨네요",
          "original_form": "앗 완전 제대로 드셨네요",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022233.1.3",
          "form": "name1은 뭐 드셨나요",
          "original_form": "&name1&은 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022233.1.4",
          "form": "저는 요거트에 시리얼이요",
          "original_form": "저는 요거트에 시리얼이요",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022233.1.5",
          "form": "아침은 간단하게 먹는것도 괜찮은 거 같아요",
          "original_form": "아침은 간단하게 먹는것도 괜찮은 거 같아요",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022233.1.6",
          "form": "저는 사실 제대로 먹는게 좋은데 귀찮아서..",
          "original_form": "저는 사실 제대로 먹는게 좋은데 귀찮아서..",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022233.1.7",
          "form": "아침은 늘 간단하게 드시나요?",
          "original_form": "아침은 늘 간단하게 드시나요?",
          "speaker_id": "2",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022233.1.8",
          "form": "그런편이에요ㅠㅠ",
          "original_form": "그런편이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022233.1.9",
          "form": "전날 미리 만들어놓고 드셔보시면 어때요",
          "original_form": "전날 미리 만들어놓고 드셔보시면 어때요",
          "speaker_id": "2",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022233.1.10",
          "form": "그게 잘 안되더라고요...",
          "original_form": "그게 잘 안되더라고요...",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022233.1.11",
          "form": "밥챙겨 먹는게 제일 힘든일이긴 해요",
          "original_form": "밥챙겨 먹는게 제일 힘든일이긴 해요",
          "speaker_id": "2",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022233.1.12",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022233.1.13",
          "form": "저도 귀찮을땐 토스트만 먹기도 해요",
          "original_form": "저도 귀찮을땐 토스트만 먹기도 해요",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022233.1.14",
          "form": "제로칼로리 음료 좋아하세요?",
          "original_form": "제로칼로리 음료 좋아하세요?",
          "speaker_id": "1",
