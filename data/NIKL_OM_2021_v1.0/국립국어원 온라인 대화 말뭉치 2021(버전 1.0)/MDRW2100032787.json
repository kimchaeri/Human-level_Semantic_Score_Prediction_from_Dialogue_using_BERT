      "utterance": [
        {
          "id": "MDRW2100032787.1.1",
          "form": "재미있게 일긍 책 있나요",
          "original_form": "재미있게 일긍 책 있나요",
          "speaker_id": "1",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100032787.1.2",
          "form": "저 최근에 나의비거니즘만화 읽었어요",
          "original_form": "저 최근에 나의비거니즘만화 읽었어요",
          "speaker_id": "2",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100032787.1.3",
          "form": "좋더라고요!",
          "original_form": "좋더라고요!",
          "speaker_id": "2",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100032787.1.4",
          "form": "어떵 내용인가요",
          "original_form": "어떵 내용인가요",
          "speaker_id": "1",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100032787.1.5",
          "form": "비건에 대한 내용인데 몰랐던 내용을 많이 알게 됐답니다...",
          "original_form": "비건에 대한 내용인데 몰랐던 내용을 많이 알게 됐답니다...",
          "speaker_id": "2",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100032787.1.6",
          "form": "신기하더라고요!!!",
          "original_form": "신기하더라고요!!!",
          "speaker_id": "2",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100032787.1.7",
          "form": "아하 또 어떤 책을 읽으셨나요",
          "original_form": "아하 또 어떤 책을 읽으셨나요",
          "speaker_id": "1",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100032787.1.8",
          "form": "저 지구에서 한아뿐도 읽었어요 좋도라고요 ㅎ.ㅎ",
          "original_form": "저 지구에서 한아뿐도 읽었어요 좋도라고요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100032787.1.9",
          "form": "정세랑작가의 책이에요",
          "original_form": "정세랑작가의 책이에요",
          "speaker_id": "2",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100032787.1.10",
          "form": "재밌었나요",
          "original_form": "재밌었나요",
          "speaker_id": "1",
          "time": "20210905 23:36"
        },
        {
          "id": "MDRW2100032787.1.11",
          "form": "네!! 저는 즐거웠어요! sf소설 재밌더라고요~~~!",
          "original_form": "네!! 저는 즐거웠어요! sf소설 재밌더라고요~~~!",
          "speaker_id": "2",
          "time": "20210905 23:37"
        },
        {
          "id": "MDRW2100032787.1.12",
          "form": "나중에 읽어봐야겠네요",
          "original_form": "나중에 읽어봐야겠네요",
          "speaker_id": "1",
          "time": "20210905 23:37"
        },
        {
          "id": "MDRW2100032787.1.13",
          "form": "네 꼭 읽어보세용!",
          "original_form": "네 꼭 읽어보세용!",
          "speaker_id": "2",
          "time": "20210905 23:37"
        },
        {
          "id": "MDRW2100032787.1.14",
          "form": "책 자주 읽으시나봐요",
          "original_form": "책 자주 읽으시나봐요",
          "speaker_id": "1",
          "time": "20210905 23:37"
        },
        {
          "id": "MDRW2100032787.1.15",
          "form": "ㅋㅋㅋ 아니에요 자주 읽으려고 하는편이죠",
          "original_form": "ㅋㅋㅋ 아니에요 자주 읽으려고 하는편이죠",
          "speaker_id": "2",
          "time": "20210905 23:37"
        },
        {
          "id": "MDRW2100032787.1.16",
          "form": "대단하시네뇨",
