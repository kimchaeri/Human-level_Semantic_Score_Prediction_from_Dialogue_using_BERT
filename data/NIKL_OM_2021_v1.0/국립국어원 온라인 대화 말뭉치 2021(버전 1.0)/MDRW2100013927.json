      "utterance": [
        {
          "id": "MDRW2100013927.1.1",
          "form": "그 포도주스 맛있어?",
          "original_form": "그 포도주스 맛있어?",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013927.1.2",
          "form": "어 델몬트 포도주스꿀맛",
          "original_form": "어 델몬트 포도주스꿀맛",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013927.1.3",
          "form": "나도 그거 어제머금",
          "original_form": "나도 그거 어제머금",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013927.1.4",
          "form": "쌀과자하고 같이!",
          "original_form": "쌀과자하고 같이!",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013927.1.5",
          "form": "근데 뒷맛이 조금 텁텁한넉낌",
          "original_form": "근데 뒷맛이 조금 텁텁한넉낌",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013927.1.6",
          "form": "포도주스가 다 그런가봐!",
          "original_form": "포도주스가 다 그런가봐!",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013927.1.7",
          "form": "살과자나도 먹어야겟다",
          "original_form": "살과자나도 먹어야겟다",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013927.1.8",
          "form": "그래 좋아ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래 좋아ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013927.1.9",
          "form": "엉ㅋㅋㅋㅋ",
          "original_form": "엉ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013927.1.10",
          "form": "둘이 잘어울린다구",
          "original_form": "둘이 잘어울린다구",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013927.1.11",
          "form": "우리집에 과자또 뭐있지",
          "original_form": "우리집에 과자또 뭐있지",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013927.1.12",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013927.1.13",
          "form": "그 엄마가 좋아하시는 전병?",
          "original_form": "그 엄마가 좋아하시는 전병?",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013927.1.14",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013927.1.15",
          "form": "그거노맛",
          "original_form": "그거노맛",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013927.1.16",
          "form": "맞아",
          "original_form": "맞아",
