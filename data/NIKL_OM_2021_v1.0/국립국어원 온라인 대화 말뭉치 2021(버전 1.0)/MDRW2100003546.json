      "utterance": [
        {
          "id": "MDRW2100003546.1.1",
          "form": "좋아하는 스포츠 경기 있어?",
          "original_form": "좋아하는 스포츠 경기 있어?",
          "speaker_id": "1",
          "time": "20210821 00:54"
        },
        {
          "id": "MDRW2100003546.1.2",
          "form": "나는 야구 좋아해",
          "original_form": "나는 야구 좋아해",
          "speaker_id": "2",
          "time": "20210821 00:54"
        },
        {
          "id": "MDRW2100003546.1.3",
          "form": "오늘도 롯데 경기봄",
          "original_form": "오늘도 롯데 경기봄",
          "speaker_id": "2",
          "time": "20210821 00:54"
        },
        {
          "id": "MDRW2100003546.1.4",
          "form": "야구 경기 직관한적 있어?",
          "original_form": "야구 경기 직관한적 있어?",
          "speaker_id": "1",
          "time": "20210821 00:54"
        },
        {
          "id": "MDRW2100003546.1.5",
          "form": "집에서 본거야?",
          "original_form": "집에서 본거야?",
          "speaker_id": "1",
          "time": "20210821 00:54"
        },
        {
          "id": "MDRW2100003546.1.6",
          "form": "아주 어릴때 몇번 ㅠㅠ",
          "original_form": "아주 어릴때 몇번 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 00:54"
        },
        {
          "id": "MDRW2100003546.1.7",
          "form": "어릴때 가족이랑 직관갔나 보네",
          "original_form": "어릴때 가족이랑 직관갔나 보네",
          "speaker_id": "1",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003546.1.8",
          "form": "지금은 코로나 때매 안되고",
          "original_form": "지금은 코로나 때매 안되고",
          "speaker_id": "2",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003546.1.9",
          "form": "맞아맞아 언닌?",
          "original_form": "맞아맞아 언닌?",
          "speaker_id": "2",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003546.1.10",
          "form": "코로나가 경기도 못보게 하네",
          "original_form": "코로나가 경기도 못보게 하네",
          "speaker_id": "1",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003546.1.11",
          "form": "나는 2년전에 야구 두번 직관 갔어",
          "original_form": "나는 2년전에 야구 두번 직관 갔어",
          "speaker_id": "1",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003546.1.12",
          "form": "코로나 사라져라!!",
          "original_form": "코로나 사라져라!!",
          "speaker_id": "2",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003546.1.13",
          "form": "오 무슨팀 봤더?",
          "original_form": "오 무슨팀 봤더?",
          "speaker_id": "2",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003546.1.14",
          "form": "야구 경기는 직관이 제맛인거 같아",
          "original_form": "야구 경기는 직관이 제맛인거 같아",
          "speaker_id": "1",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003546.1.15",
          "form": "롯데 경기 봤어",
          "original_form": "롯데 경기 봤어",
          "speaker_id": "1",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003546.1.16",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "2",
          "time": "20210821 00:56"
        },
        {
          "id": "MDRW2100003546.1.17",
          "form": "직관하니까 팬들이 환호해서 재미있더라",
          "original_form": "직관하니까 팬들이 환호해서 재미있더라",
          "speaker_id": "1",
          "time": "20210821 00:56"
        },
        {
          "id": "MDRW2100003546.1.18",
          "form": "악 갑자기 직관 진짜 가고싶다",
          "original_form": "악 갑자기 직관 진짜 가고싶다",
          "speaker_id": "2",
          "time": "20210821 00:56"
        },
        {
          "id": "MDRW2100003546.1.19",
          "form": "다같이 피자 치킨 맥주 먹는 그 분위기",
          "original_form": "다같이 피자 치킨 맥주 먹는 그 분위기",
          "speaker_id": "1",
          "time": "20210821 00:56"
        },
        {
          "id": "MDRW2100003546.1.20",
          "form": "분위기가 좋더라",
          "original_form": "분위기가 좋더라",
          "speaker_id": "1",
          "time": "20210821 00:57"
        },
        {
          "id": "MDRW2100003546.1.21",
          "form": "롯데가을야구 갔으몀 좋겟다ㅜㅜ",
          "original_form": "롯데가을야구 갔으몀 좋겟다ㅜㅜ",
          "speaker_id": "2",
