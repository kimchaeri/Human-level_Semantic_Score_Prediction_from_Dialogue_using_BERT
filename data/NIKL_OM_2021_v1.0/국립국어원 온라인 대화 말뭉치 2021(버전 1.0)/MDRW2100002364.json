      "utterance": [
        {
          "id": "MDRW2100002364.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210820 10:25"
        },
        {
          "id": "MDRW2100002364.1.2",
          "form": "코로나 끝나면 가고싶으신 여행지 있으세요?",
          "original_form": "코로나 끝나면 가고싶으신 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210820 10:25"
        },
        {
          "id": "MDRW2100002364.1.3",
          "form": "저는 제주도요 ㅎㅎ",
          "original_form": "저는 제주도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 10:36"
        },
        {
          "id": "MDRW2100002364.1.4",
          "form": "제주도 좋죠~",
          "original_form": "제주도 좋죠~",
          "speaker_id": "1",
          "time": "20210820 10:49"
        },
        {
          "id": "MDRW2100002364.1.5",
          "form": "시원한 바다 보고 싶네요 ㅎㅎ",
          "original_form": "시원한 바다 보고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002364.1.6",
          "form": "아~ 저도 바다 보고싶네요~",
          "original_form": "아~ 저도 바다 보고싶네요~",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002364.1.7",
          "form": "탁 튀인 수평선~",
          "original_form": "탁 튀인 수평선~",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002364.1.8",
          "form": "지금은 안돌아 다니는게  최고니 ㅜㅜ",
          "original_form": "지금은 안돌아 다니는게  최고니 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 11:28"
        },
        {
          "id": "MDRW2100002364.1.9",
          "form": "맞아요..ㅠㅠ 이렇게까지 심해질줄 몰랐는데..ㅠ",
          "original_form": "맞아요..ㅠㅠ 이렇게까지 심해질줄 몰랐는데..ㅠ",
          "speaker_id": "1",
          "time": "20210820 11:45"
        },
        {
          "id": "MDRW2100002364.1.10",
          "form": "정말 여행이 고프네요..ㅠ",
          "original_form": "정말 여행이 고프네요..ㅠ",
          "speaker_id": "1",
          "time": "20210820 11:45"
        },
        {
          "id": "MDRW2100002364.1.11",
          "form": "가까운데라도 드라이브해요 ㅎㅎ",
          "original_form": "가까운데라도 드라이브해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:03"
        },
        {
          "id": "MDRW2100002364.1.12",
          "form": "차가 없어서~ 그냥 가볍게 산책은 하고 있어요~~",
          "original_form": "차가 없어서~ 그냥 가볍게 산책은 하고 있어요~~",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002364.1.13",
          "form": "차 타고 해안가 달려보고싶네요~",
          "original_form": "차 타고 해안가 달려보고싶네요~",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002364.1.14",
          "form": "산책도 좋아요 \n사람없으면 잠시 마스크 벗고 숨쉬고 ㅎㅎㅎ",
          "original_form": "산책도 좋아요 \n사람없으면 잠시 마스크 벗고 숨쉬고 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002364.1.15",
          "form": "진찌~ 마스크 없이 여행했었던 때가 너무 그립네요~",
          "original_form": "진찌~ 마스크 없이 여행했었던 때가 너무 그립네요~",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002364.1.16",
          "form": "그래서 전 드라이브만 하네요 후규\n배경이 바뀌는거만으로도  기분전환되요",
          "original_form": "그래서 전 드라이브만 하네요 후규\n배경이 바뀌는거만으로도  기분전환되요",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002364.1.17",
          "form": "평소와 다른 배경을 본 게 언제적인지...ㅠㅠ",
          "original_form": "평소와 다른 배경을 본 게 언제적인지...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:15"
        }
      ]
