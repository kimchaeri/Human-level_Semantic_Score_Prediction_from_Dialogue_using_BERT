      "utterance": [
        {
          "id": "MDRW2100039164.1.1",
          "form": "점심은용",
          "original_form": "점심은용",
          "speaker_id": "1",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100039164.1.2",
          "form": "방금 짜장면 먹었어요",
          "original_form": "방금 짜장면 먹었어요",
          "speaker_id": "2",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100039164.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100039164.1.4",
          "form": "오 전 아직...스테이크해먹을까해요",
          "original_form": "오 전 아직...스테이크해먹을까해요",
          "speaker_id": "1",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100039164.1.5",
          "form": "점심 스테이크!",
          "original_form": "점심 스테이크!",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039164.1.6",
          "form": "ㅋㅋ 밀키트용",
          "original_form": "ㅋㅋ 밀키트용",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039164.1.7",
          "form": "아 밀키트였군요",
          "original_form": "아 밀키트였군요",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100039164.1.8",
          "form": "ㅋㅋㅋ탕수육은 안드셧나요",
          "original_form": "ㅋㅋㅋ탕수육은 안드셧나요",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039164.1.9",
          "form": "탕수육까지 먹기에는 별로 배가 안고팠어요",
          "original_form": "탕수육까지 먹기에는 별로 배가 안고팠어요",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039164.1.10",
          "form": "간짜장이요 ? _?",
          "original_form": "간짜장이요 ? _?",
          "speaker_id": "1",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039164.1.11",
          "form": "일반 짜장면 먹었죠",
          "original_form": "일반 짜장면 먹었죠",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039164.1.12",
          "form": "간짜장 안조아하시나요",
          "original_form": "간짜장 안조아하시나요",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039164.1.13",
          "form": "가끔 가다가 먹어요",
          "original_form": "가끔 가다가 먹어요",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039164.1.14",
          "form": "계란후라이 올려주는데도 잇대요",
          "original_form": "계란후라이 올려주는데도 잇대요",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039164.1.15",
          "form": "가끔가다 있는거 같긴 하더라구요",
          "original_form": "가끔가다 있는거 같긴 하더라구요",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039164.1.16",
          "form": "계란 금값일땐 어쩌나...",
          "original_form": "계란 금값일땐 어쩌나...",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039164.1.17",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039164.1.18",
          "form": "아직도 비싼가 계란..",
          "original_form": "아직도 비싼가 계란..",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039164.1.19",
          "form": "조금 내려갔어요",
          "original_form": "조금 내려갔어요",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039164.1.20",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210903 18:05"
        }
      ]
