      "utterance": [
        {
          "id": "MDRW2100015754.1.1",
          "form": "자급제 폰 쓰시면 통신사는 어디거 쓰세요./",
          "original_form": "자급제 폰 쓰시면 통신사는 어디거 쓰세요./",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015754.1.2",
          "form": "저는 그냥 케이티써요 ㅋㅋㅋ",
          "original_form": "저는 그냥 케이티써요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015754.1.3",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015754.1.4",
          "form": "저도 그냥 에스케이 앜ㅋㅋㅋ",
          "original_form": "저도 그냥 에스케이 앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015754.1.5",
          "form": "통신사에 얽매이긴 싫지만 스스로 얽매인다.",
          "original_form": "통신사에 얽매이긴 싫지만 스스로 얽매인다.",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015754.1.6",
          "form": "ㅋㅋㅋㅋㅋ알뜰폰 쓰라고는 하던데 요즘",
          "original_form": "ㅋㅋㅋㅋㅋ알뜰폰 쓰라고는 하던데 요즘",
          "speaker_id": "1",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015754.1.7",
          "form": "그거 더 저렴하고 싸다고 하는데 뭔가 하층민 되는 기분이라 정감이 안가요",
          "original_form": "그거 더 저렴하고 싸다고 하는데 뭔가 하층민 되는 기분이라 정감이 안가요",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015754.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015754.1.9",
          "form": "통신사있는게 익숙해서 결국 ㅋㅋ",
          "original_form": "통신사있는게 익숙해서 결국 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015754.1.10",
          "form": "훨씬 싸긴하더라구요",
          "original_form": "훨씬 싸긴하더라구요",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015754.1.11",
          "form": "케이티는 멤버십 할인 어떤 편이에요?",
          "original_form": "케이티는 멤버십 할인 어떤 편이에요?",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015754.1.12",
          "form": "그냥 별로에요",
          "original_form": "그냥 별로에요",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015754.1.13",
          "form": "스벅사이즈업할인정도만쓰구",
          "original_form": "스벅사이즈업할인정도만쓰구",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.14",
          "form": "ㅋㅋㅋㅋ쓰는게없네요",
          "original_form": "ㅋㅋㅋㅋ쓰는게없네요",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.15",
          "form": "에스케이도 갈 수록 혜택 줄이고 별로에요 ㅜ",
          "original_form": "에스케이도 갈 수록 혜택 줄이고 별로에요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.16",
          "form": "sk는 근데 그 11번가에서 살때",
          "original_form": "sk는 근데 그 11번가에서 살때",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.17",
          "form": "그나마 최근엔 빕스 40% 할인 받았네요..",
          "original_form": "그나마 최근엔 빕스 40% 할인 받았네요..",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.18",
          "form": "좋은거같애요",
          "original_form": "좋은거같애요",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.19",
          "form": "아!!",
          "original_form": "아!!",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.20",
          "form": "그거 11일 자주 놓쳐서 별로에요 ㅜㅜ",
          "original_form": "그거 11일 자주 놓쳐서 별로에요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.21",
          "form": "맞아요 저도 아웃백할인 가끔 ㅋㅋㅋ",
          "original_form": "맞아요 저도 아웃백할인 가끔 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.22",
          "form": "11일에 이벤트할인 많긴하죠",
          "original_form": "11일에 이벤트할인 많긴하죠",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015754.1.23",
          "form": "ㅋㅋ저는 에어랩 매달 노리는데 실패해요",
          "original_form": "ㅋㅋ저는 에어랩 매달 노리는데 실패해요",
          "speaker_id": "1",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015754.1.24",
          "form": "그런거보다 뭔가 롯데잇츠에서 5,15,25일에 세트 사는게 더 좋아요 헤헤",
          "original_form": "그런거보다 뭔가 롯데잇츠에서 5,15,25일에 세트 사는게 더 좋아요 헤헤",
          "speaker_id": "2",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015754.1.25",
          "form": "오 !!!!",
          "original_form": "오 !!!!",
          "speaker_id": "1",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015754.1.26",
          "form": "그런것도 있네요",
          "original_form": "그런것도 있네요",
          "speaker_id": "1",
          "time": "20210826 00:53"
        },
        {
