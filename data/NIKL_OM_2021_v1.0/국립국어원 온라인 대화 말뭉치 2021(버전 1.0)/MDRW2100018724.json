      "utterance": [
        {
          "id": "MDRW2100018724.1.1",
          "form": "역시 꿀복숭아",
          "original_form": "역시 꿀복숭아",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018724.1.2",
          "form": "플랫치노말고는?",
          "original_form": "플랫치노말고는?",
          "speaker_id": "1",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018724.1.3",
          "form": "바닐라 라떼",
          "original_form": "바닐라 라떼",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018724.1.4",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018724.1.5",
          "form": "커피빼구",
          "original_form": "커피빼구",
          "speaker_id": "1",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018724.1.6",
          "form": "토피넛 라떼!",
          "original_form": "토피넛 라떼!",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.7",
          "form": "그거 먼맛이야?",
          "original_form": "그거 먼맛이야?",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.8",
          "form": "호두마루맛!",
          "original_form": "호두마루맛!",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.10",
          "form": "좋지??",
          "original_form": "좋지??",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.11",
          "form": "응응 기댜돼",
          "original_form": "응응 기댜돼",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.12",
          "form": "함 묵어봐!",
          "original_form": "함 묵어봐!",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.13",
          "form": "나중에 그거먹어야집",
          "original_form": "나중에 그거먹어야집",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.14",
          "form": "아글고",
          "original_form": "아글고",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.15",
          "form": "솜사탕 어쩌구도 맛나대",
          "original_form": "솜사탕 어쩌구도 맛나대",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.16",
          "form": "그것도",
          "original_form": "그것도",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.17",
          "form": "플랫치노아냐??",
          "original_form": "플랫치노아냐??",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.19",
          "form": "근데맛잇어",
          "original_form": "근데맛잇어",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.20",
          "form": "그간 너무 양이많아",
          "original_form": "그간 너무 양이많아",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018724.1.21",
          "form": "그래서좋은건데",
          "original_form": "그래서좋은건데",
          "speaker_id": "2",
          "time": "20210827 11:28"
        },
        {
          "id": "MDRW2100018724.1.22",
          "form": "칫...",
          "original_form": "칫...",
          "speaker_id": "1",
          "time": "20210827 11:28"
        },
        {
          "id": "MDRW2100018724.1.23",
          "form": "그 내가산다던 음료수 잇자나",
          "original_form": "그 내가산다던 음료수 잇자나",
