      "utterance": [
        {
          "id": "MDRW2100033520.1.1",
          "form": "결혼하셨어요??",
          "original_form": "결혼하셨어요??",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100033520.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100033520.1.3",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100033520.1.4",
          "form": "넵... 아이가 셋입니다..ㅠㅠ",
          "original_form": "넵... 아이가 셋입니다..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100033520.1.5",
          "form": "셋이나 있으시다니",
          "original_form": "셋이나 있으시다니",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100033520.1.6",
          "form": "힘드시겠어요",
          "original_form": "힘드시겠어요",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100033520.1.7",
          "form": "아이들은 몇살인가요?",
          "original_form": "아이들은 몇살인가요?",
          "speaker_id": "1",
          "time": "20210824 18:59"
        },
        {
          "id": "MDRW2100033520.1.8",
          "form": "7살 3살 1살이에요.ㅋㅋㅋㅋ",
          "original_form": "7살 3살 1살이에요.ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100033520.1.9",
          "form": "헛..",
          "original_form": "헛..",
          "speaker_id": "1",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100033520.1.10",
          "form": "결혼 생각은 있으세요??",
          "original_form": "결혼 생각은 있으세요??",
          "speaker_id": "2",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100033520.1.11",
          "form": "어..저는 아직 없어요",
          "original_form": "어..저는 아직 없어요",
          "speaker_id": "1",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100033520.1.12",
          "form": "첫째하고 막내하고 터울이 많이나네요",
          "original_form": "첫째하고 막내하고 터울이 많이나네요",
          "speaker_id": "1",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100033520.1.13",
          "form": "흑 어쩌다 보니 그렇게 되었어요..",
          "original_form": "흑 어쩌다 보니 그렇게 되었어요..",
          "speaker_id": "2",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100033520.1.14",
          "form": "고생이 많으세요..",
          "original_form": "고생이 많으세요..",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100033520.1.15",
          "form": "이제 와서 생각하지만 안하는 것도 좋다는 생각이 듭니다.ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이제 와서 생각하지만 안하는 것도 좋다는 생각이 듭니다.ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100033520.1.16",
          "form": "ㅎㅎ어떤 점에서 그렇게 느끼셨나요",
          "original_form": "ㅎㅎ어떤 점에서 그렇게 느끼셨나요",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100033520.1.17",
          "form": "음.... 일단 남쳔이 아무리 도와줘도... 남자들인 기본집안일에 대한 정보가 너무 없습니다...",
          "original_form": "음.... 일단 남쳔이 아무리 도와줘도... 남자들인 기본집안일에 대한 정보가 너무 없습니다...",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100033520.1.18",
          "form": "남편이요 ㅋㅋㅋ",
          "original_form": "남편이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100033520.1.19",
          "form": "아..힘드시겠어요",
          "original_form": "아..힘드시겠어요",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100033520.1.20",
          "form": "하나 하나 가르쳐서 사람만들어야하는데... 내가 하는게.........",
          "original_form": "하나 하나 가르쳐서 사람만들어야하는데... 내가 하는게.........",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100033520.1.21",
          "form": "아.. 결혼은 하지 않는걸로...ㅎ",
          "original_form": "아.. 결혼은 하지 않는걸로...ㅎ",
