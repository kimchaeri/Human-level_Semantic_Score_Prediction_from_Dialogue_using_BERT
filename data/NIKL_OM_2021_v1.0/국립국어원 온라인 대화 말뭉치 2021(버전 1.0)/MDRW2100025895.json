      "utterance": [
        {
          "id": "MDRW2100025895.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100025895.1.2",
          "form": "그렇게마니좋아하는편은 아닌데",
          "original_form": "그렇게마니좋아하는편은 아닌데",
          "speaker_id": "1",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100025895.1.3",
          "form": "코로나라서 못가니깐 더 가고싶긴하네요 ㅠㅠ",
          "original_form": "코로나라서 못가니깐 더 가고싶긴하네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025895.1.4",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025895.1.5",
          "form": "저는 여행좋아하는데 코로나때뭄에 답답하네요 어디제일 가고싶으신가요",
          "original_form": "저는 여행좋아하는데 코로나때뭄에 답답하네요 어디제일 가고싶으신가요",
          "speaker_id": "2",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025895.1.6",
          "form": "저는 해외면 다가고싶어요 ㅠㅠ",
          "original_form": "저는 해외면 다가고싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025895.1.7",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025895.1.8",
          "form": "오키나와 젤먼저 가볼까해용 남편이 바다를 좋아해서",
          "original_form": "오키나와 젤먼저 가볼까해용 남편이 바다를 좋아해서",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025895.1.9",
          "form": "오 오키나와도 좋쵸!!바다색이 정말이쁘더라구요!",
          "original_form": "오 오키나와도 좋쵸!!바다색이 정말이쁘더라구요!",
          "speaker_id": "1",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100025895.1.10",
          "form": "우왕 오키나와 가보셨어요?",
          "original_form": "우왕 오키나와 가보셨어요?",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100025895.1.11",
          "form": "아니요...사진으로보니깐 이쁘더라구요 ㅎㅎㅎ",
          "original_form": "아니요...사진으로보니깐 이쁘더라구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025895.1.12",
          "form": "아아 ㅎㅎ 코로나 끝나면 다같이 베트남 가고싶네요",
          "original_form": "아아 ㅎㅎ 코로나 끝나면 다같이 베트남 가고싶네요",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025895.1.13",
          "form": "저두요!!호캉스하면서 맛있는거먹으러 다니고싶어요!!",
          "original_form": "저두요!!호캉스하면서 맛있는거먹으러 다니고싶어요!!",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025895.1.14",
          "form": "꺄 마사지받고 싼 옷 막 사고 맛있는거 먹고싶네용",
          "original_form": "꺄 마사지받고 싼 옷 막 사고 맛있는거 먹고싶네용",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025895.1.15",
          "form": "저두요!!!맘같아선 한달동안 있고싶네요 ㅠ",
          "original_form": "저두요!!!맘같아선 한달동안 있고싶네요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100025895.1.16",
          "form": "또 가고싶은곳 있으세요?",
          "original_form": "또 가고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100025895.1.17",
          "form": "당장은 휴양짗가고싶은데 괌도 꼭 가보려구요!",
          "original_form": "당장은 휴양짗가고싶은데 괌도 꼭 가보려구요!",
          "speaker_id": "2",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100025895.1.18",
          "form": "저 괌 가봤는데 정말 좋아요!",
          "original_form": "저 괌 가봤는데 정말 좋아요!",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100025895.1.19",
          "form": "바다색도 정말이쁘구!!",
          "original_form": "바다색도 정말이쁘구!!",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100025895.1.20",
          "form": "캬 사진보니 진짜 좋아보이더라구요!",
          "original_form": "캬 사진보니 진짜 좋아보이더라구요!",
          "speaker_id": "2",
          "time": "20210901 18:08"
        }
