      "utterance": [
        {
          "id": "MDRW2100006088.1.1",
          "form": "백신 예약하셨어요?",
          "original_form": "백신 예약하셨어요?",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006088.1.2",
          "form": "네 9월30일로요",
          "original_form": "네 9월30일로요",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006088.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006088.1.4",
          "form": "저는 24일에 예약했어요",
          "original_form": "저는 24일에 예약했어요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006088.1.5",
          "form": "아직 백신 종류 안정해졌죠?",
          "original_form": "아직 백신 종류 안정해졌죠?",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006088.1.6",
          "form": "맞으실거예요?",
          "original_form": "맞으실거예요?",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006088.1.7",
          "form": "신랑이랑 같이 맞고 같이 아플라고 24일 같이 예약했어요 ㅎ",
          "original_form": "신랑이랑 같이 맞고 같이 아플라고 24일 같이 예약했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006088.1.8",
          "form": "맞아야쥬~",
          "original_form": "맞아야쥬~",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006088.1.9",
          "form": "무서워서 고민중이라",
          "original_form": "무서워서 고민중이라",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006088.1.10",
          "form": "주변에서 다 맞아서....",
          "original_form": "주변에서 다 맞아서....",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006088.1.11",
          "form": "그니까요 다맞긴하더라구요",
          "original_form": "그니까요 다맞긴하더라구요",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006088.1.12",
          "form": "오늘도 백신휴가 2명이나 냈더라구요",
          "original_form": "오늘도 백신휴가 2명이나 냈더라구요",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006088.1.13",
          "form": "화이자 맞겠죠?",
          "original_form": "화이자 맞겠죠?",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006088.1.14",
          "form": "목금은 부장님 백신휴가라고 하시고",
          "original_form": "목금은 부장님 백신휴가라고 하시고",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006088.1.15",
          "form": "지금까지는 화이자던데...",
          "original_form": "지금까지는 화이자던데...",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006088.1.16",
          "form": "다들 휴가내긴하던데 전 목욜이라",
          "original_form": "다들 휴가내긴하던데 전 목욜이라",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006088.1.17",
          "form": "모더나가 들어왔나봐요 모더나 맞을수 있을지",
          "original_form": "모더나가 들어왔나봐요 모더나 맞을수 있을지",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006088.1.18",
          "form": "금욜휴가낼지 고민중이예요ㅎ",
          "original_form": "금욜휴가낼지 고민중이예요ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006088.1.19",
          "form": "여긴 목요일에 백신접종휴가",
          "original_form": "여긴 목요일에 백신접종휴가",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006088.1.20",
          "form": "금요일은 질병휴가",
          "original_form": "금요일은 질병휴가",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006088.1.21",
          "form": "꼭 목금 내는건아닌데 대부분 2일 내더라구요",
          "original_form": "꼭 목금 내는건아닌데 대부분 2일 내더라구요",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100006088.1.22",
          "form": "지방은 모더나맞는다는 얘기도있던데",
          "original_form": "지방은 모더나맞는다는 얘기도있던데",
          "speaker_id": "2",
          "time": "20210907 20:20"
        }
