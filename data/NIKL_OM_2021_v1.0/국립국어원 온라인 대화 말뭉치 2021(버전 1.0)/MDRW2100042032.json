      "utterance": [
        {
          "id": "MDRW2100042032.1.1",
          "form": "그러게말여",
          "original_form": "그러게말여",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100042032.1.2",
          "form": "양말 살수있음 조은데",
          "original_form": "양말 살수있음 조은데",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100042032.1.3",
          "form": "그래도 티랑 모자 잘샀어",
          "original_form": "그래도 티랑 모자 잘샀어",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100042032.1.4",
          "form": "티는 공짜 모자는 만원",
          "original_form": "티는 공짜 모자는 만원",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100042032.1.5",
          "form": "굿이야 완전득템인뎅",
          "original_form": "굿이야 완전득템인뎅",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100042032.1.6",
          "form": "또기회가왓음좋겟다",
          "original_form": "또기회가왓음좋겟다",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100042032.1.7",
          "form": "그러게 기다려보자 유입이 작아지면 또 허겠지",
          "original_form": "그러게 기다려보자 유입이 작아지면 또 허겠지",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042032.1.8",
          "form": "아  그러면할수도",
          "original_form": "아  그러면할수도",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042032.1.9",
          "form": "이번에손해가컷나",
          "original_form": "이번에손해가컷나",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100042032.1.10",
          "form": "그럴수도있오 ㅋㅋㅋ 다 퍼줘서",
          "original_form": "그럴수도있오 ㅋㅋㅋ 다 퍼줘서",
          "speaker_id": "2",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100042032.1.11",
          "form": "ㅜㅜ사랑스런곳이엇어흑흑",
          "original_form": "ㅜㅜ사랑스런곳이엇어흑흑",
          "speaker_id": "1",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100042032.1.12",
          "form": "맞아 또 오겠지 여기도 그래 ",
          "original_form": "맞아 또 오겠지 여기도 그래 {emoji:♡}",
