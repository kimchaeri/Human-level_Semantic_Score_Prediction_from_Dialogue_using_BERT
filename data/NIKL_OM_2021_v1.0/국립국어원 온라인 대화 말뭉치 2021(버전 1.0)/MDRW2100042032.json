          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042032.1.13",
          "form": "근데힘들다 ㅋㅋ",
          "original_form": "근데힘들다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042032.1.14",
          "form": "ㅋㅋㅋㄲㅋㅋㄱㅋㄲㄱㅋ",
          "original_form": "ㅋㅋㅋㄲㅋㅋㄱㅋㄲㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042032.1.15",
          "form": "맞어힘들긴해",
          "original_form": "맞어힘들긴해",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042032.1.16",
          "form": "수영구 카드 나온거같아",
          "original_form": "수영구 카드 나온거같아",
