      "utterance": [
        {
          "id": "MDRW2100041624.1.1",
          "form": "떡갈비를요?",
          "original_form": "떡갈비를요?",
          "speaker_id": "2",
          "time": "20210906 13:21"
        },
        {
          "id": "MDRW2100041624.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 13:21"
        },
        {
          "id": "MDRW2100041624.1.3",
          "form": "어떻게요?",
          "original_form": "어떻게요?",
          "speaker_id": "2",
          "time": "20210906 13:21"
        },
        {
          "id": "MDRW2100041624.1.4",
          "form": "소고기랑 돼지고기 사다가 직접 다졌어요",
          "original_form": "소고기랑 돼지고기 사다가 직접 다졌어요",
          "speaker_id": "1",
          "time": "20210906 13:21"
        },
        {
          "id": "MDRW2100041624.1.5",
          "form": "세상에..삼겹살요?",
          "original_form": "세상에..삼겹살요?",
          "speaker_id": "2",
          "time": "20210906 13:21"
        },
        {
          "id": "MDRW2100041624.1.6",
          "form": "네 삼겹살이랑 소고기 부채살이 싸서 ㅎㅎ",
          "original_form": "네 삼겹살이랑 소고기 부채살이 싸서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 13:21"
        },
        {
          "id": "MDRW2100041624.1.7",
          "form": "간고기 산것도 아니고 다지신거에요? 직접?",
          "original_form": "간고기 산것도 아니고 다지신거에요? 직접?",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041624.1.8",
          "form": "네 티비에서 직접다지는거 보고 저도 해보고 싶어서 ㅋㅋㅋ",
          "original_form": "네 티비에서 직접다지는거 보고 저도 해보고 싶어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041624.1.9",
          "form": "정말 대단하시네요 맛있었겠어요",
          "original_form": "정말 대단하시네요 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041624.1.10",
          "form": "아 그게 생각보다 엄청 안다져지더라고요 ㅋㅋ큐ㅠ칼로는...",
          "original_form": "아 그게 생각보다 엄청 안다져지더라고요 ㅋㅋ큐ㅠ칼로는...",
          "speaker_id": "1",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041624.1.11",
          "form": "삼겹살은 지방도 있고 소고기 부채살도 힘줄 있어서 힘들거같긴 해요",
          "original_form": "삼겹살은 지방도 있고 소고기 부채살도 힘줄 있어서 힘들거같긴 해요",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041624.1.12",
          "form": "네ㅠㅠ 그부분이 힘들어서 엄청 오래걸렸어요 ㅋㅋㅋ",
          "original_form": "네ㅠㅠ 그부분이 힘들어서 엄청 오래걸렸어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:23"
        },
        {
          "id": "MDRW2100041624.1.13",
          "form": "맛은요?",
          "original_form": "맛은요?",
          "speaker_id": "2",
          "time": "20210906 13:23"
        },
        {
          "id": "MDRW2100041624.1.14",
          "form": "맛도 있었는데 그냥 앞으로는 사서먹으려고요..",
          "original_form": "맛도 있었는데 그냥 앞으로는 사서먹으려고요..",
          "speaker_id": "1",
          "time": "20210906 13:23"
        },
        {
          "id": "MDRW2100041624.1.15",
          "form": "직접다지는게 힘들어요 ㅋㅋㅋ",
          "original_form": "직접다지는게 힘들어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:23"
        }
