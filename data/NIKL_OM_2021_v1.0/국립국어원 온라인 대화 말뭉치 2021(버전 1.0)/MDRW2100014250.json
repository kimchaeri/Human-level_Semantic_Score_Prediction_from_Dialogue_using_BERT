      "utterance": [
        {
          "id": "MDRW2100014250.1.1",
          "form": "안녕하세요~ 저녁 뭐 드세요?",
          "original_form": "안녕하세요~ 저녁 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014250.1.2",
          "form": "신전 로제떡볶이 먹어요!",
          "original_form": "신전 로제떡볶이 먹어요!",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014250.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014250.1.4",
          "form": "오 로제떡볶이 저도 최근에 먹었는데ㅋㅋ",
          "original_form": "오 로제떡볶이 저도 최근에 먹었는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014250.1.5",
          "form": "오늘은 아마 회 먹울거같아요!",
          "original_form": "오늘은 아마 회 먹울거같아요!",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014250.1.6",
          "form": "회 드시는구나 부러워요...... ... .",
          "original_form": "회 드시는구나 부러워요...... ... .",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014250.1.7",
          "form": "술도 같이 먹나요? ㅋㅋ",
          "original_form": "술도 같이 먹나요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014250.1.8",
          "form": "당연하죠^^*ㅋㅋㅋ 후 근데 제가 시켜먹고 싶은 가게가 아직 준비중이어서 걱정이에요\r\n설마 오픈을 안하진않을까..",
          "original_form": "당연하죠^^*ㅋㅋㅋ 후 근데 제가 시켜먹고 싶은 가게가 아직 준비중이어서 걱정이에요\r\n설마 오픈을 안하진않을까..",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014250.1.9",
          "form": "헉 저녁 전엔 그래도 오픈하지않을까요",
          "original_form": "헉 저녁 전엔 그래도 오픈하지않을까요",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014250.1.10",
          "form": "전 서브웨이 먹을까 또 고민중",
          "original_form": "전 서브웨이 먹을까 또 고민중",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014250.1.11",
          "form": "아 떡볶이 다 먹었어요? 서브웨이도 맛있죠",
          "original_form": "아 떡볶이 다 먹었어요? 서브웨이도 맛있죠",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014250.1.12",
          "form": "플랫브레드 드세요?",
          "original_form": "플랫브레드 드세요?",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014250.1.13",
          "form": "네네네 저 플랫브레드밖에 안먹어요ㅋㅋㅋㅋ",
          "original_form": "네네네 저 플랫브레드밖에 안먹어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014250.1.14",
          "form": "주로 에그마요+ 플랫브레드",
          "original_form": "주로 에그마요+ 플랫브레드",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014250.1.15",
          "form": "이렇게먹는듯",
          "original_form": "이렇게먹는듯",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014250.1.16",
          "form": "오 저도 에그마요먹어요!ㅋㅋ",
          "original_form": "오 저도 에그마요먹어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014250.1.17",
          "form": "전 매운거좋아해서 페퍼로니추가하고 핫칠리로 먹어요ㅋㅋ",
          "original_form": "전 매운거좋아해서 페퍼로니추가하고 핫칠리로 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014250.1.18",
          "form": "오 페퍼로니는 많으면 많을수록 좋죠",
          "original_form": "오 페퍼로니는 많으면 많을수록 좋죠",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014250.1.19",
          "form": "저는 편식을 많이해서",
          "original_form": "저는 편식을 많이해서",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014250.1.20",
          "form": "사브웨이 먹을때항상 올리브 피클 할라피뇨 다 빼고",
          "original_form": "사브웨이 먹을때항상 올리브 피클 할라피뇨 다 빼고",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014250.1.21",
          "form": "먹어요",
          "original_form": "먹어요",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014250.1.22",
          "form": "ㅋ ㅋ  ㅠ",
          "original_form": "ㅋ ㅋ  ㅠ",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014250.1.23",
          "form": "저는 올리브 토마토 오이..ㅋㅋㅋ 빼는거 넣는거 갯수가 비슷해서 말하기 애매해요ㅋㅋㅋ",
          "original_form": "저는 올리브 토마토 오이..ㅋㅋㅋ 빼는거 넣는거 갯수가 비슷해서 말하기 애매해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014250.1.24",
          "form": "ㅋㅋㅋㅋ 그럴수있죠",
          "original_form": "ㅋㅋㅋㅋ 그럴수있죠",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014250.1.25",
          "form": "배달음식은 자주드세요?",
          "original_form": "배달음식은 자주드세요?",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014250.1.26",
          "form": "음 주 1회는 먹는거 같아요 많으면 주2회?",
          "original_form": "음 주 1회는 먹는거 같아요 많으면 주2회?",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014250.1.27",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014250.1.28",
          "form": "음 저도 주 1회 정도 먹는것같아요",
          "original_form": "음 저도 주 1회 정도 먹는것같아요",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014250.1.29",
          "form": "근데 이제 먹을때마다 항상 떡볶이 ㅋㅋㅋ",
          "original_form": "근데 이제 먹을때마다 항상 떡볶이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014250.1.30",
          "form": "떡볶이를 좋아해서...",
          "original_form": "떡볶이를 좋아해서...",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014250.1.31",
          "form": "아ㅋㅋ 떡볶이 좋아하는 사람들은 진짜 자주 먹더라구요",
          "original_form": "아ㅋㅋ 떡볶이 좋아하는 사람들은 진짜 자주 먹더라구요",
          "speaker_id": "1",
