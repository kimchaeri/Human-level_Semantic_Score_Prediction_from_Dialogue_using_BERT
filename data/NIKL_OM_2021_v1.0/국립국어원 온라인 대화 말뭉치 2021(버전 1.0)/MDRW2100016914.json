      "utterance": [
        {
          "id": "MDRW2100016914.1.1",
          "form": "오늘 저녁으로 점찍어둔 메뉴가 있나요?",
          "original_form": "오늘 저녁으로 점찍어둔 메뉴가 있나요?",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016914.1.2",
          "form": "정말 양장피 먹을까 해요ㅎㅎ",
          "original_form": "정말 양장피 먹을까 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016914.1.3",
          "form": "요새 한동안 안먹어서",
          "original_form": "요새 한동안 안먹어서",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016914.1.4",
          "form": "name2님은 깐풍기?",
          "original_form": "&name2&님은 깐풍기?",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016914.1.5",
          "form": "짜장도 시키실 건가요",
          "original_form": "짜장도 시키실 건가요",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016914.1.6",
          "form": "네 ㅋㅋㅋ 아 죄송해여 누구랑 어떤이야기했는지 헷갈리네요 ㅠ",
          "original_form": "네 ㅋㅋㅋ 아 죄송해여 누구랑 어떤이야기했는지 헷갈리네요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100016914.1.7",
          "form": "짜장 시킬거에요~ㅎㅎ",
          "original_form": "짜장 시킬거에요~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100016914.1.8",
          "form": "ㅎㅎ 굿 초이스네요",
          "original_form": "ㅎㅎ 굿 초이스네요",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100016914.1.9",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100016914.1.10",
          "form": "혹시 백종원선생님 유툽 구독했나요?",
          "original_form": "혹시 백종원선생님 유툽 구독했나요?",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100016914.1.11",
          "form": "아뇨 유튭은 잘 안봐서요",
          "original_form": "아뇨 유튭은 잘 안봐서요",
          "speaker_id": "1",
          "time": "20210826 20:04"
        },
        {
          "id": "MDRW2100016914.1.12",
          "form": "백선생님 예능방송은 봅니다",
          "original_form": "백선생님 예능방송은 봅니다",
          "speaker_id": "1",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100016914.1.13",
          "form": "유툽이 요리얘기가 알찬가요?",
          "original_form": "유툽이 요리얘기가 알찬가요?",
          "speaker_id": "1",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100016914.1.14",
          "form": "아하! 네 ㅋㅋ 구독해놓으면 꾸준히 유익한영상 올라오더라구요~",
          "original_form": "아하! 네 ㅋㅋ 구독해놓으면 꾸준히 유익한영상 올라오더라구요~",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100016914.1.15",
          "form": "요새는 네이버검색보다 유툽영상찾는게 더 효과적인거같아요",
          "original_form": "요새는 네이버검색보다 유툽영상찾는게 더 효과적인거같아요",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100016914.1.16",
          "form": "오오 입짧은햇님만 구독해두고 배고프면 봤는데",
          "original_form": "오오 입짧은햇님만 구독해두고 배고프면 봤는데",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100016914.1.17",
          "form": "주로 쿡방인가요?",
          "original_form": "주로 쿡방인가요?",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100016914.1.18",
          "form": "전사실 장르불문하고 다봐요ㅋㅋ",
          "original_form": "전사실 장르불문하고 다봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100016914.1.19",
          "form": "쿡방 먹방 다 좋아해요",
          "original_form": "쿡방 먹방 다 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100016914.1.20",
          "form": "ㅎㅎ 저도요",
          "original_form": "ㅎㅎ 저도요",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100016914.1.21",
          "form": "하지만 쿡방 뒤에 꼭 시식 잘 잡아줘야 해요",
          "original_form": "하지만 쿡방 뒤에 꼭 시식 잘 잡아줘야 해요",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100016914.1.22",
          "form": "냉부가 없어져서 너무 아쉽네요ㅜ",
          "original_form": "냉부가 없어져서 너무 아쉽네요ㅜ",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100016914.1.23",
          "form": "아 그쵸그쵸 ㅋㅋㅋ",
          "original_form": "아 그쵸그쵸 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100016914.1.24",
          "form": "냉부해 폐지됬나요? 티비안본지 오래되서 몰랐어요!",
          "original_form": "냉부해 폐지됬나요? 티비안본지 오래되서 몰랐어요!",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100016914.1.25",
          "form": "몇년 됐어요ㅜㅜㅜ",
          "original_form": "몇년 됐어요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100016914.1.26",
          "form": "김풍 요리 보면서 무척 웃었는데",
          "original_form": "김풍 요리 보면서 무척 웃었는데",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100016914.1.27",
          "form": "아 그렇구나 ㅜㅜ",
          "original_form": "아 그렇구나 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100016914.1.28",
          "form": "맞아요 ㅋㅋㅋ 김풍님 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 김풍님 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:22"
        }
      ]
