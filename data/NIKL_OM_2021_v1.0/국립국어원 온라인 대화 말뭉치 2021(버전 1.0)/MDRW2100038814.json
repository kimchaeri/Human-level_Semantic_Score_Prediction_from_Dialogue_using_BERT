      "utterance": [
        {
          "id": "MDRW2100038814.1.1",
          "form": "홈트 방금 좀 했는데",
          "original_form": "홈트 방금 좀 했는데",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100038814.1.2",
          "form": "오늘은 몸이 많이 쑤시네요",
          "original_form": "오늘은 몸이 많이 쑤시네요",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038814.1.3",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038814.1.4",
          "form": "하고나면 잠 잘온다아니에요?",
          "original_form": "하고나면 잠 잘온다아니에요?",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038814.1.5",
          "form": "맞긴 한데 오늘은 약간 그른거 같아요",
          "original_form": "맞긴 한데 오늘은 약간 그른거 같아요",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100038814.1.6",
          "form": "안돼요!!!ㅋㅋㅋ너무 무리해서 그런강",
          "original_form": "안돼요!!!ㅋㅋㅋ너무 무리해서 그런강",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100038814.1.7",
          "form": "그냥 스트레칭 하고 있어요",
          "original_form": "그냥 스트레칭 하고 있어요",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100038814.1.8",
          "form": "저는 스트레칭이라도 해야되요ㅜㅜ운동을 전혀 아예 안하고있어서ㅠㅠ",
          "original_form": "저는 스트레칭이라도 해야되요ㅜㅜ운동을 전혀 아예 안하고있어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100038814.1.9",
          "form": "몸 굳은건 아니신가 몰라요...",
          "original_form": "몸 굳은건 아니신가 몰라요...",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100038814.1.10",
          "form": "빨리 자리에서 일어나십셔",
          "original_form": "빨리 자리에서 일어나십셔",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100038814.1.11",
          "form": "굿모닝입니당 ㄱㅋㅋㄱ",
          "original_form": "굿모닝입니당 ㄱㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 06:53"
        },
        {
          "id": "MDRW2100038814.1.12",
          "form": "운동은 오늘1만보자동걷기로 채웠어요ㅋㅋㄱ",
          "original_form": "운동은 오늘1만보자동걷기로 채웠어요ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 06:54"
        },
        {
          "id": "MDRW2100038814.1.13",
          "form": "미션완료",
          "original_form": "미션완료",
          "speaker_id": "1",
          "time": "20210903 06:54"
        },
        {
          "id": "MDRW2100038814.1.14",
          "form": "1만보 자동걷기는 뭔가요?",
          "original_form": "1만보 자동걷기는 뭔가요?",
          "speaker_id": "2",
          "time": "20210903 08:20"
        },
        {
          "id": "MDRW2100038814.1.15",
          "form": "만보걷기 기계가 있는데 거기에다가 폰을 꽂아놓으면",
          "original_form": "만보걷기 기계가 있는데 거기에다가 폰을 꽂아놓으면",
          "speaker_id": "1",
          "time": "20210903 08:36"
        },
        {
          "id": "MDRW2100038814.1.16",
          "form": "기계가 알아서 움직이거든요 위아래나 왼쪽오른쬐",
          "original_form": "기계가 알아서 움직이거든요 위아래나 왼쪽오른쬐",
          "speaker_id": "1",
          "time": "20210903 08:36"
        },
        {
          "id": "MDRW2100038814.1.17",
          "form": "움직이는 횟수에따라서 숫자가 올라가서ㅋㅋㅋ",
          "original_form": "움직이는 횟수에따라서 숫자가 올라가서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100038814.1.18",
          "form": "아침에 일어났을때는 만보채워져있어요",
          "original_form": "아침에 일어났을때는 만보채워져있어요",
          "speaker_id": "1",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100038814.1.19",
          "form": "아 진짜요?? 저는 매번 제가 흔들었어요",
          "original_form": "아 진짜요?? 저는 매번 제가 흔들었어요",
          "speaker_id": "2",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100038814.1.20",
          "form": "만보달성 100원캐시를 얻기위해",
          "original_form": "만보달성 100원캐시를 얻기위해",
          "speaker_id": "1",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100038814.1.21",
          "form": "ㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100038814.1.22",
          "form": "가상운동.....",
          "original_form": "가상운동.....",
          "speaker_id": "1",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100038814.1.23",
          "form": "운동은 코로나끝나고 하면되죠머",
          "original_form": "운동은 코로나끝나고 하면되죠머",
          "speaker_id": "1",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100038814.1.24",
          "form": "캐시워크로 하세요?",
          "original_form": "캐시워크로 하세요?",
          "speaker_id": "2",
          "time": "20210903 08:55"
        },
        {
          "id": "MDRW2100038814.1.25",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:55"
        },
        {
          "id": "MDRW2100038814.1.26",
          "form": "캐시워크 유명하자나요",
          "original_form": "캐시워크 유명하자나요",
          "speaker_id": "1",
          "time": "20210903 08:55"
        },
        {
          "id": "MDRW2100038814.1.27",
          "form": "운동맞음",
          "original_form": "운동맞음",
          "speaker_id": "1",
          "time": "20210903 08:55"
        },
        {
          "id": "MDRW2100038814.1.28",
          "form": "자동운동",
          "original_form": "자동운동",
          "speaker_id": "1",
          "time": "20210903 08:55"
        },
        {
          "id": "MDRW2100038814.1.29",
          "form": "저는 토스도 합해서 하루에 200원 벌고 있지요",
          "original_form": "저는 토스도 합해서 하루에 200원 벌고 있지요",
          "speaker_id": "2",
          "time": "20210903 08:56"
        },
        {
          "id": "MDRW2100038814.1.30",
          "form": "아진짜요? 토스 만보도 해요?난 이거 너무 힘들던데",
          "original_form": "아진짜요? 토스 만보도 해요?난 이거 너무 힘들던데",
          "speaker_id": "1",
