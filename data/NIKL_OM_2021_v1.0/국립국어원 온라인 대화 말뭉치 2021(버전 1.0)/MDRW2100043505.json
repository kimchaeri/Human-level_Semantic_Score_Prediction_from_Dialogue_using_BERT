      "utterance": [
        {
          "id": "MDRW2100043505.1.1",
          "form": "넷플보시나툐",
          "original_form": "넷플보시나툐",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043505.1.2",
          "form": "넷플은 보고싶은거 있을때만",
          "original_form": "넷플은 보고싶은거 있을때만",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043505.1.3",
          "form": "결제해서봐요",
          "original_form": "결제해서봐요",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043505.1.4",
          "form": "최근엔 어떤거보셨어요",
          "original_form": "최근엔 어떤거보셨어요",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043505.1.5",
          "form": "음 마지막으로 가입한게 2월인데요",
          "original_form": "음 마지막으로 가입한게 2월인데요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043505.1.6",
          "form": "뭐봤더라",
          "original_form": "뭐봤더라",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043505.1.7",
          "form": "오리지날인데 제목은기억이안나고",
          "original_form": "오리지날인데 제목은기억이안나고",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043505.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043505.1.9",
          "form": "과학발달에따른 sf단막극이었는데",
          "original_form": "과학발달에따른 sf단막극이었는데",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043505.1.10",
          "form": "아주흥미롭더라구요",
          "original_form": "아주흥미롭더라구요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043505.1.11",
          "form": "오 재밌을것같아요",
          "original_form": "오 재밌을것같아요",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043505.1.12",
          "form": "5편있는데 각각이 다른 에피소드였어요",
          "original_form": "5편있는데 각각이 다른 에피소드였어요",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043505.1.13",
          "form": "넷플 오리지날자주보시나봐여",
          "original_form": "넷플 오리지날자주보시나봐여",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043505.1.14",
          "form": "저도 하도볼게없어서 검색하니까",
          "original_form": "저도 하도볼게없어서 검색하니까",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043505.1.15",
          "form": "추천글이있어서",
          "original_form": "추천글이있어서",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043505.1.16",
          "form": "오리지날은 내가 사랑했던 남자들에게인가,,",
          "original_form": "오리지날은 내가 사랑했던 남자들에게인가,,",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043505.1.17",
          "form": "그거 남주가 잘생겨서",
          "original_form": "그거 남주가 잘생겨서",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043505.1.18",
          "form": "아 ㅋㅋㅋㅋ저도 봤어요 시즌1만",
          "original_form": "아 ㅋㅋㅋㅋ저도 봤어요 시즌1만",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043505.1.19",
          "form": "그 남주나오는건 다봤어요",
          "original_form": "그 남주나오는건 다봤어요",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043505.1.20",
          "form": "아 2는 실망스러웠어요",
          "original_form": "아 2는 실망스러웠어요",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043505.1.21",
          "form": "1보고 기대했는데",
          "original_form": "1보고 기대했는데",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043505.1.22",
          "form": "1만 괜찮았어요",
          "original_form": "1만 괜찮았어요",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043505.1.23",
          "form": "미국하이틴 유교걸인저에겐 초큼 이해못할부분많더라구여",
          "original_form": "미국하이틴 유교걸인저에겐 초큼 이해못할부분많더라구여",
          "speaker_id": "1",
          "time": "20210907 19:24"
        }
      ]
