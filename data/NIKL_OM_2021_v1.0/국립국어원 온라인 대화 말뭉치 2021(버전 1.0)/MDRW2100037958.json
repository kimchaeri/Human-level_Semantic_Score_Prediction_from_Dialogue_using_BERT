      "utterance": [
        {
          "id": "MDRW2100037958.1.1",
          "form": "당근 거래 해 보셨어요?",
          "original_form": "당근 거래 해 보셨어요?",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100037958.1.2",
          "form": "아뇨 당근은 안해봤어요",
          "original_form": "아뇨 당근은 안해봤어요",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100037958.1.3",
          "form": "눈팅만 해봤어요",
          "original_form": "눈팅만 해봤어요",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100037958.1.4",
          "form": "전 몇 번 해봤는데",
          "original_form": "전 몇 번 해봤는데",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100037958.1.5",
          "form": "노력에 비해 돈이 너무 안 되더라고요ㅋㅋ",
          "original_form": "노력에 비해 돈이 너무 안 되더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100037958.1.6",
          "form": "그렇군요 네이버페이 쓰시나요?",
          "original_form": "그렇군요 네이버페이 쓰시나요?",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037958.1.7",
          "form": "내 페이쓰죠~",
          "original_form": "내 페이쓰죠~",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037958.1.8",
          "form": "네이버가 할인도 많이 되고 포인트도 많이 적립해주더라구요",
          "original_form": "네이버가 할인도 많이 되고 포인트도 많이 적립해주더라구요",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037958.1.9",
          "form": "그렇더라구요 네이버 멤버쉽 가입하면",
          "original_form": "그렇더라구요 네이버 멤버쉽 가입하면",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037958.1.10",
          "form": "포인트 더 주는거도 쏠쏠해요",
          "original_form": "포인트 더 주는거도 쏠쏠해요",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037958.1.11",
          "form": "다른데는 포인트 잘 안 쌓이는데",
          "original_form": "다른데는 포인트 잘 안 쌓이는데",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037958.1.12",
          "form": "네이버는 진짜 쏠쏠해요",
          "original_form": "네이버는 진짜 쏠쏠해요",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100037958.1.13",
          "form": "충전해서 쓰면 2.5%적립인가 그렇죠?",
          "original_form": "충전해서 쓰면 2.5%적립인가 그렇죠?",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037958.1.14",
          "form": "그거만 해도 웬만한 신용카드 무찌르겠던데요",
          "original_form": "그거만 해도 웬만한 신용카드 무찌르겠던데요",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037958.1.15",
          "form": "네이버 페이에 진심이시 군요ㅋㅋㅋㅋㅋ",
          "original_form": "네이버 페이에 진심이시 군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037958.1.16",
          "form": "ㅋㅋ그래보이나요 ㅋㅋ",
          "original_form": "ㅋㅋ그래보이나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100037958.1.17",
          "form": "저도 쇼핑에는 진심이라ㅋㅋㅋㅋ",
          "original_form": "저도 쇼핑에는 진심이라ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100037958.1.18",
          "form": "네이버는 최저가 검색하기도 편하고",
          "original_form": "네이버는 최저가 검색하기도 편하고",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037958.1.19",
          "form": "포인트도 좋고",
          "original_form": "포인트도 좋고",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037958.1.20",
          "form": "네 편리해보였어요",
          "original_form": "네 편리해보였어요",
