      "utterance": [
        {
          "id": "MDRW2100046700.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046700.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211105 00:35"
        },
        {
          "id": "MDRW2100046700.1.3",
          "form": "최근에 즐겨하시는 취미가 있으신가요?",
          "original_form": "최근에 즐겨하시는 취미가 있으신가요?",
          "speaker_id": "2",
          "time": "20211105 00:38"
        },
        {
          "id": "MDRW2100046700.1.4",
          "form": "요샌 딱히 코로나때문에 밖에 못 나가서",
          "original_form": "요샌 딱히 코로나때문에 밖에 못 나가서",
          "speaker_id": "1",
          "time": "20211105 00:38"
        },
        {
          "id": "MDRW2100046700.1.5",
          "form": "책 읽고 넷플릭스 봅니다",
          "original_form": "책 읽고 넷플릭스 봅니다",
          "speaker_id": "1",
          "time": "20211105 00:38"
        },
        {
          "id": "MDRW2100046700.1.6",
          "form": "코로나가 전국민 취미 대통합을 이뤄냈네요",
          "original_form": "코로나가 전국민 취미 대통합을 이뤄냈네요",
          "speaker_id": "2",
          "time": "20211105 00:40"
        },
        {
          "id": "MDRW2100046700.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 00:40"
        },
        {
          "id": "MDRW2100046700.1.8",
          "form": "저도 넷플릭스 좋아합니다 ㅎ",
          "original_form": "저도 넷플릭스 좋아합니다 ㅎ",
          "speaker_id": "2",
          "time": "20211105 00:40"
        },
        {
          "id": "MDRW2100046700.1.9",
          "form": "책은 어떤 장르 좋아하세요?",
          "original_form": "책은 어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20211105 00:40"
        },
        {
          "id": "MDRW2100046700.1.10",
          "form": "그냥 수필이나",
          "original_form": "그냥 수필이나",
          "speaker_id": "1",
          "time": "20211105 00:41"
        },
        {
          "id": "MDRW2100046700.1.11",
          "form": "그때그때 베스트셀러 중에",
          "original_form": "그때그때 베스트셀러 중에",
          "speaker_id": "1",
          "time": "20211105 00:42"
        },
        {
          "id": "MDRW2100046700.1.12",
          "form": "한 권씩 사서 봅니당",
          "original_form": "한 권씩 사서 봅니당",
          "speaker_id": "1",
          "time": "20211105 00:42"
        },
        {
          "id": "MDRW2100046700.1.13",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20211105 00:42"
        },
        {
          "id": "MDRW2100046700.1.14",
          "form": "저는 최근에",
          "original_form": "저는 최근에",
          "speaker_id": "2",
          "time": "20211105 00:42"
        },
        {
          "id": "MDRW2100046700.1.15",
          "form": "명상살인이라는",
          "original_form": "명상살인이라는",
          "speaker_id": "2",
          "time": "20211105 00:42"
        },
        {
          "id": "MDRW2100046700.1.16",
          "form": "책을 샀어요!",
          "original_form": "책을 샀어요!",
          "speaker_id": "2",
          "time": "20211105 00:42"
        },
        {
          "id": "MDRW2100046700.1.17",
          "form": "혹시 읽으셨던 것 중에",
          "original_form": "혹시 읽으셨던 것 중에",
          "speaker_id": "2",
          "time": "20211105 00:42"
        },
        {
          "id": "MDRW2100046700.1.18",
          "form": "추천해주실만한 책이 있을까요?",
          "original_form": "추천해주실만한 책이 있을까요?",
          "speaker_id": "2",
          "time": "20211105 00:43"
        },
        {
          "id": "MDRW2100046700.1.19",
          "form": "영원의밤",
          "original_form": "영원의밤",
          "speaker_id": "1",
          "time": "20211105 00:43"
        },
        {
          "id": "MDRW2100046700.1.20",
          "form": "추천드려요",
          "original_form": "추천드려요",
          "speaker_id": "1",
          "time": "20211105 00:43"
        },
        {
          "id": "MDRW2100046700.1.21",
          "form": "읽는 내내 책에서 손을 놓을 수 없어요~^^",
          "original_form": "읽는 내내 책에서 손을 놓을 수 없어요~^^",
          "speaker_id": "1",
          "time": "20211105 00:43"
        },
        {
          "id": "MDRW2100046700.1.22",
          "form": "추리소설인가요?!",
          "original_form": "추리소설인가요?!",
          "speaker_id": "2",
          "time": "20211105 00:44"
        },
        {
          "id": "MDRW2100046700.1.23",
          "form": "발레 지젤에 관한 내용이고",
          "original_form": "발레 지젤에 관한 내용이고",
          "speaker_id": "1",
          "time": "20211105 00:45"
        },
        {
          "id": "MDRW2100046700.1.24",
          "form": "미스테리? 추리라기보단",
          "original_form": "미스테리? 추리라기보단",
          "speaker_id": "1",
          "time": "20211105 00:46"
        },
        {
          "id": "MDRW2100046700.1.25",
          "form": "발레 지젤..?! 서점 가서 한 번 봐야겠어요",
          "original_form": "발레 지젤..?! 서점 가서 한 번 봐야겠어요",
          "speaker_id": "2",
          "time": "20211105 00:46"
        },
        {
          "id": "MDRW2100046700.1.26",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 00:46"
        },
        {
          "id": "MDRW2100046700.1.27",
          "form": "명상살인도 추리소설인가요~",
          "original_form": "명상살인도 추리소설인가요~",
          "speaker_id": "1",
          "time": "20211105 00:48"
        },
        {
          "id": "MDRW2100046700.1.28",
          "form": "네네 추리, 범죄, 심리 뭐 이런 장르에요 ㅎㅎ\r\n",
          "original_form": "네네 추리, 범죄, 심리 뭐 이런 장르에요 ㅎㅎ\r\n",
          "speaker_id": "2",
          "time": "20211105 00:48"
        }
      ]
