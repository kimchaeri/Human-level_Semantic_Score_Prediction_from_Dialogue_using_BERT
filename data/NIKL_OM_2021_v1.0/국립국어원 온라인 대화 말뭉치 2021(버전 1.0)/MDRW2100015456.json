      "utterance": [
        {
          "id": "MDRW2100015456.1.1",
          "form": "초밥왜요?!",
          "original_form": "초밥왜요?!",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015456.1.2",
          "form": "상할까봐요?",
          "original_form": "상할까봐요?",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015456.1.3",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015456.1.4",
          "form": "맛도 좀 불인하고..",
          "original_form": "맛도 좀 불인하고..",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015456.1.5",
          "form": "전 야식한번도 시켜볹적이 엄서서..",
          "original_form": "전 야식한번도 시켜볹적이 엄서서..",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015456.1.6",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015456.1.7",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015456.1.8",
          "form": "번쩍배달이라고",
          "original_form": "번쩍배달이라고",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015456.1.9",
          "form": "요즘은 금방오더라고요",
          "original_form": "요즘은 금방오더라고요",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015456.1.10",
          "form": "참 배달문화는 세계1등인듯...",
          "original_form": "참 배달문화는 세계1등인듯...",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015456.1.11",
          "form": "배달은 1주일에 몇번정도 드세요?",
          "original_form": "배달은 1주일에 몇번정도 드세요?",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015456.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015456.1.13",
          "form": "저 천생연분등급입니다",
          "original_form": "저 천생연분등급입니다",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015456.1.14",
          "form": "배민이요",
          "original_form": "배민이요",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015456.1.15",
          "form": "대답이 되었나요?",
          "original_form": "대답이 되었나요?",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015456.1.16",
          "form": "ㅠㅠ 그게 모에요? 배민도 안써서.. ㅋㅋ\r\n진짜 전 배달 안시켜서여..ㅎㅎㅎ",
          "original_form": "ㅠㅠ 그게 모에요? 배민도 안써서.. ㅋㅋ\r\n진짜 전 배달 안시켜서여..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015456.1.17",
          "form": "ㅋㅋㅋ배달을",
          "original_form": "ㅋㅋㅋ배달을",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015456.1.18",
          "form": "한달에 20번이상 시키는사람에게",
          "original_form": "한달에 20번이상 시키는사람에게",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015456.1.19",
          "form": "주는 등급이랍니다",
          "original_form": "주는 등급이랍니다",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015456.1.20",
          "form": "헉.. 대박... \r\n선생님이라고 불러 되나요?ㅋㅋㅋㅋ",
          "original_form": "헉.. 대박... \r\n선생님이라고 불러 되나요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015456.1.21",
          "form": "거의 매일이네요? ㅋㅋㅋ",
          "original_form": "거의 매일이네요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015456.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015456.1.23",
          "form": "제가 이런사람입니다",
          "original_form": "제가 이런사람입니다",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015456.1.24",
          "form": "맛집 고르는 노하우가 있겠네여? ㅋㅋ",
          "original_form": "맛집 고르는 노하우가 있겠네여? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015456.1.25",
          "form": "ㅋㅋㅋㅋ 배민에서요?",
          "original_form": "ㅋㅋㅋㅋ 배민에서요?",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100015456.1.26",
          "form": "네.. 어플에서요",
          "original_form": "네.. 어플에서요",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100015456.1.27",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100015456.1.28",
          "form": "좀 있는편이죠",
          "original_form": "좀 있는편이죠",
