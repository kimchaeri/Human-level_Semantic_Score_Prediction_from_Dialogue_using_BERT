      "utterance": [
        {
          "id": "MDRW2100042322.1.1",
          "form": "나도 좋아해",
          "original_form": "나도 좋아해",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042322.1.2",
          "form": "나중에 내가 만들어줄게",
          "original_form": "나중에 내가 만들어줄게",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042322.1.3",
          "form": "김치볶음밥에 마요네즈 넣으면",
          "original_form": "김치볶음밥에 마요네즈 넣으면",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042322.1.4",
          "form": "짱맛있어",
          "original_form": "짱맛있어",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042322.1.5",
          "form": "헉 그렇게는 안 먹어봤는데",
          "original_form": "헉 그렇게는 안 먹어봤는데",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042322.1.6",
          "form": "안 느끼해?",
          "original_form": "안 느끼해?",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042322.1.7",
          "form": "니가 만들어 주는것도 맛있겠다",
          "original_form": "니가 만들어 주는것도 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042322.1.8",
          "form": "응 오히려 김치 감칠맛이랑",
          "original_form": "응 오히려 김치 감칠맛이랑",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042322.1.9",
          "form": "ㅎㅎㅎㅎ당근이지",
          "original_form": "ㅎㅎㅎㅎ당근이지",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042322.1.10",
          "form": "잘어울려",
          "original_form": "잘어울려",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042322.1.11",
          "form": "의외다 마요네즈는 불닭이랑 참치랑 연어에만 뿌리는 줄",
          "original_form": "의외다 마요네즈는 불닭이랑 참치랑 연어에만 뿌리는 줄",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042322.1.12",
          "form": "다음에 김치볶음밥 해 먹을 때 뿌려봐야겠다",
          "original_form": "다음에 김치볶음밥 해 먹을 때 뿌려봐야겠다",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042322.1.13",
          "form": "나는 불닭 너무 맵더라",
          "original_form": "나는 불닭 너무 맵더라",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042322.1.14",
          "form": "넌 어때",
          "original_form": "넌 어때",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042322.1.15",
          "form": "나도 매워 흑흑",
          "original_form": "나도 매워 흑흑",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042322.1.16",
          "form": "먹고 나면 다음날 돈고의 심판을 받곤 해",
          "original_form": "먹고 나면 다음날 돈고의 심판을 받곤 해",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042322.1.17",
          "form": "진짜 웃긴다",
          "original_form": "진짜 웃긴다",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042322.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042322.1.19",
          "form": "불닭 까르보 대신먹어",
