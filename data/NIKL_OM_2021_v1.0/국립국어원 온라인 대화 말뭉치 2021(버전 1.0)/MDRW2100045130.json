      "utterance": [
        {
          "id": "MDRW2100045130.1.1",
          "form": "맛집투어 자주하시나여?",
          "original_form": "맛집투어 자주하시나여?",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045130.1.2",
          "form": "네 좋아하는데 요즘은 자주 못가요 ㅜㅜ",
          "original_form": "네 좋아하는데 요즘은 자주 못가요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045130.1.3",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045130.1.4",
          "form": "저두여ㅜ코로나라ㅜㅜ",
          "original_form": "저두여ㅜ코로나라ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045130.1.5",
          "form": "혼밥도잘하시는편이세요?",
          "original_form": "혼밥도잘하시는편이세요?",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045130.1.6",
          "form": "음 .. 혼밥은 거의 안해봤어요...!",
          "original_form": "음 .. 혼밥은 거의 안해봤어요...!",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045130.1.7",
          "form": "혼밥 자주 하시나요?",
          "original_form": "혼밥 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045130.1.8",
          "form": "네 ㅋㅋㅋ웬만한곳은 다갈수있어요",
          "original_form": "네 ㅋㅋㅋ웬만한곳은 다갈수있어요",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045130.1.9",
          "form": "오오 저는 영화나 쇼핑은 혼자서 자주 보고 가는데 밥은 잘 안먹어봤어요",
          "original_form": "오오 저는 영화나 쇼핑은 혼자서 자주 보고 가는데 밥은 잘 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045130.1.10",
          "form": "여행도 혼자자주해서 뭔가 익숙해진거같아여 ㅋㅋㅋㅋㅋ",
          "original_form": "여행도 혼자자주해서 뭔가 익숙해진거같아여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045130.1.11",
          "form": "근데 혼밥이 편할 것 같긴해여 자기가 먹고싶은거 그냥 바로 먹을수있어서요 ㅎㅎ",
          "original_form": "근데 혼밥이 편할 것 같긴해여 자기가 먹고싶은거 그냥 바로 먹을수있어서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045130.1.12",
          "form": "네 ㅎㅎ그게 젤편하고 뭐든계획이 제맘대로할수있으니까여",
          "original_form": "네 ㅎㅎ그게 젤편하고 뭐든계획이 제맘대로할수있으니까여",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045130.1.13",
          "form": "그쵸ㅋㅋ 요즘 혼밥사는 사람들 많더라구요 코로나라 더더욱이요 ㅎㅎ",
          "original_form": "그쵸ㅋㅋ 요즘 혼밥사는 사람들 많더라구요 코로나라 더더욱이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045130.1.14",
          "form": "네 ㅎㅎ맞아여",
          "original_form": "네 ㅎㅎ맞아여",
          "speaker_id": "1",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045130.1.15",
          "form": "주로 외식하실때 뭐드시나여?",
          "original_form": "주로 외식하실때 뭐드시나여?",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045130.1.16",
          "form": "저는 양식이나 일식 자주 먹어요!! 파스타나 초밥 요런거요",
          "original_form": "저는 양식이나 일식 자주 먹어요!! 파스타나 초밥 요런거요",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045130.1.17",
          "form": "어떤거 자주드세요?",
          "original_form": "어떤거 자주드세요?",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045130.1.18",
          "form": "저도 외식할땐 양식이랑일식자주먹어요 ㅎㅎ",
          "original_form": "저도 외식할땐 양식이랑일식자주먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:57"
