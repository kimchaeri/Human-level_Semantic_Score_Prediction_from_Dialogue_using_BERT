      "utterance": [
        {
          "id": "MDRW2100018351.1.1",
          "form": "키우는 동물 있으세요?",
          "original_form": "키우는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100018351.1.2",
          "form": "없어요 키워본 적도 없습니다",
          "original_form": "없어요 키워본 적도 없습니다",
          "speaker_id": "2",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100018351.1.3",
          "form": "안좋아하세요?",
          "original_form": "안좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 06:15"
        },
        {
          "id": "MDRW2100018351.1.4",
          "form": "키우고 싶긴 한데 관리를 잘 해줄 자신이 없어요",
          "original_form": "키우고 싶긴 한데 관리를 잘 해줄 자신이 없어요",
          "speaker_id": "2",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018351.1.5",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018351.1.6",
          "form": "딱 똑같네요",
          "original_form": "딱 똑같네요",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018351.1.7",
          "form": "키워보고 싶은 동물 있으신가요?",
          "original_form": "키워보고 싶은 동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100018351.1.8",
          "form": "저 강아지요",
          "original_form": "저 강아지요",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100018351.1.9",
          "form": "리트리버",
          "original_form": "리트리버",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100018351.1.10",
          "form": "리트리버 귀엽죠 다 크면 조금 크지만요",
          "original_form": "리트리버 귀엽죠 다 크면 조금 크지만요",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100018351.1.11",
          "form": "순둥이라 좋아요",
          "original_form": "순둥이라 좋아요",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100018351.1.12",
          "form": "근데 어릴때 아주  활발 ㅋㅋㅋ",
          "original_form": "근데 어릴때 아주  활발 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100018351.1.13",
          "form": "맞아요 장난꾸러기 그 자체이던데",
          "original_form": "맞아요 장난꾸러기 그 자체이던데",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100018351.1.14",
          "form": "키우기 힘들어보였어요 ㅋㅋㅋ",
          "original_form": "키우기 힘들어보였어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100018351.1.15",
          "form": "유튜브에서 보면서 대리만족하거든요ㅎㅎ",
          "original_form": "유튜브에서 보면서 대리만족하거든요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100018351.1.16",
          "form": "유튜브 반려동물 브이로그가 참 좋죠",
          "original_form": "유튜브 반려동물 브이로그가 참 좋죠",
