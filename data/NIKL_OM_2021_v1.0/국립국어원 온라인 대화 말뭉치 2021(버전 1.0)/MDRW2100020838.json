        {
          "id": "MDRW2100020838.1.4",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020838.1.5",
          "form": "아뇨 저는 솔로에요",
          "original_form": "아뇨 저는 솔로에요",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020838.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020838.1.7",
          "form": "저두 솔로에여 ㅠ",
          "original_form": "저두 솔로에여 ㅠ",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020838.1.8",
          "form": "연애 안하신지 얼마나 되셧어요?",
          "original_form": "연애 안하신지 얼마나 되셧어요?",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020838.1.9",
          "form": "전 모솔이에여ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 모솔이에여ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020838.1.10",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020838.1.11",
          "form": "반갑네요...",
          "original_form": "반갑네요...",
          "speaker_id": "1",
          "time": "20210828 13:20"
        },
        {
          "id": "MDRW2100020838.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:20"
        },
        {
          "id": "MDRW2100020838.1.13",
          "form": "아앗..ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아앗..ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020838.1.14",
          "form": "욘애할 생각 있으세요??",
          "original_form": "욘애할 생각 있으세요??",
          "speaker_id": "2",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020838.1.15",
          "form": "예전엔 있었는데 주변에 사람들 만날환경이 아니라서ㅋㅋㅋㅋ관심이 점점 사라졋어요ㅋㅋㅋㅋ",
          "original_form": "예전엔 있었는데 주변에 사람들 만날환경이 아니라서ㅋㅋㅋㅋ관심이 점점 사라졋어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020838.1.16",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020838.1.17",
          "form": "저두요ㅋㅋㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020838.1.18",
          "form": "만날 여유도 별로 없고",
          "original_form": "만날 여유도 별로 없고",
          "speaker_id": "2",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020838.1.19",
          "form": "아아 그렇군요 ㅠㅠ",
          "original_form": "아아 그렇군요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020838.1.20",
          "form": "전에는 관심이 많으셧나요?",
          "original_form": "전에는 관심이 많으셧나요?",
          "speaker_id": "1",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020838.1.21",
          "form": "제가 고등학교를 공학을 다녀서 그때부터 좀 관심이 없어진거같애요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 고등학교를 공학을 다녀서 그때부터 좀 관심이 없어진거같애요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020838.1.22",
          "form": "약간 환상이 없어져서ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "약간 환상이 없어져서ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020838.1.23",
          "form": "name1님은 결혼생각은 있으세요??",
          "original_form": "&name1&님은 결혼생각은 있으세요??",
          "speaker_id": "2",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020838.1.24",
          "form": "아 그렇군요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 그렇군요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:26"
        },
        {
          "id": "MDRW2100020838.1.25",
          "form": "소개팅은 자주하세요?",
          "original_form": "소개팅은 자주하세요?",
          "speaker_id": "1",
          "time": "20210828 13:26"
        },
        {
          "id": "MDRW2100020838.1.26",
          "form": "아아 아니요 결혼샐각 없어요 ㅠ",
          "original_form": "아아 아니요 결혼샐각 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 13:26"
        },
        {
          "id": "MDRW2100020838.1.27",
          "form": "소개팅 한 번도 해본적 없어요",
          "original_form": "소개팅 한 번도 해본적 없어요",
          "speaker_id": "2",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020838.1.28",
          "form": "저도 결혼생각이 없어서",
          "original_form": "저도 결혼생각이 없어서",
          "speaker_id": "2",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020838.1.29",
          "form": "딱히 안 찾게 되는거같더라구요",
          "original_form": "딱히 안 찾게 되는거같더라구요",
          "speaker_id": "2",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020838.1.30",
          "form": "소개팅 해보셨어요??",
          "original_form": "소개팅 해보셨어요??",
          "speaker_id": "2",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020838.1.31",
          "form": "아니욬ㅋㅋㅋㅋ 저는 자만추입니다..",
          "original_form": "아니욬ㅋㅋㅋㅋ 저는 자만추입니다..",
          "speaker_id": "1",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020838.1.32",
          "form": "근데 그래서 모솔같기두하구...",
          "original_form": "근데 그래서 모솔같기두하구...",
          "speaker_id": "1",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020838.1.33",
          "form": "엌ㅋㅋㅋㅋㅋㅋ 저도 자만추에욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋ 저도 자만추에욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020838.1.34",
          "form": "사귀게 되더라도 충분히 알아보고 만나고싶어서ㅋㅋㅋㅋ",
          "original_form": "사귀게 되더라도 충분히 알아보고 만나고싶어서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020838.1.35",
          "form": "썸타보신적은 있나요??",
          "original_form": "썸타보신적은 있나요??",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020838.1.36",
          "form": "ㅋㅋㅋㅋㅋ친구들보면 소개팅 열심히 하도라규요",
          "original_form": "ㅋㅋㅋㅋㅋ친구들보면 소개팅 열심히 하도라규요",
          "speaker_id": "1",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020838.1.37",
          "form": "약간 애매하게 있어요ㅋㅋㅋㅋㅋ",
          "original_form": "약간 애매하게 있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020838.1.38",
          "form": "근데 저는 약간 짝사랑많이하는 타입 ㅠ",
          "original_form": "근데 저는 약간 짝사랑많이하는 타입 ㅠ",
          "speaker_id": "1",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020838.1.39",
          "form": "아.. 표현을 직접적으로 안 하시는 스타일이신가봐요",
          "original_form": "아.. 표현을 직접적으로 안 하시는 스타일이신가봐요",
