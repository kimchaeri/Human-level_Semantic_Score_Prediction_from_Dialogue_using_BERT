      "utterance": [
        {
          "id": "MDRW2100030737.1.1",
          "form": "좋은 선택이었습니다",
          "original_form": "좋은 선택이었습니다",
          "speaker_id": "2",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030737.1.2",
          "form": "전자레인지도 싸게 샀어요",
          "original_form": "전자레인지도 싸게 샀어요",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030737.1.3",
          "form": "만 원에",
          "original_form": "만 원에",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030737.1.4",
          "form": "저는 전자레인지 사고 싶어요 오래 되어서",
          "original_form": "저는 전자레인지 사고 싶어요 오래 되어서",
          "speaker_id": "2",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030737.1.5",
          "form": "버튼이 잘 안 눌러져요",
          "original_form": "버튼이 잘 안 눌러져요",
          "speaker_id": "2",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030737.1.6",
          "form": "저도 만 원에 팔고 왔어요",
          "original_form": "저도 만 원에 팔고 왔어요",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030737.1.7",
          "form": "어디서 사는게 좋을까요",
          "original_form": "어디서 사는게 좋을까요",
          "speaker_id": "2",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030737.1.8",
          "form": "무슨 버튼 이요",
          "original_form": "무슨 버튼 이요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.9",
          "form": "아 전자레인지",
          "original_form": "아 전자레인지",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.10",
          "form": "누르면 30초 조리되는 간단 버튼 이요",
          "original_form": "누르면 30초 조리되는 간단 버튼 이요",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.11",
          "form": "최저가를 비교해 봐야죠",
          "original_form": "최저가를 비교해 봐야죠",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.12",
          "form": "다른 버튼은 다 잘 눌러지는데 그 버튼만 고장이 났어요",
          "original_form": "다른 버튼은 다 잘 눌러지는데 그 버튼만 고장이 났어요",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.13",
          "form": "전자레인지는 필수품이에요",
          "original_form": "전자레인지는 필수품이에요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.14",
          "form": "10년은 된 것 같아요 얼마나 눌렀으면 그 버튼이 안 될까요",
          "original_form": "10년은 된 것 같아요 얼마나 눌렀으면 그 버튼이 안 될까요",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.15",
          "form": "1분 하다가 멈추면 안 되나요",
          "original_form": "1분 하다가 멈추면 안 되나요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.16",
          "form": "버리기 아깝긴 하네요",
          "original_form": "버리기 아깝긴 하네요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.17",
          "form": "그런 식으로 하고 있어요",
          "original_form": "그런 식으로 하고 있어요",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030737.1.18",
          "form": "10년이면 오래 섰네요",
          "original_form": "10년이면 오래 섰네요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        }
      ]
