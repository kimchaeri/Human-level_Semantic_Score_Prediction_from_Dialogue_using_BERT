      "utterance": [
        {
          "id": "MDRW2100010794.1.1",
          "form": "육퇴했습니까??",
          "original_form": "육퇴했습니까??",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010794.1.2",
          "form": "오늘은 아버님이 재우는날",
          "original_form": "오늘은 아버님이 재우는날",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010794.1.3",
          "form": "육퇴아지그ㅡ",
          "original_form": "육퇴아지그ㅡ",
          "speaker_id": "1",
          "time": "20210823 22:03"
        },
        {
          "id": "MDRW2100010794.1.4",
          "form": "첫째안잔다",
          "original_form": "첫째안잔다",
          "speaker_id": "1",
          "time": "20210823 22:03"
        },
        {
          "id": "MDRW2100010794.1.5",
          "form": "뒹굴거리고ㅡㅡ",
          "original_form": "뒹굴거리고ㅡㅡ",
          "speaker_id": "1",
          "time": "20210823 22:03"
        },
        {
          "id": "MDRW2100010794.1.6",
          "form": "오늘따라 안자네",
          "original_form": "오늘따라 안자네",
          "speaker_id": "1",
          "time": "20210823 22:03"
        },
        {
          "id": "MDRW2100010794.1.7",
          "form": "오잉 ㅎㅎ 오늘 왜이래 늦게 잔대요?",
          "original_form": "오잉 ㅎㅎ 오늘 왜이래 늦게 잔대요?",
          "speaker_id": "2",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010794.1.8",
          "form": "내가 뭐한다고 자꾸 거실에서 소리내서",
          "original_form": "내가 뭐한다고 자꾸 거실에서 소리내서",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010794.1.9",
          "form": "ㅋㅋ못자네",
          "original_form": "ㅋㅋ못자네",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010794.1.10",
          "form": "이제잔다",
          "original_form": "이제잔다",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010794.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 언니가 잘못했네",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 언니가 잘못했네",
          "speaker_id": "2",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010794.1.12",
          "form": "우리원투도 아직 잠이 안들었는가",
          "original_form": "우리원투도 아직 잠이 안들었는가",
          "speaker_id": "2",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010794.1.13",
          "form": "신랑이 나오지 못하고 있는군",
          "original_form": "신랑이 나오지 못하고 있는군",
          "speaker_id": "2",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010794.1.14",
          "form": "니도 얼른자거라ㅋ내일을위해 난 이제설거지 하러 고고",
          "original_form": "니도 얼른자거라ㅋ내일을위해 난 이제설거지 하러 고고",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100010794.1.15",
          "form": "잘잤슈??",
          "original_form": "잘잤슈??",
          "speaker_id": "2",
          "time": "20210824 08:14"
        },
        {
          "id": "MDRW2100010794.1.16",
          "form": "비오니 컴컴하고",
          "original_form": "비오니 컴컴하고",
          "speaker_id": "2",
          "time": "20210824 08:14"
        },
        {
          "id": "MDRW2100010794.1.17",
          "form": "더 자고 싶네;;;",
          "original_form": "더 자고 싶네;;;",
          "speaker_id": "2",
          "time": "20210824 08:14"
        },
        {
          "id": "MDRW2100010794.1.18",
          "form": "출근했슈??",
          "original_form": "출근했슈??",
          "speaker_id": "2",
          "time": "20210824 08:45"
        },
        {
          "id": "MDRW2100010794.1.19",
          "form": "비많이온다ㅡㅡ",
          "original_form": "비많이온다ㅡㅡ",
          "speaker_id": "1",
          "time": "20210824 08:46"
        },
        {
          "id": "MDRW2100010794.1.20",
          "form": "출근했지ㅋ",
          "original_form": "출근했지ㅋ",
          "speaker_id": "1",
          "time": "20210824 08:46"
        },
        {
          "id": "MDRW2100010794.1.21",
          "form": "옹 비 좀 오네유",
          "original_form": "옹 비 좀 오네유",
          "speaker_id": "2",
          "time": "20210824 08:46"
        },
        {
          "id": "MDRW2100010794.1.22",
          "form": "애들은 뭐해요??",
          "original_form": "애들은 뭐해요??",
          "speaker_id": "2",
          "time": "20210824 08:46"
        },
        {
          "id": "MDRW2100010794.1.23",
          "form": "애들 이제방과후가겠지?",
          "original_form": "애들 이제방과후가겠지?",
          "speaker_id": "1",
          "time": "20210824 09:46"
        },
        {
          "id": "MDRW2100010794.1.24",
          "form": "아 방과후 계속 가고 있지",
          "original_form": "아 방과후 계속 가고 있지",
          "speaker_id": "2",
          "time": "20210824 09:48"
        },
        {
          "id": "MDRW2100010794.1.25",
          "form": "우리첫째도 name3네 집 이제 안간다고",
          "original_form": "우리첫째도 &name3&네 집 이제 안간다고",
          "speaker_id": "2",
          "time": "20210824 09:49"
        },
        {
          "id": "MDRW2100010794.1.26",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "2",
          "time": "20210824 09:49"
        },
        {
          "id": "MDRW2100010794.1.27",
          "form": "어제 1시30분쯤에 바로 영어학원갔음",
          "original_form": "어제 1시30분쯤에 바로 영어학원갔음",
          "speaker_id": "2",
          "time": "20210824 09:49"
        },
        {
          "id": "MDRW2100010794.1.28",
          "form": "가서 105동 투 있다고 가서 놀으라 했지\r\nㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "가서 105동 투 있다고 가서 놀으라 했지\r\nㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:49"
        },
        {
          "id": "MDRW2100010794.1.29",
          "form": "같이ㅇ놀았겠네ㅋㅋ어제일찍갔었는데",
          "original_form": "같이ㅇ놀았겠네ㅋㅋ어제일찍갔었는데",
          "speaker_id": "1",
          "time": "20210824 10:04"
        },
        {
          "id": "MDRW2100010794.1.30",
          "form": "ㅋㅋㅋㅋㅋ 오늘도 일찍 가겠죠",
          "original_form": "ㅋㅋㅋㅋㅋ 오늘도 일찍 가겠죠",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100010794.1.31",
          "form": "요즘 무슨 바람이 불었는지",
          "original_form": "요즘 무슨 바람이 불었는지",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100010794.1.32",
          "form": "name3 집에 안간다고 해서",
          "original_form": "&name3& 집에 안간다고 해서",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100010794.1.33",
          "form": "그래?ㅋㅋ",
          "original_form": "그래?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100010794.1.34",
          "form": "왜그러지?",
          "original_form": "왜그러지?",
          "speaker_id": "1",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100010794.1.35",
          "form": "몰라 ㅋㅋㅋㅋㄱ집에 있으니\r\n계속 싸워서 이제 서로의 집에는 안가야겠다고 하던데",
          "original_form": "몰라 ㅋㅋㅋㅋㄱ집에 있으니\r\n계속 싸워서 이제 서로의 집에는 안가야겠다고 하던데",
          "speaker_id": "2",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100010794.1.36",
          "form": "무슨 속셈인지 알수없음",
          "original_form": "무슨 속셈인지 알수없음",
          "speaker_id": "2",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100010794.1.37",
          "form": "우물속을 아는게 낫지",
          "original_form": "우물속을 아는게 낫지",
          "speaker_id": "2",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100010794.1.38",
          "form": "우리아들 마음은 알다가도 모름",
          "original_form": "우리아들 마음은 알다가도 모름",
          "speaker_id": "2",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100010794.1.39",
          "form": "우린 오늘도 영어학원 빨리갈듯",
          "original_form": "우린 오늘도 영어학원 빨리갈듯",
          "speaker_id": "1",
          "time": "20210824 12:01"
        },
        {
          "id": "MDRW2100010794.1.40",
          "form": "나도 우리아이들마음 모른다ㅋㅋㅋ",
          "original_form": "나도 우리아이들마음 모른다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:01"
        },
        {
          "id": "MDRW2100010794.1.41",
          "form": "비와서 겉옷들 우짜노ㅜ",
          "original_form": "비와서 겉옷들 우짜노ㅜ",
          "speaker_id": "1",
          "time": "20210824 12:02"
        },
        {
          "id": "MDRW2100010794.1.42",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100010794.1.43",
          "form": "비가 많이 오네",
          "original_form": "비가 많이 오네",
          "speaker_id": "2",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100010794.1.44",
          "form": "이제비안오는데?",
          "original_form": "이제비안오는데?",
          "speaker_id": "1",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100010794.1.45",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100010794.1.46",
          "form": "거기비오나",
          "original_form": "거기비오나",
          "speaker_id": "1",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100010794.1.47",
