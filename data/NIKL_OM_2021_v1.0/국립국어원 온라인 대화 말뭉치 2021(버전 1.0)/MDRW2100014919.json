      "utterance": [
        {
          "id": "MDRW2100014919.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014919.1.2",
          "form": "여행 자주는 못가지만",
          "original_form": "여행 자주는 못가지만",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014919.1.3",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014919.1.4",
          "form": "지금은 아무래도 코로나때문에 못가는 상황이라",
          "original_form": "지금은 아무래도 코로나때문에 못가는 상황이라",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014919.1.5",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014919.1.6",
          "form": "저도 여행 많이는 안가지만 가면 좋더라구요!",
          "original_form": "저도 여행 많이는 안가지만 가면 좋더라구요!",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014919.1.7",
          "form": "가장 기억 나는 여행지 어디신가요?",
          "original_form": "가장 기억 나는 여행지 어디신가요?",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014919.1.8",
          "form": "제주도요!",
          "original_form": "제주도요!",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014919.1.9",
          "form": "제주도 특유의 감성이 있는 것 같아요",
          "original_form": "제주도 특유의 감성이 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014919.1.10",
          "form": "바다도 가깝고 조용하고",
          "original_form": "바다도 가깝고 조용하고",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014919.1.11",
          "form": "사람많을 땐 시끄럽지만",
          "original_form": "사람많을 땐 시끄럽지만",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014919.1.12",
          "form": "유난히 좀 여유로운 분위기더라구요",
          "original_form": "유난히 좀 여유로운 분위기더라구요",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014919.1.13",
          "form": "아 맞아요 제주도는 그리고 바다 근처에 카페도 많고 관광명소도 특히 많은 거 같아서 좋은 거 같아요",
          "original_form": "아 맞아요 제주도는 그리고 바다 근처에 카페도 많고 관광명소도 특히 많은 거 같아서 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014919.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014919.1.15",
          "form": "전 산, 바다 같은 자연을 좋아해서",
          "original_form": "전 산, 바다 같은 자연을 좋아해서",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014919.1.16",
          "form": "더 잘 맞는 것 같아요",
          "original_form": "더 잘 맞는 것 같아요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014919.1.17",
          "form": "name1님은 기억에 남는 여행지 있으신가요?",
          "original_form": "&name1&님은 기억에 남는 여행지 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014919.1.18",
          "form": "그렇군요! 전 강원도가 기억에 남네요ㅎㅎ",
          "original_form": "그렇군요! 전 강원도가 기억에 남네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014919.1.19",
          "form": "강원도",
          "original_form": "강원도",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014919.1.20",
          "form": "듣기만해도 시원해져요",
          "original_form": "듣기만해도 시원해져요",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014919.1.21",
          "form": "그때 양떼목장을 갔다가 만석 닭강정을 먹었는데 엄청 맛있었던 기억이 나요ㅋㅋㅋ",
          "original_form": "그때 양떼목장을 갔다가 만석 닭강정을 먹었는데 엄청 맛있었던 기억이 나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014919.1.22",
          "form": "만석닭강정",
          "original_form": "만석닭강정",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014919.1.23",
          "form": "인기 엄청 많아보여요",
          "original_form": "인기 엄청 많아보여요",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014919.1.24",
          "form": "이 밤에 닭강정이라니 배고파지네요",
          "original_form": "이 밤에 닭강정이라니 배고파지네요",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014919.1.25",
          "form": "아ㅋㅋㅋ 저도 배고파지네요! 그리고 코로나 끝나면 호캉스도 가고 싶네요!",
          "original_form": "아ㅋㅋㅋ 저도 배고파지네요! 그리고 코로나 끝나면 호캉스도 가고 싶네요!",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100014919.1.26",
          "form": "호캉스 좋죠",
          "original_form": "호캉스 좋죠",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100014919.1.27",
          "form": "코로나 끝나면 할 일 쌓여있네요",
          "original_form": "코로나 끝나면 할 일 쌓여있네요",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100014919.1.28",
          "form": "여행!!",
          "original_form": "여행!!",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100014919.1.29",
          "form": "맞아욬ㅋㅋ 빨리 끝났으면 좋겠어요!",
          "original_form": "맞아욬ㅋㅋ 빨리 끝났으면 좋겠어요!",
          "speaker_id": "1",
          "time": "20210825 22:11"
        }
      ]
