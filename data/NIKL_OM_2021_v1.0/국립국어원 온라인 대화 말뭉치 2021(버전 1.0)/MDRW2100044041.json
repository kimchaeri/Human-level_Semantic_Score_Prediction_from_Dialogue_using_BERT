      "utterance": [
        {
          "id": "MDRW2100044041.1.1",
          "form": "방금 신라호텔 했다가 에코백 떠서",
          "original_form": "방금 신라호텔 했다가 에코백 떠서",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044041.1.2",
          "form": "식사권으로 바꿔뒀어..",
          "original_form": "식사권으로 바꿔뒀어..",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044041.1.3",
          "form": "아닠 허위매물에 걸렷군...",
          "original_form": "아닠 허위매물에 걸렷군...",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044041.1.4",
          "form": "ㅋㅋ큐ㅠㅠ 에코백... 싫다....",
          "original_form": "ㅋㅋ큐ㅠㅠ 에코백... 싫다....",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044041.1.5",
          "form": "에코백을 대량 생산하면 그게 에코백이냐",
          "original_form": "에코백을 대량 생산하면 그게 에코백이냐",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044041.1.6",
          "form": "안티에코백이지",
          "original_form": "안티에코백이지",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044041.1.7",
          "form": "그니까....",
          "original_form": "그니까....",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044041.1.8",
          "form": "디자인도 걍 캔버스 천으로 만들었을뿐인 일반 가방이던데",
          "original_form": "디자인도 걍 캔버스 천으로 만들었을뿐인 일반 가방이던데",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044041.1.9",
          "form": "은근히 거래 잘되나보더라구",
          "original_form": "은근히 거래 잘되나보더라구",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044041.1.10",
          "form": "싱기...",
          "original_form": "싱기...",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044041.1.11",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044041.1.12",
          "form": "제주신라는 곰돌이 키링이 있는거 같아서",
          "original_form": "제주신라는 곰돌이 키링이 있는거 같아서",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044041.1.13",
          "form": "쪼끔이해가는데",
          "original_form": "쪼끔이해가는데",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044041.1.14",
          "form": "에코백이랑 곰돌이 젤 열심히 본듯 ㅇㅇㅋㅋㅋ",
          "original_form": "에코백이랑 곰돌이 젤 열심히 본듯 ㅇㅇㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044041.1.15",
          "form": "(머가 붙어있으니까",
          "original_form": "(머가 붙어있으니까",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044041.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044041.1.17",
          "form": "그게 또 시즌마다 리뉴얼되나보더라구",
          "original_form": "그게 또 시즌마다 리뉴얼되나보더라구",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044041.1.18",
          "form": "아니 기념품 왜케 팔아 싶고 (이거 아님",
          "original_form": "아니 기념품 왜케 팔아 싶고 (이거 아님",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044041.1.19",
          "form": "앟",
          "original_form": "앟",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044041.1.20",
          "form": "그렇군..",
          "original_form": "그렇군..",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044041.1.21",
          "form": "마치 스벅처럼",
          "original_form": "마치 스벅처럼",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044041.1.22",
          "form": "ㅇㅇ 스벅 엠디 유행의 원조?라는 얘기를 봄",
          "original_form": "ㅇㅇ 스벅 엠디 유행의 원조?라는 얘기를 봄",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
