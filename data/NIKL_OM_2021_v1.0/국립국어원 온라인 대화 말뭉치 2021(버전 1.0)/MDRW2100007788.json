      "utterance": [
        {
          "id": "MDRW2100007788.1.1",
          "form": "중고거래도 자주하시나요?",
          "original_form": "중고거래도 자주하시나요?",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007788.1.2",
          "form": "네 오늘도 했어요 당근으라요",
          "original_form": "네 오늘도 했어요 당근으라요",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100007788.1.3",
          "form": "아! 저도 직거래는 당근자주 이용해요ㅎ",
          "original_form": "아! 저도 직거래는 당근자주 이용해요ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100007788.1.4",
          "form": "저렴하고 좋더라고요",
          "original_form": "저렴하고 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100007788.1.5",
          "form": "네 중독처럼 뭐살 것도 없는데 기웃거리기도 해요",
          "original_form": "네 중독처럼 뭐살 것도 없는데 기웃거리기도 해요",
          "speaker_id": "1",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100007788.1.6",
          "form": "맞아요 ㅎㅎ 이런것도 파는구나 하면서",
          "original_form": "맞아요 ㅎㅎ 이런것도 파는구나 하면서",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100007788.1.7",
          "form": "가격비싼건 욕심이 많으시네 하면서 평가하고 ㅎ",
          "original_form": "가격비싼건 욕심이 많으시네 하면서 평가하고 ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100007788.1.8",
          "form": "ㅋㅋㅋ 그쵸ㅋㅋ \r\n저는 쓸떼없이 산거 새제품 팔기도 하구요",
          "original_form": "ㅋㅋㅋ 그쵸ㅋㅋ \r\n저는 쓸떼없이 산거 새제품 팔기도 하구요",
          "speaker_id": "1",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100007788.1.9",
          "form": "잘팔리나요?",
          "original_form": "잘팔리나요?",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100007788.1.10",
          "form": "약속잡는것도 일이라 ㅎ",
          "original_form": "약속잡는것도 일이라 ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100007788.1.11",
          "form": "맞아요 약속잡고도 취소하는 경우도 많고 늦게오는 경우도 많아서ㅠㅠ",
          "original_form": "맞아요 약속잡고도 취소하는 경우도 많고 늦게오는 경우도 많아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100007788.1.12",
          "form": "그니깐요 ㅜ",
          "original_form": "그니깐요 ㅜ",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100007788.1.13",
          "form": "중고거래도 장단점이 있죠^^",
          "original_form": "중고거래도 장단점이 있죠^^",
          "speaker_id": "1",
