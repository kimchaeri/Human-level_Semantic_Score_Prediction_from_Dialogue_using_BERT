        {
          "id": "MDRW2100019969.1.1",
          "form": "책좋아하시나봐요!",
          "original_form": "책좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019969.1.2",
          "form": "사는걸 좋아해요ㅋ",
          "original_form": "사는걸 좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019969.1.3",
          "form": "아 ㅋㅋ 최근 어떤책사셧나요?",
          "original_form": "아 ㅋㅋ 최근 어떤책사셧나요?",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019969.1.4",
          "form": "어떻게말해줘야할까",
          "original_form": "어떻게말해줘야할까",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019969.1.5",
          "form": "육아책이에여",
          "original_form": "육아책이에여",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019969.1.6",
          "form": "아하~ 오 출산하신지 얼마안되셨나봐요!!",
          "original_form": "아하~ 오 출산하신지 얼마안되셨나봐요!!",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019969.1.7",
          "form": "아녀 10살ㅎ",
          "original_form": "아녀 10살ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019969.1.8",
          "form": "아 그렇구나!! 아들인가요?",
          "original_form": "아 그렇구나!! 아들인가요?",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100019969.1.9",
          "form": "네 아들이여",
          "original_form": "네 아들이여",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019969.1.10",
          "form": "축하드려요! 이게맞나모르겟어요 ㅋㅋ",
          "original_form": "축하드려요! 이게맞나모르겟어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100019969.1.11",
          "form": "ㅋㅋ 그러게요",
          "original_form": "ㅋㅋ 그러게요",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019969.1.12",
          "form": "ㅋㅋㅋ 아참 좋아하시는 작가 잇나요?",
          "original_form": "ㅋㅋㅋ 아참 좋아하시는 작가 잇나요?",
