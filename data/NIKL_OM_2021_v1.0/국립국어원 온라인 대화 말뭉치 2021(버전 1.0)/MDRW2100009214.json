      "utterance": [
        {
          "id": "MDRW2100009214.1.1",
          "form": "요새 온라인 수업인가요?",
          "original_form": "요새 온라인 수업인가요?",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009214.1.2",
          "form": "코로나때문에 그렇지않나요?",
          "original_form": "코로나때문에 그렇지않나요?",
          "speaker_id": "1",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009214.1.3",
          "form": "아니다 아직 방학인가 ㅋㅋ",
          "original_form": "아니다 아직 방학인가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009214.1.4",
          "form": "가물가물해서  ㅋㅋㅋ",
          "original_form": "가물가물해서  ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009214.1.5",
          "form": "저도요ㅎ",
          "original_form": "저도요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009214.1.6",
          "form": "오래되서",
          "original_form": "오래되서",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009214.1.7",
          "form": "아 그거 신선했어요",
          "original_form": "아 그거 신선했어요",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009214.1.8",
          "form": "요새 야자가 9시까지라고",
          "original_form": "요새 야자가 9시까지라고",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009214.1.9",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009214.1.10",
          "form": "그리고 야자도 진짜 자율인거 같고요 ㅎㅎ",
          "original_form": "그리고 야자도 진짜 자율인거 같고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009214.1.11",
          "form": "그럼하긴할까요?",
          "original_form": "그럼하긴할까요?",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009214.1.12",
          "form": "오히려 벌점 많이 받으면 야자를 못하게 하더라고요",
          "original_form": "오히려 벌점 많이 받으면 야자를 못하게 하더라고요",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009214.1.13",
          "form": "하긴 요샌 애들이 더 챙기죠",
          "original_form": "하긴 요샌 애들이 더 챙기죠",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009214.1.14",
          "form": "뭔가 넌 문제아 아웃",
          "original_form": "뭔가 넌 문제아 아웃",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009214.1.15",
          "form": "이런 느낌이기도하고 ㅋㅋ",
          "original_form": "이런 느낌이기도하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009214.1.16",
          "form": "애들도 힘들겟어여ㅋ",
          "original_form": "애들도 힘들겟어여ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009214.1.17",
          "form": "그래도 학생이것 자체가 부럽기도해요ㆍ",
          "original_form": "그래도 학생이것 자체가 부럽기도해요ㆍ",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009214.1.18",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009214.1.19",
          "form": "그건 ㄸᆢ그렇네여ㅎ",
          "original_form": "그건 ㄸᆢ그렇네여ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:04"
        }
