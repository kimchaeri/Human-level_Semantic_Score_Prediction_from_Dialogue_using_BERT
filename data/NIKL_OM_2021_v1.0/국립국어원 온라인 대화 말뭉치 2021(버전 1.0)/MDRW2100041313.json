      "utterance": [
        {
          "id": "MDRW2100041313.1.1",
          "form": "네네 아니면 아예 계정을 따로",
          "original_form": "네네 아니면 아예 계정을 따로",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041313.1.2",
          "form": "파셔도 되구요!!!",
          "original_form": "파셔도 되구요!!!",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041313.1.3",
          "form": "저 대학원에서 방학동안 인턴이었던 후배가",
          "original_form": "저 대학원에서 방학동안 인턴이었던 후배가",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041313.1.4",
          "form": "그렇게 해서 쿠폰 짱많이 받고",
          "original_form": "그렇게 해서 쿠폰 짱많이 받고",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041313.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041313.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041313.1.7",
          "form": "에너지바 이런것도 협찬받고 그러더라구요",
          "original_form": "에너지바 이런것도 협찬받고 그러더라구요",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041313.1.8",
          "form": "사실 진짜 식당! 이런건",
          "original_form": "사실 진짜 식당! 이런건",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041313.1.9",
          "form": "블로그 협찬이 많은데",
          "original_form": "블로그 협찬이 많은데",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041313.1.10",
          "form": "자잘자잘한 디저트 같은건 인스타가 많은거같아요",
          "original_form": "자잘자잘한 디저트 같은건 인스타가 많은거같아요",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100041313.1.11",
          "form": "ㅋㅋㅋㅋㅋ아 근데 정말 에센에스는",
          "original_form": "ㅋㅋㅋㅋㅋ아 근데 정말 에센에스는",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100041313.1.12",
          "form": "부지런함의 다른말입니다",
          "original_form": "부지런함의 다른말입니다",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100041313.1.13",
          "form": "진짜 부지런하지 않으면 할수가 없어요",
          "original_form": "진짜 부지런하지 않으면 할수가 없어요",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100041313.1.14",
          "form": "아 ㅋㅋㅋㅋㅋ 아 그런건가요?!",
          "original_form": "아 ㅋㅋㅋㅋㅋ 아 그런건가요?!",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041313.1.15",
          "form": "이건 좀 다른 이야긴데 아까",
          "original_form": "이건 좀 다른 이야긴데 아까",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041313.1.16",
          "form": "루프탑 카페에서 친구랑 먹고 사진찍고 이러눈데",
          "original_form": "루프탑 카페에서 친구랑 먹고 사진찍고 이러눈데",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041313.1.17",
          "form": "옆테이블에 여자분들 4명 정도 있었거든요",
          "original_form": "옆테이블에 여자분들 4명 정도 있었거든요",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041313.1.18",
          "form": "근데 진짜..... 사진을 연속촬영을 1시간 내내 찍으시더라구요...",
          "original_form": "근데 진짜..... 사진을 연속촬영을 1시간 내내 찍으시더라구요...{emoji:☆}",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100041313.1.19",
          "form": "커피..식어요..",
          "original_form": "커피..식어요..",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041313.1.20",
          "form": "드세요....",
          "original_form": "드세요....",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041313.1.21",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041313.1.22",
          "form": "아니진짜 음식을 안먹고",
          "original_form": "아니진짜 음식을 안먹고",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041313.1.23",
          "form": "계속 소품으로만 쓰시더라구요...",
          "original_form": "계속 소품으로만 쓰시더라구요...",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100041313.1.24",
          "form": "정말 대단했어요....",
          "original_form": "정말 대단했어요....",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100041313.1.25",
          "form": "약간 거기 협찬으로 오셨나..?",
          "original_form": "약간 거기 협찬으로 오셨나..?",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041313.1.26",
          "form": "그리고 드시긴 했나요..?(궁금",
          "original_form": "그리고 드시긴 했나요..?(궁금",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041313.1.27",
          "form": "아 진짜 그럴......지도 모르겠네요 흠 워낙 핫플인데",
          "original_form": "아 진짜 그럴......지도 모르겠네요 흠 워낙 핫플인데",
          "speaker_id": "2",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041313.1.28",
          "form": "그리고 오늘 느낀 건",
          "original_form": "그리고 오늘 느낀 건",
          "speaker_id": "2",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041313.1.29",
          "form": "확실히 밖에서 먹으면 다 돈이란 거였습니다,,",
          "original_form": "확실히 밖에서 먹으면 다 돈이란 거였습니다,,",
          "speaker_id": "2",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041313.1.30",
          "form": "맞습니다..",
          "original_form": "맞습니다..",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100041313.1.31",
          "form": "너무 살인적인 물가지 않습니까?",
          "original_form": "너무 살인적인 물가지 않습니까?",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100041313.1.32",
          "form": "맞아요... 이러다 정말",
          "original_form": "맞아요... 이러다 정말",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100041313.1.33",
          "form": "솔직히 이나라 물가 미11쳤습니저",
          "original_form": "솔직히 이나라 물가 미11쳤습니저",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100041313.1.34",
          "form": "다",
          "original_form": "다",
