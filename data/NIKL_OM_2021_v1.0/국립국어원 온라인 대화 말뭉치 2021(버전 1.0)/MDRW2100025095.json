      "utterance": [
        {
          "id": "MDRW2100025095.1.1",
          "form": "아니 그게 뭐야??",
          "original_form": "아니 그게 뭐야??",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.2",
          "form": "한달 결제하면 그곳에 있는 책들을 읽을 수 있는 어플이야",
          "original_form": "한달 결제하면 그곳에 있는 책들을 읽을 수 있는 어플이야",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.3",
          "form": "서점이름인가?",
          "original_form": "서점이름인가?",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.4",
          "form": "나는 그걸 결제했어",
          "original_form": "나는 그걸 결제했어",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.5",
          "form": "오 어플이름이구나",
          "original_form": "오 어플이름이구나",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.6",
          "form": "사실은 3개월 무료로 쓰고 있어",
          "original_form": "사실은 3개월 무료로 쓰고 있어",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.7",
          "form": "그건 한달에 얼마정도해??",
          "original_form": "그건 한달에 얼마정도해??",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.8",
          "form": "나는 책을 좋아하거든",
          "original_form": "나는 책을 좋아하거든",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.9",
          "form": "잘 몰라. 난 항상 무료로 썼어",
          "original_form": "잘 몰라. 난 항상 무료로 썼어",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.10",
          "form": "이벤트를 자주해.",
          "original_form": "이벤트를 자주해.",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.11",
          "form": "그러게 무료로 쓰면 얼마인지 모르겠구나",
          "original_form": "그러게 무료로 쓰면 얼마인지 모르겠구나",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.12",
          "form": "그럼 폰으로 읽는고야?",
          "original_form": "그럼 폰으로 읽는고야?",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025095.1.13",
          "form": "근데 난 책을 읽을 시기는 아닌데 무료로 준대서 가입했어",
          "original_form": "근데 난 책을 읽을 시기는 아닌데 무료로 준대서 가입했어",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025095.1.14",
          "form": "시간을 내어 읽어야해",
          "original_form": "시간을 내어 읽어야해",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025095.1.15",
          "form": "응 아이패드도 좋아",
          "original_form": "응 아이패드도 좋아",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025095.1.16",
          "form": "폰이 누워서 읽기엔 딱이더라",
          "original_form": "폰이 누워서 읽기엔 딱이더라",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025095.1.17",
          "form": "나도 가끔 패드로 책을 읽어",
          "original_form": "나도 가끔 패드로 책을 읽어",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025095.1.18",
          "form": "책이 나름 많아 거기엔!",
          "original_form": "책이 나름 많아 거기엔!",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025095.1.19",
          "form": "너도 책을 좋아하는구나",
          "original_form": "너도 책을 좋아하는구나",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025095.1.20",
          "form": "응! 다음에 어플을 써봐야겠어",
          "original_form": "응! 다음에 어플을 써봐야겠어",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025095.1.21",
          "form": "응 근데 눈건강 조심ㅎ9",
          "original_form": "응 근데 눈건강 조심ㅎ9",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025095.1.22",
          "form": "너는 읽기싫은데 꼭 읽고싶은 책 있니?",
          "original_form": "너는 읽기싫은데 꼭 읽고싶은 책 있니?",
          "speaker_id": "2",
          "time": "20210831 23:28"
        }
      ]
