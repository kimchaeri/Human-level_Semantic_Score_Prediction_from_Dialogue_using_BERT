      "utterance": [
        {
          "id": "MDRW2100013601.1.1",
          "form": "저도 추천해드릴까하는데",
          "original_form": "저도 추천해드릴까하는데",
          "speaker_id": "1",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013601.1.2",
          "form": "부산오실일 있으신가요?",
          "original_form": "부산오실일 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013601.1.3",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013601.1.4",
          "form": "저 이번주에 부산가요!!",
          "original_form": "저 이번주에 부산가요!!",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013601.1.5",
          "form": "우와!",
          "original_form": "우와!",
          "speaker_id": "1",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013601.1.6",
          "form": "해운대쪽 가시나요?",
          "original_form": "해운대쪽 가시나요?",
          "speaker_id": "1",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013601.1.7",
          "form": "해운대나 서면 갈것같아요!",
          "original_form": "해운대나 서면 갈것같아요!",
          "speaker_id": "2",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013601.1.8",
          "form": "장어 싫어하시는거 아니시면",
          "original_form": "장어 싫어하시는거 아니시면",
          "speaker_id": "1",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013601.1.9",
          "form": "해운대에 해목이라고 유명해서 아실거같긴한데",
          "original_form": "해운대에 해목이라고 유명해서 아실거같긴한데",
          "speaker_id": "1",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013601.1.10",
          "form": "장어 좋죠!",
          "original_form": "장어 좋죠!",
          "speaker_id": "2",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013601.1.11",
          "form": "저 아무것도 몰라요..ㅠㅠ",
          "original_form": "저 아무것도 몰라요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100013601.1.12",
          "form": "장어덮밥 맛있어요",
          "original_form": "장어덮밥 맛있어요",
          "speaker_id": "1",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100013601.1.13",
          "form": "다 추천해주세요!!",
          "original_form": "다 추천해주세요!!",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100013601.1.14",
          "form": "오.>!!",
          "original_form": "오.>!!",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100013601.1.15",
          "form": "연어 좋아하시면 가볍게 서면에 노르웨이식당이라는곳이 좋구요",
          "original_form": "연어 좋아하시면 가볍게 서면에 노르웨이식당이라는곳이 좋구요",
          "speaker_id": "1",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100013601.1.16",
          "form": "요즘코로나라 안가서 아직 있는지 모르겠는데  서면 모루카레라고 카레맛있는곳도 있어요",
          "original_form": "요즘코로나라 안가서 아직 있는지 모르겠는데  서면 모루카레라고 카레맛있는곳도 있어요",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100013601.1.17",
          "form": "오......",
          "original_form": "오......",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100013601.1.18",
          "form": "저 열심히 메모하고있어요",
          "original_form": "저 열심히 메모하고있어요",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100013601.1.19",
          "form": "원래는 부산역에 하이디라오? 거기가고싶었는데 사람 많다구해서 망설여지더라구요",
          "original_form": "원래는 부산역에 하이디라오? 거기가고싶었는데 사람 많다구해서 망설여지더라구요",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100013601.1.20",
          "form": "하이디라오는 지점이 많아서 다른지점가시는게 더. 좋으실수도 있어요",
          "original_form": "하이디라오는 지점이 많아서 다른지점가시는게 더. 좋으실수도 있어요",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013601.1.21",
          "form": "부산역근처는 그집이라고 곱도리탕 맛집이 있어요",
          "original_form": "부산역근처는 그집이라고 곱도리탕 맛집이 있어요",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013601.1.22",
          "form": "서울이 많긴하죠..ㅠㅠ",
          "original_form": "서울이 많긴하죠..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013601.1.23",
          "form": "옹...",
          "original_form": "옹...",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013601.1.24",
          "form": "전 해운대하면 그 초록만두였나... 거기밖에 기억안나요...ㅎㅎ",
          "original_form": "전 해운대하면 그 초록만두였나... 거기밖에 기억안나요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013601.1.25",
          "form": "너무나도 오래된...ㅠ",
          "original_form": "너무나도 오래된...ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013601.1.26",
          "form": "오신지 오래되셨구나",
          "original_form": "오신지 오래되셨구나",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013601.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013601.1.28",
          "form": "팩폭을....",
          "original_form": "팩폭을....",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013601.1.29",
          "form": "헉 팩폭이였나요",
          "original_form": "헉 팩폭이였나요",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013601.1.30",
          "form": "맞았네요....",
          "original_form": "맞았네요....",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013601.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013601.1.32",
          "form": "안간지 오래되긴했어요..ㅠㅠ",
          "original_form": "안간지 오래되긴했어요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013601.1.33",
          "form": "서울분이신거 같은데 당연히 부산 자주오기 힘들죠",
          "original_form": "서울분이신거 같은데 당연히 부산 자주오기 힘들죠",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013601.1.34",
          "form": "7년..?",
          "original_form": "7년..?",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013601.1.35",
          "form": "저 고향은 부산이에요!",
          "original_form": "저 고향은 부산이에요!",
