        },
        {
          "id": "MDRW2100012559.1.2",
          "form": "안녕하세요 어떤음식 좋아하세요?",
          "original_form": "안녕하세요 어떤음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012559.1.3",
          "form": "저는 다좋아요",
          "original_form": "저는 다좋아요",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012559.1.4",
          "form": "치킨좋아하세요?",
          "original_form": "치킨좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012559.1.5",
          "form": "네 오늘도 먹었어요",
          "original_form": "네 오늘도 먹었어요",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012559.1.6",
          "form": "어떤브랜드 좋아하세요?",
          "original_form": "어떤브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012559.1.7",
          "form": "전 굽네치킨 좋아해요",
          "original_form": "전 굽네치킨 좋아해요",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012559.1.8",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012559.1.9",
          "form": "전 페리카나요",
          "original_form": "전 페리카나요",
