      "utterance": [
        {
          "id": "MDRW2100025383.1.1",
          "form": "중고거래 해보셨나요?",
          "original_form": "중고거래 해보셨나요?",
          "speaker_id": "1",
          "time": "20210901 10:34"
        },
        {
          "id": "MDRW2100025383.1.2",
          "form": "네네!! 저최근에도",
          "original_form": "네네!! 저최근에도",
          "speaker_id": "2",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025383.1.3",
          "form": "에어팟 팔았어요",
          "original_form": "에어팟 팔았어요",
          "speaker_id": "2",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025383.1.4",
          "form": "와우.. 대단해요~",
          "original_form": "와우.. 대단해요~",
          "speaker_id": "1",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025383.1.5",
          "form": "전 당근마켓 하는데 님은요??",
          "original_form": "전 당근마켓 하는데 님은요??",
          "speaker_id": "1",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025383.1.6",
          "form": "저도 당근마켓해요",
          "original_form": "저도 당근마켓해요",
          "speaker_id": "2",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025383.1.7",
          "form": "저는 소소하게 물건들 팔려고 시작했는데 보다보면  사고픈 게 막 생겨요",
          "original_form": "저는 소소하게 물건들 팔려고 시작했는데 보다보면  사고픈 게 막 생겨요",
          "speaker_id": "1",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100025383.1.8",
          "form": "맞어요 파는거보다 사는게 많아지는 ㅠㅠㅠ",
          "original_form": "맞어요 파는거보다 사는게 많아지는 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100025383.1.9",
          "form": "제가 그래서 몇개 샀다가.. 요새는 자제하고 열심히 팔고만 있어요..ㅎㅎㅎ",
          "original_form": "제가 그래서 몇개 샀다가.. 요새는 자제하고 열심히 팔고만 있어요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025383.1.10",
          "form": "물론 전 저렴하게 내놔서 많이 팔아도 돈은 안되지만요",
          "original_form": "물론 전 저렴하게 내놔서 많이 팔아도 돈은 안되지만요",
          "speaker_id": "1",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025383.1.11",
          "form": "저는 주로  기프티콘팔아요 ㅠㅠ",
          "original_form": "저는 주로  기프티콘팔아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:39"
        },
        {
          "id": "MDRW2100025383.1.12",
          "form": "진짜 용돈 같은 느낌...?",
          "original_form": "진짜 용돈 같은 느낌...?",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025383.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025383.1.14",
          "form": "아~ 기프티콘은 한번도 도전안해봤네요..",
          "original_form": "아~ 기프티콘은 한번도 도전안해봤네요..",
          "speaker_id": "1",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025383.1.15",
          "form": "그거 잘 가려서 팔아야한다던에..",
          "original_form": "그거 잘 가려서 팔아야한다던에..",
          "speaker_id": "1",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025383.1.16",
          "form": "만나서 주고 받는 거죠? 카톡으로?",
          "original_form": "만나서 주고 받는 거죠? 카톡으로?",
          "speaker_id": "1",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025383.1.17",
          "form": "아니요 그냥 채팅으로 돈받으면 바코드를 주는식이예오",
          "original_form": "아니요 그냥 채팅으로 돈받으면 바코드를 주는식이예오",
          "speaker_id": "2",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025383.1.18",
          "form": "아.. 그래요? 신기하네요",
          "original_form": "아.. 그래요? 신기하네요",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025383.1.19",
          "form": "저는 당근에서 카세트 테이프 팔았어요....ㅎㅎ",
          "original_form": "저는 당근에서 카세트 테이프 팔았어요....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025383.1.20",
          "form": "믿음으로 파는거죠머..ㅎ",
          "original_form": "믿음으로 파는거죠머..ㅎ",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025383.1.21",
          "form": "요즘 복고열풍이라서 은근히 수요가 있더라구요",
          "original_form": "요즘 복고열풍이라서 은근히 수요가 있더라구요",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025383.1.22",
          "form": "레트로 감성이네요 요즘그런게 유행이자나요",
          "original_form": "레트로 감성이네요 요즘그런게 유행이자나요",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025383.1.23",
          "form": "그니까요.. 씨디도 좀 나가더라구요..",
          "original_form": "그니까요.. 씨디도 좀 나가더라구요..",
          "speaker_id": "1",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100025383.1.24",
          "form": "전 폰으로 마니 해서 그런지",
          "original_form": "전 폰으로 마니 해서 그런지",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025383.1.25",
          "form": "씨디 나테이프는 조끔어렵더라규여 옛날에는",
          "original_form": "씨디 나테이프는 조끔어렵더라규여 옛날에는",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025383.1.26",
          "form": "연필로 돌려서 쓰고 그랬는데 ㅋㅋㅋ",
          "original_form": "연필로 돌려서 쓰고 그랬는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025383.1.27",
          "form": "맞아요.. ㅎㅎ",
          "original_form": "맞아요.. ㅎㅎ",
