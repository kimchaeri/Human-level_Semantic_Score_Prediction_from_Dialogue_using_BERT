        {
          "id": "MDRW2100019080.1.1",
          "form": "저도 감자칩 좋아해요",
          "original_form": "저도 감자칩 좋아해요",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019080.1.2",
          "form": "특히 콰삭칩...진짜 맛있어요",
          "original_form": "특히 콰삭칩...진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019080.1.3",
          "form": "콰삭칩 맛있나요?",
          "original_form": "콰삭칩 맛있나요?",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019080.1.4",
          "form": "네! 꼭 드셔보세요",
          "original_form": "네! 꼭 드셔보세요",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019080.1.5",
          "form": "콰사콰삭한 식감인가요",
          "original_form": "콰사콰삭한 식감인가요",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019080.1.6",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019080.1.7",
          "form": "다른 간식은 뭐좋아하세요",
          "original_form": "다른 간식은 뭐좋아하세요",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019080.1.8",
          "form": "사탕류는 별로 안좋아해요",
          "original_form": "사탕류는 별로 안좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019080.1.9",
          "form": "아이스크림은요?",
          "original_form": "아이스크림은요?",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019080.1.10",
          "form": "아이스크림좋아하죠",
          "original_form": "아이스크림좋아하죠",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019080.1.11",
          "form": "유제품좋아하세요?",
          "original_form": "유제품좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019080.1.12",
          "form": "넹ㅋㅋ유제품 진짜좋아해요",
          "original_form": "넹ㅋㅋ유제품 진짜좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019080.1.13",
          "form": "저둔데 초콜릿도 좋아해요",
          "original_form": "저둔데 초콜릿도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019080.1.14",
          "form": "파베초콜릿 먹고싶네요",
          "original_form": "파베초콜릿 먹고싶네요",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
