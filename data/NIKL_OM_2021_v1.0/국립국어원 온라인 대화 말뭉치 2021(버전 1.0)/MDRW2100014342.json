      "utterance": [
        {
          "id": "MDRW2100014342.1.1",
          "form": "나는 장봤어..",
          "original_form": "나는 장봤어..",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014342.1.2",
          "form": "affiliation 다니는 형부동생찬스로",
          "original_form": "&affiliation& 다니는 형부동생찬스로",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014342.1.3",
          "form": "3마넌에 졸라마니담았어",
          "original_form": "3마넌에 졸라마니담았어",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014342.1.4",
          "form": "아차차 엄청 많이 담았어",
          "original_form": "아차차 엄청 많이 담았어",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014342.1.5",
          "form": "와 형부동생 affiliation다녀?",
          "original_form": "와 형부동생 &affiliation&다녀?",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014342.1.6",
          "form": "아니 affiliation..",
          "original_form": "아니 &affiliation&..",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014342.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014342.1.8",
          "form": "근데 왜 인터넷으로 만두사?",
          "original_form": "근데 왜 인터넷으로 만두사?",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014342.1.9",
          "form": "김치만두 산 거 맛있니",
          "original_form": "김치만두 산 거 맛있니",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014342.1.10",
          "form": "ㅎ..풀무원은 아니잖아",
          "original_form": "ㅎ..풀무원은 아니잖아",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014342.1.11",
          "form": "진짜맛있어",
          "original_form": "진짜맛있어",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014342.1.12",
          "form": "근데 고기깻잎만두 산게 더 맛있어",
          "original_form": "근데 고기깻잎만두 산게 더 맛있어",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014342.1.13",
          "form": "나도 저번에 그 이벤트한거",
          "original_form": "나도 저번에 그 이벤트한거",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014342.1.14",
          "form": "오맞아 그거 고기깻잎만두 산 거 집에있는데",
          "original_form": "오맞아 그거 고기깻잎만두 산 거 집에있는데",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014342.1.15",
          "form": "내일 먹고 맛있으면 또 사야지",
          "original_form": "내일 먹고 맛있으면 또 사야지",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014342.1.16",
          "form": "ㅋㅋㅋ그냥고기보다 맛있어",
          "original_form": "ㅋㅋㅋ그냥고기보다 맛있어",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014342.1.17",
          "form": "완전 핫딜이어서 만족이야",
          "original_form": "완전 핫딜이어서 만족이야",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014342.1.18",
          "form": "하남주꾸미 먹고싶었는데",
          "original_form": "하남주꾸미 먹고싶었는데",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014342.1.19",
          "form": "사고싶었는데",
          "original_form": "사고싶었는데",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014342.1.20",
          "form": "다음에 신기루급 핫딜 뜨면 알려드림",
          "original_form": "다음에 신기루급 핫딜 뜨면 알려드림",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014342.1.21",
          "form": "근데 보통 지에스몰 할인이 많더라",
          "original_form": "근데 보통 지에스몰 할인이 많더라",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014342.1.22",
          "form": "지에스 우리집에 배달이 안돼서",
          "original_form": "지에스 우리집에 배달이 안돼서",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014342.1.23",
          "form": "살수가없어",
          "original_form": "살수가없어",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014342.1.24",
          "form": "또륵 그래서 스마일클럽 할인할때 샀어야..",
          "original_form": "또륵 그래서 스마일클럽 할인할때 샀어야..",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014342.1.25",
          "form": "지마켓 그거 가입하면",
          "original_form": "지마켓 그거 가입하면",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014342.1.26",
          "form": "쇼핑할 때 좋나",
          "original_form": "쇼핑할 때 좋나",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014342.1.27",
          "form": "예전엔 진짜 조았는대",
          "original_form": "예전엔 진짜 조았는대",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014342.1.28",
          "form": "지금은 평타야",
          "original_form": "지금은 평타야",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014342.1.29",
          "form": "오 참고할게",
          "original_form": "오 참고할게",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014342.1.30",
          "form": "우산을 살까봐",
          "original_form": "우산을 살까봐",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100014342.1.31",
          "form": "내가 젤 아끼는 우산 구구남이 뽀리고 그 다음 건 헬스장에서 뽀려짐",
          "original_form": "내가 젤 아끼는 우산 구구남이 뽀리고 그 다음 건 헬스장에서 뽀려짐",
          "speaker_id": "1",
