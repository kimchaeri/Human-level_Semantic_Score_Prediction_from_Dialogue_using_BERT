        {
          "id": "MDRW2100007367.1.1",
          "form": "여행 가고 싶네요",
          "original_form": "여행 가고 싶네요",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007367.1.2",
          "form": "그러게요~",
          "original_form": "그러게요~",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007367.1.3",
          "form": "여행간다면 바다로 가고싶어요!",
          "original_form": "여행간다면 바다로 가고싶어요!",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007367.1.4",
          "form": "코로나 다 끝나면 제주도! 갈 생각이에요",
          "original_form": "코로나 다 끝나면 제주도! 갈 생각이에요",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007367.1.5",
          "form": "바다 좋죠",
          "original_form": "바다 좋죠",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007367.1.6",
          "form": "부산도 좋은데",
          "original_form": "부산도 좋은데",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007367.1.7",
          "form": "부산 좋죠...",
          "original_form": "부산 좋죠...",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007367.1.8",
          "form": "저어번에 부산 다녀왔었어요!",
          "original_form": "저어번에 부산 다녀왔었어요!",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007367.1.9",
          "form": "오랜만에 갔더니 해운대가 참 예쁘더라구요",
          "original_form": "오랜만에 갔더니 해운대가 참 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007367.1.10",
          "form": "부산 가고 싶어요",
          "original_form": "부산 가고 싶어요",
          "speaker_id": "2",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007367.1.11",
          "form": "돼지국밥!",
          "original_form": "돼지국밥!",
          "speaker_id": "2",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007367.1.12",
          "form": "맛있죠!!",
          "original_form": "맛있죠!!",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007367.1.13",
          "form": "부산은 너무 맛있는게 많아요",
          "original_form": "부산은 너무 맛있는게 많아요",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007367.1.14",
          "form": "호떡 좋아하는데 거기가 또 씨앗호떡 맛집이잖아요~",
          "original_form": "호떡 좋아하는데 거기가 또 씨앗호떡 맛집이잖아요~",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007367.1.15",
          "form": "아 그러게요!!!!",
          "original_form": "아 그러게요!!!!",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007367.1.16",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007367.1.17",
          "form": "또 부산에 맛있는게",
          "original_form": "또 부산에 맛있는게",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
