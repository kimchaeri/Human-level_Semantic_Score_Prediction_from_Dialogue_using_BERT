          "original_form": "아!!",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007367.1.19",
          "form": "낙곱새죠 낙곱새",
          "original_form": "낙곱새죠 낙곱새",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
