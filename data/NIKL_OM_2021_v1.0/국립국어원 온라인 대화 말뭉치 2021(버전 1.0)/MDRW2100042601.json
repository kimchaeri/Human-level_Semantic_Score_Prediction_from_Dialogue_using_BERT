      "utterance": [
        {
          "id": "MDRW2100042601.1.1",
          "form": "나  아쿠아리움다녀왔어",
          "original_form": "나  아쿠아리움다녀왔어",
          "speaker_id": "2",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100042601.1.2",
          "form": "그게 어디에 있지",
          "original_form": "그게 어디에 있지",
          "speaker_id": "1",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100042601.1.3",
          "form": "잠실에 있는 아쿠아리움",
          "original_form": "잠실에 있는 아쿠아리움",
          "speaker_id": "2",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100042601.1.4",
          "form": "그럼 어떤 걸 보고 왔어",
          "original_form": "그럼 어떤 걸 보고 왔어",
          "speaker_id": "1",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100042601.1.5",
          "form": "다양한 내부 공연이 있던데",
          "original_form": "다양한 내부 공연이 있던데",
          "speaker_id": "2",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100042601.1.6",
          "form": "인어공주 쇼도 있나",
          "original_form": "인어공주 쇼도 있나",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042601.1.7",
          "form": "맞아 맞아 무척 멋있던데",
          "original_form": "맞아 맞아 무척 멋있던데",
          "speaker_id": "2",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042601.1.8",
          "form": "그분들 안에서 정말 자유롭게 멋있게 움직이지",
          "original_form": "그분들 안에서 정말 자유롭게 멋있게 움직이지",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100042601.1.9",
          "form": "정말 한 마리의 인어 같아서",
          "original_form": "정말 한 마리의 인어 같아서",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042601.1.10",
          "form": "또 어떤 공연을 보았니",
          "original_form": "또 어떤 공연을 보았니",
          "speaker_id": "1",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042601.1.11",
          "form": "돌고래 쇼도 봤어",
          "original_form": "돌고래 쇼도 봤어",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042601.1.12",
          "form": "돌고래 너무 귀여워",
          "original_form": "돌고래 너무 귀여워",
          "speaker_id": "1",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042601.1.13",
          "form": "심지어 똑똑하기까지 해",
          "original_form": "심지어 똑똑하기까지 해",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100042601.1.14",
          "form": "사육사와 호흡하는 거 보면 정말 똑똑해 보여",
          "original_form": "사육사와 호흡하는 거 보면 정말 똑똑해 보여",
          "speaker_id": "1",
          "time": "20210906 23:20"
        }
