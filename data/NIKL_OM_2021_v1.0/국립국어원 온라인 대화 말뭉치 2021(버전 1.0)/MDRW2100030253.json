          "original_form": "무슨 대화 하고 있었지?",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030253.1.4",
          "form": "과제얘기",
          "original_form": "과제얘기",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030253.1.5",
          "form": "아아 그렇군",
          "original_form": "아아 그렇군",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030253.1.6",
          "form": "어떤 과제 해야하는데?",
          "original_form": "어떤 과제 해야하는데?",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030253.1.7",
          "form": "나 영어2랑",
          "original_form": "나 영어2랑",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030253.1.8",
          "form": "중국어1",
          "original_form": "중국어1",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030253.1.9",
          "form": "노트필기 과제",
          "original_form": "노트필기 과제",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.10",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.11",
          "form": "뭐가 이렇게",
          "original_form": "뭐가 이렇게",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.12",
          "form": "일단 강의는 다 못 들을 거 같고",
          "original_form": "일단 강의는 다 못 들을 거 같고",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.13",
          "form": "많아...",
          "original_form": "많아...",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.14",
          "form": "판서에 있는 내용",
          "original_form": "판서에 있는 내용",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 강의 째기",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 강의 째기",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.16",
          "form": "다 적어야 함..",
          "original_form": "다 적어야 함..",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.17",
          "form": "옮겨적기?",
          "original_form": "옮겨적기?",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ강의는 째는 맛이지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ강의는 째는 맛이지",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030253.1.19",
          "form": "와...대박...",
          "original_form": "와...대박...",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
