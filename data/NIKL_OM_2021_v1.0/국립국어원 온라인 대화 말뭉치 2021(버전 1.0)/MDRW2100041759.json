      "utterance": [
        {
          "id": "MDRW2100041759.1.1",
          "form": "중고거래 해봤나요?",
          "original_form": "중고거래 해봤나요?",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041759.1.2",
          "form": "네 좋아해요 ㅋㅋ",
          "original_form": "네 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041759.1.3",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041759.1.4",
          "form": "저는 최근에 당근마켓 시작했어요",
          "original_form": "저는 최근에 당근마켓 시작했어요",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041759.1.5",
          "form": "아 그러시구나 ㅋㅋ",
          "original_form": "아 그러시구나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041759.1.6",
          "form": "신기하죠 ㅋ",
          "original_form": "신기하죠 ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041759.1.7",
          "form": "코로나 계기로 집에 있는 시간이 많아지다보니",
          "original_form": "코로나 계기로 집에 있는 시간이 많아지다보니",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041759.1.8",
          "form": "처분하고 싶은 물건들이 보이더라구요",
          "original_form": "처분하고 싶은 물건들이 보이더라구요",
          "speaker_id": "2",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041759.1.9",
          "form": "그냥 버리자니 멀쩡한거 쓰레게 되는 거 싫고 그래서",
          "original_form": "그냥 버리자니 멀쩡한거 쓰레게 되는 거 싫고 그래서",
          "speaker_id": "2",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041759.1.10",
          "form": "중고거래 쪽을 알아보다가 당근 가입했는데",
          "original_form": "중고거래 쪽을 알아보다가 당근 가입했는데",
          "speaker_id": "2",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041759.1.11",
          "form": "진작 할걸 그랬다 싶었어요",
          "original_form": "진작 할걸 그랬다 싶었어요",
          "speaker_id": "2",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041759.1.12",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041759.1.13",
          "form": "방금도 하나 팔고 왔네요",
          "original_form": "방금도 하나 팔고 왔네요",
          "speaker_id": "2",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041759.1.14",
          "form": "당근에 의외로 여러가지를 팔더라구요",
          "original_form": "당근에 의외로 여러가지를 팔더라구요",
          "speaker_id": "1",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041759.1.15",
          "form": "오오 뭐 파셨어요?",
          "original_form": "오오 뭐 파셨어요?",
          "speaker_id": "1",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100041759.1.16",
          "form": "허리띠요",
          "original_form": "허리띠요",
          "speaker_id": "2",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041759.1.17",
          "form": "와 ㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041759.1.18",
          "form": "거의 15년전쯤 사서 안차고 다닌 새 허리띠 있는데",
          "original_form": "거의 15년전쯤 사서 안차고 다닌 새 허리띠 있는데",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041759.1.19",
          "form": "아 그러시구나 ㅋ",
          "original_form": "아 그러시구나 ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041759.1.20",
          "form": "이미 유행도 지났고 새것인데 버리기도 그렇고",
          "original_form": "이미 유행도 지났고 새것인데 버리기도 그렇고",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041759.1.21",
          "form": "하긴 저도 10년 된 가방 팔았어요",
          "original_form": "하긴 저도 10년 된 가방 팔았어요",
          "speaker_id": "1",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041759.1.22",
          "form": "계속 서랍속에 있었다가 오늘 나갓네요",
          "original_form": "계속 서랍속에 있었다가 오늘 나갓네요",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041759.1.23",
          "form": "맞아요 ㅋㅋ 아깝죠",
          "original_form": "맞아요 ㅋㅋ 아깝죠",
