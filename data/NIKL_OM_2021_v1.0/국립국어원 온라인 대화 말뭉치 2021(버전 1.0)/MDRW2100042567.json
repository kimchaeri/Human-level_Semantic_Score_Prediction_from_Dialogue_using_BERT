      "utterance": [
        {
          "id": "MDRW2100042567.1.1",
          "form": "응 김현중 안지현 인교진 순이다",
          "original_form": "응 김현중 안지현 인교진 순이다",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042567.1.2",
          "form": "두 부부가 부지런히 많이 나왔네",
          "original_form": "두 부부가 부지런히 많이 나왔네",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042567.1.3",
          "form": "헉 이건 시청률이 더 심한데",
          "original_form": "헉 이건 시청률이 더 심한데",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042567.1.4",
          "form": "얼만데요?",
          "original_form": "얼만데요?",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042567.1.5",
          "form": "시청률이 0.1%다 kbs스토리 채널에서 한거라",
          "original_form": "시청률이 0.1%다 kbs스토리 채널에서 한거라",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042567.1.6",
          "form": "헐,,  거의 안 봤다는 거네",
          "original_form": "헐,,  거의 안 봤다는 거네",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042567.1.7",
          "form": "그렇지 김현중주연인데도 안봣네",
          "original_form": "그렇지 김현중주연인데도 안봣네",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042567.1.8",
          "form": "kbs 스토리 채널도 생소해서 찾아서 보 지 는 않겠어요",
          "original_form": "kbs 스토리 채널도 생소해서 찾아서 보 지 는 않겠어요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042567.1.9",
          "form": "다시 죽어도좋아 드라마하고",
          "original_form": "다시 죽어도좋아 드라마하고",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042567.1.10",
          "form": "아 김현중은 말도 많고 비호감이라서...",
          "original_form": "아 김현중은 말도 많고 비호감이라서...",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042567.1.11",
          "form": "그래도 0.1%는 심하다",
          "original_form": "그래도 0.1%는 심하다",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042567.1.12",
          "form": "그렇긴 하네요. 김현중 사고 치고 뒤에 나온거 같은데",
          "original_form": "그렇긴 하네요. 김현중 사고 치고 뒤에 나온거 같은데",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042567.1.13",
          "form": "나의 나라에서 박문복 조연하고",
          "original_form": "나의 나라에서 박문복 조연하고",
