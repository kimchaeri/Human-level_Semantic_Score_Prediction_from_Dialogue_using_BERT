      "utterance": [
        {
          "id": "MDRW2100036176.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100036176.1.2",
          "form": "술 드시나요?",
          "original_form": "술 드시나요?",
          "speaker_id": "1",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100036176.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100036176.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100036176.1.5",
          "form": "저는 종종 마셔요?",
          "original_form": "저는 종종 마셔요?",
          "speaker_id": "1",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100036176.1.6",
          "form": "술 좋아하시나봐요",
          "original_form": "술 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100036176.1.7",
          "form": "그냥 그렇지만 가족끼리 종종 먹어요",
          "original_form": "그냥 그렇지만 가족끼리 종종 먹어요",
          "speaker_id": "1",
          "time": "20210830 18:19"
        },
        {
          "id": "MDRW2100036176.1.8",
          "form": "치맥이라든가..",
          "original_form": "치맥이라든가..",
          "speaker_id": "1",
          "time": "20210830 18:22"
        },
        {
          "id": "MDRW2100036176.1.9",
          "form": "맥주 좋아하세요?",
          "original_form": "맥주 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100036176.1.10",
          "form": "전 술중에선 맥주가 젤 나아요 탄산 좋아해서요",
          "original_form": "전 술중에선 맥주가 젤 나아요 탄산 좋아해서요",
          "speaker_id": "1",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100036176.1.11",
          "form": "아하 탄산 좋아하시는군요?",
          "original_form": "아하 탄산 좋아하시는군요?",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100036176.1.12",
          "form": "네 탄산수도 한때 주기적으로 시켜먹었을 정도에요",
          "original_form": "네 탄산수도 한때 주기적으로 시켜먹었을 정도에요",
          "speaker_id": "1",
          "time": "20210830 18:55"
        },
        {
          "id": "MDRW2100036176.1.13",
          "form": "오 정말 좋아하시나봐요 \r\n저는 탄산을 안 좋아해서..",
          "original_form": "오 정말 좋아하시나봐요 \r\n저는 탄산을 안 좋아해서..",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100036176.1.14",
          "form": "그럼 어떤 음료를 좋아하세요?",
          "original_form": "그럼 어떤 음료를 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100036176.1.15",
          "form": "저는 음료는 커피빼고는 많이 안 좋아해요\r\n과일에이드 종류들은 가끔 먹지만요",
          "original_form": "저는 음료는 커피빼고는 많이 안 좋아해요\r\n과일에이드 종류들은 가끔 먹지만요",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100036176.1.16",
          "form": "탄산도 이온음료도 음료를 즐기지는 않아서요",
          "original_form": "탄산도 이온음료도 음료를 즐기지는 않아서요",
          "speaker_id": "2",
          "time": "20210830 21:05"
        }
      ]
