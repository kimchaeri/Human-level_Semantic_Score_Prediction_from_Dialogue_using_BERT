      "utterance": [
        {
          "id": "MDRW2100040223.1.1",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100040223.1.2",
          "form": "네 밥먹었네요",
          "original_form": "네 밥먹었네요",
          "speaker_id": "2",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100040223.1.3",
          "form": "양념불고기에 밥 후다닥 먹었어요 ㅎ",
          "original_form": "양념불고기에 밥 후다닥 먹었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100040223.1.4",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "2",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100040223.1.5",
          "form": "저는 찜닭 먹었습니다 ㅎㅎ",
          "original_form": "저는 찜닭 먹었습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040223.1.6",
          "form": "닭 좋아하시나요?",
          "original_form": "닭 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040223.1.7",
          "form": "오 닭은 사랑이죠",
          "original_form": "오 닭은 사랑이죠",
          "speaker_id": "2",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040223.1.8",
          "form": "찜닭이든 치킨이든 ㅎㅎㅎ",
          "original_form": "찜닭이든 치킨이든 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040223.1.9",
          "form": "저도 닭 진짜 좋아해요!",
          "original_form": "저도 닭 진짜 좋아해요!",
          "speaker_id": "1",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040223.1.10",
          "form": "치킨은 후라이드/양념 뭐 드시나요",
          "original_form": "치킨은 후라이드/양념 뭐 드시나요",
          "speaker_id": "1",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040223.1.11",
          "form": "저는 저녁은 치킨 먹으려구요",
          "original_form": "저는 저녁은 치킨 먹으려구요",
          "speaker_id": "2",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040223.1.12",
          "form": "후라이드 좋아해요 ㅎㅎ",
          "original_form": "후라이드 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:24"
        },
        {
          "id": "MDRW2100040223.1.13",
          "form": "오 저녁으로!! 멋져요",
          "original_form": "오 저녁으로!! 멋져요",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040223.1.14",
          "form": "전 어렸을 땐 양념만 먹었는데",
          "original_form": "전 어렸을 땐 양념만 먹었는데",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040223.1.15",
          "form": "요즘은 후라이드 좋아합니다",
          "original_form": "요즘은 후라이드 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040223.1.16",
          "form": "아 그러세요?",
          "original_form": "아 그러세요?",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040223.1.17",
          "form": "저는 어릴때부터 후라이드 ㅎㅎ",
          "original_form": "저는 어릴때부터 후라이드 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040223.1.18",
          "form": "쭈욱~~~ ㅋㅋ",
          "original_form": "쭈욱~~~ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040223.1.19",
          "form": "요즘은 후라이드도 별의 별개 많아서",
          "original_form": "요즘은 후라이드도 별의 별개 많아서",
          "speaker_id": "2",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040223.1.20",
          "form": "브랜드는 어디 꺼 드세요?",
          "original_form": "브랜드는 어디 꺼 드세요?",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100040223.1.21",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100040223.1.22",
          "form": "저는 여기저기 많이 먹었는데",
          "original_form": "저는 여기저기 많이 먹었는데",
          "speaker_id": "2",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100040223.1.23",
          "form": "요즘은 바른 치킨인가? 거기가 젤 기본적인거 같아서",
          "original_form": "요즘은 바른 치킨인가? 거기가 젤 기본적인거 같아서",
          "speaker_id": "2",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100040223.1.24",
          "form": "아님 교촌 좋아해요",
          "original_form": "아님 교촌 좋아해요",
          "speaker_id": "2",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100040223.1.25",
          "form": "오오 저는 큰 브랜드는 아닌데",
          "original_form": "오오 저는 큰 브랜드는 아닌데",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100040223.1.26",
          "form": "후라이드 깔끔하게 기본 좋아하는데",
          "original_form": "후라이드 깔끔하게 기본 좋아하는데",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100040223.1.27",
          "form": "시켜서 먹으면 멀자꾸 섞어서...맵고 이래요",
          "original_form": "시켜서 먹으면 멀자꾸 섞어서...맵고 이래요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100040223.1.28",
          "form": "또봉이 통닭이라고 완전 예전 양념치킨 맛 나는 곳 있어서 거기 자주 시킵니다",
          "original_form": "또봉이 통닭이라고 완전 예전 양념치킨 맛 나는 곳 있어서 거기 자주 시킵니다",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100040223.1.29",
          "form": "들어봤어요",
          "original_form": "들어봤어요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100040223.1.30",
          "form": "아직 있나요?",
          "original_form": "아직 있나요?",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100040223.1.31",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 매운 거 좋아하시나요?",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 매운 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100040223.1.32",
          "form": "네 여기는 아직 있어요!",
          "original_form": "네 여기는 아직 있어요!",
          "speaker_id": "1",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100040223.1.33",
          "form": "아뇨 안매운거 좋아해요 ㅎ",
          "original_form": "아뇨 안매운거 좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 14:32"
        }
      ]
