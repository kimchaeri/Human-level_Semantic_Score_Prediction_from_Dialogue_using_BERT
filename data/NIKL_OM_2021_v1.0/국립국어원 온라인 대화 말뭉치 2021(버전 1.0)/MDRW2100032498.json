      "utterance": [
        {
          "id": "MDRW2100032498.1.1",
          "form": "지금 시간이면 이미 저녁 드셨겠죠",
          "original_form": "지금 시간이면 이미 저녁 드셨겠죠",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100032498.1.2",
          "form": "저녁 뭐 드셨나요?",
          "original_form": "저녁 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100032498.1.3",
          "form": "완탕면이랑 볶음밥이여~",
          "original_form": "완탕면이랑 볶음밥이여~",
          "speaker_id": "1",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100032498.1.4",
          "form": "중국음식 드셨군요 완탕면은 뭔가요?!",
          "original_form": "중국음식 드셨군요 완탕면은 뭔가요?!",
          "speaker_id": "2",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100032498.1.5",
          "form": "고기나 새우볼인데",
          "original_form": "고기나 새우볼인데",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100032498.1.6",
          "form": "홍콩음식이예여",
          "original_form": "홍콩음식이예여",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100032498.1.7",
          "form": "그렇군요 ㅎㅎ 전 샐러드랑 쌀식빵 먹었어요",
          "original_form": "그렇군요 ㅎㅎ 전 샐러드랑 쌀식빵 먹었어요",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100032498.1.8",
          "form": "오 드레싱 없이요?",
          "original_form": "오 드레싱 없이요?",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100032498.1.9",
          "form": "아뇨 발사믹 드레싱이랑요",
          "original_form": "아뇨 발사믹 드레싱이랑요",
          "speaker_id": "2",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100032498.1.10",
          "form": "오 좋쵸",
          "original_form": "오 좋쵸",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100032498.1.11",
          "form": "샐러드 채소 사셨어요?",
          "original_form": "샐러드 채소 사셨어요?",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100032498.1.12",
          "form": "전문집에서 포장이요 ㅎㅎ",
          "original_form": "전문집에서 포장이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100032498.1.13",
          "form": "오 전문집이 근처시군여",
          "original_form": "오 전문집이 근처시군여",
          "speaker_id": "1",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100032498.1.14",
          "form": "전 루꼴이 조아해여",
          "original_form": "전 루꼴이 조아해여",
          "speaker_id": "1",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100032498.1.15",
          "form": "오 요리에 자주 넣어 드시나요?",
          "original_form": "오 요리에 자주 넣어 드시나요?",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100032498.1.16",
          "form": "샐러드로 토마토랑 간단하게 좋아해요",
          "original_form": "샐러드로 토마토랑 간단하게 좋아해요",
