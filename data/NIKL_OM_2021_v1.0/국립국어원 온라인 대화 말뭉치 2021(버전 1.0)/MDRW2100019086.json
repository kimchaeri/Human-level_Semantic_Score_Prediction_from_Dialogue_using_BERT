      "utterance": [
        {
          "id": "MDRW2100019086.1.1",
          "form": "서양인들의 식사메뉴인 빵",
          "original_form": "서양인들의 식사메뉴인 빵",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100019086.1.2",
          "form": "무슨 빵을 먹었을까",
          "original_form": "무슨 빵을 먹었을까",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100019086.1.3",
          "form": "바게트!",
          "original_form": "바게트!",
          "speaker_id": "2",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100019086.1.4",
          "form": "땡 바게트 아닙니다",
          "original_form": "땡 바게트 아닙니다",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019086.1.5",
          "form": "기회 3번 남았습니다",
          "original_form": "기회 3번 남았습니다",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019086.1.6",
          "form": "바게트와 같이 유명한 빵 종류입니다",
          "original_form": "바게트와 같이 유명한 빵 종류입니다",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019086.1.7",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019086.1.8",
          "form": "힌트를더주세요",
          "original_form": "힌트를더주세요",
          "speaker_id": "2",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019086.1.9",
          "form": "일상생활에서 버스를 타거나",
          "original_form": "일상생활에서 버스를 타거나",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019086.1.10",
          "form": "지하철을 탈때",
          "original_form": "지하철을 탈때",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019086.1.11",
          "form": "혹은 번화가를 걸어갈때 자주 보는 빵입니다",
          "original_form": "혹은 번화가를 걸어갈때 자주 보는 빵입니다",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019086.1.12",
          "form": "도넛",
          "original_form": "도넛",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019086.1.13",
          "form": "정답입니다",
          "original_form": "정답입니다",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019086.1.14",
          "form": "다음문제입니다",
          "original_form": "다음문제입니다",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019086.1.15",
          "form": "도넛과 함께 먹은 음료는 무엇일까요",
          "original_form": "도넛과 함께 먹은 음료는 무엇일까요",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019086.1.16",
          "form": "1. 우유",
          "original_form": "1. 우유",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019086.1.17",
          "form": "2. 콜라",
          "original_form": "2. 콜라",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019086.1.18",
          "form": "둘중에",
          "original_form": "둘중에",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019086.1.19",
          "form": "정답이있습니다",
          "original_form": "정답이있습니다",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
