        },
        {
          "id": "MDRW2100044110.1.2",
          "form": "희한해..",
          "original_form": "희한해..",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044110.1.3",
          "form": "내비두면 알아서 한다구...",
          "original_form": "내비두면 알아서 한다구...",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044110.1.4",
          "form": "말해보면 똑같은데 다른집은 안그런대..",
          "original_form": "말해보면 똑같은데 다른집은 안그런대..",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044110.1.5",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044110.1.6",
          "form": "결국 치우긴 한다구..",
          "original_form": "결국 치우긴 한다구..",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044110.1.7",
          "form": "k장녀의 기질이 있어서(?",
          "original_form": "k장녀의 기질이 있어서(?",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044110.1.8",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋ 피에 흐르는 K장녀의 기질..",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋ 피에 흐르는 K장녀의 기질..",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044110.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 버리지 못한 기질 때문에",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 버리지 못한 기질 때문에",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044110.1.10",
          "form": "사서 고생이지;",
          "original_form": "사서 고생이지;",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044110.1.11",
          "form": "(대체",
          "original_form": "(대체",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044110.1.12",
          "form": "ㅇㅇ 진짜 사서 고생이라는 말 밖에는",
          "original_form": "ㅇㅇ 진짜 사서 고생이라는 말 밖에는",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044110.1.13",
          "form": "표현할 방법이 없음",
          "original_form": "표현할 방법이 없음",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044110.1.14",
          "form": "나도 좀 동생처럼",
          "original_form": "나도 좀 동생처럼",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044110.1.15",
          "form": "막나가고싶다",
          "original_form": "막나가고싶다",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044110.1.16",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044110.1.17",
          "form": "진짜 막 나가도 쟨 저러니까 하면서",
          "original_form": "진짜 막 나가도 쟨 저러니까 하면서",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044110.1.18",
          "form": "좀 그러려니 이해받고 싶다",
          "original_form": "좀 그러려니 이해받고 싶다",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044110.1.19",
          "form": "그니까...",
          "original_form": "그니까...",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044110.1.20",
          "form": "걍 내비두면서",
          "original_form": "걍 내비두면서",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044110.1.21",
          "form": "내가 손벌이면",
          "original_form": "내가 손벌이면",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044110.1.22",
          "form": "또 도와주고",
          "original_form": "또 도와주고",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044110.1.23",
          "form": "나도 글케 날로먹고싶다",
          "original_form": "나도 글케 날로먹고싶다",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044110.1.24",
          "form": "ㅜㅠㅠㅠ",
          "original_form": "ㅜㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044110.1.25",
          "form": "날로 먹고...",
          "original_form": "날로 먹고...",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044110.1.26",
          "form": "사는 삶..",
          "original_form": "사는 삶..",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044110.1.27",
          "form": "돈을 벌고 다 탕진해도",
          "original_form": "돈을 벌고 다 탕진해도",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044110.1.28",
          "form": "쟤는 벌긴 하니까 쯧쯧 하면서 다시 돈 받아가는 삶됐니",
          "original_form": "쟤는 벌긴 하니까 쯧쯧 하면서 다시 돈 받아가는 삶됐니",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044110.1.29",
          "form": "심심친구? 왜 초성만 썼다고 나쁜말이야",
          "original_form": "심심친구? 왜 초성만 썼다고 나쁜말이야",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044110.1.30",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044110.1.31",
          "form": "내가 탕진해도",
          "original_form": "내가 탕진해도",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044110.1.32",
          "form": "쟤가 좀 경제관념이 없어서 그렇지",
          "original_form": "쟤가 좀 경제관념이 없어서 그렇지",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044110.1.33",
          "form": "애는 착한데....",
          "original_form": "애는 착한데....",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044110.1.34",
          "form": "하면서 용돈 주는 삶 살고싶다",
          "original_form": "하면서 용돈 주는 삶 살고싶다",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044110.1.35",
          "form": "ㅋㅋㅋ큐ㅠㅠ 완전 워너비 아닌가",
          "original_form": "ㅋㅋㅋ큐ㅠㅠ 완전 워너비 아닌가",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044110.1.36",
          "form": "부잣집 막내아들 스타일",
          "original_form": "부잣집 막내아들 스타일",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044110.1.37",
          "form": "부잣집 아니어됴",
          "original_form": "부잣집 아니어됴",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044110.1.38",
          "form": "아니야 우리집 아들정도만 되어도 그정도는 될거같아",
          "original_form": "아니야 우리집 아들정도만 되어도 그정도는 될거같아",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044110.1.39",
          "form": "아들들은 다 그렇게 살더라고 ㅎ",
          "original_form": "아들들은 다 그렇게 살더라고 ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
