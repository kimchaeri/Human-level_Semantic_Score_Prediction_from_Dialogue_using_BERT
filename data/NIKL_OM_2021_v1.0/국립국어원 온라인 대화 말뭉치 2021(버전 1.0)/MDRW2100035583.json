      "utterance": [
        {
          "id": "MDRW2100035583.1.1",
          "form": "연애중이신가용",
          "original_form": "연애중이신가용",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100035583.1.2",
          "form": "이상형은요?",
          "original_form": "이상형은요?",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100035583.1.3",
          "form": "지금은 연애 안 하고 있어요",
          "original_form": "지금은 연애 안 하고 있어요",
          "speaker_id": "2",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100035583.1.4",
          "form": "이상형은 예의 바르고 저랑 공통점이 많은 사람을 좋아해요.",
          "original_form": "이상형은 예의 바르고 저랑 공통점이 많은 사람을 좋아해요.",
          "speaker_id": "2",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100035583.1.5",
          "form": "우와 저도 그래요",
          "original_form": "우와 저도 그래요",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100035583.1.6",
          "form": "그리고 대화 잘되는 사람",
          "original_form": "그리고 대화 잘되는 사람",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100035583.1.7",
          "form": "맞아요~ 저도 대화 잘 통하는 사람이 좋아요",
          "original_form": "맞아요~ 저도 대화 잘 통하는 사람이 좋아요",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100035583.1.8",
          "form": "똑똑한사람 그리고 잘생긴사람 ㅋㅋ",
          "original_form": "똑똑한사람 그리고 잘생긴사람 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100035583.1.9",
          "form": "그러려면 취향이나 가치관 같은게 비슷할 수록 좋은 거 같아요",
          "original_form": "그러려면 취향이나 가치관 같은게 비슷할 수록 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100035583.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100035583.1.11",
          "form": "다른거 다보는 대신 키는 안봐요 ㅋㅋ",
          "original_form": "다른거 다보는 대신 키는 안봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100035583.1.12",
          "form": "키는 저보다만 크면 돼요 166이상",
          "original_form": "키는 저보다만 크면 돼요 166이상",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100035583.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100035583.1.14",
          "form": "진짜 똑똑해야 해요~ 비논리적으로 막 우기는 사람 너무 싫어요...",
          "original_form": "진짜 똑똑해야 해요~ 비논리적으로 막 우기는 사람 너무 싫어요...",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100035583.1.15",
          "form": "헐 맞아요 정말 ...",
          "original_form": "헐 맞아요 정말 ...",
          "speaker_id": "1",
          "time": "20210901 20:14"
        },
        {
          "id": "MDRW2100035583.1.16",
          "form": "잘생긴 것도 중요하죠ㅋㅋㅋ",
          "original_form": "잘생긴 것도 중요하죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100035583.1.17",
          "form": "일단 호감이 생겨야 하니까..ㅎㅎ",
          "original_form": "일단 호감이 생겨야 하니까..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100035583.1.18",
          "form": "그니까용 ㅋㅋ",
          "original_form": "그니까용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100035583.1.19",
          "form": "생각만해도 기쁘네요 ㅋㅋ",
          "original_form": "생각만해도 기쁘네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100035583.1.20",
          "form": "돈도 잘 벌면 좋겠고..ㅎㅎ",
          "original_form": "돈도 잘 벌면 좋겠고..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100035583.1.21",
          "form": "이렇게 눈이 높아서 연애를 못 하나 봐요ㅋㅋㅋㅋ",
          "original_form": "이렇게 눈이 높아서 연애를 못 하나 봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100035583.1.22",
          "form": "제가 그냥 똑똑해지고 예의바르게 행동하고 돈 많이 버는 게 빠르겠어요ㅋㅋㅋㅋ",
          "original_form": "제가 그냥 똑똑해지고 예의바르게 행동하고 돈 많이 버는 게 빠르겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100035583.1.23",
          "form": "맞아요 ㅋㅋㅋ 또 언젠가 여유가 생기면 눈에 들어오는 분이 생길거에용",
