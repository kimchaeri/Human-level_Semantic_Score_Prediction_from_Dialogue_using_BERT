      "utterance": [
        {
          "id": "MDRW2100034806.1.1",
          "form": "안녕하세요,  중고거래 해보셨나요?",
          "original_form": "안녕하세요,  중고거래 해보셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034806.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100034806.1.3",
          "form": "예전에는 중고나라에서 구매만 해보다가 요즘은 당근마켓이요!",
          "original_form": "예전에는 중고나라에서 구매만 해보다가 요즘은 당근마켓이요!",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100034806.1.4",
          "form": "저도 당근마켓 주로 사용해요ㅎㅎ",
          "original_form": "저도 당근마켓 주로 사용해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034806.1.5",
          "form": "오오 거래도 많이 해보셨나요??",
          "original_form": "오오 거래도 많이 해보셨나요??",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100034806.1.6",
          "form": "몇 번 해봤어요!",
          "original_form": "몇 번 해봤어요!",
          "speaker_id": "1",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100034806.1.7",
          "form": "오~  노하우 있나요?",
          "original_form": "오~  노하우 있나요?",
          "speaker_id": "2",
          "time": "20210828 20:12"
        },
        {
          "id": "MDRW2100034806.1.8",
          "form": "올렸는데 연락이 안오네오ㅠ",
          "original_form": "올렸는데 연락이 안오네오ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:12"
        },
        {
          "id": "MDRW2100034806.1.9",
          "form": "저도 거의 1, 2달쯤 기다렸어요...사진이 제일 중요한 것 같아요ㅠ",
          "original_form": "저도 거의 1, 2달쯤 기다렸어요...사진이 제일 중요한 것 같아요ㅠ",
          "speaker_id": "1",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100034806.1.10",
          "form": "사진! 사진에도 신경을 써야하는군요..",
          "original_form": "사진! 사진에도 신경을 써야하는군요..",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100034806.1.11",
          "form": "쉽지 않은거였네요",
          "original_form": "쉽지 않은거였네요",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100034806.1.12",
          "form": "있어보이거나 깔끔하게 찍는 게 중요해요!",
          "original_form": "있어보이거나 깔끔하게 찍는 게 중요해요!",
          "speaker_id": "1",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100034806.1.13",
          "form": "스킬이 필요하군요!",
          "original_form": "스킬이 필요하군요!",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034806.1.14",
          "form": "지금 다시 찍고 있어요ㅎㅎ",
          "original_form": "지금 다시 찍고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034806.1.15",
          "form": "좋은 결과가 있기를 바라요ㅠ",
          "original_form": "좋은 결과가 있기를 바라요ㅠ",
          "speaker_id": "1",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100034806.1.16",
          "form": "넵 감사합니당",
          "original_form": "넵 감사합니당",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100034806.1.17",
          "form": "다시 올렸어요~!",
          "original_form": "다시 올렸어요~!",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100034806.1.18",
          "form": "여유를 갖고 기다리시다 보면 연락이 오리라 믿어요!",
          "original_form": "여유를 갖고 기다리시다 보면 연락이 오리라 믿어요!",
          "speaker_id": "1",
          "time": "20210828 20:48"
        }
      ]
