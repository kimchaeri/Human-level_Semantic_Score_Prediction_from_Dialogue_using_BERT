      "utterance": [
        {
          "id": "MDRW2100039935.1.1",
          "form": "회사에서 스트레스 받으면",
          "original_form": "회사에서 스트레스 받으면",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100039935.1.2",
          "form": "퇴근후 불볶면을 먹었어요",
          "original_form": "퇴근후 불볶면을 먹었어요",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100039935.1.3",
          "form": "스트레스엔 매운 거죠!",
          "original_form": "스트레스엔 매운 거죠!",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100039935.1.4",
          "form": "떡볶이도 좋아하시나요",
          "original_form": "떡볶이도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100039935.1.5",
          "form": "네 떡볶이 엄청 좋아해요 회사에서도 종종 시켜먹어요",
          "original_form": "네 떡볶이 엄청 좋아해요 회사에서도 종종 시켜먹어요",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039935.1.6",
          "form": "야근할 때요?",
          "original_form": "야근할 때요?",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039935.1.7",
          "form": "저희는 회사에 구내식당이 있는데",
          "original_form": "저희는 회사에 구내식당이 있는데",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100039935.1.8",
          "form": "이용안하고 떡볶아 시켜서 따로 먹기도하고",
          "original_form": "이용안하고 떡볶아 시켜서 따로 먹기도하고",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100039935.1.9",
          "form": "도시락 싸기도해요",
          "original_form": "도시락 싸기도해요",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100039935.1.10",
          "form": "아 구내식당...좋은 회사네요",
          "original_form": "아 구내식당...좋은 회사네요",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100039935.1.11",
          "form": "제 직장은 지역상생이란 미명하에..",
          "original_form": "제 직장은 지역상생이란 미명하에..",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100039935.1.12",
          "form": "그런거 없습니다ㅜㅇㅜ",
          "original_form": "그런거 없습니다ㅜㅇㅜ",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100039935.1.13",
          "form": "복지는 개나 줬죠",
          "original_form": "복지는 개나 줬죠",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100039935.1.14",
          "form": "그럼 식사를 매번 식당에서 하시나요",
          "original_form": "그럼 식사를 매번 식당에서 하시나요",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100039935.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100039935.1.16",
          "form": "회장은 건물 빈 층으로 임대장사하고 있죠ㅜㅜㅜ",
          "original_form": "회장은 건물 빈 층으로 임대장사하고 있죠ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100039935.1.17",
          "form": "어이고매번메뉴 고르는것도 안좋을거 같아요",
          "original_form": "어이고매번메뉴 고르는것도 안좋을거 같아요",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039935.1.18",
          "form": "처음엔 부러웠는데 또막상 그렇지도 않더라고요...",
          "original_form": "처음엔 부러웠는데 또막상 그렇지도 않더라고요...",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039935.1.19",
          "form": "코로나 이후론 인원수도 딱딱 갈라 앉아야 하고요ㅎㅎ",
          "original_form": "코로나 이후론 인원수도 딱딱 갈라 앉아야 하고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039935.1.20",
          "form": "지역상생이라니 너무 잘 붙이신듯",
          "original_form": "지역상생이라니 너무 잘 붙이신듯",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039935.1.21",
          "form": "전직장은 구내식당있었는데",
          "original_form": "전직장은 구내식당있었는데",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039935.1.22",
          "form": "그때가 그립네요",
          "original_form": "그때가 그립네요",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039935.1.23",
          "form": "말은 번지르르하죠ㅎㅎ",
          "original_form": "말은 번지르르하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039935.1.24",
          "form": "ㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋ그러게요",
          "speaker_id": "1",
          "time": "20210903 23:12"
        }
      ]
    }
