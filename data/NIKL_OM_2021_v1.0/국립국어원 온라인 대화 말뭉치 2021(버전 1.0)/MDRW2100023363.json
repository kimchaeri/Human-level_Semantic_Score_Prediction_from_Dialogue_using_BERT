      "utterance": [
        {
          "id": "MDRW2100023363.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100023363.1.2",
          "form": "안녕하세요~ \n어떤 계절 좋아하세요?",
          "original_form": "안녕하세요~ \n어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100023363.1.3",
          "form": "저는 겨울이요!",
          "original_form": "저는 겨울이요!",
          "speaker_id": "2",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100023363.1.4",
          "form": "겨울 좋아하시는군요 \n어떤 점이 좋으세요?",
          "original_form": "겨울 좋아하시는군요 \n어떤 점이 좋으세요?",
          "speaker_id": "1",
          "time": "20210830 01:28"
        },
        {
          "id": "MDRW2100023363.1.5",
          "form": "그냥 눈오고 흰바탕이 되는게좋아요ㅋㅋㅋ",
          "original_form": "그냥 눈오고 흰바탕이 되는게좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100023363.1.6",
          "form": "눈오는거 좋아하시는분들 많으시더라구요",
          "original_form": "눈오는거 좋아하시는분들 많으시더라구요",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023363.1.7",
          "form": "저도 눈 내리는건 좋아하는데",
          "original_form": "저도 눈 내리는건 좋아하는데",
          "speaker_id": "1",
          "time": "20210830 01:37"
        },
        {
          "id": "MDRW2100023363.1.8",
          "form": "나중에 눈이 녹으면서 지저분해지는건 싫더라구요ㅜㅜ",
          "original_form": "나중에 눈이 녹으면서 지저분해지는건 싫더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100023363.1.9",
          "form": "아 저도그건..쫌",
          "original_form": "아 저도그건..쫌",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023363.1.10",
          "form": "그쵸 눈 내리는 하얀 풍경 까지만 딱 좋아요",
          "original_form": "그쵸 눈 내리는 하얀 풍경 까지만 딱 좋아요",
          "speaker_id": "1",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023363.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023363.1.12",
          "form": "거기까지죠",
          "original_form": "거기까지죠",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023363.1.13",
          "form": "맞아요\n오늘 날씨 맑고 좋네요~",
          "original_form": "맞아요\n오늘 날씨 맑고 좋네요~",
          "speaker_id": "1",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023363.1.14",
          "form": "선선하더라구요~ 이런날 놀러가면좋은데말이죠",
          "original_form": "선선하더라구요~ 이런날 놀러가면좋은데말이죠",
          "speaker_id": "2",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100023363.1.15",
          "form": "맞아요~ 지난주는 날씨가 많이 흐렸는데, 이번주는 계속 좋으면 좋겠네요",
          "original_form": "맞아요~ 지난주는 날씨가 많이 흐렸는데, 이번주는 계속 좋으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100023363.1.16",
          "form": "그러게요 좋아졌으면ㅠ",
          "original_form": "그러게요 좋아졌으면ㅠ",
          "speaker_id": "2",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100023363.1.17",
          "form": "근데 비소식이 또 있더라구요ㅜㅜ",
          "original_form": "근데 비소식이 또 있더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023363.1.18",
          "form": "아 요즘 예보를 안보다보니 그렇군요ㅠ",
          "original_form": "아 요즘 예보를 안보다보니 그렇군요ㅠ",
          "speaker_id": "2",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100023363.1.19",
          "form": "지역마다 다를 수 있으니까요 \n제가 있는 곳은 그렇네요",
          "original_form": "지역마다 다를 수 있으니까요 \n제가 있는 곳은 그렇네요",
          "speaker_id": "1",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100023363.1.20",
          "form": "예보가 안맞은적이 많아서 잘안보게되더라구요",
          "original_form": "예보가 안맞은적이 많아서 잘안보게되더라구요",
          "speaker_id": "2",
          "time": "20210830 16:32"
        },
        {
          "id": "MDRW2100023363.1.21",
          "form": "가끔 맞을때는 항상 우산이 없고",
          "original_form": "가끔 맞을때는 항상 우산이 없고",
          "speaker_id": "2",
          "time": "20210830 16:32"
        },
        {
          "id": "MDRW2100023363.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 16:36"
        },
        {
          "id": "MDRW2100023363.1.23",
          "form": "또 우산을 챙겨나갔는데 비가 안 오기도 하죠",
          "original_form": "또 우산을 챙겨나갔는데 비가 안 오기도 하죠",
          "speaker_id": "1",
          "time": "20210830 16:36"
        }
      ]
    }
