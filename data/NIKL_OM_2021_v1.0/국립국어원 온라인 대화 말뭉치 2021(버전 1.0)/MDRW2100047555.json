      "utterance": [
        {
          "id": "MDRW2100047555.1.1",
          "form": "샤인머스캣 드셔보셨어요?",
          "original_form": "샤인머스캣 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100047555.1.2",
          "form": "회사에서 5시쯤 그거먹고 운동 갔다가",
          "original_form": "회사에서 5시쯤 그거먹고 운동 갔다가",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100047555.1.3",
          "form": "집에가는길이예요",
          "original_form": "집에가는길이예요",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100047555.1.4",
          "form": "네 너무 맛나요",
          "original_form": "네 너무 맛나요",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100047555.1.5",
          "form": "근데 비싸죠 ㅡㅡ",
          "original_form": "근데 비싸죠 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100047555.1.6",
          "form": "저는 첨먹어봤는데요",
          "original_form": "저는 첨먹어봤는데요",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047555.1.7",
          "form": "오 운동에 알차게 하고 집에 가시네요",
          "original_form": "오 운동에 알차게 하고 집에 가시네요",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047555.1.8",
          "form": "달긴 진짜 달더라구요",
          "original_form": "달긴 진짜 달더라구요",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047555.1.9",
          "form": "딸이 좋아해요",
          "original_form": "딸이 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047555.1.10",
          "form": "너무 달아서 몇알먹고나니",
          "original_form": "너무 달아서 몇알먹고나니",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047555.1.11",
          "form": "마트만 가면 사달래요",
          "original_form": "마트만 가면 사달래요",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047555.1.12",
          "form": "입안이 달달해서",
          "original_form": "입안이 달달해서",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047555.1.13",
          "form": "많이는 못먹겠더라구요",
          "original_form": "많이는 못먹겠더라구요",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100047555.1.14",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100047555.1.15",
          "form": "그러게요 ㅎㅎ진짜 달라요 그럼 저녁은 안드시고",
          "original_form": "그러게요 ㅎㅎ진짜 달라요 그럼 저녁은 안드시고",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100047555.1.16",
          "form": "포도만 먹고 마나요?",
          "original_form": "포도만 먹고 마나요?",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100047555.1.17",
          "form": "지금 배고파서 두유하나 쪽쪽 ㅎㅎ",
          "original_form": "지금 배고파서 두유하나 쪽쪽 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100047555.1.18",
          "form": "마셔요 ㅎㅎ",
          "original_form": "마셔요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100047555.1.19",
          "form": "ㅋㅋ결국 밥빼고 이것저것 먹게돼요 ㅋㅋ",
          "original_form": "ㅋㅋ결국 밥빼고 이것저것 먹게돼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100047555.1.20",
          "form": "오늘은 집에가서 배고프면",
          "original_form": "오늘은 집에가서 배고프면",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047555.1.21",
          "form": "집에가서 좀 드셔요",
          "original_form": "집에가서 좀 드셔요",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047555.1.22",
          "form": "바나나 하나로 버텨보려구요",
          "original_form": "바나나 하나로 버텨보려구요",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047555.1.23",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047555.1.24",
          "form": "과연될지 모르겠지만요 ㅋ",
          "original_form": "과연될지 모르겠지만요 ㅋ",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047555.1.25",
          "form": "어머 다욧 식간인데요 ㅋㅋ",
          "original_form": "어머 다욧 식간인데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047555.1.26",
          "form": "저도 맨날 실패입니다",
          "original_form": "저도 맨날 실패입니다",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047555.1.27",
          "form": "어제 피자먹었으니",
          "original_form": "어제 피자먹었으니",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047555.1.28",
          "form": "아까 아이스크림통 ㅋㅋㄹ고 막었네요",
          "original_form": "아까 아이스크림통 ㅋㅋㄹ고 막었네요",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047555.1.29",
          "form": "하루쯤은 이렇게 ㅋㅋ",
          "original_form": "하루쯤은 이렇게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100047555.1.30",
          "form": "들고 퍼서 막고",
          "original_form": "들고 퍼서 막고",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100047555.1.31",
          "form": "먹고",
          "original_form": "먹고",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100047555.1.32",
          "form": "그럼요 하루쯤 가볍게좋나요",
          "original_form": "그럼요 하루쯤 가볍게좋나요",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100047555.1.33",
          "form": "아요",
          "original_form": "아요",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100047555.1.34",
          "form": "운동도 하셨고",
          "original_form": "운동도 하셨고",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100047555.1.35",
          "form": "참는것도 ㅋㅋ",
          "original_form": "참는것도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:22"
        }
      ]
    }
