      "utterance": [
        {
          "id": "MDRW2100040792.1.1",
          "form": "데려올때는",
          "original_form": "데려올때는",
          "speaker_id": "1",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100040792.1.2",
          "form": "정말 아가였는데..",
          "original_form": "정말 아가였는데..",
          "speaker_id": "1",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100040792.1.3",
          "form": "지금 커졋나요?",
          "original_form": "지금 커졋나요?",
          "speaker_id": "2",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100040792.1.4",
          "form": "너무 커져서요 ㅋㅋㅋ",
          "original_form": "너무 커져서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100040792.1.5",
          "form": "애가 밥주는것도 무서워해요 ㅋㅋㅋㅋ",
          "original_form": "애가 밥주는것도 무서워해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100040792.1.6",
          "form": "햄스터도 종류가 많던데",
          "original_form": "햄스터도 종류가 많던데",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.7",
          "form": "애가 밥주면 물려고 덜려들고..그냥 보기만 해요 ㅋㅋㅋㅋ",
          "original_form": "애가 밥주면 물려고 덜려들고..그냥 보기만 해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.8",
          "form": "몸집이 커지는 햄스터인가봐요",
          "original_form": "몸집이 커지는 햄스터인가봐요",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.9",
          "form": "물려고 해요?",
          "original_form": "물려고 해요?",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.10",
          "form": "공부를 좀했어야했는데..",
          "original_form": "공부를 좀했어야했는데..",
          "speaker_id": "1",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.11",
          "form": "그 파시는분 말만듣고",
          "original_form": "그 파시는분 말만듣고",
          "speaker_id": "1",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.12",
          "form": "아이가 놀랄만하네요",
          "original_form": "아이가 놀랄만하네요",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.13",
          "form": "덥썩 분양해왔어요~",
          "original_form": "덥썩 분양해왔어요~",
          "speaker_id": "1",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.14",
          "form": "물면 안돼죠",
          "original_form": "물면 안돼죠",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.15",
          "form": "골드햄스터인가요?",
          "original_form": "골드햄스터인가요?",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.16",
          "form": "정말 빨라서 저도 놀라거든요ㅠ",
          "original_form": "정말 빨라서 저도 놀라거든요ㅠ",
          "speaker_id": "1",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100040792.1.17",
          "form": "어떤 햄스터가 커지는걸까",
          "original_form": "어떤 햄스터가 커지는걸까",
          "speaker_id": "2",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040792.1.18",
          "form": "신랑외에는 컨트롤이 안되요,,;;",
          "original_form": "신랑외에는 컨트롤이 안되요,,;;",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040792.1.19",
          "form": "곤든햄스터라고~",
          "original_form": "곤든햄스터라고~",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040792.1.20",
          "form": "그러면 남편 몫이 된거네요",
          "original_form": "그러면 남편 몫이 된거네요",
          "speaker_id": "2",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040792.1.21",
          "form": "막 탈출해요??",
          "original_form": "막 탈출해요??",
          "speaker_id": "2",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040792.1.22",
          "form": "탈출하려고 케이지도 갉아요 ㅋㅋㅋㅋ",
          "original_form": "탈출하려고 케이지도 갉아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040792.1.23",
          "form": "들어보니까 케이지도 나가고 그런다고 하더라구요",
          "original_form": "들어보니까 케이지도 나가고 그런다고 하더라구요",
          "speaker_id": "2",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040792.1.24",
          "form": "밤새 ㅋㅋㅋㅋ",
          "original_form": "밤새 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100040792.1.25",
          "form": "그럼 거의 강아지 키우는 수준이겠는데요",
          "original_form": "그럼 거의 강아지 키우는 수준이겠는데요",
