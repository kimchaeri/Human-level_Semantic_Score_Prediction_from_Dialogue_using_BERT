      "utterance": [
        {
          "id": "MDRW2100008370.1.1",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008370.1.2",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008370.1.3",
          "form": "솔직히 아니요...잘 읽게되질않네요",
          "original_form": "솔직히 아니요...잘 읽게되질않네요",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008370.1.4",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008370.1.5",
          "form": "요즘 막 재밌어지더라고요!",
          "original_form": "요즘 막 재밌어지더라고요!",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008370.1.6",
          "form": "여기서 채팅하면서 추천도서 한권씩 받아가고있어요 ㅎㅎ",
          "original_form": "여기서 채팅하면서 추천도서 한권씩 받아가고있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008370.1.7",
          "form": "우와 추천받으신거 저도 궁금한데 알려주실수 있어요?",
          "original_form": "우와 추천받으신거 저도 궁금한데 알려주실수 있어요?",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008370.1.8",
          "form": "앗 좋아요",
          "original_form": "앗 좋아요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008370.1.9",
          "form": "루팡의 딸?",
          "original_form": "루팡의 딸?",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008370.1.10",
          "form": "그리고 모모 라는 책도 추천받았고",
          "original_form": "그리고 모모 라는 책도 추천받았고",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008370.1.11",
          "form": "1인가구 살림법이라는 책도 추천받았내요!",
          "original_form": "1인가구 살림법이라는 책도 추천받았내요!",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008370.1.12",
          "form": "헉 1인가구 살림법 너무 솔깃한 제목이다",
          "original_form": "헉 1인가구 살림법 너무 솔깃한 제목이다",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008370.1.13",
          "form": "메모 했어요 정보감사해요",
          "original_form": "메모 했어요 정보감사해요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008370.1.14",
          "form": "그쵸 저도 너무 솔깃했어요",
          "original_form": "그쵸 저도 너무 솔깃했어요",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008370.1.15",
          "form": "어릴때 좋아하던 동화 있으새요?",
          "original_form": "어릴때 좋아하던 동화 있으새요?",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008370.1.16",
          "form": "갑자기 생각하려니 생각나는게 없네요",
          "original_form": "갑자기 생각하려니 생각나는게 없네요",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008370.1.17",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008370.1.18",
          "form": "음 저는 누가 내머리애 똥쌌어?",
          "original_form": "음 저는 누가 내머리애 똥쌌어?",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008370.1.19",
          "form": "아! 표지 기억나요",
          "original_form": "아! 표지 기억나요",
          "speaker_id": "2",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008370.1.20",
          "form": "귀엽고 따땃한 내용이였던거 같은데맞나요?",
          "original_form": "귀엽고 따땃한 내용이였던거 같은데맞나요?",
          "speaker_id": "2",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008370.1.21",
          "form": "결말은 기억이 안나는데",
          "original_form": "결말은 기억이 안나는데",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008370.1.22",
          "form": "엄청 귀여웠던건 맞는거같아요",
          "original_form": "엄청 귀여웠던건 맞는거같아요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008370.1.23",
          "form": "지금 스팀 프사도 그거얘요 ㅎㅎ",
          "original_form": "지금 스팀 프사도 그거얘요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008370.1.24",
          "form": "앜ㅋㅋㅋㄱ 귀엽겠어요",
          "original_form": "앜ㅋㅋㅋㄱ 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008370.1.25",
          "form": "추천해주신 책 잘읽어볼게요",
          "original_form": "추천해주신 책 잘읽어볼게요",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008370.1.26",
          "form": "좋아요ㅎㅎ",
          "original_form": "좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008370.1.27",
          "form": "최근에는 얼음고래 쟈밌개 봤는데",
          "original_form": "최근에는 얼음고래 쟈밌개 봤는데",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008370.1.28",
          "form": "이것도 추천해요",
          "original_form": "이것도 추천해요",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008370.1.29",
          "form": "감사해요!",
          "original_form": "감사해요!",
          "speaker_id": "2",
          "time": "20210822 15:44"
        }
      ]
    }
  ]
