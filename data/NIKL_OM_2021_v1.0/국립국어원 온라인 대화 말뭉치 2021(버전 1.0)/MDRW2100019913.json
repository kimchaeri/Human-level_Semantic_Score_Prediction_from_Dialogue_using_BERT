      "utterance": [
        {
          "id": "MDRW2100019913.1.1",
          "form": "게임하시나요",
          "original_form": "게임하시나요",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019913.1.2",
          "form": "게임 하시나요?",
          "original_form": "게임 하시나요?",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019913.1.3",
          "form": "네 게임 좋아해요 ㅋㅋ",
          "original_form": "네 게임 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019913.1.4",
          "form": "두 번이나 물어보시다니!!",
          "original_form": "두 번이나 물어보시다니!!",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019913.1.5",
          "form": "아 제가 위에꺼가 안떠요",
          "original_form": "아 제가 위에꺼가 안떠요",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019913.1.6",
          "form": "무슨 게임 하세용?",
          "original_form": "무슨 게임 하세용?",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019913.1.7",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019913.1.8",
          "form": "최근에 vr게임하고 있어요",
          "original_form": "최근에 vr게임하고 있어요",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019913.1.9",
          "form": "오 재밌나요!",
          "original_form": "오 재밌나요!",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019913.1.10",
          "form": "name1님은 게임 하세요??",
          "original_form": "&name1&님은 게임 하세요??",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019913.1.11",
          "form": "vr 관심이 있는데 기계값 때문에",
          "original_form": "vr 관심이 있는데 기계값 때문에",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019913.1.12",
          "form": "네 전 스팀으로 합니다",
          "original_form": "네 전 스팀으로 합니다",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019913.1.13",
          "form": "오 스팀겜 최근에 어떤거 해보셨어요??",
          "original_form": "오 스팀겜 최근에 어떤거 해보셨어요??",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019913.1.14",
          "form": "프로젝트 좀보이드라고 좀비랑 싸우는거예요",
          "original_form": "프로젝트 좀보이드라고 좀비랑 싸우는거예요",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019913.1.15",
          "form": "좀비겜 ㅎㄷ",
          "original_form": "좀비겜 ㅎㄷ",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019913.1.16",
          "form": "좀비 나오는 게임 좋아하시는거예요???",
          "original_form": "좀비 나오는 게임 좋아하시는거예요???",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019913.1.17",
          "form": "네 생존이랑 연결된거면 재밌어해요",
          "original_form": "네 생존이랑 연결된거면 재밌어해요",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019913.1.18",
          "form": "아항 저도 생존겜은 좋아해여!",
          "original_form": "아항 저도 생존겜은 좋아해여!",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019913.1.19",
          "form": "좀비겜은 약간 무서워서 못 하겠음 ㅠㅜ",
          "original_form": "좀비겜은 약간 무서워서 못 하겠음 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019913.1.20",
          "form": "돈스타브라고 우리나라말로 굶지마인데 그거 정말 재밌어요",
          "original_form": "돈스타브라고 우리나라말로 굶지마인데 그거 정말 재밌어요",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019913.1.21",
          "form": "아 그 겜 엄청 많이 들어봤어요",
          "original_form": "아 그 겜 엄청 많이 들어봤어요",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019913.1.22",
          "form": "친구가 추천은 해줬는데 해봐야지만 생각하고 안 해본,,,ㅠ",
          "original_form": "친구가 추천은 해줬는데 해봐야지만 생각하고 안 해본,,,ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019913.1.23",
          "form": "재밌어요 할인도 자주 하니 언제 한번 해보셔요",
          "original_form": "재밌어요 할인도 자주 하니 언제 한번 해보셔요",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
