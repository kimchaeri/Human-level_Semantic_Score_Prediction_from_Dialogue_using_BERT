      "utterance": [
        {
          "id": "MDRW2100031778.1.1",
          "form": "저는 밀크커피만 마셔요",
          "original_form": "저는 밀크커피만 마셔요",
          "speaker_id": "1",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031778.1.2",
          "form": "아메리카노는 써서..",
          "original_form": "아메리카노는 써서..",
          "speaker_id": "1",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031778.1.3",
          "form": "아 아메리카노는 좀 쓰기도하죠!",
          "original_form": "아 아메리카노는 좀 쓰기도하죠!",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031778.1.4",
          "form": "밀크커피는 안마셔요 ?",
          "original_form": "밀크커피는 안마셔요 ?",
          "speaker_id": "1",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031778.1.5",
          "form": "가끔",
          "original_form": "가끔",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100031778.1.6",
          "form": "달달한거마시고싶을땐",
          "original_form": "달달한거마시고싶을땐",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100031778.1.7",
          "form": "마셔요",
          "original_form": "마셔요",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100031778.1.8",
          "form": "안좋아해요 ?",
          "original_form": "안좋아해요 ?",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100031778.1.9",
          "form": "어좀 입이 텁텁해지는것같아요ㅋㅋ",
          "original_form": "어좀 입이 텁텁해지는것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031778.1.10",
          "form": "아 무슨말인지 알것같네요 ㅎㅎ",
          "original_form": "아 무슨말인지 알것같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031778.1.11",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031778.1.12",
          "form": "자주시켜먹진 않아요",
          "original_form": "자주시켜먹진 않아요",
          "speaker_id": "1",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031778.1.13",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "1",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031778.1.14",
          "form": "저는 회사에서 점심을 늘 시켜먹었어요",
          "original_form": "저는 회사에서 점심을 늘 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031778.1.15",
          "form": "아이고.. 돈 많이 들었겠어요",
          "original_form": "아이고.. 돈 많이 들었겠어요",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031778.1.16",
          "form": "네 요즘 배달비도 엄청 비싸서,,,",
          "original_form": "네 요즘 배달비도 엄청 비싸서,,,",
          "speaker_id": "2",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031778.1.17",
          "form": "맞아요 배달비가 따로 많이 붙더라고요",
          "original_form": "맞아요 배달비가 따로 많이 붙더라고요",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031778.1.18",
          "form": "네 이제곧 또 오른대요ㅠ",
          "original_form": "네 이제곧 또 오른대요ㅠ",
          "speaker_id": "2",
          "time": "20210905 00:23"
        }
      ]
