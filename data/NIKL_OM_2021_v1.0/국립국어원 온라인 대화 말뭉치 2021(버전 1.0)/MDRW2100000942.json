      "utterance": [
        {
          "id": "MDRW2100000942.1.1",
          "form": "애들은 게임 끝났나",
          "original_form": "애들은 게임 끝났나",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000942.1.2",
          "form": "ㅇㅇ 방금 껐어",
          "original_form": "ㅇㅇ 방금 껐어",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000942.1.3",
          "form": "시간 잘 정해서 해야해",
          "original_form": "시간 잘 정해서 해야해",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000942.1.4",
          "form": "알아용 ㅎㅎ",
          "original_form": "알아용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000942.1.5",
          "form": "가끔 색시 까먹고 엄청 오래 하게 두더만",
          "original_form": "가끔 색시 까먹고 엄청 오래 하게 두더만",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000942.1.6",
          "form": "사실 30분씩 하면 너무 금방 끄게 되니까",
          "original_form": "사실 30분씩 하면 너무 금방 끄게 되니까",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000942.1.7",
          "form": "그래도 약속은 약속이니 지키게 해줘요",
          "original_form": "그래도 약속은 약속이니 지키게 해줘요",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000942.1.8",
          "form": "웅 알았응",
          "original_form": "웅 알았응",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000942.1.9",
          "form": "애들 이제 곧 개학이네",
          "original_form": "애들 이제 곧 개학이네",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000942.1.10",
          "form": "방학 숙제 마무리해야지",
          "original_form": "방학 숙제 마무리해야지",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000942.1.11",
          "form": "독서기록장이랑 다 했지?",
          "original_form": "독서기록장이랑 다 했지?",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000942.1.12",
          "form": "웅 다했지",
          "original_form": "웅 다했지",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000942.1.13",
          "form": "개학하자마자 수행평가 잔뜩이더만",
          "original_form": "개학하자마자 수행평가 잔뜩이더만",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000942.1.14",
          "form": "엥 첫주부터 평가야?",
          "original_form": "엥 첫주부터 평가야?",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000942.1.15",
          "form": "1학기가 아직 일주일 남아서 마지막주에 하는듯",
          "original_form": "1학기가 아직 일주일 남아서 마지막주에 하는듯",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000942.1.16",
          "form": "1학기 내용 기억하려나 ㅋㅋ",
          "original_form": "1학기 내용 기억하려나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000942.1.17",
          "form": "2학기 예습만 했는데 며칠 동안이라도 복습시켜야할듯",
          "original_form": "2학기 예습만 했는데 며칠 동안이라도 복습시켜야할듯",
          "speaker_id": "1",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000942.1.18",
          "form": "바쁘겠구만 ㅋㅋ",
          "original_form": "바쁘겠구만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000942.1.19",
          "form": "내가 학생인 것 같아 ㅎㅎ",
          "original_form": "내가 학생인 것 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000942.1.20",
          "form": "고생이 많다",
          "original_form": "고생이 많다",
          "speaker_id": "2",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000942.1.21",
          "form": "웅 난 진짜 고생이 많음",
          "original_form": "웅 난 진짜 고생이 많음",
          "speaker_id": "1",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000942.1.22",
          "form": "토닥토닥 힘내",
          "original_form": "토닥토닥 힘내",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000942.1.23",
          "form": "ㅎㅎ 2학기도 잘 지내야할텐데",
          "original_form": "ㅎㅎ 2학기도 잘 지내야할텐데",
          "speaker_id": "1",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000942.1.24",
          "form": "평소에 하던 게 있으니 잘 하겠지",
          "original_form": "평소에 하던 게 있으니 잘 하겠지",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000942.1.25",
          "form": "줌수업할 때 보니 우리 배울 때랑 많이 달라서",
          "original_form": "줌수업할 때 보니 우리 배울 때랑 많이 달라서",
          "speaker_id": "1",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000942.1.26",
          "form": "다른 애들도 다 하는데 뭘 ㅎ",
          "original_form": "다른 애들도 다 하는데 뭘 ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000942.1.27",
          "form": "아예 안보면 되는데 자꾸 보면서 걱정이 늘어",
          "original_form": "아예 안보면 되는데 자꾸 보면서 걱정이 늘어",
          "speaker_id": "1",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000942.1.28",
          "form": "애들 하던대로 믿고 하면 돼",
          "original_form": "애들 하던대로 믿고 하면 돼",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000942.1.29",
          "form": "그래야지 ㅎㅎ",
          "original_form": "그래야지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:11"
        }
      ]
    }
  ]
