      "utterance": [
        {
          "id": "MDRW2100034129.1.1",
          "form": "카페가면 주로 어떤거 드세요?",
          "original_form": "카페가면 주로 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034129.1.2",
          "form": "저는 커피를 못마셔서\n프라푸치노나 과일들어간\n음료를 주로 마셔요:)",
          "original_form": "저는 커피를 못마셔서\n프라푸치노나 과일들어간\n음료를 주로 마셔요:)",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100034129.1.3",
          "form": "아하 저도 자몽청 좋아해요ㅋㅋㅋ",
          "original_form": "아하 저도 자몽청 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100034129.1.4",
          "form": "저는 아메아니면 에이드류 주로 마셔요",
          "original_form": "저는 아메아니면 에이드류 주로 마셔요",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100034129.1.5",
          "form": "자몽 맛있죠 ㅋㅋㅋㅋ",
          "original_form": "자몽 맛있죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100034129.1.6",
          "form": "아 아메 좋아하세요?",
          "original_form": "아 아메 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100034129.1.7",
          "form": "네 근데 드립말고 콜드브루 더 좋아해요",
          "original_form": "네 근데 드립말고 콜드브루 더 좋아해요",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100034129.1.8",
          "form": "뭔가 내림방식 차인데 더 깔끔하더라구요ㅎㅎㅎ",
          "original_form": "뭔가 내림방식 차인데 더 깔끔하더라구요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100034129.1.9",
          "form": "커피는 왜 못드세요? 심장?",
          "original_form": "커피는 왜 못드세요? 심장?",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100034129.1.10",
          "form": "커피를 못배웠어요 ㅋㅋㅋ",
          "original_form": "커피를 못배웠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100034129.1.11",
          "form": "전 직장에서도\n콜드브루만 마시던\n동료있었는데 그런 이유에서였나 보네요ㅋ",
          "original_form": "전 직장에서도\n콜드브루만 마시던\n동료있었는데 그런 이유에서였나 보네요ㅋ",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100034129.1.12",
          "form": "ㅋㅋㅋ네 이거도 취향 엄청 갈리더라구요",
          "original_form": "ㅋㅋㅋ네 이거도 취향 엄청 갈리더라구요",
          "speaker_id": "1",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100034129.1.13",
          "form": "제친구는 드립커피만 맛있대요",
          "original_form": "제친구는 드립커피만 맛있대요",
          "speaker_id": "1",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100034129.1.14",
          "form": "커피는 진짜 모르겠어요ㅋㅋ",
          "original_form": "커피는 진짜 모르겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:08"
        },
        {
          "id": "MDRW2100034129.1.15",
          "form": "안마셔서 더 그렇긴 하겠지만\n뭔 종류들이 그렇게나 많고ㅋㅋ",
          "original_form": "안마셔서 더 그렇긴 하겠지만\n뭔 종류들이 그렇게나 많고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034129.1.16",
          "form": "1도 모르겠어요",
          "original_form": "1도 모르겠어요",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034129.1.17",
          "form": "함 도전해보세요 거의 체력포션이에요ㅋㅋㅋ",
          "original_form": "함 도전해보세요 거의 체력포션이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034129.1.18",
          "form": "근데 저도 아메 아니면 못마시긴 해요...^^..",
          "original_form": "근데 저도 아메 아니면 못마시긴 해요...^^..",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034129.1.19",
          "form": "텁텁한걸 무슨 맛으로 먹는건지...",
          "original_form": "텁텁한걸 무슨 맛으로 먹는건지...",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034129.1.20",
          "form": "오호! 아포가토는 먹어보고 싶었어요ㅋㅋ",
          "original_form": "오호! 아포가토는 먹어보고 싶었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034129.1.21",
          "form": "아 그건 맛있죠 커피맛 아이스크림 느낌!",
          "original_form": "아 그건 맛있죠 커피맛 아이스크림 느낌!",
          "speaker_id": "1",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034129.1.22",
          "form": "카페가시면 디저트는 좋아하시나요?",
          "original_form": "카페가시면 디저트는 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034129.1.23",
          "form": "네:) 디저트 좋아해요",
          "original_form": "네:) 디저트 좋아해요",
          "speaker_id": "2",
          "time": "20210827 17:20"
        },
        {
          "id": "MDRW2100034129.1.24",
          "form": "주로 어떤거 드세요?",
          "original_form": "주로 어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210827 17:20"
        },
        {
          "id": "MDRW2100034129.1.25",
          "form": "저는 와플이나 타르트 좋아해요",
          "original_form": "저는 와플이나 타르트 좋아해요",
          "speaker_id": "1",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100034129.1.26",
          "form": "너무 맛있어요..",
          "original_form": "너무 맛있어요..",
          "speaker_id": "1",
          "time": "20210827 17:21"
        }
      ]
