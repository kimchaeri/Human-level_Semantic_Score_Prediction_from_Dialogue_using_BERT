      "utterance": [
        {
          "id": "MDRW2100018376.1.1",
          "form": "게임하시나요?",
          "original_form": "게임하시나요?",
          "speaker_id": "1",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018376.1.2",
          "form": "요즘은 스위치로 게임하고있어요~",
          "original_form": "요즘은 스위치로 게임하고있어요~",
          "speaker_id": "2",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018376.1.3",
          "form": "스위치가 머쥬ㅜㅜ 게임잘몰라서 죄송합니다ㅜㅋ",
          "original_form": "스위치가 머쥬ㅜㅜ 게임잘몰라서 죄송합니다ㅜㅋ",
          "speaker_id": "1",
          "time": "20210827 00:52"
        },
        {
          "id": "MDRW2100018376.1.4",
          "form": "닌텐도는 아는데ㅋㄱ",
          "original_form": "닌텐도는 아는데ㅋㄱ",
          "speaker_id": "1",
          "time": "20210827 00:52"
        },
        {
          "id": "MDRW2100018376.1.5",
          "form": "아 네 닌텐도 스위치 말한거였어요!",
          "original_form": "아 네 닌텐도 스위치 말한거였어요!",
          "speaker_id": "2",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018376.1.6",
          "form": "아 ㅋㅋ  회사동생하는거봤어요ㅋㅋ",
          "original_form": "아 ㅋㅋ  회사동생하는거봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018376.1.7",
          "form": "저는 겜은 거의안하고 이모티콘받으려 미션게임?ㅋㅋ",
          "original_form": "저는 겜은 거의안하고 이모티콘받으려 미션게임?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018376.1.8",
          "form": "ㅋㅋㅋㅋ 저도 평소에는 휴대폰 게임도 잘 안하긴 해요",
          "original_form": "ㅋㅋㅋㅋ 저도 평소에는 휴대폰 게임도 잘 안하긴 해요",
          "speaker_id": "2",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100018376.1.9",
          "form": "예전에 스타랑 총게임하러 남자친구랑 다녔는데\n그남자는 머하나몰겟네요ㅋㅋㅋㅋ",
          "original_form": "예전에 스타랑 총게임하러 남자친구랑 다녔는데\n그남자는 머하나몰겟네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018376.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 예전에 쥬니어 네이버 게임같은건 하셨었나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 예전에 쥬니어 네이버 게임같은건 하셨었나요?",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018376.1.11",
          "form": "아 햇져... 퍼즐같은것도 하고 그거 이제 없어진거맞나요?",
          "original_form": "아 햇져... 퍼즐같은것도 하고 그거 이제 없어진거맞나요?",
          "speaker_id": "1",
          "time": "20210827 01:03"
        },
        {
          "id": "MDRW2100018376.1.12",
          "form": "몇년전에 없어졌던거 같아요",
          "original_form": "몇년전에 없어졌던거 같아요",
          "speaker_id": "2",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018376.1.13",
          "form": "그니까요 그거 은근 재미난거많았는데",
          "original_form": "그니까요 그거 은근 재미난거많았는데",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018376.1.14",
          "form": "닌텐도스위치 그거 팩 신작도 많던데",
          "original_form": "닌텐도스위치 그거 팩 신작도 많던데",
          "speaker_id": "1",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018376.1.15",
          "form": "계속 사시나요?ㅋ",
          "original_form": "계속 사시나요?ㅋ",
          "speaker_id": "1",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018376.1.16",
          "form": "ㅋㅋㅋㅋ 사실 링피트랑 저스트 댄스로 운동해볼까 하고 산거여서",
          "original_form": "ㅋㅋㅋㅋ 사실 링피트랑 저스트 댄스로 운동해볼까 하고 산거여서",
          "speaker_id": "2",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018376.1.17",
          "form": "아직 다른 게임은 별로 구매 안했어요",
          "original_form": "아직 다른 게임은 별로 구매 안했어요",
          "speaker_id": "2",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100018376.1.18",
          "form": "저스트댄스 열심히따라하시나여ㅋㅋㅋ",
          "original_form": "저스트댄스 열심히따라하시나여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:08"
        },
        {
          "id": "MDRW2100018376.1.19",
          "form": "저 그거 pc로 연동해서 가끔해요ㅋㅋ",
          "original_form": "저 그거 pc로 연동해서 가끔해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:08"
        },
        {
          "id": "MDRW2100018376.1.20",
          "form": "ㅋㅋㅋㅋ  재밌어서 자주 하는것 같아요",
          "original_form": "ㅋㅋㅋㅋ  재밌어서 자주 하는것 같아요",
