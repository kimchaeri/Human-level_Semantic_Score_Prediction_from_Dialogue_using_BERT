      "utterance": [
        {
          "id": "MDRW2100006407.1.1",
          "form": "동물 기르시나요?",
          "original_form": "동물 기르시나요?",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100006407.1.2",
          "form": "부모님집에 개한마리요ㅋ",
          "original_form": "부모님집에 개한마리요ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100006407.1.3",
          "form": "동물키우세요?",
          "original_form": "동물키우세요?",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100006407.1.4",
          "form": "전 동물 좋아하는데 기르진 않아요",
          "original_form": "전 동물 좋아하는데 기르진 않아요",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100006407.1.5",
          "form": "책임감도 필요하고 돈도많이들어서",
          "original_form": "책임감도 필요하고 돈도많이들어서",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006407.1.6",
          "form": "애키우는거랑 같다고 하더라구요",
          "original_form": "애키우는거랑 같다고 하더라구요",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006407.1.7",
          "form": "믹스견인데 할머니라 오늘내일해요",
          "original_form": "믹스견인데 할머니라 오늘내일해요",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006407.1.8",
          "form": "무지개다리건너면 슬프시겠어요",
          "original_form": "무지개다리건너면 슬프시겠어요",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100006407.1.9",
          "form": "보낼때 넘 맘아파서 전 못키우겠어요",
          "original_form": "보낼때 넘 맘아파서 전 못키우겠어요",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100006407.1.10",
          "form": "엄마도 다시는 안키운다고",
          "original_form": "엄마도 다시는 안키운다고",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100006407.1.11",
          "form": "그러게요. 수명이 짧아서 먼저 가니깐요",
          "original_form": "그러게요. 수명이 짧아서 먼저 가니깐요",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100006407.1.12",
          "form": "그래도 키울땐 애교부리고 의지되고 기쁨조예요",
          "original_form": "그래도 키울땐 애교부리고 의지되고 기쁨조예요",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100006407.1.13",
          "form": "저도 보기만해 좋아요~~",
          "original_form": "저도 보기만해 좋아요~~",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006407.1.14",
          "form": "조건없이 내편이 되어주는~",
          "original_form": "조건없이 내편이 되어주는~",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006407.1.15",
          "form": "기회됨 키워보고싶긴해요",
          "original_form": "기회됨 키워보고싶긴해요",
          "speaker_id": "1",
