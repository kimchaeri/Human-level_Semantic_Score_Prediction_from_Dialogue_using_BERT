      "utterance": [
        {
          "id": "MDRW2100019215.1.1",
          "form": "이사만이 답이죠ㅠㅜ",
          "original_form": "이사만이 답이죠ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019215.1.2",
          "form": "습기때문에 이사를 생각하시다니ㅠ",
          "original_form": "습기때문에 이사를 생각하시다니ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100019215.1.3",
          "form": "그것보다도ㅋㅋㅋㅋ",
          "original_form": "그것보다도ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019215.1.4",
          "form": "오늘도 습한건 마찬가지네요ㅎㅎ",
          "original_form": "오늘도 습한건 마찬가지네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019215.1.5",
          "form": "다른 이유가 있으셨나요?",
          "original_form": "다른 이유가 있으셨나요?",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019215.1.6",
          "form": "맞아요 습하죠ㅠ",
          "original_form": "맞아요 습하죠ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019215.1.7",
          "form": "전세는 언젠간 가야하니깐요ㅠㅜ",
          "original_form": "전세는 언젠간 가야하니깐요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019215.1.8",
          "form": "아하... 전세는 생각도 못했네요",
          "original_form": "아하... 전세는 생각도 못했네요",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019215.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019215.1.10",
          "form": "뭐 중요한건 아니닌깐요ㅎㅎ",
          "original_form": "뭐 중요한건 아니닌깐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019215.1.11",
          "form": "다음번에 집 구하실 때는 습기가 기준이 되는건가요",
          "original_form": "다음번에 집 구하실 때는 습기가 기준이 되는건가요",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019215.1.12",
          "form": "기준 붕 하나가 될거 같네요ㅎㅎ",
          "original_form": "기준 붕 하나가 될거 같네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019215.1.13",
          "form": "웃프네요",
          "original_form": "웃프네요",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019215.1.14",
          "form": "인생이라는게ㅋㅋㅋ",
          "original_form": "인생이라는게ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019215.1.15",
          "form": "그러네요ㅋㅋㅋ",
          "original_form": "그러네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019215.1.16",
          "form": "그럼 웃기고 재밌는 일도 있겠죠!",
          "original_form": "그럼 웃기고 재밌는 일도 있겠죠!",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019215.1.17",
          "form": "그러길 빌어요!!",
          "original_form": "그러길 빌어요!!",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019215.1.18",
          "form": "얼른 날씨 좋아져서 놀러가고 싶네요ㅎㅎ",
          "original_form": "얼른 날씨 좋아져서 놀러가고 싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:06"
        }
      ]
