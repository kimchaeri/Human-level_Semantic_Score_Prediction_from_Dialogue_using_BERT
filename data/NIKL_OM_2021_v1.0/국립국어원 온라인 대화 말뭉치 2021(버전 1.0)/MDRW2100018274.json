      "utterance": [
        {
          "id": "MDRW2100018274.1.1",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018274.1.2",
          "form": "앗 배달은 잘 안 시켜먹어요 엄마가 음식을 잘 하셔서...",
          "original_form": "앗 배달은 잘 안 시켜먹어요 엄마가 음식을 잘 하셔서...",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018274.1.3",
          "form": "아니면 포장!!",
          "original_form": "아니면 포장!!",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018274.1.4",
          "form": "어머니께서 요리를 잘하시는구나",
          "original_form": "어머니께서 요리를 잘하시는구나",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018274.1.5",
          "form": "어머니께서 뭘 제일 잘하세요?",
          "original_form": "어머니께서 뭘 제일 잘하세요?",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018274.1.6",
          "form": "진짜 다 잘하시는데",
          "original_form": "진짜 다 잘하시는데",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018274.1.7",
          "form": "김치찌개요 ㅋㅋㅋ",
          "original_form": "김치찌개요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018274.1.8",
          "form": "제가 좋아해서 그런가...",
          "original_form": "제가 좋아해서 그런가...",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018274.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ부럽네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ부럽네요",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018274.1.10",
          "form": "김치찌개에 보통 돼지고기인가요 참치인가요?",
          "original_form": "김치찌개에 보통 돼지고기인가요 참치인가요?",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018274.1.11",
          "form": "전 참치파요",
          "original_form": "전 참치파요",
          "speaker_id": "2",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100018274.1.12",
          "form": "존맛",
          "original_form": "존맛",
          "speaker_id": "2",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100018274.1.13",
          "form": "참치>>꽁치>>돼지",
          "original_form": "참치>>꽁치>>돼지",
          "speaker_id": "2",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100018274.1.14",
          "form": "전 돼지>>참치>>꽁치예요 ㅋㅋㅋㅋ",
          "original_form": "전 돼지>>참치>>꽁치예요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:11"
        },
        {
          "id": "MDRW2100018274.1.15",
          "form": "오 ㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018274.1.16",
          "form": "사실 셋다 맛있죠",
          "original_form": "사실 셋다 맛있죠",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018274.1.17",
          "form": "돼지도 고기가 야들야들하먄존맛탱",
          "original_form": "돼지도 고기가 야들야들하먄존맛탱",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018274.1.18",
          "form": "맞아요 사실 김치찌개면 다맛잇어요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 사실 김치찌개면 다맛잇어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018274.1.19",
          "form": "맞아요 저 김치 진짜 좋아하거든요ㅜㅜ",
          "original_form": "맞아요 저 김치 진짜 좋아하거든요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018274.1.20",
          "form": "김치를 만두피처럼 해서 안에 만두소 넣어서",
          "original_form": "김치를 만두피처럼 해서 안에 만두소 넣어서",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018274.1.21",
          "form": "드셔보세요",
          "original_form": "드셔보세요",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018274.1.22",
          "form": "ㄹㅇ개전맛",
          "original_form": "ㄹㅇ개전맛",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018274.1.23",
          "form": "오오 김치 진짜 좋아하시는구나",
          "original_form": "오오 김치 진짜 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018274.1.24",
