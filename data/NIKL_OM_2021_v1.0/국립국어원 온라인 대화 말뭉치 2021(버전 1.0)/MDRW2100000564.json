      "utterance": [
        {
          "id": "MDRW2100000564.1.1",
          "form": "쇼핑은 좋아하시나요?",
          "original_form": "쇼핑은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000564.1.2",
          "form": "쇼핑좋죠~ 요즘 플립3나온거 사고싶어요 ㅠ",
          "original_form": "쇼핑좋죠~ 요즘 플립3나온거 사고싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000564.1.3",
          "form": "와ㅡ너무좋죠ㅠ",
          "original_form": "와ㅡ너무좋죠ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000564.1.4",
          "form": "아직폰바꾼지 1년되가지고 비용이 좀 부담되서 못바꾸겠어요 ㅠ",
          "original_form": "아직폰바꾼지 1년되가지고 비용이 좀 부담되서 못바꾸겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000564.1.5",
          "form": "엇 아직1년이라니저는2년은 채웠답니다",
          "original_form": "엇 아직1년이라니저는2년은 채웠답니다",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000564.1.6",
          "form": "노트20울트라 사전예약으로 백넘게주고삿는데 지금와서 또 갈아타기엔 좀 아까워 ㅋㅋㅋㅋ",
          "original_form": "노트20울트라 사전예약으로 백넘게주고삿는데 지금와서 또 갈아타기엔 좀 아까워 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000564.1.7",
          "form": "오 핸드폰뭐에여?",
          "original_form": "오 핸드폰뭐에여?",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000564.1.8",
          "form": "와.!그것도 제게는 최신폰. ㅋㅋ전노트9",
          "original_form": "와.!그것도 제게는 최신폰. ㅋㅋ전노트9",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000564.1.9",
          "form": "갈아타고싶은데 살수가없네요ㅠ요즘폰다 너무비싸요ㅠ",
          "original_form": "갈아타고싶은데 살수가없네요ㅠ요즘폰다 너무비싸요ㅠ",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000564.1.10",
          "form": "저 이폰갈아타기전에 노트9썻었어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저 이폰갈아타기전에 노트9썻었어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000564.1.11",
          "form": "역시 갤럭시 최고",
          "original_form": "역시 갤럭시 최고",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000564.1.12",
          "form": "맞아용 ㅋㅋ 갤럭시가 너무. 편해요 아이폰은 못쓰겠더라구요",
          "original_form": "맞아용 ㅋㅋ 갤럭시가 너무. 편해요 아이폰은 못쓰겠더라구요",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000564.1.13",
          "form": "아이폰단점이 허용되지않는 어플 못깔드라구여 ㅜ 카메라는 무조건 아이폰이긴한데",
          "original_form": "아이폰단점이 허용되지않는 어플 못깔드라구여 ㅜ 카메라는 무조건 아이폰이긴한데",
          "speaker_id": "2",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000564.1.14",
          "form": "맞어요ㅋㅋ 그리고 모든게 캡쳐가가능!!",
          "original_form": "맞어요ㅋㅋ 그리고 모든게 캡쳐가가능!!",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000564.1.15",
          "form": "이쁨 .ㅋㅋ",
          "original_form": "이쁨 .ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000564.1.16",
          "form": "맞아요 ㅠㅠㅠ 애플워치보면 또 사고싶긴한데 잘깨지고 추위도타고 더위도타고 사람도아니고",
          "original_form": "맞아요 ㅠㅠㅠ 애플워치보면 또 사고싶긴한데 잘깨지고 추위도타고 더위도타고 사람도아니고",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000564.1.17",
          "form": "ㅋㅋㅋㅋㅋ 아그런게 있나요",
          "original_form": "ㅋㅋㅋㅋㅋ 아그런게 있나요",
          "speaker_id": "1",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000564.1.18",
          "form": "아이폰 추위 더위타기로 유명ㅎㅐ요 ㅋㅋㅋㅋ",
          "original_form": "아이폰 추위 더위타기로 유명ㅎㅐ요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
