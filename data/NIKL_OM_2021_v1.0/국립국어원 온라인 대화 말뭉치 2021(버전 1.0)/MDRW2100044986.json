      "utterance": [
        {
          "id": "MDRW2100044986.1.1",
          "form": "철창 닫히는 소리에",
          "original_form": "철창 닫히는 소리에",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044986.1.2",
          "form": "길막이가 하하하를 잘따랐으니",
          "original_form": "길막이가 하하하를 잘따랐으니",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044986.1.3",
          "form": "기겁을 하고 펄쩍 뛰는데",
          "original_form": "기겁을 하고 펄쩍 뛰는데",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044986.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ같이 잘 따르는걸수도",
          "original_form": "ㅋㅋㅋㅋㅋㅋ같이 잘 따르는걸수도",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044986.1.5",
          "form": "저는 매탈남 점남이를 보면",
          "original_form": "저는 매탈남 점남이를 보면",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044986.1.6",
          "form": "정말 아무 일도 없다는 듯이...",
          "original_form": "정말 아무 일도 없다는 듯이...",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044986.1.7",
          "form": "연님이가 생각나",
          "original_form": "연님이가 생각나",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044986.1.8",
          "form": "만지지는 말되 관심은 가지고 모든 행사에 참석해주는",
          "original_form": "만지지는 말되 관심은 가지고 모든 행사에 참석해주는",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044986.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044986.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.11",
          "form": "그래도 연님이도",
          "original_form": "그래도 연님이도",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.12",
          "form": "하하하 좋아하는 티는 났어",
          "original_form": "하하하 좋아하는 티는 났어",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.13",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아근데 진짜 귀여워",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아근데 진짜 귀여워",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.15",
          "form": "사나운 성격에",
          "original_form": "사나운 성격에",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.16",
          "form": "손타는거 빼고 다 해준거 보면",
          "original_form": "손타는거 빼고 다 해준거 보면",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.17",
          "form": "연님이 사나운데",
          "original_form": "연님이 사나운데",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.18",
          "form": "진짜 좋아한거지..",
          "original_form": "진짜 좋아한거지..",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.19",
          "form": "애정있어",
          "original_form": "애정있어",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.20",
          "form": "산책도같이가고",
          "original_form": "산책도같이가고",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044986.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ넘우 구엽자나",
          "original_form": "ㅋㅋㅋㅋㅋㅋ넘우 구엽자나",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044986.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ연님이.. 멋진 모험을 하고 있니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ연님이.. 멋진 모험을 하고 있니",
