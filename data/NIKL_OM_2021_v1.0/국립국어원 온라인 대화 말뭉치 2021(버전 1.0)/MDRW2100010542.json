      "utterance": [
        {
          "id": "MDRW2100010542.1.1",
          "form": "주위에 친구들이 결혼을 슬슬 하네요 ㅋㅋ",
          "original_form": "주위에 친구들이 결혼을 슬슬 하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010542.1.2",
          "form": "어! 제 주변에도요~",
          "original_form": "어! 제 주변에도요~",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010542.1.3",
          "form": "원래 작년에 한다고 했었는데",
          "original_form": "원래 작년에 한다고 했었는데",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010542.1.4",
          "form": "다 올해로 연기되가지고",
          "original_form": "다 올해로 연기되가지고",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010542.1.5",
          "form": "올해 많이들 가네요~",
          "original_form": "올해 많이들 가네요~",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010542.1.6",
          "form": "코로나로 연기되나보네요",
          "original_form": "코로나로 연기되나보네요",
          "speaker_id": "1",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010542.1.7",
          "form": "더이상 연기할수도 별로 나아질기미도 없어서",
          "original_form": "더이상 연기할수도 별로 나아질기미도 없어서",
          "speaker_id": "1",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010542.1.8",
          "form": "쩔수없나봐요",
          "original_form": "쩔수없나봐요",
          "speaker_id": "1",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010542.1.9",
          "form": "그런 거 같아요~",
          "original_form": "그런 거 같아요~",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010542.1.10",
          "form": "안타깝기도하고..ㅠㅠ",
          "original_form": "안타깝기도하고..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010542.1.11",
          "form": "부럽기도 하고~",
          "original_form": "부럽기도 하고~",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010542.1.12",
          "form": "제주위에 결혼한남편보니까 아주아주 착해요",
          "original_form": "제주위에 결혼한남편보니까 아주아주 착해요",
          "speaker_id": "1",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100010542.1.13",
          "form": "착한남자만 품절되나봐요",
          "original_form": "착한남자만 품절되나봐요",
          "speaker_id": "1",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100010542.1.14",
          "form": "좋은 사람들은 확실히 젊을 때 빨리들 결혼하는 거 같더라고요~",
          "original_form": "좋은 사람들은 확실히 젊을 때 빨리들 결혼하는 거 같더라고요~",
          "speaker_id": "2",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010542.1.15",
          "form": "음 오래오래 행복하길 바랄뿐ㅜ ㅋㅋㅋ 한 삼년은 신혼이라 행복한거 같아요",
          "original_form": "음 오래오래 행복하길 바랄뿐ㅜ ㅋㅋㅋ 한 삼년은 신혼이라 행복한거 같아요",
          "speaker_id": "1",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010542.1.16",
          "form": "ㅎㅎㅎ 삼년 지나면..",
          "original_form": "ㅎㅎㅎ 삼년 지나면..",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010542.1.17",
          "form": "그때부터 의리로 살게되는 건가요??ㅎ",
          "original_form": "그때부터 의리로 살게되는 건가요??ㅎ",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010542.1.18",
          "form": "아기태어나니까 정신없나봐요",
          "original_form": "아기태어나니까 정신없나봐요",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010542.1.19",
          "form": "아~ 맞아요 그런거 같더라고요~",
          "original_form": "아~ 맞아요 그런거 같더라고요~",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010542.1.20",
          "form": "확실히 아기가 생기면",
          "original_form": "확실히 아기가 생기면",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010542.1.21",
          "form": "정신도 없고 책임감은 더 커지고",
          "original_form": "정신도 없고 책임감은 더 커지고",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010542.1.22",
          "form": "아기는 울고 나도울고..그런대요 ㅜ ㅋㅋ",
          "original_form": "아기는 울고 나도울고..그런대요 ㅜ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010542.1.23",
          "form": "아이고...ㅠㅠ첫아이때는 정말 힘들겠어요",
          "original_form": "아이고...ㅠㅠ첫아이때는 정말 힘들겠어요",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010542.1.24",
          "form": "육아는 어려운거니까 사이좋게 분담해서 나아가면 좋겠다는 친구들을 향한 개인적인 바램ㅎㅎ",
          "original_form": "육아는 어려운거니까 사이좋게 분담해서 나아가면 좋겠다는 친구들을 향한 개인적인 바램ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100010542.1.25",
          "form": "그쵸그쵸 독박육아는 정말 아니될말!!",
          "original_form": "그쵸그쵸 독박육아는 정말 아니될말!!",
          "speaker_id": "2",
          "time": "20210823 20:11"
        }
      ]
    }
