      "utterance": [
        {
          "id": "MDRW2100039247.1.1",
          "form": "노래 듣는것 좋아하시나요?",
          "original_form": "노래 듣는것 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039247.1.2",
          "form": "좋아해요~ 저는 치즈노래좋아해요",
          "original_form": "좋아해요~ 저는 치즈노래좋아해요",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039247.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039247.1.4",
          "form": "저도 노래듣는거 좋아해요~",
          "original_form": "저도 노래듣는거 좋아해요~",
          "speaker_id": "1",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039247.1.5",
          "form": "치즈 노래 정말 좋죠",
          "original_form": "치즈 노래 정말 좋죠",
          "speaker_id": "1",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039247.1.6",
          "form": "말씀주신 김에 지금 틀어야겠어요",
          "original_form": "말씀주신 김에 지금 틀어야겠어요",
          "speaker_id": "1",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039247.1.7",
          "form": "저도 듣고싶은데 회사라 흑흑",
          "original_form": "저도 듣고싶은데 회사라 흑흑",
          "speaker_id": "2",
          "time": "20210903 15:08"
        },
        {
          "id": "MDRW2100039247.1.8",
          "form": "ㅠㅠ 전 몰래 틀었어요 ㅋㅋ",
          "original_form": "ㅠㅠ 전 몰래 틀었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:09"
        },
        {
          "id": "MDRW2100039247.1.9",
          "form": "오늘의 기분 틀었습니다 ㅋㅋㅋ",
          "original_form": "오늘의 기분 틀었습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:09"
        },
        {
          "id": "MDRW2100039247.1.10",
          "form": "헉 그건 모르는 노래네눀ㅋㅋㅋㅋ",
          "original_form": "헉 그건 모르는 노래네눀ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100039247.1.11",
          "form": "저도들어봐여지",
          "original_form": "저도들어봐여지",
          "speaker_id": "2",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100039247.1.12",
          "form": "이어폰 몰래 껴봅니다",
          "original_form": "이어폰 몰래 껴봅니다",
          "speaker_id": "2",
          "time": "20210903 15:11"
        },
        {
          "id": "MDRW2100039247.1.13",
          "form": "ㅋㅋ회사에서 노래 몰래 들으려고 무선이어폰샀습니다",
          "original_form": "ㅋㅋ회사에서 노래 몰래 들으려고 무선이어폰샀습니다",
          "speaker_id": "1",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100039247.1.14",
          "form": "저도 이번에 폰 사면서 받았어요",
          "original_form": "저도 이번에 폰 사면서 받았어요",
          "speaker_id": "2",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100039247.1.15",
          "form": "노래 좀 추천해주세욬ㅋㅋㅋ",
          "original_form": "노래 좀 추천해주세욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100039247.1.16",
          "form": "저는 아이돌노래 즐겨 들어요 ㅋㅋ 힘나서요 ㅋㅋㅋ",
          "original_form": "저는 아이돌노래 즐겨 들어요 ㅋㅋ 힘나서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100039247.1.17",
          "form": "아이돌노래 들으시나요?",
          "original_form": "아이돌노래 들으시나요?",
          "speaker_id": "1",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100039247.1.18",
          "form": "어 네 들어요",
          "original_form": "어 네 들어요",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100039247.1.19",
          "form": "여자친구같는노래좋아하는데",
          "original_form": "여자친구같는노래좋아하는데",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100039247.1.20",
          "form": "오 약간 벅차오르는 그런 느낌 좋아하세요? 소녀소녀하면서도",
          "original_form": "오 약간 벅차오르는 그런 느낌 좋아하세요? 소녀소녀하면서도",
          "speaker_id": "1",
          "time": "20210903 15:23"
        },
        {
          "id": "MDRW2100039247.1.21",
          "form": "헉 완점 취향저격이에요!!",
          "original_form": "헉 완점 취향저격이에요!!",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100039247.1.22",
          "form": "여자친구 노래랑은 조금 다르지만",
          "original_form": "여자친구 노래랑은 조금 다르지만",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100039247.1.23",
          "form": "우주소녀 노래들도 그런 느낌이에요!! 벅차오르는 기분",
          "original_form": "우주소녀 노래들도 그런 느낌이에요!! 벅차오르는 기분",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100039247.1.24",
          "form": "오 우주소녀도좋아요! 러블리즈나",
          "original_form": "오 우주소녀도좋아요! 러블리즈나",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039247.1.25",
          "form": "고 라인 벅차오르는 라인 좋아요",
          "original_form": "고 라인 벅차오르는 라인 좋아요",
