      "utterance": [
        {
          "id": "MDRW2100044043.1.1",
          "form": "name1님은 또 하시는 게임 있으신가요?",
          "original_form": "&name1&님은 또 하시는 게임 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044043.1.2",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044043.1.3",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044043.1.4",
          "form": "막",
          "original_form": "막",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044043.1.5",
          "form": "퍼즐게임도좋아해요",
          "original_form": "퍼즐게임도좋아해요",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044043.1.6",
          "form": "보드게임카페",
          "original_form": "보드게임카페",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044043.1.7",
          "form": "가셔보셨는지오",
          "original_form": "가셔보셨는지오",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044043.1.8",
          "form": "저 방탈출도좋아한답니다",
          "original_form": "저 방탈출도좋아한답니다",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044043.1.9",
          "form": "방탈출게임 잘하시나요??",
          "original_form": "방탈출게임 잘하시나요??",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044043.1.10",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044043.1.11",
          "form": "꽤나합니다",
          "original_form": "꽤나합니다",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044043.1.12",
          "form": "사실 힌트 냅다 써버려서",
          "original_form": "사실 힌트 냅다 써버려서",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044043.1.13",
          "form": "그냥 탈출을목적으로하는",
          "original_form": "그냥 탈출을목적으로하는",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044043.1.14",
          "form": "그런게임을좋아하긴합니다",
          "original_form": "그런게임을좋아하긴합니다",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044043.1.15",
          "form": "그래도뭔가 퍼즐문제를푸는게",
          "original_form": "그래도뭔가 퍼즐문제를푸는게",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044043.1.16",
          "form": "너무재밌네요",
          "original_form": "너무재밌네요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044043.1.17",
          "form": "그래도 미션 다 완료하시면 잘하시는거죠",
          "original_form": "그래도 미션 다 완료하시면 잘하시는거죠",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044043.1.18",
          "form": "껄껄",
          "original_form": "껄껄",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044043.1.19",
          "form": "감사합니댜",
          "original_form": "감사합니댜",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044043.1.20",
          "form": "방탈출",
          "original_form": "방탈출",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044043.1.21",
          "form": "잘하시나요",
          "original_form": "잘하시나요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044043.1.22",
          "form": "저는 저희동네에서 하는거 너무 질려서",
          "original_form": "저는 저희동네에서 하는거 너무 질려서",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044043.1.23",
          "form": "이제 좀더 큰곳으로가야할거같네요",
          "original_form": "이제 좀더 큰곳으로가야할거같네요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044043.1.24",
          "form": "저는 잘 못해요",
          "original_form": "저는 잘 못해요",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044043.1.25",
          "form": "메이플 몇 년 하셨나요?",
          "original_form": "메이플 몇 년 하셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044043.1.26",
          "form": "이야...",
          "original_form": "이야...",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044043.1.27",
          "form": "저 몇달전에시작했는데",
          "original_form": "저 몇달전에시작했는데",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044043.1.28",
          "form": "케릭터 한 10개있고",
          "original_form": "케릭터 한 10개있고",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044043.1.29",
          "form": "좀 제가 근성이없어서",
          "original_form": "좀 제가 근성이없어서",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044043.1.30",
          "form": "하다가 새로만들고",
          "original_form": "하다가 새로만들고",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044043.1.31",
          "form": "뭔가 케릭터이름짓고",
          "original_form": "뭔가 케릭터이름짓고",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044043.1.32",
          "form": "어떤능력있는지확인하다가",
          "original_form": "어떤능력있는지확인하다가",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044043.1.33",
          "form": "재밌으면하는거죵",
          "original_form": "재밌으면하는거죵",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044043.1.34",
          "form": "근데메이플 너무 망겜이에요",
          "original_form": "근데메이플 너무 망겜이에요",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044043.1.35",
          "form": "너무 ..",
          "original_form": "너무 ..",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044043.1.36",
          "form": "노가다가 너무 심하네요..",
          "original_form": "노가다가 너무 심하네요..",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044043.1.37",
          "form": "메이플이 돈질 안하면 어려운 게임이라 들었는데 사실인가요?",
          "original_form": "메이플이 돈질 안하면 어려운 게임이라 들었는데 사실인가요?",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044043.1.38",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044043.1.39",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044043.1.40",
          "form": "종종",
          "original_form": "종종",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044043.1.41",
          "form": "하고싶은욕구가",
          "original_form": "하고싶은욕구가",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044043.1.42",
          "form": "피어오르네요",
          "original_form": "피어오르네요",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044043.1.43",
          "form": "하지만 저는",
          "original_form": "하지만 저는",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044043.1.44",
          "form": "돈이없는 가난한 사람이라",
          "original_form": "돈이없는 가난한 사람이라",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044043.1.45",
          "form": "하지않았습니다",
          "original_form": "하지않았습니다",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044043.1.46",
          "form": "아직까진...",
          "original_form": "아직까진...",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044043.1.47",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044043.1.48",
          "form": "사실 게임현질이 한 번 하면 계속 하게 된다고 해서 망설여지네요",
          "original_form": "사실 게임현질이 한 번 하면 계속 하게 된다고 해서 망설여지네요",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044043.1.49",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044043.1.50",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044043.1.51",
          "form": "그리고 메이플이",
          "original_form": "그리고 메이플이",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044043.1.52",
          "form": "확률이 점점 낮아진다네요",
          "original_form": "확률이 점점 낮아진다네요",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044043.1.53",
          "form": "뭔가 초심자의 행운이라고해야하나",
          "original_form": "뭔가 초심자의 행운이라고해야하나",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044043.1.54",
          "form": "처음에는 랜덤뽑기가 잘 나오는데",
          "original_form": "처음에는 랜덤뽑기가 잘 나오는데",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044043.1.55",
          "form": "하면할수록 똥 확률로",
          "original_form": "하면할수록 똥 확률로",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044043.1.56",
          "form": "나온데요",
          "original_form": "나온데요",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044043.1.57",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 21:40"
        }
      ]
