      "utterance": [
        {
          "id": "MDRW2100028375.1.1",
          "form": "너는 게임해?",
          "original_form": "너는 게임해?",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028375.1.2",
          "form": "응 게임하지",
          "original_form": "응 게임하지",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028375.1.3",
          "form": "컴퓨터 게임?",
          "original_form": "컴퓨터 게임?",
          "speaker_id": "1",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028375.1.4",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028375.1.5",
          "form": "혹시 롤?",
          "original_form": "혹시 롤?",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028375.1.6",
          "form": "아니 오버워치",
          "original_form": "아니 오버워치",
          "speaker_id": "2",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028375.1.7",
          "form": "너 오버워치해?",
          "original_form": "너 오버워치해?",
          "speaker_id": "2",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028375.1.8",
          "form": "나 옛날에 했었어",
          "original_form": "나 옛날에 했었어",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028375.1.9",
          "form": "랭크 몇인데?",
          "original_form": "랭크 몇인데?",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028375.1.10",
          "form": "나 마스터였어^^",
          "original_form": "나 마스터였어^^",
          "speaker_id": "1",
          "time": "20210903 10:28"
        },
        {
          "id": "MDRW2100028375.1.11",
          "form": "엥 거짓말하지마 ㅋㅋㅋㅋ",
          "original_form": "엥 거짓말하지마 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100028375.1.12",
          "form": "진짜야 나 잘해",
          "original_form": "진짜야 나 잘해",
          "speaker_id": "1",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100028375.1.13",
          "form": "너는 최고 티어 뭐야?",
          "original_form": "너는 최고 티어 뭐야?",
          "speaker_id": "1",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100028375.1.14",
          "form": "나는 최고티어 브론즌데...",
          "original_form": "나는 최고티어 브론즌데...",
          "speaker_id": "2",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100028375.1.15",
          "form": "오 멋있다",
          "original_form": "오 멋있다",
          "speaker_id": "1",
          "time": "20210903 10:32"
        },
        {
          "id": "MDRW2100028375.1.16",
          "form": "주 포지가 뭔데?",
          "original_form": "주 포지가 뭔데?",
          "speaker_id": "1",
          "time": "20210903 10:32"
        },
        {
          "id": "MDRW2100028375.1.17",
          "form": "주로 힐탱하고 있어",
          "original_form": "주로 힐탱하고 있어",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100028375.1.18",
          "form": "딜러는 안해?",
          "original_form": "딜러는 안해?",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100028375.1.19",
          "form": "딜러는 거의 안해요",
          "original_form": "딜러는 거의 안해요",
          "speaker_id": "2",
          "time": "20210904 21:08"
        },
        {
          "id": "MDRW2100028375.1.20",
          "form": "매칭시간이 길어서?",
          "original_form": "매칭시간이 길어서?",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100028375.1.21",
          "form": "응 맞아!",
          "original_form": "응 맞아!",
          "speaker_id": "2",
          "time": "20210904 21:57"
        },
        {
          "id": "MDRW2100028375.1.22",
          "form": "너는 무슨 포지션 주로 해?",
          "original_form": "너는 무슨 포지션 주로 해?",
          "speaker_id": "2",
          "time": "20210904 21:58"
        },
        {
          "id": "MDRW2100028375.1.23",
          "form": "나는 힐러!",
          "original_form": "나는 힐러!",
          "speaker_id": "1",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100028375.1.24",
          "form": "어떤 캐릭이 주캐야?",
          "original_form": "어떤 캐릭이 주캐야?",
          "speaker_id": "2",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100028375.1.25",
          "form": "나는 아나!",
          "original_form": "나는 아나!",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100028375.1.26",
          "form": "오 그렇구나 나는 주로 모이라를 해 ㅎㅎ",
          "original_form": "오 그렇구나 나는 주로 모이라를 해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100028375.1.27",
          "form": "오 모이라 하는 사람 별로 없든데",
          "original_form": "오 모이라 하는 사람 별로 없든데",
          "speaker_id": "1",
          "time": "20210904 22:02"
        }
      ]
