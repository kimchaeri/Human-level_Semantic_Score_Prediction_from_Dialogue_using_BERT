      "utterance": [
        {
          "id": "MDRW2100005095.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005095.1.2",
          "form": "안녕히세요!",
          "original_form": "안녕히세요!",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005095.1.3",
          "form": "무슨 스포츠 좋아하세요?",
          "original_form": "무슨 스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005095.1.4",
          "form": "저는 배구나 수영 좋아해요!",
          "original_form": "저는 배구나 수영 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005095.1.5",
          "form": "저도 배구 좋아해요!!",
          "original_form": "저도 배구 좋아해요!!",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005095.1.6",
          "form": "이번에 올림픽 보셨어요??",
          "original_form": "이번에 올림픽 보셨어요??",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005095.1.7",
          "form": "네!! 사실 그 전부터 조금 관심있었는데 올림픽보고 완전 빠져버렸어요",
          "original_form": "네!! 사실 그 전부터 조금 관심있었는데 올림픽보고 완전 빠져버렸어요",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005095.1.8",
          "form": "오 저도요!!",
          "original_form": "오 저도요!!",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005095.1.9",
          "form": "중학교 때 배구 많이 했었는데",
          "original_form": "중학교 때 배구 많이 했었는데",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005095.1.10",
          "form": "올림픽 보고 나니까 다시 하고 싶더라고요ㅜ",
          "original_form": "올림픽 보고 나니까 다시 하고 싶더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005095.1.11",
          "form": "저는 중3 체육시간때 처음 배구해봤는데",
          "original_form": "저는 중3 체육시간때 처음 배구해봤는데",
          "speaker_id": "1",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005095.1.12",
          "form": "그땐 그냥 아프기만하고.. 별로 재미없었는데",
          "original_form": "그땐 그냥 아프기만하고.. 별로 재미없었는데",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005095.1.13",
          "form": "지금은 왜 그 매력을 몰랐나 싶어요ㅠ",
          "original_form": "지금은 왜 그 매력을 몰랐나 싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005095.1.14",
          "form": "저도 초중고 졸업하고 나니까 체육시간 때 왜 열심히 안 하고 재미 못 느꼈나 싶더라고요",
          "original_form": "저도 초중고 졸업하고 나니까 체육시간 때 왜 열심히 안 하고 재미 못 느꼈나 싶더라고요",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005095.1.15",
          "form": "저는 땀흘리는 걸 좋아하긴 했는데 시작하기까지가 너무 힘들더라구요 귀찮아하고 ..",
          "original_form": "저는 땀흘리는 걸 좋아하긴 했는데 시작하기까지가 너무 힘들더라구요 귀찮아하고 ..",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005095.1.16",
          "form": "저는 아직 고딩인데 학년이 올라갈수록 체육시감이 주는게 슬퍼요ㅠ",
          "original_form": "저는 아직 고딩인데 학년이 올라갈수록 체육시감이 주는게 슬퍼요ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005095.1.17",
          "form": "아 고등학생이시구나ㅜ",
          "original_form": "아 고등학생이시구나ㅜ",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005095.1.18",
          "form": "고등학교 땐 솔직히",
          "original_form": "고등학교 땐 솔직히",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005095.1.19",
          "form": "차라리 공부하는 게 낫지",
          "original_form": "차라리 공부하는 게 낫지",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005095.1.20",
          "form": "시간 아까워서 싫어했었어요 전...",
          "original_form": "시간 아까워서 싫어했었어요 전...",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005095.1.21",
          "form": "지금 생각하면 그때 체력 길러둘걸... 싶긴 하지만",
          "original_form": "지금 생각하면 그때 체력 길러둘걸... 싶긴 하지만",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005095.1.22",
          "form": "앜ㅋㅋㅋㅋㅋ 저는 공부하길 너무 싫어했어서ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 저는 공부하길 너무 싫어했어서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:42"
        }
      ]
    }
