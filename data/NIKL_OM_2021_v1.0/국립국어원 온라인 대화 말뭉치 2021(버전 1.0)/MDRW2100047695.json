      "utterance": [
        {
          "id": "MDRW2100047695.1.1",
          "form": "그 외사 경찰?",
          "original_form": "그 외사 경찰?",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.2",
          "form": "외국어 잘하는.분들 특채로 뽑는거요",
          "original_form": "외국어 잘하는.분들 특채로 뽑는거요",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.3",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.4",
          "form": "오케이.",
          "original_form": "오케이.",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.5",
          "form": "그거하고싶었는데",
          "original_form": "그거하고싶었는데",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.6",
          "form": "엄... 하 나 불가능하네",
          "original_form": "엄... 하 나 불가능하네",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.7",
          "form": "외국어가 날 반대해서",
          "original_form": "외국어가 날 반대해서",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.8",
          "form": "영어토론실패자",
          "original_form": "영어토론실패자",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.9",
          "form": "못했어요",
          "original_form": "못했어요",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.11",
          "form": "베트남 음식만 맞는자.",
          "original_form": "베트남 음식만 맞는자.",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.12",
          "form": "아놔 진짜웃겨",
          "original_form": "아놔 진짜웃겨",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.13",
          "form": "오케이. 미투.",
          "original_form": "오케이. 미투.",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100047695.1.15",
          "form": "예아",
