      "utterance": [
        {
          "id": "MDRW2100033706.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100033706.1.2",
          "form": "안녕하세요~ 아침 챙겨 드셨나요?",
          "original_form": "안녕하세요~ 아침 챙겨 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 09:15"
        },
        {
          "id": "MDRW2100033706.1.3",
          "form": "아니요~",
          "original_form": "아니요~",
          "speaker_id": "1",
          "time": "20210825 11:31"
        },
        {
          "id": "MDRW2100033706.1.4",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "1",
          "time": "20210825 11:31"
        },
        {
          "id": "MDRW2100033706.1.5",
          "form": "저도 아직이요ㅎㅎ 좀 기다렸다가 점심먹으려고요",
          "original_form": "저도 아직이요ㅎㅎ 좀 기다렸다가 점심먹으려고요",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033706.1.6",
          "form": "식사 메뉴 정하셨나요?",
          "original_form": "식사 메뉴 정하셨나요?",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033706.1.7",
          "form": "전 치킨먹었어요ㅋㅋㅋ",
          "original_form": "전 치킨먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033706.1.8",
          "form": "점심뭐먹나요?",
          "original_form": "점심뭐먹나요?",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033706.1.9",
          "form": "친구들이랑 마라탕 먹으려고요!",
          "original_form": "친구들이랑 마라탕 먹으려고요!",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033706.1.10",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100033706.1.11",
          "form": "옥수수사리vs당면사리?",
          "original_form": "옥수수사리vs당면사리?",
          "speaker_id": "1",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100033706.1.12",
          "form": "당면이요! 옥수수면 불은 거 먹고 한 번 실망했더니 잘 안 먹게 되더라고요ㅋㅋㅋ",
          "original_form": "당면이요! 옥수수면 불은 거 먹고 한 번 실망했더니 잘 안 먹게 되더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100033706.1.13",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100033706.1.14",
          "form": "전 둘다좋아하는데",
          "original_form": "전 둘다좋아하는데",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100033706.1.15",
          "form": "옥수수면은",
          "original_form": "옥수수면은",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100033706.1.16",
          "form": "배달로는 절대안시켜요ㅋㅋㅋ",
          "original_form": "배달로는 절대안시켜요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100033706.1.17",
          "form": "고기는 양고기랑 소고기 중에 어떤 걸로 드시나요?",
          "original_form": "고기는 양고기랑 소고기 중에 어떤 걸로 드시나요?",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100033706.1.18",
          "form": "아묻따 소고기죠",
          "original_form": "아묻따 소고기죠",
          "speaker_id": "1",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100033706.1.19",
          "form": "전 양고기는 구운것만먹어요ㅋㅋㅋ",
          "original_form": "전 양고기는 구운것만먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100033706.1.20",
          "form": "양꼬치나 라무진이요",
          "original_form": "양꼬치나 라무진이요",
          "speaker_id": "1",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100033706.1.21",
          "form": "저도요! 꿔바로우는 좋아하시나요?",
          "original_form": "저도요! 꿔바로우는 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100033706.1.22",
          "form": "ㅋㅋㅋ좋아하죠",
          "original_form": "ㅋㅋㅋ좋아하죠",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100033706.1.23",
          "form": "찍먹vs부먹?",
          "original_form": "찍먹vs부먹?",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100033706.1.24",
          "form": "저는 찍먹이요!",
          "original_form": "저는 찍먹이요!",
          "speaker_id": "2",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100033706.1.25",
          "form": "어떻게 드세요?",
          "original_form": "어떻게 드세요?",
          "speaker_id": "2",
          "time": "20210825 11:59"
        },
