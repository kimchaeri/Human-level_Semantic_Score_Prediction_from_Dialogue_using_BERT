      "utterance": [
        {
          "id": "MDRW2100022223.1.1",
          "form": "그 다담에",
          "original_form": "그 다담에",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022223.1.2",
          "form": "반려동물 키워요?",
          "original_form": "반려동물 키워요?",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022223.1.3",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022223.1.4",
          "form": "부모님이 키우고 계세요",
          "original_form": "부모님이 키우고 계세요",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022223.1.5",
          "form": "어떤 종 키우시나요",
          "original_form": "어떤 종 키우시나요",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022223.1.6",
          "form": "강아지 키워요",
          "original_form": "강아지 키워요",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022223.1.7",
          "form": "잡종이에요",
          "original_form": "잡종이에요",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.8",
          "form": "귀엽겠다",
          "original_form": "귀엽겠다",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.9",
          "form": "무슨색이에요?",
          "original_form": "무슨색이에요?",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.10",
          "form": "ㅠㅠ 하나도 안귀여워요",
          "original_form": "ㅠㅠ 하나도 안귀여워요",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.11",
          "form": "안귀여워오ㅡ?",
          "original_form": "안귀여워오ㅡ?",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.12",
          "form": "하얀색인데 성격이 비글이에요",
          "original_form": "하얀색인데 성격이 비글이에요",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.13",
          "form": "왜요ㅋㅋㅋ",
          "original_form": "왜요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.14",
          "form": "사고 치고 다녀서요",
          "original_form": "사고 치고 다녀서요",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.15",
          "form": "아 활동적이구나",
          "original_form": "아 활동적이구나",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.16",
          "form": "그리고 엄청 막 무서워요",
          "original_form": "그리고 엄청 막 무서워요",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.17",
          "form": "왜요 물어요?",
          "original_form": "왜요 물어요?",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022223.1.18",
          "form": "자기 마음에 안들면 콱 물고 그래요 ..",
          "original_form": "자기 마음에 안들면 콱 물고 그래요 ..",
          "speaker_id": "1",
          "time": "20210829 14:20"
