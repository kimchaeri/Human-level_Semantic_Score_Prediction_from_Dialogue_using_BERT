      "utterance": [
        {
          "id": "MDRW2100023272.1.1",
          "form": "나는 비와서 가족끼리 전 부쳐먹는대",
          "original_form": "나는 비와서 가족끼리 전 부쳐먹는대",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100023272.1.2",
          "form": "전 부쳐먹다,, 부처먹다,,",
          "original_form": "전 부쳐먹다,, 부처먹다,,",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100023272.1.3",
          "form": "마라탕 먹으면 뭔가 꿔바로우도 먹고싶오",
          "original_form": "마라탕 먹으면 뭔가 꿔바로우도 먹고싶오",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100023272.1.4",
          "form": "난 있어도 그만 없어도 그만",
          "original_form": "난 있어도 그만 없어도 그만",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100023272.1.5",
          "form": "난 꿔바로우가 넘 좋아",
          "original_form": "난 꿔바로우가 넘 좋아",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100023272.1.6",
          "form": "찹쌀탕수육,, 꿔바로우,, 매콤한 국물,,",
          "original_form": "찹쌀탕수육,, 꿔바로우,, 매콤한 국물,,",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100023272.1.7",
          "form": "근데 저번에 꿔바로우랑 같이 먹었어",
          "original_form": "근데 저번에 꿔바로우랑 같이 먹었어",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100023272.1.8",
          "form": "오랜만에 먹으니까 맛있드라",
          "original_form": "오랜만에 먹으니까 맛있드라",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100023272.1.9",
          "form": "그치 난 꿔바로우가 더 좋아",
          "original_form": "그치 난 꿔바로우가 더 좋아",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100023272.1.10",
          "form": "오늘 저녁 머머금?",
          "original_form": "오늘 저녁 머머금?",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100023272.1.11",
          "form": "저녁은 아직..,,??",
          "original_form": "저녁은 아직..,,??",
          "speaker_id": "1",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100023272.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100023272.1.13",
          "form": "근데 오늘 허니콤보 먹었어",
          "original_form": "근데 오늘 허니콤보 먹었어",
          "speaker_id": "1",
          "time": "20210901 00:19"
        },
        {
          "id": "MDRW2100023272.1.14",
          "form": "지금이 오전 열두신데 저녁을 아직 안먹었다니,,",
          "original_form": "지금이 오전 열두신데 저녁을 아직 안먹었다니,,",
          "speaker_id": "2",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100023272.1.15",
          "form": "헐 허니콤보 나도 줘",
          "original_form": "헐 허니콤보 나도 줘",
          "speaker_id": "2",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100023272.1.16",
          "form": "어디서 났음?",
          "original_form": "어디서 났음?",
          "speaker_id": "2",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100023272.1.17",
          "form": "엄마가 시켜서 반절 먹음",
          "original_form": "엄마가 시켜서 반절 먹음",
          "speaker_id": "1",
          "time": "20210901 02:26"
        },
        {
          "id": "MDRW2100023272.1.18",
          "form": "개부럽다 나도 허콤 주라",
          "original_form": "개부럽다 나도 허콤 주라",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100023272.1.19",
          "form": "허콤 첨에 먹고 놀랐었는데 치킨 넘 달아서",
          "original_form": "허콤 첨에 먹고 놀랐었는데 치킨 넘 달아서",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100023272.1.20",
          "form": "근데 지금은 없어서 못먹음",
          "original_form": "근데 지금은 없어서 못먹음",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100023272.1.21",
          "form": "당에 익숙해지는 거 위험한디",
          "original_form": "당에 익숙해지는 거 위험한디",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100023272.1.22",
          "form": "ㅌㅋㅋㅋㅋ 나는 뭔가 교촌이 맛있는지 모르겠어서 허콤도 별로일줄알았는데 생각보다 괜참ㅎ더라",
          "original_form": "ㅌㅋㅋㅋㅋ 나는 뭔가 교촌이 맛있는지 모르겠어서 허콤도 별로일줄알았는데 생각보다 괜참ㅎ더라",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100023272.1.23",
          "form": "치킨 최애 머임",
          "original_form": "치킨 최애 머임",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100023272.1.24",
          "form": "난 요즘 허콤 골드킹 이런 게 좋더라",
          "original_form": "난 요즘 허콤 골드킹 이런 게 좋더라",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100023272.1.25",
          "form": "그치 허콤 존맛",
          "original_form": "그치 허콤 존맛",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100023272.1.26",
          "form": "골드킹은 뭐야",
          "original_form": "골드킹은 뭐야",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100023272.1.27",
          "form": "난 원앤온리 고추바사삭",
          "original_form": "난 원앤온리 고추바사삭",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100023272.1.28",
          "form": "골드킹 비에이치씨인가? 그런데 약간 간장맛인데 좀 더 달달한 그런 느낌",
          "original_form": "골드킹 비에이치씨인가? 그런데 약간 간장맛인데 좀 더 달달한 그런 느낌",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100023272.1.29",
          "form": "고바삭은 이제 질림",
          "original_form": "고바삭은 이제 질림",
          "speaker_id": "2",
