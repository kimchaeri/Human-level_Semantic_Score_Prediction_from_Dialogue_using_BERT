      "utterance": [
        {
          "id": "MDRW2100002808.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002808.1.2",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002808.1.3",
          "form": "저도 여행 좋아해요",
          "original_form": "저도 여행 좋아해요",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002808.1.4",
          "form": "최근에 가본 여행지 있으신가요",
          "original_form": "최근에 가본 여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002808.1.5",
          "form": "최근에는 코로나 때문에 여행을",
          "original_form": "최근에는 코로나 때문에 여행을",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002808.1.6",
          "form": "못갔네요",
          "original_form": "못갔네요",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002808.1.7",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002808.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002808.1.9",
          "form": "기억에 남는 곳은?",
          "original_form": "기억에 남는 곳은?",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002808.1.10",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002808.1.11",
          "form": "잘 생각이...",
          "original_form": "잘 생각이...",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002808.1.12",
          "form": "먼저 말씀해주시겠어요",
          "original_form": "먼저 말씀해주시겠어요",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002808.1.13",
          "form": "저는 강릉이요",
          "original_form": "저는 강릉이요",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002808.1.14",
          "form": "오 강릉 좋지요",
          "original_form": "오 강릉 좋지요",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002808.1.15",
          "form": "뭐가 기억에 남으셨나요",
          "original_form": "뭐가 기억에 남으셨나요",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002808.1.16",
          "form": "저는 안목해변이요",
          "original_form": "저는 안목해변이요",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002808.1.17",
          "form": "저는 강릉에 가본 적은 없어요",
          "original_form": "저는 강릉에 가본 적은 없어요",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002808.1.18",
          "form": "좋다고 들어서 ㅋㅋㅋ",
          "original_form": "좋다고 들어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002808.1.19",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002808.1.20",
          "form": "코로나때문에 안타깝네요",
          "original_form": "코로나때문에 안타깝네요",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002808.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002808.1.22",
          "form": "여행 가고싶네요 ㅠㅠ",
          "original_form": "여행 가고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:49"
        }
