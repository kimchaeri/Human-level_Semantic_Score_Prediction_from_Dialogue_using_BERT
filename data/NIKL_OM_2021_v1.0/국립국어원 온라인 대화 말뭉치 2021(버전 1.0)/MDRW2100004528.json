      "utterance": [
        {
          "id": "MDRW2100004528.1.1",
          "form": "이제 이시아에서 나와서 경산가는길ㅋ",
          "original_form": "이제 이시아에서 나와서 경산가는길ㅋ",
          "speaker_id": "2",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004528.1.2",
          "form": "설거지못하고왔네ㅠ",
          "original_form": "설거지못하고왔네ㅠ",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004528.1.3",
          "form": "집에가서 하연되징ㅎㄹ",
          "original_form": "집에가서 하연되징ㅎㄹ",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004528.1.4",
          "form": "언제하냐ㅠ설거지 ㅜㅠ",
          "original_form": "언제하냐ㅠ설거지 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004528.1.5",
          "form": "빨래도 ㅋㅋ 세탁기 가득이다",
          "original_form": "빨래도 ㅋㅋ 세탁기 가득이다",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004528.1.6",
          "form": "앗.\r\n나도 건조기 돌려야 해ㅠ",
          "original_form": "앗.\r\n나도 건조기 돌려야 해ㅠ",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004528.1.7",
          "form": "ㅋㅋㅋ 어서 돌리고온나",
          "original_form": "ㅋㅋㅋ 어서 돌리고온나",
          "speaker_id": "2",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004528.1.8",
          "form": "내일 일하고 와서 세탁기돌려야겠다",
          "original_form": "내일 일하고 와서 세탁기돌려야겠다",
          "speaker_id": "2",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004528.1.9",
          "form": "내일? 일욜아니가? 어디일가노",
          "original_form": "내일? 일욜아니가? 어디일가노",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004528.1.10",
          "form": "아ㅋㅋ월요일인줄",
          "original_form": "아ㅋㅋ월요일인줄",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004528.1.11",
          "form": "알았네ㅋㅋㅋ",
          "original_form": "알았네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004528.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ아 웃겨라ㅎ 그럴것 같더라 집에만.있는것도 아닌데 왜그랴",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 웃겨라ㅎ 그럴것 같더라 집에만.있는것도 아닌데 왜그랴",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004528.1.13",
          "form": "ㅋㅋ 그러니깐ㅋㅋ웃겨ㅋㅋ",
          "original_form": "ㅋㅋ 그러니깐ㅋㅋ웃겨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004528.1.14",
          "form": "저녁먹어?",
          "original_form": "저녁먹어?",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004528.1.15",
          "form": "아니 ㅋㅋ준비만 대충 해놓고 다시 앉음.ㅋㅋ짜장하고 오징어 부추전 먹으려고",
          "original_form": "아니 ㅋㅋ준비만 대충 해놓고 다시 앉음.ㅋㅋ짜장하고 오징어 부추전 먹으려고",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004528.1.16",
          "form": "오오 부추전좋다ㅋㅋ",
          "original_form": "오오 부추전좋다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004528.1.17",
          "form": "맛있겠다. 먹고싶네",
          "original_form": "맛있겠다. 먹고싶네",
          "speaker_id": "2",
          "time": "20210821 18:06"
        },
        {
          "id": "MDRW2100004528.1.18",
          "form": "name3이 좋아하겠다. 메뉴 좋다ㅋ",
          "original_form": "&name3&이 좋아하겠다. 메뉴 좋다ㅋ",
          "speaker_id": "2",
          "time": "20210821 18:07"
        },
        {
          "id": "MDRW2100004528.1.19",
          "form": "응 비도오고ㅋㅋ",
          "original_form": "응 비도오고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004528.1.20",
          "form": "비오는 날에는 전이 딱이지",
          "original_form": "비오는 날에는 전이 딱이지",
          "speaker_id": "2",
