      "utterance": [
        {
          "id": "MDRW2100044646.1.1",
          "form": "나두 안봤는데 여기서 나온 출연진이 주연으로 드라마 많이 찍어서",
          "original_form": "나두 안봤는데 여기서 나온 출연진이 주연으로 드라마 많이 찍어서",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044646.1.2",
          "form": "그렇더라구요. 조연이 대부분 주연으로",
          "original_form": "그렇더라구요. 조연이 대부분 주연으로",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044646.1.3",
          "form": "그러니까 신기하다",
          "original_form": "그러니까 신기하다",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044646.1.4",
          "form": "그러게요. 송중기랑도 찍고ㅋ",
          "original_form": "그러게요. 송중기랑도 찍고ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044646.1.5",
          "form": "조정석은 생각보다 출연작이 별로 없네",
          "original_form": "조정석은 생각보다 출연작이 별로 없네",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044646.1.6",
          "form": "광고만 많이 찍나봐요",
          "original_form": "광고만 많이 찍나봐요",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044646.1.7",
          "form": "80년생인데 뮤지컬로 04년에 데뷔",
          "original_form": "80년생인데 뮤지컬로 04년에 데뷔",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044646.1.8",
          "form": "뮤지컬 많이 했나봐요",
          "original_form": "뮤지컬 많이 했나봐요",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044646.1.9",
          "form": "그러게 드라마는 첨부터 주연으로 시작",
          "original_form": "그러게 드라마는 첨부터 주연으로 시작",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044646.1.10",
          "form": "뭐였어요?",
          "original_form": "뭐였어요?",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044646.1.11",
          "form": "211년도 왓츠업",
          "original_form": "211년도 왓츠업",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044646.1.12",
          "form": "영화에서 떴자나요",
          "original_form": "영화에서 떴자나요",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044646.1.13",
          "form": "왓츠업 재미없었나 기억 안나네요",
          "original_form": "왓츠업 재미없었나 기억 안나네요",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044646.1.14",
          "form": "영화를 많이 찍엇네",
          "original_form": "영화를 많이 찍엇네",
