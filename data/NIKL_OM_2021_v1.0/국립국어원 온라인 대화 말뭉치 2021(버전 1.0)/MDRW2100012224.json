      "utterance": [
        {
          "id": "MDRW2100012224.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012224.1.2",
          "form": "안녕하세요.ㅎㅎ \n집안일은 해도해도 끝이없네요.",
          "original_form": "안녕하세요.ㅎㅎ \n집안일은 해도해도 끝이없네요.",
          "speaker_id": "2",
          "time": "20210824 20:29"
        },
        {
          "id": "MDRW2100012224.1.3",
          "form": "그러게요 저도 청소해야 되는데 날이 습하니까 기운이 안 나네요 ㅎㅎ",
          "original_form": "그러게요 저도 청소해야 되는데 날이 습하니까 기운이 안 나네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012224.1.4",
          "form": "그쵸 창고에서",
          "original_form": "그쵸 창고에서",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012224.1.5",
          "form": "제습기도꺼내야하는데",
          "original_form": "제습기도꺼내야하는데",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012224.1.6",
          "form": "귀찮아서 그냥살고있네요",
          "original_form": "귀찮아서 그냥살고있네요",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012224.1.7",
          "form": "집안일 중에 제일 힘든 게 어떤 거예요?",
          "original_form": "집안일 중에 제일 힘든 게 어떤 거예요?",
          "speaker_id": "1",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012224.1.8",
          "form": "집안일이 힘들지는않은데",
          "original_form": "집안일이 힘들지는않은데",
          "speaker_id": "2",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012224.1.9",
          "form": "귀찮은게 젤 힘든거같아요",
          "original_form": "귀찮은게 젤 힘든거같아요",
          "speaker_id": "2",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012224.1.10",
          "form": "그건 그래요 ㅎㅎㅎ 시작하기가 힘든..",
          "original_form": "그건 그래요 ㅎㅎㅎ 시작하기가 힘든..",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012224.1.11",
          "form": "전 화장실청소가 제일 귀찮더라고요",
          "original_form": "전 화장실청소가 제일 귀찮더라고요",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012224.1.12",
          "form": "아예 시작을 안해요",
          "original_form": "아예 시작을 안해요",
          "speaker_id": "2",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012224.1.13",
          "form": "어젠 집이너무더러워서",
          "original_form": "어젠 집이너무더러워서",
          "speaker_id": "2",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012224.1.14",
          "form": "청소연구소를 이용해봤어요",
          "original_form": "청소연구소를 이용해봤어요",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012224.1.15",
          "form": "오 그거 이름만 들어봤어요 어때요",
          "original_form": "오 그거 이름만 들어봤어요 어때요",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012224.1.16",
          "form": "ㅋㅋㅋ 진짜 대만족이에요",
          "original_form": "ㅋㅋㅋ 진짜 대만족이에요",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012224.1.17",
          "form": "집 대청소 싹",
          "original_form": "집 대청소 싹",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100012224.1.18",
          "form": "오 정말요 너무 끌리네요 ㅋㅋㅋ",
          "original_form": "오 정말요 너무 끌리네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012224.1.19",
          "form": "광고하는건 많이 봤는데",
          "original_form": "광고하는건 많이 봤는데",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012224.1.20",
          "form": "5만2천원정도하면 4시간동안",
          "original_form": "5만2천원정도하면 4시간동안",
          "speaker_id": "2",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100012224.1.21",
          "form": "청소 싹해주시고 가셔서 좋아요",
          "original_form": "청소 싹해주시고 가셔서 좋아요",
          "speaker_id": "2",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100012224.1.22",
          "form": "오 가격 생각보다 괜찮네요!!",
          "original_form": "오 가격 생각보다 괜찮네요!!",
          "speaker_id": "1",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012224.1.23",
          "form": "필요한 날에 부르는 거니까 효율적이네요",
          "original_form": "필요한 날에 부르는 거니까 효율적이네요",
          "speaker_id": "1",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012224.1.24",
          "form": "그쵸? 가끔 부르면 좋은듯해요",
          "original_form": "그쵸? 가끔 부르면 좋은듯해요",
          "speaker_id": "2",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012224.1.25",
