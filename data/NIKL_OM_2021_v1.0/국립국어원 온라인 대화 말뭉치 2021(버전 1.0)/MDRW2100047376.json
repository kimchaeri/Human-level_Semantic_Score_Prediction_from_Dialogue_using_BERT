        },
        {
          "id": "MDRW2100047376.1.2",
          "form": "브리타 정수기요",
          "original_form": "브리타 정수기요",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100047376.1.3",
          "form": "앗 저도 정수기 사야하는데",
