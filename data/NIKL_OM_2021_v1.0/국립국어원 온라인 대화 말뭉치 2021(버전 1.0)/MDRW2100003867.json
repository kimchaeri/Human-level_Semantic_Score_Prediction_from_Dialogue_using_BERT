      "utterance": [
        {
          "id": "MDRW2100003867.1.1",
          "form": "오빠. 오늘은 광주에 있어서 못 온다는 거 같아요",
          "original_form": "오빠. 오늘은 광주에 있어서 못 온다는 거 같아요",
          "speaker_id": "1",
          "time": "20210821 13:06"
        },
        {
          "id": "MDRW2100003867.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210821 13:07"
        },
        {
          "id": "MDRW2100003867.1.3",
          "form": "오늘 저녁에라도 온다고 할줄 알았는디",
          "original_form": "오늘 저녁에라도 온다고 할줄 알았는디",
          "speaker_id": "2",
          "time": "20210821 13:07"
        },
        {
          "id": "MDRW2100003867.1.4",
          "form": "글믄 언제부터 온다는거임?",
          "original_form": "글믄 언제부터 온다는거임?",
          "speaker_id": "2",
          "time": "20210821 13:07"
        },
        {
          "id": "MDRW2100003867.1.5",
          "form": "잘 모르겠습니다 다음주나 다다음주에 올듯여",
          "original_form": "잘 모르겠습니다 다음주나 다다음주에 올듯여",
          "speaker_id": "1",
          "time": "20210821 13:11"
        },
        {
          "id": "MDRW2100003867.1.6",
          "form": "아니 언제부터 사는디",
          "original_form": "아니 언제부터 사는디",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003867.1.7",
          "form": "다음주에 온다는거임ㅡ.ㅡ",
          "original_form": "다음주에 온다는거임ㅡ.ㅡ",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003867.1.8",
          "form": "아마 8월말부터 오지 않을까요",
          "original_form": "아마 8월말부터 오지 않을까요",
          "speaker_id": "1",
          "time": "20210821 13:15"
        },
        {
          "id": "MDRW2100003867.1.9",
          "form": "엄청나군 선불인감",
          "original_form": "엄청나군 선불인감",
          "speaker_id": "2",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003867.1.10",
          "form": "몰라요",
          "original_form": "몰라요",
          "speaker_id": "1",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003867.1.11",
          "form": "배민 접속해줘봐",
          "original_form": "배민 접속해줘봐",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003867.1.12",
          "form": "알았어요 잠시만요",
          "original_form": "알았어요 잠시만요",
          "speaker_id": "1",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003867.1.13",
          "form": "접속했으면 말 좀",
          "original_form": "접속했으면 말 좀",
          "speaker_id": "2",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003867.1.14",
          "form": "네 잠시만요",
          "original_form": "네 잠시만요",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003867.1.15",
          "form": "접속해볼게요.",
          "original_form": "접속해볼게요.",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003867.1.16",
          "form": "접속했어요",
          "original_form": "접속했어요",
          "speaker_id": "1",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003867.1.17",
          "form": "근데 등록이 안 되네",
          "original_form": "근데 등록이 안 되네",
          "speaker_id": "2",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003867.1.18",
          "form": "잠시만요 시도 중이에요",
          "original_form": "잠시만요 시도 중이에요",
          "speaker_id": "1",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100003867.1.19",
          "form": "오늘 저녁 식사 메뉴는 뭘로 할까요?",
          "original_form": "오늘 저녁 식사 메뉴는 뭘로 할까요?",
          "speaker_id": "1",
          "time": "20210821 13:42"
        },
        {
          "id": "MDRW2100003867.1.20",
          "form": "음 오늘 점심 때 먹은 건",
          "original_form": "음 오늘 점심 때 먹은 건",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003867.1.21",
          "form": "나쁘지 않았는데",
          "original_form": "나쁘지 않았는데",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003867.1.22",
          "form": "치킨이 땡기네",
          "original_form": "치킨이 땡기네",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003867.1.23",
          "form": "치킨나라 피자공주",
          "original_form": "치킨나라 피자공주",
          "speaker_id": "2",
          "time": "20210821 13:57"
        }
