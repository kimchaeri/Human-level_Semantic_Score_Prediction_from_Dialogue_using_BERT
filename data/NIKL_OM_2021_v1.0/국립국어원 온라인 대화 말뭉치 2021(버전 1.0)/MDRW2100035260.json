      "utterance": [
        {
          "id": "MDRW2100035260.1.1",
          "form": "새벽부터 비내리더니 이제 매미가 우네요",
          "original_form": "새벽부터 비내리더니 이제 매미가 우네요",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100035260.1.2",
          "form": "거긴 어떤가요??",
          "original_form": "거긴 어떤가요??",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100035260.1.3",
          "form": "여기도 해가나요",
          "original_form": "여기도 해가나요",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100035260.1.4",
          "form": "좋아지니 다행이에요~",
          "original_form": "좋아지니 다행이에요~",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100035260.1.5",
          "form": "네 산책하기좋아요",
          "original_form": "네 산책하기좋아요",
          "speaker_id": "1",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035260.1.6",
          "form": "그러게요! 햇빛이 쨍쨍여서 저녁에 산책다녀올까봐요~",
          "original_form": "그러게요! 햇빛이 쨍쨍여서 저녁에 산책다녀올까봐요~",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100035260.1.7",
          "form": "미세먼지는 없어서 다행이에요",
          "original_form": "미세먼지는 없어서 다행이에요",
          "speaker_id": "1",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100035260.1.8",
          "form": "맞아요 확실히 비가와서 그런지 푸르긴 하네요",
          "original_form": "맞아요 확실히 비가와서 그런지 푸르긴 하네요",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100035260.1.9",
          "form": "가을옷도 준비해야하네요",
          "original_form": "가을옷도 준비해야하네요",
          "speaker_id": "1",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100035260.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100035260.1.11",
          "form": "아침저녁으로 기온차가 커지니 가디건을 꺼낼때네요",
          "original_form": "아침저녁으로 기온차가 커지니 가디건을 꺼낼때네요",
          "speaker_id": "2",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100035260.1.12",
          "form": "다음주 날씬 어떨지",
          "original_form": "다음주 날씬 어떨지",
          "speaker_id": "1",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100035260.1.13",
          "form": "적어도 비는 안왔음 좋겟어요!",
          "original_form": "적어도 비는 안왔음 좋겟어요!",
          "speaker_id": "2",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100035260.1.14",
          "form": "맞아요 지겹네요",
          "original_form": "맞아요 지겹네요",
          "speaker_id": "1",
          "time": "20210829 16:27"
        }
