      "utterance": [
        {
          "id": "MDRW2100027794.1.1",
          "form": "라오킹도 그렇고 제2의나라도 그렇고 폰 너무 뜨거워져요",
          "original_form": "라오킹도 그렇고 제2의나라도 그렇고 폰 너무 뜨거워져요",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027794.1.2",
          "form": "혹시 마썸카 아시나요",
          "original_form": "혹시 마썸카 아시나요",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027794.1.3",
          "form": "마썸카요? 아니요 게임인가요",
          "original_form": "마썸카요? 아니요 게임인가요",
          "speaker_id": "2",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027794.1.4",
          "form": "네 스팀게임인데 진짜 웃겨요 저도 유투브로 보기만 한...",
          "original_form": "네 스팀게임인데 진짜 웃겨요 저도 유투브로 보기만 한...",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027794.1.5",
          "form": "차조립게임인데 인생이 담겨있는 ㅋㅋ",
          "original_form": "차조립게임인데 인생이 담겨있는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027794.1.6",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027794.1.7",
          "form": "저는 트위치 게임 방송 많이 보네요",
          "original_form": "저는 트위치 게임 방송 많이 보네요",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027794.1.8",
          "form": "제 라이브러리로....",
          "original_form": "제 라이브러리로....",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027794.1.9",
          "form": "게임 방송 따로 본게 없는데 어떤걸 주로 보시나요?",
          "original_form": "게임 방송 따로 본게 없는데 어떤걸 주로 보시나요?",
          "speaker_id": "2",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027794.1.10",
          "form": "보통 스팀 스위치 플스게임 여러가지 하시더라구요",
          "original_form": "보통 스팀 스위치 플스게임 여러가지 하시더라구요",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027794.1.11",
          "form": "요즘 스트리머 사이에서 유행게임은",
          "original_form": "요즘 스트리머 사이에서 유행게임은",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027794.1.12",
          "form": "좀보이드? 좀비게임이에요 ㅎㅎ",
          "original_form": "좀보이드? 좀비게임이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027794.1.13",
          "form": "좀비게임이면 슈팅같은건가요 ㅎㅎ",
          "original_form": "좀비게임이면 슈팅같은건가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027794.1.14",
          "form": "좀비때문에 망한 세상에서 생존하는 생존 게임이에요",
          "original_form": "좀비때문에 망한 세상에서 생존하는 생존 게임이에요",
          "speaker_id": "1",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100027794.1.15",
          "form": "오호 생존게임류 좋아하는데",
          "original_form": "오호 생존게임류 좋아하는데",
          "speaker_id": "2",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027794.1.16",
          "form": "추천하시는 스트리머가 있나요?",
          "original_form": "추천하시는 스트리머가 있나요?",
          "speaker_id": "2",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027794.1.17",
          "form": "오오 저듀요",
          "original_form": "오오 저듀요",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027794.1.18",
          "form": "풍월량님 좋아해요",
          "original_form": "풍월량님 좋아해요",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027794.1.19",
          "form": "욕을 안하심",
          "original_form": "욕을 안하심",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027794.1.20",
          "form": "아 그 분 이름 많이 들어봤어요",
          "original_form": "아 그 분 이름 많이 들어봤어요",
          "speaker_id": "2",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027794.1.21",
          "form": "어렸을때 무인도 이야기 게임 했었는데",
          "original_form": "어렸을때 무인도 이야기 게임 했었는데",
          "speaker_id": "1",
          "time": "20210902 15:26"
        }
      ]
