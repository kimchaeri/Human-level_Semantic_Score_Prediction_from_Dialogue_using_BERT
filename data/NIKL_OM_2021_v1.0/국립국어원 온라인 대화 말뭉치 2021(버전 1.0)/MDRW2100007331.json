      "utterance": [
        {
          "id": "MDRW2100007331.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007331.1.2",
          "form": "혹시 당근마켓 쓰시나요?",
          "original_form": "혹시 당근마켓 쓰시나요?",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007331.1.3",
          "form": "안녕하세요 혹시 최근에 사신 물건 있으신가요?",
          "original_form": "안녕하세요 혹시 최근에 사신 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007331.1.4",
          "form": "엇 ㅎㅎ 저는 오늘 갤워치4 샀어요",
          "original_form": "엇 ㅎㅎ 저는 오늘 갤워치4 샀어요",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007331.1.5",
          "form": "네! 이젠 당근 없으면 못살죠",
          "original_form": "네! 이젠 당근 없으면 못살죠",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007331.1.6",
          "form": "와 대박 멋지다... 저도 사고 싶네요",
          "original_form": "와 대박 멋지다... 저도 사고 싶네요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007331.1.7",
          "form": "스마트워치는 처음 사봐서 설레요",
          "original_form": "스마트워치는 처음 사봐서 설레요",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007331.1.8",
          "form": "최근에 당근에서 뭐 거래하셨어요?",
          "original_form": "최근에 당근에서 뭐 거래하셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007331.1.9",
          "form": "저는 프로틴바 거래했어요!ㅋㅋㅋ",
          "original_form": "저는 프로틴바 거래했어요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007331.1.10",
          "form": "오히려 학생일 땐 중고나라에서 거래 많이 해봤는데",
          "original_form": "오히려 학생일 땐 중고나라에서 거래 많이 해봤는데",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007331.1.11",
          "form": "아 맞아요 학생땐 중고나라였죠ㅋㅋㅋ",
          "original_form": "아 맞아요 학생땐 중고나라였죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007331.1.12",
          "form": "지금이 더 중고거래에 용기(?)가 안 나는 것 같아요 저는 ㅋㅋ",
          "original_form": "지금이 더 중고거래에 용기(?)가 안 나는 것 같아요 저는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007331.1.13",
          "form": "당근은 직거래 동네 위주라 그런가",
          "original_form": "당근은 직거래 동네 위주라 그런가",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007331.1.14",
          "form": "괜히 더 떨리고",
          "original_form": "괜히 더 떨리고",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007331.1.15",
          "form": "맞아욬ㅋㅋㅋ 나올때 무슨 마피아 접선하는 것처럼",
          "original_form": "맞아욬ㅋㅋㅋ 나올때 무슨 마피아 접선하는 것처럼",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007331.1.16",
          "form": "당근으로 거래하면 보통 환불은 안 되는 걸로 보죠?",
          "original_form": "당근으로 거래하면 보통 환불은 안 되는 걸로 보죠?",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007331.1.17",
          "form": "그렇죠 아마도?",
          "original_form": "그렇죠 아마도?",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007331.1.18",
          "form": "혹시 구매자가 환불해달라고 할까봐 그것도 떨려요",
          "original_form": "혹시 구매자가 환불해달라고 할까봐 그것도 떨려요",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007331.1.19",
          "form": "옷 같은 거",
          "original_form": "옷 같은 거",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007331.1.20",
          "form": "안 맞는다고 ㅋㅋㅋ",
          "original_form": "안 맞는다고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007331.1.21",
          "form": "아ㅋㅋㅋㅋ 저도 그런 거 상상한 적 있어요",
          "original_form": "아ㅋㅋㅋㅋ 저도 그런 거 상상한 적 있어요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007331.1.22",
          "form": "그래서 빨리 거래 후기 써놓음",
          "original_form": "그래서 빨리 거래 후기 써놓음",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007331.1.23",
          "form": "아 그러면 파기가 안 되나요? 팁이네요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 그러면 파기가 안 되나요? 팁이네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007331.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ나중에 써먹어보세요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ나중에 써먹어보세요",
