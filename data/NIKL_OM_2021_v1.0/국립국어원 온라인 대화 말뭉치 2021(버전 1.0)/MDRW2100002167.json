      "utterance": [
        {
          "id": "MDRW2100002167.1.1",
          "form": "요새 배달음식 많이 시켜먹는다는데",
          "original_form": "요새 배달음식 많이 시켜먹는다는데",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002167.1.2",
          "form": "자주 시켜드세요?",
          "original_form": "자주 시켜드세요?",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002167.1.3",
          "form": "음 저는 자주시켜먹는 편은 아니에요, name2님은요?",
          "original_form": "음 저는 자주시켜먹는 편은 아니에요, &name2&님은요?",
          "speaker_id": "1",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002167.1.4",
          "form": "좋아하는 배달음식 있으신가요",
          "original_form": "좋아하는 배달음식 있으신가요",
          "speaker_id": "1",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002167.1.5",
          "form": "저는 줄일려고는 하는데 먹게 되네요",
          "original_form": "저는 줄일려고는 하는데 먹게 되네요",
          "speaker_id": "2",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002167.1.6",
          "form": "다양한데 마라탕을 좀 자주 시켜먹어요",
          "original_form": "다양한데 마라탕을 좀 자주 시켜먹어요",
          "speaker_id": "2",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002167.1.7",
          "form": "마라탕은 무슨맛인가요?",
          "original_form": "마라탕은 무슨맛인가요?",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002167.1.8",
          "form": "안먹어봤어요",
          "original_form": "안먹어봤어요",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002167.1.9",
          "form": "마라..라는 향신료가 뭔가 안맞을거같아서요",
          "original_form": "마라..라는 향신료가 뭔가 안맞을거같아서요",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002167.1.10",
          "form": "마라탕 되게 자극적인 맛이긴해요",
          "original_form": "마라탕 되게 자극적인 맛이긴해요",
          "speaker_id": "2",
          "time": "20210820 00:16"
        },
        {
          "id": "MDRW2100002167.1.11",
          "form": "근데 요즘 한국인한테 맞춰서도 나오니까 괜찮을거에요!",
          "original_form": "근데 요즘 한국인한테 맞춰서도 나오니까 괜찮을거에요!",
          "speaker_id": "2",
          "time": "20210820 00:16"
        },
        {
          "id": "MDRW2100002167.1.12",
          "form": "고수랑 비교하면 어떤가요",
          "original_form": "고수랑 비교하면 어떤가요",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002167.1.13",
          "form": "저 쌀국수 처음 먹었을 때 놀래서 뱉었거든요",
          "original_form": "저 쌀국수 처음 먹었을 때 놀래서 뱉었거든요",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002167.1.14",
          "form": "저도 고수는 못 먹는데 마라탕은 괜찮아요",
          "original_form": "저도 고수는 못 먹는데 마라탕은 괜찮아요",
          "speaker_id": "2",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002167.1.15",
          "form": "그대신 진짜 한국인 입맛에 맞춰진 곳으로 가야할 거 같아요!!!",
          "original_form": "그대신 진짜 한국인 입맛에 맞춰진 곳으로 가야할 거 같아요!!!",
          "speaker_id": "2",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002167.1.16",
          "form": "쌀국수도 향신료 때문에 놀라신 거에요??!",
          "original_form": "쌀국수도 향신료 때문에 놀라신 거에요??!",
          "speaker_id": "2",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002167.1.17",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002167.1.18",
          "form": "퐁퐁맛이..",
          "original_form": "퐁퐁맛이..",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002167.1.19",
          "form": "놀래서 뱉었어요",
          "original_form": "놀래서 뱉었어요",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002167.1.20",
          "form": "혹시 마라탕 프랜차이즈 중 추천해주실 곳 있나요",
          "original_form": "혹시 마라탕 프랜차이즈 중 추천해주실 곳 있나요",
          "speaker_id": "1",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002167.1.21",
          "form": "탕화쿵부 라화쿵부 뭐 이런곳이요 !!! 아마 대중적일 거 같아요",
          "original_form": "탕화쿵부 라화쿵부 뭐 이런곳이요 !!! 아마 대중적일 거 같아요",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002167.1.22",
          "form": "아니면 지역 적고 마라탕가게 쳐봐도 후기 많이 나올 거에요 ㅎ",
          "original_form": "아니면 지역 적고 마라탕가게 쳐봐도 후기 많이 나올 거에요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002167.1.23",
          "form": "탕화쿵부 집근처에 있는 곳이네요! 먹어볼게요! ㅎㅎ 뭐 넣어먹는게 가장 맛있어요?",
          "original_form": "탕화쿵부 집근처에 있는 곳이네요! 먹어볼게요! ㅎㅎ 뭐 넣어먹는게 가장 맛있어요?",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002167.1.24",
          "form": "야채 필수요 !!! 배추 청경채 이런 것도 맛있고",
          "original_form": "야채 필수요 !!! 배추 청경채 이런 것도 맛있고",
          "speaker_id": "2",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002167.1.25",
          "form": "목이버섯도 맛있어요 !!!!",
          "original_form": "목이버섯도 맛있어요 !!!!",
          "speaker_id": "2",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002167.1.26",
          "form": "분모자도... 푸주도... 건두부도.. 그냥 다 맛있네요 하하",
          "original_form": "분모자도... 푸주도... 건두부도.. 그냥 다 맛있네요 하하",
          "speaker_id": "2",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002167.1.27",
          "form": "추천하신거 꼭 넣어서 먹어볼게요!!!",
          "original_form": "추천하신거 꼭 넣어서 먹어볼게요!!!",
          "speaker_id": "1",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002167.1.28",
          "form": "푸주?",
          "original_form": "푸주?",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002167.1.29",
          "form": "아 건두부면이구나 꼭 넣어 먹을게요",
          "original_form": "아 건두부면이구나 꼭 넣어 먹을게요",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002167.1.30",
          "form": "고마워요",
          "original_form": "고마워요",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002167.1.31",
          "form": "입맛에 맞아야 할텐데 맞으시면 좋겠네요 !!!!",
          "original_form": "입맛에 맞아야 할텐데 맞으시면 좋겠네요 !!!!",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002167.1.32",
          "form": "후기 쳐보고 가보세요 !!!!",
          "original_form": "후기 쳐보고 가보세요 !!!!",
