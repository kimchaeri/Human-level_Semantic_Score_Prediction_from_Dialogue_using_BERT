      "utterance": [
        {
          "id": "MDRW2100044961.1.1",
          "form": "봉대박이런건 기억나는데",
          "original_form": "봉대박이런건 기억나는데",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044961.1.2",
          "form": "야 선영이네 봉대박 파스타다 이름이 ㅋㅋㅋ",
          "original_form": "야 선영이네 봉대박 파스타다 이름이 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044961.1.3",
          "form": "ㅇㅇㅇㅇㅇㅇ나도찾아봄ㅋㅋ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ나도찾아봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044961.1.4",
          "form": "ㅋㅋㅋㅋㅋ 같은곳이었어 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 같은곳이었어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044961.1.5",
          "form": "있어구나 이런게",
          "original_form": "있어구나 이런게",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044961.1.6",
          "form": "나는 그 좀 피자헛쪽으로 내려가다보면 있는 파스타집 생각함",
          "original_form": "나는 그 좀 피자헛쪽으로 내려가다보면 있는 파스타집 생각함",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044961.1.7",
          "form": "거기는 기억안남ㅜㅜ",
          "original_form": "거기는 기억안남ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044961.1.8",
          "form": "아니야기억날까야 우리여길 훨신자주갔어",
          "original_form": "아니야기억날까야 우리여길 훨신자주갔어",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044961.1.9",
          "form": "음....뭐먹었을까",
          "original_form": "음....뭐먹었을까",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044961.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044961.1.11",
          "form": "치킨크림스파게티 이런거",
          "original_form": "치킨크림스파게티 이런거",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044961.1.12",
          "form": "사진을 많이 찍었어야 했는데....",
          "original_form": "사진을 많이 찍었어야 했는데....",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044961.1.13",
          "form": "이렇게 기억이 안날수가ㅜㅜ",
          "original_form": "이렇게 기억이 안날수가ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044961.1.14",
          "form": "진짜 사진너무안찍었다 우리 ㅋㅋㅋㅋ",
          "original_form": "진짜 사진너무안찍었다 우리 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044961.1.15",
          "form": "아니 딱 학교나와서 길건너 그럼 우리 옛날에 막 정항우 케익있고그랬잖아",
          "original_form": "아니 딱 학교나와서 길건너 그럼 우리 옛날에 막 정항우 케익있고그랬잖아",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044961.1.16",
          "form": "그옆에 무슨 케이크가게 또있고 그옆에 스킨푸드 있고 정류장있잖아",
          "original_form": "그옆에 무슨 케이크가게 또있고 그옆에 스킨푸드 있고 정류장있잖아",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044961.1.17",
          "form": "ㅇㅇ그치그치",
          "original_form": "ㅇㅇ그치그치",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044961.1.18",
          "form": "그까지는 기억남",
          "original_form": "그까지는 기억남",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044961.1.19",
          "form": "거기서 더 내려가면 젤라또가게있잖아",
          "original_form": "거기서 더 내려가면 젤라또가게있잖아",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044961.1.20",
          "form": "거기서 더 가면 피자헛이고",
          "original_form": "거기서 더 가면 피자헛이고",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044961.1.21",
          "form": "헐 젤라또가게!",
          "original_form": "헐 젤라또가게!",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044961.1.22",
          "form": "피자헛가기전에있었음 ㅋㅋㅋ",
          "original_form": "피자헛가기전에있었음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044961.1.23",
          "form": "남는건 사진뿐인데 이렇게 기억이 안나다니",
          "original_form": "남는건 사진뿐인데 이렇게 기억이 안나다니",
          "speaker_id": "2",
          "time": "20210908 00:38"
        }
      ]
    }
  ]
