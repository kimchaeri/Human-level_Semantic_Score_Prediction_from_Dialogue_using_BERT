      "utterance": [
        {
          "id": "MDRW2100025046.1.1",
          "form": "동물키우세요?",
          "original_form": "동물키우세요?",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025046.1.2",
          "form": "아니요 반려동물 키우시나요?",
          "original_form": "아니요 반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025046.1.3",
          "form": "전 열대어요",
          "original_form": "전 열대어요",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025046.1.4",
          "form": "열대어는 키우는데 어떤장비들이 필요한가요?",
          "original_form": "열대어는 키우는데 어떤장비들이 필요한가요?",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025046.1.5",
          "form": "어항 기포기 여과기 끝",
          "original_form": "어항 기포기 여과기 끝",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025046.1.6",
          "form": "강아지도 키우고싶네요",
          "original_form": "강아지도 키우고싶네요",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025046.1.7",
          "form": "어떤 강아지 키우고 싶으세요?",
          "original_form": "어떤 강아지 키우고 싶으세요?",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025046.1.8",
          "form": "닥스훈트ㅋ",
          "original_form": "닥스훈트ㅋ",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025046.1.9",
          "form": "어떤점이 좋으신가요?",
          "original_form": "어떤점이 좋으신가요?",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100025046.1.10",
          "form": "다리가 짧아서요",
          "original_form": "다리가 짧아서요",
          "speaker_id": "1",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100025046.1.11",
          "form": "다리짧아서 귀엽죠",
          "original_form": "다리짧아서 귀엽죠",
          "speaker_id": "2",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100025046.1.12",
          "form": "웰시코기도 다리 짧아서 귀여워요",
          "original_form": "웰시코기도 다리 짧아서 귀여워요",
          "speaker_id": "2",
          "time": "20210901 14:31"
        },
        {
          "id": "MDRW2100025046.1.13",
          "form": "네 귀요미들이죠",
          "original_form": "네 귀요미들이죠",
          "speaker_id": "1",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100025046.1.14",
          "form": "열대어는 몇마리 키우시나요?",
          "original_form": "열대어는 몇마리 키우시나요?",
          "speaker_id": "2",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025046.1.15",
          "form": "출산해서 30마리",
          "original_form": "출산해서 30마리",
          "speaker_id": "1",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100025046.1.16",
          "form": "우와 스케일 크시네요",
          "original_form": "우와 스케일 크시네요",
          "speaker_id": "2",
          "time": "20210901 14:37"
        }
