      "utterance": [
        {
          "id": "MDRW2100045323.1.1",
          "form": "곤성은 잘 하고있나 모르겠네..",
          "original_form": "곤성은 잘 하고있나 모르겠네..",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045323.1.2",
          "form": "애드립이나",
          "original_form": "애드립이나",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045323.1.3",
          "form": "이런것들이 약간",
          "original_form": "이런것들이 약간",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045323.1.4",
          "form": "다섯이 서로돌아가면서 하기에는",
          "original_form": "다섯이 서로돌아가면서 하기에는",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045323.1.5",
          "form": "다른게너무 많아서 그런듯",
          "original_form": "다른게너무 많아서 그런듯",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045323.1.6",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.7",
          "form": "특히 조는 아예 극진행 방식자체가 달라서",
          "original_form": "특히 조는 아예 극진행 방식자체가 달라서",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.8",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.9",
          "form": "까딱하면",
          "original_form": "까딱하면",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.10",
          "form": "실수나겠어",
          "original_form": "실수나겠어",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제가 제이민은 아직 못보고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제가 제이민은 아직 못보고",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.12",
          "form": "스탭들도",
          "original_form": "스탭들도",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.13",
          "form": "김려원 유리아 이영미 이렇겍만 봤는데",
          "original_form": "김려원 유리아 이영미 이렇겍만 봤는데",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.14",
          "form": "긴장 많이 하고 들어갈듯",
          "original_form": "긴장 많이 하고 들어갈듯",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.15",
          "form": "영미언니 노래..",
          "original_form": "영미언니 노래..",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.16",
          "form": "도랐어...",
          "original_form": "도랐어...",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.18",
          "form": "우리 밴디트할때",
          "original_form": "우리 밴디트할때",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045323.1.19",
          "form": "그때보다도 훨씬더좋음",
          "original_form": "그때보다도 훨씬더좋음",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045323.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045323.1.21",
          "form": "목소리독보적...",
          "original_form": "목소리독보적...",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045323.1.22",
          "form": "그게 너무.. 옛날이긴 해",
          "original_form": "그게 너무.. 옛날이긴 해",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045323.1.23",
          "form": "우리 고딩때니꺼",
          "original_form": "우리 고딩때니꺼",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045323.1.24",
          "form": "마자..",
          "original_form": "마자..",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045323.1.25",
          "form": "와우..",
          "original_form": "와우..",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045323.1.26",
          "form": "몇년전이여...",
          "original_form": "몇년전이여...",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045323.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ약간 유리아는 찐사랑 느낌이고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ약간 유리아는 찐사랑 느낌이고",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045323.1.28",
          "form": "김려원은 애증 중에서도 증오 ㄴ낌",
          "original_form": "김려원은 애증 중에서도 증오 ㄴ낌",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045323.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 김려원제이민이랑 하는 헤드윅들이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 김려원제이민이랑 하는 헤드윅들이",
          "speaker_id": "1",
          "time": "20210908 01:54"
        }
      ]
    }
