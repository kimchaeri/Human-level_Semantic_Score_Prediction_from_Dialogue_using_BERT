      "utterance": [
        {
          "id": "MDRW2100044563.1.1",
          "form": "아 응응... 칠면조!! 걔 화보사진 찍는거잔아",
          "original_form": "아 응응... 칠면조!! 걔 화보사진 찍는거잔아",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044563.1.2",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044563.1.3",
          "form": "(ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아웃겨ㅠ",
          "original_form": "(ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아웃겨ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044563.1.4",
          "form": "근데 칠면조 였구나",
          "original_form": "근데 칠면조 였구나",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044563.1.5",
          "form": "비둘기두 있지 않았어???",
          "original_form": "비둘기두 있지 않았어???",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.6",
          "form": "응 비둘기는 다른 애였어",
          "original_form": "응 비둘기는 다른 애였어",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.7",
          "form": "머야 그래서 저 비둘기 친구는...",
          "original_form": "머야 그래서 저 비둘기 친구는...",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.8",
          "form": "걔는...전기구이되잔아(욜라",
          "original_form": "걔는...전기구이되잔아(욜라",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.9",
          "form": "기생충이 안사는 걸까...?",
          "original_form": "기생충이 안사는 걸까...?",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.10",
          "form": "아놔 지금 생각남",
          "original_form": "아놔 지금 생각남",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.11",
          "form": "그 메추리랑 같이 나오는 친구구나",
          "original_form": "그 메추리랑 같이 나오는 친구구나",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.12",
          "form": "진짜 웃기다",
          "original_form": "진짜 웃기다",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.13",
          "form": "문 열고 들어갔을 때 목욕하는거보면",
          "original_form": "문 열고 들어갔을 때 목욕하는거보면",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.14",
          "form": "아니 왜 욕조에서 뜬금없이",
          "original_form": "아니 왜 욕조에서 뜬금없이",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044563.1.15",
          "form": "위생적인친구인가보지",
          "original_form": "위생적인친구인가보지",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.16",
          "form": "응ㅇ으응",
          "original_form": "응ㅇ으응",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.17",
          "form": "전기 구이 통닭당하는 건지",
          "original_form": "전기 구이 통닭당하는 건지",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.18",
          "form": "아니..나는 더",
          "original_form": "아니..나는 더",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.19",
          "form": "뜬/금없었던게",
          "original_form": "뜬/금없었던게",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.20",
          "form": "걔가 제일 어이 없게 죽음",
          "original_form": "걔가 제일 어이 없게 죽음",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.21",
          "form": "웬 아기새가",
          "original_form": "웬 아기새가",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.22",
          "form": "감금당해..?있었잖ㄴ아",
          "original_form": "감금당해..?있었잖ㄴ아",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.23",
          "form": "다른 애들은",
          "original_form": "다른 애들은",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.24",
          "form": "머 해달라는거 해주면",
          "original_form": "머 해달라는거 해주면",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.25",
          "form": "그부분부터 엄청 웃겼음 어이가 없었음 메드사이언티스트 그런건가",
          "original_form": "그부분부터 엄청 웃겼음 어이가 없었음 메드사이언티스트 그런건가",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044563.1.26",
          "form": "사망하셨는데",
          "original_form": "사망하셨는데",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
