      "utterance": [
        {
          "id": "MDRW2100036200.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100036200.1.2",
          "form": "좋아해요 ㅋㅋ",
          "original_form": "좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036200.1.3",
          "form": "여유있게 여행만 다니면서 살고프네요 ㅋ",
          "original_form": "여유있게 여행만 다니면서 살고프네요 ㅋ",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036200.1.4",
          "form": "기억에 남는 여행지는요~?",
          "original_form": "기억에 남는 여행지는요~?",
          "speaker_id": "1",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100036200.1.5",
          "form": "너무 오래되서 다들 가물가물해요ㅜ",
          "original_form": "너무 오래되서 다들 가물가물해요ㅜ",
          "speaker_id": "2",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100036200.1.6",
          "form": "가고싶은곳은 많은데ㅜㅜ",
          "original_form": "가고싶은곳은 많은데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100036200.1.7",
          "form": "그러시군요 코로나 진정되면? 어디먼저 가고싶으신가요",
          "original_form": "그러시군요 코로나 진정되면? 어디먼저 가고싶으신가요",
          "speaker_id": "1",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100036200.1.8",
          "form": "저는 일본이요!",
          "original_form": "저는 일본이요!",
          "speaker_id": "2",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100036200.1.9",
          "form": "가본적 있으세요?",
          "original_form": "가본적 있으세요?",
          "speaker_id": "2",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100036200.1.10",
          "form": "네 10년도 더 된것 같아요",
          "original_form": "네 10년도 더 된것 같아요",
          "speaker_id": "1",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100036200.1.11",
          "form": "저는 한번도 안가봐서 환상? 로망이 좀 있어요 ㅋㅋ",
          "original_form": "저는 한번도 안가봐서 환상? 로망이 좀 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100036200.1.12",
          "form": "그럼 국내는 어디가 좋으셨나요",
          "original_form": "그럼 국내는 어디가 좋으셨나요",
          "speaker_id": "1",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100036200.1.13",
          "form": "저희동네 출렁다리 갔다왔어요 ㅋㅋ",
          "original_form": "저희동네 출렁다리 갔다왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100036200.1.14",
          "form": "출렁다리가 어디에 있나요? 산이겠죠?",
          "original_form": "출렁다리가 어디에 있나요? 산이겠죠?",
          "speaker_id": "1",
          "time": "20210830 19:27"
        },
        {
          "id": "MDRW2100036200.1.15",
          "form": "바다요 바로밑이 바다에요 나름 국내최장이라더라고요",
          "original_form": "바다요 바로밑이 바다에요 나름 국내최장이라더라고요",
          "speaker_id": "2",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100036200.1.16",
          "form": "어머 신기하네요. 관광지로 인기많겠어요",
          "original_form": "어머 신기하네요. 관광지로 인기많겠어요",
          "speaker_id": "1",
          "time": "20210830 19:34"
        }
      ]
