      "utterance": [
        {
          "id": "MDRW2100010265.1.1",
          "form": "그러게 필라테스 좋으니까",
          "original_form": "그러게 필라테스 좋으니까",
          "speaker_id": "2",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100010265.1.2",
          "form": "나도 자전거 센서 얼른 왔으면",
          "original_form": "나도 자전거 센서 얼른 왔으면",
          "speaker_id": "2",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100010265.1.3",
          "form": "그러게 센서 산 것 와서",
          "original_form": "그러게 센서 산 것 와서",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010265.1.4",
          "form": "집에서 사이클도 타고",
          "original_form": "집에서 사이클도 타고",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010265.1.5",
          "form": "어 그거 쯔위프트하면",
          "original_form": "어 그거 쯔위프트하면",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010265.1.6",
          "form": "운동하면 좋은데",
          "original_form": "운동하면 좋은데",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010265.1.7",
          "form": "다른사람들이랑 경주할수있대",
          "original_form": "다른사람들이랑 경주할수있대",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010265.1.8",
          "form": "오 건강도 챙기고",
          "original_form": "오 건강도 챙기고",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010265.1.9",
          "form": "재미도 있겠다",
          "original_form": "재미도 있겠다",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010265.1.10",
          "form": "그래서 다른나라 사람들 국기달고 앞으로 달려가면",
          "original_form": "그래서 다른나라 사람들 국기달고 앞으로 달려가면",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010265.1.11",
          "form": "앞지르려고 하다가 운동량 늘어난다는",
          "original_form": "앞지르려고 하다가 운동량 늘어난다는",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010265.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010265.1.13",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010265.1.14",
          "form": "운동하면서 국위선양",
          "original_form": "운동하면서 국위선양",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010265.1.15",
          "form": "어 등에 태극기 달아서",
          "original_form": "어 등에 태극기 달아서",
          "speaker_id": "2",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010265.1.16",
          "form": "다들 열심이라고 ㅋㅋㅋ",
          "original_form": "다들 열심이라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010265.1.17",
          "form": "국가대표 선수들이네",
          "original_form": "국가대표 선수들이네",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010265.1.18",
          "form": "그치 안방 국가대표들",
          "original_form": "그치 안방 국가대표들",
          "speaker_id": "2",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010265.1.19",
          "form": "재밌게 운동할 수 있어 좋겠다~",
          "original_form": "재밌게 운동할 수 있어 좋겠다~",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010265.1.20",
          "form": "응 너도 같이 하자 ㅋㅋ 첫 25키로 주행은 그냥 할 수 있으니까",
          "original_form": "응 너도 같이 하자 ㅋㅋ 첫 25키로 주행은 그냥 할 수 있으니까",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010265.1.21",
          "form": "싸이클링~",
          "original_form": "싸이클링~",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010265.1.22",
          "form": "여차하면 계정 하나로 같이해도 좋고",
          "original_form": "여차하면 계정 하나로 같이해도 좋고",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010265.1.23",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010265.1.24",
          "form": "같이 운동하고 건강 챙기고",
          "original_form": "같이 운동하고 건강 챙기고",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010265.1.25",
          "form": "이제 진짜 운동안하면 안되겄어",
          "original_form": "이제 진짜 운동안하면 안되겄어",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010265.1.26",
          "form": "후달려",
          "original_form": "후달려",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010265.1.27",
          "form": "아까 그 자전거 프로그램 재미있더라",
          "original_form": "아까 그 자전거 프로그램 재미있더라",
          "speaker_id": "2",
          "time": "20210824 23:41"
        }
      ]
    }
