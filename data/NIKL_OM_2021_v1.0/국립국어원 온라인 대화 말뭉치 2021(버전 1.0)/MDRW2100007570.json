      "utterance": [
        {
          "id": "MDRW2100007570.1.1",
          "form": "웹툰님은 맘에 들었던 색이 무엇이었나요?",
          "original_form": "웹툰님은 맘에 들었던 색이 무엇이었나요?",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007570.1.2",
          "form": "저는 애쉬브라운이 제일 좋았어요!!!",
          "original_form": "저는 애쉬브라운이 제일 좋았어요!!!",
          "speaker_id": "2",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007570.1.3",
          "form": "너무 이뻤겠다",
          "original_form": "너무 이뻤겠다",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007570.1.4",
          "form": "애쉬색이 은은하니 이쁘더라구요",
          "original_form": "애쉬색이 은은하니 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007570.1.5",
          "form": "그럼 가장 파격적인 색은?",
          "original_form": "그럼 가장 파격적인 색은?",
          "speaker_id": "1",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007570.1.6",
          "form": "마저요마저요",
          "original_form": "마저요마저요",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007570.1.7",
          "form": "헉 저는 노랑이 그나마 좀",
          "original_form": "헉 저는 노랑이 그나마 좀",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007570.1.8",
          "form": "파격적이엌ㅅ던것같아요",
          "original_form": "파격적이엌ㅅ던것같아요",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007570.1.9",
          "form": "머리는 혹시 긴편인가요?",
          "original_form": "머리는 혹시 긴편인가요?",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007570.1.10",
          "form": "노랑은 파격적이죠",
          "original_form": "노랑은 파격적이죠",
          "speaker_id": "1",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007570.1.11",
          "form": "중단발정도요",
          "original_form": "중단발정도요",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007570.1.12",
          "form": "그 염색을 끝부분에만 하시는 분들이 많더라고요",
          "original_form": "그 염색을 끝부분에만 하시는 분들이 많더라고요",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007570.1.13",
          "form": "머리 상하는 것 때뭉에",
          "original_form": "머리 상하는 것 때뭉에",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007570.1.14",
          "form": "그 옴브레처럼요?",
          "original_form": "그 옴브레처럼요?",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007570.1.15",
          "form": "그것도 매력있더라구요",
          "original_form": "그것도 매력있더라구요",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007570.1.16",
          "form": "아아 옴브레",
          "original_form": "아아 옴브레",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007570.1.17",
          "form": "맞아욬ㅋㅋㅋㅋ옴브레였어요 이름이",
          "original_form": "맞아욬ㅋㅋㅋㅋ옴브레였어요 이름이",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007570.1.18",
          "form": "시부레인가 이랬는대",
          "original_form": "시부레인가 이랬는대",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007570.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007570.1.20",
          "form": "끝에만 하는게 관리하기는 편해요",
          "original_form": "끝에만 하는게 관리하기는 편해요",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007570.1.21",
          "form": "근데 염색 자주하시면 진짜 관리 어케하시는 편이세요?!",
          "original_form": "근데 염색 자주하시면 진짜 관리 어케하시는 편이세요?!",
          "speaker_id": "2",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007570.1.22",
          "form": "전 트리트먼트 자주해줘요",
          "original_form": "전 트리트먼트 자주해줘요",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007570.1.23",
          "form": "아무레도 샵 관리는 비싸서",
          "original_form": "아무레도 샵 관리는 비싸서",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007570.1.24",
          "form": "트리트먼트만 해줘도 많이 좋아지더라구요",
          "original_form": "트리트먼트만 해줘도 많이 좋아지더라구요",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007570.1.25",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007570.1.26",
          "form": "트리트먼트 어떤거 쓰세요?",
          "original_form": "트리트먼트 어떤거 쓰세요?",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007570.1.27",
          "form": "미쟝센 살롱 10이요",
          "original_form": "미쟝센 살롱 10이요",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007570.1.28",
          "form": "이거 괜찮더라구요",
          "original_form": "이거 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007570.1.29",
          "form": "아이가릿 메모메모",
          "original_form": "아이가릿 메모메모",
          "speaker_id": "2",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007570.1.30",
          "form": "다이어트 안해요 웹툰님은요?",
          "original_form": "다이어트 안해요 웹툰님은요?",
          "speaker_id": "1",
          "time": "20210822 03:29"
        },
        {
          "id": "MDRW2100007570.1.31",
          "form": "한 번 해봤는데 못하겠더라구요",
          "original_form": "한 번 해봤는데 못하겠더라구요",
          "speaker_id": "1",
