      "utterance": [
        {
          "id": "MDRW2100003948.1.1",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003948.1.2",
          "form": "군대....",
          "original_form": "군대....",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003948.1.3",
          "form": "예엣날에 한참 남자동기들이나 선배들한테 들어본적은 있는것같은데",
          "original_form": "예엣날에 한참 남자동기들이나 선배들한테 들어본적은 있는것같은데",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003948.1.4",
          "form": "한귀로 듣고 한귀로 흘려서",
          "original_form": "한귀로 듣고 한귀로 흘려서",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003948.1.5",
          "form": "기억나는 에피소드가 하나도 없네요",
          "original_form": "기억나는 에피소드가 하나도 없네요",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003948.1.6",
          "form": "흠 전 남편한테 군대 물어봐볼게요",
          "original_form": "흠 전 남편한테 군대 물어봐볼게요",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003948.1.7",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003948.1.8",
          "form": "남자들에게 군대 물어보는건",
          "original_form": "남자들에게 군대 물어보는건",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003948.1.9",
          "form": "금기어 아닌가요",
          "original_form": "금기어 아닌가요",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003948.1.10",
          "form": "예전에 한번 말해줬는데",
          "original_form": "예전에 한번 말해줬는데",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003948.1.11",
          "form": "기억이 안나네요ㅋㅋ",
          "original_form": "기억이 안나네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003948.1.12",
          "form": "그쵸ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003948.1.13",
          "form": "아 저는 동생이",
          "original_form": "아 저는 동생이",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003948.1.14",
          "form": "다음주에 제대하네요",
          "original_form": "다음주에 제대하네요",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003948.1.15",
          "form": "좀 더 있다 오면 좋겠는데",
          "original_form": "좀 더 있다 오면 좋겠는데",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003948.1.16",
          "form": "왜이리 빨리오는지...",
          "original_form": "왜이리 빨리오는지...",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003948.1.17",
          "form": "요즘 군대가 예전보다 짧아졌다더라구요",
          "original_form": "요즘 군대가 예전보다 짧아졌다더라구요",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003948.1.18",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003948.1.19",
          "form": "남의군대는 빨리 끝나는듯여",
          "original_form": "남의군대는 빨리 끝나는듯여",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003948.1.20",
          "form": "집안 군대도 빨레 끝나네요...",
          "original_form": "집안 군대도 빨레 끝나네요...",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003948.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003948.1.22",
          "form": "빨리..",
          "original_form": "빨리..",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003948.1.23",
          "form": "즐겨보는 유튜버가 얼마전에 군대가서 볼거리가 하나 줄었어요ㅋ",
          "original_form": "즐겨보는 유튜버가 얼마전에 군대가서 볼거리가 하나 줄었어요ㅋ",
          "speaker_id": "2",
