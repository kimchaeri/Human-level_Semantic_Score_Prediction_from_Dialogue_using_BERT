      "utterance": [
        {
          "id": "MDRW2100002857.1.1",
          "form": "name2님은 동남아 어디요?",
          "original_form": "&name2&님은 동남아 어디요?",
          "speaker_id": "1",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002857.1.2",
          "form": "저는 태국이요",
          "original_form": "저는 태국이요",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002857.1.3",
          "form": "약간 느리고 여유로운 분위기가 좋더라구요",
          "original_form": "약간 느리고 여유로운 분위기가 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002857.1.4",
          "form": "아~~가보신적 있으세요?",
          "original_form": "아~~가보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002857.1.5",
          "form": "네 한 번 가봤어요ㅎㅎ",
          "original_form": "네 한 번 가봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002857.1.6",
          "form": "name1님은 해외 가보신 적 있으신가요?",
          "original_form": "&name1&님은 해외 가보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002857.1.7",
          "form": "저는 한번이요ㅠ",
          "original_form": "저는 한번이요ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002857.1.8",
          "form": "어디다녀오셨나요?",
          "original_form": "어디다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002857.1.9",
          "form": "저는 발리 다녀왔어요 ㅎ",
          "original_form": "저는 발리 다녀왔어요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002857.1.10",
          "form": "너무 좋았겠다 대표적인 휴양지잖아요",
          "original_form": "너무 좋았겠다 대표적인 휴양지잖아요",
          "speaker_id": "2",
          "time": "20210820 16:06"
        },
        {
          "id": "MDRW2100002857.1.11",
          "form": "가서 뭐 하셨나요?",
          "original_form": "가서 뭐 하셨나요?",
          "speaker_id": "2",
          "time": "20210820 16:06"
        },
        {
          "id": "MDRW2100002857.1.12",
          "form": "네ㅎㅎ 주로 먹고 마사지 받고 쉬고 했어요 ㅎㅎ 1일1마사지 ㅎㅎ",
          "original_form": "네ㅎㅎ 주로 먹고 마사지 받고 쉬고 했어요 ㅎㅎ 1일1마사지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:07"
        },
        {
          "id": "MDRW2100002857.1.13",
          "form": "완전 제대로 휴양하다 오셨네요~\r\n발리음식은 어때요?",
          "original_form": "완전 제대로 휴양하다 오셨네요~\r\n발리음식은 어때요?",
          "speaker_id": "2",
          "time": "20210820 16:08"
        },
        {
          "id": "MDRW2100002857.1.14",
          "form": "음식점마다 다른데 아무래도 좀 비싼곳이 입맛에 맛더라고요",
          "original_form": "음식점마다 다른데 아무래도 좀 비싼곳이 입맛에 맛더라고요",
          "speaker_id": "1",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002857.1.15",
          "form": "어떤음식을 드셨나요? 음 발리는 가본적이없어서 어떤음식이 있는지 생소하네요",
          "original_form": "어떤음식을 드셨나요? 음 발리는 가본적이없어서 어떤음식이 있는지 생소하네요",
          "speaker_id": "2",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002857.1.16",
          "form": "대표음식이 나시고랭 이런거였어요",
          "original_form": "대표음식이 나시고랭 이런거였어요",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002857.1.17",
          "form": "아 그게 발리음식이구나",
          "original_form": "아 그게 발리음식이구나",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002857.1.18",
          "form": "몰랐어요 그거 맛있잖아요ㅎㅎ",
          "original_form": "몰랐어요 그거 맛있잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:11"
        }
      ]
