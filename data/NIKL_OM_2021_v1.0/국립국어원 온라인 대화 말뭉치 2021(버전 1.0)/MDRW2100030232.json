      "utterance": [
        {
          "id": "MDRW2100030232.1.1",
          "form": "근데 저 엄청 내성적이에요ㅎㅎㅎ",
          "original_form": "근데 저 엄청 내성적이에요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030232.1.2",
          "form": "음.. 슬의생에 곰과 같애요ㅎㅎ",
          "original_form": "음.. 슬의생에 곰과 같애요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030232.1.3",
          "form": "아들도 그래요?ㅎ",
          "original_form": "아들도 그래요?ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030232.1.4",
          "form": "내성적이진 않는데",
          "original_form": "내성적이진 않는데",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030232.1.5",
          "form": "부끄러움을 잘타요 ㅋ",
          "original_form": "부끄러움을 잘타요 ㅋ",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030232.1.6",
          "form": "ㅎㅎ그게비슷한거아니에요?ㅎㅎ",
          "original_form": "ㅎㅎ그게비슷한거아니에요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030232.1.7",
          "form": "그럼 질문하면 알겠네요ㅎ 엘레베이터 탈때 어디쪽에 타요? 아들은?",
          "original_form": "그럼 질문하면 알겠네요ㅎ 엘레베이터 탈때 어디쪽에 타요? 아들은?",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030232.1.8",
          "form": "음 그냥 제옆에요  ㅎ",
          "original_form": "음 그냥 제옆에요  ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030232.1.9",
          "form": "중간 ? 사이드?",
          "original_form": "중간 ? 사이드?",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030232.1.10",
          "form": "아~ 아직은 옆에 있겠구나ㅎㅎ",
          "original_form": "아~ 아직은 옆에 있겠구나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030232.1.11",
          "form": "사이드부분에 있으면 내성적인거고",
          "original_form": "사이드부분에 있으면 내성적인거고",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030232.1.12",
          "form": "엘베 눌르고 저한테 와요 ㅎㅎ",
          "original_form": "엘베 눌르고 저한테 와요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030232.1.13",
          "form": "중간에 있으면 외향적인거래요ㅎ",
          "original_form": "중간에 있으면 외향적인거래요ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030232.1.14",
          "form": "모든 애들이 그러니ㅎ 그건패쓰~ㅎ",
          "original_form": "모든 애들이 그러니ㅎ 그건패쓰~ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030232.1.15",
          "form": "근데 보통 다 사이드 있지 않나요 ㅎㅎ",
          "original_form": "근데 보통 다 사이드 있지 않나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030232.1.16",
          "form": "어린이집에서 애들하고 잘지내요?ㅎ",
          "original_form": "어린이집에서 애들하고 잘지내요?ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030232.1.17",
          "form": "중앙에 있는분도 있죠ㅎㅎ",
          "original_form": "중앙에 있는분도 있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030232.1.18",
          "form": "아 ㅋㅋ",
