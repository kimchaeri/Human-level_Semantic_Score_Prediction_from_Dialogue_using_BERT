      "utterance": [
        {
          "id": "MDRW2100025455.1.1",
          "form": "연애하시나요??",
          "original_form": "연애하시나요??",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025455.1.2",
          "form": "아니요! 지금은 안하고 있어요",
          "original_form": "아니요! 지금은 안하고 있어요",
          "speaker_id": "2",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025455.1.3",
          "form": "연애 하시나요??",
          "original_form": "연애 하시나요??",
          "speaker_id": "2",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025455.1.4",
          "form": "네네!저는 남자친구있습니다",
          "original_form": "네네!저는 남자친구있습니다",
          "speaker_id": "1",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025455.1.5",
          "form": "사귄지 얼마안됬어요!",
          "original_form": "사귄지 얼마안됬어요!",
          "speaker_id": "1",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025455.1.6",
          "form": "오 너무 좋으실때네요ㅎㅎㅎ",
          "original_form": "오 너무 좋으실때네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025455.1.7",
          "form": "데이트는 어디로 많이 가세요??",
          "original_form": "데이트는 어디로 많이 가세요??",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025455.1.8",
          "form": "요즘은 주로 실내에서 마니 놀아요",
          "original_form": "요즘은 주로 실내에서 마니 놀아요",
          "speaker_id": "1",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025455.1.9",
          "form": "둘다 직장인이라서 잘 못만나요 ㅠㅠ",
          "original_form": "둘다 직장인이라서 잘 못만나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025455.1.10",
          "form": "둘다 바쁘시면 그렇겠네요ㅜㅜㅜ",
          "original_form": "둘다 바쁘시면 그렇겠네요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025455.1.11",
          "form": "요즘 코로나라 밖에서 놀기 힘든거같아요",
          "original_form": "요즘 코로나라 밖에서 놀기 힘든거같아요",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025455.1.12",
          "form": "맞아요 어디 가기가 무섭더라구요",
          "original_form": "맞아요 어디 가기가 무섭더라구요",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025455.1.13",
          "form": "그래도 둘다 1차 접종해서 다행이예오!",
          "original_form": "그래도 둘다 1차 접종해서 다행이예오!",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025455.1.14",
          "form": "오 벌써 접종 하셨나요???",
          "original_form": "오 벌써 접종 하셨나요???",
          "speaker_id": "2",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100025455.1.15",
          "form": "아프진 않으셨어요???",
          "original_form": "아프진 않으셨어요???",
          "speaker_id": "2",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100025455.1.16",
          "form": "네네다음날이 아프더라구요 ㅠㅠ",
          "original_form": "네네다음날이 아프더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025455.1.17",
          "form": "근육통따뭄에",
          "original_form": "근육통따뭄에",
          "speaker_id": "1",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025455.1.18",
          "form": "저도 곧 맞는데 걱정이네요ㅜㅜㅜ",
          "original_form": "저도 곧 맞는데 걱정이네요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 13:32"
        },
        {
          "id": "MDRW2100025455.1.19",
          "form": "저는곧 2차맞아요 저도 무서워요 ㅠㅠ",
          "original_form": "저는곧 2차맞아요 저도 무서워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025455.1.20",
          "form": "2차가 더 아프다는데",
          "original_form": "2차가 더 아프다는데",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025455.1.21",
          "form": "진짜 걱정되세겠어요",
          "original_form": "진짜 걱정되세겠어요",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025455.1.22",
          "form": "연애는 해보셨나요?",
          "original_form": "연애는 해보셨나요?",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025455.1.23",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025455.1.24",
          "form": "코로나 이후엔 새로운 사람을 못만나고 있어서 그런가 연애를 못하고 있지만요ㅠ",
          "original_form": "코로나 이후엔 새로운 사람을 못만나고 있어서 그런가 연애를 못하고 있지만요ㅠ",
          "speaker_id": "2",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025455.1.25",
          "form": "맞아요 나가기가 두렵죠 연애하시면 주로 어떤 데이트하시나요",
          "original_form": "맞아요 나가기가 두렵죠 연애하시면 주로 어떤 데이트하시나요",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100025455.1.26",
          "form": "저는 카페가는거 좋아해서 카페 자주 찾아다닌거같아요!",
          "original_form": "저는 카페가는거 좋아해서 카페 자주 찾아다닌거같아요!",
          "speaker_id": "2",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100025455.1.27",
          "form": "저는 카페 좋긴한데 사람많은곳은 안가게 되더라구여 ㅠ",
          "original_form": "저는 카페 좋긴한데 사람많은곳은 안가게 되더라구여 ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:16"
        }
      ]
    }
  ]
