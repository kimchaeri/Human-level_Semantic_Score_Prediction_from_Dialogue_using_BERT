      "utterance": [
        {
          "id": "MDRW2100034290.1.1",
          "form": "좀 눈치주는 사람이 있어야거 잘 빠질거같아요 ㅋㅋㅋㅋ",
          "original_form": "좀 눈치주는 사람이 있어야거 잘 빠질거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100034290.1.2",
          "form": "그렇긴해요 조절도 좀 하게되고ㅋㅋ",
          "original_form": "그렇긴해요 조절도 좀 하게되고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100034290.1.3",
          "form": "근데 그러니까 오히려 피티 아예 끝냈을때",
          "original_form": "근데 그러니까 오히려 피티 아예 끝냈을때",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100034290.1.4",
          "form": "안그럼 저는 정말",
          "original_form": "안그럼 저는 정말",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100034290.1.5",
          "form": "이제 먹고싶은거 다 먹을수있다",
          "original_form": "이제 먹고싶은거 다 먹을수있다",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100034290.1.6",
          "form": "끝없이 먹게되더라고요ㅠㅋㅋㅋ",
          "original_form": "끝없이 먹게되더라고요ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100034290.1.7",
          "form": "이런 마음 되가지구",
          "original_form": "이런 마음 되가지구",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100034290.1.8",
          "form": "하 ..그맘알죠",
          "original_form": "하 ..그맘알죠",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100034290.1.9",
          "form": "전보다 더먹게 되더라구요ㅋㅋ",
          "original_form": "전보다 더먹게 되더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034290.1.10",
          "form": "뭔가 고삐풀린느낌?",
          "original_form": "뭔가 고삐풀린느낌?",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034290.1.11",
          "form": "마자요ㅋㅋ",
          "original_form": "마자요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034290.1.12",
          "form": "다이어트 한번 손대고 나면",
          "original_form": "다이어트 한번 손대고 나면",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034290.1.13",
          "form": "이제 자유라는 느낌ㅋㅋ",
          "original_form": "이제 자유라는 느낌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034290.1.14",
          "form": "안좋아하던 음식도 좋아하게 되고...",
          "original_form": "안좋아하던 음식도 좋아하게 되고...",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034290.1.15",
          "form": "식탐이 더생겨요",
          "original_form": "식탐이 더생겨요",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034290.1.16",
          "form": "그니까요 다맛있어요ㅋㅋ",
          "original_form": "그니까요 다맛있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034290.1.17",
          "form": "그래서 식단 평생할거 아니면",
          "original_form": "그래서 식단 평생할거 아니면",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034290.1.18",
          "form": "안하는게 낫다그러더라구요\r\n요요 심하게  온다구",
          "original_form": "안하는게 낫다그러더라구요\r\n요요 심하게  온다구",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100034290.1.19",
          "form": "맞아요...ㅠㅠ",
          "original_form": "맞아요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100034290.1.20",
          "form": "근데 이미 시작해버린... 말만 다이어터지만요 저는..ㅋㅋㅋ",
          "original_form": "근데 이미 시작해버린... 말만 다이어터지만요 저는..ㅋㅋㅋ",
          "speaker_id": "1",
