      "utterance": [
        {
          "id": "MDRW2100030359.1.1",
          "form": "냉채족발 도어 정말 맛있죠",
          "original_form": "냉채족발 도어 정말 맛있죠",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.2",
          "form": "해파리도 맛있죠",
          "original_form": "해파리도 맛있죠",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.3",
          "form": "그래도 전 불족발이 제일 좋아요",
          "original_form": "그래도 전 불족발이 제일 좋아요",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.4",
          "form": "톡 쏘는 맛이지요",
          "original_form": "톡 쏘는 맛이지요",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.5",
          "form": "역시 매운게 최고죠",
          "original_form": "역시 매운게 최고죠",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.6",
          "form": "전화 사비를 잘 못 먹어서요",
          "original_form": "전화 사비를 잘 못 먹어서요",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.7",
          "form": "매운 거 먹으면 스트레스 풀려요",
          "original_form": "매운 거 먹으면 스트레스 풀려요",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.8",
          "form": "와사비 하기 싫어요",
          "original_form": "와사비 하기 싫어요",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.9",
          "form": "향이",
          "original_form": "향이",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.10",
          "form": "엽기 떡볶이에서 닭발 시켜 먹어 보셨나요",
          "original_form": "엽기 떡볶이에서 닭발 시켜 먹어 보셨나요",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030359.1.11",
          "form": "국물 닭발 시켜 먹어보시면 진짜 반 할 거예요",
          "original_form": "국물 닭발 시켜 먹어보시면 진짜 반 할 거예요",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.12",
          "form": "어디 있는지도 몰라요",
          "original_form": "어디 있는지도 몰라요",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.13",
          "form": "많이 익혀서 뼈와살이 사르르 분리 되거든요",
          "original_form": "많이 익혀서 뼈와살이 사르르 분리 되거든요",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.14",
          "form": "주문해 볼게요",
          "original_form": "주문해 볼게요",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.15",
          "form": "제주도에는 엽기떡볶이 없나요",
          "original_form": "제주도에는 엽기떡볶이 없나요",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.16",
          "form": "기승전 닭발",
          "original_form": "기승전 닭발",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.17",
          "form": "신전떡볶이 있는 있나요",
          "original_form": "신전떡볶이 있는 있나요",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.18",
          "form": "이것도 인식하네요",
          "original_form": "이것도 인식하네요",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.19",
          "form": "닭발은 콜라겐이 많아서 피부에도 좋아요",
          "original_form": "닭발은 콜라겐이 많아서 피부에도 좋아요",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.20",
          "form": "신전떡볶이는 본 거 같아요",
          "original_form": "신전떡볶이는 본 거 같아요",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030359.1.21",
          "form": "매일 먹고 싶네요",
          "original_form": "매일 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030359.1.22",
          "form": "신전떡볶이 치즈떡볶이 맛있어요",
          "original_form": "신전떡볶이 치즈떡볶이 맛있어요",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030359.1.23",
          "form": "매운 김밥도 판답니다",
          "original_form": "매운 김밥도 판답니다",
          "speaker_id": "2",
