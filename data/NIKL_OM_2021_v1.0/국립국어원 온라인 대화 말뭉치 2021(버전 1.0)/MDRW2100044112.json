      "utterance": [
        {
          "id": "MDRW2100044112.1.1",
          "form": "주위에 비혼 있나요?",
          "original_form": "주위에 비혼 있나요?",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044112.1.2",
          "form": "네 있긴 있어요!",
          "original_form": "네 있긴 있어요!",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044112.1.3",
          "form": "저도 비혼인디",
          "original_form": "저도 비혼인디",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044112.1.4",
          "form": "제주위엔 친언니말곤... ㅋㅋ",
          "original_form": "제주위엔 친언니말곤... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044112.1.5",
          "form": "뭔가 확실한 비혼은 아직 주변에 없는거 같아여",
          "original_form": "뭔가 확실한 비혼은 아직 주변에 없는거 같아여",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044112.1.6",
          "form": "사실 비혼이랬다가 결혼하는 사람들이 많죠",
          "original_form": "사실 비혼이랬다가 결혼하는 사람들이 많죠",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044112.1.7",
          "form": "네 대부분 그런거 같아요ㅎㅎ 분위기에 휩쓸리기도 하고",
          "original_form": "네 대부분 그런거 같아요ㅎㅎ 분위기에 휩쓸리기도 하고",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044112.1.8",
          "form": "맞아요 나중에 다 떠나가고ㅋㅋ",
          "original_form": "맞아요 나중에 다 떠나가고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044112.1.9",
          "form": "저보다 나이차 있는 언니들은 비혼 있으신데",
          "original_form": "저보다 나이차 있는 언니들은 비혼 있으신데",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044112.1.10",
          "form": "확실히 안정된 직장과 집이 있으니",
          "original_form": "확실히 안정된 직장과 집이 있으니",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044112.1.11",
          "form": "걱정없어서 비혼이여도 좋은거 같아요....",
          "original_form": "걱정없어서 비혼이여도 좋은거 같아요....",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044112.1.12",
          "form": "뭐든 불안할때 결혼하면 안돼요!",
          "original_form": "뭐든 불안할때 결혼하면 안돼요!",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044112.1.13",
          "form": "자기가 풍족되었을때 그때 여유있게 결혼해야해요 연애도",
          "original_form": "자기가 풍족되었을때 그때 여유있게 결혼해야해요 연애도",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044112.1.14",
          "form": "맞아요 본인이 부족하거나 불안할때 하는 결혼은...정말 아닌거 같아요",
          "original_form": "맞아요 본인이 부족하거나 불안할때 하는 결혼은...정말 아닌거 같아요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044112.1.15",
          "form": "그건 정말 싫더라구요ㅠㅠㅎㅎ",
          "original_form": "그건 정말 싫더라구요ㅠㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044112.1.16",
          "form": "맞아요 결혼으로 도망치는거",
          "original_form": "맞아요 결혼으로 도망치는거",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044112.1.17",
          "form": "다들 근데 결혼하는거 보면 신기해요ㅎㅎ",
          "original_form": "다들 근데 결혼하는거 보면 신기해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044112.1.18",
          "form": "숙제하듯 하는 친구도 있고",
          "original_form": "숙제하듯 하는 친구도 있고",
          "speaker_id": "2",
          "time": "20210907 23:28"
        }
      ]
