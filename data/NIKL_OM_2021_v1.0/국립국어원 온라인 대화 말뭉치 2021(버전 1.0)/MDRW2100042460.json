      "utterance": [
        {
          "id": "MDRW2100042460.1.1",
          "form": "친구가 거기 살아서...",
          "original_form": "친구가 거기 살아서...",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042460.1.2",
          "form": "앗 저도 친구가 부천 살아요ㅎㅎ",
          "original_form": "앗 저도 친구가 부천 살아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042460.1.3",
          "form": "ㅋㅋ한적하고 음식점도 많아요",
          "original_form": "ㅋㅋ한적하고 음식점도 많아요",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042460.1.4",
          "form": "부천에 맛집이 꽤 있더라고요",
          "original_form": "부천에 맛집이 꽤 있더라고요",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042460.1.5",
          "form": "7호선쪽 오시면",
          "original_form": "7호선쪽 오시면",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042460.1.6",
          "form": "네네 카페도 엄청 많고요",
          "original_form": "네네 카페도 엄청 많고요",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042460.1.7",
          "form": "핏제리아라고 신중동역 근처에 피자집도 괜찮아요",
          "original_form": "핏제리아라고 신중동역 근처에 피자집도 괜찮아요",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042460.1.8",
          "form": "파스타도 맛있고",
          "original_form": "파스타도 맛있고",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042460.1.9",
          "form": "아 저장해둬야겠어요 자주 부천에서 만나요",
          "original_form": "아 저장해둬야겠어요 자주 부천에서 만나요",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042460.1.10",
          "form": "네네 핏제리아 추천이요",
          "original_form": "네네 핏제리아 추천이요",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042460.1.11",
          "form": "어떤 음식 좋아하세요,",
          "original_form": "어떤 음식 좋아하세요,",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042460.1.12",
          "form": "까치울 리세스 좋대요",
          "original_form": "까치울 리세스 좋대요",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042460.1.13",
          "form": "저 음식 안가려욪ㅋㅋ",
          "original_form": "저 음식 안가려욪ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042460.1.14",
          "form": "리세스는 어떤 음식점인가욤?",
          "original_form": "리세스는 어떤 음식점인가욤?",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042460.1.15",
          "form": "거기 카펜데 아인슈패너맛집이라며ㅋㅋ",
          "original_form": "거기 카펜데 아인슈패너맛집이라며ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042460.1.16",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042460.1.17",
          "form": "아인슈페너 좋아하는데",
          "original_form": "아인슈페너 좋아하는데",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042460.1.18",
          "form": "정원같은 뷰 사진으로 보내왔어요",
          "original_form": "정원같은 뷰 사진으로 보내왔어요",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042460.1.19",
          "form": "한번 가봐야겠어요",
          "original_form": "한번 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042460.1.20",
          "form": "맛집 얘기하니 신나네요",
          "original_form": "맛집 얘기하니 신나네요",
          "speaker_id": "1",
          "time": "20210906 22:25"
        }
      ]
