        },
        {
          "id": "MDRW2100018325.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018325.1.3",
          "form": "오늘 거기도 습해서 더웠나요?",
          "original_form": "오늘 거기도 습해서 더웠나요?",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018325.1.4",
          "form": "네 끈적끈적했어요 동남아 같이",
          "original_form": "네 끈적끈적했어요 동남아 같이",
          "speaker_id": "1",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100018325.1.5",
          "form": "요즘 안오던 비가 몰아서 와서 너무 습해요ㅠㅠ",
          "original_form": "요즘 안오던 비가 몰아서 와서 너무 습해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018325.1.6",
          "form": "네 맞아요 한참 덥더니",
          "original_form": "네 맞아요 한참 덥더니",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100018325.1.7",
          "form": "갑자기 늦장마ㅠ",
          "original_form": "갑자기 늦장마ㅠ",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100018325.1.8",
          "form": "장마 그치고나면 그래도 좀 시원해지겠죠?",
          "original_form": "장마 그치고나면 그래도 좀 시원해지겠죠?",
          "speaker_id": "2",
          "time": "20210827 09:35"
        },
        {
          "id": "MDRW2100018325.1.9",
          "form": "네 이럴거면 진짜 한번 날잡고 쏟아붓고",
          "original_form": "네 이럴거면 진짜 한번 날잡고 쏟아붓고",
          "speaker_id": "1",
          "time": "20210827 09:37"
        },
        {
          "id": "MDRW2100018325.1.10",
          "form": "온도 좀 팍팍 ㅎㅎ",
          "original_form": "온도 좀 팍팍 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 09:37"
        },
        {
          "id": "MDRW2100018325.1.11",
          "form": "맞아요 그냥 한번에 쏟아붇지 계속 흐리면서 뿌려대니...",
          "original_form": "맞아요 그냥 한번에 쏟아붇지 계속 흐리면서 뿌려대니...",
          "speaker_id": "2",
          "time": "20210827 09:38"
        },
        {
          "id": "MDRW2100018325.1.12",
          "form": "이젠 약한 비는 그냥 맞고 다녀요",
          "original_form": "이젠 약한 비는 그냥 맞고 다녀요",
          "speaker_id": "2",
          "time": "20210827 09:38"
        },
        {
          "id": "MDRW2100018325.1.13",
          "form": "어우 그래도 머리 상해요 ㅎㅎ",
          "original_form": "어우 그래도 머리 상해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 09:40"
        },
        {
          "id": "MDRW2100018325.1.14",
          "form": "그래도 요새 미세먼지는",
          "original_form": "그래도 요새 미세먼지는",
          "speaker_id": "1",
          "time": "20210827 09:40"
        },
        {
          "id": "MDRW2100018325.1.15",
          "form": "확실히 덜한거 같죠",
          "original_form": "확실히 덜한거 같죠",
          "speaker_id": "1",
          "time": "20210827 09:40"
        },
        {
          "id": "MDRW2100018325.1.16",
          "form": "네! 그점이 너무 좋아요",
          "original_form": "네! 그점이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210827 09:42"
        },
        {
          "id": "MDRW2100018325.1.17",
          "form": "호흡기가 안좋아서 요즘 너무 편해요",
          "original_form": "호흡기가 안좋아서 요즘 너무 편해요",
          "speaker_id": "2",
          "time": "20210827 09:42"
        },
        {
          "id": "MDRW2100018325.1.18",
          "form": "그건 코로나의 순기능인가 싶기도...",
          "original_form": "그건 코로나의 순기능인가 싶기도...",
          "speaker_id": "1",
          "time": "20210827 09:42"
        },
        {
          "id": "MDRW2100018325.1.19",
          "form": "근데 마스크 맨날 끼고 다니기",
          "original_form": "근데 마스크 맨날 끼고 다니기",
          "speaker_id": "1",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018325.1.20",
          "form": "좋은점도 있어야.. ..",
          "original_form": "좋은점도 있어야.. ..",
          "speaker_id": "2",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018325.1.21",
          "form": "너무 힘들어요ㅋㅋㅋㅋ",
          "original_form": "너무 힘들어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018325.1.22",
          "form": "그쵸ㅠㅠㅠㅠ",
          "original_form": "그쵸ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018325.1.23",
          "form": "지나가겠죠 ㅎㅎ",
          "original_form": "지나가겠죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018325.1.24",
          "form": "일단 미세먼지 없는건 좋아서",
          "original_form": "일단 미세먼지 없는건 좋아서",
