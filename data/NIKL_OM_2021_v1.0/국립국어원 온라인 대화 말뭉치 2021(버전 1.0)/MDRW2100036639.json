      "utterance": [
        {
          "id": "MDRW2100036639.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 03:34"
        },
        {
          "id": "MDRW2100036639.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 03:34"
        },
        {
          "id": "MDRW2100036639.1.3",
          "form": "고기 뭐좋아하세요",
          "original_form": "고기 뭐좋아하세요",
          "speaker_id": "1",
          "time": "20210831 03:34"
        },
        {
          "id": "MDRW2100036639.1.4",
          "form": "음 닭/돼지/소 고기 다 잘 먹어요",
          "original_form": "음 닭/돼지/소 고기 다 잘 먹어요",
          "speaker_id": "2",
          "time": "20210831 03:35"
        },
        {
          "id": "MDRW2100036639.1.5",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "1",
          "time": "20210831 03:35"
        },
        {
          "id": "MDRW2100036639.1.6",
          "form": "그러면 오리고기는여?",
          "original_form": "그러면 오리고기는여?",
          "speaker_id": "1",
          "time": "20210831 03:35"
        },
        {
          "id": "MDRW2100036639.1.7",
          "form": "닭고기나 돼지고기를 선호하구요",
          "original_form": "닭고기나 돼지고기를 선호하구요",
          "speaker_id": "2",
          "time": "20210831 03:35"
        },
        {
          "id": "MDRW2100036639.1.8",
          "form": "아 오리고기도 좋아해요",
          "original_form": "아 오리고기도 좋아해요",
          "speaker_id": "2",
          "time": "20210831 03:35"
        },
        {
          "id": "MDRW2100036639.1.9",
          "form": "훈제?? 로스??",
          "original_form": "훈제?? 로스??",
          "speaker_id": "1",
          "time": "20210831 03:35"
        },
        {
          "id": "MDRW2100036639.1.10",
          "form": "뭐가좋아여",
          "original_form": "뭐가좋아여",
          "speaker_id": "1",
          "time": "20210831 03:36"
        },
        {
          "id": "MDRW2100036639.1.11",
          "form": "하지만 양이나 염소 이 쪽은 못 먹어요",
          "original_form": "하지만 양이나 염소 이 쪽은 못 먹어요",
          "speaker_id": "2",
          "time": "20210831 03:36"
        },
        {
          "id": "MDRW2100036639.1.12",
          "form": "오  염소 드셔보셧어요?",
          "original_form": "오  염소 드셔보셧어요?",
          "speaker_id": "1",
          "time": "20210831 03:36"
        },
        {
          "id": "MDRW2100036639.1.13",
          "form": "양이나??",
          "original_form": "양이나??",
          "speaker_id": "1",
          "time": "20210831 03:36"
        },
        {
          "id": "MDRW2100036639.1.14",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210831 03:37"
        },
        {
          "id": "MDRW2100036639.1.15",
          "form": "염소는 염소탕 먹어봤고, 양은 양고기집에 갔는데 음.. 못 먹겠어서 한입을 먹었던가 안 먹었던가 그래요",
          "original_form": "염소는 염소탕 먹어봤고, 양은 양고기집에 갔는데 음.. 못 먹겠어서 한입을 먹었던가 안 먹었던가 그래요",
          "speaker_id": "2",
          "time": "20210831 03:38"
        },
        {
          "id": "MDRW2100036639.1.16",
          "form": "오.. 그렇군요",
          "original_form": "오.. 그렇군요",
          "speaker_id": "1",
          "time": "20210831 03:40"
        },
        {
          "id": "MDRW2100036639.1.17",
          "form": "양고기먹어봣는데\n질기기도하고 그거 진짜 손가락 반만하게 잘라서 향신료 진하게해갖고 먹어야겟더라고요..",
          "original_form": "양고기먹어봣는데\n질기기도하고 그거 진짜 손가락 반만하게 잘라서 향신료 진하게해갖고 먹어야겟더라고요..",
          "speaker_id": "1",
          "time": "20210831 03:40"
        },
        {
          "id": "MDRW2100036639.1.18",
          "form": "손가락한마디 반",
          "original_form": "손가락한마디 반",
          "speaker_id": "1",
          "time": "20210831 03:40"
        },
        {
          "id": "MDRW2100036639.1.19",
          "form": "그쵸 향신료를 찍어 먹어야한다던데 저는 향신료부터 거부감이 크더라구요ㅜㅜ",
          "original_form": "그쵸 향신료를 찍어 먹어야한다던데 저는 향신료부터 거부감이 크더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 03:43"
        },
        {
          "id": "MDRW2100036639.1.20",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 . 그거 좀 그래요..ㅜ\n제동생은 진짜 잘먹어요..",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 . 그거 좀 그래요..ㅜ\n제동생은 진짜 잘먹어요..",
          "speaker_id": "1",
          "time": "20210831 03:45"
        },
        {
          "id": "MDRW2100036639.1.21",
          "form": "양고기 좋아하시는분들은 진짜 좋아하시더라구요",
          "original_form": "양고기 좋아하시는분들은 진짜 좋아하시더라구요",
          "speaker_id": "2",
          "time": "20210831 03:48"
        },
        {
          "id": "MDRW2100036639.1.22",
          "form": "제동샏은 제가 먹다남은거 다 줫더니 신낫더라고요 ㅎㅎㅎ",
          "original_form": "제동샏은 제가 먹다남은거 다 줫더니 신낫더라고요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 03:50"
        },
        {
          "id": "MDRW2100036639.1.23",
          "form": "그리고 그 찍어먹는 향신료 좋아해요",
          "original_form": "그리고 그 찍어먹는 향신료 좋아해요",
          "speaker_id": "1",
          "time": "20210831 03:50"
        },
        {
          "id": "MDRW2100036639.1.24",
          "form": "정말 좋아하나보네요",
          "original_form": "정말 좋아하나보네요",
          "speaker_id": "2",
          "time": "20210831 03:51"
        },
        {
          "id": "MDRW2100036639.1.25",
          "form": "네.. 워낙 뭐든 다 잘먹어서 ㅎㅎㅎㅎ\n외국에서도 혼자 잘 먹고다닐 인간이죠",
          "original_form": "네.. 워낙 뭐든 다 잘먹어서 ㅎㅎㅎㅎ\n외국에서도 혼자 잘 먹고다닐 인간이죠",
          "speaker_id": "1",
          "time": "20210831 03:51"
        },
        {
          "id": "MDRW2100036639.1.26",
          "form": "동생분이 새로운 음식에 거부감이 없는 편인가봐요",
          "original_form": "동생분이 새로운 음식에 거부감이 없는 편인가봐요",
          "speaker_id": "2",
          "time": "20210831 03:51"
        },
        {
          "id": "MDRW2100036639.1.27",
          "form": "네.ㅋㅋㅋㅋ 막 해달래요..\n별낫어요. 개고기도 먹을수잇대요",
          "original_form": "네.ㅋㅋㅋㅋ 막 해달래요..\n별낫어요. 개고기도 먹을수잇대요",
          "speaker_id": "1",
          "time": "20210831 03:52"
        }
      ]
    }
