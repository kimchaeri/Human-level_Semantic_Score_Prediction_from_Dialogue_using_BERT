      "utterance": [
        {
          "id": "MDRW2100012699.1.1",
          "form": "등대 알지?",
          "original_form": "등대 알지?",
          "speaker_id": "1",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100012699.1.2",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100012699.1.3",
          "form": "남해바다가 보이는 100년된 등대가 있데",
          "original_form": "남해바다가 보이는 100년된 등대가 있데",
          "speaker_id": "1",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100012699.1.4",
          "form": "엄청 오래되었네",
          "original_form": "엄청 오래되었네",
          "speaker_id": "2",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100012699.1.5",
          "form": "엉 근데 이 등대 옆 숙소에서 선착순 공짜로 숙박할수 있데",
          "original_form": "엉 근데 이 등대 옆 숙소에서 선착순 공짜로 숙박할수 있데",
          "speaker_id": "1",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100012699.1.6",
          "form": "정말 좋네",
          "original_form": "정말 좋네",
          "speaker_id": "2",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100012699.1.7",
          "form": "엉 1박2일로 근데 공짜이다 보니 경쟁률이 엄청 큼",
          "original_form": "엉 1박2일로 근데 공짜이다 보니 경쟁률이 엄청 큼",
          "speaker_id": "1",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012699.1.8",
          "form": "그렇겠다",
          "original_form": "그렇겠다",
          "speaker_id": "2",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012699.1.9",
          "form": "이 등대에서 1박2일 얘들한테 엄청 난 추억이겠다",
          "original_form": "이 등대에서 1박2일 얘들한테 엄청 난 추억이겠다",
          "speaker_id": "1",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100012699.1.10",
          "form": "그렇겠다 어디에 신청하는거야?",
          "original_form": "그렇겠다 어디에 신청하는거야?",
          "speaker_id": "2",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012699.1.11",
          "form": "여 수 지 방 해 양 항 만 청에 신청한데",
          "original_form": "여 수 지 방 해 양 항 만 청에 신청한데",
          "speaker_id": "1",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012699.1.12",
          "form": "아~좋았어",
          "original_form": "아~좋았어",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012699.1.13",
          "form": "코르나때문에 지금도 하는줄 모르겠다",
          "original_form": "코르나때문에 지금도 하는줄 모르겠다",
          "speaker_id": "1",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012699.1.14",
          "form": "그러게 홈피에 들어가보면 있겠지",
          "original_form": "그러게 홈피에 들어가보면 있겠지",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012699.1.15",
          "form": "엉 최대8명이 잘수 있다니 좋을듯",
          "original_form": "엉 최대8명이 잘수 있다니 좋을듯",
          "speaker_id": "1",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012699.1.16",
          "form": "나주에 보면 국립박물관이 있거든",
          "original_form": "나주에 보면 국립박물관이 있거든",
