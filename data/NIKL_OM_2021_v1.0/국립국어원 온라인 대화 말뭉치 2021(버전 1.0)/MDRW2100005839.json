      "utterance": [
        {
          "id": "MDRW2100005839.1.1",
          "form": "마요네즈같은거요?",
          "original_form": "마요네즈같은거요?",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005839.1.2",
          "form": "음식에 넣는 모든 소스요. 소금이건 케찹이건",
          "original_form": "음식에 넣는 모든 소스요. 소금이건 케찹이건",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005839.1.3",
          "form": "요리를 잘 안해요",
          "original_form": "요리를 잘 안해요",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005839.1.4",
          "form": "그냥 발라도 되니까 잼처럼 저는 다양하게 좋아해요",
          "original_form": "그냥 발라도 되니까 잼처럼 저는 다양하게 좋아해요",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005839.1.5",
          "form": "좋아하는 소스는요?",
          "original_form": "좋아하는 소스는요?",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005839.1.6",
          "form": "오리고기에는 머스터드가 좋고요",
          "original_form": "오리고기에는 머스터드가 좋고요",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005839.1.7",
          "form": "그리고요",
          "original_form": "그리고요",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100005839.1.8",
          "form": "마파두부 소스도 좋아하고 똠얌꿍 아시나요",
          "original_form": "마파두부 소스도 좋아하고 똠얌꿍 아시나요",
          "speaker_id": "1",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100005839.1.9",
          "form": "새콤한 태국 전골이요",
          "original_form": "새콤한 태국 전골이요",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100005839.1.10",
          "form": "그 소스로 볶음밥해도 맛있고 피자로 먹어도맛있어요",
          "original_form": "그 소스로 볶음밥해도 맛있고 피자로 먹어도맛있어요",
          "speaker_id": "1",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100005839.1.11",
          "form": "아직 먹어보진않았어요",
          "original_form": "아직 먹어보진않았어요",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100005839.1.12",
          "form": "오리엔탈 소스는 샐러드로 좋고요",
          "original_form": "오리엔탈 소스는 샐러드로 좋고요",
          "speaker_id": "1",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100005839.1.13",
          "form": "저는 케찹만 알아요",
          "original_form": "저는 케찹만 알아요",
          "speaker_id": "2",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100005839.1.14",
          "form": "굴소스는 해산물 볶음에 딱이에요",
          "original_form": "굴소스는 해산물 볶음에 딱이에요",
          "speaker_id": "1",
          "time": "20210907 13:58"
        }
