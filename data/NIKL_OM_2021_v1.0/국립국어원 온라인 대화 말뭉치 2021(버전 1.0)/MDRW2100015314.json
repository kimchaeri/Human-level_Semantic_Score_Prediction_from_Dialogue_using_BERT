      "utterance": [
        {
          "id": "MDRW2100015314.1.1",
          "form": "싸이월드 해 보신 적 있으신가요",
          "original_form": "싸이월드 해 보신 적 있으신가요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015314.1.2",
          "form": "아이고 흑역사 대창고",
          "original_form": "아이고 흑역사 대창고",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015314.1.3",
          "form": "물론이죠",
          "original_form": "물론이죠",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015314.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015314.1.5",
          "form": "저두 한때 열심히 했었죠",
          "original_form": "저두 한때 열심히 했었죠",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015314.1.6",
          "form": "이번에 부활한다고 하는데",
          "original_form": "이번에 부활한다고 하는데",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015314.1.7",
          "form": "부활하면 다시 들어가 볼 생각 있으신가요",
          "original_form": "부활하면 다시 들어가 볼 생각 있으신가요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015314.1.8",
          "form": "들어가볼까 했는데",
          "original_form": "들어가볼까 했는데",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015314.1.9",
          "form": "판도라의 상자를 ... 열어야할지 고민이네요",
          "original_form": "판도라의 상자를 ... 열어야할지 고민이네요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015314.1.10",
          "form": "그저 그때 듣던 브금이나 찾아듣고 있습니다",
          "original_form": "그저 그때 듣던 브금이나 찾아듣고 있습니다",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015314.1.11",
          "form": "저도....들어가서 없애버려야 하나 아님 들어가서 맞이해야하나 고민중이에요",
          "original_form": "저도....들어가서 없애버려야 하나 아님 들어가서 맞이해야하나 고민중이에요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015314.1.12",
          "form": "그때 나름 아바타도 꾸미고 스킨도 사고 열심히 꾸몄었는데",
          "original_form": "그때 나름 아바타도 꾸미고 스킨도 사고 열심히 꾸몄었는데",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015314.1.13",
          "form": "다 추억이네요",
          "original_form": "다 추억이네요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015314.1.14",
          "form": "도토리 좀 넣으셨나봐요",
          "original_form": "도토리 좀 넣으셨나봐요",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015314.1.15",
          "form": "가족이 줘서 한창 쓰고 그랬었어요",
          "original_form": "가족이 줘서 한창 쓰고 그랬었어요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015314.1.16",
          "form": "그땐 돈이 없을때라 저도 제 돈으로 충전해 본 적은 없는것 같아요ㅋㅋㅋ",
          "original_form": "그땐 돈이 없을때라 저도 제 돈으로 충전해 본 적은 없는것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015314.1.17",
          "form": "오 좋은 가족을 두셨네요",
          "original_form": "오 좋은 가족을 두셨네요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015314.1.18",
          "form": "저는 학교에서 주는 문상으로 쪼금씩 넣어서",
          "original_form": "저는 학교에서 주는 문상으로 쪼금씩 넣어서",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015314.1.19",
          "form": "음악하나 사고...  그랬죠...",
          "original_form": "음악하나 사고...  그랬죠...",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015314.1.20",
          "form": "지금 생각해 보면 배경음악 돈 주고 사는것도 되게 웃긴데",
          "original_form": "지금 생각해 보면 배경음악 돈 주고 사는것도 되게 웃긴데",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015314.1.21",
          "form": "그때는 되게 당연했던것 같아요",
          "original_form": "그때는 되게 당연했던것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015314.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아니 그때 노래 듣는건 불법다운해서 소리바다로 들으면서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아니 그때 노래 듣는건 불법다운해서 소리바다로 들으면서",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015314.1.23",
          "form": "그걸 도토리 모아서 샀었네요",
          "original_form": "그걸 도토리 모아서 샀었네요",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015314.1.24",
          "form": "신기한 저작권 인식",
          "original_form": "신기한 저작권 인식",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015314.1.25",
          "form": "그땐 다 그랬죠 뭐ㅋㅋㅋㅋㅋ",
          "original_form": "그땐 다 그랬죠 뭐ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015314.1.26",
          "form": "지금은 사람들이 배경음악이나 배경스킨 돈 주고 안 살텐데",
          "original_form": "지금은 사람들이 배경음악이나 배경스킨 돈 주고 안 살텐데",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015314.1.27",
          "form": "뭐로 돈 벌지 궁굼해요",
          "original_form": "뭐로 돈 벌지 궁굼해요",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015314.1.28",
          "form": "그러게요 ...",
          "original_form": "그러게요 ...",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015314.1.29",
          "form": "사실 싸이월드 다시 하고싶지도 않은데 ㅋㅋㅋ",
          "original_form": "사실 싸이월드 다시 하고싶지도 않은데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015314.1.30",
          "form": "하는 사람이 있으려나",
          "original_form": "하는 사람이 있으려나",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015314.1.31",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015314.1.32",
          "form": "싸이월드에 있는 흑역사 지우러 간다는 사람이 더 많더라구요ㅋㅋㅋㅌ",
          "original_form": "싸이월드에 있는 흑역사 지우러 간다는 사람이 더 많더라구요ㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015314.1.33",
          "form": "지금은 워낙 다른 SNS가 많아서 싸이는 안 할것 같긴 해요",
          "original_form": "지금은 워낙 다른 SNS가 많아서 싸이는 안 할것 같긴 해요",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015314.1.34",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015314.1.35",
          "form": "전 그냥 sns를 안해서",
          "original_form": "전 그냥 sns를 안해서",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015314.1.36",
          "form": "싸이도 귀찮네요",
          "original_form": "싸이도 귀찮네요",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015314.1.37",
          "form": "인스타도 하다가 말았는데",
          "original_form": "인스타도 하다가 말았는데",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015314.1.38",
          "form": "미니홈피를 꾸미라니!",
          "original_form": "미니홈피를 꾸미라니!",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015314.1.39",
          "form": "저도 나이 드니깐 sns에 손아 잘 안가서",
          "original_form": "저도 나이 드니깐 sns에 손아 잘 안가서",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015314.1.40",
          "form": "부활해도 안 할것 같아요ㅋㅋㅋ",
          "original_form": "부활해도 안 할것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:55"
        }
      ]
