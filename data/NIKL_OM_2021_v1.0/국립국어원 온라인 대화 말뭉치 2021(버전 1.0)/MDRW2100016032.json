        },
        {
          "id": "MDRW2100016032.1.2",
          "form": "유아크립토커런시트레이더",
          "original_form": "유아크립토커런시트레이더",
          "speaker_id": "1",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100016032.1.3",
          "form": "나 투자금 늘림",
          "original_form": "나 투자금 늘림",
          "speaker_id": "1",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100016032.1.4",
          "form": "300만원 추가",
          "original_form": "300만원 추가",
          "speaker_id": "1",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100016032.1.5",
          "form": "총 500으로 할  예정",
          "original_form": "총 500으로 할  예정",
          "speaker_id": "1",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100016032.1.6",
          "form": "월세 벌쟈",
          "original_form": "월세 벌쟈",
          "speaker_id": "1",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100016032.1.7",
          "form": "응.. 안전하게 돈버는거 알려줄게",
          "original_form": "응.. 안전하게 돈버는거 알려줄게",
          "speaker_id": "2",
          "time": "20210826 10:21"
        },
        {
          "id": "MDRW2100016032.1.8",
          "form": "톡으로 보냄",
          "original_form": "톡으로 보냄",
          "speaker_id": "2",
          "time": "20210826 10:21"
        },
        {
          "id": "MDRW2100016032.1.9",
          "form": "안왔어",
          "original_form": "안왔어",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100016032.1.10",
          "form": "안전해?",
          "original_form": "안전해?",
          "speaker_id": "1",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016032.1.11",
          "form": "리스키해보이는데",
          "original_form": "리스키해보이는데",
          "speaker_id": "1",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016032.1.12",
          "form": "안전해 9시전에만 팔면",
          "original_form": "안전해 9시전에만 팔면",
          "speaker_id": "2",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016032.1.13",
          "form": "아니면 몇일 구경해봐",
          "original_form": "아니면 몇일 구경해봐",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016032.1.14",
          "form": "잡코 펌핑장 끝나면 저것도 없지",
          "original_form": "잡코 펌핑장 끝나면 저것도 없지",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016032.1.15",
          "form": "그간 9싀 구간 15분 차트 넘겨봐바",
          "original_form": "그간 9싀 구간 15분 차트 넘겨봐바",
          "speaker_id": "2",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100016032.1.16",
          "form": "하락장아니면 괜춘",
          "original_form": "하락장아니면 괜춘",
          "speaker_id": "2",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100016032.1.17",
          "form": "확실한건 9시 채택안되면 3-4퍼 떨어져",
          "original_form": "확실한건 9시 채택안되면 3-4퍼 떨어져",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100016032.1.18",
          "form": "비코 우상향중에 저차트 나온거아",
          "original_form": "비코 우상향중에 저차트 나온거아",
          "speaker_id": "2",
          "time": "20210826 10:38"
        },
        {
          "id": "MDRW2100016032.1.19",
          "form": "9시전이 안전하다는 거잖아",
          "original_form": "9시전이 안전하다는 거잖아",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100016032.1.20",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016032.1.21",
          "form": "9시 되자마자 파는게 젤 비쌈",
          "original_form": "9시 되자마자 파는게 젤 비쌈",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016032.1.22",
