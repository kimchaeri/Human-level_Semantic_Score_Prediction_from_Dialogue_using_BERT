      "utterance": [
        {
          "id": "MDRW2100042481.1.1",
          "form": "우리 학기 초에",
          "original_form": "우리 학기 초에",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042481.1.2",
          "form": "우리 name3도 그 선생님 반되고 싶다",
          "original_form": "우리 &name3&도 그 선생님 반되고 싶다",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042481.1.3",
          "form": "애들 초상권 그 뭐지 동의서 받아서",
          "original_form": "애들 초상권 그 뭐지 동의서 받아서",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042481.1.4",
          "form": "네이버 카페 만들어서 사진 찍어서 올려줘요",
          "original_form": "네이버 카페 만들어서 사진 찍어서 올려줘요",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042481.1.5",
          "form": "선생님이",
          "original_form": "선생님이",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042481.1.6",
          "form": "계속 이렇게 애들 사진 찍고 해줬나 봐요",
          "original_form": "계속 이렇게 애들 사진 찍고 해줬나 봐요",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.7",
          "form": "오 정말 좋다",
          "original_form": "오 정말 좋다",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.8",
          "form": "근데 엄마들이",
          "original_form": "근데 엄마들이",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.9",
          "form": "내 사진은 왜 없냐 왜 뒤통수 만났냐",
          "original_form": "내 사진은 왜 없냐 왜 뒤통수 만났냐",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.10",
          "form": "하아",
          "original_form": "하아",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.11",
          "form": "그래서 미리 적어놨더라고요",
          "original_form": "그래서 미리 적어놨더라고요",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.12",
          "form": "또 시작이네.....",
          "original_form": "또 시작이네.....",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.13",
          "form": "왜 그러지??",
          "original_form": "왜 그러지??",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.14",
          "form": "일부러 그러는 거 아니니까",
          "original_form": "일부러 그러는 거 아니니까",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.15",
          "form": "사진 이 중요한가??",
          "original_form": "사진 이 중요한가??",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.16",
          "form": "양해 좀 해달라고",
          "original_form": "양해 좀 해달라고",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.17",
          "form": "그냥 그렇게 사진 찍어주는 것만으로도 정말 좋은 선생님 같은데",
          "original_form": "그냥 그렇게 사진 찍어주는 것만으로도 정말 좋은 선생님 같은데",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.18",
          "form": "배가 불렀구만",
          "original_form": "배가 불렀구만",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042481.1.19",
          "form": "의외로 언니 자기의 사진 없다고 뭐라하는 사람들 있는가 봐요",
          "original_form": "의외로 언니 자기의 사진 없다고 뭐라하는 사람들 있는가 봐요",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042481.1.20",
          "form": "양해를 해달라니....",
          "original_form": "양해를 해달라니....",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042481.1.21",
          "form": "아 진짜",
          "original_form": "아 진짜",
          "speaker_id": "1",
          "time": "20210906 22:24"
        }
      ]
    }
