      "utterance": [
        {
          "id": "MDRW2100029503.1.1",
          "form": "감자 튀김 말해뭐해요",
          "original_form": "감자 튀김 말해뭐해요",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100029503.1.2",
          "form": "존맛이죠",
          "original_form": "존맛이죠",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100029503.1.3",
          "form": "튀김은 다 존맛이죠",
          "original_form": "튀김은 다 존맛이죠",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100029503.1.4",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100029503.1.5",
          "form": "새우튀김 좋아하나요",
          "original_form": "새우튀김 좋아하나요",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100029503.1.6",
          "form": "엄청요",
          "original_form": "엄청요",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100029503.1.7",
          "form": "튀김 맛집 아시나요",
          "original_form": "튀김 맛집 아시나요",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100029503.1.8",
          "form": "오징어튀김도 좋아해요",
          "original_form": "오징어튀김도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100029503.1.9",
          "form": "저는 남포동 튀김골목 좋아합니다",
          "original_form": "저는 남포동 튀김골목 좋아합니다",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100029503.1.10",
          "form": "대신동 시장에 튀김 맛있는곳 이어요",
          "original_form": "대신동 시장에 튀김 맛있는곳 이어요",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100029503.1.11",
          "form": "깔끔해요",
          "original_form": "깔끔해요",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100029503.1.12",
          "form": "본것같군요 튀김맛집",
          "original_form": "본것같군요 튀김맛집",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100029503.1.13",
          "form": "순대도 파나요",
          "original_form": "순대도 파나요",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100029503.1.14",
          "form": "떡볶이랑 먹으면 존맛이에요",
          "original_form": "떡볶이랑 먹으면 존맛이에요",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100029503.1.15",
          "form": "아뇨퇴김만요",
          "original_form": "아뇨퇴김만요",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100029503.1.16",
          "form": "떡볶이 얘기는 항상하네요",
          "original_form": "떡볶이 얘기는 항상하네요",
          "speaker_id": "1",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100029503.1.17",
          "form": "맛있으니까요",
          "original_form": "맛있으니까요",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100029503.1.18",
          "form": "살찌지만 맛있죠",
          "original_form": "살찌지만 맛있죠",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100029503.1.19",
          "form": "튀김원푸드라 전문성 느껴지네요",
