        {
          "id": "MDRW2100043389.1.1",
          "form": "비가 계속 오네요",
          "original_form": "비가 계속 오네요",
          "speaker_id": "1",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043389.1.2",
          "form": "ㅜㅜ주말엔 그쳣으면 좋겟어요",
          "original_form": "ㅜㅜ주말엔 그쳣으면 좋겟어요",
          "speaker_id": "2",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043389.1.3",
          "form": "네ㅠ 비가 길어지네요",
          "original_form": "네ㅠ 비가 길어지네요",
          "speaker_id": "1",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100043389.1.4",
          "form": "거기는 많이오나요?",
          "original_form": "거기는 많이오나요?",
          "speaker_id": "2",
          "time": "20210907 18:45"
        },
        {
          "id": "MDRW2100043389.1.5",
          "form": "그냥 계속 쭉 비가 와요",
          "original_form": "그냥 계속 쭉 비가 와요",
          "speaker_id": "1",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043389.1.6",
          "form": "가을장마 징하네요ㅜ",
          "original_form": "가을장마 징하네요ㅜ",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043389.1.7",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043389.1.8",
          "form": "가을에 이렇게 비가 많이 내렸었나...",
          "original_form": "가을에 이렇게 비가 많이 내렸었나...",
          "speaker_id": "1",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043389.1.9",
          "form": "내일도 올꺼같아여",
          "original_form": "내일도 올꺼같아여",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043389.1.10",
          "form": "전 내일 야외행사가 있어서",
          "original_form": "전 내일 야외행사가 있어서",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043389.1.11",
          "form": "걱정이예요 날씨",
          "original_form": "걱정이예요 날씨",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
