      "utterance": [
        {
          "id": "MDRW2100036197.1.1",
          "form": "피자마루 꿀고구마 치즈크러스트 추가 했어요!",
          "original_form": "피자마루 꿀고구마 치즈크러스트 추가 했어요!",
          "speaker_id": "2",
          "time": "20210830 18:52"
        },
        {
          "id": "MDRW2100036197.1.2",
          "form": "치즈크러스트 추가라니 ...",
          "original_form": "치즈크러스트 추가라니 ...",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036197.1.3",
          "form": "배우신분이군요?",
          "original_form": "배우신분이군요?",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036197.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036197.1.5",
          "form": "얼마에요?!",
          "original_form": "얼마에요?!",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036197.1.6",
          "form": "단돈 만오천사백원 이였슴닷ㅋㅋㅋㅋㄲ",
          "original_form": "단돈 만오천사백원 이였슴닷ㅋㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036197.1.7",
          "form": "허겁지겁 먹어버렸어요",
          "original_form": "허겁지겁 먹어버렸어요",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036197.1.8",
          "form": "내 ㅍㅣ자..",
          "original_form": "내 ㅍㅣ자..",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036197.1.9",
          "form": "역시 싸군요 ㅜㅜ",
          "original_form": "역시 싸군요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100036197.1.10",
          "form": "한판 다 드셨어요?!",
          "original_form": "한판 다 드셨어요?!",
          "speaker_id": "1",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100036197.1.11",
          "form": "그쵸ㅠㅠ 진짜 가성비 갑이예요",
          "original_form": "그쵸ㅠㅠ 진짜 가성비 갑이예요",
          "speaker_id": "2",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100036197.1.12",
          "form": "그럴 기세로 먹었지만 가족과 함께 했슴닷",
          "original_form": "그럴 기세로 먹었지만 가족과 함께 했슴닷",
          "speaker_id": "2",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100036197.1.13",
          "form": "ㅋㅋㅋㅋㅋ저도 진짜 좋아하지만",
          "original_form": "ㅋㅋㅋㅋㅋ저도 진짜 좋아하지만",
          "speaker_id": "1",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100036197.1.14",
          "form": "피자는 1인 1판 불가능이에요",
          "original_form": "피자는 1인 1판 불가능이에요",
          "speaker_id": "1",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100036197.1.15",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100036197.1.16",
          "form": "1인1닭은 가능하신가요 ㅎㅎ",
          "original_form": "1인1닭은 가능하신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036197.1.17",
          "form": "저 진짜 잘먹을때 토핑 없는 치즈피자는 다 먹었는데 토핑있는건 힘들더라구요ㅋㅋㅋ",
          "original_form": "저 진짜 잘먹을때 토핑 없는 치즈피자는 다 먹었는데 토핑있는건 힘들더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036197.1.18",
          "form": "아 옛날통닭은 가능합니닷!",
          "original_form": "아 옛날통닭은 가능합니닷!",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036197.1.19",
          "form": "굽네랑요ㅎㅎ",
          "original_form": "굽네랑요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036197.1.20",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036197.1.21",
          "form": "헐 토핑 없는건 한판 다 먹으면 안느끼해요 ???",
          "original_form": "헐 토핑 없는건 한판 다 먹으면 안느끼해요 ???",
          "speaker_id": "1",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036197.1.22",
          "form": "전 굽네랑 교촌도 가능합니다 ㅋㅋ !",
          "original_form": "전 굽네랑 교촌도 가능합니다 ㅋㅋ !",
          "speaker_id": "1",
          "time": "20210830 19:31"
        },
        {
          "id": "MDRW2100036197.1.23",
          "form": "저에겐 탄산이 있습니닷!!!",
          "original_form": "저에겐 탄산이 있습니닷!!!",
          "speaker_id": "2",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036197.1.24",
          "form": "위에 핫소스도 뿌리며ㅋㅋㅋㅋ 치즈 좋아해서그런지 괜찮더라구요!",
          "original_form": "위에 핫소스도 뿌리며ㅋㅋㅋㅋ 치즈 좋아해서그런지 괜찮더라구요!",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100036197.1.25",
          "form": "배우신분 배우신분",
          "original_form": "배우신분 배우신분",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100036197.1.26",
          "form": "제 주변엔 가능한 친구가 없어서 같이 즐기지 못했는데 너무 신나네요ㅋㅋㅋ",
          "original_form": "제 주변엔 가능한 친구가 없어서 같이 즐기지 못했는데 너무 신나네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100036197.1.27",
          "form": "아 하긴 탄산과 핫소스가 있다면 가능하겠군요 !",
          "original_form": "아 하긴 탄산과 핫소스가 있다면 가능하겠군요 !",
          "speaker_id": "1",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100036197.1.28",
          "form": "헐 친구분들 다들 위가 작으시군요???!",
          "original_form": "헐 친구분들 다들 위가 작으시군요???!",
          "speaker_id": "1",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100036197.1.29",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100036197.1.30",
          "form": "그니까요ㅜㅠ 같이먹으면 저만 배 안부른건가 해요",
          "original_form": "그니까요ㅜㅠ 같이먹으면 저만 배 안부른건가 해요",
          "speaker_id": "2",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100036197.1.31",
          "form": "아 ㅋㅋㅋ 그거 뭔지 알아요 ...",
          "original_form": "아 ㅋㅋㅋ 그거 뭔지 알아요 ...",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100036197.1.32",
          "form": "끝까지 먹고 있는 제 자신을 발견하죠 ...",
          "original_form": "끝까지 먹고 있는 제 자신을 발견하죠 ...",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100036197.1.33",
          "form": "혼자 살아남은 ㅋㅋ",
          "original_form": "혼자 살아남은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100036197.1.34",
          "form": "최후의 1인이잖아욬ㄱㄲ 저도 그래요 ...",
          "original_form": "최후의 1인이잖아욬ㄱㄲ 저도 그래요 ...",
