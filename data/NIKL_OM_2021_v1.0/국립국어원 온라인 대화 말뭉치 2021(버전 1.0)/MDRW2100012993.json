        {
          "id": "MDRW2100012993.1.2",
          "form": "안녕하세요~^^",
          "original_form": "안녕하세요~^^",
          "speaker_id": "2",
          "time": "20210825 10:15"
        },
        {
          "id": "MDRW2100012993.1.3",
          "form": "운동 좋아하시나요",
          "original_form": "운동 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012993.1.4",
          "form": "아뇨 저는 운동은 안좋아합니다",
          "original_form": "아뇨 저는 운동은 안좋아합니다",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100012993.1.5",
          "form": "성형에 대해 얘기나눠보는거 어때요",
          "original_form": "성형에 대해 얘기나눠보는거 어때요",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100012993.1.6",
          "form": "오 성형 하신적있나요",
          "original_form": "오 성형 하신적있나요",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100012993.1.7",
          "form": "넹 마니 했어요",
          "original_form": "넹 마니 했어요",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100012993.1.8",
          "form": "어디 하셨나용",
          "original_form": "어디 하셨나용",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100012993.1.9",
          "form": "눈 코 턱 가슴 양악이요",
          "original_form": "눈 코 턱 가슴 양악이요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100012993.1.10",
          "form": "비용은 어느정도인가요",
          "original_form": "비용은 어느정도인가요",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100012993.1.11",
          "form": "비싸져~~~",
          "original_form": "비싸져~~~",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100012993.1.12",
          "form": "안아프신가여ㅠ",
          "original_form": "안아프신가여ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100012993.1.13",
          "form": "몇번 하다보면 갠찬아여 ㅋㅋㅋ",
          "original_form": "몇번 하다보면 갠찬아여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100012993.1.14",
          "form": "가가구님은 성형해본적 없어요?",
          "original_form": "가가구님은 성형해본적 없어요?",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100012993.1.15",
          "form": "네 아직 없스비다ㅎㅎ",
          "original_form": "네 아직 없스비다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100012993.1.16",
          "form": "하고싶은곳은여?",
          "original_form": "하고싶은곳은여?",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100012993.1.17",
