      "utterance": [
        {
          "id": "MDRW2100031635.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031635.1.2",
          "form": "대학생이신가요?",
          "original_form": "대학생이신가요?",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031635.1.3",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031635.1.4",
          "form": "졸업했어요ㅎㅎ",
          "original_form": "졸업했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031635.1.5",
          "form": "무슨 학과 다니셨어요??",
          "original_form": "무슨 학과 다니셨어요??",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031635.1.6",
          "form": "저는 예체능이었어유",
          "original_form": "저는 예체능이었어유",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031635.1.7",
          "form": "미술쪽!",
          "original_form": "미술쪽!",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031635.1.8",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031635.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031635.1.10",
          "form": "예체능 !! 너무 멋있어요",
          "original_form": "예체능 !! 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031635.1.11",
          "form": "저는 물리치료학과 다녀요!",
          "original_form": "저는 물리치료학과 다녀요!",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031635.1.12",
          "form": "예체능 항상 대단하다고 생각해요",
          "original_form": "예체능 항상 대단하다고 생각해요",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031635.1.13",
          "form": "그냥 겉보기에만 좋은 학과",
          "original_form": "그냥 겉보기에만 좋은 학과",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100031635.1.14",
          "form": "내실은없어유ㅋㅋㅋ",
          "original_form": "내실은없어유ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100031635.1.15",
          "form": "저는오히려 기술이 더부러워용",
          "original_form": "저는오히려 기술이 더부러워용",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100031635.1.16",
          "form": "아니예요 진짜 멋져요",
          "original_form": "아니예요 진짜 멋져요",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031635.1.17",
          "form": "애니과세요..? 아님 디자인?",
          "original_form": "애니과세요..? 아님 디자인?",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031635.1.18",
          "form": "순수미술이에요ㅋㅋ",
          "original_form": "순수미술이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031635.1.19",
          "form": "회회과용",
          "original_form": "회회과용",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031635.1.20",
          "form": "우와 너무 멋지세요 진짜 ㅠㅠㅠ",
          "original_form": "우와 너무 멋지세요 진짜 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031635.1.21",
          "form": "그림 그리는게 보통일이 아니잖아요",
          "original_form": "그림 그리는게 보통일이 아니잖아요",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031635.1.22",
          "form": "물감 칠하는 것도 그렇고,,,",
          "original_form": "물감 칠하는 것도 그렇고,,,",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031635.1.23",
          "form": "4년동안 하다니,,진짜 존경",
          "original_form": "4년동안 하다니,,진짜 존경",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031635.1.24",
          "form": "ㅋㅋㅋ 재료값만 엄청 들었죠 머",
          "original_form": "ㅋㅋㅋ 재료값만 엄청 들었죠 머",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031635.1.25",
          "form": "졸업하고나서는 그림잘안그려용",
          "original_form": "졸업하고나서는 그림잘안그려용",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031635.1.26",
          "form": "정말요??ㅠㅠ",
          "original_form": "정말요??ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100031635.1.27",
          "form": "재능이 아까워요",
          "original_form": "재능이 아까워요",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100031635.1.28",
          "form": "투자한 시간이랑 노력도..ㅠ",
          "original_form": "투자한 시간이랑 노력도..ㅠ",
          "speaker_id": "1",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100031635.1.29",
          "form": "저는근데",
          "original_form": "저는근데",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031635.1.30",
          "form": "입시+대학때 그림을워낙많이그려서",
          "original_form": "입시+대학때 그림을워낙많이그려서",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031635.1.31",
          "form": "이제더 안그리고싶은느낌?",
          "original_form": "이제더 안그리고싶은느낌?",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031635.1.32",
          "form": "애초에 그리는걸좋아하는게 아니고 보는게 더 좋은타입이엇드라고요",
          "original_form": "애초에 그리는걸좋아하는게 아니고 보는게 더 좋은타입이엇드라고요",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031635.1.33",
          "form": "졸업하고알았지만ㅋㅋㅋ",
          "original_form": "졸업하고알았지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031635.1.34",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100031635.1.35",
          "form": "끝을 찍으셨군요",
          "original_form": "끝을 찍으셨군요",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031635.1.36",
          "form": "이것도 엄청 대단하세요",
          "original_form": "이것도 엄청 대단하세요",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031635.1.37",
          "form": "전시회 많이 가시나요??",
          "original_form": "전시회 많이 가시나요??",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031635.1.38",
          "form": "넹 자주가는편이에요!",
          "original_form": "넹 자주가는편이에요!",
          "speaker_id": "2",
          "time": "20210905 00:54"
        }
      ]
    }
