      "utterance": [
        {
          "id": "MDRW2100034796.1.1",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100034796.1.2",
          "form": "겨울빼고 다좋아요",
          "original_form": "겨울빼고 다좋아요",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100034796.1.3",
          "form": "겨울 너무 춥죠 ㅠㅠ",
          "original_form": "겨울 너무 춥죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034796.1.4",
          "form": "전 가을이 선선하니 좋더라구요",
          "original_form": "전 가을이 선선하니 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034796.1.5",
          "form": "가을이 왔네요 ㅎㅎ",
          "original_form": "가을이 왔네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034796.1.6",
          "form": "선선해요",
          "original_form": "선선해요",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034796.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034796.1.8",
          "form": "전 여름이 너무 싫더라구요 ㅠㅠ",
          "original_form": "전 여름이 너무 싫더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034796.1.9",
          "form": "땀도 나고 기운이 없어서요",
          "original_form": "땀도 나고 기운이 없어서요",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034796.1.10",
          "form": "여름은 그래도 낮이 길고",
          "original_form": "여름은 그래도 낮이 길고",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034796.1.11",
          "form": "빨래가 쉬워서좋아요",
          "original_form": "빨래가 쉬워서좋아요",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034796.1.12",
          "form": "아 빨래 잘 마르는건 좋아요",
          "original_form": "아 빨래 잘 마르는건 좋아요",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034796.1.13",
          "form": "장마 빼구요 ㅋㅋㅋ",
          "original_form": "장마 빼구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034796.1.14",
          "form": "장마는 힘들어요ㅜㅜㅜ",
          "original_form": "장마는 힘들어요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034796.1.15",
          "form": "맞아요 ㅜ",
          "original_form": "맞아요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034796.1.16",
          "form": "우산들기도 귀찮아요",
          "original_form": "우산들기도 귀찮아요",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034796.1.17",
          "form": "마자요ㅜ맞을수도없구",
          "original_form": "마자요ㅜ맞을수도없구",
          "speaker_id": "2",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034796.1.18",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034796.1.19",
          "form": "우산도 짐이에요",
          "original_form": "우산도 짐이에요",
          "speaker_id": "1",
          "time": "20210828 19:49"
        },
        {
          "id": "MDRW2100034796.1.20",
          "form": "전 양산겸 작은거 갖구다녀요",
          "original_form": "전 양산겸 작은거 갖구다녀요",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100034796.1.21",
          "form": "그런거도 좋겠어요!",
          "original_form": "그런거도 좋겠어요!",
          "speaker_id": "1",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100034796.1.22",
          "form": "양산겸용이면 편할거같아요",
          "original_form": "양산겸용이면 편할거같아요",
          "speaker_id": "1",
          "time": "20210828 19:51"
        }
      ]
