        {
          "id": "MDRW2100042942.1.1",
          "form": "요즘날씨에 딱이네용",
          "original_form": "요즘날씨에 딱이네용",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042942.1.2",
          "form": "진짜 가응이에영",
          "original_form": "진짜 가응이에영",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042942.1.3",
          "form": "네 긴팔 입어도 전혀 덥지 않아요",
          "original_form": "네 긴팔 입어도 전혀 덥지 않아요",
          "speaker_id": "1",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100042942.1.4",
          "form": "온종일 비가 오네요ㅜ",
          "original_form": "온종일 비가 오네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100042942.1.5",
          "form": "네그래도 이제 안찝찝하네요",
          "original_form": "네그래도 이제 안찝찝하네요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100042942.1.6",
          "form": "아 저흰 아직 습도가 높아요ㅜ",
          "original_form": "아 저흰 아직 습도가 높아요ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100042942.1.7",
          "form": "아항 ㅠㅠ 온돈 어떤가요",
          "original_form": "아항 ㅠㅠ 온돈 어떤가요",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100042942.1.8",
          "form": "살짝 쌀쌀합니다~",
          "original_form": "살짝 쌀쌀합니다~",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100042942.1.9",
          "form": "그래도 덥진 않아 좋지않나요 ㅋㅋ",
          "original_form": "그래도 덥진 않아 좋지않나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100042942.1.10",
          "form": "아 더워서 찝찝한 건 없어졌죠~",
          "original_form": "아 더워서 찝찝한 건 없어졌죠~",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100042942.1.11",
          "form": "그것만으로 조아요 ㅋㅋ",
          "original_form": "그것만으로 조아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100042942.1.12",
          "form": "더위와 땀에선 해방이죠ㅋㅋ",
          "original_form": "더위와 땀에선 해방이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100042942.1.13",
          "form": "빨리 맨투맨 입고싶어요",
          "original_form": "빨리 맨투맨 입고싶어요",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100042942.1.14",
          "form": "아 서늘한 날씨를 선호하시나봐요ㅎㅎ",
          "original_form": "아 서늘한 날씨를 선호하시나봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100042942.1.15",
