          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001878.1.4",
          "form": "강아지 vs 고양이!!",
          "original_form": "강아지 vs 고양이!!",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001878.1.5",
          "form": "전 댕댕이 파 입니다!!",
          "original_form": "전 댕댕이 파 입니다!!",
          "speaker_id": "2",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001878.1.6",
          "form": "둘다 좋지만.. ㅎ",
          "original_form": "둘다 좋지만.. ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001878.1.7",
          "form": "저도 둘다 좋은데 강아지 키워서 강아지가 더 좋은것같아요",
          "original_form": "저도 둘다 좋은데 강아지 키워서 강아지가 더 좋은것같아요",
          "speaker_id": "1",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001878.1.8",
          "form": "근데 털 되게 날리네요",
          "original_form": "근데 털 되게 날리네요",
          "speaker_id": "1",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001878.1.9",
          "form": "강아지 키우세요?",
          "original_form": "강아지 키우세요?",
          "speaker_id": "1",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001878.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001878.1.11",
          "form": "강아지 키우세요?!!!!",
          "original_form": "강아지 키우세요?!!!!",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001878.1.12",
          "form": "전 안키워요..ㅠㅠㅠ",
          "original_form": "전 안키워요..ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001878.1.13",
          "form": "강아지 부럽다..",
          "original_form": "강아지 부럽다..",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001878.1.14",
          "form": "종이 뭐에요..?",
          "original_form": "종이 뭐에요..?",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001878.1.15",
          "form": "시츄키워요",
          "original_form": "시츄키워요",
          "speaker_id": "1",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001878.1.16",
          "form": "키우시는 동물 있어요?",
          "original_form": "키우시는 동물 있어요?",
          "speaker_id": "1",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001878.1.17",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "2",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001878.1.18",
          "form": "아부지가 반대하세요..",
          "original_form": "아부지가 반대하세요..",
          "speaker_id": "2",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001878.1.19",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001878.1.20",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001878.1.21",
          "form": "폐가 안좋거나하면",
          "original_form": "폐가 안좋거나하면",
          "speaker_id": "1",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001878.1.22",
          "form": "그럴 수 있더라구요",
          "original_form": "그럴 수 있더라구요",
          "speaker_id": "1",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001878.1.23",
          "form": "털도 그렇고 이것저것때문에",
          "original_form": "털도 그렇고 이것저것때문에",
          "speaker_id": "1",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001878.1.24",
          "form": "와 귀엽다..",
          "original_form": "와 귀엽다..",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001878.1.25",
          "form": "시츄..",
          "original_form": "시츄..",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001878.1.26",
          "form": "이름이 뭐에요??",
          "original_form": "이름이 뭐에요??",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001878.1.27",
          "form": "name3이에요",
          "original_form": "&name3&이에요",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001878.1.28",
          "form": "저희 부모님이 지으셨어요",
          "original_form": "저희 부모님이 지으셨어요",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001878.1.29",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001878.1.30",
          "form": "귀엽다아!!!",
          "original_form": "귀엽다아!!!",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001878.1.31",
          "form": "저도 고모가 강아지 키우는데",
          "original_form": "저도 고모가 강아지 키우는데",
          "speaker_id": "2",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001878.1.32",
          "form": "명절이면 데려오는데",
          "original_form": "명절이면 데려오는데",
          "speaker_id": "2",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001878.1.33",
          "form": "너무 귀엽더라구요",
          "original_form": "너무 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001878.1.34",
          "form": "근데 좀 짖어서 무섭긴 한데..",
          "original_form": "근데 좀 짖어서 무섭긴 한데..",
          "speaker_id": "2",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001878.1.35",
          "form": "제 친구도 저희집 오면 강아지 되게 무서워 하더라구요...",
          "original_form": "제 친구도 저희집 오면 강아지 되게 무서워 하더라구요...",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001878.1.36",
          "form": "근데 지금은 친하게 지내요 ㅎㅎ",
          "original_form": "근데 지금은 친하게 지내요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001878.1.37",
          "form": "으아.. 저도 친구중에 강아치 키우는 친구있으면",
          "original_form": "으아.. 저도 친구중에 강아치 키우는 친구있으면",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001878.1.38",
          "form": "맨날 갈텐데",
          "original_form": "맨날 갈텐데",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001878.1.39",
          "form": "확실히 강아지나 고양이가 귀여워서..",
          "original_form": "확실히 강아지나 고양이가 귀여워서..",
          "speaker_id": "1",
          "time": "20210819 22:10"
        }
      ]
