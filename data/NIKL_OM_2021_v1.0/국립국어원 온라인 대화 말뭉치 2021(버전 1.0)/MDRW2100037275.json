        {
          "id": "MDRW2100037275.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037275.1.3",
          "form": "태국요리 좋아하나요?",
          "original_form": "태국요리 좋아하나요?",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037275.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037275.1.5",
          "form": "팟씨유 누들은 아시는지?",
          "original_form": "팟씨유 누들은 아시는지?",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037275.1.6",
          "form": "내.",
          "original_form": "내.",
          "speaker_id": "1",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037275.1.7",
          "form": "그런데요??",
          "original_form": "그런데요??",
          "speaker_id": "1",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037275.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:21"
        },
        {
          "id": "MDRW2100037275.1.9",
          "form": "드셔보셨는지여?",
          "original_form": "드셔보셨는지여?",
          "speaker_id": "2",
          "time": "20210831 22:21"
        },
        {
          "id": "MDRW2100037275.1.10",
          "form": "안먹어봣지만.\n맛잇대서 제취향",
          "original_form": "안먹어봣지만.\n맛잇대서 제취향",
          "speaker_id": "1",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100037275.1.11",
          "form": "임돳",
          "original_form": "임돳",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100037275.1.12",
          "form": "만들어먹으실 계획은 잇나요?",
          "original_form": "만들어먹으실 계획은 잇나요?",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037275.1.13",
          "form": "엄마가 어디 나가몈 할려고욥",
          "original_form": "엄마가 어디 나가몈 할려고욥",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037275.1.14",
          "form": "ㅋㅋㅋㅋ\n팟씨유 드시고싶으신가요?",
          "original_form": "ㅋㅋㅋㅋ\n팟씨유 드시고싶으신가요?",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100037275.1.15",
          "form": "네 맨날 눈앞에 있내요",
          "original_form": "네 맨날 눈앞에 있내요",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100037275.1.16",
          "form": "이런.. 꿈에서 드세요... 이제 느므느므 늦엇어욥",
          "original_form": "이런.. 꿈에서 드세요... 이제 느므느므 늦엇어욥",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037275.1.17",
          "form": "지금 먹을생각은 없어요...",
          "original_form": "지금 먹을생각은 없어요...",
          "speaker_id": "2",
          "time": "20210831 22:56"
        }
