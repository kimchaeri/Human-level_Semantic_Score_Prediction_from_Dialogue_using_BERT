      "utterance": [
        {
          "id": "MDRW2100043103.1.1",
          "form": "그니까..... 사람이 실수할 수도 있지 급하다고 막 하고 그럼 안된다",
          "original_form": "그니까..... 사람이 실수할 수도 있지 급하다고 막 하고 그럼 안된다",
          "speaker_id": "2",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043103.1.2",
          "form": "결제창 뜨지도 않았는데 카드 꽂았다가 빼는 사람들도 은근 많어 ㅋㅋㅋ....",
          "original_form": "결제창 뜨지도 않았는데 카드 꽂았다가 빼는 사람들도 은근 많어 ㅋㅋㅋ....",
          "speaker_id": "2",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043103.1.3",
          "form": "컴이 느리다구",
          "original_form": "컴이 느리다구",
          "speaker_id": "2",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043103.1.4",
          "form": "마자 공공기관이 컴 갖다놔봤자",
          "original_form": "마자 공공기관이 컴 갖다놔봤자",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043103.1.5",
          "form": "얼마나 새거겟냐",
          "original_form": "얼마나 새거겟냐",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043103.1.6",
          "form": "기다려려",
          "original_form": "기다려려",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043103.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 마자마자",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 마자마자",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043103.1.8",
          "form": "결국 껐다 켰다 지금도",
          "original_form": "결국 껐다 켰다 지금도",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043103.1.9",
          "form": "애가 맛이 자주가...",
          "original_form": "애가 맛이 자주가...",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043103.1.10",
          "form": "지금은 좀 사람 빠졌어?",
          "original_form": "지금은 좀 사람 빠졌어?",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043103.1.11",
          "form": "몰려올땐 엄청 오고 아닐땐 다른 일 하다가",
          "original_form": "몰려올땐 엄청 오고 아닐땐 다른 일 하다가",
          "speaker_id": "2",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043103.1.12",
          "form": "시간 남으면 무인 등록기에 입렫된거 좀 쳐내고",
          "original_form": "시간 남으면 무인 등록기에 입렫된거 좀 쳐내고",
          "speaker_id": "2",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043103.1.13",
          "form": "지금에야 좀 시간 났엉",
          "original_form": "지금에야 좀 시간 났엉",
          "speaker_id": "2",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100043103.1.14",
          "form": "바빴구나...",
          "original_form": "바빴구나...",
          "speaker_id": "1",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043103.1.15",
          "form": "바빴지.. 오늘은 마지막 30분은 시간 남아서",
          "original_form": "바빴지.. 오늘은 마지막 30분은 시간 남아서",
          "speaker_id": "2",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100043103.1.16",
          "form": "멍좀 때렸어",
          "original_form": "멍좀 때렸어",
          "speaker_id": "2",
          "time": "20210907 18:45"
        },
        {
          "id": "MDRW2100043103.1.17",
          "form": "그리고 난 퇴근하는 길이지",
          "original_form": "그리고 난 퇴근하는 길이지",
          "speaker_id": "2",
          "time": "20210907 18:45"
        },
        {
          "id": "MDRW2100043103.1.18",
          "form": "ㅋㅋㅋㅋㅋ 퇴근 축하해",
          "original_form": "ㅋㅋㅋㅋㅋ 퇴근 축하해",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043103.1.19",
          "form": "이제 집 도착했어?",
          "original_form": "이제 집 도착했어?",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043103.1.20",
          "form": "ㅋㅋㅋ ㅇㅇ 집 도착했어",
          "original_form": "ㅋㅋㅋ ㅇㅇ 집 도착했어",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043103.1.21",
          "form": "그랭...",
          "original_form": "그랭...",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043103.1.22",
          "form": "밥도 먹었지...",
          "original_form": "밥도 먹었지...",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043103.1.23",
          "form": "내일 출근을 위해 푹 쉬렴",
          "original_form": "내일 출근을 위해 푹 쉬렴",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043103.1.24",
          "form": "이제 내일만 나가면 목요일날 쉰다 크으",
          "original_form": "이제 내일만 나가면 목요일날 쉰다 크으",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043103.1.25",
          "form": "아 목욜?",
          "original_form": "아 목욜?",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043103.1.26",
          "form": "아 백신?",
          "original_form": "아 백신?",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043103.1.27",
          "form": "ㅇㅇ 백신!",
          "original_form": "ㅇㅇ 백신!",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043103.1.28",
          "form": "우리회사는",
          "original_form": "우리회사는",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043103.1.29",
          "form": "맞은 사람들 다 다음날 병가 냈어",
          "original_form": "맞은 사람들 다 다음날 병가 냈어",
