      "utterance": [
        {
          "id": "MDRW2100020105.1.1",
          "form": "치킨좋아하세요",
          "original_form": "치킨좋아하세요",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020105.1.2",
          "form": "넹 근데 피자를 더 좋아해요 ㅎㅎㅎ",
          "original_form": "넹 근데 피자를 더 좋아해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020105.1.3",
          "form": "저도 피자를 더 좋아하긴해요",
          "original_form": "저도 피자를 더 좋아하긴해요",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020105.1.4",
          "form": "피자는 완전식품이니까",
          "original_form": "피자는 완전식품이니까",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020105.1.5",
          "form": "입안에서 펼쳐지는 페스티벌같은 존재라구요",
          "original_form": "입안에서 펼쳐지는 페스티벌같은 존재라구요",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020105.1.6",
          "form": "먹잘알",
          "original_form": "먹잘알",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020105.1.7",
          "form": "표현력 만렙",
          "original_form": "표현력 만렙",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020105.1.8",
          "form": "하버드 출신인가요?",
          "original_form": "하버드 출신인가요?",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020105.1.9",
          "form": "아니요 먹잘대 출신입니다ㅎ",
          "original_form": "아니요 먹잘대 출신입니다ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020105.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020105.1.11",
          "form": "먹(는걸) 잘(하는) 대(학교)",
          "original_form": "먹(는걸) 잘(하는) 대(학교)",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020105.1.12",
          "form": "라면에 계란 푼다 안푼다",
          "original_form": "라면에 계란 푼다 안푼다",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020105.1.13",
          "form": "아 약간 멀어질거같은데",
          "original_form": "아 약간 멀어질거같은데",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020105.1.14",
          "form": "저 푸는거좋아해요 약간 탁해지는 맛이 좋아서",
          "original_form": "저 푸는거좋아해요 약간 탁해지는 맛이 좋아서",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020105.1.15",
          "form": "대박 저도요",
          "original_form": "대박 저도요",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020105.1.16",
          "form": "근데 풀어서 넣는거 별로고",
          "original_form": "근데 풀어서 넣는거 별로고",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020105.1.17",
          "form": "넣어서 휘저어야해요",
          "original_form": "넣어서 휘저어야해요",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020105.1.18",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020105.1.19",
          "form": "그래야 약간 흰자와  노른자가 적절히",
          "original_form": "그래야 약간 흰자와  노른자가 적절히",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020105.1.20",
          "form": "된다구요 역시 가방끈이 기십니다",
          "original_form": "된다구요 역시 가방끈이 기십니다",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020105.1.21",
          "form": "맞아요!!! 이거 맞는사람 진짜 처음봣어요ㅠㅠㅠㅠ",
          "original_form": "맞아요!!! 이거 맞는사람 진짜 처음봣어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020105.1.22",
          "form": "감동이네요",
          "original_form": "감동이네요",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020105.1.23",
          "form": "우리 약간 더 가까워졌습니다",
          "original_form": "우리 약간 더 가까워졌습니다",
          "speaker_id": "1",
