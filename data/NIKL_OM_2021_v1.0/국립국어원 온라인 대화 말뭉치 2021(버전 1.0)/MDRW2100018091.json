      "utterance": [
        {
          "id": "MDRW2100018091.1.1",
          "form": "저 요즘 완전",
          "original_form": "저 요즘 완전",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018091.1.2",
          "form": "커뮤니티중독자에요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "커뮤니티중독자에요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018091.1.3",
          "form": "맘카페...",
          "original_form": "맘카페...",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018091.1.4",
          "form": "맨날 침대에 누워서 맘카페 글만 봐도 시간이 순삭",
          "original_form": "맨날 침대에 누워서 맘카페 글만 봐도 시간이 순삭",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018091.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 전 뭔가 그렇게 주기적으로? 해야하는 끈기같은게 없어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 전 뭔가 그렇게 주기적으로? 해야하는 끈기같은게 없어서",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018091.1.6",
          "form": "예전에 아이돌 좋아했을 때 잠깐 팬카페? 정도 했아요",
          "original_form": "예전에 아이돌 좋아했을 때 잠깐 팬카페? 정도 했아요",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018091.1.7",
          "form": "앜ㅋㅋㅋㅋㅋ 저도 팬클럽 가입한적 있네요",
          "original_form": "앜ㅋㅋㅋㅋㅋ 저도 팬클럽 가입한적 있네요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018091.1.8",
          "form": "맘카페에 은근 쏠쏠한 정보들이 많죠",
          "original_form": "맘카페에 은근 쏠쏠한 정보들이 많죠",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018091.1.9",
          "form": "박효신 ...ㅋㅋㅋ 아이돌은아니에요",
          "original_form": "박효신 ...ㅋㅋㅋ 아이돌은아니에요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018091.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 전.. 비스트.. 라고... ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 전.. 비스트.. 라고... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018091.1.11",
          "form": "동네 맛집 병원 ㅋㅋㅋㅋㅋ온갖정보",
          "original_form": "동네 맛집 병원 ㅋㅋㅋㅋㅋ온갖정보",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018091.1.12",
          "form": "저 두준이 좋아했어요",
          "original_form": "저 두준이 좋아했어요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018091.1.13",
          "form": "두준두준",
          "original_form": "두준두준",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018091.1.14",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 저도 엄청 열심히 팬활동 햏엏는데",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 저도 엄청 열심히 팬활동 햏엏는데",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018091.1.15",
          "form": "최근엔 알고리즘에빠져서",
          "original_form": "최근엔 알고리즘에빠져서",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
