      "utterance": [
        {
          "id": "MDRW2100014684.1.1",
          "form": "재택근무 하신적 있나요?",
          "original_form": "재택근무 하신적 있나요?",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014684.1.2",
          "form": "아뇨 저는 재택근무는 해 본적이 없어요",
          "original_form": "아뇨 저는 재택근무는 해 본적이 없어요",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014684.1.3",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014684.1.4",
          "form": "근데 거래처가 재택근무를 하니까 불편하더라구요",
          "original_form": "근데 거래처가 재택근무를 하니까 불편하더라구요",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014684.1.5",
          "form": "아 요즘 재택근무 많이들 하시죠",
          "original_form": "아 요즘 재택근무 많이들 하시죠",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014684.1.6",
          "form": "그럼 미팅도 화상으로 하겠네요",
          "original_form": "그럼 미팅도 화상으로 하겠네요",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014684.1.7",
          "form": "아뇨 통화만...",
          "original_form": "아뇨 통화만...",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014684.1.8",
          "form": "그게 회사가 아니다 보니까 연락이 잘 안되더라구요",
          "original_form": "그게 회사가 아니다 보니까 연락이 잘 안되더라구요",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014684.1.9",
          "form": "아 답답하시겠어요",
          "original_form": "아 답답하시겠어요",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014684.1.10",
          "form": "네 자료도 그렇고",
          "original_form": "네 자료도 그렇고",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014684.1.11",
          "form": "아님 나도 재택하면 이해라도 할텐데",
          "original_form": "아님 나도 재택하면 이해라도 할텐데",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014684.1.12",
          "form": "아 그럼 일 진행도 느려져서 힘드시겠어요",
          "original_form": "아 그럼 일 진행도 느려져서 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014684.1.13",
          "form": "재택근무 당사자는 편할지 몰라도 애로사항이 많네요",
          "original_form": "재택근무 당사자는 편할지 몰라도 애로사항이 많네요",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014684.1.14",
          "form": "네 그래도 통근시간이 줄어서 하는 사람들은 좋겠어요",
          "original_form": "네 그래도 통근시간이 줄어서 하는 사람들은 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014684.1.15",
          "form": "네 그쵸 아무래도",
          "original_form": "네 그쵸 아무래도",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014684.1.16",
          "form": "통근이 제일 지옥인데ㅜㅜ",
          "original_form": "통근이 제일 지옥인데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014684.1.17",
          "form": "일찍일어나는거 너무 싫어요",
          "original_form": "일찍일어나는거 너무 싫어요",
          "speaker_id": "1",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014684.1.18",
          "form": "맞아요ㅜㅜ 재택근무하면 출근시간 십분 전에만 일어나도 될텐데ㅋㅋ",
          "original_form": "맞아요ㅜㅜ 재택근무하면 출근시간 십분 전에만 일어나도 될텐데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014684.1.19",
          "form": "상의만 갈아입고 마스크도 안쓰고",
          "original_form": "상의만 갈아입고 마스크도 안쓰고",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014684.1.20",
          "form": "name1님도 기회가 되면 재택근무 하고싶으신가요?",
          "original_form": "&name1&님도 기회가 되면 재택근무 하고싶으신가요?",
