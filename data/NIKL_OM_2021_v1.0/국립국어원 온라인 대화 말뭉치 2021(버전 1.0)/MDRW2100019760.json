      "utterance": [
        {
          "id": "MDRW2100019760.1.1",
          "form": "배달시키세요ㅜㅜ",
          "original_form": "배달시키세요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019760.1.2",
          "form": "너무 비싸더라구요",
          "original_form": "너무 비싸더라구요",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019760.1.3",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019760.1.4",
          "form": "내일 과자 사먹으려구요",
          "original_form": "내일 과자 사먹으려구요",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100019760.1.5",
          "form": "아이고ㅜㅜ 내일 외출해서 사드세요",
          "original_form": "아이고ㅜㅜ 내일 외출해서 사드세요",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019760.1.6",
          "form": "그럴까요...",
          "original_form": "그럴까요...",
          "speaker_id": "1",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019760.1.7",
          "form": "먹고싶으면 먹어야죠",
          "original_form": "먹고싶으면 먹어야죠",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019760.1.8",
          "form": "제가 아토피가있어서 치료중이에요",
          "original_form": "제가 아토피가있어서 치료중이에요",
          "speaker_id": "1",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019760.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019760.1.10",
          "form": "그럼 좀 참으셔야겠네요",
          "original_form": "그럼 좀 참으셔야겠네요",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019760.1.11",
          "form": "네ㅜㅜ  병원비로 엄청 썼거든요",
          "original_form": "네ㅜㅜ  병원비로 엄청 썼거든요",
          "speaker_id": "1",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100019760.1.12",
          "form": "그럼 참으시는 게 낫겠어요",
          "original_form": "그럼 참으시는 게 낫겠어요",
          "speaker_id": "2",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100019760.1.13",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019760.1.14",
          "form": "뚱카롱 말고 비건 디저트는 어때요?",
          "original_form": "뚱카롱 말고 비건 디저트는 어때요?",
          "speaker_id": "2",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019760.1.15",
          "form": "가격대가 다 쎄서ㅜㅜ",
          "original_form": "가격대가 다 쎄서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019760.1.16",
          "form": "잘 케어하고있는 나에게 셀프 선물인거죠",
          "original_form": "잘 케어하고있는 나에게 셀프 선물인거죠",
