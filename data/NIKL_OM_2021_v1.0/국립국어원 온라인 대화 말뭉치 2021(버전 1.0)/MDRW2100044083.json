      "utterance": [
        {
          "id": "MDRW2100044083.1.1",
          "form": "엉 혜은이 남편",
          "original_form": "엉 혜은이 남편",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044083.1.2",
          "form": "더한 제목은 머예요?",
          "original_form": "더한 제목은 머예요?",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044083.1.3",
          "form": "눈짓에서 몸짓까지 ㅋㅋ",
          "original_form": "눈짓에서 몸짓까지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044083.1.4",
          "form": "ㅋㅋ 제목이 선정적이다",
          "original_form": "ㅋㅋ 제목이 선정적이다",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044083.1.5",
          "form": "다시 상대역 이영하",
          "original_form": "다시 상대역 이영하",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044083.1.6",
          "form": "이영하 대박으로 나왔네",
          "original_form": "이영하 대박으로 나왔네",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044083.1.7",
          "form": "그리고 연인들의 이야기",
          "original_form": "그리고 연인들의 이야기",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044083.1.8",
          "form": "이건 제목이 정상적이네",
          "original_form": "이건 제목이 정상적이네",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044083.1.9",
          "form": "여긴 상대역이 송승환",
          "original_form": "여긴 상대역이 송승환",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044083.1.10",
          "form": "아 송승환ㅋㅋ 난타",
          "original_form": "아 송승환ㅋㅋ 난타",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044083.1.11",
          "form": "맞어 그리고 안나오네",
          "original_form": "맞어 그리고 안나오네",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044083.1.12",
          "form": "공백기 들어가네요",
          "original_form": "공백기 들어가네요",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044083.1.13",
          "form": "감독한다고 안하나보다",
          "original_form": "감독한다고 안하나보다",
          "speaker_id": "2",
          "time": "20210907 21:33"
