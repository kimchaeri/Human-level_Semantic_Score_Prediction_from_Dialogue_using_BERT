      "utterance": [
        {
          "id": "MDRW2100031875.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210905 06:48"
        },
        {
          "id": "MDRW2100031875.1.2",
          "form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "original_form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "speaker_id": "1",
          "time": "20210905 06:48"
        },
        {
          "id": "MDRW2100031875.1.3",
          "form": "가을이오면 어느 것이 가장 기대되시나요?",
          "original_form": "가을이오면 어느 것이 가장 기대되시나요?",
          "speaker_id": "1",
          "time": "20210905 06:48"
        },
        {
          "id": "MDRW2100031875.1.4",
          "form": "가을이면 뭐니 뭐니 해도 단풍놀이 아니겠습니까",
          "original_form": "가을이면 뭐니 뭐니 해도 단풍놀이 아니겠습니까",
          "speaker_id": "2",
          "time": "20210905 06:49"
        },
        {
          "id": "MDRW2100031875.1.5",
          "form": "설악산 줄기 따라 볼게 물들 볼게 물든 단풍잎을 보고 싶군요",
          "original_form": "설악산 줄기 따라 볼게 물들 볼게 물든 단풍잎을 보고 싶군요",
          "speaker_id": "2",
          "time": "20210905 06:50"
        },
        {
          "id": "MDRW2100031875.1.6",
          "form": "가을 정말 좋은 계절인 거 같습니다",
          "original_form": "가을 정말 좋은 계절인 거 같습니다",
          "speaker_id": "2",
          "time": "20210905 06:50"
        },
        {
          "id": "MDRW2100031875.1.7",
          "form": "설악산 좋죠",
          "original_form": "설악산 좋죠",
          "speaker_id": "1",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031875.1.8",
          "form": "가을은 풍성한 느낌이 들어서 좋습니다",
          "original_form": "가을은 풍성한 느낌이 들어서 좋습니다",
          "speaker_id": "1",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031875.1.9",
          "form": "먹거리도 풍성하고",
          "original_form": "먹거리도 풍성하고",
          "speaker_id": "1",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031875.1.10",
          "form": "나무들의 색도 다양하니 풍성하고",
          "original_form": "나무들의 색도 다양하니 풍성하고",
          "speaker_id": "1",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031875.1.11",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "2",
          "time": "20210905 07:29"
        },
        {
          "id": "MDRW2100031875.1.12",
          "form": "가을 먹거리",
          "original_form": "가을 먹거리",
          "speaker_id": "2",
          "time": "20210905 07:30"
        },
        {
          "id": "MDRW2100031875.1.13",
          "form": "설악산 앞에 있는 두부집에서 막걸리랑 한잔 먹고 싶네요",
          "original_form": "설악산 앞에 있는 두부집에서 막걸리랑 한잔 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210905 07:30"
        },
        {
          "id": "MDRW2100031875.1.14",
          "form": "설악산은 자주 가는 편이십니까?",
          "original_form": "설악산은 자주 가는 편이십니까?",
          "speaker_id": "1",
          "time": "20210905 07:30"
        },
        {
          "id": "MDRW2100031875.1.15",
          "form": "근처 사시나요?",
          "original_form": "근처 사시나요?",
          "speaker_id": "1",
          "time": "20210905 07:30"
        },
        {
          "id": "MDRW2100031875.1.16",
          "form": "아니요 한 번도 안 가봤습니다",
          "original_form": "아니요 한 번도 안 가봤습니다",
          "speaker_id": "2",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100031875.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031875.1.18",
          "form": "잘 아시는 듯 해서요",
          "original_form": "잘 아시는 듯 해서요",
          "speaker_id": "1",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031875.1.19",
          "form": "한국은 산이 많아서",
          "original_form": "한국은 산이 많아서",
          "speaker_id": "1",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031875.1.20",
          "form": "사계절 다양하게 즐길 수 있어 좋습니다",
          "original_form": "사계절 다양하게 즐길 수 있어 좋습니다",
          "speaker_id": "1",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031875.1.21",
          "form": "맞습니다 맞습니다",
          "original_form": "맞습니다 맞습니다",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031875.1.22",
          "form": "설악산 고등학교 때 가본 거 같습니다",
          "original_form": "설악산 고등학교 때 가본 거 같습니다",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031875.1.23",
          "form": "아 수학여행을 거기로 갔네요",
          "original_form": "아 수학여행을 거기로 갔네요",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031875.1.24",
          "form": "한국은 산이 많은 거 같습니다",
          "original_form": "한국은 산이 많은 거 같습니다",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031875.1.25",
          "form": "계절별로 가봐야 할 산이 많습니다",
          "original_form": "계절별로 가봐야 할 산이 많습니다",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031875.1.26",
          "form": "오늘 날씨 좋네요",
          "original_form": "오늘 날씨 좋네요",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031875.1.27",
          "form": "근처에 산이 있습니까",
          "original_form": "근처에 산이 있습니까",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031875.1.28",
          "form": "여긴 있지요",
          "original_form": "여긴 있지요",
          "speaker_id": "1",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031875.1.29",
          "form": "한국은 어딜가나 산이라서요",
          "original_form": "한국은 어딜가나 산이라서요",
          "speaker_id": "1",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031875.1.30",
          "form": "등산하기 좋은 산이라 종종 갔었습니다",
          "original_form": "등산하기 좋은 산이라 종종 갔었습니다",
          "speaker_id": "1",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031875.1.31",
          "form": "요즘은 코로나라서 자제하고 있지요",
          "original_form": "요즘은 코로나라서 자제하고 있지요",
          "speaker_id": "1",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031875.1.32",
          "form": "여기에  비슬 산이 있는데",
          "original_form": "여기에  비슬 산이 있는데",
          "speaker_id": "2",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031875.1.33",
          "form": "한번씩 올라갑니다",
          "original_form": "한번씩 올라갑니다",
          "speaker_id": "2",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100031875.1.34",
          "form": "오를 산이 근처에 있다는 건 참 복인듯 싶습니다",
          "original_form": "오를 산이 근처에 있다는 건 참 복인듯 싶습니다",
          "speaker_id": "1",
          "time": "20210905 08:13"
        },
        {
          "id": "MDRW2100031875.1.35",
          "form": "산을 타면 피톤치드도 실컷 마실 수 있고",
          "original_form": "산을 타면 피톤치드도 실컷 마실 수 있고",
          "speaker_id": "1",
          "time": "20210905 08:14"
        },
        {
          "id": "MDRW2100031875.1.36",
          "form": "좋은 풍경도 볼 수 있지요",
          "original_form": "좋은 풍경도 볼 수 있지요",
          "speaker_id": "1",
          "time": "20210905 08:14"
        },
        {
          "id": "MDRW2100031875.1.37",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 08:15"
        },
        {
          "id": "MDRW2100031875.1.38",
          "form": "땀흘리고 막걸리한잔",
          "original_form": "땀흘리고 막걸리한잔",
          "speaker_id": "2",
          "time": "20210905 08:15"
        },
        {
          "id": "MDRW2100031875.1.39",
          "form": "시원한 느낌",
          "original_form": "시원한 느낌",
          "speaker_id": "2",
          "time": "20210905 08:15"
        },
        {
          "id": "MDRW2100031875.1.40",
          "form": "상쾌한바람",
          "original_form": "상쾌한바람",
          "speaker_id": "2",
          "time": "20210905 08:15"
        },
        {
          "id": "MDRW2100031875.1.41",
          "form": "좋지요",
          "original_form": "좋지요",
