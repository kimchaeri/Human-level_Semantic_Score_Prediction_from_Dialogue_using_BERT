      "utterance": [
        {
          "id": "MDRW2100030509.1.1",
          "form": "저는 사실 아이돌 무지한이었는데",
          "original_form": "저는 사실 아이돌 무지한이었는데",
          "speaker_id": "2",
          "time": "20210904 02:27"
        },
        {
          "id": "MDRW2100030509.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.3",
          "form": "제가 자꾸... 아까 일본부터 ㅋㅋㅋㅋㅋ",
          "original_form": "제가 자꾸... 아까 일본부터 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.4",
          "form": "오마이걸이 너무좋아져서",
          "original_form": "오마이걸이 너무좋아져서",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.6",
          "form": "일본 -> 선바 -> 에스파",
          "original_form": "일본 -> 선바 -> 에스파",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.7",
          "form": "name2님을",
          "original_form": "&name2&님을",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.8",
          "form": "당황시키는군욬ㅋㅋㅋㅋ앜ㅋㅋ",
          "original_form": "당황시키는군욬ㅋㅋㅋㅋ앜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.9",
          "form": "오오 오마이걸!!!",
          "original_form": "오오 오마이걸!!!",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.11",
          "form": "당황하는거 좋아요(?)",
          "original_form": "당황하는거 좋아요(?)",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.12",
          "form": "오마이걸을 계기로 케이팝에 눈을떴어요",
          "original_form": "오마이걸을 계기로 케이팝에 눈을떴어요",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.13",
          "form": "그래서 에스파노래도듣고..",
          "original_form": "그래서 에스파노래도듣고..",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030509.1.14",
          "form": "사실 그래도 아직 대부분의 아이돌을 모르긴해요",
          "original_form": "사실 그래도 아직 대부분의 아이돌을 모르긴해요",
          "speaker_id": "2",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100030509.1.15",
          "form": "오!!!!",
          "original_form": "오!!!!",
          "speaker_id": "1",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100030509.1.16",
          "form": "아진짜요?! 우와 근데 그럴 수 있어요",
          "original_form": "아진짜요?! 우와 근데 그럴 수 있어요",
          "speaker_id": "1",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100030509.1.17",
          "form": "저도 드라마에 문외한이거든요...",
          "original_form": "저도 드라마에 문외한이거든요...",
          "speaker_id": "1",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100030509.1.18",
          "form": "근데 또 제가 파는 것만 파섴ㅋㅋㅋㅋ",
          "original_form": "근데 또 제가 파는 것만 파섴ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100030509.1.19",
          "form": "오마이걸 누가 가장 좋으세요?!",
          "original_form": "오마이걸 누가 가장 좋으세요?!",
          "speaker_id": "1",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100030509.1.20",
          "form": "앟 저도 드라마는 아직 문외한이에요",
          "original_form": "앟 저도 드라마는 아직 문외한이에요",
          "speaker_id": "2",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100030509.1.21",
          "form": "저 미미요ㅜㅜ",
          "original_form": "저 미미요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100030509.1.22",
          "form": "저도 잘은 모르는데 유아가 너무 예쁘더라구요",
          "original_form": "저도 잘은 모르는데 유아가 너무 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100030509.1.23",
          "form": "너무 매력있던데요",
          "original_form": "너무 매력있던데요",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100030509.1.24",
          "form": "오 미미 보고 올게요",
          "original_form": "오 미미 보고 올게요",
          "speaker_id": "1",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100030509.1.25",
          "form": "우왕맞죠",
          "original_form": "우왕맞죠",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100030509.1.26",
          "form": "저도 유아때문에 오마이걸 알게됐어요",
          "original_form": "저도 유아때문에 오마이걸 알게됐어요",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100030509.1.27",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100030509.1.28",
          "form": "미미란 분은 되게 천의 얼굴이시네요",
          "original_form": "미미란 분은 되게 천의 얼굴이시네요",
          "speaker_id": "1",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100030509.1.29",
          "form": "앟 그런가요??",
          "original_form": "앟 그런가요??",
          "speaker_id": "2",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100030509.1.30",
          "form": "오마이걸 노래 좋은거 많아용",
          "original_form": "오마이걸 노래 좋은거 많아용",
          "speaker_id": "2",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100030509.1.31",
          "form": "네네 표정이랑 스타일에 따라서",
          "original_form": "네네 표정이랑 스타일에 따라서",
          "speaker_id": "1",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100030509.1.32",
          "form": "혹시 안들어보셨다면..",
          "original_form": "혹시 안들어보셨다면..",
          "speaker_id": "2",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100030509.1.33",
          "form": "되게 달라보여요!!!",
          "original_form": "되게 달라보여요!!!",
          "speaker_id": "1",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100030509.1.34",
          "form": "네네 추천해주세요!!",
          "original_form": "네네 추천해주세요!!",
          "speaker_id": "1",
          "time": "20210904 02:31"
        },
        {
          "id": "MDRW2100030509.1.35",
          "form": "돌핀은 알아요!!!",
          "original_form": "돌핀은 알아요!!!",
          "speaker_id": "1",
          "time": "20210904 02:31"
        }
      ]
    }
