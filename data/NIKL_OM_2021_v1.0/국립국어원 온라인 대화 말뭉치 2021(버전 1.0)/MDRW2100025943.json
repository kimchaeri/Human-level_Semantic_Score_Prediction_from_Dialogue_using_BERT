        },
        {
          "id": "MDRW2100025943.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025943.1.3",
          "form": "오늘 날씨가... 흐리네요....",
          "original_form": "오늘 날씨가... 흐리네요....",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025943.1.4",
          "form": "이짝은 오늘... 비가 안왔어요....",
          "original_form": "이짝은 오늘... 비가 안왔어요....",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025943.1.5",
          "form": "여기는 아침에 비가오고 축축하고 눅눅해요",
          "original_form": "여기는 아침에 비가오고 축축하고 눅눅해요",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025943.1.6",
          "form": "에어컨 틀었음",
          "original_form": "에어컨 틀었음",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025943.1.7",
          "form": "벌써 9월인데",
          "original_form": "벌써 9월인데",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025943.1.8",
          "form": "9월인데 더워요",
          "original_form": "9월인데 더워요",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025943.1.9",
          "form": "저희는... 난방틀었는데ㅋㅋㅋㅋ",
          "original_form": "저희는... 난방틀었는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025943.1.10",
          "form": "어머니가... 추위를 많이타셔서... 저는쪄죽어요",
          "original_form": "어머니가... 추위를 많이타셔서... 저는쪄죽어요",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025943.1.11",
          "form": "아니? ㅋㅋㅋㅋ 추위를.많이",
          "original_form": "아니? ㅋㅋㅋㅋ 추위를.많이",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025943.1.12",
          "form": "아하 어머님이 많이 타시는군요",
          "original_form": "아하 어머님이 많이 타시는군요",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025943.1.13",
          "form": "저는 이제 땀을 넘 흘려서",
          "original_form": "저는 이제 땀을 넘 흘려서",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025943.1.14",
          "form": "찬물샤워 한번 하려고요",
          "original_form": "찬물샤워 한번 하려고요",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025943.1.15",
          "form": "등목하러 가야하는데",
          "original_form": "등목하러 가야하는데",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025943.1.16",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025943.1.17",
          "form": "샤워한번해야겠어요",
          "original_form": "샤워한번해야겠어요",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025943.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
