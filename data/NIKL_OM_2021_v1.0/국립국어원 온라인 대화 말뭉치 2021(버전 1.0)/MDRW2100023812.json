      "utterance": [
        {
          "id": "MDRW2100023812.1.1",
          "form": "네 그 송곳니 옆에 있는",
          "original_form": "네 그 송곳니 옆에 있는",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023812.1.2",
          "form": "작은 어금니요",
          "original_form": "작은 어금니요",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023812.1.3",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023812.1.4",
          "form": "마취해서 그걸 걍 한번에 뽑는대요",
          "original_form": "마취해서 그걸 걍 한번에 뽑는대요",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023812.1.5",
          "form": "저는 안쪽에 있는걸 뽑는줄 알았어요",
          "original_form": "저는 안쪽에 있는걸 뽑는줄 알았어요",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023812.1.6",
          "form": "그건 정말 더 무섭네요",
          "original_form": "그건 정말 더 무섭네요",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100023812.1.7",
          "form": "사랑니 뽑아본 적 있으세요?",
          "original_form": "사랑니 뽑아본 적 있으세요?",
          "speaker_id": "2",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100023812.1.8",
          "form": "저는 사랑니는 없어요! name2은 있으신가요",
          "original_form": "저는 사랑니는 없어요! &name2&은 있으신가요",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100023812.1.9",
          "form": "저는 아래 두 개를 뺐어요",
          "original_form": "저는 아래 두 개를 뺐어요",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100023812.1.10",
          "form": "헐 안아프셨나요",
          "original_form": "헐 안아프셨나요",
          "speaker_id": "1",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100023812.1.11",
          "form": "저는 많이 나온거를  뽑아서 그런지 아프지는 않더라구요",
          "original_form": "저는 많이 나온거를  뽑아서 그런지 아프지는 않더라구요",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100023812.1.12",
          "form": "뽑는 과정이 너무 무서워요..",
          "original_form": "뽑는 과정이 너무 무서워요..",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100023812.1.13",
          "form": "악 너무 무서워요",
          "original_form": "악 너무 무서워요",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100023812.1.14",
          "form": "치과의사선생님이 펜치같은걸로 꽉꽉 누르는 느낌이에요..",
          "original_form": "치과의사선생님이 펜치같은걸로 꽉꽉 누르는 느낌이에요..",
          "speaker_id": "2",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100023812.1.15",
          "form": "마취하고 생니 잡아뽑는건가요?",
          "original_form": "마취하고 생니 잡아뽑는건가요?",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100023812.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100023812.1.17",
          "form": "그래서 뽑을때는 아프지는 않아요",
          "original_form": "그래서 뽑을때는 아프지는 않아요",
          "speaker_id": "2",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100023812.1.18",
          "form": "헐 근데",
          "original_form": "헐 근데",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100023812.1.19",
          "form": "생니뽑으면 피 많이나지않아요?",
          "original_form": "생니뽑으면 피 많이나지않아요?",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100023812.1.20",
          "form": "네 많이 나요",
          "original_form": "네 많이 나요",
          "speaker_id": "2",
          "time": "20210830 18:02"
        }
      ]
