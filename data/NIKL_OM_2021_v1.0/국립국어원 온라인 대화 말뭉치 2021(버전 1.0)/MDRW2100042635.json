      "utterance": [
        {
          "id": "MDRW2100042635.1.1",
          "form": "혹시 바디로션도 바르세요??",
          "original_form": "혹시 바디로션도 바르세요??",
          "speaker_id": "1",
          "time": "20210907 00:06"
        },
        {
          "id": "MDRW2100042635.1.2",
          "form": "바디로션은 겨울에만 가끔 발라요",
          "original_form": "바디로션은 겨울에만 가끔 발라요",
          "speaker_id": "2",
          "time": "20210907 00:07"
        },
        {
          "id": "MDRW2100042635.1.3",
          "form": "오 저는 아토피가있어서",
          "original_form": "오 저는 아토피가있어서",
          "speaker_id": "1",
          "time": "20210907 00:07"
        },
        {
          "id": "MDRW2100042635.1.4",
          "form": "자주발라요",
          "original_form": "자주발라요",
          "speaker_id": "1",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042635.1.5",
          "form": "아토피가 있어요 ?",
          "original_form": "아토피가 있어요 ?",
          "speaker_id": "2",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042635.1.6",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042635.1.7",
          "form": "고생이겠네요 ㅠㅠ",
          "original_form": "고생이겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042635.1.8",
          "form": "네 정말 최악이랍니다ㅠ",
          "original_form": "네 정말 최악이랍니다ㅠ",
          "speaker_id": "1",
          "time": "20210907 00:09"
        },
        {
          "id": "MDRW2100042635.1.9",
          "form": "아토피 약이 있나요 ?",
          "original_form": "아토피 약이 있나요 ?",
          "speaker_id": "2",
          "time": "20210907 00:09"
        },
        {
          "id": "MDRW2100042635.1.10",
          "form": "병원을다니긴하는데",
          "original_form": "병원을다니긴하는데",
          "speaker_id": "1",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042635.1.11",
          "form": "완치가 어려운것같아요",
          "original_form": "완치가 어려운것같아요",
          "speaker_id": "1",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042635.1.12",
          "form": "아토피는 먹는것이 많이 중요하대요",
          "original_form": "아토피는 먹는것이 많이 중요하대요",
          "speaker_id": "2",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042635.1.13",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042635.1.14",
          "form": "밀가루를",
          "original_form": "밀가루를",
          "speaker_id": "1",
          "time": "20210907 00:11"
        },
        {
          "id": "MDRW2100042635.1.15",
          "form": "줄여야하는데ㅠ",
          "original_form": "줄여야하는데ㅠ",
          "speaker_id": "1",
          "time": "20210907 00:11"
        },
        {
          "id": "MDRW2100042635.1.16",
          "form": "식단조절이 참 어렵죠",
          "original_form": "식단조절이 참 어렵죠",
          "speaker_id": "2",
          "time": "20210907 00:11"
        },
        {
          "id": "MDRW2100042635.1.17",
          "form": "네 식단조절이 가장 힘든것같아요ㅠ",
          "original_form": "네 식단조절이 가장 힘든것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210907 00:11"
        }
