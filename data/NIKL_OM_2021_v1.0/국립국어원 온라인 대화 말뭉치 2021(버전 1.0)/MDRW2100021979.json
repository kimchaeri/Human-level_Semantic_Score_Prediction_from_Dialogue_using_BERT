      "utterance": [
        {
          "id": "MDRW2100021979.1.1",
          "form": "혹시 지금 직장 다니는 중이실까요?",
          "original_form": "혹시 지금 직장 다니는 중이실까요?",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021979.1.2",
          "form": "어떤 일 하세요?",
          "original_form": "어떤 일 하세요?",
          "speaker_id": "2",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021979.1.3",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021979.1.4",
          "form": "저는 학교에서 일해요ㅎㅎ",
          "original_form": "저는 학교에서 일해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021979.1.5",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021979.1.6",
          "form": "저는 사무직이에요",
          "original_form": "저는 사무직이에요",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021979.1.7",
          "form": "학교에서 일하시면 교사이신가요?",
          "original_form": "학교에서 일하시면 교사이신가요?",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021979.1.8",
          "form": "네 맞아요 ㅎㅎㅎ",
          "original_form": "네 맞아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021979.1.9",
          "form": "멋진 일 하시네요!",
          "original_form": "멋진 일 하시네요!",
          "speaker_id": "2",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021979.1.10",
          "form": "저도 약간 비슷한 계열이라면 그럴수도 있는 일인데 멋있진 않아요ㅋㅋㅋㅋ",
          "original_form": "저도 약간 비슷한 계열이라면 그럴수도 있는 일인데 멋있진 않아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021979.1.11",
          "form": "오 무슨 일이신데요?",
          "original_form": "오 무슨 일이신데요?",
          "speaker_id": "1",
          "time": "20210829 00:43"
        },
        {
          "id": "MDRW2100021979.1.12",
          "form": "저는 걍 평범한 공노비요....ㅎㅎㅎ",
          "original_form": "저는 걍 평범한 공노비요....ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:44"
        },
        {
          "id": "MDRW2100021979.1.13",
          "form": "아....ㅎㅎㅎ",
          "original_form": "아....ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:44"
        },
        {
          "id": "MDRW2100021979.1.14",
          "form": "그쪽도 코로나 때문에 일 정말 많아지셨죠.....",
          "original_form": "그쪽도 코로나 때문에 일 정말 많아지셨죠.....",
          "speaker_id": "1",
          "time": "20210829 00:44"
        },
        {
          "id": "MDRW2100021979.1.15",
          "form": "그래도 제가 일하는 기관은 그렇게 코로나 영향을 받지는 않아서 괜찮아요",
          "original_form": "그래도 제가 일하는 기관은 그렇게 코로나 영향을 받지는 않아서 괜찮아요",
          "speaker_id": "2",
          "time": "20210829 00:45"
        },
        {
          "id": "MDRW2100021979.1.16",
          "form": "어떤 기관에서 일하시는지 혹시 여쭤봐도 될까요",
          "original_form": "어떤 기관에서 일하시는지 혹시 여쭤봐도 될까요",
          "speaker_id": "1",
          "time": "20210829 00:45"
        },
        {
          "id": "MDRW2100021979.1.17",
          "form": "저는 국가기관인데 코로나 관련 부처는 아니에요",
          "original_form": "저는 국가기관인데 코로나 관련 부처는 아니에요",
          "speaker_id": "2",
          "time": "20210829 00:47"
        },
        {
          "id": "MDRW2100021979.1.18",
          "form": "아하 그럼 업무 환경이 그렇게 크게 바뀌진 않으셨겠어요 완전 럭키...!",
