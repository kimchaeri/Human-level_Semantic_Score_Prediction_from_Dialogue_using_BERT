          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022727.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022727.1.6",
          "form": "그래서 다음에는 어두운 옷 입어야지 ㅎㅎ 하면",
          "original_form": "그래서 다음에는 어두운 옷 입어야지 ㅎㅎ 하면",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022727.1.7",
          "form": "진짜 어두운 옷 입은 날에는 밥 건너뛰고",
          "original_form": "진짜 어두운 옷 입은 날에는 밥 건너뛰고",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022727.1.8",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022727.1.9",
          "form": "name2님은 만약에",
          "original_form": "&name2&님은 만약에",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022727.1.10",
          "form": "돈과 시간과 기회 아무 제한 없이 자유롭다면",
          "original_form": "돈과 시간과 기회 아무 제한 없이 자유롭다면",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022727.1.11",
          "form": "(두근)",
          "original_form": "(두근)",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022727.1.12",
          "form": "어떤 스타일 패션을 입고싶으세요?!",
          "original_form": "어떤 스타일 패션을 입고싶으세요?!",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022727.1.13",
          "form": "아우",
          "original_form": "아우",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022727.1.14",
          "form": "돈만많으면",
          "original_form": "돈만많으면",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022727.1.15",
          "form": "수트 빼입고다니죠",
          "original_form": "수트 빼입고다니죠",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022727.1.16",
          "form": "우쒸",
          "original_form": "우쒸",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022727.1.17",
          "form": "수트 너무 좋은데 어찌 그리 비싼지",
          "original_form": "수트 너무 좋은데 어찌 그리 비싼지",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022727.1.18",
          "form": "와 저도 수트",
          "original_form": "와 저도 수트",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022727.1.19",
          "form": "캬 수트",
          "original_form": "캬 수트",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022727.1.20",
          "form": "한세트 맞추려면 인터넷 쇼핑몰에서 사도 2~30은 써야되잖아요",
          "original_form": "한세트 맞추려면 인터넷 쇼핑몰에서 사도 2~30은 써야되잖아요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022727.1.21",
          "form": "전 그진데!",
          "original_form": "전 그진데!",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022727.1.22",
          "form": "맞아요 심지어 그 가격조차",
          "original_form": "맞아요 심지어 그 가격조차",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022727.1.23",
          "form": "수트계에서는 초저가라는 소리가 있는데",
          "original_form": "수트계에서는 초저가라는 소리가 있는데",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022727.1.24",
          "form": "비싼 수트는 상상이 안 돼요",
          "original_form": "비싼 수트는 상상이 안 돼요",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022727.1.25",
          "form": "싸구려옷 여러벌 사느니 비싼 옷 적게사는게 낫다고 하는데",
          "original_form": "싸구려옷 여러벌 사느니 비싼 옷 적게사는게 낫다고 하는데",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022727.1.26",
          "form": "전 싸구려옷을 적게산단말이에요,,",
          "original_form": "전 싸구려옷을 적게산단말이에요,,",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022727.1.27",
          "form": "ㅋㅌㅋㅋㅋㅋㅋ저도요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋ저도요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022727.1.28",
          "form": "왜 제 옷장 사찰하세요",
          "original_form": "왜 제 옷장 사찰하세요",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022727.1.29",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022727.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022727.1.31",
          "form": "여성복 보통",
          "original_form": "여성복 보통",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022727.1.32",
          "form": "한철입게 만들잖아요",
          "original_form": "한철입게 만들잖아요",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022727.1.33",
          "form": "전 그걸 한 5년입거든요 거적떼기.",
          "original_form": "전 그걸 한 5년입거든요 거적떼기.",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022727.1.34",
          "form": "ㅋㅋㅋㅋ저도요 티같은 것도 다 늘어날 때까지 마르고 닳도록 입다가",
          "original_form": "ㅋㅋㅋㅋ저도요 티같은 것도 다 늘어날 때까지 마르고 닳도록 입다가",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022727.1.35",
          "form": "늘어나면 집에서 마저 잠옷으로 입어요",
          "original_form": "늘어나면 집에서 마저 잠옷으로 입어요",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022727.1.36",
          "form": "네버다이",
          "original_form": "네버다이",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022727.1.37",
          "form": "아 근데 분명 외출복으로 입으려고 산 옷도",
          "original_form": "아 근데 분명 외출복으로 입으려고 산 옷도",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022727.1.38",
          "form": "집에서 한번 입으면 잠옷되지않아요?",
          "original_form": "집에서 한번 입으면 잠옷되지않아요?",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022727.1.39",
          "form": "짜증나 이러려고 산게 아닌데",
          "original_form": "짜증나 이러려고 산게 아닌데",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022727.1.40",
          "form": "새옷인디..",
          "original_form": "새옷인디..",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022727.1.41",
          "form": "아 맞아요 목을 왜 다들",
          "original_form": "아 맞아요 목을 왜 다들",
          "speaker_id": "1",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022727.1.42",
          "form": "허벌로 만들며",
          "original_form": "허벌로 만들며",
          "speaker_id": "1",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022727.1.43",
          "form": "재질은 왜 보풀요정이 되게 만드나",
          "original_form": "재질은 왜 보풀요정이 되게 만드나",
          "speaker_id": "1",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022727.1.44",
          "form": "ㅋㅋㄱㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022727.1.45",
          "form": "글고 그냥 집에서 한번 입는순간",
          "original_form": "글고 그냥 집에서 한번 입는순간",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022727.1.46",
          "form": "무의식이 그 옷을 잠옷으로 인식시키는거같아요",
          "original_form": "무의식이 그 옷을 잠옷으로 인식시키는거같아요",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022727.1.47",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 맞아요 좀 그러더라구요",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 맞아요 좀 그러더라구요",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022727.1.48",
          "form": "왠지 모르겠는데",
          "original_form": "왠지 모르겠는데",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022727.1.49",
          "form": "밖에서 입는 옷은 밖에 나갈 때만 고이고이 아껴입게 되고",
          "original_form": "밖에서 입는 옷은 밖에 나갈 때만 고이고이 아껴입게 되고",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022727.1.50",
          "form": "집에서 입는 순간 영원히 잠옷행",
          "original_form": "집에서 입는 순간 영원히 잠옷행",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022727.1.51",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ마자요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ마자요",
