      "utterance": [
        {
          "id": "MDRW2100000393.1.1",
          "form": "스포츠관람 좋아하세요?",
          "original_form": "스포츠관람 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000393.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000393.1.3",
          "form": "특히 야구 배구 광팬입니다ㅋ",
          "original_form": "특히 야구 배구 광팬입니다ㅋ",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000393.1.4",
          "form": "아~어떤종목이요?",
          "original_form": "아~어떤종목이요?",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000393.1.5",
          "form": "아~야구!!",
          "original_form": "아~야구!!",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000393.1.6",
          "form": "좋아하는 팀 있으세요?",
          "original_form": "좋아하는 팀 있으세요?",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000393.1.7",
          "form": "저는 응원하는 팀은 없는데 저희 아빠가 엔씨팬이에요",
          "original_form": "저는 응원하는 팀은 없는데 저희 아빠가 엔씨팬이에요",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000393.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000393.1.9",
          "form": "전 SSG",
          "original_form": "전 SSG",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000393.1.10",
          "form": "예전엔 자주 관람갔는데",
          "original_form": "예전엔 자주 관람갔는데",
          "speaker_id": "2",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000393.1.11",
          "form": "지그은 못가서 슬퍼요",
          "original_form": "지그은 못가서 슬퍼요",
          "speaker_id": "2",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000393.1.12",
          "form": "맞아요 코로나땜에ㅠ",
          "original_form": "맞아요 코로나땜에ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000393.1.13",
          "form": "전 스케쥴이 스포츠 위주로ㅋ",
          "original_form": "전 스케쥴이 스포츠 위주로ㅋ",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000393.1.14",
          "form": "아 정말요!직업이랑 관련있으세요?",
          "original_form": "아 정말요!직업이랑 관련있으세요?",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000393.1.15",
          "form": "아뇨 ㅋ",
          "original_form": "아뇨 ㅋ",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000393.1.16",
          "form": "전혀 없어요",
          "original_form": "전혀 없어요",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000393.1.17",
          "form": "응원하는 팀 과 선수가 있음 잼나요",
          "original_form": "응원하는 팀 과 선수가 있음 잼나요",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000393.1.18",
          "form": "아~~스포츠를 사랑하시는군요",
          "original_form": "아~~스포츠를 사랑하시는군요",
          "speaker_id": "1",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000393.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000393.1.20",
          "form": "여름엔 야구 겨울엔 배구",
          "original_form": "여름엔 야구 겨울엔 배구",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000393.1.21",
          "form": "시즌이 있어서",
          "original_form": "시즌이 있어서",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000393.1.22",
          "form": "아~배구도요!오~~",
          "original_form": "아~배구도요!오~~",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000393.1.23",
          "form": "ㅋ네",
          "original_form": "ㅋ네",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000393.1.24",
          "form": "집에서 혼자 응원하세요?",
          "original_form": "집에서 혼자 응원하세요?",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000393.1.25",
          "form": "식구들이 다 좋아해서",
          "original_form": "식구들이 다 좋아해서",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000393.1.26",
          "form": "같이 가기도하고",
          "original_form": "같이 가기도하고",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000393.1.27",
          "form": "근데 야구는 응원하는 팀이 같은데",
          "original_form": "근데 야구는 응원하는 팀이 같은데",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000393.1.28",
          "form": "ㅋㅋ배구는 달라서 혼자 응원",
          "original_form": "ㅋㅋ배구는 달라서 혼자 응원",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000393.1.29",
          "form": "아~보통 야구는 가족들이 같은팀 좋아하더라고요",
          "original_form": "아~보통 야구는 가족들이 같은팀 좋아하더라고요",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000393.1.30",
          "form": "맞아요 ㅋ",
          "original_form": "맞아요 ㅋ",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000393.1.31",
          "form": "부모님이 데리고 다니는 경우가 많아요",
          "original_form": "부모님이 데리고 다니는 경우가 많아요",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000393.1.32",
          "form": "저희는 아빠가 야구광팬인데 혼자만 좋아하세요 ㅋㅋ",
          "original_form": "저희는 아빠가 야구광팬인데 혼자만 좋아하세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000393.1.33",
          "form": "ㅋㅋㅋ외로우시겠다",
          "original_form": "ㅋㅋㅋ외로우시겠다",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000393.1.34",
          "form": "그러실수도 있겠네요 ㅋㅋ",
          "original_form": "그러실수도 있겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000393.1.35",
          "form": "NC다이노스ㅋ 요즘 분위기가ㅋ",
          "original_form": "NC다이노스ㅋ 요즘 분위기가ㅋ",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000393.1.36",
          "form": "안좋나요?",
          "original_form": "안좋나요?",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000393.1.37",
          "form": "ㅋㅋ진짜 관심 없으시구나",
          "original_form": "ㅋㅋ진짜 관심 없으시구나",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000393.1.38",
          "form": "네 ㅋㅋ 올림픽때 좀 보고 ㅎㅎ",
          "original_form": "네 ㅋㅋ 올림픽때 좀 보고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:23"
        }
      ]
