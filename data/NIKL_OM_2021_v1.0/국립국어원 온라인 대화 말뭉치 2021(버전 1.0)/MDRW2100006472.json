      "utterance": [
        {
          "id": "MDRW2100006472.1.1",
          "form": "그거 재밌어보이더라구요",
          "original_form": "그거 재밌어보이더라구요",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006472.1.2",
          "form": "근데 컴게임은 사양 딸려서 ㅋㅋㅋㅋ ㅠ 잘 못하는데",
          "original_form": "근데 컴게임은 사양 딸려서 ㅋㅋㅋㅋ ㅠ 잘 못하는데",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006472.1.3",
          "form": "모바일도 내줫음 좋겟어요",
          "original_form": "모바일도 내줫음 좋겟어요",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006472.1.4",
          "form": "아마 내줄 거 같기도 한대여 ㅎ",
          "original_form": "아마 내줄 거 같기도 한대여 ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006472.1.5",
          "form": "그쵸 검은사막 낸거보면 ㅋㅋㅋㅋ",
          "original_form": "그쵸 검은사막 낸거보면 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006472.1.6",
          "form": "근데 한참 뒤일거같아요",
          "original_form": "근데 한참 뒤일거같아요",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006472.1.7",
          "form": "붉은 사막도 만든다고 하니 ㅎ",
          "original_form": "붉은 사막도 만든다고 하니 ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006472.1.8",
          "form": "근데 팀이 다른 거 같긴 한데",
          "original_form": "근데 팀이 다른 거 같긴 한데",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006472.1.9",
          "form": "붉은 사막은 무기한 연기라더라구요",
          "original_form": "붉은 사막은 무기한 연기라더라구요",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006472.1.10",
          "form": "헐 그것도 나름 괜찮은 거 같던데 ㅠ",
          "original_form": "헐 그것도 나름 괜찮은 거 같던데 ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006472.1.11",
          "form": "그쵸 저도 오픈월드라길래 기대하고 있었는데",
          "original_form": "그쵸 저도 오픈월드라길래 기대하고 있었는데",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006472.1.12",
          "form": "당장은 도깨비에 집중할건가봐요",
          "original_form": "당장은 도깨비에 집중할건가봐요",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006472.1.13",
          "form": "흠 글쿤요",
          "original_form": "흠 글쿤요",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100006472.1.14",
          "form": "둘 다 국산콘솔 게임의 희망이네요 ㅋㅋ",
          "original_form": "둘 다 국산콘솔 게임의 희망이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100006472.1.15",
          "form": "그쵸 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006472.1.16",
          "form": "눈물을 마시는 새?",
          "original_form": "눈물을 마시는 새?",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006472.1.17",
          "form": "그것도 게임으로 나온다고 들었어요",
          "original_form": "그것도 게임으로 나온다고 들었어요",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006472.1.18",
          "form": "오 그건 머지 찾아봐야겠네요",
          "original_form": "오 그건 머지 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006472.1.19",
          "form": "네네 판타지 소설 원작인데",
          "original_form": "네네 판타지 소설 원작인데",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006472.1.20",
          "form": "크래프톤에서 낼거래요",
          "original_form": "크래프톤에서 낼거래요",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006472.1.21",
          "form": "아 영상은 있네요",
          "original_form": "아 영상은 있네요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006472.1.22",
          "form": "네 영상이랑 컨셉샷? 조금 공개됐어요",
          "original_form": "네 영상이랑 컨셉샷? 조금 공개됐어요",
