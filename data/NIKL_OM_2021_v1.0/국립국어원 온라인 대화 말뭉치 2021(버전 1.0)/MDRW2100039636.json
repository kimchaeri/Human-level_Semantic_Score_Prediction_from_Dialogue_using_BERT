        },
        {
          "id": "MDRW2100039636.1.2",
          "form": "안녕하세요~~~~~",
          "original_form": "안녕하세요~~~~~",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039636.1.3",
          "form": "동물키우시나요?ㅎ",
          "original_form": "동물키우시나요?ㅎ",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100039636.1.4",
          "form": "아뇨! 키우고 싶긴한데 책임이 막중해서 못키우고있어요",
          "original_form": "아뇨! 키우고 싶긴한데 책임이 막중해서 못키우고있어요",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039636.1.5",
          "form": "동물키우시나요",
          "original_form": "동물키우시나요",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039636.1.6",
          "form": "아뇨ㅎㅎ주변에서는 많이 봤는데 저는ㅠ",
          "original_form": "아뇨ㅎㅎ주변에서는 많이 봤는데 저는ㅠ",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039636.1.7",
          "form": "저두요 ㅠ ㅎㅎ 돈도 많이들고 집도 좁고 이것저것 여건땜에 못키워요",
          "original_form": "저두요 ㅠ ㅎㅎ 돈도 많이들고 집도 좁고 이것저것 여건땜에 못키워요",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039636.1.8",
          "form": "애들은 엄청 좋아하는데ㅜㅋ",
          "original_form": "애들은 엄청 좋아하는데ㅜㅋ",
          "speaker_id": "1",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100039636.1.9",
          "form": "맞아요ㅠ 오늘 아침에 출근했는데 길냥이 식빵구우면서 자고있더라고요",
          "original_form": "맞아요ㅠ 오늘 아침에 출근했는데 길냥이 식빵구우면서 자고있더라고요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100039636.1.10",
          "form": "길냥이요?",
          "original_form": "길냥이요?",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100039636.1.11",
          "form": "사무실근처서요?ㅎ",
          "original_form": "사무실근처서요?ㅎ",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100039636.1.12",
          "form": "네네 ㅎ 길고양이",
          "original_form": "네네 ㅎ 길고양이",
          "speaker_id": "2",
          "time": "20210904 14:34"
        },
        {
          "id": "MDRW2100039636.1.13",
          "form": "회사근처서 지내는 애인데 자고있더라고요",
          "original_form": "회사근처서 지내는 애인데 자고있더라고요",
          "speaker_id": "2",
          "time": "20210904 14:34"
        },
        {
          "id": "MDRW2100039636.1.14",
          "form": "ㅎㅎㅎ주변에서 많이 이뻐하겠어요",
          "original_form": "ㅎㅎㅎ주변에서 많이 이뻐하겠어요",
          "speaker_id": "1",
          "time": "20210904 14:39"
        },
        {
          "id": "MDRW2100039636.1.15",
          "form": "아뇨 ㅎ 직원들은 그닥 안좋아하더라고요",
          "original_form": "아뇨 ㅎ 직원들은 그닥 안좋아하더라고요",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100039636.1.16",
          "form": "하긴 사람 나름이죠ㅋ",
          "original_form": "하긴 사람 나름이죠ㅋ",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100039636.1.17",
          "form": "키우는 사람들 보면 대단해요ㅜ",
          "original_form": "키우는 사람들 보면 대단해요ㅜ",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100039636.1.18",
          "form": "네네 그니깐요 진짜 애기 키우는거랑 같다고 하던데",
          "original_form": "네네 그니깐요 진짜 애기 키우는거랑 같다고 하던데",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100039636.1.19",
          "form": "힘들다고",
          "original_form": "힘들다고",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100039636.1.20",
          "form": "애키우는것보다 더 힘들것같아요..",
          "original_form": "애키우는것보다 더 힘들것같아요..",
          "speaker_id": "1",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100039636.1.21",
          "form": "애는 말이라도 통하죠 ㅠ",
          "original_form": "애는 말이라도 통하죠 ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100039636.1.22",
          "form": "동물들은 말도안통해서 어려울것같아요",
          "original_form": "동물들은 말도안통해서 어려울것같아요",
          "speaker_id": "2",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100039636.1.23",
          "form": "그렇죠ㅎㅎ 뭐든 키운다는건 참ㅋㅋ",
          "original_form": "그렇죠ㅎㅎ 뭐든 키운다는건 참ㅋㅋ",
