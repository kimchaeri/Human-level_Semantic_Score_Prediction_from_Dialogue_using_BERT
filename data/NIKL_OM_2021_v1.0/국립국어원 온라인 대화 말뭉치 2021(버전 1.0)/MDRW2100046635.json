          "time": "20211103 21:49"
        },
        {
          "id": "MDRW2100046635.1.4",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211103 21:49"
        },
        {
          "id": "MDRW2100046635.1.5",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20211103 21:49"
        },
        {
          "id": "MDRW2100046635.1.6",
          "form": "ㅇㅇ 반가워",
          "original_form": "ㅇㅇ 반가워",
          "speaker_id": "1",
          "time": "20211103 21:49"
        },
        {
          "id": "MDRW2100046635.1.7",
          "form": "먹었지",
          "original_form": "먹었지",
          "speaker_id": "1",
          "time": "20211103 21:50"
        },
        {
          "id": "MDRW2100046635.1.8",
          "form": "너는 먹었니",
          "original_form": "너는 먹었니",
          "speaker_id": "1",
          "time": "20211103 21:50"
        },
        {
          "id": "MDRW2100046635.1.9",
          "form": "네 저도 먹었어요 ㅎㅎ",
          "original_form": "네 저도 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211103 21:50"
        },
        {
          "id": "MDRW2100046635.1.10",
          "form": "저녁 메뉴는 무엇이었나요?",
          "original_form": "저녁 메뉴는 무엇이었나요?",
          "speaker_id": "2",
          "time": "20211103 21:50"
        },
        {
          "id": "MDRW2100046635.1.11",
          "form": "돈까스 먹었어",
          "original_form": "돈까스 먹었어",
          "speaker_id": "1",
          "time": "20211103 21:51"
        },
        {
          "id": "MDRW2100046635.1.12",
          "form": "헉 돈까스 넘 맛있죠",
          "original_form": "헉 돈까스 넘 맛있죠",
          "speaker_id": "2",
          "time": "20211103 21:51"
        },
        {
          "id": "MDRW2100046635.1.13",
          "form": "ㅇㅇ 맛있었음",
          "original_form": "ㅇㅇ 맛있었음",
          "speaker_id": "1",
          "time": "20211103 21:51"
        },
        {
          "id": "MDRW2100046635.1.14",
          "form": "경양식 돈까스예요 아님 일본식 돈까스예요?",
          "original_form": "경양식 돈까스예요 아님 일본식 돈까스예요?",
          "speaker_id": "2",
          "time": "20211103 21:51"
        },
        {
          "id": "MDRW2100046635.1.15",
          "form": "일본식",
          "original_form": "일본식",
          "speaker_id": "1",
          "time": "20211103 21:51"
        },
        {
          "id": "MDRW2100046635.1.16",
          "form": "아하 일본식 돈까스 맛있죠 ㅎㅎ 전 경양식 돈까스가 먹고싶네요",
          "original_form": "아하 일본식 돈까스 맛있죠 ㅎㅎ 전 경양식 돈까스가 먹고싶네요",
          "speaker_id": "2",
          "time": "20211103 21:52"
        },
        {
          "id": "MDRW2100046635.1.17",
          "form": "그냥 돈까스?",
          "original_form": "그냥 돈까스?",
          "speaker_id": "1",
          "time": "20211103 21:52"
        },
        {
          "id": "MDRW2100046635.1.18",
          "form": "치즈 아님 고구마 들어간건 어때",
          "original_form": "치즈 아님 고구마 들어간건 어때",
          "speaker_id": "1",
          "time": "20211103 21:53"
        },
        {
          "id": "MDRW2100046635.1.19",
          "form": "전 오리지널 파이긴 한데",
          "original_form": "전 오리지널 파이긴 한데",
          "speaker_id": "2",
          "time": "20211103 21:53"
        },
        {
          "id": "MDRW2100046635.1.20",
          "form": "들어간다면 고구마요!!",
          "original_form": "들어간다면 고구마요!!",
          "speaker_id": "2",
          "time": "20211103 21:53"
        },
        {
          "id": "MDRW2100046635.1.21",
          "form": "고구마 맛있지",
          "original_form": "고구마 맛있지",
          "speaker_id": "1",
          "time": "20211103 21:54"
        },
        {
          "id": "MDRW2100046635.1.22",
          "form": "달달한게",
          "original_form": "달달한게",
          "speaker_id": "1",
          "time": "20211103 21:54"
        },
        {
          "id": "MDRW2100046635.1.23",
          "form": "인정이요",
          "original_form": "인정이요",
          "speaker_id": "2",
          "time": "20211103 21:54"
        },
        {
          "id": "MDRW2100046635.1.24",
          "form": "겨울 되면 꼭 생각나요 ㅋㅋ",
          "original_form": "겨울 되면 꼭 생각나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 21:54"
        },
        {
          "id": "MDRW2100046635.1.25",
          "form": "군고구마",
          "original_form": "군고구마",
          "speaker_id": "1",
          "time": "20211103 21:54"
        },
        {
          "id": "MDRW2100046635.1.26",
          "form": "먹고싶다 갑자기",
          "original_form": "먹고싶다 갑자기",
          "speaker_id": "1",
          "time": "20211103 21:54"
        },
        {
          "id": "MDRW2100046635.1.27",
          "form": "요즘엔 길거리에서 군고구마를 안팔더라구요 ㅠ",
          "original_form": "요즘엔 길거리에서 군고구마를 안팔더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20211103 21:55"
        },
        {
          "id": "MDRW2100046635.1.28",
          "form": "너무 아쉬워요",
          "original_form": "너무 아쉬워요",
          "speaker_id": "2",
          "time": "20211103 21:55"
        },
        {
          "id": "MDRW2100046635.1.29",
          "form": "편의점에 팔던데",
          "original_form": "편의점에 팔던데",
          "speaker_id": "1",
          "time": "20211103 21:56"
        }
      ]
