      "utterance": [
        {
          "id": "MDRW2100006312.1.1",
          "form": "야식시간입니다",
          "original_form": "야식시간입니다",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006312.1.2",
          "form": "야식드세요",
          "original_form": "야식드세요",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006312.1.3",
          "form": "ㅋㅋㅋㅋㅋ 라면?",
          "original_form": "ㅋㅋㅋㅋㅋ 라면?",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006312.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006312.1.5",
          "form": "이상적인 야식",
          "original_form": "이상적인 야식",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006312.1.6",
          "form": "라면 뭐 좋아해요?",
          "original_form": "라면 뭐 좋아해요?",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006312.1.7",
          "form": "요샌 비빔면 많이 먹었고",
          "original_form": "요샌 비빔면 많이 먹었고",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006312.1.8",
          "form": "짜파게티에 파김치나 신김치\r\n조화도 넘 좋아하고",
          "original_form": "짜파게티에 파김치나 신김치\r\n조화도 넘 좋아하고",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006312.1.9",
          "form": "너구리도 좋아해요ㅋㅋ",
          "original_form": "너구리도 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006312.1.10",
          "form": "오짬도 맛있고 ㅋㅋ",
          "original_form": "오짬도 맛있고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006312.1.11",
          "form": "이렇게 다 드시는데",
          "original_form": "이렇게 다 드시는데",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006312.1.12",
          "form": "왜 아무것도 안드셨어요",
          "original_form": "왜 아무것도 안드셨어요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006312.1.13",
          "form": "정말 별 생각이 없었어요",
          "original_form": "정말 별 생각이 없었어요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006312.1.14",
          "form": "허기지다 이런것도 없고",
          "original_form": "허기지다 이런것도 없고",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006312.1.15",
          "form": "뭐 먹고싶다 이것도 없고ㅋㅋㅈ",
          "original_form": "뭐 먹고싶다 이것도 없고ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006312.1.16",
          "form": "근데 먹고싶은거 생겨서\r\n내일이 빨리 오면 좋겠어요",
          "original_form": "근데 먹고싶은거 생겨서\r\n내일이 빨리 오면 좋겠어요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006312.1.17",
          "form": "내일 드시게요?",
          "original_form": "내일 드시게요?",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006312.1.18",
          "form": "네! 곱도리 드셔보셨어요?",
          "original_form": "네! 곱도리 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006312.1.19",
          "form": "곱도리?",
          "original_form": "곱도리?",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006312.1.20",
          "form": "그게 뭔가요",
          "original_form": "그게 뭔가요",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006312.1.21",
          "form": "곱창이랑 닭도리탕이라는데",
          "original_form": "곱창이랑 닭도리탕이라는데",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100006312.1.22",
          "form": "꿀조화 아닌가요? ㅋㅋ",
          "original_form": "꿀조화 아닌가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006312.1.23",
          "form": "맛있을까요?",
          "original_form": "맛있을까요?",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006312.1.24",
          "form": "먹어본적이 없어서",
          "original_form": "먹어본적이 없어서",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006312.1.25",
          "form": "저도 ㅋㅋ 안먹어봤는데",
          "original_form": "저도 ㅋㅋ 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006312.1.26",
          "form": "맛있을거 같아서 낼 먹어보려구용",
          "original_form": "맛있을거 같아서 낼 먹어보려구용",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006312.1.27",
          "form": "어디서요?",
          "original_form": "어디서요?",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006312.1.28",
          "form": "시켜서요?",
          "original_form": "시켜서요?",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006312.1.29",
          "form": "네ㅋㅋ 배달요",
          "original_form": "네ㅋㅋ 배달요",
          "speaker_id": "1",
          "time": "20210907 23:18"
        }
      ]
    }
  ]
