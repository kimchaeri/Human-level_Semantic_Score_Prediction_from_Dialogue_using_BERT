      "utterance": [
        {
          "id": "MDRW2100023067.1.1",
          "form": "공복운동?",
          "original_form": "공복운동?",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023067.1.2",
          "form": "저는예전에 공복에 복근운동 두세트 하던 때가 있었는데 늘 복근 달고 체중이 날로 줄었어요",
          "original_form": "저는예전에 공복에 복근운동 두세트 하던 때가 있었는데 늘 복근 달고 체중이 날로 줄었어요",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023067.1.3",
          "form": "오오 좋네요",
          "original_form": "오오 좋네요",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023067.1.4",
          "form": "사개월동안 몸을 만들어봐야겠어요",
          "original_form": "사개월동안 몸을 만들어봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023067.1.5",
          "form": "너무 무리하지마세요",
          "original_form": "너무 무리하지마세요",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023067.1.6",
          "form": "그럴게요",
          "original_form": "그럴게요",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023067.1.7",
          "form": "그래서 온수풀도 다녀와볼 거에요",
          "original_form": "그래서 온수풀도 다녀와볼 거에요",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023067.1.8",
          "form": "겨울 호캉스 너무좋네요",
          "original_form": "겨울 호캉스 너무좋네요",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023067.1.9",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023067.1.10",
          "form": "운동 시작도ㅈ안했지만 결과가 기대되는 사람",
          "original_form": "운동 시작도ㅈ안했지만 결과가 기대되는 사람",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023067.1.11",
          "form": "ㅋㅋ 웃기네용",
          "original_form": "ㅋㅋ 웃기네용",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023067.1.12",
          "form": "박봄운동부터 시작해보겠습니딘",
          "original_form": "박봄운동부터 시작해보겠습니딘",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023067.1.13",
          "form": "떨리는 살이 없을때까지!",
          "original_form": "떨리는 살이 없을때까지!",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023067.1.14",
          "form": "떨리면 지방이다!",
          "original_form": "떨리면 지방이다!",
          "speaker_id": "1",
          "time": "20210829 21:29"
        }
      ]
