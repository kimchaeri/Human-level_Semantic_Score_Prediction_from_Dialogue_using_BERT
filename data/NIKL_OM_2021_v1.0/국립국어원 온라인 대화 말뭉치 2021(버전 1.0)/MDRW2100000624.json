      "utterance": [
        {
          "id": "MDRW2100000624.1.1",
          "form": "동호회같은거 하는거 있어?",
          "original_form": "동호회같은거 하는거 있어?",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000624.1.2",
          "form": "아니 집에 있는 걸 좋아해서 딱히 하고 있는 건 없어",
          "original_form": "아니 집에 있는 걸 좋아해서 딱히 하고 있는 건 없어",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000624.1.3",
          "form": "게임클렌은 있어ㅋㅋ",
          "original_form": "게임클렌은 있어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000624.1.4",
          "form": "오 그렇구나ㅋㅋㅋㅋㅋㅋㅋ \r\n난 예전에 rpg게임 길드모임 해본적있어",
          "original_form": "오 그렇구나ㅋㅋㅋㅋㅋㅋㅋ \r\n난 예전에 rpg게임 길드모임 해본적있어",
          "speaker_id": "2",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000624.1.5",
          "form": "헉 길드모임?ㅋㅋㅋㅋㅋ어땠어? 난 걍 킹덤모임이야ㅋㄱ",
          "original_form": "헉 길드모임?ㅋㅋㅋㅋㅋ어땠어? 난 걍 킹덤모임이야ㅋㄱ",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000624.1.6",
          "form": "쿠키런킹덤?",
          "original_form": "쿠키런킹덤?",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000624.1.7",
          "form": "딱 한번 오프라인 정모ㅋㅋㅋ \r\n나쁘지않았는데 또 가게되진않더라",
          "original_form": "딱 한번 오프라인 정모ㅋㅋㅋ \r\n나쁘지않았는데 또 가게되진않더라",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000624.1.8",
          "form": "되게 어색할 것 같아ㅋㅋㅋ닉넴 부르면서 ㅇㅇ님 맞으세요? 이래야하잖아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "되게 어색할 것 같아ㅋㅋㅋ닉넴 부르면서 ㅇㅇ님 맞으세요? 이래야하잖아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000624.1.9",
          "form": "넷상에서는 친하게지냈었으니까 어색해도 안어색한척 해야지ㅋㅋㅋㅋㅋ",
          "original_form": "넷상에서는 친하게지냈었으니까 어색해도 안어색한척 해야지ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000624.1.10",
          "form": "킹덤도 서로 채팅하는거야?",
          "original_form": "킹덤도 서로 채팅하는거야?",
          "speaker_id": "2",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000624.1.11",
          "form": "ㅇㅇㅇ조합이나 토핑추천해줌ㅋㅋㅋ",
          "original_form": "ㅇㅇㅇ조합이나 토핑추천해줌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000624.1.12",
          "form": "유익한 모임이네 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "유익한 모임이네 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000624.1.13",
          "form": "친목은 안하고?",
          "original_form": "친목은 안하고?",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000624.1.14",
          "form": "ㅇㅇ친목하면 파벌 생긴다고 친목 금지야ㅋㅋㅋ",
          "original_form": "ㅇㅇ친목하면 파벌 생긴다고 친목 금지야ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000624.1.15",
          "form": "맞아 괜히 친목해서 안좋게흘러갈수도있으니깐",
          "original_form": "맞아 괜히 친목해서 안좋게흘러갈수도있으니깐",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000624.1.16",
          "form": "난 길드모임은 이제 안하는데",
          "original_form": "난 길드모임은 이제 안하는데",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000624.1.17",
          "form": "그중에 잘맞는친구랑은 아직도 친구로 지내",
