      "utterance": [
        {
          "id": "MDRW2100031333.1.1",
          "form": "티비 보시나요?",
          "original_form": "티비 보시나요?",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031333.1.2",
          "form": "오늘 뭐 보신거 있으세요?",
          "original_form": "오늘 뭐 보신거 있으세요?",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031333.1.3",
          "form": "네 저 유튜브 틀어놓기도 귀찮아서 ㅋㅋㅋㅋ 티비봐요",
          "original_form": "네 저 유튜브 틀어놓기도 귀찮아서 ㅋㅋㅋㅋ 티비봐요",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031333.1.4",
          "form": "저 지금 모던패밀리 보고있어요ㅋㅋㅋ",
          "original_form": "저 지금 모던패밀리 보고있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031333.1.5",
          "form": "올만에 보니 재밌네요 ㅎ",
          "original_form": "올만에 보니 재밌네요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031333.1.6",
          "form": "뭐 보세요?",
          "original_form": "뭐 보세요?",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031333.1.7",
          "form": "저 친구한테 추천받아서 브리저튼 봤었는데 그거 마무리까지 봤어요 ㅋㅋㅋ",
          "original_form": "저 친구한테 추천받아서 브리저튼 봤었는데 그거 마무리까지 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031333.1.8",
          "form": "오 저도 그건 마무리까지 봤어요ㅋㅋㅋ",
          "original_form": "오 저도 그건 마무리까지 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031333.1.9",
          "form": "드레스 장난아니네여",
          "original_form": "드레스 장난아니네여",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031333.1.10",
          "form": "남주가 멋진거 같으면서 아닌거 같고 그랬지만ㅋㅋㄱㄱ재밌었어요",
          "original_form": "남주가 멋진거 같으면서 아닌거 같고 그랬지만ㅋㅋㄱㄱ재밌었어요",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031333.1.11",
          "form": "솔직히 끝에쯤 가니까 남주여주 붙는씬도 빨리감기로 봤어욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "솔직히 끝에쯤 가니까 남주여주 붙는씬도 빨리감기로 봤어욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031333.1.12",
          "form": "여주인공이 아까웠어옄ㅋㅋㅋ",
          "original_form": "여주인공이 아까웠어옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031333.1.13",
          "form": "ㅋㅋ그쵸 저도요 대충 넘겨가면서 봤어요",
          "original_form": "ㅋㅋ그쵸 저도요 대충 넘겨가면서 봤어요",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031333.1.14",
          "form": "남자 그냥 지위가 높은거지 뭐랄까 잘생기지 않아서 더 끌리지 않았던..",
          "original_form": "남자 그냥 지위가 높은거지 뭐랄까 잘생기지 않아서 더 끌리지 않았던..",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100031333.1.15",
          "form": "공작이었나 그 시대에 실장님 느낌인가봐요",
          "original_form": "공작이었나 그 시대에 실장님 느낌인가봐요",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031333.1.16",
          "form": "그생각하니까 웃겨서",
          "original_form": "그생각하니까 웃겨서",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031333.1.17",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100031333.1.18",
          "form": "애기 안갖는다고 그럴때도 이상했던 ㅎ",
          "original_form": "애기 안갖는다고 그럴때도 이상했던 ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100031333.1.19",
          "form": "이해는갔지만 그래도..",
          "original_form": "이해는갔지만 그래도..",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100031333.1.20",
          "form": "ㅋㅋㅋㅋ 결혼을 말던가!!!",
          "original_form": "ㅋㅋㅋㅋ 결혼을 말던가!!!",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100031333.1.21",
          "form": "싶었는데 뭐 딩크족 할수도 있으니",
          "original_form": "싶었는데 뭐 딩크족 할수도 있으니",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100031333.1.22",
          "form": "걍 name1님 말대로 얼굴이 문제같아요",
          "original_form": "걍 &name1&님 말대로 얼굴이 문제같아요",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100031333.1.23",
          "form": "그쵸ㅋㅋ역시 얼굴이 문제죠",
          "original_form": "그쵸ㅋㅋ역시 얼굴이 문제죠",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031333.1.24",
          "form": "좀더 잘생긴 남주였다면 환장했을거같은데요",
          "original_form": "좀더 잘생긴 남주였다면 환장했을거같은데요",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031333.1.25",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 납득이 되는 얼굴을 달라!!!",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 납득이 되는 얼굴을 달라!!!",
