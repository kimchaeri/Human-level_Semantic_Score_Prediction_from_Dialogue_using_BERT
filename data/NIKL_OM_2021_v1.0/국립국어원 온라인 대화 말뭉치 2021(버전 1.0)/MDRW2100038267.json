      "utterance": [
        {
          "id": "MDRW2100038267.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210902 06:22"
        },
        {
          "id": "MDRW2100038267.1.2",
          "form": "집안일많이하시나요,",
          "original_form": "집안일많이하시나요,",
          "speaker_id": "2",
          "time": "20210902 06:23"
        },
        {
          "id": "MDRW2100038267.1.3",
          "form": "동생이랑 둘이 살아서",
          "original_form": "동생이랑 둘이 살아서",
          "speaker_id": "1",
          "time": "20210902 06:37"
        },
        {
          "id": "MDRW2100038267.1.4",
          "form": "최대한 나눠하려고 하지만",
          "original_form": "최대한 나눠하려고 하지만",
          "speaker_id": "1",
          "time": "20210902 06:37"
        },
        {
          "id": "MDRW2100038267.1.5",
          "form": "저보다는 동생이 많이 해요ㅎㅎ",
          "original_form": "저보다는 동생이 많이 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 06:37"
        },
        {
          "id": "MDRW2100038267.1.6",
          "form": "전 제가 다해요ㅠ",
          "original_form": "전 제가 다해요ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100038267.1.7",
          "form": "ㅜㅜ가족들이랑 함께 사시나요?",
          "original_form": "ㅜㅜ가족들이랑 함께 사시나요?",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100038267.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100038267.1.9",
          "form": "근데 다 제가하게 되네요",
          "original_form": "근데 다 제가하게 되네요",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100038267.1.10",
          "form": "ㅠㅠ집안일 너무 많은데...",
          "original_form": "ㅠㅠ집안일 너무 많은데...",
          "speaker_id": "1",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100038267.1.11",
          "form": "가족구성이 어떻게 되세요?",
          "original_form": "가족구성이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100038267.1.12",
          "form": "가족은 많은데 다 제차지ㅠ",
          "original_form": "가족은 많은데 다 제차지ㅠ",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100038267.1.13",
          "form": "시켜야 해여ㅜㅜ",
          "original_form": "시켜야 해여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100038267.1.14",
          "form": "저는 둘이서 방 한칸 하는데도",
          "original_form": "저는 둘이서 방 한칸 하는데도",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100038267.1.15",
          "form": "너무 힘든데요?",
          "original_form": "너무 힘든데요?",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100038267.1.16",
          "form": "설거지 너무 싫어요",
          "original_form": "설거지 너무 싫어요",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100038267.1.17",
          "form": "그런건 나눠서해야죠ㅜㅜㅜㅜ",
          "original_form": "그런건 나눠서해야죠ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 07:14"
        },
        {
          "id": "MDRW2100038267.1.18",
          "form": "시키세요!",
          "original_form": "시키세요!",
          "speaker_id": "1",
          "time": "20210903 07:14"
        },
        {
          "id": "MDRW2100038267.1.19",
          "form": "그냥 제가해야하는",
          "original_form": "그냥 제가해야하는",
          "speaker_id": "2",
          "time": "20210903 12:23"
        },
        {
          "id": "MDRW2100038267.1.20",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:23"
        },
        {
          "id": "MDRW2100038267.1.21",
          "form": "집안일은 가족",
          "original_form": "집안일은 가족",
          "speaker_id": "1",
          "time": "20210903 13:12"
        }
