        {
          "id": "MDRW2100028585.1.1",
          "form": "비빔냉면은 좋아!",
          "original_form": "비빔냉면은 좋아!",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028585.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028585.1.3",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028585.1.4",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028585.1.5",
          "form": "이상하게 갠찬더라",
          "original_form": "이상하게 갠찬더라",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028585.1.6",
          "form": "오 그래?",
          "original_form": "오 그래?",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028585.1.7",
          "form": "비빔냉면은 좋아",
          "original_form": "비빔냉면은 좋아",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028585.1.8",
          "form": "양많은 참치회가 좋아",
          "original_form": "양많은 참치회가 좋아",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100028585.1.9",
          "form": "닭갈비는 숯불이 맛일을까",
          "original_form": "닭갈비는 숯불이 맛일을까",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100028585.1.10",
          "form": "아님 그냥이 맛잇을까",
          "original_form": "아님 그냥이 맛잇을까",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100028585.1.11",
          "form": "엄마가 해주신 비빔국수",
          "original_form": "엄마가 해주신 비빔국수",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100028585.1.12",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100028585.1.13",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100028585.1.14",
          "form": "궁금해",
          "original_form": "궁금해",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100028585.1.15",
          "form": "너 전에안먹어봐서?",
          "original_form": "너 전에안먹어봐서?",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100028585.1.16",
          "form": "응 그것도잇고",
          "original_form": "응 그것도잇고",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100028585.1.17",
          "form": "약간 짠데 맛나",
          "original_form": "약간 짠데 맛나",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100028585.1.18",
          "form": "깻잎같아?",
          "original_form": "깻잎같아?",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100028585.1.19",
          "form": "그건또아냐",
          "original_form": "그건또아냐",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100028585.1.20",
          "form": "좀 달짝찌근??",
          "original_form": "좀 달짝찌근??",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100028585.1.21",
          "form": "살짝그래",
          "original_form": "살짝그래",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100028585.1.22",
          "form": "오오옹좋다",
          "original_form": "오오옹좋다",
