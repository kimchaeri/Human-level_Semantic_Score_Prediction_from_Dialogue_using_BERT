      "utterance": [
        {
          "id": "MDRW2100043710.1.1",
          "form": "수시로 감시해야겠네요",
          "original_form": "수시로 감시해야겠네요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043710.1.2",
          "form": "그래서 아기침대에 넣어놨어요 둘째를",
          "original_form": "그래서 아기침대에 넣어놨어요 둘째를",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043710.1.3",
          "form": "지금 첫째는 그 침대를 타고 넘을꺼 같아요",
          "original_form": "지금 첫째는 그 침대를 타고 넘을꺼 같아요",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043710.1.4",
          "form": "아. 맞네요.. 우리 첫째는 아가때라... ㅠ",
          "original_form": "아. 맞네요.. 우리 첫째는 아가때라... ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043710.1.5",
          "form": "부디 둘째를 잘 돌봐주기를 바라는데",
          "original_form": "부디 둘째를 잘 돌봐주기를 바라는데",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043710.1.6",
          "form": "나이차가 좀 있어서 예뻐해주지 않을까요??",
          "original_form": "나이차가 좀 있어서 예뻐해주지 않을까요??",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043710.1.7",
          "form": "지금은 자기가 동생태어나면 키워준다는데 ㅋㅋㅋ",
          "original_form": "지금은 자기가 동생태어나면 키워준다는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043710.1.8",
          "form": "그럼 맡깁시다 ㅋㅋㅋㅋ",
          "original_form": "그럼 맡깁시다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043710.1.9",
          "form": "니가 키워",
          "original_form": "니가 키워",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043710.1.10",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043710.1.11",
          "form": "얼릉 넘겨주렵니다",
          "original_form": "얼릉 넘겨주렵니다",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043710.1.12",
          "form": "수유도 넘겨주면 좋을텐데요",
          "original_form": "수유도 넘겨주면 좋을텐데요",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043710.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043710.1.14",
          "form": "아...ㅋㅋㅋ 그 부분이 어렵겠네요",
          "original_form": "아...ㅋㅋㅋ 그 부분이 어렵겠네요",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043710.1.15",
          "form": "첫째가 자기도 젖 먹는다고는 안하겠죠?",
          "original_form": "첫째가 자기도 젖 먹는다고는 안하겠죠?",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043710.1.16",
          "form": "6살인데 설마...ㅋㅋㅋㅋ",
          "original_form": "6살인데 설마...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043710.1.17",
          "form": "우리 첫째는 3살이었으니 그런거였겠죠??",
          "original_form": "우리 첫째는 3살이었으니 그런거였겠죠??",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043710.1.18",
          "form": "우리 첫째는 자기도 젖 먹는다고 ㅠ",
          "original_form": "우리 첫째는 자기도 젖 먹는다고 ㅠ",
          "speaker_id": "1",
