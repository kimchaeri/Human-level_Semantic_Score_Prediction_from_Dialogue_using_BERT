      "utterance": [
        {
          "id": "MDRW2100017227.1.1",
          "form": "감자전 오늘 먹는다구?",
          "original_form": "감자전 오늘 먹는다구?",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017227.1.2",
          "form": "응 오늘!\r\n어제 남긴 치킨과 함께ㅋㅋ",
          "original_form": "응 오늘!\r\n어제 남긴 치킨과 함께ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017227.1.3",
          "form": "넌 오늘 뭐먹어?",
          "original_form": "넌 오늘 뭐먹어?",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017227.1.4",
          "form": "굿굿~\r\n난 오늘 부추삼겹살!ㅋㅋ",
          "original_form": "굿굿~\r\n난 오늘 부추삼겹살!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017227.1.5",
          "form": "오호! 배달? 아님 직접해먹는거?",
          "original_form": "오호! 배달? 아님 직접해먹는거?",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017227.1.6",
          "form": "외식이얌 후후",
          "original_form": "외식이얌 후후",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017227.1.7",
          "form": "외식?  오!  name3이가 쏘는거야?ㅋㅋ",
          "original_form": "외식?  오!  &name3&이가 쏘는거야?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017227.1.8",
          "form": "어제 샀었어서 오늘 또 사는지는 모르겠네? 쐈으면 좋겠는데ㅋㅋㅋ",
          "original_form": "어제 샀었어서 오늘 또 사는지는 모르겠네? 쐈으면 좋겠는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017227.1.9",
          "form": "아ㅋㅋㅋㅋㅋ또 사달라고 해봐ㄲㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ또 사달라고 해봐ㄲㅋ",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017227.1.10",
          "form": "어디서먹어?",
          "original_form": "어디서먹어?",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017227.1.11",
          "form": "만나면 말해야겠당ㅋㅋ 사당!",
          "original_form": "만나면 말해야겠당ㅋㅋ 사당!",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017227.1.12",
          "form": "ㅋㅋㅋㅋ오 막 근처에서 먹는건아니네",
          "original_form": "ㅋㅋㅋㅋ오 막 근처에서 먹는건아니네",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017227.1.13",
          "form": "일끝나고 바로 갈거구나?",
          "original_form": "일끝나고 바로 갈거구나?",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017227.1.14",
          "form": "응 그리고 부추삼겹살이 사당이 유명한가봐",
          "original_form": "응 그리고 부추삼겹살이 사당이 유명한가봐",
          "speaker_id": "2",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017227.1.15",
          "form": "돈닭 어떤거 먹었어? 간장?",
          "original_form": "돈닭 어떤거 먹었어? 간장?",
          "speaker_id": "2",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017227.1.16",
          "form": "응 간장! 간장이 제일 나은거같아\r\n이번에 처음 가보는거지?",
          "original_form": "응 간장! 간장이 제일 나은거같아\r\n이번에 처음 가보는거지?",
          "speaker_id": "1",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017227.1.17",
          "form": "name3이는 몇번 갔었어! 괜찮다고 하더라구",
          "original_form": "&name3&이는 몇번 갔었어! 괜찮다고 하더라구",
          "speaker_id": "2",
          "time": "20210826 18:51"
        }
