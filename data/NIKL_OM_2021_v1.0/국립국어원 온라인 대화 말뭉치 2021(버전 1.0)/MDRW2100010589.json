      "utterance": [
        {
          "id": "MDRW2100010589.1.1",
          "form": "영상은 뭘 좋아하셔요?",
          "original_form": "영상은 뭘 좋아하셔요?",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010589.1.2",
          "form": "저는 연예인이나 노래 찾아보는 것같아요",
          "original_form": "저는 연예인이나 노래 찾아보는 것같아요",
          "speaker_id": "1",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010589.1.3",
          "form": "오 노래 장르는 뭘 좋아해요?",
          "original_form": "오 노래 장르는 뭘 좋아해요?",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100010589.1.4",
          "form": "전 팝송좋아하는 편이에요",
          "original_form": "전 팝송좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100010589.1.5",
          "form": "저는 케이팝이요ㅋㅋㅋㅋ",
          "original_form": "저는 케이팝이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100010589.1.6",
          "form": "케이팝 아니면 재즈힙합이나 로파이 노래같이 마음이 편안해지는 노래 좋아해요",
          "original_form": "케이팝 아니면 재즈힙합이나 로파이 노래같이 마음이 편안해지는 노래 좋아해요",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100010589.1.7",
          "form": "오 케이팝",
          "original_form": "오 케이팝",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100010589.1.8",
          "form": "전 뭔가 아이유나 발라드 노래는 좋아해요",
          "original_form": "전 뭔가 아이유나 발라드 노래는 좋아해요",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100010589.1.9",
          "form": "잔잔하고 힐링되는 노래 좋아하시는 편인가봐요",
          "original_form": "잔잔하고 힐링되는 노래 좋아하시는 편인가봐요",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100010589.1.10",
          "form": "팝은 빌리 아일리쉬같은 거 좋아해요",
          "original_form": "팝은 빌리 아일리쉬같은 거 좋아해요",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100010589.1.11",
          "form": "잔잔한거 좋아하긴해요",
          "original_form": "잔잔한거 좋아하긴해요",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100010589.1.12",
          "form": "오옹 무슨 느낌인지 알 것같아요",
          "original_form": "오옹 무슨 느낌인지 알 것같아요",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100010589.1.13",
          "form": "새로운 노래에 도전하시는 편인가요?",
          "original_form": "새로운 노래에 도전하시는 편인가요?",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100010589.1.14",
          "form": "넵엡",
          "original_form": "넵엡",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100010589.1.15",
          "form": "오 대단하시네요 전 뭔가 잘 안하게 되더라구요",
          "original_form": "오 대단하시네요 전 뭔가 잘 안하게 되더라구요",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100010589.1.16",
          "form": "그 유튜브에서 틀어놓으면 알아서 재생하니깐 새로운 노래도 잘만들어진건 좋더라고요",
          "original_form": "그 유튜브에서 틀어놓으면 알아서 재생하니깐 새로운 노래도 잘만들어진건 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100010589.1.17",
          "form": "아 그쵸그쵸ㅋㅋㅋㅋ",
          "original_form": "아 그쵸그쵸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100010589.1.18",
          "form": "유튜브말고 보는 영상 컨텐츠 있으신가요?",
          "original_form": "유튜브말고 보는 영상 컨텐츠 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100010589.1.19",
          "form": "아주 가끔 넷플 보긴햐요",
          "original_form": "아주 가끔 넷플 보긴햐요",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100010589.1.20",
          "form": "거의 안보긴하지만요 애니보려고 봐요",
          "original_form": "거의 안보긴하지만요 애니보려고 봐요",
