        {
          "id": "MDRW2100016436.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016436.1.2",
          "form": "네! 안녕하세요",
          "original_form": "네! 안녕하세요",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016436.1.3",
          "form": "집안일하시나요",
          "original_form": "집안일하시나요",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016436.1.4",
          "form": "별로 안좋아해요.....ㅋㅋㅋㅋㅋ",
          "original_form": "별로 안좋아해요.....ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016436.1.5",
          "form": "저두요ㅜ",
          "original_form": "저두요ㅜ",
          "speaker_id": "1",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016436.1.6",
          "form": "진짜 어쩔수없이 겨우겨우 하고 있어요...",
          "original_form": "진짜 어쩔수없이 겨우겨우 하고 있어요...",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016436.1.7",
          "form": "집안실 너무 싫어...",
          "original_form": "집안실 너무 싫어...",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016436.1.8",
          "form": "제일 싫은게 뭐에요?",
          "original_form": "제일 싫은게 뭐에요?",
          "speaker_id": "1",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016436.1.9",
          "form": "저 청소랑 설거지요..",
          "original_form": "저 청소랑 설거지요..",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016436.1.10",
          "form": "저는 음식물 처리하는게ㅜ",
          "original_form": "저는 음식물 처리하는게ㅜ",
          "speaker_id": "1",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016436.1.11",
          "form": "아맞아요ㅠㅠ 그거도 너무 싫죠",
          "original_form": "아맞아요ㅠㅠ 그거도 너무 싫죠",
          "speaker_id": "2",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016436.1.12",
          "form": "세상에 귀찮은게 너무 많네요ㅠㅠ",
          "original_form": "세상에 귀찮은게 너무 많네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016436.1.13",
          "form": "룸메이트랑 같이 생활해본적있나요?",
          "original_form": "룸메이트랑 같이 생활해본적있나요?",
          "speaker_id": "1",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016436.1.14",
          "form": "네! 한국인 중국인 일본인 다 겪어봤어요ㅎㅎ",
          "original_form": "네! 한국인 중국인 일본인 다 겪어봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016436.1.15",
          "form": "글로벌하시네요",
          "original_form": "글로벌하시네요",
