      "utterance": [
        {
          "id": "MDRW2100034701.1.1",
          "form": "이혼가정편견이 있구나 몰랏네",
          "original_form": "이혼가정편견이 있구나 몰랏네",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100034701.1.2",
          "form": "편견이라면 편견일수도",
          "original_form": "편견이라면 편견일수도",
          "speaker_id": "1",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100034701.1.3",
          "form": "아버지의 바람기를 물려받았나 걱정하는거 자체가",
          "original_form": "아버지의 바람기를 물려받았나 걱정하는거 자체가",
          "speaker_id": "1",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100034701.1.4",
          "form": "세간엔 별게많군",
          "original_form": "세간엔 별게많군",
          "speaker_id": "2",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100034701.1.5",
          "form": "엥 긍가..",
          "original_form": "엥 긍가..",
          "speaker_id": "2",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100034701.1.6",
          "form": "여하간 이번에 한전은 이혼가정 아니라서 그냥 맘이 편하대여",
          "original_form": "여하간 이번에 한전은 이혼가정 아니라서 그냥 맘이 편하대여",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100034701.1.7",
          "form": "아 사진도 봄",
          "original_form": "아 사진도 봄",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100034701.1.8",
          "form": "오 잘생김?",
          "original_form": "오 잘생김?",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100034701.1.9",
          "form": "음 내동생이 보여주기전에 중하랬음",
          "original_form": "음 내동생이 보여주기전에 중하랬음",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100034701.1.10",
          "form": "근데 음 왜 그런지 알거같...",
          "original_form": "근데 음 왜 그런지 알거같...",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100034701.1.11",
          "form": "ㅋㅋㅋㄲ",
          "original_form": "ㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100034701.1.12",
          "form": "외모는 안중요하군ㅋㅋ",
          "original_form": "외모는 안중요하군ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100034701.1.13",
          "form": "못생긴건아님 비호감상도 아님!ㅋㅋㅋ",
          "original_form": "못생긴건아님 비호감상도 아님!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100034701.1.14",
          "form": "단지...",
          "original_form": "단지...",
          "speaker_id": "1",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100034701.1.15",
          "form": "단지?",
          "original_form": "단지?",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100034701.1.16",
          "form": "평범하게 생김 과하게",
          "original_form": "평범하게 생김 과하게",
          "speaker_id": "1",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100034701.1.17",
          "form": "동네오빠나 교회오빠다운 훈훈함",
          "original_form": "동네오빠나 교회오빠다운 훈훈함",
          "speaker_id": "1",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100034701.1.18",
          "form": "서글서글하게생겼다",
          "original_form": "서글서글하게생겼다",
          "speaker_id": "1",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100034701.1.19",
          "form": "시원시원하게  생겼다",
          "original_form": "시원시원하게  생겼다",
          "speaker_id": "1",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100034701.1.20",
          "form": "이런게 없는 얼굴",
          "original_form": "이런게 없는 얼굴",
          "speaker_id": "1",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100034701.1.21",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100034701.1.22",
          "form": "그래도 성격이 좋대잖아",
          "original_form": "그래도 성격이 좋대잖아",
          "speaker_id": "2",
          "time": "20210828 17:56"
        },
        {
          "id": "MDRW2100034701.1.23",
          "form": "사진 5장넘게 보여줬는데 기억에남는게 하나도없어 ㅋㅋ",
          "original_form": "사진 5장넘게 보여줬는데 기억에남는게 하나도없어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:56"
        },
        {
          "id": "MDRW2100034701.1.24",
          "form": "그 경찰서에서 범인 줄세우듯 세워놓고 찾으래도 못찾을듯 ㅋㅋ",
          "original_form": "그 경찰서에서 범인 줄세우듯 세워놓고 찾으래도 못찾을듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034701.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034701.1.26",
          "form": "너무 박한  평가아니냔",
          "original_form": "너무 박한  평가아니냔",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034701.1.27",
          "form": "내가봐도 그럴거같긴함",
          "original_form": "내가봐도 그럴거같긴함",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034701.1.28",
          "form": "그러게요 ㅋㅋ근데 진짜 그렇게 생김!",
          "original_form": "그러게요 ㅋㅋ근데 진짜 그렇게 생김!",
          "speaker_id": "1",
          "time": "20210828 17:59"
        }
      ]
    }
  ]
