      "utterance": [
        {
          "id": "MDRW2100037415.1.1",
          "form": "안녕하세요 배민에서 맛집 배달 많이 하시는 편이세요?",
          "original_form": "안녕하세요 배민에서 맛집 배달 많이 하시는 편이세요?",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100037415.1.2",
          "form": "네 거의 중독이여ㅠ",
          "original_form": "네 거의 중독이여ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100037415.1.3",
          "form": "배민에서 제일 잘 드시는 거 뭐예요?",
          "original_form": "배민에서 제일 잘 드시는 거 뭐예요?",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100037415.1.4",
          "form": "저는 피자 좋아해요",
          "original_form": "저는 피자 좋아해요",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100037415.1.5",
          "form": "저도 피자  치킨 많이 시켜요",
          "original_form": "저도 피자  치킨 많이 시켜요",
          "speaker_id": "2",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100037415.1.6",
          "form": "오늘도 시키셨나요?",
          "original_form": "오늘도 시키셨나요?",
          "speaker_id": "2",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100037415.1.7",
          "form": "아뇨 오늘은 안 시켰어요",
          "original_form": "아뇨 오늘은 안 시켰어요",
          "speaker_id": "1",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100037415.1.8",
          "form": "그렇군여",
          "original_form": "그렇군여",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100037415.1.9",
          "form": "전 내일 시키려고요",
          "original_form": "전 내일 시키려고요",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100037415.1.10",
          "form": "가족이랑 같이 드시나요?",
          "original_form": "가족이랑 같이 드시나요?",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100037415.1.11",
          "form": "네 같이 먹어여",
          "original_form": "네 같이 먹어여",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100037415.1.12",
          "form": "님도요?",
          "original_form": "님도요?",
          "speaker_id": "2",
          "time": "20210831 23:36"
        },
        {
          "id": "MDRW2100037415.1.13",
          "form": "네 저도 가족이랑 같이 먹어요",
          "original_form": "네 저도 가족이랑 같이 먹어요",
          "speaker_id": "1",
          "time": "20210831 23:36"
        },
        {
          "id": "MDRW2100037415.1.14",
          "form": "참 맛나요ㅎ",
          "original_form": "참 맛나요ㅎ",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100037415.1.15",
          "form": "네 살찔 걱정만 없으면 좋겠어요",
          "original_form": "네 살찔 걱정만 없으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100037415.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100037415.1.17",
          "form": "누가 개발해줬으면",
          "original_form": "누가 개발해줬으면",
          "speaker_id": "2",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100037415.1.18",
          "form": "배고프네요 밥먹고싶어요",
          "original_form": "배고프네요 밥먹고싶어요",
          "speaker_id": "1",
          "time": "20210901 00:45"
        }
      ]
