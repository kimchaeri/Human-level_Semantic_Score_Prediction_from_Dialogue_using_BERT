      "utterance": [
        {
          "id": "MDRW2100019830.1.1",
          "form": "여행은 좋아하시나요?",
          "original_form": "여행은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019830.1.2",
          "form": "저 제가 익숙한 곳으로 가는 여행만 좋아해요 히히",
          "original_form": "저 제가 익숙한 곳으로 가는 여행만 좋아해요 히히",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019830.1.3",
          "form": "제가 아는 곳에서 새로운 데를 찾고싶은 맴...",
          "original_form": "제가 아는 곳에서 새로운 데를 찾고싶은 맴...",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019830.1.4",
          "form": "아하 주로 어디 지역 여행하세요??",
          "original_form": "아하 주로 어디 지역 여행하세요??",
          "speaker_id": "1",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019830.1.5",
          "form": "저 보통 강릉이나 경주요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 보통 강릉이나 경주요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100019830.1.6",
          "form": "진짜 생각해보니 맨날 가던데만 가네유",
          "original_form": "진짜 생각해보니 맨날 가던데만 가네유",
          "speaker_id": "2",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100019830.1.7",
          "form": "강릉 어떠셨어요? 한번도 안 가봐서요ㅋㅋ",
          "original_form": "강릉 어떠셨어요? 한번도 안 가봐서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100019830.1.8",
          "form": "저는 바다를 좋아해서 ㅋㅋㅋㅋ",
          "original_form": "저는 바다를 좋아해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100019830.1.9",
          "form": "조용하고 좋았어요!",
          "original_form": "조용하고 좋았어요!",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100019830.1.10",
          "form": "카페거리라 카페에서 바다 멍때리기 최고",
          "original_form": "카페거리라 카페에서 바다 멍때리기 최고",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100019830.1.11",
          "form": "아ㅋㅋ바다 좋죠!!",
          "original_form": "아ㅋㅋ바다 좋죠!!",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100019830.1.12",
          "form": "저도 카페거리 한 번 가봤는데 사람 많을 때 가서 그런지 북적북적 하더라구요",
          "original_form": "저도 카페거리 한 번 가봤는데 사람 많을 때 가서 그런지 북적북적 하더라구요",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100019830.1.13",
          "form": "저도 조용한 걸 즐기려고 했는데ㅠ",
          "original_form": "저도 조용한 걸 즐기려고 했는데ㅠ",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100019830.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아 그쵸 아무래도 휴가철이면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아 그쵸 아무래도 휴가철이면",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100019830.1.15",
          "form": "근데 진짜 조용한 관광지는 어딨는건지 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 진짜 조용한 관광지는 어딨는건지 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100019830.1.16",
          "form": "ㅋㅋㅋㅋㅋ그러게요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ그러게요ㅠ",
          "speaker_id": "1",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100019830.1.17",
          "form": "저도 예전에 강원도 가봤는데 9월쯤 가서 그런지",
          "original_form": "저도 예전에 강원도 가봤는데 9월쯤 가서 그런지",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100019830.1.18",
          "form": "메밀꽃 펴서 예쁘더라구요",
          "original_form": "메밀꽃 펴서 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100019830.1.19",
          "form": "우와 메밀꽃 진짜 예쁘겠어요",
          "original_form": "우와 메밀꽃 진짜 예쁘겠어요",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100019830.1.20",
          "form": "곧이네요 9월이면",
          "original_form": "곧이네요 9월이면",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100019830.1.21",
          "form": "9월엔... 좀 풀리겠지해서 여행 가려고했는데 ㅎ....",
          "original_form": "9월엔... 좀 풀리겠지해서 여행 가려고했는데 ㅎ....",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100019830.1.22",
          "form": "이번에도 안되겠군요ㅜ",
          "original_form": "이번에도 안되겠군요ㅜ",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100019830.1.23",
          "form": "그러게요ㅠㅠㅠ 가을쯤이면 괜찮겠지 했는데",
          "original_form": "그러게요ㅠㅠㅠ 가을쯤이면 괜찮겠지 했는데",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100019830.1.24",
          "form": "여행 쉽게 갈 수 없는 상황이라 아쉽네요",
          "original_form": "여행 쉽게 갈 수 없는 상황이라 아쉽네요",
          "speaker_id": "1",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100019830.1.25",
          "form": "맞아요 빨리 쉽게 떠나고싶네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 빨리 쉽게 떠나고싶네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100019830.1.26",
          "form": "코로나 이후에 어디 먼저 가보고 싶으세요?",
          "original_form": "코로나 이후에 어디 먼저 가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100019830.1.27",
          "form": "국내는 경주부터요...",
          "original_form": "국내는 경주부터요...",
          "speaker_id": "2",
          "time": "20210829 07:24"
        },
        {
          "id": "MDRW2100019830.1.28",
          "form": "해외는 일본..!",
          "original_form": "해외는 일본..!",
          "speaker_id": "2",
          "time": "20210829 07:24"
        },
        {
          "id": "MDRW2100019830.1.29",
          "form": "아하 저는 대만부터 가고 싶어요ㅠㅠ",
          "original_form": "아하 저는 대만부터 가고 싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100019830.1.30",
          "form": "하 전 대만 한번도 안가봤어요",
          "original_form": "하 전 대만 한번도 안가봤어요",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100019830.1.31",
          "form": "대만은 어디가 좋은가요??",
          "original_form": "대만은 어디가 좋은가요??",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100019830.1.32",
          "form": "지우펀이랑 시먼딩 가봤는데",
          "original_form": "지우펀이랑 시먼딩 가봤는데",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100019830.1.33",
          "form": "볼 거리 먹을 거리 구경하는 건 시먼딩이 편했어요",
          "original_form": "볼 거리 먹을 거리 구경하는 건 시먼딩이 편했어요",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100019830.1.34",
          "form": "지우펀은 사람이 워낙 많아서ㅠ",
          "original_form": "지우펀은 사람이 워낙 많아서ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100019830.1.35",
          "form": "시먼딩... 기억합니다요",
          "original_form": "시먼딩... 기억합니다요",
          "speaker_id": "2",
          "time": "20210830 08:06"
        },
        {
          "id": "MDRW2100019830.1.36",
          "form": "왜냐면 일본갔다가 대만갈거거든요",
          "original_form": "왜냐면 일본갔다가 대만갈거거든요",
          "speaker_id": "2",
          "time": "20210830 08:06"
        },
        {
          "id": "MDRW2100019830.1.37",
          "form": "좋네요!!",
          "original_form": "좋네요!!",
          "speaker_id": "1",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100019830.1.38",
          "form": "대만 가면 일본 느낌도 있어서 일본 갔다가 가는 거 좋은 루트인 것 같아요ㅠㅠ",
          "original_form": "대만 가면 일본 느낌도 있어서 일본 갔다가 가는 거 좋은 루트인 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100019830.1.39",
          "form": "아 정말요? 얻어걸린 좋은 루틐ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 정말요? 얻어걸린 좋은 루틐ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100019830.1.40",
          "form": "개이득이네요",
          "original_form": "개이득이네요",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100019830.1.41",
          "form": "ㅋㅋㅋㅋ그렇군요",
          "original_form": "ㅋㅋㅋㅋ그렇군요",
          "speaker_id": "1",
          "time": "20210831 01:00"
        },
        {
          "id": "MDRW2100019830.1.42",
          "form": "저도 코로나 이후에 일본-대만 순서로 한번 가보고 싶네요",
          "original_form": "저도 코로나 이후에 일본-대만 순서로 한번 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210831 01:01"
        },
        {
          "id": "MDRW2100019830.1.43",
          "form": "진짜 너무 재밌을거같아요 ㅋㅋㅋㅋ",
          "original_form": "진짜 너무 재밌을거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100019830.1.44",
          "form": "코로나 끜나면 사람들 우르르 여행갈것 같아요",
          "original_form": "코로나 끜나면 사람들 우르르 여행갈것 같아요",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100019830.1.45",
          "form": "그러게요ㅋㅋㅋㅠ 다들 여행가려고 돈 열심히 모아 두더라구요",
          "original_form": "그러게요ㅋㅋㅋㅠ 다들 여행가려고 돈 열심히 모아 두더라구요",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100019830.1.46",
          "form": "ㅋㅋㅋㅋㅋ 전... 배달로 탕진하는데....",
          "original_form": "ㅋㅋㅋㅋㅋ 전... 배달로 탕진하는데....",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100019830.1.47",
          "form": "ㅋㅋㅋㅋ그건 저도 그래요..",
          "original_form": "ㅋㅋㅋㅋ그건 저도 그래요..",
          "speaker_id": "1",
