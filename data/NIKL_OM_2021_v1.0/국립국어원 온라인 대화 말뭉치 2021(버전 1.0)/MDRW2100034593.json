      "utterance": [
        {
          "id": "MDRW2100034593.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ아ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100034593.1.2",
          "form": "근데 확실히 식욕이 없는사람이 마른편인거같아요 \r\n당연한얘기지만ㅋㅋ",
          "original_form": "근데 확실히 식욕이 없는사람이 마른편인거같아요 \r\n당연한얘기지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.3",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.4",
          "form": "식욕없는 연예인들 은근 있더라구요!",
          "original_form": "식욕없는 연예인들 은근 있더라구요!",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.5",
          "form": "음식에 대한 관심차이가 좌우하는거같아요",
          "original_form": "음식에 대한 관심차이가 좌우하는거같아요",
          "speaker_id": "2",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.6",
          "form": "슬리피나.. 이던이나..",
          "original_form": "슬리피나.. 이던이나..",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.7",
          "form": "그니까요 귀찮아서 안먹는다",
          "original_form": "그니까요 귀찮아서 안먹는다",
          "speaker_id": "2",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.8",
          "form": "맞아요 제관심좀 팔고 싶어요",
          "original_form": "맞아요 제관심좀 팔고 싶어요",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.9",
          "form": "이러더라구요",
          "original_form": "이러더라구요",
          "speaker_id": "2",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.10",
          "form": "어떻게 귀찮을수있죠??!!!",
          "original_form": "어떻게 귀찮을수있죠??!!!",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.11",
          "form": "아ㅋㅋㅋㅋ제 관심도요ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ제 관심도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100034593.1.12",
          "form": "그니까요 먹는게 귀찮다니ㅋㅋ",
          "original_form": "그니까요 먹는게 귀찮다니ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100034593.1.13",
          "form": "부러워요ㅠㅋㅋㅋㅋ",
          "original_form": "부러워요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100034593.1.14",
          "form": "이해할수없어요ㅋㅋㅋㅋ",
          "original_form": "이해할수없어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100034593.1.15",
          "form": "안찔수밖에없는.",
          "original_form": "안찔수밖에없는.",
          "speaker_id": "1",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100034593.1.16",
          "form": "관심사가 다른데만 있나봐여",
          "original_form": "관심사가 다른데만 있나봐여",
          "speaker_id": "1",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100034593.1.17",
          "form": "그니까요 한번 그렇게 살아보고싶네요",
          "original_form": "그니까요 한번 그렇게 살아보고싶네요",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100034593.1.18",
          "form": "그런가봐요ㅠ",
          "original_form": "그런가봐요ㅠ",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100034593.1.19",
          "form": "근데 입 짧은사람들은",
          "original_form": "근데 입 짧은사람들은",
          "speaker_id": "2",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100034593.1.20",
          "form": "어릴때부터 그런 경우가  많더라구요",
          "original_form": "어릴때부터 그런 경우가  많더라구요",
          "speaker_id": "2",
          "time": "20210828 15:59"
