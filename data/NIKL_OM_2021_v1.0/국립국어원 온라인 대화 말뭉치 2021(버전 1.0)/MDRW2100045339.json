      "utterance": [
        {
          "id": "MDRW2100045339.1.1",
          "form": "모바일 게임 좋아하세요?",
          "original_form": "모바일 게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045339.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045339.1.3",
          "form": "로오히 해요",
          "original_form": "로오히 해요",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045339.1.4",
          "form": "아 저랑 같네요",
          "original_form": "아 저랑 같네요",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045339.1.5",
          "form": "최애캐는..?",
          "original_form": "최애캐는..?",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045339.1.6",
          "form": "로드?",
          "original_form": "로드?",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045339.1.7",
          "form": "프라우",
          "original_form": "프라우",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045339.1.8",
          "form": "로드 좋아요",
          "original_form": "로드 좋아요",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045339.1.9",
          "form": "착한사람",
          "original_form": "착한사람",
          "speaker_id": "1",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045339.1.10",
          "form": "맞아요 좋은 사람",
          "original_form": "맞아요 좋은 사람",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045339.1.11",
          "form": "저는 좀 갈팡질팡 하는데",
          "original_form": "저는 좀 갈팡질팡 하는데",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045339.1.12",
          "form": "암헬가 엄청 좋아하거든요",
          "original_form": "암헬가 엄청 좋아하거든요",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045339.1.13",
          "form": "그것도 괜찮아요",
          "original_form": "그것도 괜찮아요",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.14",
          "form": "뭐야 이거",
          "original_form": "뭐야 이거",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.15",
          "form": "암헬가 키우시나오?",
          "original_form": "암헬가 키우시나오?",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.16",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.17",
          "form": "들어봐요",
          "original_form": "들어봐요",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.18",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.19",
          "form": "잔몹 정리계의 신 !",
          "original_form": "잔몹 정리계의 신 !",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.20",
          "form": "암프라우 사러갈껀데요",
          "original_form": "암프라우 사러갈껀데요",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.21",
          "form": "탑 티어 !",
          "original_form": "탑 티어 !",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.22",
          "form": "쓸고 또 쓸어 !",
          "original_form": "쓸고 또 쓸어 !",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.23",
          "form": "풀라플이 있는디",
          "original_form": "풀라플이 있는디",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.24",
          "form": "왜 암헬가 키움",
          "original_form": "왜 암헬가 키움",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.25",
          "form": "암프라우는 7천7만777...골드고",
          "original_form": "암프라우는 7천7만777...골드고",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.26",
          "form": "풀프라우는 무료",
          "original_form": "풀프라우는 무료",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.27",
          "form": "암헬가는 50만 명성 ! 아 좋다 !",
          "original_form": "암헬가는 50만 명성 ! 아 좋다 !",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.28",
          "form": "같은 효과의 굿스커",
          "original_form": "같은 효과의 굿스커",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.29",
          "form": "슈터",
          "original_form": "슈터",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.30",
          "form": "풀라플이랑 연계해서 쓰고",
          "original_form": "풀라플이랑 연계해서 쓰고",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045339.1.31",
          "form": "안죽은 애들 있으면 한번더 스킬 가능 !",
          "original_form": "안죽은 애들 있으면 한번더 스킬 가능 !",
          "speaker_id": "2",
          "time": "20210908 02:12"
        }
      ]
    }
  ]
