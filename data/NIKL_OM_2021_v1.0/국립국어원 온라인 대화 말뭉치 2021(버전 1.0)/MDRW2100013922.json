      "utterance": [
        {
          "id": "MDRW2100013922.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013922.1.2",
          "form": "아...거북이 한마리요",
          "original_form": "아...거북이 한마리요",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013922.1.3",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013922.1.4",
          "form": "거북이 귀엽겠어요",
          "original_form": "거북이 귀엽겠어요",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013922.1.5",
          "form": "네 작아요",
          "original_form": "네 작아요",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013922.1.6",
          "form": "주인을 알아보는듯한 느낌적인 느낌이랄까요",
          "original_form": "주인을 알아보는듯한 느낌적인 느낌이랄까요",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013922.1.7",
          "form": "거북이가 사람을 알아보는군요",
          "original_form": "거북이가 사람을 알아보는군요",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100013922.1.8",
          "form": "처음 알았어요",
          "original_form": "처음 알았어요",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100013922.1.9",
          "form": "아..그냥 느낌이요..ㅎ",
          "original_form": "아..그냥 느낌이요..ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100013922.1.10",
          "form": "거북이 이름이 뭐예요",
          "original_form": "거북이 이름이 뭐예요",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100013922.1.11",
          "form": "대북이요 ㅎ",
          "original_form": "대북이요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100013922.1.12",
          "form": "키우는 동물 있으세요",
          "original_form": "키우는 동물 있으세요",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100013922.1.13",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100013922.1.14",
          "form": "거북이는 손이 많이안가서 좋아요",
          "original_form": "거북이는 손이 많이안가서 좋아요",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100013922.1.15",
          "form": "가끔 어항만 씻어주면되고",
          "original_form": "가끔 어항만 씻어주면되고",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100013922.1.16",
          "form": "거북이가 아픈적이 있나요?",
          "original_form": "거북이가 아픈적이 있나요?",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100013922.1.17",
          "form": "두마리였는데 한마리가 죽었어요",
          "original_form": "두마리였는데 한마리가 죽었어요",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100013922.1.18",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100013922.1.19",
          "form": "요놈은...가끔 밥을안먹을때 아픈듯하긴해요",
          "original_form": "요놈은...가끔 밥을안먹을때 아픈듯하긴해요",
          "speaker_id": "2",
          "time": "20210825 23:45"
        }
      ]
