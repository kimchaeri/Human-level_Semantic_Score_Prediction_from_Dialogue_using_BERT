      "utterance": [
        {
          "id": "MDRW2100018202.1.1",
          "form": "안녕하세요 연예인 좋아하시나요",
          "original_form": "안녕하세요 연예인 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018202.1.2",
          "form": "안녕하세요.\r\n아이돌은 잘 모르고 드라마 배우나 예능 프로그램에 나오는 연예인들 좋아해요",
          "original_form": "안녕하세요.\r\n아이돌은 잘 모르고 드라마 배우나 예능 프로그램에 나오는 연예인들 좋아해요",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018202.1.3",
          "form": "아하! 요즘 무슨 프로그램 시청하시나요",
          "original_form": "아하! 요즘 무슨 프로그램 시청하시나요",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018202.1.4",
          "form": "가장 최근에 본건 펜트하우스 에요\r\n좀 막장이긴하지만 재밌어요",
          "original_form": "가장 최근에 본건 펜트하우스 에요\r\n좀 막장이긴하지만 재밌어요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018202.1.5",
          "form": "전 드라마 최근에 본게 그 공부하는 드라마",
          "original_form": "전 드라마 최근에 본게 그 공부하는 드라마",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018202.1.6",
          "form": "스카이캐슬! 이 마지막이네요",
          "original_form": "스카이캐슬! 이 마지막이네요",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018202.1.7",
          "form": "넷플릭스 같은거 결제하셨나용",
          "original_form": "넷플릭스 같은거 결제하셨나용",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018202.1.8",
          "form": "name1도 드라마 많이 안보시는군요 ㅋㅋ",
          "original_form": "&name1&도 드라마 많이 안보시는군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018202.1.9",
          "form": "저는 안했구 종종 친구거 빌려봐요",
          "original_form": "저는 안했구 종종 친구거 빌려봐요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018202.1.10",
          "form": "저는 웨이브 결제했는뎅",
          "original_form": "저는 웨이브 결제했는뎅",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018202.1.11",
          "form": "6개월 정도 돈만 나가고 있네용",
          "original_form": "6개월 정도 돈만 나가고 있네용",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018202.1.12",
          "form": "유튜브는 많이 보세요?",
          "original_form": "유튜브는 많이 보세요?",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018202.1.13",
          "form": "영상 자체를 많이보는 편은 아닌데 동물 관련 영상은 종종봐요",
          "original_form": "영상 자체를 많이보는 편은 아닌데 동물 관련 영상은 종종봐요",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018202.1.14",
          "form": "name1은 어떤류 좋아하시나요?",
          "original_form": "&name1&은 어떤류 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018202.1.15",
          "form": "저는 요리하는거랑",
          "original_form": "저는 요리하는거랑",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018202.1.16",
          "form": "아이돌 영상들 자주 봐요!",
          "original_form": "아이돌 영상들 자주 봐요!",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018202.1.17",
          "form": "계속 알고리즘에 그것들 위주로 뜨더라고요",
          "original_form": "계속 알고리즘에 그것들 위주로 뜨더라고요",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018202.1.18",
          "form": "아 요리 좋아하셔서 닉네임도 name1이신가요?",
          "original_form": "아 요리 좋아하셔서 닉네임도 &name1&이신가요?",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018202.1.19",
          "form": "요리 좋아하긴 하는데 ㅋㅋ 그냥 생각나는 웹툰 이름으로 햤어용 ㅎㅎ",
          "original_form": "요리 좋아하긴 하는데 ㅋㅋ 그냥 생각나는 웹툰 이름으로 햤어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018202.1.20",
          "form": "아이돌은 누구 좋아하시는건가요?",
          "original_form": "아이돌은 누구 좋아하시는건가요?",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018202.1.21",
          "form": "아이돌은 딱히 없는데 다 두루두루 뜨면 많이 봐용",
          "original_form": "아이돌은 딱히 없는데 다 두루두루 뜨면 많이 봐용",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018202.1.22",
          "form": "요즘 오마이걸이라는 아이돌 많이 보고 있어요 ㅋㅋ",
          "original_form": "요즘 오마이걸이라는 아이돌 많이 보고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018202.1.23",
          "form": "아 오마이걸은 노래 돌핀 좋더라구요",
          "original_form": "아 오마이걸은 노래 돌핀 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018202.1.24",
          "form": "후렴구가 계속 귀에남아요",
          "original_form": "후렴구가 계속 귀에남아요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018202.1.25",
          "form": "맞아여 ㅎㅎ 유튜브가 워낙 잘돼있어서",
          "original_form": "맞아여 ㅎㅎ 유튜브가 워낙 잘돼있어서",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018202.1.26",
          "form": "뭘 봐도 다 재밌긴 하더라구요",
          "original_form": "뭘 봐도 다 재밌긴 하더라구요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018202.1.27",
          "form": "네 맞아요 ㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018202.1.28",
          "form": "이번에 디즈니도 들어온다던데",
          "original_form": "이번에 디즈니도 들어온다던데",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018202.1.29",
          "form": "디즈니 구독하실생각 있으신가요?",
          "original_form": "디즈니 구독하실생각 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 23:27"
        }
      ]
    }
