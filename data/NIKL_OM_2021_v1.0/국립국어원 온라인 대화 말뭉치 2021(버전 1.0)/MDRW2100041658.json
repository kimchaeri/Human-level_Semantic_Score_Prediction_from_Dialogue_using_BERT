        {
          "id": "MDRW2100041658.1.1",
          "form": "아이스크림 좋아하세요?",
          "original_form": "아이스크림 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041658.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041658.1.3",
          "form": "주로 어떤 아이스크림 좋아하시나요?",
          "original_form": "주로 어떤 아이스크림 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041658.1.4",
          "form": "저는 바닐라 아이스크림이나 호두맛 아이스크림 좋아해요",
          "original_form": "저는 바닐라 아이스크림이나 호두맛 아이스크림 좋아해요",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041658.1.5",
          "form": "아 저도 호두맛 좋아해요!",
          "original_form": "아 저도 호두맛 좋아해요!",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041658.1.6",
          "form": "달달하고 고소해서 맛있죠",
          "original_form": "달달하고 고소해서 맛있죠",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041658.1.7",
          "form": "빙과류는 안좋아하세요?",
          "original_form": "빙과류는 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041658.1.8",
          "form": "빙과류요?",
          "original_form": "빙과류요?",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041658.1.9",
          "form": "네 얼음들어간거나 쭈쭈바 같은거요",
          "original_form": "네 얼음들어간거나 쭈쭈바 같은거요",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041658.1.10",
          "form": "쭈쭈바는 안먹은지 오래되었어요",
          "original_form": "쭈쭈바는 안먹은지 오래되었어요",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041658.1.11",
          "form": "저는 여름엔 늘 탱크보이 먹거든요 ㅋㅋ",
          "original_form": "저는 여름엔 늘 탱크보이 먹거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041658.1.12",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041658.1.13",
          "form": "엄청 시원해요 배맛이구요",
          "original_form": "엄청 시원해요 배맛이구요",
