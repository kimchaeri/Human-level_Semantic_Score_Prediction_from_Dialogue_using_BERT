      "utterance": [
        {
          "id": "MDRW2100005737.1.1",
          "form": "특별 방역대책을. ...",
          "original_form": "특별 방역대책을. ...",
          "speaker_id": "2",
          "time": "20210907 10:22"
        },
        {
          "id": "MDRW2100005737.1.2",
          "form": "하긴 하나요...?",
          "original_form": "하긴 하나요...?",
          "speaker_id": "2",
          "time": "20210907 10:22"
        },
        {
          "id": "MDRW2100005737.1.3",
          "form": "그냥 거리두기만 하는거같은데",
          "original_form": "그냥 거리두기만 하는거같은데",
          "speaker_id": "2",
          "time": "20210907 10:22"
        },
        {
          "id": "MDRW2100005737.1.4",
          "form": "전 나름 방역대책으로",
          "original_form": "전 나름 방역대책으로",
          "speaker_id": "1",
          "time": "20210907 10:23"
        },
        {
          "id": "MDRW2100005737.1.5",
          "form": "명절에 집에 안가요 ㅋㅋ",
          "original_form": "명절에 집에 안가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:23"
        },
        {
          "id": "MDRW2100005737.1.6",
          "form": "ㅋㅋㅋㅋ저희는 시골안간지 몇년돼서",
          "original_form": "ㅋㅋㅋㅋ저희는 시골안간지 몇년돼서",
          "speaker_id": "2",
          "time": "20210907 10:24"
        },
        {
          "id": "MDRW2100005737.1.7",
          "form": "마지막으로 간지가 언젠지도 모르겟어요",
          "original_form": "마지막으로 간지가 언젠지도 모르겟어요",
          "speaker_id": "2",
          "time": "20210907 10:25"
        },
        {
          "id": "MDRW2100005737.1.8",
          "form": "그럼 이번추석은 제주도에서 혼자계시는건가요?",
          "original_form": "그럼 이번추석은 제주도에서 혼자계시는건가요?",
          "speaker_id": "2",
          "time": "20210907 10:25"
        },
        {
          "id": "MDRW2100005737.1.9",
          "form": "네 친구가 놀러온대뇨",
          "original_form": "네 친구가 놀러온대뇨",
          "speaker_id": "1",
          "time": "20210907 10:26"
        },
        {
          "id": "MDRW2100005737.1.10",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210907 10:26"
        },
        {
          "id": "MDRW2100005737.1.11",
          "form": "최고다~~ 친구분도 좋으시겠어욬ㅋㅋ",
          "original_form": "최고다~~ 친구분도 좋으시겠어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100005737.1.12",
          "form": "친구가 제주살다니",
          "original_form": "친구가 제주살다니",
          "speaker_id": "2",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100005737.1.13",
          "form": "저라도 그 기회 못놓칠듯요 ㅋㅋㅋㅋ",
          "original_form": "저라도 그 기회 못놓칠듯요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100005737.1.14",
          "form": "다른 방역 대책은 명절이라 어렵지 않을까요",
          "original_form": "다른 방역 대책은 명절이라 어렵지 않을까요",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100005737.1.15",
          "form": "뭐 지금 거리두기 연장하고 또 뭐 백신 맞은사람에 한해 어쩌구 하긴하는데",
          "original_form": "뭐 지금 거리두기 연장하고 또 뭐 백신 맞은사람에 한해 어쩌구 하긴하는데",
          "speaker_id": "2",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100005737.1.16",
          "form": "의미가 있나 싶어요.",
          "original_form": "의미가 있나 싶어요.",
          "speaker_id": "2",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100005737.1.17",
          "form": "흠 전 내일 1차인데 긴장되네요",
          "original_form": "흠 전 내일 1차인데 긴장되네요",
          "speaker_id": "1",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100005737.1.18",
          "form": "와 저랑 비슷하게 맞으시네요",
          "original_form": "와 저랑 비슷하게 맞으시네요",
          "speaker_id": "2",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100005737.1.19",
          "form": "저 낼모레요",
          "original_form": "저 낼모레요",
          "speaker_id": "2",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100005737.1.20",
          "form": "그럼 엄빠집 근처에서 맞고 내려가시는거에요",
          "original_form": "그럼 엄빠집 근처에서 맞고 내려가시는거에요",
          "speaker_id": "2",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100005737.1.21",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100005737.1.22",
          "form": "그럼 예방접종 부작용 잡담을 ㅋㅋ",
          "original_form": "그럼 예방접종 부작용 잡담을 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100005737.1.23",
          "form": "아뇨 돌아가서 바로 맞아요",
          "original_form": "아뇨 돌아가서 바로 맞아요",
          "speaker_id": "1",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100005737.1.24",
          "form": "그럼 오늘 가시나요?",
          "original_form": "그럼 오늘 가시나요?",
          "speaker_id": "2",
          "time": "20210907 10:34"
        },
        {
          "id": "MDRW2100005737.1.25",
          "form": "내일 오후요",
          "original_form": "내일 오후요",
          "speaker_id": "1",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100005737.1.26",
          "form": "가자마자 바로 맞고 쉬시는거에요?",
          "original_form": "가자마자 바로 맞고 쉬시는거에요?",
          "speaker_id": "2",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100005737.1.27",
          "form": "동생은 늦게 예약해서 추석이후에 맞는데",
          "original_form": "동생은 늦게 예약해서 추석이후에 맞는데",
          "speaker_id": "1",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100005737.1.28",
          "form": "피곤하시겠어요",
          "original_form": "피곤하시겠어요",
          "speaker_id": "2",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100005737.1.29",
          "form": "아빠가 맞고 오라고 난리",
