      "utterance": [
        {
          "id": "MDRW2100042955.1.1",
          "form": "점심 뭐 먹어?",
          "original_form": "점심 뭐 먹어?",
          "speaker_id": "1",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100042955.1.2",
          "form": "주꾸미볶음밥쓰",
          "original_form": "주꾸미볶음밥쓰",
          "speaker_id": "2",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100042955.1.3",
          "form": "급식은 안왔습니다 아직",
          "original_form": "급식은 안왔습니다 아직",
          "speaker_id": "2",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100042955.1.4",
          "form": "name1님 급식메뉴는 뭐쥬",
          "original_form": "&name1&님 급식메뉴는 뭐쥬",
          "speaker_id": "2",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100042955.1.5",
          "form": "제육볶음 앤 조랭이떡국",
          "original_form": "제육볶음 앤 조랭이떡국",
          "speaker_id": "1",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100042955.1.6",
          "form": "도시락 은 석식인가요",
          "original_form": "도시락 은 석식인가요",
          "speaker_id": "1",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100042955.1.7",
          "form": "아마..? 아직 안옴",
          "original_form": "아마..? 아직 안옴",
          "speaker_id": "2",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100042955.1.8",
          "form": "배송시각이 아쉽군",
          "original_form": "배송시각이 아쉽군",
          "speaker_id": "1",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100042955.1.9",
          "form": "다음날 점심으로 먹거나 해야겠네",
          "original_form": "다음날 점심으로 먹거나 해야겠네",
          "speaker_id": "1",
          "time": "20210907 12:46"
        },
        {
          "id": "MDRW2100042955.1.10",
          "form": "새벽배송을 못받으니까 ㅋㅋ",
          "original_form": "새벽배송을 못받으니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100042955.1.11",
          "form": "1층현관 비번이 문제야",
          "original_form": "1층현관 비번이 문제야",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100042955.1.12",
          "form": "그러게...",
          "original_form": "그러게...",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042955.1.13",
          "form": "뭐 안되면 저녁으로 먹어도 되고",
          "original_form": "뭐 안되면 저녁으로 먹어도 되고",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042955.1.14",
          "form": "밥을 1층 앞에 놓고 가달라고 할 수도 없고",
          "original_form": "밥을 1층 앞에 놓고 가달라고 할 수도 없고",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042955.1.15",
          "form": "매일오면 받아서 다음날 점심으로 먹어도 되고",
          "original_form": "매일오면 받아서 다음날 점심으로 먹어도 되고",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042955.1.16",
          "form": "그랴 저녁이든 다음날 점심이든",
          "original_form": "그랴 저녁이든 다음날 점심이든",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042955.1.17",
          "form": "1층에 놓고 가기도 하더라",
          "original_form": "1층에 놓고 가기도 하더라",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042955.1.18",
          "form": "컬리보면",
          "original_form": "컬리보면",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042955.1.19",
          "form": "걍 우리가 받아서 올라오면 되긴해",
          "original_form": "걍 우리가 받아서 올라오면 되긴해",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042955.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042955.1.21",
          "form": "한번 해보고 결정하지 뭐",
          "original_form": "한번 해보고 결정하지 뭐",
          "speaker_id": "1",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100042955.1.22",
          "form": "맞아 계속 먹을지 아닐지도 모르니까",
          "original_form": "맞아 계속 먹을지 아닐지도 모르니까",
          "speaker_id": "2",
          "time": "20210907 13:22"
        }
