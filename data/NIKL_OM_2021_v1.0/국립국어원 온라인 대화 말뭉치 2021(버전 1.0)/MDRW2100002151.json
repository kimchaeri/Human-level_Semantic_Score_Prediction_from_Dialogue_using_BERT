      "utterance": [
        {
          "id": "MDRW2100002151.1.1",
          "form": "잠이 너무 부족해",
          "original_form": "잠이 너무 부족해",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002151.1.2",
          "form": "후딱 자 그러면ㅋㅋㅋ",
          "original_form": "후딱 자 그러면ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002151.1.3",
          "form": "ㅋㅋㅋ잠이 보약이 맞긴 한가봐",
          "original_form": "ㅋㅋㅋ잠이 보약이 맞긴 한가봐",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002151.1.4",
          "form": "넥슬라이스 한대 맞고 기절했다 눈뜨면",
          "original_form": "넥슬라이스 한대 맞고 기절했다 눈뜨면",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002151.1.5",
          "form": "무병장수할것같은 기분 들걸",
          "original_form": "무병장수할것같은 기분 들걸",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002151.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ눈 뜰 수 있긴 한거지?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ눈 뜰 수 있긴 한거지?",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002151.1.7",
          "form": "그건 환자의 의지에 달렸습니다",
          "original_form": "그건 환자의 의지에 달렸습니다",
          "speaker_id": "2",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002151.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002151.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ내 목숨...소중해...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ내 목숨...소중해...",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002151.1.10",
          "form": "운동은 하는거 있어?",
          "original_form": "운동은 하는거 있어?",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002151.1.11",
          "form": "운동은 걷기와 숨쉬기면 충분하지 않아?",
          "original_form": "운동은 걷기와 숨쉬기면 충분하지 않아?",
          "speaker_id": "1",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002151.1.12",
          "form": "우리는 그걸 신진대사라고 부르기로 사회적 약속을 했어요 선생님",
          "original_form": "우리는 그걸 신진대사라고 부르기로 사회적 약속을 했어요 선생님",
          "speaker_id": "2",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002151.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002151.1.14",
          "form": "ㅋㄱㅋㅋㄱㅋㄱㅋ선돌이니..?",
          "original_form": "ㅋㄱㅋㅋㄱㅋㄱㅋ선돌이니..?",
          "speaker_id": "1",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002151.1.15",
          "form": "그게 모임",
          "original_form": "그게 모임",
          "speaker_id": "2",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002151.1.16",
          "form": "선바 팬,,,",
          "original_form": "선바 팬,,,",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002151.1.17",
          "form": "뭐 어쨋든",
          "original_form": "뭐 어쨋든",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002151.1.18",
          "form": "운동 뭐해??",
          "original_form": "운동 뭐해??",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002151.1.19",
          "form": "나도 숨쉬기ㄲㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 숨쉬기ㄲㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002151.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ나는 가끔 자전거 타",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ나는 가끔 자전거 타",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002151.1.21",
          "form": "오 나도 출퇴근 자전거",
          "original_form": "오 나도 출퇴근 자전거",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002151.1.22",
