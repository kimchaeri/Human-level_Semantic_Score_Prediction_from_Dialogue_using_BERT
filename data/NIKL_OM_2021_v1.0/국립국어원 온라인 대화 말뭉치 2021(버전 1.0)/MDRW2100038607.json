        {
          "id": "MDRW2100038607.1.1",
          "form": "최근 잘 산템있나요",
          "original_form": "최근 잘 산템있나요",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100038607.1.2",
          "form": "글쎄요 최근산건 쌀인데 ㅋㅋ 그냥 별 감흥없는?",
          "original_form": "글쎄요 최근산건 쌀인데 ㅋㅋ 그냥 별 감흥없는?",
          "speaker_id": "1",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100038607.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100038607.1.4",
          "form": "쌀 사셨나요 ㅋㅋ",
          "original_form": "쌀 사셨나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038607.1.5",
          "form": "전 구두샀어요",
          "original_form": "전 구두샀어요",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038607.1.6",
          "form": "오오 어떤 색깔인가요 검은색? 갈색?",
          "original_form": "오오 어떤 색깔인가요 검은색? 갈색?",
          "speaker_id": "1",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038607.1.7",
          "form": "브라운 샀어요 ㅋㅋ",
          "original_form": "브라운 샀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100038607.1.8",
          "form": "편하다고해서",
          "original_form": "편하다고해서",
          "speaker_id": "2",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100038607.1.9",
          "form": "로퍼인가요? ? 어떤 브랜드인가요",
          "original_form": "로퍼인가요? ? 어떤 브랜드인가요",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038607.1.10",
          "form": "전 락포트를 샀는데 생각보다 불편했어요",
          "original_form": "전 락포트를 샀는데 생각보다 불편했어요",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038607.1.11",
          "form": "네네 로퍼에용 인스타공구로 샀어용ㅋㅋ",
          "original_form": "네네 로퍼에용 인스타공구로 샀어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038607.1.12",
          "form": "아 락포트가 불편하다니!!",
          "original_form": "아 락포트가 불편하다니!!",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038607.1.13",
          "form": "ㅠ전그래서 모든 구두가 불편해요,,",
          "original_form": "ㅠ전그래서 모든 구두가 불편해요,,",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038607.1.14",
          "form": "아하ㅠㅠㅠㅠ",
          "original_form": "아하ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100038607.1.15",
          "form": "운동화 매니아이신가용",
          "original_form": "운동화 매니아이신가용",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100038607.1.16",
          "form": "그게 음 뭐랄가 ㅋㅋㅋ 구두신으면 발이 찢어질 것 같아서 운동화를 신는게 매나아인진 잘ㅋㅋ",
          "original_form": "그게 음 뭐랄가 ㅋㅋㅋ 구두신으면 발이 찢어질 것 같아서 운동화를 신는게 매나아인진 잘ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100038607.1.17",
          "form": "휠체어 매니아 이런 말은 없잖아욬",
          "original_form": "휠체어 매니아 이런 말은 없잖아욬",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100038607.1.18",
          "form": "발이 편해야해요 그죠ㅠㅠ",
          "original_form": "발이 편해야해요 그죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038607.1.19",
          "form": "구두가 아무리 이뻐도 제가 아프면 소용이 없어서 ㅋㅋ 락포트는 진짜 어쩌다가 신어요",
          "original_form": "구두가 아무리 이뻐도 제가 아프면 소용이 없어서 ㅋㅋ 락포트는 진짜 어쩌다가 신어요",
          "speaker_id": "1",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100038607.1.20",
          "form": "맞아요 발 아프면 만사 짜증나요",
          "original_form": "맞아요 발 아프면 만사 짜증나요",
