      "utterance": [
        {
          "id": "MDRW2100033709.1.1",
          "form": "저는 에어프라이기 사서 한동안 거기에 고기 구워먹었어요",
          "original_form": "저는 에어프라이기 사서 한동안 거기에 고기 구워먹었어요",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100033709.1.2",
          "form": "그건 소고기 구워야될거같고ㅎ",
          "original_form": "그건 소고기 구워야될거같고ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100033709.1.3",
          "form": "통삼겹 구워도 맛있어요",
          "original_form": "통삼겹 구워도 맛있어요",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100033709.1.4",
          "form": "에프있음 그것도 좋겠어요",
          "original_form": "에프있음 그것도 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100033709.1.5",
          "form": "요리하는거 좋아하세요?",
          "original_form": "요리하는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100033709.1.6",
          "form": "휴가나 주말 때는 가끔 하는데",
          "original_form": "휴가나 주말 때는 가끔 하는데",
          "speaker_id": "2",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100033709.1.7",
          "form": "평일에는 간단히 먹거나 차려준 음식 먹어요ㅎㅎ",
          "original_form": "평일에는 간단히 먹거나 차려준 음식 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033709.1.8",
          "form": "전 혼자라 제가가끔 해먹고ㅎ 인스턴트데워먹고",
          "original_form": "전 혼자라 제가가끔 해먹고ㅎ 인스턴트데워먹고",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033709.1.9",
          "form": "아 혼자 있으면 배달음식도 많이 먹게 되지 않나요?",
          "original_form": "아 혼자 있으면 배달음식도 많이 먹게 되지 않나요?",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033709.1.10",
          "form": "오히려 더 안시켜먹게 돼요",
          "original_form": "오히려 더 안시켜먹게 돼요",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033709.1.11",
          "form": "양이 많아서 남김 버리게되니",
          "original_form": "양이 많아서 남김 버리게되니",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033709.1.12",
          "form": "아 그럴 수도 있겠네요",
          "original_form": "아 그럴 수도 있겠네요",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033709.1.13",
          "form": "배달음식은 아무래도 양이 많으니",
          "original_form": "배달음식은 아무래도 양이 많으니",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033709.1.14",
          "form": "네 그래서 먹고싶어도 참을때가 많아요",
          "original_form": "네 그래서 먹고싶어도 참을때가 많아요",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033709.1.15",
          "form": "name2님은 배달자주시켜드세요?",
          "original_form": "&name2&님은 배달자주시켜드세요?",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033709.1.16",
          "form": "ㅎㅎ건강엔 좋겠어요",
          "original_form": "ㅎㅎ건강엔 좋겠어요",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033709.1.17",
          "form": "저도 가족들 있을때나 시켜먹고",
          "original_form": "저도 가족들 있을때나 시켜먹고",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033709.1.18",
          "form": "아니면 집에 있는 걸로 챙겨먹으려는 편이예요",
          "original_form": "아니면 집에 있는 걸로 챙겨먹으려는 편이예요",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033709.1.19",
          "form": "집밥이 더좋죠^^",
          "original_form": "집밥이 더좋죠^^",
          "speaker_id": "1",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033709.1.20",
          "form": "네네 집밥 최고!",
          "original_form": "네네 집밥 최고!",
