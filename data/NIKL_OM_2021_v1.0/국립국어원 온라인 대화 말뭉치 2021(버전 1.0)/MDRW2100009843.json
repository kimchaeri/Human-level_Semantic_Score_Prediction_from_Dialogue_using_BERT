        {
          "id": "MDRW2100009843.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009843.1.2",
          "form": "모바일 게임 하나 하고 있어요!",
          "original_form": "모바일 게임 하나 하고 있어요!",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009843.1.3",
          "form": "마이리플포레스트라고 농사짓는 게임이에요",
          "original_form": "마이리플포레스트라고 농사짓는 게임이에요",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009843.1.4",
          "form": "리얼팜 같은거요?",
          "original_form": "리얼팜 같은거요?",
          "speaker_id": "1",
          "time": "20210823 10:52"
        },
        {
          "id": "MDRW2100009843.1.5",
          "form": "비슷한 거 같아요!",
          "original_form": "비슷한 거 같아요!",
          "speaker_id": "2",
          "time": "20210824 01:07"
        },
        {
          "id": "MDRW2100009843.1.6",
          "form": "약간 동물의 숲이랑 더 비슷한 느낌이에요",
          "original_form": "약간 동물의 숲이랑 더 비슷한 느낌이에요",
          "speaker_id": "2",
          "time": "20210824 01:07"
        },
        {
          "id": "MDRW2100009843.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100009843.1.8",
          "form": "name1님은 어떤 게임 하시나요?",
          "original_form": "&name1&님은 어떤 게임 하시나요?",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100009843.1.9",
          "form": "폰게임뇨",
          "original_form": "폰게임뇨",
          "speaker_id": "1",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100009843.1.10",
          "form": "폰게임 어떤 거 하세요?",
          "original_form": "폰게임 어떤 거 하세요?",
          "speaker_id": "2",
