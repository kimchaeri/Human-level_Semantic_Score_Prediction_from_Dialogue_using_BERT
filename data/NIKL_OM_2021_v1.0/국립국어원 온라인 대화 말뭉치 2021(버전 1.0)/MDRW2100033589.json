      "utterance": [
        {
          "id": "MDRW2100033589.1.1",
          "form": "저 일좀하고올게요ㅎ",
          "original_form": "저 일좀하고올게요ㅎ",
          "speaker_id": "2",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100033589.1.2",
          "form": "이따얘기해요^^",
          "original_form": "이따얘기해요^^",
          "speaker_id": "2",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100033589.1.3",
          "form": "넵 ^^",
          "original_form": "넵 ^^",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100033589.1.4",
          "form": "오늘 요기요는 못하신건가요?",
          "original_form": "오늘 요기요는 못하신건가요?",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100033589.1.5",
          "form": "저 9시 땡하고 쿠폰받고 냉큼 사왔어요",
          "original_form": "저 9시 땡하고 쿠폰받고 냉큼 사왔어요",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100033589.1.6",
          "form": "오~~~  축하드립니다!!",
          "original_form": "오~~~  축하드립니다!!",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100033589.1.7",
          "form": "네ㅋ오늘은 우딜만했어요",
          "original_form": "네ㅋ오늘은 우딜만했어요",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100033589.1.8",
          "form": "우딜핫하던데  ㅎ",
          "original_form": "우딜핫하던데  ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100033589.1.9",
          "form": "픽업도하세요?",
          "original_form": "픽업도하세요?",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100033589.1.10",
          "form": "쿠폰은 이번달까진데 왜 핫한거죠?",
          "original_form": "쿠폰은 이번달까진데 왜 핫한거죠?",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100033589.1.11",
          "form": "픽업까지요ㅡ좀비쿠폰이고",
          "original_form": "픽업까지요ㅡ좀비쿠폰이고",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100033589.1.12",
          "form": "과자가열봉에 3520원인데 우친으로배달까지",
          "original_form": "과자가열봉에 3520원인데 우친으로배달까지",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100033589.1.13",
          "form": "하면800원에 열봉이에요",
          "original_form": "하면800원에 열봉이에요",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100033589.1.14",
          "form": "자기꺼 자기가 배달",
          "original_form": "자기꺼 자기가 배달",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100033589.1.15",
          "form": "주말에만 좀비쿠폰이였다던데",
          "original_form": "주말에만 좀비쿠폰이였다던데",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100033589.1.16",
          "form": "지금도돼요?",
          "original_form": "지금도돼요?",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100033589.1.17",
          "form": "열시이후는 우친이 안되욪",
          "original_form": "열시이후는 우친이 안되욪",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100033589.1.18",
          "form": "전문기사들배정되나보더라구요",
          "original_form": "전문기사들배정되나보더라구요",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100033589.1.19",
          "form": "낼해보세용",
          "original_form": "낼해보세용",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100033589.1.20",
          "form": "주문말고 좀비쿠폰이요",
          "original_form": "주문말고 좀비쿠폰이요",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100033589.1.21",
          "form": "한번쓰면 끝인거같던데",
          "original_form": "한번쓰면 끝인거같던데",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100033589.1.22",
          "form": "오늘 두번 했는데용",
          "original_form": "오늘 두번 했는데용",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100033589.1.23",
          "form": "쿠폰이 4천쿠폰?",
          "original_form": "쿠폰이 4천쿠폰?",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100033589.1.24",
          "form": "2만원이상이던데 그거쓰시는거예요?",
          "original_form": "2만원이상이던데 그거쓰시는거예요?",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100033589.1.25",
          "form": "네~그게왜인지 먹혀요",
          "original_form": "네~그게왜인지 먹혀요",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100033589.1.26",
          "form": "그거랑 배달비 쿠폰이랑",
          "original_form": "그거랑 배달비 쿠폰이랑",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100033589.1.27",
          "form": "통신사",
          "original_form": "통신사",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100033589.1.28",
          "form": "30일 바나나 100원딜이길래 그때쓸까했는데",
          "original_form": "30일 바나나 100원딜이길래 그때쓸까했는데",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100033589.1.29",
          "form": "그래서다들 그거하는군요 ㅎㅎ",
          "original_form": "그래서다들 그거하는군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100033589.1.30",
          "form": "백원딜은 여러번되는거죠?",
          "original_form": "백원딜은 여러번되는거죠?",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100033589.1.31",
          "form": "그건모르겠어요",
          "original_form": "그건모르겠어요",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100033589.1.32",
          "form": "복숭아할때 딱한번써봐서",
          "original_form": "복숭아할때 딱한번써봐서",
