      "utterance": [
        {
          "id": "MDRW2100024663.1.1",
          "form": "일하다보니 전에 먼말했는지 까먹었어요",
          "original_form": "일하다보니 전에 먼말했는지 까먹었어요",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024663.1.2",
          "form": "거기도 비 많이와요??",
          "original_form": "거기도 비 많이와요??",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024663.1.3",
          "form": "여기 비 1도안옴요 ㅋㅋㅋㅋ",
          "original_form": "여기 비 1도안옴요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024663.1.4",
          "form": "퇴근할때 비올거같은 느,낌",
          "original_form": "퇴근할때 비올거같은 느,낌",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024663.1.5",
          "form": "완전어두컴컴하네요",
          "original_form": "완전어두컴컴하네요",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024663.1.6",
          "form": "여기는 쏟아지네요",
          "original_form": "여기는 쏟아지네요",
          "speaker_id": "1",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024663.1.7",
          "form": "지금은 또 잠잠해졌네 오긴 오는데",
          "original_form": "지금은 또 잠잠해졌네 오긴 오는데",
          "speaker_id": "1",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024663.1.8",
          "form": "3주 내내 비가 오는듯?",
          "original_form": "3주 내내 비가 오는듯?",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024663.1.9",
          "form": "곧 추석인데",
          "original_form": "곧 추석인데",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024663.1.10",
          "form": "과일 당분 다 날아가겠어요",
          "original_form": "과일 당분 다 날아가겠어요",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024663.1.11",
          "form": "그러네요 차례지내세요 ??",
          "original_form": "그러네요 차례지내세요 ??",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024663.1.12",
          "form": "저 과일진짜 환장하는데",
          "original_form": "저 과일진짜 환장하는데",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024663.1.13",
          "form": "네 지냅니다ㅋㅋㅋ",
          "original_form": "네 지냅니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024663.1.14",
          "form": "뭐 좋아해여?",
          "original_form": "뭐 좋아해여?",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024663.1.15",
          "form": "그래서 전 잘부친다는거였구나 ㅋㅋㅋ",
          "original_form": "그래서 전 잘부친다는거였구나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024663.1.16",
          "form": "샤인머스캣이요",
          "original_form": "샤인머스캣이요",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024663.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ걸려꼬?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ걸려꼬?",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024663.1.18",
          "form": "망고",
          "original_form": "망고",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024663.1.19",
          "form": "와 님 매우 날카로운데요",
          "original_form": "와 님 매우 날카로운데요",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024663.1.20",
          "form": "딱복",
          "original_form": "딱복",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024663.1.21",
          "form": "딱복이건 물복이건 맛있으면 되는거 아닙니까",
          "original_form": "딱복이건 물복이건 맛있으면 되는거 아닙니까",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024663.1.22",
          "form": "물복시러요",
          "original_form": "물복시러요",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024663.1.23",
          "form": "망고는 조아하시잖아요",
          "original_form": "망고는 조아하시잖아요",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024663.1.24",
          "form": "알고보니 저희동네 호우주의보였음요",
          "original_form": "알고보니 저희동네 호우주의보였음요",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024663.1.25",
          "form": "어쩐지",
          "original_form": "어쩐지",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024663.1.26",
          "form": "망고랑은 다릅니다",
          "original_form": "망고랑은 다릅니다",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024663.1.27",
          "form": "제발 퇴근할때만 비안왓으면 ㅠ",
          "original_form": "제발 퇴근할때만 비안왓으면 ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024663.1.28",
          "form": "물렁한거 다 거기거 거기 아닌가",
          "original_form": "물렁한거 다 거기거 거기 아닌가",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024663.1.29",
          "form": "우산은 있죠?",
          "original_form": "우산은 있죠?",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024663.1.30",
          "form": "애플망고같은경우 딱복과비슷",
          "original_form": "애플망고같은경우 딱복과비슷",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024663.1.31",
          "form": "우산남는거많아요 ㅋㅋㅋㅋ",
          "original_form": "우산남는거많아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024663.1.32",
          "form": "누가 기부하고 가나요?",
          "original_form": "누가 기부하고 가나요?",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024663.1.33",
          "form": "피시방처럼?",
          "original_form": "피시방처럼?",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024663.1.34",
          "form": "애들이 기부하고 갑니다",
          "original_form": "애들이 기부하고 갑니다",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024663.1.35",
          "form": "아 요즘 우산공유서비스 이런거있어서",
          "original_form": "아 요즘 우산공유서비스 이런거있어서",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024663.1.36",
          "form": "건물 1층에서 그거하고있어ㅓㅅ 없으면 빌려가면됌 ㅋㅋㅋㅋ",
          "original_form": "건물 1층에서 그거하고있어ㅓㅅ 없으면 빌려가면됌 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024663.1.37",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024663.1.38",
          "form": "별게 다 있네요",
          "original_form": "별게 다 있네요",
          "speaker_id": "1",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024663.1.39",
          "form": "좋은데 사시네",
          "original_form": "좋은데 사시네",
          "speaker_id": "1",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024663.1.40",
          "form": "요새 대전에서 밀고있던데요?",
          "original_form": "요새 대전에서 밀고있던데요?",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024663.1.41",
          "form": "일주일내내 비오는듯,,",
          "original_form": "일주일내내 비오는듯,,",
          "speaker_id": "2",
