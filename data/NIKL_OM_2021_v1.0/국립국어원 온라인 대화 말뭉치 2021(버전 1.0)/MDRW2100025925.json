      "utterance": [
        {
          "id": "MDRW2100025925.1.1",
          "form": "과는 무슨과 재학중이세요?",
          "original_form": "과는 무슨과 재학중이세요?",
          "speaker_id": "1",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100025925.1.2",
          "form": "어느 학과 재학중이세요?",
          "original_form": "어느 학과 재학중이세요?",
          "speaker_id": "1",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100025925.1.3",
          "form": "전 상경이요 name1은요?",
          "original_form": "전 상경이요 &name1&은요?",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025925.1.4",
          "form": "저는 사회복지에요",
          "original_form": "저는 사회복지에요",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100025925.1.5",
          "form": "아하~ 동아리 활동 하시거나 하셨던거 있으세요?",
          "original_form": "아하~ 동아리 활동 하시거나 하셨던거 있으세요?",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025925.1.6",
          "form": "지금은 못하는데",
          "original_form": "지금은 못하는데",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025925.1.7",
          "form": "1학년땐 봉사동아리랑 농구동아리 했어요",
          "original_form": "1학년땐 봉사동아리랑 농구동아리 했어요",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025925.1.8",
          "form": "동아리활동 하시나요?",
          "original_form": "동아리활동 하시나요?",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025925.1.9",
          "form": "저는 안해봤는데 배드민턴 동아리는 좀 궁금하더라고요",
          "original_form": "저는 안해봤는데 배드민턴 동아리는 좀 궁금하더라고요",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025925.1.10",
          "form": "그렇게라도 해보려고 했는데 상황이 이래서 쩝..",
          "original_form": "그렇게라도 해보려고 했는데 상황이 이래서 쩝..",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025925.1.11",
          "form": "배드민턴 동아리 정도면",
          "original_form": "배드민턴 동아리 정도면",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025925.1.12",
          "form": "상황이 이래도 할거같은데",
          "original_form": "상황이 이래도 할거같은데",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025925.1.13",
          "form": "아예 집합금지인가봐요?",
          "original_form": "아예 집합금지인가봐요?",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025925.1.14",
          "form": "사실 그 뒤로 흥미가 떨어져서 찾아보진 않았어요ㅋㅋ",
          "original_form": "사실 그 뒤로 흥미가 떨어져서 찾아보진 않았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025925.1.15",
          "form": "농구동아리는 많이 힘든가요?",
          "original_form": "농구동아리는 많이 힘든가요?",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025925.1.16",
          "form": "농구를 좋아해서",
          "original_form": "농구를 좋아해서",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025925.1.17",
          "form": "힘들진 않았어요",
          "original_form": "힘들진 않았어요",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025925.1.18",
          "form": "좋아하는거 하면 그렇게 되나봐요",
          "original_form": "좋아하는거 하면 그렇게 되나봐요",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025925.1.19",
          "form": "봉사동아리에선 주로 뭘 하나요?",
          "original_form": "봉사동아리에선 주로 뭘 하나요?",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025925.1.20",
          "form": "과가 사회복지여서",
          "original_form": "과가 사회복지여서",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025925.1.21",
          "form": "과동아리였는데",
          "original_form": "과동아리였는데",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025925.1.22",
          "form": "술만 열심히 마신거같아요 ㅋㅋ",
          "original_form": "술만 열심히 마신거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025925.1.23",
          "form": "ㅋㅋㅋ 근데 다들 그러더라구요 친목느낌으로",
          "original_form": "ㅋㅋㅋ 근데 다들 그러더라구요 친목느낌으로",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025925.1.24",
          "form": "요즘은 비대면이라서",
          "original_form": "요즘은 비대면이라서",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025925.1.25",
          "form": "동아리들 줌으로 활동한다곤 들은거같아요",
          "original_form": "동아리들 줌으로 활동한다곤 들은거같아요",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100025925.1.26",
          "form": "다들 부지런하시네요ㄷㄷ",
          "original_form": "다들 부지런하시네요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 18:35"
        }
      ]
    }
  ]
