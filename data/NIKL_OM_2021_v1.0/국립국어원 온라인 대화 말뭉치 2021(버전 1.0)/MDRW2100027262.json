          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027262.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027262.1.5",
          "form": "진한거좋아한7요",
          "original_form": "진한거좋아한7요",
          "speaker_id": "2",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100027262.1.6",
          "form": "아메리카노는 많이줘요",
          "original_form": "아메리카노는 많이줘요",
          "speaker_id": "1",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100027262.1.7",
          "form": "연한커피좋아하나요",
          "original_form": "연한커피좋아하나요",
          "speaker_id": "2",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100027262.1.8",
          "form": "연한커피가 좋아요",
          "original_form": "연한커피가 좋아요",
          "speaker_id": "1",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100027262.1.9",
          "form": "물타서더 연하게마시면",
          "original_form": "물타서더 연하게마시면",
          "speaker_id": "2",
          "time": "20210902 10:55"
