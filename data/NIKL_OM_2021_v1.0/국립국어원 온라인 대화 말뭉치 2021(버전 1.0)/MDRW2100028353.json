        },
        {
          "id": "MDRW2100028353.1.2",
          "form": "얼려둔 게 없는 건가",
          "original_form": "얼려둔 게 없는 건가",
          "speaker_id": "2",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100028353.1.3",
          "form": "ㅇㅇ... 그래서 지금 얼리는 중",
          "original_form": "ㅇㅇ... 그래서 지금 얼리는 중",
          "speaker_id": "1",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100028353.1.4",
          "form": "안타깝다",
          "original_form": "안타깝다",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028353.1.5",
          "form": "님은 뜨거운 거 잘 드시나요",
          "original_form": "님은 뜨거운 거 잘 드시나요",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028353.1.6",
          "form": "나는 얼죽아인",
          "original_form": "나는 얼죽아인",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028353.1.7",
          "form": "그냥저냥 둘 다 좋아하긴 함",
          "original_form": "그냥저냥 둘 다 좋아하긴 함",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028353.1.8",
          "form": "근데 커피는 아이스로마심무조건",
          "original_form": "근데 커피는 아이스로마심무조건",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028353.1.9",
          "form": "무조건이지",
          "original_form": "무조건이지",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028353.1.10",
          "form": "전 라떼는 뜨거운 걸로도 가끔 마시는데",
          "original_form": "전 라떼는 뜨거운 걸로도 가끔 마시는데",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028353.1.11",
          "form": "여름엔 절대 아이스",
          "original_form": "여름엔 절대 아이스",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028353.1.12",
          "form": "절대아이스",
          "original_form": "절대아이스",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028353.1.13",
          "form": "내가 뜨거운 커피 마시는 건 딱",
          "original_form": "내가 뜨거운 커피 마시는 건 딱",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028353.1.14",
          "form": "그... 포카포카 그 뭐냐 그거",
          "original_form": "그... 포카포카 그 뭐냐 그거",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028353.1.15",
          "form": "아포가토먹을 때뿐",
          "original_form": "아포가토먹을 때뿐",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028353.1.16",
          "form": "포카포카 뭐야",
          "original_form": "포카포카 뭐야",
          "speaker_id": "1",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028353.1.17",
          "form": "아포가토가 뜨거운 커피라고 할 수 있냐고요",
          "original_form": "아포가토가 뜨거운 커피라고 할 수 있냐고요",
          "speaker_id": "1",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028353.1.18",
          "form": "이름이 생각이 안 나서(;",
          "original_form": "이름이 생각이 안 나서(;",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028353.1.19",
          "form": "그만큼 안 마시신다는 거지~",
          "original_form": "그만큼 안 마시신다는 거지~",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028353.1.20",
          "form": "그럴 수 있지 아이스 짱",
          "original_form": "그럴 수 있지 아이스 짱",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028353.1.21",
