      "utterance": [
        {
          "id": "MDRW2100046094.1.1",
          "form": "건강을 위해서 무엇을 하나요",
          "original_form": "건강을 위해서 무엇을 하나요",
          "speaker_id": "1",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046094.1.2",
          "form": "따로 운동은 안하게되요\r\n걷는게 고작이요",
          "original_form": "따로 운동은 안하게되요\r\n걷는게 고작이요",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046094.1.3",
          "form": "저도 걷기운동만 해요",
          "original_form": "저도 걷기운동만 해요",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046094.1.4",
          "form": "하루에 만보는 걸으려고 하는데 힘들더라고요",
          "original_form": "하루에 만보는 걸으려고 하는데 힘들더라고요",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046094.1.5",
          "form": "만보 걷는것 꽤 힘들죠",
          "original_form": "만보 걷는것 꽤 힘들죠",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046094.1.6",
          "form": "한시간 반정도 걸려요",
          "original_form": "한시간 반정도 걸려요",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046094.1.7",
          "form": "매일 만보씩 걷나요",
          "original_form": "매일 만보씩 걷나요",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046094.1.8",
          "form": "평일은 가능하면 그럴려고해요",
          "original_form": "평일은 가능하면 그럴려고해요",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046094.1.9",
          "form": "엄청 열심히 하시네요",
          "original_form": "엄청 열심히 하시네요",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046094.1.10",
          "form": "딴걸 전혀 안하니까요ㅋㅋ",
          "original_form": "딴걸 전혀 안하니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046094.1.11",
          "form": "저도 걷기만 하는데 만보 못걸어 봣어요",
          "original_form": "저도 걷기만 하는데 만보 못걸어 봣어요",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046094.1.12",
          "form": "어렵긴하죠",
          "original_form": "어렵긴하죠",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046094.1.13",
          "form": "꾸준히 한다는것도 참 어려운 일이죠",
          "original_form": "꾸준히 한다는것도 참 어려운 일이죠",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046094.1.14",
          "form": "진짜 그래요",
          "original_form": "진짜 그래요",
