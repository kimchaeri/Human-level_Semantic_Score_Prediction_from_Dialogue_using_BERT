        },
        {
          "id": "MDRW2100005928.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100005928.1.3",
          "form": "어떤거 하세요",
          "original_form": "어떤거 하세요",
          "speaker_id": "2",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100005928.1.4",
          "form": "스위치게임 포켓몬 젤다 동숲도하구 스팀게임도해여",
          "original_form": "스위치게임 포켓몬 젤다 동숲도하구 스팀게임도해여",
          "speaker_id": "1",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100005928.1.5",
          "form": "하시는겜있너요??",
          "original_form": "하시는겜있너요??",
          "speaker_id": "1",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100005928.1.6",
          "form": "아 저는 없어요",
          "original_form": "아 저는 없어요",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005928.1.7",
          "form": "오 동숲 기기 있으신가봐요",
          "original_form": "오 동숲 기기 있으신가봐요",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100005928.1.8",
          "form": "네 혹시 취미가 머에요?",
          "original_form": "네 혹시 취미가 머에요?",
          "speaker_id": "1",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100005928.1.9",
          "form": "아 저는 아이돌 댄스가 취미입니다",
          "original_form": "아 저는 아이돌 댄스가 취미입니다",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005928.1.10",
          "form": "제페토 앱 게임 해요",
          "original_form": "제페토 앱 게임 해요",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005928.1.11",
          "form": "그걸로 댄스도 할수있어요??",
          "original_form": "그걸로 댄스도 할수있어요??",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005928.1.12",
          "form": "아 네네 제페토 안에도 댄스 제스처가 있어요",
          "original_form": "아 네네 제페토 안에도 댄스 제스처가 있어요",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100005928.1.13",
          "form": "저는 그냥 레슨가서 직젖 춰요",
          "original_form": "저는 그냥 레슨가서 직젖 춰요",
          "speaker_id": "2",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100005928.1.14",
          "form": "우와 최근엔 무슨춤췄어요?",
          "original_form": "우와 최근엔 무슨춤췄어요?",
          "speaker_id": "1",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100005928.1.15",
          "form": "아스트로 애프터미드나잇 이요 들어보셨어요?",
          "original_form": "아스트로 애프터미드나잇 이요 들어보셨어요?",
          "speaker_id": "2",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100005928.1.16",
          "form": "오 넹 아스트로는 차은우땜에 ㅋㅋㅋ",
          "original_form": "오 넹 아스트로는 차은우땜에 ㅋㅋㅋ",
