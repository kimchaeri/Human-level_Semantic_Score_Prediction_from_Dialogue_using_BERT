          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100017030.1.3",
          "form": "왜 바꾼거임ㅋㅋ",
          "original_form": "왜 바꾼거임ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100017030.1.4",
          "form": "암튼 카트리그 봐라",
          "original_form": "암튼 카트리그 봐라",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100017030.1.5",
          "form": "쿠키런킹덤하쉴",
          "original_form": "쿠키런킹덤하쉴",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017030.1.6",
          "form": "하면 봐줄거냐구",
          "original_form": "하면 봐줄거냐구",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017030.1.7",
          "form": "조금은 볼 수 있지",
          "original_form": "조금은 볼 수 있지",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017030.1.8",
          "form": "카트도 보는재미가 있으니까",
          "original_form": "카트도 보는재미가 있으니까",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017030.1.9",
          "form": "맞어 근데 요번리그는 모르겠다",
          "original_form": "맞어 근데 요번리그는 모르겠다",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017030.1.10",
          "form": "옵저버 진짜 못잡음",
          "original_form": "옵저버 진짜 못잡음",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017030.1.11",
          "form": "옵저버 상태가 안좋으면",
          "original_form": "옵저버 상태가 안좋으면",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017030.1.12",
          "form": "리그 패망길 바로 가는디",
          "original_form": "리그 패망길 바로 가는디",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017030.1.13",
          "form": "여긴 옵저버 잘하는애 계속 안써가지구",
          "original_form": "여긴 옵저버 잘하는애 계속 안써가지구",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017030.1.14",
          "form": "옵저버 뽑아서 썼음 좋겠다",
          "original_form": "옵저버 뽑아서 썼음 좋겠다",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017030.1.15",
          "form": "리그는 보는 재미가 있어야해서",
          "original_form": "리그는 보는 재미가 있어야해서",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017030.1.16",
          "form": "카메라가 잘 잡혀야하는데",
          "original_form": "카메라가 잘 잡혀야하는데",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017030.1.17",
          "form": "사이퍼즈 리그 보는사람만 보는이유도",
          "original_form": "사이퍼즈 리그 보는사람만 보는이유도",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017030.1.18",
          "form": "초보는 봐도 화면 이해가 안가게끔 옵저버를 잡음;;",
          "original_form": "초보는 봐도 화면 이해가 안가게끔 옵저버를 잡음;;",
          "speaker_id": "1",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017030.1.19",
          "form": "ㅋㅋㅋㅋㅋ옵저버 진짜 왜이렇게 못하는지...",
          "original_form": "ㅋㅋㅋㅋㅋ옵저버 진짜 왜이렇게 못하는지...",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017030.1.20",
          "form": "ㄹㅇ ㅋㅋ",
          "original_form": "ㄹㅇ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017030.1.21",
          "form": "진짜루... 자신 없으면 옵저버 다수로 깔아놓고",
          "original_form": "진짜루... 자신 없으면 옵저버 다수로 깔아놓고",
