      "utterance": [
        {
          "id": "MDRW2100026214.1.1",
          "form": "게임하세오?",
          "original_form": "게임하세오?",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026214.1.2",
          "form": "뮤슨게임하세요",
          "original_form": "뮤슨게임하세요",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100026214.1.3",
          "form": "저는 메이플 가끔 해요!",
          "original_form": "저는 메이플 가끔 해요!",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100026214.1.4",
          "form": "게임 좋아하세요??",
          "original_form": "게임 좋아하세요??",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100026214.1.5",
          "form": "헐저도 메이플해요",
          "original_form": "헐저도 메이플해요",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100026214.1.6",
          "form": "베라섭입니다 섭어디세요??",
          "original_form": "베라섭입니다 섭어디세요??",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100026214.1.7",
          "form": "전 루나섭이요!",
          "original_form": "전 루나섭이요!",
          "speaker_id": "2",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100026214.1.8",
          "form": "요즘 무슨 직업 키우세요??",
          "original_form": "요즘 무슨 직업 키우세요??",
          "speaker_id": "2",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100026214.1.9",
          "form": "헐 대도시네요!",
          "original_form": "헐 대도시네요!",
          "speaker_id": "1",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100026214.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100026214.1.11",
          "form": "요즘은안햐요 본캐만 키우고있어요",
          "original_form": "요즘은안햐요 본캐만 키우고있어요",
          "speaker_id": "1",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100026214.1.12",
          "form": "소마입니당",
          "original_form": "소마입니당",
          "speaker_id": "1",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100026214.1.13",
          "form": "본캐머세요??",
          "original_form": "본캐머세요??",
          "speaker_id": "1",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100026214.1.14",
          "form": "맞아요 사람 너무 많아서 가끔 안들어가져요 좀 소도시로 옮기고 싶어요ㅜㅜ",
          "original_form": "맞아요 사람 너무 많아서 가끔 안들어가져요 좀 소도시로 옮기고 싶어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100026214.1.15",
          "form": "저는 아델이에요!",
          "original_form": "저는 아델이에요!",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100026214.1.16",
          "form": "본캐라고 하기엔 렙이 막 엄청 높진 않아요ㅜㅜ",
          "original_form": "본캐라고 하기엔 렙이 막 엄청 높진 않아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100026214.1.17",
          "form": "저도 아델있어요",
          "original_form": "저도 아델있어요",
          "speaker_id": "1",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100026214.1.18",
          "form": "아델 키우기 쉬워서 너무 좋아요!",
          "original_form": "아델 키우기 쉬워서 너무 좋아요!",
          "speaker_id": "2",
          "time": "20210903 13:08"
        },
        {
          "id": "MDRW2100026214.1.19",
          "form": "본캐는 레벨이 몇이세요??",
          "original_form": "본캐는 레벨이 몇이세요??",
          "speaker_id": "2",
          "time": "20210903 13:08"
        },
        {
          "id": "MDRW2100026214.1.20",
          "form": "저230인거같아요!",
          "original_form": "저230인거같아요!",
          "speaker_id": "1",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100026214.1.21",
          "form": "완전 높으시네요!",
          "original_form": "완전 높으시네요!",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100026214.1.22",
          "form": "저는 200 넘고 링크?? 그거 뭔말인지 모르겠어서 200에서 포기했어요...ㅎㅎ",
          "original_form": "저는 200 넘고 링크?? 그거 뭔말인지 모르겠어서 200에서 포기했어요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100026214.1.23",
          "form": "링크스킬??",
          "original_form": "링크스킬??",
          "speaker_id": "1",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100026214.1.24",
          "form": "200부터 시작인데 ㅠㅠ",
          "original_form": "200부터 시작인데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100026214.1.25",
          "form": "응! 난 그거 어렵더라ㅜㅜㅜ",
          "original_form": "응! 난 그거 어렵더라ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100026214.1.26",
          "form": "그거 배우면 확실히 재밌나요??",
          "original_form": "그거 배우면 확실히 재밌나요??",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100026214.1.27",
          "form": "저는 할만하더라구요",
          "original_form": "저는 할만하더라구요",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100026214.1.28",
          "form": "근데 지금은 또 메태기와서",
          "original_form": "근데 지금은 또 메태기와서",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100026214.1.29",
          "form": "안하는중..ㅎ",
          "original_form": "안하는중..ㅎ",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100026214.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ약간 메이플이 하다가 말다가 하다가 말다가 하는느낌이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ약간 메이플이 하다가 말다가 하다가 말다가 하는느낌이에요",
          "speaker_id": "2",
          "time": "20210903 18:00"
        }
      ]
