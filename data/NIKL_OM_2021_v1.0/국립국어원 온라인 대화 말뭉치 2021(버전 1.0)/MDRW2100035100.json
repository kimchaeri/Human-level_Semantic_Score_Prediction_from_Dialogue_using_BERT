      "utterance": [
        {
          "id": "MDRW2100035100.1.1",
          "form": "콰이어트플레이스에요 ㅋㅋㅋㅋㅋ",
          "original_form": "콰이어트플레이스에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.2",
          "form": "앗 그거 시즌투 나왔던데",
          "original_form": "앗 그거 시즌투 나왔던데",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.3",
          "form": "재밌나용?",
          "original_form": "재밌나용?",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.4",
          "form": "아세요???",
          "original_form": "아세요???",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.5",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.6",
          "form": "제가본게 2에요 ㅋㅋㅋ",
          "original_form": "제가본게 2에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.7",
          "form": "아아ㅋㅋㅋ",
          "original_form": "아아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.8",
          "form": "시즌투가 소리내면 안되는건가용?",
          "original_form": "시즌투가 소리내면 안되는건가용?",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.9",
          "form": "스토리가ㅋㅋ",
          "original_form": "스토리가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.10",
          "form": "1은 안봣는데",
          "original_form": "1은 안봣는데",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.11",
          "form": "네 소리가 나면 괴물이 와요!!",
          "original_form": "네 소리가 나면 괴물이 와요!!",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035100.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.13",
          "form": "아하 전 1은 봤어요!",
          "original_form": "아하 전 1은 봤어요!",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.14",
          "form": "1은 눈뜨면 안되는 스토리에용ㅋㅋ",
          "original_form": "1은 눈뜨면 안되는 스토리에용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.16",
          "form": "이거 결말이 어떻게 되나용?",
          "original_form": "이거 결말이 어떻게 되나용?",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.17",
          "form": "와 안보이면 못듣는것보다 더 답답할꺼같은데",
          "original_form": "와 안보이면 못듣는것보다 더 답답할꺼같은데",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.18",
          "form": "다 살아요?",
          "original_form": "다 살아요?",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.19",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.20",
          "form": "눈 가리고 다녀요 천으로",
          "original_form": "눈 가리고 다녀요 천으로",
          "speaker_id": "1",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.21",
          "form": "청각장애있는 여자아이가",
          "original_form": "청각장애있는 여자아이가",
          "speaker_id": "2",
          "time": "20210829 02:41"
        },
        {
          "id": "MDRW2100035100.1.22",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210829 02:42"
        }
      ]
