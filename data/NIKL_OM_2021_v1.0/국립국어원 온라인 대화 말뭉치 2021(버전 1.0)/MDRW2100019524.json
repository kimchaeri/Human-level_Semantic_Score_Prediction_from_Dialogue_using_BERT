      "utterance": [
        {
          "id": "MDRW2100019524.1.1",
          "form": "곧 펜트하우스해요",
          "original_form": "곧 펜트하우스해요",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019524.1.2",
          "form": "저도 대기중이에요",
          "original_form": "저도 대기중이에요",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019524.1.3",
          "form": "오늘은 어떤 막장일지",
          "original_form": "오늘은 어떤 막장일지",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019524.1.4",
          "form": "전 이지아배우님 너무 좋아해서",
          "original_form": "전 이지아배우님 너무 좋아해서",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019524.1.5",
          "form": "오늘 약혼이죠ㅜㅜ",
          "original_form": "오늘 약혼이죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019524.1.6",
          "form": "네 진짜 천사같을듯",
          "original_form": "네 진짜 천사같을듯",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019524.1.7",
          "form": "기대되네요",
          "original_form": "기대되네요",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019524.1.8",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019524.1.9",
          "form": "백준기는  어떻게될까요",
          "original_form": "백준기는  어떻게될까요",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019524.1.10",
          "form": "죄를지었음 벌받아야죠",
          "original_form": "죄를지었음 벌받아야죠",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019524.1.11",
          "form": "그러겠죠?",
          "original_form": "그러겠죠?",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019524.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019524.1.13",
          "form": "펜트하우스 빨리보고싶어요",
          "original_form": "펜트하우스 빨리보고싶어요",
