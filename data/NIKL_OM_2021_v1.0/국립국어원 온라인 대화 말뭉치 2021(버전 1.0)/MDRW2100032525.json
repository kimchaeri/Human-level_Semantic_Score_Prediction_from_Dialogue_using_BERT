      "utterance": [
        {
          "id": "MDRW2100032525.1.1",
          "form": "지금 내 다리는 좀...",
          "original_form": "지금 내 다리는 좀...",
          "speaker_id": "2",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100032525.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100032525.1.3",
          "form": "왜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "왜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100032525.1.4",
          "form": "살을 근육으로바꾸자",
          "original_form": "살을 근육으로바꾸자",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100032525.1.5",
          "form": "아니 두껍고의 문제가 아니라",
          "original_form": "아니 두껍고의 문제가 아니라",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100032525.1.6",
          "form": "허벅지가 맞으면 허리가 안맞고",
          "original_form": "허벅지가 맞으면 허리가 안맞고",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100032525.1.7",
          "form": "아아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100032525.1.8",
          "form": "뭔지 알짘ㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭔지 알짘ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100032525.1.9",
          "form": "진짜 스트레스",
          "original_form": "진짜 스트레스",
          "speaker_id": "2",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100032525.1.10",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032525.1.11",
          "form": "아 너 그글 봤어?",
          "original_form": "아 너 그글 봤어?",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032525.1.12",
          "form": "뭔글?",
          "original_form": "뭔글?",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032525.1.13",
          "form": "그 회사에",
          "original_form": "그 회사에",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032525.1.14",
          "form": "남자 여러명있고",
          "original_form": "남자 여러명있고",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032525.1.15",
          "form": "여자 한명 있는데",
          "original_form": "여자 한명 있는데",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032525.1.16",
          "form": "그 여자가 평소에 엄청 안꾸미고 옷도 편하거 입고 그러고 다녔대",
          "original_form": "그 여자가 평소에 엄청 안꾸미고 옷도 편하거 입고 그러고 다녔대",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032525.1.17",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032525.1.18",
          "form": "근데 회사에 여자 한명이 새로 오고 그 여자가",
          "original_form": "근데 회사에 여자 한명이 새로 오고 그 여자가",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032525.1.19",
          "form": "꾸미고 오니까 원래 있던 여자도 꾸미고 왔다면서",
          "original_form": "꾸미고 오니까 원래 있던 여자도 꾸미고 왔다면서",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032525.1.20",
          "form": "그 이유가 남자를 상대로 한 경쟁상대가 생겨서 그런거라고",
          "original_form": "그 이유가 남자를 상대로 한 경쟁상대가 생겨서 그런거라고",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032525.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032525.1.22",
          "form": "개소리 하는 남자 있더라",
          "original_form": "개소리 하는 남자 있더라",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032525.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032525.1.24",
          "form": "뭐라니 진짜",
          "original_form": "뭐라니 진짜",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032525.1.25",
          "form": "여자들이ㅋㅋㅋ지때문에경쟁ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "여자들이ㅋㅋㅋ지때문에경쟁ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032525.1.26",
          "form": "내말잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "내말잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032525.1.27",
          "form": "일본ㅇ ㅑ망가 얼마나본거임?",
          "original_form": "일본ㅇ ㅑ망가 얼마나본거임?",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032525.1.28",
          "form": "진짜 자의식 과잉",
          "original_form": "진짜 자의식 과잉",
          "speaker_id": "2",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032525.1.29",
          "form": "너무 심함ㅁ",
