      "utterance": [
        {
          "id": "MDRW2100033357.1.1",
          "form": "안녕하세요 연애관이 어떻게 되시나요?",
          "original_form": "안녕하세요 연애관이 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033357.1.2",
          "form": "안녕하세요 ㅋㅋ",
          "original_form": "안녕하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100033357.1.3",
          "form": "음 글쎄요 딱 정의한건없는거같아요",
          "original_form": "음 글쎄요 딱 정의한건없는거같아요",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100033357.1.4",
          "form": "저는 제대로 처음 연애했던 사람이랑 5년만났는데 바람+바람녀와 임신해서 결혼했거든요",
          "original_form": "저는 제대로 처음 연애했던 사람이랑 5년만났는데 바람+바람녀와 임신해서 결혼했거든요",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100033357.1.5",
          "form": "그래서 연애에 있어서 더 나아가서 삶에서 진짜 작은 거짓말도 잘하지 않는 사람을 만나는게 제 연애관이 되었어뇨",
          "original_form": "그래서 연애에 있어서 더 나아가서 삶에서 진짜 작은 거짓말도 잘하지 않는 사람을 만나는게 제 연애관이 되었어뇨",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100033357.1.6",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "2",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100033357.1.7",
          "form": "오래만나셨는데",
          "original_form": "오래만나셨는데",
          "speaker_id": "2",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100033357.1.8",
          "form": "정말 나쁜 사람이네요",
          "original_form": "정말 나쁜 사람이네요",
          "speaker_id": "2",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100033357.1.9",
          "form": "그 상대방한테도 이름 폰번호 빼고 다 속였고 심지어 임신사실도 제가 폭로해서 밝혀진거고 부모님께 알릴생각도 없었더라구요",
          "original_form": "그 상대방한테도 이름 폰번호 빼고 다 속였고 심지어 임신사실도 제가 폭로해서 밝혀진거고 부모님께 알릴생각도 없었더라구요",
          "speaker_id": "1",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100033357.1.10",
          "form": "그런데 둘이 결혼하더라구요 거참",
          "original_form": "그런데 둘이 결혼하더라구요 거참",
          "speaker_id": "1",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100033357.1.11",
          "form": "와................",
          "original_form": "와................",
          "speaker_id": "2",
          "time": "20210906 23:31"
        },
        {
          "id": "MDRW2100033357.1.12",
          "form": "그런 쓰.레기 수거 해가서 다행이다 생각하죠 ㅠ",
          "original_form": "그런 쓰.레기 수거 해가서 다행이다 생각하죠 ㅠ",
          "speaker_id": "2",
          "time": "20210906 23:32"
        },
        {
          "id": "MDRW2100033357.1.13",
          "form": "진짜....미쳤네요",
          "original_form": "진짜....미쳤네요",
          "speaker_id": "2",
          "time": "20210906 23:32"
        },
        {
          "id": "MDRW2100033357.1.14",
          "form": "저도 그렇게 생각해요 다행이라고요",
          "original_form": "저도 그렇게 생각해요 다행이라고요",
          "speaker_id": "1",
          "time": "20210906 23:35"
        },
        {
          "id": "MDRW2100033357.1.15",
          "form": "저도 8년 사귀고 그렇게 20대청춘 다보내고 나니까",
          "original_form": "저도 8년 사귀고 그렇게 20대청춘 다보내고 나니까",
          "speaker_id": "2",
          "time": "20210906 23:39"
        },
        {
          "id": "MDRW2100033357.1.16",
          "form": "헤어지자고 이야기하더라구요",
          "original_form": "헤어지자고 이야기하더라구요",
          "speaker_id": "2",
          "time": "20210906 23:39"
        },
        {
          "id": "MDRW2100033357.1.17",
          "form": "어머니 반대로",
          "original_form": "어머니 반대로",
          "speaker_id": "2",
          "time": "20210906 23:39"
        },
        {
          "id": "MDRW2100033357.1.18",
          "form": "세상 허무했어요",
          "original_form": "세상 허무했어요",
          "speaker_id": "2",
          "time": "20210906 23:39"
        },
        {
          "id": "MDRW2100033357.1.19",
          "form": "사랑이 뭔지 정말",
          "original_form": "사랑이 뭔지 정말",
          "speaker_id": "2",
          "time": "20210906 23:39"
        },
        {
          "id": "MDRW2100033357.1.20",
          "form": "우와 8년이면... 정말 허무하셨겠어요",
          "original_form": "우와 8년이면... 정말 허무하셨겠어요",
          "speaker_id": "1",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100033357.1.21",
          "form": "어느 사연이 더 불쌍한가는 따지지도 못하겠지만 ㅠ 그 사람은 정말 내 인연이 아니었고 그 정도 되는 사람 밖에 안되는 사람이구나 생각해요..",
          "original_form": "어느 사연이 더 불쌍한가는 따지지도 못하겠지만 ㅠ 그 사람은 정말 내 인연이 아니었고 그 정도 되는 사람 밖에 안되는 사람이구나 생각해요..",
          "speaker_id": "2",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100033357.1.22",
          "form": "맞아요 그렇게 상대방이 자신을 소중히 하지않는데",
          "original_form": "맞아요 그렇게 상대방이 자신을 소중히 하지않는데",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100033357.1.23",
          "form": "연애를 지속하고 결혼을 했다면 더 힘들고 괴롭고 불행해졌을거라고 생각해요",
          "original_form": "연애를 지속하고 결혼을 했다면 더 힘들고 괴롭고 불행해졌을거라고 생각해요",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100033357.1.24",
          "form": "부디 좋은 인연이 있길 기도할껭ㅅ",
          "original_form": "부디 좋은 인연이 있길 기도할껭ㅅ",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100033357.1.25",
          "form": "할께요",
          "original_form": "할께요",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100033357.1.26",
          "form": "저도 기도할께요..",
          "original_form": "저도 기도할께요..",
          "speaker_id": "2",
          "time": "20210906 23:54"
        },
        {
          "id": "MDRW2100033357.1.27",
          "form": "힘내세요!!",
          "original_form": "힘내세요!!",
          "speaker_id": "1",
          "time": "20210906 23:54"
        }
      ]
    }
