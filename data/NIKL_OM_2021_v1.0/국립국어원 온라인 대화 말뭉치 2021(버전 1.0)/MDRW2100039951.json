      "utterance": [
        {
          "id": "MDRW2100039951.1.1",
          "form": "대만은 근데 음식이 괜찮나요?",
          "original_form": "대만은 근데 음식이 괜찮나요?",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100039951.1.2",
          "form": "네 대만은 중국처럼 향신료가 강하긴 한데요",
          "original_form": "네 대만은 중국처럼 향신료가 강하긴 한데요",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100039951.1.3",
          "form": "1인 샤브샤브 이런 것 파는 곳도 많아서 무난한 것 같아요",
          "original_form": "1인 샤브샤브 이런 것 파는 곳도 많아서 무난한 것 같아요",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039951.1.4",
          "form": "야시장 음식은 큐브스테이크나 굴전 같은 것도 팔아서 은근 한국음식 같았어요 ㅋㅋㅋ",
          "original_form": "야시장 음식은 큐브스테이크나 굴전 같은 것도 팔아서 은근 한국음식 같았어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039951.1.5",
          "form": "아! 제가 간곳은 스팀생선찜같은거여서..",
          "original_form": "아! 제가 간곳은 스팀생선찜같은거여서..",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039951.1.6",
          "form": "먹긴했지만  맛있지는 않았누데",
          "original_form": "먹긴했지만  맛있지는 않았누데",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039951.1.7",
          "form": "야시장이 괜찮네요",
          "original_form": "야시장이 괜찮네요",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039951.1.8",
          "form": "헐 생선찜인데....아쉽네요",
          "original_form": "헐 생선찜인데....아쉽네요",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039951.1.9",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039951.1.10",
          "form": "생선찜도 향신료 가득하지 않아요???",
          "original_form": "생선찜도 향신료 가득하지 않아요???",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039951.1.11",
          "form": "과자가랑 세븐일레븐 커피가 제일 맛있었어요ㅜㅜ",
          "original_form": "과자가랑 세븐일레븐 커피가 제일 맛있었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039951.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039951.1.13",
          "form": "앗ㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039951.1.14",
          "form": "오향..이..ㅜㅜ",
          "original_form": "오향..이..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039951.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039951.1.16",
          "form": "식사보다는 간식이 훌륭하죠 ㅋㅋㅋ",
          "original_form": "식사보다는 간식이 훌륭하죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039951.1.17",
          "form": "길거리 음식이랑요 ㅋㅋㅋㅋㅋ",
          "original_form": "길거리 음식이랑요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039951.1.18",
          "form": "음식은 버블티라던지 녹두앙금으로된 과자도 있었는데",
          "original_form": "음식은 버블티라던지 녹두앙금으로된 과자도 있었는데",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039951.1.19",
          "form": "진짜 과자천국이더라구요ㅋㅋ",
          "original_form": "진짜 과자천국이더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039951.1.20",
          "form": "와 녹두앙금 과자도 있었어요??? 전 그건 못본 거 같아요",
          "original_form": "와 녹두앙금 과자도 있었어요??? 전 그건 못본 거 같아요",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039951.1.21",
          "form": "육포도 다양한데 한국에 못들고가서 넘 아쉬웠어요 ㅋㅋㅋㅋ",
          "original_form": "육포도 다양한데 한국에 못들고가서 넘 아쉬웠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039951.1.22",
          "form": "맞아요ㅋㅋㅋㄱ",
          "original_form": "맞아요ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039951.1.23",
          "form": "누가크래커파는 곳에 있었는데",
          "original_form": "누가크래커파는 곳에 있었는데",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039951.1.24",
          "form": "누가보다 훨씬맛있었어요ㅋㅋ",
          "original_form": "누가보다 훨씬맛있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039951.1.25",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100039951.1.26",
          "form": "코로나 끝나면 누가크래커 집을 털러가야겠네요",
          "original_form": "코로나 끝나면 누가크래커 집을 털러가야겠네요",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100039951.1.27",
          "form": "추천드립니다ㅋㅋㅋㅋ",
          "original_form": "추천드립니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039951.1.28",
          "form": "앙금과자 커피랑 먹으면 맛날 거 같아요ㅠㅋㅋㅋㅋ",
          "original_form": "앙금과자 커피랑 먹으면 맛날 거 같아요ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
