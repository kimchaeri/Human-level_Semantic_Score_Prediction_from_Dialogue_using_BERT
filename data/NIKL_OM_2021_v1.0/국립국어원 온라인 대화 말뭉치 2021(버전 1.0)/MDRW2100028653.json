      "utterance": [
        {
          "id": "MDRW2100028653.1.1",
          "form": "와 맛있겠어요 저는 지금 민트초코파이 한개 남은거 먹고 있어요",
          "original_form": "와 맛있겠어요 저는 지금 민트초코파이 한개 남은거 먹고 있어요",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028653.1.2",
          "form": "맛있었는데 이제 안팔더라고요",
          "original_form": "맛있었는데 이제 안팔더라고요",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028653.1.3",
          "form": "민초파시군요",
          "original_form": "민초파시군요",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028653.1.4",
          "form": "민초 좋아하세요?",
          "original_form": "민초 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028653.1.5",
          "form": "아뇨ㅠㅠ 민초는 못먹어요",
          "original_form": "아뇨ㅠㅠ 민초는 못먹어요",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028653.1.6",
          "form": "너무 화~해서",
          "original_form": "너무 화~해서",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028653.1.7",
          "form": "ㅎㅎ 아이스크림 어떤 맛 좋아하세요?",
          "original_form": "ㅎㅎ 아이스크림 어떤 맛 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028653.1.8",
          "form": "저~ 요거트나 초코 좋아해요!!!!",
          "original_form": "저~ 요거트나 초코 좋아해요!!!!",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028653.1.9",
          "form": "딸기도좋고요",
          "original_form": "딸기도좋고요",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028653.1.10",
          "form": "저도 요거트나 치즈 들어간 종류 좋아해요!! 사실 아이스크림은 뭐든 맛있긴 해요",
          "original_form": "저도 요거트나 치즈 들어간 종류 좋아해요!! 사실 아이스크림은 뭐든 맛있긴 해요",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028653.1.11",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028653.1.12",
          "form": "가끔 먹는데 너무 맛잇어요",
          "original_form": "가끔 먹는데 너무 맛잇어요",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028653.1.13",
          "form": "폴바셋? 거기 아이스크림 너무 맛있더라고요",
          "original_form": "폴바셋? 거기 아이스크림 너무 맛있더라고요",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028653.1.14",
          "form": "맞아요 맛있더라고요!!",
          "original_form": "맞아요 맛있더라고요!!",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028653.1.15",
          "form": "저는 아이스크림은 다 안가려서 ㅎㅎ",
          "original_form": "저는 아이스크림은 다 안가려서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028653.1.16",
          "form": "아이스크림 좋아하시군요!!!",
          "original_form": "아이스크림 좋아하시군요!!!",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028653.1.17",
          "form": "또 좋아하는음식있으세요?  저는 김밥이요",
          "original_form": "또 좋아하는음식있으세요?  저는 김밥이요",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028653.1.18",
          "form": "아 진짜 김밥 맛있죠 근데 김밥은 집김밥이 진짜 계속 들어가는거 같아요",
          "original_form": "아 진짜 김밥 맛있죠 근데 김밥은 집김밥이 진짜 계속 들어가는거 같아요",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028653.1.19",
          "form": "집에서 싸면 끝없이 들어가요",
          "original_form": "집에서 싸면 끝없이 들어가요",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028653.1.20",
          "form": "맞아요 ㅎ그리고 통으로 들고 먹는거 좋아해요",
          "original_form": "맞아요 ㅎ그리고 통으로 들고 먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100028653.1.21",
          "form": "전 당근 많이 넣은 김밥 좋아해요 쌀때 힘든데 맛있더라고요",
          "original_form": "전 당근 많이 넣은 김밥 좋아해요 쌀때 힘든데 맛있더라고요",
          "speaker_id": "2",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100028653.1.22",
          "form": "맞아요ㅋㅋ 맛있어요 우엉들어가도 맛있어요",
          "original_form": "맞아요ㅋㅋ 맛있어요 우엉들어가도 맛있어요",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100028653.1.23",
          "form": "돈까스 김밥도 좋아하고",
          "original_form": "돈까스 김밥도 좋아하고",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100028653.1.24",
          "form": "최근에 갓김치 김밥 먹어봤는데 맛있더라고요",
          "original_form": "최근에 갓김치 김밥 먹어봤는데 맛있더라고요",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100028653.1.25",
          "form": "오 갓김치도 넣어먹으면 맛있겠어요!!",
          "original_form": "오 갓김치도 넣어먹으면 맛있겠어요!!",
