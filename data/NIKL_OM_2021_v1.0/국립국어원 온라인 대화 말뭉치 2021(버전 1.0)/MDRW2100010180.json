      "utterance": [
        {
          "id": "MDRW2100010180.1.1",
          "form": "매일 식사 직접 차리시나요ㅠ",
          "original_form": "매일 식사 직접 차리시나요ㅠ",
          "speaker_id": "2",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010180.1.2",
          "form": "요새 배달 많이 시켰어요 ㅜㅜ",
          "original_form": "요새 배달 많이 시켰어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010180.1.3",
          "form": "맞아요. 배달 진짜 편하죠",
          "original_form": "맞아요. 배달 진짜 편하죠",
          "speaker_id": "2",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010180.1.4",
          "form": "치킨 족발 햄버거요",
          "original_form": "치킨 족발 햄버거요",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010180.1.5",
          "form": "뭐 배달하세요?",
          "original_form": "뭐 배달하세요?",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010180.1.6",
          "form": "저희도 아무래도 치킨 족발!!",
          "original_form": "저희도 아무래도 치킨 족발!!",
          "speaker_id": "2",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100010180.1.7",
          "form": "배달은 다 비슷한가봐요 ㅋㅋ",
          "original_form": "배달은 다 비슷한가봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010180.1.8",
          "form": "전 깻잎 두마리 치킨 먹었어요",
          "original_form": "전 깻잎 두마리 치킨 먹었어요",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010180.1.9",
          "form": "무슨 치킨 드셨어요?",
          "original_form": "무슨 치킨 드셨어요?",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010180.1.10",
          "form": "앗 안먹어봤어요ㅠ",
          "original_form": "앗 안먹어봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010180.1.11",
          "form": "치킨은 노랑통닭이요ㅋ",
          "original_form": "치킨은 노랑통닭이요ㅋ",
          "speaker_id": "2",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010180.1.12",
          "form": "오 맛있나요? 한번도 안먹어 봤어요?",
          "original_form": "오 맛있나요? 한번도 안먹어 봤어요?",
          "speaker_id": "1",
          "time": "20210823 16:51"
        },
        {
          "id": "MDRW2100010180.1.13",
          "form": "노랑통닭 드시는분 많던데",
          "original_form": "노랑통닭 드시는분 많던데",
          "speaker_id": "1",
          "time": "20210823 18:18"
        },
        {
          "id": "MDRW2100010180.1.14",
          "form": "부스러기가 많아서 호불호가 갈리는데",
          "original_form": "부스러기가 많아서 호불호가 갈리는데",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100010180.1.15",
          "form": "전 그게 매력이에요ㅋㅋ",
          "original_form": "전 그게 매력이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:33"
        },
        {
          "id": "MDRW2100010180.1.16",
          "form": "오부스러기가 많다는건 겉에 튀김옷 잔뜩 바삭 한가봐요",
          "original_form": "오부스러기가 많다는건 겉에 튀김옷 잔뜩 바삭 한가봐요",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100010180.1.17",
          "form": "좋은데요 ㅎㅎ",
          "original_form": "좋은데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100010180.1.18",
          "form": "집근처인데 한번 먹어봐야겠네요",
          "original_form": "집근처인데 한번 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100010180.1.19",
          "form": "네 순살 3종 괜찮습니다.",
          "original_form": "네 순살 3종 괜찮습니다.",
          "speaker_id": "2",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100010180.1.20",
          "form": "다만 순살은 외국닭이에요ㅠ",
          "original_form": "다만 순살은 외국닭이에요ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100010180.1.21",
          "form": "오 먹어볼께요",
          "original_form": "오 먹어볼께요",
          "speaker_id": "1",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100010180.1.22",
          "form": "전 외국닭 괜찮아요",
          "original_form": "전 외국닭 괜찮아요",
          "speaker_id": "1",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100010180.1.23",
          "form": "3종 골고루 라 여러맛 먹을수 있겠네요",
          "original_form": "3종 골고루 라 여러맛 먹을수 있겠네요",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100010180.1.24",
          "form": "갑자기 치킨 얘기 하니 치킨 먹고 싶네요ㅎㅎ",
          "original_form": "갑자기 치킨 얘기 하니 치킨 먹고 싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:47"
        }
      ]
    }
