        },
        {
          "id": "MDRW2100013600.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013600.1.3",
          "form": "온라인 커뮤니티 해보신 적 있나요?",
          "original_form": "온라인 커뮤니티 해보신 적 있나요?",
          "speaker_id": "1",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013600.1.4",
          "form": "네이버 카페라던가",
          "original_form": "네이버 카페라던가",
          "speaker_id": "1",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013600.1.5",
          "form": "아 네 눈팅해요 주로ㅋㅋ",
          "original_form": "아 네 눈팅해요 주로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100013600.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100013600.1.7",
          "form": "저는 예전에 힙플이라고 힙합 관련 사이트 했었어요",
          "original_form": "저는 예전에 힙플이라고 힙합 관련 사이트 했었어요",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013600.1.8",
          "form": "첨들어봐요 힙합을 하시는 분인가요?",
          "original_form": "첨들어봐요 힙합을 하시는 분인가요?",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013600.1.9",
          "form": "아뇨ㅋㅋㅋ 그냥 그때 이센스 쌈디 이런 가수들 정보 찾다가",
          "original_form": "아뇨ㅋㅋㅋ 그냥 그때 이센스 쌈디 이런 가수들 정보 찾다가",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013600.1.10",
          "form": "재밌는 이야기도 많고 해서 사이트에 가입했었어요",
          "original_form": "재밌는 이야기도 많고 해서 사이트에 가입했었어요",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013600.1.11",
          "form": "글쿠나 저도 아이돌덕질할때 사이트 가입했던 기억이나네요ㅋㅋ",
          "original_form": "글쿠나 저도 아이돌덕질할때 사이트 가입했던 기억이나네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013600.1.12",
          "form": "ㅋㅋㅋ 뭔가 덕질할때 그런곳 가입하면 재밌지 않아요?",
          "original_form": "ㅋㅋㅋ 뭔가 덕질할때 그런곳 가입하면 재밌지 않아요?",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013600.1.13",
          "form": "모든사람이 내가 좋아하는걸 이야기하고있어!",
          "original_form": "모든사람이 내가 좋아하는걸 이야기하고있어!",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013600.1.14",
          "form": "ㅋㅋㅋㅋ맞아요 덕질도 같이해야 재밌어요",
          "original_form": "ㅋㅋㅋㅋ맞아요 덕질도 같이해야 재밌어요",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013600.1.15",
          "form": "막 내생각이랑 다르게 말하면 반박도 해줘야되고",
          "original_form": "막 내생각이랑 다르게 말하면 반박도 해줘야되고",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013600.1.16",
          "form": "과해지면 안좋지만 그냥 의견교류정도면 진짜 재밌고 좋은것같아요",
          "original_form": "과해지면 안좋지만 그냥 의견교류정도면 진짜 재밌고 좋은것같아요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013600.1.17",
          "form": "맞아요 자료나 정보같은것도 많고",
          "original_form": "맞아요 자료나 정보같은것도 많고",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013600.1.18",
          "form": "잘만 이용하면 순기능이많죠",
          "original_form": "잘만 이용하면 순기능이많죠",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013600.1.19",
          "form": "가끔 근데 찐으로 싸우시는분들 있죠ㅋㅋㅋ",
          "original_form": "가끔 근데 찐으로 싸우시는분들 있죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013600.1.20",
          "form": "어휴 쌈 많이나요..조금만 선넘으면 난리나잖아요",
          "original_form": "어휴 쌈 많이나요..조금만 선넘으면 난리나잖아요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013600.1.21",
          "form": "그런걸 진짜 조심해야돼요",
          "original_form": "그런걸 진짜 조심해야돼요",
          "speaker_id": "1",
          "time": "20210825 15:41"
        }
      ]
