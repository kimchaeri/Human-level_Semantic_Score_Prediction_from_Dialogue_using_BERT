      "utterance": [
        {
          "id": "MDRW2100041720.1.1",
          "form": "슬프게도 안합니다",
          "original_form": "슬프게도 안합니다",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041720.1.2",
          "form": "그래서 살이쪄요 ㅎ",
          "original_form": "그래서 살이쪄요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041720.1.3",
          "form": "ㅋㅋㅋㅋ먹는건 좋아하세용?",
          "original_form": "ㅋㅋㅋㅋ먹는건 좋아하세용?",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041720.1.4",
          "form": "네 당연하죠",
          "original_form": "네 당연하죠",
          "speaker_id": "2",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041720.1.5",
          "form": "자극적인 거 맨날 먹고 흑",
          "original_form": "자극적인 거 맨날 먹고 흑",
          "speaker_id": "2",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041720.1.6",
          "form": "건강에는 안좋지만요",
          "original_form": "건강에는 안좋지만요",
          "speaker_id": "2",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041720.1.7",
          "form": "저도 매운거 좋아해요 ㅠㅠ",
          "original_form": "저도 매운거 좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041720.1.8",
          "form": "전 매운 거 잘 못 먹는데 좋아해요",
          "original_form": "전 매운 거 잘 못 먹는데 좋아해요",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041720.1.9",
          "form": "잘 드세요?",
          "original_form": "잘 드세요?",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041720.1.10",
          "form": "저도 고수는 아니예요 ㅠㅋㅋㅋㅋ",
          "original_form": "저도 고수는 아니예요 ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041720.1.11",
          "form": "요즘 고수분들 워낙많아서..",
          "original_form": "요즘 고수분들 워낙많아서..",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041720.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 진짜요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 진짜요",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041720.1.13",
          "form": "정말 대단해보여요",
          "original_form": "정말 대단해보여요",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041720.1.14",
          "form": "성형해보셨나요?",
          "original_form": "성형해보셨나요?",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041720.1.15",
          "form": "아뇨 해보셨어요?",
          "original_form": "아뇨 해보셨어요?",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041720.1.16",
          "form": "저두 아니용 ㅋㅋ제친구들만 봐도 요즘은 쌍수랑 코는 기본이던데요",
          "original_form": "저두 아니용 ㅋㅋ제친구들만 봐도 요즘은 쌍수랑 코는 기본이던데요",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041720.1.17",
          "form": "네 많이 하더라고요",
          "original_form": "네 많이 하더라고요",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041720.1.18",
          "form": "하고싶기는 하세요?",
          "original_form": "하고싶기는 하세요?",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041720.1.19",
          "form": "아뇨 ㅋㅋ있는대로 살려고요 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ있는대로 살려고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041720.1.20",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041720.1.21",
          "form": "전 하고싶은데",
          "original_form": "전 하고싶은데",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041720.1.22",
          "form": "무서워요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "무서워요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
