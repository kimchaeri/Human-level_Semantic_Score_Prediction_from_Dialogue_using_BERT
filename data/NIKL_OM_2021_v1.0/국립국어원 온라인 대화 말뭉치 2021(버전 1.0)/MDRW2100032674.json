      "utterance": [
        {
          "id": "MDRW2100032674.1.1",
          "form": "저는 딱히 없는데",
          "original_form": "저는 딱히 없는데",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032674.1.2",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032674.1.3",
          "form": "음 저는",
          "original_form": "음 저는",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032674.1.4",
          "form": "책선물받았어요",
          "original_form": "책선물받았어요",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032674.1.5",
          "form": "무슨책이요?",
          "original_form": "무슨책이요?",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032674.1.6",
          "form": "아직읽지는못했는데",
          "original_form": "아직읽지는못했는데",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032674.1.7",
          "form": "에세이?!같은책이에요",
          "original_form": "에세이?!같은책이에요",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032674.1.8",
          "form": "에세이 좋아해요?",
          "original_form": "에세이 좋아해요?",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032674.1.9",
          "form": "그냥 편식없이 읽으려고 노력합니다ㅎㅎ",
          "original_form": "그냥 편식없이 읽으려고 노력합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032674.1.10",
          "form": "좋은 습관이네요 ㅎㅎ",
          "original_form": "좋은 습관이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032674.1.11",
          "form": "그럼 요즘 사고싶은 물건있으세요?",
          "original_form": "그럼 요즘 사고싶은 물건있으세요?",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032674.1.12",
          "form": "꿀을 사려고해요",
          "original_form": "꿀을 사려고해요",
          "speaker_id": "2",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032674.1.13",
          "form": "꿀이요?!",
          "original_form": "꿀이요?!",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032674.1.14",
          "form": "꿀좋아하세요?!",
          "original_form": "꿀좋아하세요?!",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032674.1.15",
          "form": "꿀을 조금씩먹는게 몸에 좋다는것같더라고요",
          "original_form": "꿀을 조금씩먹는게 몸에 좋다는것같더라고요",
          "speaker_id": "2",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032674.1.16",
          "form": "아ㅋㅋㅋ 저도 들어본것같아요!",
          "original_form": "아ㅋㅋㅋ 저도 들어본것같아요!",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032674.1.17",
          "form": "근데 아직 사지는 않았어요",
          "original_form": "근데 아직 사지는 않았어요",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032674.1.18",
          "form": "왜요??",
          "original_form": "왜요??",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032674.1.19",
          "form": "할인을 딱히 안하더라고요",
          "original_form": "할인을 딱히 안하더라고요",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032674.1.20",
          "form": "아 할인기간 꼭 맞춰구매해야",
          "original_form": "아 할인기간 꼭 맞춰구매해야",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032674.1.21",
          "form": "안억울한것같아요ㅠ",
          "original_form": "안억울한것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:09"
        }
      ]
