      "utterance": [
        {
          "id": "MDRW2100045001.1.1",
          "form": "뭐 샀수??",
          "original_form": "뭐 샀수??",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045001.1.2",
          "form": "젤 최근에 산건 식품건조기!",
          "original_form": "젤 최근에 산건 식품건조기!",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045001.1.3",
          "form": "댕댕이 간식 만들어주려구 샀어따 ㅎ.ㅎ",
          "original_form": "댕댕이 간식 만들어주려구 샀어따 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045001.1.4",
          "form": "오, 그런건 얼마나 혀??\n본가에 있겠네?",
          "original_form": "오, 그런건 얼마나 혀??\n본가에 있겠네?",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045001.1.5",
          "form": "ㄴㄴㄴㄴ자취방에 갖고옴!",
          "original_form": "ㄴㄴㄴㄴ자취방에 갖고옴!",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045001.1.6",
          "form": "나는 리큅 미개봉 새제품 삿는데 5만원인가??",
          "original_form": "나는 리큅 미개봉 새제품 삿는데 5만원인가??",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045001.1.7",
          "form": "멍멍이는 할머니댁에 있지 않어?",
          "original_form": "멍멍이는 할머니댁에 있지 않어?",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045001.1.8",
          "form": "상태따라 되게 다른데",
          "original_form": "상태따라 되게 다른데",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045001.1.9",
          "form": "귀찮아서 잘 안함....ㅠ",
          "original_form": "귀찮아서 잘 안함....ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045001.1.10",
          "form": "앗 부산에 같이이쑴다!",
          "original_form": "앗 부산에 같이이쑴다!",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045001.1.11",
          "form": "오, 이왕 산거 많이 말려주라구",
          "original_form": "오, 이왕 산거 많이 말려주라구",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045001.1.12",
          "form": "ㅋㅋㅋㅋ생각보다 진짜 귀찮도라",
          "original_form": "ㅋㅋㅋㅋ생각보다 진짜 귀찮도라",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045001.1.13",
          "form": "후기 안좋아서",
          "original_form": "후기 안좋아서",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045001.1.14",
          "form": "사야하나 했는데",
          "original_form": "사야하나 했는데",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045001.1.15",
          "form": "안사는게 맞았던듯",
          "original_form": "안사는게 맞았던듯",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045001.1.16",
          "form": "ㅋㅋㅋㅋㅋ 저런... 안타깝군요",
          "original_form": "ㅋㅋㅋㅋㅋ 저런... 안타깝군요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045001.1.17",
          "form": "물건구매는 신중해야혀...",
          "original_form": "물건구매는 신중해야혀...",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045001.1.18",
          "form": "ㅇㄱㄹㅇ...나는 50마넌짜리 태블릿 산거 아까웡\n당시는 진짜 사고싶었지만 그닥쓰진 않음",
          "original_form": "ㅇㄱㄹㅇ...나는 50마넌짜리 태블릿 산거 아까웡\n당시는 진짜 사고싶었지만 그닥쓰진 않음",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045001.1.19",
          "form": "앜ㅋㅋㅋㅋ ㄴㅓ 태블릿도 있냐",
          "original_form": "앜ㅋㅋㅋㅋ ㄴㅓ 태블릿도 있냐",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045001.1.20",
          "form": "갤탭?",
          "original_form": "갤탭?",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045001.1.21",
          "form": "ㄴㄴ 레노버 요가북이라고 있다, 키보드랑 펜 딸린 거라능",
          "original_form": "ㄴㄴ 레노버 요가북이라고 있다, 키보드랑 펜 딸린 거라능",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045001.1.22",
          "form": "이젠 늙어서 배터리도 빨리 닳어",
          "original_form": "이젠 늙어서 배터리도 빨리 닳어",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045001.1.23",
          "form": "앗 먼가 들었던거같다",
          "original_form": "앗 먼가 들었던거같다",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045001.1.24",
          "form": "한~~~참 예전에 샀제?",
          "original_form": "한~~~참 예전에 샀제?",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045001.1.25",
          "form": "ㅋㅋㅋㅋㅋ늙어서",
          "original_form": "ㅋㅋㅋㅋㅋ늙어서",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045001.1.26",
          "form": "ㅇㅇ 몇년 됐지ㅋㅋㅋ",
          "original_form": "ㅇㅇ 몇년 됐지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045001.1.27",
          "form": "내동생은 오늘 태블릿 질렀다",
          "original_form": "내동생은 오늘 태블릿 질렀다",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045001.1.28",
          "form": "10만원대래",
          "original_form": "10만원대래",
          "speaker_id": "2",
          "time": "20210908 00:52"
        }
      ]
    }
