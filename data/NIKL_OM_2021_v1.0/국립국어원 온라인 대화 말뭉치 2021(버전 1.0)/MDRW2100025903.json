      "utterance": [
        {
          "id": "MDRW2100025903.1.1",
          "form": "좋아하는 가수가 있나요",
          "original_form": "좋아하는 가수가 있나요",
          "speaker_id": "1",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025903.1.2",
          "form": "아이즈원이요",
          "original_form": "아이즈원이요",
          "speaker_id": "2",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025903.1.3",
          "form": "아이즈원 누굴 젤 좋아하나요",
          "original_form": "아이즈원 누굴 젤 좋아하나요",
          "speaker_id": "1",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025903.1.4",
          "form": "미야와키 사쿠라가 제일 좋습니다",
          "original_form": "미야와키 사쿠라가 제일 좋습니다",
          "speaker_id": "2",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025903.1.5",
          "form": "파이리는요?",
          "original_form": "파이리는요?",
          "speaker_id": "2",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025903.1.6",
          "form": "저는 에이프릴 좋아했었는데 요새는 오마이걸이 눈에 들어오네요",
          "original_form": "저는 에이프릴 좋아했었는데 요새는 오마이걸이 눈에 들어오네요",
          "speaker_id": "1",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100025903.1.7",
          "form": "전부 걸그룹이네요ㅎㅎ",
          "original_form": "전부 걸그룹이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100025903.1.8",
          "form": "하이브 신규그룹 같이 덕질해요",
          "original_form": "하이브 신규그룹 같이 덕질해요",
          "speaker_id": "2",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100025903.1.9",
          "form": "하이브 추가 멤버,나왔나요?",
          "original_form": "하이브 추가 멤버,나왔나요?",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025903.1.10",
          "form": "아직요 사쿠라  채원밖에 몰라요",
          "original_form": "아직요 사쿠라  채원밖에 몰라요",
          "speaker_id": "2",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025903.1.11",
          "form": "그치만 기대돼요",
          "original_form": "그치만 기대돼요",
          "speaker_id": "2",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025903.1.12",
          "form": "허윤진은요?",
          "original_form": "허윤진은요?",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025903.1.13",
          "form": "안좋아해요 안티질은 안할거지만 딱히 좋지도 않네요",
          "original_form": "안좋아해요 안티질은 안할거지만 딱히 좋지도 않네요",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025903.1.14",
          "form": "아쉽네요 아이즈원 다싶모이는줄 알았는데",
          "original_form": "아쉽네요 아이즈원 다싶모이는줄 알았는데",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025903.1.15",
          "form": "하이브에서 다른 걸그룹 데려왔던데요",
          "original_form": "하이브에서 다른 걸그룹 데려왔던데요",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025903.1.16",
          "form": "누구요?",
          "original_form": "누구요?",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100025903.1.17",
          "form": "프로미스나인?",
          "original_form": "프로미스나인?",
          "speaker_id": "1",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100025903.1.18",
          "form": "헐 프로미스나인을 하이브가요?",
          "original_form": "헐 프로미스나인을 하이브가요?",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025903.1.19",
          "form": "냉 걔들은 프듀 애들보다 오래간다 생각했네요",
          "original_form": "냉 걔들은 프듀 애들보다 오래간다 생각했네요",
          "speaker_id": "1",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025903.1.20",
          "form": "인기도 없더만 그룹 명줄은 디게 기네요",
          "original_form": "인기도 없더만 그룹 명줄은 디게 기네요",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025903.1.21",
          "form": "그니까요 차라리 그 프로그램에서 되는게 계약이 없었나봐요",
          "original_form": "그니까요 차라리 그 프로그램에서 되는게 계약이 없었나봐요",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025903.1.22",
          "form": "부러워서 막말나오네요",
          "original_form": "부러워서 막말나오네요",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025903.1.23",
          "form": "걔네는 손익분기점 못넘던거 같은데",
          "original_form": "걔네는 손익분기점 못넘던거 같은데",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025903.1.24",
          "form": "그래도 뭔가 가치를 보지않았을까요 방시혁이",
          "original_form": "그래도 뭔가 가치를 보지않았을까요 방시혁이",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025903.1.25",
          "form": "아이즈원과 반대로 됐네요",
          "original_form": "아이즈원과 반대로 됐네요",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025903.1.26",
          "form": "이상하게 그렇더라구요",
          "original_form": "이상하게 그렇더라구요",
          "speaker_id": "1",
          "time": "20210901 17:58"
        }
      ]
