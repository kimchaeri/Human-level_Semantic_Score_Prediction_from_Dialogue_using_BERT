      "utterance": [
        {
          "id": "MDRW2100016165.1.1",
          "form": "최근에 포켓몬 리메 신작 나온단 정보를 들어서",
          "original_form": "최근에 포켓몬 리메 신작 나온단 정보를 들어서",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016165.1.2",
          "form": "오늘 꿈에도 그게 나왔음;;",
          "original_form": "오늘 꿈에도 그게 나왔음;;",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016165.1.3",
          "form": "ㅋㅋㅋ요번거 재밌어보이던데",
          "original_form": "ㅋㅋㅋ요번거 재밌어보이던데",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016165.1.4",
          "form": "이전에 나온 티저는 별로였는데",
          "original_form": "이전에 나온 티저는 별로였는데",
          "speaker_id": "2",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016165.1.5",
          "form": "새로나온 티저 괜찮더라",
          "original_form": "새로나온 티저 괜찮더라",
          "speaker_id": "2",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016165.1.6",
          "form": "맞아ㅋㅋㅋ 할거임?",
          "original_form": "맞아ㅋㅋㅋ 할거임?",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016165.1.7",
          "form": "아니 닌텐도 없다",
          "original_form": "아니 닌텐도 없다",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016165.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ그리고 박력이 부족해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그리고 박력이 부족해",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016165.1.9",
          "form": "포켓몬 기술적인 전성기가 BW이후로 경신이 안된것같아서",
          "original_form": "포켓몬 기술적인 전성기가 BW이후로 경신이 안된것같아서",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016165.1.10",
          "form": "ㅋㅋㅋㅋ해볼까 사전 할인 안하나?",
          "original_form": "ㅋㅋㅋㅋ해볼까 사전 할인 안하나?",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016165.1.11",
          "form": "포켓몬 해본적있음?",
          "original_form": "포켓몬 해본적있음?",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016165.1.12",
          "form": "어릴때 복제판치던 시절엔 해봤지",
          "original_form": "어릴때 복제판치던 시절엔 해봤지",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016165.1.13",
          "form": "나도 그때 접해봤음ㅋㅋ",
          "original_form": "나도 그때 접해봤음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016165.1.14",
          "form": "루비사파이어 시절에 게임기로 잠깐해보고",
          "original_form": "루비사파이어 시절에 게임기로 잠깐해보고",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016165.1.15",
          "form": "어른이 되서 하려니 시간 너무 많이 잡아먹는게임이라",
          "original_form": "어른이 되서 하려니 시간 너무 많이 잡아먹는게임이라",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016165.1.16",
          "form": "그렇긴 하지 게임도 비싸구ㅋㅋ",
          "original_form": "그렇긴 하지 게임도 비싸구ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016165.1.17",
          "form": "그래서 스위치도 안사고있음 장롱행 될거같아서ㅍ",
          "original_form": "그래서 스위치도 안사고있음 장롱행 될거같아서ㅍ",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016165.1.18",
          "form": "난 그냥 샀는데",
          "original_form": "난 그냥 샀는데",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016165.1.19",
          "form": "사고 시프니깐..ㅋㅋㅋ",
          "original_form": "사고 시프니깐..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016165.1.20",
          "form": "ㅋㅋㅋㅋ 사고싶음 사야지",
          "original_form": "ㅋㅋㅋㅋ 사고싶음 사야지",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100016165.1.21",
