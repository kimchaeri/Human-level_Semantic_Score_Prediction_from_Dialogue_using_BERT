      "utterance": [
        {
          "id": "MDRW2100002429.1.1",
          "form": "카페 가는 거 좋아하시나요??",
          "original_form": "카페 가는 거 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210820 12:07"
        },
        {
          "id": "MDRW2100002429.1.2",
          "form": "시간 있을 때 가는 거 좋아해요",
          "original_form": "시간 있을 때 가는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:07"
        },
        {
          "id": "MDRW2100002429.1.3",
          "form": "카페가서 주로 뭐 드시나요",
          "original_form": "카페가서 주로 뭐 드시나요",
          "speaker_id": "2",
          "time": "20210820 12:07"
        },
        {
          "id": "MDRW2100002429.1.4",
          "form": "저는 요즘 흑임자에 빠져서.. 흑임자라떼도 좋고 그냥 아아도 좋아요",
          "original_form": "저는 요즘 흑임자에 빠져서.. 흑임자라떼도 좋고 그냥 아아도 좋아요",
          "speaker_id": "1",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100002429.1.5",
          "form": "흑임자 좋아하세요??",
          "original_form": "흑임자 좋아하세요??",
          "speaker_id": "1",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100002429.1.6",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "2",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100002429.1.7",
          "form": "흑임자 인절미 이런거 좋아해요",
          "original_form": "흑임자 인절미 이런거 좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100002429.1.8",
          "form": "꼬숩잖아요",
          "original_form": "꼬숩잖아요",
          "speaker_id": "2",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100002429.1.9",
          "form": "아 맞아요ㅠㅠㅠ",
          "original_form": "아 맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100002429.1.10",
          "form": "저 펄도 좋아해서 흑당라때 덜달게 해서 먹는것도 좋아해요",
          "original_form": "저 펄도 좋아해서 흑당라때 덜달게 해서 먹는것도 좋아해요",
          "speaker_id": "1",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100002429.1.11",
          "form": "흑당 진짜 맛있는데",
          "original_form": "흑당 진짜 맛있는데",
          "speaker_id": "2",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100002429.1.12",
          "form": "달달하고 당충전 되는 기분",
          "original_form": "달달하고 당충전 되는 기분",
          "speaker_id": "2",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100002429.1.13",
          "form": "디져트류는 뭐 좋아하세요,",
          "original_form": "디져트류는 뭐 좋아하세요,",
          "speaker_id": "2",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100002429.1.14",
          "form": "단 게 확 느껴져요!",
          "original_form": "단 게 확 느껴져요!",
          "speaker_id": "1",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100002429.1.15",
          "form": "저는 타르트도 좋고 케이크도 좋아요",
          "original_form": "저는 타르트도 좋고 케이크도 좋아요",
          "speaker_id": "1",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100002429.1.16",
          "form": "한때 투썸 딸기케이크에 빠졌는데 스초생 드셔보셨나요??",
          "original_form": "한때 투썸 딸기케이크에 빠졌는데 스초생 드셔보셨나요??",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002429.1.17",
          "form": "뭔가 포켓몬스터 초코롤 느낌도 나는.. 스초생",
          "original_form": "뭔가 포켓몬스터 초코롤 느낌도 나는.. 스초생",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002429.1.18",
          "form": "그거 유명하잖아요",
          "original_form": "그거 유명하잖아요",
          "speaker_id": "2",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002429.1.19",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002429.1.20",
          "form": "전 그거랑 아이스박스좋아해요",
          "original_form": "전 그거랑 아이스박스좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002429.1.21",
          "form": "달콤한게 진짜",
          "original_form": "달콤한게 진짜",
          "speaker_id": "2",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002429.1.22",
          "form": "아이스박스도 맛있죠.. 그게 투탑같아요",
          "original_form": "아이스박스도 맛있죠.. 그게 투탑같아요",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002429.1.23",
          "form": "기분좋아져요",
          "original_form": "기분좋아져요",
          "speaker_id": "2",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002429.1.24",
          "form": "말하니까 먹고싶네요",
          "original_form": "말하니까 먹고싶네요",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100002429.1.25",
          "form": "대화나누니꺼 또 가고싶네요",
          "original_form": "대화나누니꺼 또 가고싶네요",
