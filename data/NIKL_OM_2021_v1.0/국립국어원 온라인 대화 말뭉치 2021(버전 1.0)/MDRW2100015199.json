      "utterance": [
        {
          "id": "MDRW2100015199.1.1",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015199.1.2",
          "form": "공연보는거 좋아하시나요?",
          "original_form": "공연보는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015199.1.3",
          "form": "네 좋아해요 name1님은요?",
          "original_form": "네 좋아해요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015199.1.4",
          "form": "오 그러시구나!",
          "original_form": "오 그러시구나!",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015199.1.5",
          "form": "저는 공연같은거 제대로 본적은 없는거같아요!ㅠ",
          "original_form": "저는 공연같은거 제대로 본적은 없는거같아요!ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015199.1.6",
          "form": "가면 재미있을거같은데!",
          "original_form": "가면 재미있을거같은데!",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015199.1.7",
          "form": "어떤 공연들 보셨어요?",
          "original_form": "어떤 공연들 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015199.1.8",
          "form": "저는 거의 뮤지컬봤었어요",
          "original_form": "저는 거의 뮤지컬봤었어요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015199.1.9",
          "form": "연극이나 ㅋㅋ근데 많이 안봤어요",
          "original_form": "연극이나 ㅋㅋ근데 많이 안봤어요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015199.1.10",
          "form": "대학로 에서요",
          "original_form": "대학로 에서요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015199.1.11",
          "form": "아하 그러시구나",
          "original_form": "아하 그러시구나",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015199.1.12",
          "form": "대학로에서 그런거 보면 재미있을거같아요!ㅎㅎ",
          "original_form": "대학로에서 그런거 보면 재미있을거같아요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015199.1.13",
          "form": "아 저는 서커스 본적있어요!",
          "original_form": "아 저는 서커스 본적있어요!",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015199.1.14",
          "form": "오 재밌었어요?? 신기할거같아요",
          "original_form": "오 재밌었어요?? 신기할거같아요",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015199.1.15",
          "form": "네 생각보다 스토리도 있고 재미있더라구요!",
          "original_form": "네 생각보다 스토리도 있고 재미있더라구요!",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015199.1.16",
          "form": "처음에는 그렇게 보고싶었던건 아니엇는데 직접보니깐 진짜 놀라웠어요!ㅋㅋㅋ",
          "original_form": "처음에는 그렇게 보고싶었던건 아니엇는데 직접보니깐 진짜 놀라웠어요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015199.1.17",
          "form": "오 어디서 보셨나요??",
          "original_form": "오 어디서 보셨나요??",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015199.1.18",
          "form": "잠실쪽에서 태양의 서커스라는거 봤었어요!",
          "original_form": "잠실쪽에서 태양의 서커스라는거 봤었어요!",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015199.1.19",
          "form": "와 꼭 봐봐야겠네요 궁금해요",
          "original_form": "와 꼭 봐봐야겠네요 궁금해요",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015199.1.20",
          "form": "영화관에서 영화는 안보세요??",
          "original_form": "영화관에서 영화는 안보세요??",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015199.1.21",
          "form": "생각보다 정말 재미있었어요!ㅎㅎ 추천합니당",
          "original_form": "생각보다 정말 재미있었어요!ㅎㅎ 추천합니당",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015199.1.22",
          "form": "아 영화는 생각을 못했네요 영화도 좋아해요!",
          "original_form": "아 영화는 생각을 못했네요 영화도 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015199.1.23",
          "form": "어떤 영화 좋아하시나요?",
          "original_form": "어떤 영화 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015199.1.24",
          "form": "저는최근에  극장에서 블랙위도우",
          "original_form": "저는최근에  극장에서 블랙위도우",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015199.1.25",
          "form": "봫어요!!",
          "original_form": "봫어요!!",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015199.1.26",
          "form": "오 저 마블영화엄청 좋아해요!",
          "original_form": "오 저 마블영화엄청 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015199.1.27",
          "form": "블랙위도우 재미있더라구요ㅎㅎ",
          "original_form": "블랙위도우 재미있더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015199.1.28",
          "form": "오 저도 엄청좋아해요",
          "original_form": "오 저도 엄청좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015199.1.29",
          "form": "마블영화는 무조건",
          "original_form": "마블영화는 무조건",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015199.1.30",
          "form": "극장에서 봐야죠 ㅋㅋㅋ",
          "original_form": "극장에서 봐야죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015199.1.31",
          "form": "맞아요 마블영화는 진짜 극장에서 봐야죠!ㅋㅋㅋ",
          "original_form": "맞아요 마블영화는 진짜 극장에서 봐야죠!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015199.1.32",
          "form": "저 엔드게임은 영화관에서만 두번봤아요!ㅋㅋㅋㅋ",
          "original_form": "저 엔드게임은 영화관에서만 두번봤아요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015199.1.33",
          "form": "진짜 그거보고 울었어요 젤 재밌어요 ㅠㅠ",
