      "utterance": [
        {
          "id": "MDRW2100021625.1.1",
          "form": "안녕하세요 저녁 뭐 드셨나요?",
          "original_form": "안녕하세요 저녁 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021625.1.2",
          "form": "네ㅎ 오늘 치킨이네요 루꼴라님은요?",
          "original_form": "네ㅎ 오늘 치킨이네요 루꼴라님은요?",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021625.1.3",
          "form": "엇 저도 치킨 먹었는데!",
          "original_form": "엇 저도 치킨 먹었는데!",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021625.1.4",
          "form": "불토 치킨이네요ㅎ",
          "original_form": "불토 치킨이네요ㅎ",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021625.1.5",
          "form": "닭똥집 튀김이랑 같이 먹었어요",
          "original_form": "닭똥집 튀김이랑 같이 먹었어요",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021625.1.6",
          "form": "앗 저는 그냥  치킨만요",
          "original_form": "앗 저는 그냥  치킨만요",
          "speaker_id": "2",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021625.1.7",
          "form": "무슨 치킨 드셨어요?",
          "original_form": "무슨 치킨 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021625.1.8",
          "form": "교촌이요 오랜만에먹으니 맛있네요",
          "original_form": "교촌이요 오랜만에먹으니 맛있네요",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021625.1.9",
          "form": "젤 맛있는거 드셨네요 ㅋㅋㅋㅋ",
          "original_form": "젤 맛있는거 드셨네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021625.1.10",
          "form": "어디꺼 주로드세요?",
          "original_form": "어디꺼 주로드세요?",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021625.1.11",
          "form": "식구가 다 대식가라 두마리치킨 쪽을 주로 먹어요 ㅋㅋ",
          "original_form": "식구가 다 대식가라 두마리치킨 쪽을 주로 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021625.1.12",
          "form": "아ㅎ 두마리 좋죠ㅎㅎ 사실교촌은 간식이죠 양이 너무작아요",
          "original_form": "아ㅎ 두마리 좋죠ㅎㅎ 사실교촌은 간식이죠 양이 너무작아요",
          "speaker_id": "2",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021625.1.13",
          "form": "그쵸그쵸 거긴 맛 없었으면 애저녁에 망했을거에요",
          "original_form": "그쵸그쵸 거긴 맛 없었으면 애저녁에 망했을거에요",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021625.1.14",
          "form": "편하게 치킨이 좋네요",
          "original_form": "편하게 치킨이 좋네요",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021625.1.15",
          "form": "후라이드파세요?",
          "original_form": "후라이드파세요?",
          "speaker_id": "1",
          "time": "20210828 22:51"
        }
      ]
