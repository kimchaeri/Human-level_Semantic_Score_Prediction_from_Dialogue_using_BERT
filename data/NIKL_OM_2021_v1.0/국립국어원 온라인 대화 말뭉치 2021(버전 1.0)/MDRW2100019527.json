      "utterance": [
        {
          "id": "MDRW2100019527.1.1",
          "form": "난 토끼를 키워봤어",
          "original_form": "난 토끼를 키워봤어",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019527.1.2",
          "form": "난 한번도 안키워봤어",
          "original_form": "난 한번도 안키워봤어",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019527.1.3",
          "form": "귀여웠어 정말",
          "original_form": "귀여웠어 정말",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019527.1.4",
          "form": "어떤 점이?",
          "original_form": "어떤 점이?",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019527.1.5",
          "form": "나를 좋아해주는게 느껴져",
          "original_form": "나를 좋아해주는게 느껴져",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019527.1.6",
          "form": "토끼도 애교가 많아?",
          "original_form": "토끼도 애교가 많아?",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019527.1.7",
          "form": "내 바니는 그랬어",
          "original_form": "내 바니는 그랬어",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019527.1.8",
          "form": "먹이론 뭘 줬어?",
          "original_form": "먹이론 뭘 줬어?",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019527.1.9",
          "form": "사료를 주로 줬어",
          "original_form": "사료를 주로 줬어",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019527.1.10",
          "form": "야채를 먹는줄 알았는데",
          "original_form": "야채를 먹는줄 알았는데",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019527.1.11",
          "form": "그것도 주긴 주지",
          "original_form": "그것도 주긴 주지",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019527.1.12",
          "form": "간식으로?",
          "original_form": "간식으로?",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019527.1.13",
          "form": "맞아 잘 아는구나",
          "original_form": "맞아 잘 아는구나",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019527.1.14",
          "form": "ㅎㅎ귀여웠겠다",
          "original_form": "ㅎㅎ귀여웠겠다",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019527.1.15",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
