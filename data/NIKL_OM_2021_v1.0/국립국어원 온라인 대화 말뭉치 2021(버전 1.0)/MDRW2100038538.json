      "utterance": [
        {
          "id": "MDRW2100038538.1.1",
          "form": "더 속이 허한 기분이에요",
          "original_form": "더 속이 허한 기분이에요",
          "speaker_id": "1",
          "time": "20210902 15:32"
        },
        {
          "id": "MDRW2100038538.1.2",
          "form": "그리고 괜히 아 저녁 늦게 먹으니까",
          "original_form": "그리고 괜히 아 저녁 늦게 먹으니까",
          "speaker_id": "1",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100038538.1.3",
          "form": "과자먹어둬야겠다고 합리화 ㅋㅋㅋㅋㅋ",
          "original_form": "과자먹어둬야겠다고 합리화 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100038538.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100038538.1.5",
          "form": "오늘은 약과 사와사 먹는데",
          "original_form": "오늘은 약과 사와사 먹는데",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100038538.1.6",
          "form": "은근 땡기네요 이거",
          "original_form": "은근 땡기네요 이거",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100038538.1.7",
          "form": "오 그 미니약과?",
          "original_form": "오 그 미니약과?",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038538.1.8",
          "form": "아니면 큰 거 사오셨나요",
          "original_form": "아니면 큰 거 사오셨나요",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038538.1.9",
          "form": "네네 미니약과!",
          "original_form": "네네 미니약과!",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038538.1.10",
          "form": "ㅋㅋ저희집 냉장고에도 있어요 ㅋㅋㅋ그래도 이건 엄마도 가끔 당떨어지실때 드시더라구요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ저희집 냉장고에도 있어요 ㅋㅋㅋ그래도 이건 엄마도 가끔 당떨어지실때 드시더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100038538.1.11",
          "form": "저도 가끔 생각날때가 잇더라구요 ㅋㅋㅋ",
          "original_form": "저도 가끔 생각날때가 잇더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100038538.1.12",
          "form": "전 첨 사먹어봣는데 좋네요 ㅋㅋㅋ",
          "original_form": "전 첨 사먹어봣는데 좋네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100038538.1.13",
          "form": "ㅋㅋㅋ음료랑 같이 드셨나요?",
          "original_form": "ㅋㅋㅋ음료랑 같이 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100038538.1.14",
          "form": "네네 요구르트",
          "original_form": "네네 요구르트",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100038538.1.15",
          "form": "헐 조합 짱이네요 ㅋㅋㅋ 요즘엔 요구르트도 빅요구르트 크게나오던데 드셔보셨나용",
          "original_form": "헐 조합 짱이네요 ㅋㅋㅋ 요즘엔 요구르트도 빅요구르트 크게나오던데 드셔보셨나용",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100038538.1.16",
          "form": "네 ㅋㅋㅋㅋ 좋아하는데 뭔가 충치생길거같은 기분이에요",
          "original_form": "네 ㅋㅋㅋㅋ 좋아하는데 뭔가 충치생길거같은 기분이에요",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100038538.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋ 마자요 그리고 요즘 젊은 당뇨인이 많다해서 먹으면서 걱정해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋ 마자요 그리고 요즘 젊은 당뇨인이 많다해서 먹으면서 걱정해요",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038538.1.18",
          "form": "마자요ㅠ 당뇨 걱정해야겠더라구요 진짜",
          "original_form": "마자요ㅠ 당뇨 걱정해야겠더라구요 진짜",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100038538.1.19",
          "form": "점심먹고 단 커피 맨날 먹는데 그게 젤 위험하다고 저격당했어요",
          "original_form": "점심먹고 단 커피 맨날 먹는데 그게 젤 위험하다고 저격당했어요",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100038538.1.20",
          "form": "그쵸 저도 밥먹고 맨날 달달한 과자나 과일먹거든요 ㅠㅠ",
          "original_form": "그쵸 저도 밥먹고 맨날 달달한 과자나 과일먹거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100038538.1.21",
          "form": "당뇨 조심합시당 ㅠ",
          "original_form": "당뇨 조심합시당 ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:18"
        }
      ]
