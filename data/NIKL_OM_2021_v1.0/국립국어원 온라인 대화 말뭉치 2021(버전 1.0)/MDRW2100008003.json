      "utterance": [
        {
          "id": "MDRW2100008003.1.1",
          "form": "나 요새 농장겜 다시 시작했다",
          "original_form": "나 요새 농장겜 다시 시작했다",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100008003.1.2",
          "form": "너도 스샷 봤으려나?",
          "original_form": "너도 스샷 봤으려나?",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100008003.1.3",
          "form": "옹",
          "original_form": "옹",
          "speaker_id": "1",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100008003.1.4",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100008003.1.5",
          "form": "응이야 아니얔ㅋㅋ",
          "original_form": "응이야 아니얔ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100008003.1.6",
          "form": "아니 스샷 안봤어",
          "original_form": "아니 스샷 안봤어",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008003.1.7",
          "form": "어디다 올렸었어?",
          "original_form": "어디다 올렸었어?",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008003.1.8",
          "form": "트위터에",
          "original_form": "트위터에",
          "speaker_id": "2",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100008003.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100008003.1.10",
          "form": "요새 sns 잘 안보니깐 몰랐어ㅠ",
          "original_form": "요새 sns 잘 안보니깐 몰랐어ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100008003.1.11",
          "form": "묻혔나봐",
          "original_form": "묻혔나봐",
          "speaker_id": "1",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100008003.1.12",
          "form": "5주년 기념 이벤트 한다고 해서 게임 다시 시작했지",
          "original_form": "5주년 기념 이벤트 한다고 해서 게임 다시 시작했지",
          "speaker_id": "2",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100008003.1.13",
          "form": "농장겜 너무 이쁜것같어ㅠ",
          "original_form": "농장겜 너무 이쁜것같어ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100008003.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100008003.1.15",
          "form": "벌써 5주년",
          "original_form": "벌써 5주년",
          "speaker_id": "1",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100008003.1.16",
          "form": "맞아 특히 그래픽이 아기자기한게",
          "original_form": "맞아 특히 그래픽이 아기자기한게",
          "speaker_id": "2",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100008003.1.17",
          "form": "5주년 진짜 안믿기지?",
          "original_form": "5주년 진짜 안믿기지?",
          "speaker_id": "2",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100008003.1.18",
          "form": "그거 게임 출시할 때가 엊그제같은데ㅋㅋ",
          "original_form": "그거 게임 출시할 때가 엊그제같은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100008003.1.19",
          "form": "어쨌든 오랜만에 들어갔는데",
          "original_form": "어쨌든 오랜만에 들어갔는데",
          "speaker_id": "2",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008003.1.20",
          "form": "오랜만이라 그런가 의욕이 나서 요새 열심히 하고 있음",
          "original_form": "오랜만이라 그런가 의욕이 나서 요새 열심히 하고 있음",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008003.1.21",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "1",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008003.1.22",
          "form": "5주년 무슨 이벤트중이야?",
          "original_form": "5주년 무슨 이벤트중이야?",
          "speaker_id": "1",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008003.1.23",
          "form": "테마 아이템 주는 이벤트\r\n젬도 좀 주고",
          "original_form": "테마 아이템 주는 이벤트\r\n젬도 좀 주고",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008003.1.24",
          "form": "근데 5주년 이벤트치고는 좀 싱거워",
          "original_form": "근데 5주년 이벤트치고는 좀 싱거워",
          "speaker_id": "2",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008003.1.25",
          "form": "그렇네",
          "original_form": "그렇네",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008003.1.26",
          "form": "젬 많이 줘야하는데",
          "original_form": "젬 많이 줘야하는데",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008003.1.27",
          "form": "응 정말 별 거 없고, 돈 쓰게 하는 이벤트만 너무 심하게 열더라고",
          "original_form": "응 정말 별 거 없고, 돈 쓰게 하는 이벤트만 너무 심하게 열더라고",
          "speaker_id": "2",
          "time": "20210822 19:58"
        },
        {
          "id": "MDRW2100008003.1.28",
          "form": "아쉬웠어",
          "original_form": "아쉬웠어",
          "speaker_id": "2",
          "time": "20210822 19:59"
        }
      ]
    }
