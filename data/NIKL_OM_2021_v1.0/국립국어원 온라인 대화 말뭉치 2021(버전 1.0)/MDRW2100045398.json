      "utterance": [
        {
          "id": "MDRW2100045398.1.1",
          "form": "국물이 있는 요리 인가요?",
          "original_form": "국물이 있는 요리 인가요?",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045398.1.2",
          "form": "네! 국물요리입니다",
          "original_form": "네! 국물요리입니다",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045398.1.3",
          "form": "맑은 국물인가요?",
          "original_form": "맑은 국물인가요?",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045398.1.4",
          "form": "아니요 국물이 빨개요!",
          "original_form": "아니요 국물이 빨개요!",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045398.1.5",
          "form": "중식인가요?",
          "original_form": "중식인가요?",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045398.1.6",
          "form": "아뇨 한식입니다 홀홀",
          "original_form": "아뇨 한식입니다 홀홀",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045398.1.7",
          "form": "쌀로 만든 재료가 들어가나요??",
          "original_form": "쌀로 만든 재료가 들어가나요??",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045398.1.8",
          "form": "쌀으로 만든 밥과 곁들여 먹여요",
          "original_form": "쌀으로 만든 밥과 곁들여 먹여요",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045398.1.9",
          "form": "먹어요",
          "original_form": "먹어요",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045398.1.10",
          "form": "채소가 들어가나요?",
          "original_form": "채소가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045398.1.11",
          "form": "네! 야채가 들어갑니다",
          "original_form": "네! 야채가 들어갑니다",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045398.1.12",
          "form": "두부도 들어가나요??",
          "original_form": "두부도 들어가나요??",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045398.1.13",
          "form": "네! 두부도 들어가요",
          "original_form": "네! 두부도 들어가요",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045398.1.14",
          "form": "설마 김치찌개인가요?!",
          "original_form": "설마 김치찌개인가요?!",
          "speaker_id": "2",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045398.1.15",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045398.1.16",
          "form": "어떻게 바로 맞추셨어요 대박",
          "original_form": "어떻게 바로 맞추셨어요 대박",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045398.1.17",
          "form": "독심술 쓰신다",
          "original_form": "독심술 쓰신다",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045398.1.18",
          "form": "와!! 순두부랑 김치 콩나물중에 고민했는데",
          "original_form": "와!! 순두부랑 김치 콩나물중에 고민했는데",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045398.1.19",
          "form": "김치찌개 정답입니다~~~~!!!",
          "original_form": "김치찌개 정답입니다~~~~!!!",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045398.1.20",
          "form": "맞춰서 좋네요!!!",
          "original_form": "맞춰서 좋네요!!!",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045398.1.21",
          "form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045398.1.22",
          "form": "잘 찍으셨어요!!",
          "original_form": "잘 찍으셨어요!!",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045398.1.23",
          "form": "혹시 점심메뉴도 기억하세요?",
          "original_form": "혹시 점심메뉴도 기억하세요?",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045398.1.24",
          "form": "그럼 제가 맞춰볼게요 ㅋㅋㅋㅋ",
          "original_form": "그럼 제가 맞춰볼게요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045398.1.25",
          "form": "당연히 기억하죠!!",
          "original_form": "당연히 기억하죠!!",
