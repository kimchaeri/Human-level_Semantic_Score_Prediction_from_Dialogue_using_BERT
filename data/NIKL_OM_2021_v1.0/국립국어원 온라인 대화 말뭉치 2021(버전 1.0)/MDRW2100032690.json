      "utterance": [
        {
          "id": "MDRW2100032690.1.1",
          "form": "젊은이들은",
          "original_form": "젊은이들은",
          "speaker_id": "1",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100032690.1.2",
          "form": "놀이공원가죠",
          "original_form": "놀이공원가죠",
          "speaker_id": "1",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032690.1.3",
          "form": "읍성은 신라인들 가는 데 아닌가요?",
          "original_form": "읍성은 신라인들 가는 데 아닌가요?",
          "speaker_id": "1",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032690.1.4",
          "form": "데이트를 특이한 곳에서 하시네요",
          "original_form": "데이트를 특이한 곳에서 하시네요",
          "speaker_id": "1",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032690.1.5",
          "form": "놀이공원 나도 가고 싳네요",
          "original_form": "놀이공원 나도 가고 싳네요",
          "speaker_id": "2",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032690.1.6",
          "form": "남친이 그런 옛스러운 걸 좋아해서요",
          "original_form": "남친이 그런 옛스러운 걸 좋아해서요",
          "speaker_id": "2",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032690.1.7",
          "form": "읍성은 별로였나요?",
          "original_form": "읍성은 별로였나요?",
          "speaker_id": "1",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032690.1.8",
          "form": "읍성 괜찮았어요 이뻤네요 풍경이",
          "original_form": "읍성 괜찮았어요 이뻤네요 풍경이",
          "speaker_id": "2",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032690.1.9",
          "form": "커플 사진 찍으셨어요?",
          "original_form": "커플 사진 찍으셨어요?",
          "speaker_id": "1",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032690.1.10",
          "form": "오늘 뭐하셨어요 데이트 안했나요",
          "original_form": "오늘 뭐하셨어요 데이트 안했나요",
          "speaker_id": "2",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032690.1.11",
          "form": "네 찍었죠",
          "original_form": "네 찍었죠",
          "speaker_id": "2",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032690.1.12",
          "form": "저는 대청소했어요",
          "original_form": "저는 대청소했어요",
          "speaker_id": "1",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032690.1.13",
          "form": "데이트 안하셨어요?",
          "original_form": "데이트 안하셨어요?",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032690.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032690.1.15",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032690.1.16",
          "form": "허 참",
          "original_form": "허 참",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032690.1.17",
          "form": "안 할 수도 있죠",
          "original_form": "안 할 수도 있죠",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032690.1.18",
          "form": "그렇군요 별로 사랑하지 않으신가요",
          "original_form": "그렇군요 별로 사랑하지 않으신가요",
          "speaker_id": "2",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032690.1.19",
          "form": "사랑합니다",
          "original_form": "사랑합니다",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032690.1.20",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032690.1.21",
          "form": "주말엔 안 만납니다",
          "original_form": "주말엔 안 만납니다",
          "speaker_id": "1",
          "time": "20210905 22:22"
        },
        {
          "id": "MDRW2100032690.1.22",
          "form": "남친 공부해야 돼요",
          "original_form": "남친 공부해야 돼요",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032690.1.23",
          "form": "남친이 시험 준비하시나봐여",
          "original_form": "남친이 시험 준비하시나봐여",
          "speaker_id": "2",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032690.1.24",
          "form": "배려가 많으시네요",
          "original_form": "배려가 많으시네요",
          "speaker_id": "2",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032690.1.25",
          "form": "남친 직업이 공부입니다",
          "original_form": "남친 직업이 공부입니다",
