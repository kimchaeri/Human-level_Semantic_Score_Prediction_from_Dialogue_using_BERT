      "utterance": [
        {
          "id": "MDRW2100024366.1.1",
          "form": "name2님네 고양이도 가끔 삐져요?ㅋㅋㅋ",
          "original_form": "&name2&님네 고양이도 가끔 삐져요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:26"
        },
        {
          "id": "MDRW2100024366.1.2",
          "form": "ㅋㅋㅋ 그냥 맛있는 거 먹는데 안 주면",
          "original_form": "ㅋㅋㅋ 그냥 맛있는 거 먹는데 안 주면",
          "speaker_id": "2",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.3",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.4",
          "form": "등돌리고 앉긴 하는데",
          "original_form": "등돌리고 앉긴 하는데",
          "speaker_id": "2",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.5",
          "form": "금방 풀려욬ㅋㅋ",
          "original_form": "금방 풀려욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.6",
          "form": "ㅋㅋㅋㅋㅋ 너무 귀엽다..",
          "original_form": "ㅋㅋㅋㅋㅋ 너무 귀엽다..",
          "speaker_id": "1",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.7",
          "form": "몇분가여??",
          "original_form": "몇분가여??",
          "speaker_id": "1",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.8",
          "form": "몇분도 아니고...",
          "original_form": "몇분도 아니고...",
          "speaker_id": "2",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.9",
          "form": "그냥 이름부르면 풀려요.....",
          "original_form": "그냥 이름부르면 풀려요.....",
          "speaker_id": "2",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.10",
          "form": "헉.. 완전 귀엽네요 ㅜㅜㅜ",
          "original_form": "헉.. 완전 귀엽네요 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.11",
          "form": "그정도면 부모님께서도 너무 좋아하실것같아요 ㅋㅋㅋ",
          "original_form": "그정도면 부모님께서도 너무 좋아하실것같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:27"
        },
        {
          "id": "MDRW2100024366.1.12",
          "form": "애교도 많아요??",
          "original_form": "애교도 많아요??",
          "speaker_id": "1",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100024366.1.13",
          "form": "네 맨날 와서 부비고 따라다니고ㅠㅠㅠ",
          "original_form": "네 맨날 와서 부비고 따라다니고ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100024366.1.14",
          "form": "배 만지는 것도 좋아하더라고요",
          "original_form": "배 만지는 것도 좋아하더라고요",
          "speaker_id": "2",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100024366.1.15",
          "form": "고양이 아닌 거 같아여..",
          "original_form": "고양이 아닌 거 같아여..",
          "speaker_id": "2",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100024366.1.16",
          "form": "ㅋㅋㅋㅋㅋ 같이 주무세요??ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 같이 주무세요??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:28"
        },
        {
          "id": "MDRW2100024366.1.17",
          "form": "ㅋㅋㅋ넹ㅋㅋ",
          "original_form": "ㅋㅋㅋ넹ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.18",
          "form": "와 진짜 세상을 다 가진기분이시겟어요 ㅋㅋ",
          "original_form": "와 진짜 세상을 다 가진기분이시겟어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.19",
          "form": "다 컸는데도",
          "original_form": "다 컸는데도",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.20",
          "form": "옆에 딱 붙어서 자요",
          "original_form": "옆에 딱 붙어서 자요",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.21",
          "form": "넘귀여뷰ㅠㅠㅠ",
          "original_form": "넘귀여뷰ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.22",
          "form": "ㅋㅋㅋㅋ아이구 ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋ아이구 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.23",
          "form": "여름에 덥진않으세여??ㅋㅋㅋ",
          "original_form": "여름에 덥진않으세여??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.24",
          "form": "근데 강아지랑 고양이가 둘 다 양쪽으로 붙어서 자면",
          "original_form": "근데 강아지랑 고양이가 둘 다 양쪽으로 붙어서 자면",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.25",
          "form": "못움직여서 괴로워요",
          "original_form": "못움직여서 괴로워요",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.26",
          "form": "아..그거 진짜 괴로운데 ㅠㅠㅜ",
          "original_form": "아..그거 진짜 괴로운데 ㅠㅠㅜ",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.27",
          "form": "덥도 추운건 전기제품들이 있어서 괜찮은데..",
          "original_form": "덥도 추운건 전기제품들이 있어서 괜찮은데..",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.28",
          "form": "네내",
          "original_form": "네내",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.29",
          "form": "못움직이는 게 고문이에요...",
          "original_form": "못움직이는 게 고문이에요...",
          "speaker_id": "2",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.30",
          "form": "몸이 피곤하죠..",
          "original_form": "몸이 피곤하죠..",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.31",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 02:29"
        },
        {
          "id": "MDRW2100024366.1.32",
          "form": "반려동물들은 주인분들이 너무 마음에드나봐요 ㅎㅎ",
          "original_form": "반려동물들은 주인분들이 너무 마음에드나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 02:30"
        },
        {
          "id": "MDRW2100024366.1.33",
          "form": "ㅋㅋㅋ제일 웃기고 귀여운 게",
          "original_form": "ㅋㅋㅋ제일 웃기고 귀여운 게",
          "speaker_id": "2",
          "time": "20210831 02:30"
        },
        {
          "id": "MDRW2100024366.1.34",
          "form": "자다보면 한마리씩 우다다다 나가서 지 볼일 보고 다시 오고 번갈아서 그러는데",
          "original_form": "자다보면 한마리씩 우다다다 나가서 지 볼일 보고 다시 오고 번갈아서 그러는데",
          "speaker_id": "2",
