        {
          "id": "MDRW2100037704.1.1",
          "form": "여행 계획 있으세요?",
          "original_form": "여행 계획 있으세요?",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100037704.1.2",
          "form": "코로나땜에 가기가 힘드네요ㅠ",
          "original_form": "코로나땜에 가기가 힘드네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100037704.1.3",
          "form": "그쵸 ㅠ 가까운 나들이도 쉽지 않네요 ㅜ",
          "original_form": "그쵸 ㅠ 가까운 나들이도 쉽지 않네요 ㅜ",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100037704.1.4",
          "form": "마스크벗고 노는게 꿈인 세상이라니ㅠ",
          "original_form": "마스크벗고 노는게 꿈인 세상이라니ㅠ",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100037704.1.5",
          "form": "그러게요 너무 답답하네요 ㅜ",
          "original_form": "그러게요 너무 답답하네요 ㅜ",
          "speaker_id": "2",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100037704.1.6",
          "form": "기념품 많으세요?",
          "original_form": "기념품 많으세요?",
          "speaker_id": "1",
          "time": "20210902 05:36"
        },
        {
          "id": "MDRW2100037704.1.7",
          "form": "전 자석과 열쇠고리요",
          "original_form": "전 자석과 열쇠고리요",
          "speaker_id": "2",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100037704.1.8",
          "form": "저도 자석ㅋ",
          "original_form": "저도 자석ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100037704.1.9",
          "form": "오 많이 모으셨나요 어찌 보관하세요?",
          "original_form": "오 많이 모으셨나요 어찌 보관하세요?",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100037704.1.10",
          "form": "처치곤란인건 그냥 버리는것같아요",
          "original_form": "처치곤란인건 그냥 버리는것같아요",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100037704.1.11",
          "form": "아 ㅎㅎ 점점 미니멀과 멀어지더라구요 ㅋㅋ ㅠ",
          "original_form": "아 ㅎㅎ 점점 미니멀과 멀어지더라구요 ㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100037704.1.12",
          "form": "맞아요 계속 쌓여요",
          "original_form": "맞아요 계속 쌓여요",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100037704.1.13",
          "form": "ㅋㅋㅋ 산건 늘어가지만 여행 못가니 다행히 정지 ㅋㅋ",
