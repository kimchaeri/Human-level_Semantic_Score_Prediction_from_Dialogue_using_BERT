      "utterance": [
        {
          "id": "MDRW2100026791.1.1",
          "form": "혹시 대학생이세요 직장인이세요??",
          "original_form": "혹시 대학생이세요 직장인이세요??",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026791.1.2",
          "form": "8월에 갓 졸업했답니다",
          "original_form": "8월에 갓 졸업했답니다",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026791.1.3",
          "form": "혹시 학생이신가요?",
          "original_form": "혹시 학생이신가요?",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026791.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026791.1.5",
          "form": "전 이번학기가 막학기에요!!",
          "original_form": "전 이번학기가 막학기에요!!",
          "speaker_id": "1",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026791.1.6",
          "form": "막학기면 바쁘시겠다",
          "original_form": "막학기면 바쁘시겠다",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026791.1.7",
          "form": "졸업논문같은거 해야되지않나여???",
          "original_form": "졸업논문같은거 해야되지않나여???",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026791.1.8",
          "form": "이번방학에 실험실인턴하면서",
          "original_form": "이번방학에 실험실인턴하면서",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026791.1.9",
          "form": "작성 끝냈답니다!",
          "original_form": "작성 끝냈답니다!",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026791.1.10",
          "form": "와 고생하셨어요~!!!",
          "original_form": "와 고생하셨어요~!!!",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026791.1.11",
          "form": "저는 영문학관데 졸업시험 토익으로 대체했어요",
          "original_form": "저는 영문학관데 졸업시험 토익으로 대체했어요",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026791.1.12",
          "form": "오오 영문",
          "original_form": "오오 영문",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026791.1.13",
          "form": "전 영어못해요ㅋㅋ",
          "original_form": "전 영어못해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026791.1.14",
          "form": "사람들이 영어영문이면 다 영어를 잘할 줄 아는데 천만의 말씀 만만의 콩떡이랍니다",
          "original_form": "사람들이 영어영문이면 다 영어를 잘할 줄 아는데 천만의 말씀 만만의 콩떡이랍니다",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026791.1.15",
          "form": "영문과도 울면서 수업들어요...",
          "original_form": "영문과도 울면서 수업들어요...",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026791.1.16",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026791.1.17",
          "form": "ㅠㅠ 저도 가끔 영강듣다 오역나고",
          "original_form": "ㅠㅠ 저도 가끔 영강듣다 오역나고",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026791.1.18",
          "form": "한귀로들으면 나머지 귀로 빠져나가고...",
          "original_form": "한귀로들으면 나머지 귀로 빠져나가고...",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026791.1.19",
          "form": "요새는 다들 또 비대면 수업하느라 고생이잖아요",
          "original_form": "요새는 다들 또 비대면 수업하느라 고생이잖아요",
          "speaker_id": "2",
          "time": "20210902 06:19"
        },
        {
          "id": "MDRW2100026791.1.20",
          "form": "물론 저같이 집이랑 학교랑 멀면 행복하게 다니겠지만",
          "original_form": "물론 저같이 집이랑 학교랑 멀면 행복하게 다니겠지만",
          "speaker_id": "2",
          "time": "20210902 06:20"
        },
        {
          "id": "MDRW2100026791.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 06:20"
        },
        {
          "id": "MDRW2100026791.1.22",
          "form": "비대면수업듣고있음 진짜 집중안되고 흘러가버리기...",
          "original_form": "비대면수업듣고있음 진짜 집중안되고 흘러가버리기...",
          "speaker_id": "2",
          "time": "20210902 06:28"
        },
        {
          "id": "MDRW2100026791.1.23",
          "form": "맞아요 녹강이면 밀리고ㅋㅋ",
          "original_form": "맞아요 녹강이면 밀리고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100026791.1.24",
          "form": "아ㅋㅋㅋ저 맨날 출석마감일맞춰서 들었었는데",
          "original_form": "아ㅋㅋㅋ저 맨날 출석마감일맞춰서 들었었는데",
          "speaker_id": "2",
          "time": "20210902 13:00"
        },
        {
          "id": "MDRW2100026791.1.25",
          "form": "진짜 사이버대학교 2년 다녔죠ㅋㅋㅋ",
          "original_form": "진짜 사이버대학교 2년 다녔죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100026791.1.26",
          "form": "저도요ㅋㅋㅋ 사이버대학 2년다니고 졸업합니다",
          "original_form": "저도요ㅋㅋㅋ 사이버대학 2년다니고 졸업합니다",
