      "utterance": [
        {
          "id": "MDRW2100042987.1.1",
          "form": "저는 당근마트요",
          "original_form": "저는 당근마트요",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042987.1.2",
          "form": "중고거래좋아하세요?",
          "original_form": "중고거래좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042987.1.3",
          "form": "사는건 좀 무서워요",
          "original_form": "사는건 좀 무서워요",
          "speaker_id": "1",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042987.1.4",
          "form": "무슨 물건사보셯어요?",
          "original_form": "무슨 물건사보셯어요?",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042987.1.5",
          "form": "저는 어제 바디워시샀어요",
          "original_form": "저는 어제 바디워시샀어요",
          "speaker_id": "1",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042987.1.6",
          "form": "온라인에서요?",
          "original_form": "온라인에서요?",
          "speaker_id": "2",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042987.1.7",
          "form": "네 온라인쇼핑을 선호해요",
          "original_form": "네 온라인쇼핑을 선호해요",
          "speaker_id": "1",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042987.1.8",
          "form": "무슨향 좋아하세요?",
          "original_form": "무슨향 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042987.1.9",
          "form": "남편이랑같이쓰다보니 무난한게좋더라구요",
          "original_form": "남편이랑같이쓰다보니 무난한게좋더라구요",
          "speaker_id": "1",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042987.1.10",
          "form": "대형마트도가세요?",
          "original_form": "대형마트도가세요?",
          "speaker_id": "2",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042987.1.11",
          "form": "한달에한번은 가는것같아요",
          "original_form": "한달에한번은 가는것같아요",
          "speaker_id": "1",
          "time": "20210907 12:36"
        },
        {
