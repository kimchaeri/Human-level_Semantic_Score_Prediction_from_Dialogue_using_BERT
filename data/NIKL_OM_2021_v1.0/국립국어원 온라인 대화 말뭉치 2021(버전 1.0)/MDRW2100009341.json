      "utterance": [
        {
          "id": "MDRW2100009341.1.1",
          "form": "카페 가면 먹는 음료 있나요",
          "original_form": "카페 가면 먹는 음료 있나요",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009341.1.2",
          "form": "스타벅스는 자몽허니블랙티 빠순이 입니다",
          "original_form": "스타벅스는 자몽허니블랙티 빠순이 입니다",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009341.1.3",
          "form": "뭐 좋아하세요??",
          "original_form": "뭐 좋아하세요??",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009341.1.4",
          "form": "악 저는 카페라떼요",
          "original_form": "악 저는 카페라떼요",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009341.1.5",
          "form": "너무 평범한",
          "original_form": "너무 평범한",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009341.1.6",
          "form": "메뉴였다",
          "original_form": "메뉴였다",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009341.1.7",
          "form": "저 스벅에서 그 이번에 나온 비건 함박스테이크 먹어봤는데",
          "original_form": "저 스벅에서 그 이번에 나온 비건 함박스테이크 먹어봤는데",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009341.1.8",
          "form": "너무 맛있더라고요",
          "original_form": "너무 맛있더라고요",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009341.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009341.1.10",
          "form": "저 내일 가서 먹어볼게요!!",
          "original_form": "저 내일 가서 먹어볼게요!!",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009341.1.11",
          "form": "비건 함박스테이크라니",
          "original_form": "비건 함박스테이크라니",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009341.1.12",
          "form": "오 맛있어요!!!!",
          "original_form": "오 맛있어요!!!!",
          "speaker_id": "2",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009341.1.13",
          "form": "그거 추천합니다",
          "original_form": "그거 추천합니다",
          "speaker_id": "2",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009341.1.14",
          "form": "그리고 두유 식빵인가 그것도 비건으로 나왔는데 그건 아주 달아요",
          "original_form": "그리고 두유 식빵인가 그것도 비건으로 나왔는데 그건 아주 달아요",
          "speaker_id": "2",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009341.1.15",
          "form": "단건 잠시 패스할게요",
          "original_form": "단건 잠시 패스할게요",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009341.1.16",
          "form": "비건에 꽂혔습니다",
          "original_form": "비건에 꽂혔습니다",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009341.1.17",
          "form": "낼 꼭먹을거에요!!!",
          "original_form": "낼 꼭먹을거에요!!!",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009341.1.18",
          "form": "어 비건이시면",
          "original_form": "어 비건이시면",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009341.1.19",
          "form": "그그 만두중에 두부만두 있거든요",
          "original_form": "그그 만두중에 두부만두 있거든요",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009341.1.20",
          "form": "그것도 추천합니다",
          "original_form": "그것도 추천합니다",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009341.1.21",
          "form": "그거 일반만두는 맛없고 납작만두 있거든여 그게 맛나요",
          "original_form": "그거 일반만두는 맛없고 납작만두 있거든여 그게 맛나요",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009341.1.22",
          "form": "글고 저는 칠리가지튀김 넘 좋아하는데 맛난 곳 아시나요",
          "original_form": "글고 저는 칠리가지튀김 넘 좋아하는데 맛난 곳 아시나요",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009341.1.23",
          "form": "칠리가지튀김은 모르겠어요ㅠ",
          "original_form": "칠리가지튀김은 모르겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009341.1.24",
          "form": "두부만두 추천 감사합니다!",
          "original_form": "두부만두 추천 감사합니다!",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009341.1.25",
          "form": "김치랑 오리지널 있는데",
          "original_form": "김치랑 오리지널 있는데",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009341.1.26",
          "form": "함 드셔보십쇼 추천추천",
          "original_form": "함 드셔보십쇼 추천추천",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009341.1.27",
          "form": "비건 음식점도 찾아다니시나요?",
          "original_form": "비건 음식점도 찾아다니시나요?",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009341.1.28",
          "form": "저는 비건은 아니지만 비건인 친구들이 많아져서",
          "original_form": "저는 비건은 아니지만 비건인 친구들이 많아져서",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009341.1.29",
          "form": "그 선물할 때 비건 빵집들 찾아다니는데 진짜 요즘 많이 늘었더라고요",
          "original_form": "그 선물할 때 비건 빵집들 찾아다니는데 진짜 요즘 많이 늘었더라고요",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009341.1.30",
          "form": "제가 비건은 아닌데 비건 음식에 관심이 많아서요",
          "original_form": "제가 비건은 아닌데 비건 음식에 관심이 많아서요",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009341.1.31",
          "form": "비건 함박 스테이크도 도전입니다!",
          "original_form": "비건 함박 스테이크도 도전입니다!",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009341.1.32",
          "form": "정말 추천합니다",
          "original_form": "정말 추천합니다",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009341.1.33",
          "form": "요즘 다들 비건에 관심이 많더라구요",
          "original_form": "요즘 다들 비건에 관심이 많더라구요",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009341.1.34",
          "form": "비건친구들도 강추했어요",
          "original_form": "비건친구들도 강추했어요",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009341.1.35",
          "form": "맞아요 약간 환경오염 심해지면서",
          "original_form": "맞아요 약간 환경오염 심해지면서",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009341.1.36",
          "form": "비건시작하는 친구들도 많더라고요",
          "original_form": "비건시작하는 친구들도 많더라고요",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009341.1.37",
          "form": "약간 육식을 줄여가는게 목표인..",
          "original_form": "약간 육식을 줄여가는게 목표인..",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009341.1.38",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009341.1.39",
          "form": "저도 점차 알아가보면서 도전하는중이긴해요ㅋㅋㅋㅋ",
          "original_form": "저도 점차 알아가보면서 도전하는중이긴해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009341.1.40",
          "form": "하지만 아직 육식을 끊기는 좀 어려워요ㅠ",
          "original_form": "하지만 아직 육식을 끊기는 좀 어려워요ㅠ",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009341.1.41",
          "form": "저도요ㅠㅠ",
          "original_form": "저도요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009341.1.42",
