      "utterance": [
        {
          "id": "MDRW2100010332.1.1",
          "form": "동물 키워보신 적 있나요?",
          "original_form": "동물 키워보신 적 있나요?",
          "speaker_id": "1",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010332.1.2",
          "form": "구피 키워요",
          "original_form": "구피 키워요",
          "speaker_id": "2",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010332.1.3",
          "form": "식물도 키우고있어요",
          "original_form": "식물도 키우고있어요",
          "speaker_id": "2",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010332.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010332.1.5",
          "form": "오오!! 구피는 열대어인가요?",
          "original_form": "오오!! 구피는 열대어인가요?",
          "speaker_id": "1",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010332.1.6",
          "form": "저는 화분 하나만요 ㅎㅎ",
          "original_form": "저는 화분 하나만요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:56"
        },
        {
          "id": "MDRW2100010332.1.7",
          "form": "네네 아침에 밥줘요",
          "original_form": "네네 아침에 밥줘요",
          "speaker_id": "2",
          "time": "20210823 16:57"
        },
        {
          "id": "MDRW2100010332.1.8",
          "form": "어떤식물인가요",
          "original_form": "어떤식물인가요",
          "speaker_id": "2",
          "time": "20210823 16:57"
        },
        {
          "id": "MDRW2100010332.1.9",
          "form": "아이비도 키웠는데 얼마전에 죽었고 ㅠㅠ 지금은 트리안 하나 남았어요 ㅎㅎ",
          "original_form": "아이비도 키웠는데 얼마전에 죽었고 ㅠㅠ 지금은 트리안 하나 남았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100010332.1.10",
          "form": "아이비 잘안죽는데",
          "original_form": "아이비 잘안죽는데",
          "speaker_id": "2",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010332.1.11",
          "form": "집에 채광이 잘 안들어오나봐요",
          "original_form": "집에 채광이 잘 안들어오나봐요",
          "speaker_id": "2",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010332.1.12",
          "form": "구피는 손이 많이 안가서 좋아요",
          "original_form": "구피는 손이 많이 안가서 좋아요",
          "speaker_id": "2",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100010332.1.13",
          "form": "저도 그런 줄 알았는데 물말림 으로 죽었나봐요.. 힘없어서 물을 줬더니 그게 과습이었더라고요",
          "original_form": "저도 그런 줄 알았는데 물말림 으로 죽었나봐요.. 힘없어서 물을 줬더니 그게 과습이었더라고요",
          "speaker_id": "1",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100010332.1.14",
          "form": "구피 이름이 뭔가요? ㅎㅎ",
          "original_form": "구피 이름이 뭔가요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100010332.1.15",
          "form": "구피는 구피에요",
          "original_form": "구피는 구피에요",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010332.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010332.1.17",
          "form": "아 과습이 또",
          "original_form": "아 과습이 또",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010332.1.18",
          "form": "수분부족과 비슷해요 현상이ㅜ",
          "original_form": "수분부족과 비슷해요 현상이ㅜ",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010332.1.19",
          "form": "안타깝네요",
          "original_form": "안타깝네요",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010332.1.20",
          "form": "구피는 구피 ㅋㅋㅋㅋ 귀여워요 한마린가요?",
          "original_form": "구피는 구피 ㅋㅋㅋㅋ 귀여워요 한마린가요?",
          "speaker_id": "1",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010332.1.21",
          "form": "다섯마리넘게 있는데 작아서 귀여워요",
          "original_form": "다섯마리넘게 있는데 작아서 귀여워요",
          "speaker_id": "2",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100010332.1.22",
          "form": "똥을 많이 싸요",
          "original_form": "똥을 많이 싸요",
          "speaker_id": "2",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010332.1.23",
          "form": "와 많네요 ㅋㅋㅋ 어항 물 갈아주는거 은근 힘들거 같아요",
          "original_form": "와 많네요 ㅋㅋㅋ 어항 물 갈아주는거 은근 힘들거 같아요",
          "speaker_id": "1",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010332.1.24",
          "form": "네 물갈이할때 잘못하면 자갈이 빠져서",
          "original_form": "네 물갈이할때 잘못하면 자갈이 빠져서",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010332.1.25",
          "form": "세면대 막힌적도 있었어요ㅜㅜㅜ",
          "original_form": "세면대 막힌적도 있었어요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010332.1.26",
          "form": "아이쿠.. 엄청 손이 가는 작업이겠네요",
          "original_form": "아이쿠.. 엄청 손이 가는 작업이겠네요",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010332.1.27",
          "form": "저도 어릴때 집에 어항이 있었는데 신경쓸 게 엄청 많았던 거 같아요",
          "original_form": "저도 어릴때 집에 어항이 있었는데 신경쓸 게 엄청 많았던 거 같아요",
          "speaker_id": "1",
          "time": "20210823 17:22"
        }
      ]
