      "utterance": [
        {
          "id": "MDRW2100029938.1.1",
          "form": "네 흐려요.ㅜ비오구요.ㅋ",
          "original_form": "네 흐려요.ㅜ비오구요.ㅋ",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029938.1.2",
          "form": "거기도 흐린가요,",
          "original_form": "거기도 흐린가요,",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029938.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029938.1.4",
          "form": "네 구름이",
          "original_form": "네 구름이",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100029938.1.5",
          "form": "여기도 이제 비그치고 흐리네요ㅋ",
          "original_form": "여기도 이제 비그치고 흐리네요ㅋ",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029938.1.6",
          "form": "제법 쌀쌀해요",
          "original_form": "제법 쌀쌀해요",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029938.1.7",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029938.1.8",
          "form": "네ㅋ산책중이요ㅋ",
          "original_form": "네ㅋ산책중이요ㅋ",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029938.1.9",
          "form": "우산은요?",
          "original_form": "우산은요?",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029938.1.10",
          "form": "지금은 비그쳤어요ㅋ",
          "original_form": "지금은 비그쳤어요ㅋ",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029938.1.11",
          "form": "집앞이에요ㅋ",
          "original_form": "집앞이에요ㅋ",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029938.1.12",
          "form": "다행이네여",
          "original_form": "다행이네여",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029938.1.13",
          "form": "네ㅋㅋㅋ산책하기 좋았어요ㅋ",
          "original_form": "네ㅋㅋㅋ산책하기 좋았어요ㅋ",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100029938.1.14",
          "form": "바람이차요",
          "original_form": "바람이차요",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100029938.1.15",
          "form": "네 바람이 차요. 쌀쌀해요",
          "original_form": "네 바람이 차요. 쌀쌀해요",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100029938.1.16",
          "form": "창문열어놓으니 ㅋㅋㅋㅋ시원하네요ㅋ",
          "original_form": "창문열어놓으니 ㅋㅋㅋㅋ시원하네요ㅋ",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100029938.1.17",
          "form": "닫거 자여",
          "original_form": "닫거 자여",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100029938.1.18",
          "form": "지금은 닫았어요ㅋㅋ",
