      "utterance": [
        {
          "id": "MDRW2100023510.1.1",
          "form": "여행좋아하시나욤",
          "original_form": "여행좋아하시나욤",
          "speaker_id": "1",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100023510.1.2",
          "form": "너무 더운 한여름때는 피하시곸 ㅋㅋㅋㅋ 봄가을에 딱이에용!",
          "original_form": "너무 더운 한여름때는 피하시곸 ㅋㅋㅋㅋ 봄가을에 딱이에용!",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100023510.1.3",
          "form": "좋지만 못간지가 너무 오래됐어요ㅜ",
          "original_form": "좋지만 못간지가 너무 오래됐어요ㅜ",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100023510.1.4",
          "form": "출산하고 육아하고",
          "original_form": "출산하고 육아하고",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100023510.1.5",
          "form": "가을 노려보겟슴다!!",
          "original_form": "가을 노려보겟슴다!!",
          "speaker_id": "1",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100023510.1.6",
          "form": "갈만하니 코로나고..ㅜ",
          "original_form": "갈만하니 코로나고..ㅜ",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100023510.1.7",
          "form": "고생이많으세요",
          "original_form": "고생이많으세요",
          "speaker_id": "1",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023510.1.8",
          "form": "ㅎㅎㅎ 넵! 통영거제 코스로",
          "original_form": "ㅎㅎㅎ 넵! 통영거제 코스로",
          "speaker_id": "2",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023510.1.9",
          "form": "세상에서 제일 힘든게 육아인거같아요",
          "original_form": "세상에서 제일 힘든게 육아인거같아요",
          "speaker_id": "1",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023510.1.10",
          "form": "ㅠㅠㅠ  너무예쁘지만 그만큼 너무 힘들죠",
          "original_form": "ㅠㅠㅠ  너무예쁘지만 그만큼 너무 힘들죠",
          "speaker_id": "2",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023510.1.11",
          "form": "저는 맘같아선 딩크하고싶지만\r\n하나 낳기로 남친이랑 합의봣어요",
          "original_form": "저는 맘같아선 딩크하고싶지만\r\n하나 낳기로 남친이랑 합의봣어요",
          "speaker_id": "1",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023510.1.12",
          "form": "더 내려놓고 준비할려구욤",
          "original_form": "더 내려놓고 준비할려구욤",
          "speaker_id": "1",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023510.1.13",
          "form": "전둘이지만",
          "original_form": "전둘이지만",
          "speaker_id": "2",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023510.1.14",
          "form": "하나도 힘들어용ㅋ",
          "original_form": "하나도 힘들어용ㅋ",
          "speaker_id": "2",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023510.1.15",
          "form": "그래도 낳는거추천이요!",
          "original_form": "그래도 낳는거추천이요!",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023510.1.16",
          "form": "하ㅠㅠ 대단하세요 정말ㅜㅜ",
          "original_form": "하ㅠㅠ 대단하세요 정말ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023510.1.17",
          "form": "주변 애낳은친구들은",
          "original_form": "주변 애낳은친구들은",
          "speaker_id": "1",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023510.1.18",
          "form": "너무너무 예쁘고 어디서 이런예쁜게 왔지? 싶거든요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무너무 예쁘고 어디서 이런예쁜게 왔지? 싶거든요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023510.1.19",
          "form": "딩크하라고 강추하더라구요",
          "original_form": "딩크하라고 강추하더라구요",
          "speaker_id": "1",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023510.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023510.1.21",
          "form": "낳아봐야 깨닫는것도있고용",
          "original_form": "낳아봐야 깨닫는것도있고용",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023510.1.22",
          "form": "ㅋㄱㅋㅋㅋ일단 지금을 즐기시구요 ㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋ일단 지금을 즐기시구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100023510.1.23",
          "form": "여행가고싶어도 애들이 생기면",
          "original_form": "여행가고싶어도 애들이 생기면",
          "speaker_id": "2",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023510.1.24",
          "form": "쉽지않아용",
          "original_form": "쉽지않아용",
          "speaker_id": "2",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023510.1.25",
          "form": "아기아기때 경주에 여행갔는데",
          "original_form": "아기아기때 경주에 여행갔는데",
          "speaker_id": "2",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023510.1.26",
          "form": "맞아요 저 신행도 멀리가고싶어서 미루고잇어욤",
          "original_form": "맞아요 저 신행도 멀리가고싶어서 미루고잇어욤",
          "speaker_id": "1",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023510.1.27",
          "form": "너무좋아하는거에요~~ㅎㅎㅎ",
          "original_form": "너무좋아하는거에요~~ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100023510.1.28",
          "form": "비단벌레 버스탔는데  엄청좋아했어요",
          "original_form": "비단벌레 버스탔는데  엄청좋아했어요",
          "speaker_id": "2",
          "time": "20210830 10:36"
        },
        {
