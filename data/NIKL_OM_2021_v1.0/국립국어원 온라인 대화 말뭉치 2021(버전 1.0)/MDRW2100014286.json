        {
          "id": "MDRW2100014286.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014286.1.2",
          "form": "게임은 즐겨하는편은 아니예요",
          "original_form": "게임은 즐겨하는편은 아니예요",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014286.1.3",
          "form": "게임좋아하세요?",
          "original_form": "게임좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014286.1.4",
          "form": "저는 심심할때나 쉴때 종종 폰게임 해요ㅎㅎ",
          "original_form": "저는 심심할때나 쉴때 종종 폰게임 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014286.1.5",
          "form": "꿈의집 예전부터 하고있는데 심심풀이로 좋아요",
          "original_form": "꿈의집 예전부터 하고있는데 심심풀이로 좋아요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014286.1.6",
          "form": "꿈의집은 어떤스타일이예요?",
          "original_form": "꿈의집은 어떤스타일이예요?",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014286.1.7",
          "form": "집꾸미기인가요? ㅎㅎ",
          "original_form": "집꾸미기인가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014286.1.8",
          "form": "캔디크러쉬같은 느낌이에요~~ㅎㅎ\r\n레벨 클리어할때마다 별을 줘서 그거로 집을 수리해요ㅎㅎ",
          "original_form": "캔디크러쉬같은 느낌이에요~~ㅎㅎ\r\n레벨 클리어할때마다 별을 줘서 그거로 집을 수리해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014286.1.9",
          "form": "캔디크러쉬는 저도 해봤어요.",
          "original_form": "캔디크러쉬는 저도 해봤어요.",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014286.1.10",
          "form": "은근히 시간잘가잖아요ㅋㅋ",
          "original_form": "은근히 시간잘가잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014286.1.11",
          "form": "ㅋㅋㅋ맞아요 그래서 게임하나는 깔아두고 하게되는것 같아요",
          "original_form": "ㅋㅋㅋ맞아요 그래서 게임하나는 깔아두고 하게되는것 같아요",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014286.1.12",
          "form": "진짜심심할때 ㅎㅎ",
          "original_form": "진짜심심할때 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014286.1.13",
          "form": "요즘 하시는 게임은 없으세요?",
          "original_form": "요즘 하시는 게임은 없으세요?",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014286.1.14",
          "form": "예전에 동물의숲 한창 열심히 했었어요",
          "original_form": "예전에 동물의숲 한창 열심히 했었어요",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014286.1.15",
          "form": "동숲아세요?",
          "original_form": "동숲아세요?",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014286.1.16",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014286.1.17",
          "form": "닌텐도ds 맨처음 나왔을때 부모님 조르고 졸라서 사주셔서 그걸로 했었어요ㅎㅎ",
          "original_form": "닌텐도ds 맨처음 나왔을때 부모님 조르고 졸라서 사주셔서 그걸로 했었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014286.1.18",
          "form": "그이후로는 새로 나오는 닌텐도시리즈를 안사서 못했네용",
          "original_form": "그이후로는 새로 나오는 닌텐도시리즈를 안사서 못했네용",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014286.1.19",
          "form": "ㅎㅎ 숲만드는재미가있었는데",
          "original_form": "ㅎㅎ 숲만드는재미가있었는데",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014286.1.20",
          "form": "그거 지을때 잠 안자고 했어요",
          "original_form": "그거 지을때 잠 안자고 했어요",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014286.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014286.1.22",
          "form": "전 대출금 빨리 갚고 빨리 확장하고싶어서",
          "original_form": "전 대출금 빨리 갚고 빨리 확장하고싶어서",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014286.1.23",
          "form": "열심히 이것저것 팔고 했어욬ㅋㅋ",
          "original_form": "열심히 이것저것 팔고 했어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014286.1.24",
          "form": "대출금ㅋㅋㅋ",
          "original_form": "대출금ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014286.1.25",
          "form": "통신하는거  재밌더라구요ㅋㅋ",
          "original_form": "통신하는거  재밌더라구요ㅋㅋ",
          "speaker_id": "2",
