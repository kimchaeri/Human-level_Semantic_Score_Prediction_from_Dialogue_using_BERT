      "utterance": [
        {
          "id": "MDRW2100000588.1.1",
          "form": "저는 강아지랑 고양이 중에 강아지가 좋아요",
          "original_form": "저는 강아지랑 고양이 중에 강아지가 좋아요",
          "speaker_id": "1",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000588.1.2",
          "form": "저도요! 키워본적은 없지만 ㅋㅋ",
          "original_form": "저도요! 키워본적은 없지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000588.1.3",
          "form": "맞아요 강아지쪽이 더 애교가 있는 느낌?",
          "original_form": "맞아요 강아지쪽이 더 애교가 있는 느낌?",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000588.1.4",
          "form": "같이 산책도 나갈 수 있구요!",
          "original_form": "같이 산책도 나갈 수 있구요!",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000588.1.5",
          "form": "맞아요! 제가 예전에 강아지 키울때 강아지 가고싶어해서 나가기 싫어도 산책하러 나가고 그랬어요",
          "original_form": "맞아요! 제가 예전에 강아지 키울때 강아지 가고싶어해서 나가기 싫어도 산책하러 나가고 그랬어요",
          "speaker_id": "1",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000588.1.6",
          "form": "예전에 어떤 강아지 키우셨어요?",
          "original_form": "예전에 어떤 강아지 키우셨어요?",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000588.1.7",
          "form": "웰시코기요",
          "original_form": "웰시코기요",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000588.1.8",
          "form": "다리짧고 ㅋㅋㅋ",
          "original_form": "다리짧고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000588.1.9",
          "form": "아 넘넘 귀여울것 같아요",
          "original_form": "아 넘넘 귀여울것 같아요",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000588.1.10",
          "form": "맞아여 정말 귀여워요 산책안가면 방문앞에서 문 긁으면서 울어요",
          "original_form": "맞아여 정말 귀여워요 산책안가면 방문앞에서 문 긁으면서 울어요",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000588.1.11",
          "form": "그래서 안갈수가 없어요 ㅋㅋ",
          "original_form": "그래서 안갈수가 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000588.1.12",
          "form": "주인 운동도 시켜주고 아주 착해요 ㅋㅋㅋ ㅋ",
          "original_form": "주인 운동도 시켜주고 아주 착해요 ㅋㅋㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000588.1.13",
          "form": "주로 저 좋으려고 가자는거지만",
          "original_form": "주로 저 좋으려고 가자는거지만",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000588.1.14",
          "form": "결과는 그렇게 되네요 ㅋㅋ",
          "original_form": "결과는 그렇게 되네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000588.1.15",
          "form": "저는 주변에도 반려동물 키우는 사람들이 거의 없어요 귀염뽀짝이들 보고싶은데",
          "original_form": "저는 주변에도 반려동물 키우는 사람들이 거의 없어요 귀염뽀짝이들 보고싶은데",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000588.1.16",
          "form": "앗 기회가 되면 키워보세요 털은 많이 뿜지만 정말 힐링이예요",
          "original_form": "앗 기회가 되면 키워보세요 털은 많이 뿜지만 정말 힐링이예요",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000588.1.17",
          "form": "저도 이제는 하늘나라 보내고 못키우지만 길가에서 산책하는 강아지들 보면 절로 광대가 올라가요",
          "original_form": "저도 이제는 하늘나라 보내고 못키우지만 길가에서 산책하는 강아지들 보면 절로 광대가 올라가요",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000588.1.18",
          "form": "웰시코기도 털 많이 뿜뿜인가요?",
          "original_form": "웰시코기도 털 많이 뿜뿜인가요?",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000588.1.19",
          "form": "엄청나요",
          "original_form": "엄청나요",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000588.1.20",
          "form": "어디로 다녔는지 알수있을만큼 뿜어요..",
          "original_form": "어디로 다녔는지 알수있을만큼 뿜어요..",
          "speaker_id": "1",
          "time": "20210819 17:30"
        }
      ]
