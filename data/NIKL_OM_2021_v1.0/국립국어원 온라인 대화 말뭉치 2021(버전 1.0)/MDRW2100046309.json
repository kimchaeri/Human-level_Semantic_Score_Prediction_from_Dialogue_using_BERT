        },
        {
          "id": "MDRW2100046309.1.2",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "2",
          "time": "20211019 13:43"
        },
        {
          "id": "MDRW2100046309.1.3",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20211019 13:44"
        },
        {
          "id": "MDRW2100046309.1.4",
          "form": "요리 못합니다. 밀키트 좋아해요.",
          "original_form": "요리 못합니다. 밀키트 좋아해요.",
          "speaker_id": "1",
          "time": "20211019 15:11"
        },
        {
          "id": "MDRW2100046309.1.5",
          "form": "아 근데 요즘은 밀키트가 너무 잘나와서",
          "original_form": "아 근데 요즘은 밀키트가 너무 잘나와서",
          "speaker_id": "2",
          "time": "20211019 15:17"
        },
        {
          "id": "MDRW2100046309.1.6",
          "form": "못해도 전혀 상관이없겠더라구요 ㅋㅋ",
          "original_form": "못해도 전혀 상관이없겠더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 15:17"
        },
        {
          "id": "MDRW2100046309.1.7",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20211019 15:17"
        },
        {
          "id": "MDRW2100046309.1.8",
          "form": "근데 밀키트도 은근 비싸요",
          "original_form": "근데 밀키트도 은근 비싸요",
          "speaker_id": "1",
          "time": "20211019 15:18"
        },
        {
          "id": "MDRW2100046309.1.9",
          "form": "맞아요 ㅠㅠ...",
          "original_form": "맞아요 ㅠㅠ...",
          "speaker_id": "2",
          "time": "20211019 15:19"
        },
        {
          "id": "MDRW2100046309.1.10",
          "form": "밀키트는 주로 어디서 많이 구매하세요?",
          "original_form": "밀키트는 주로 어디서 많이 구매하세요?",
          "speaker_id": "2",
          "time": "20211019 15:19"
        },
        {
          "id": "MDRW2100046309.1.11",
          "form": "전 무조건 이마트 주문해요",
          "original_form": "전 무조건 이마트 주문해요",
          "speaker_id": "1",
          "time": "20211019 15:19"
        },
        {
          "id": "MDRW2100046309.1.12",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20211019 15:20"
        },
        {
          "id": "MDRW2100046309.1.13",
          "form": "저는 쿠팡에서 주로 사는데",
          "original_form": "저는 쿠팡에서 주로 사는데",
          "speaker_id": "2",
          "time": "20211019 15:21"
        },
        {
          "id": "MDRW2100046309.1.14",
          "form": "이마트 것도 맛있나요??",
          "original_form": "이마트 것도 맛있나요??",
          "speaker_id": "2",
          "time": "20211019 15:21"
        },
        {
          "id": "MDRW2100046309.1.15",
          "form": "제가 쿠팡은 안 써서....",
          "original_form": "제가 쿠팡은 안 써서....",
          "speaker_id": "1",
          "time": "20211019 15:21"
        },
        {
          "id": "MDRW2100046309.1.16",
          "form": "이마트 먹을만해요",
          "original_form": "이마트 먹을만해요",
          "speaker_id": "1",
          "time": "20211019 15:21"
        },
        {
          "id": "MDRW2100046309.1.17",
          "form": "호오 다음엔 이마트에서 주문해봐야겠네요",
          "original_form": "호오 다음엔 이마트에서 주문해봐야겠네요",
          "speaker_id": "2",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046309.1.18",
          "form": "쿠팡 좋아요",
          "original_form": "쿠팡 좋아요",
          "speaker_id": "2",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046309.1.19",
          "form": "월 2900원만 내면",
          "original_form": "월 2900원만 내면",
          "speaker_id": "2",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046309.1.20",
          "form": "로켓배송 붙은건 다 무료더라구요",
          "original_form": "로켓배송 붙은건 다 무료더라구요",
          "speaker_id": "2",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046309.1.21",
          "form": "배송비가!",
          "original_form": "배송비가!",
          "speaker_id": "2",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046309.1.22",
          "form": "쿠팡 저만 안 쓰나봐요",
          "original_form": "쿠팡 저만 안 쓰나봐요",
          "speaker_id": "1",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046309.1.23",
          "form": "제 주변에도 은근 안쓰긴해요 ㅋㅋㅋㅋ ㅎㅎ",
          "original_form": "제 주변에도 은근 안쓰긴해요 ㅋㅋㅋㅋ ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 15:23"
        },
        {
          "id": "MDRW2100046309.1.24",
          "form": "다른거 잘 쓰고 계시면",
          "original_form": "다른거 잘 쓰고 계시면",
          "speaker_id": "2",
          "time": "20211019 15:23"
        },
        {
          "id": "MDRW2100046309.1.25",
          "form": "굳이 쓸 필요는 없죵 ㅎㅎ",
          "original_form": "굳이 쓸 필요는 없죵 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 15:23"
        },
        {
          "id": "MDRW2100046309.1.26",
          "form": "이마트도 등급혜택 있어서 그냥 계속 써요",
          "original_form": "이마트도 등급혜택 있어서 그냥 계속 써요",
          "speaker_id": "1",
          "time": "20211019 15:26"
        },
        {
          "id": "MDRW2100046309.1.27",
          "form": "아 그러면 더더욱.. 갈아타실 필요 없으시겠어용ㅎㅎ",
          "original_form": "아 그러면 더더욱.. 갈아타실 필요 없으시겠어용ㅎㅎ",
