      "utterance": [
        {
          "id": "MDRW2100026267.1.1",
          "form": "언니그거 아새요",
          "original_form": "언니그거 아새요",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.2",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.3",
          "form": "뭔데요 뭔데요???",
          "original_form": "뭔데요 뭔데요???",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.4",
          "form": "아 낮에 말씀드렸구나",
          "original_form": "아 낮에 말씀드렸구나",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 학생 22명인거요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 학생 22명인거요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.6",
          "form": "뭐지뭐지?!",
          "original_form": "뭐지뭐지?!",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.7",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.8",
          "form": "궁금해요!!!",
          "original_form": "궁금해요!!!",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.11",
          "form": "거의 대면 수업일 때의 두 배인데요?",
          "original_form": "거의 대면 수업일 때의 두 배인데요?",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.12",
          "form": "제가 몇 명이었더라...",
          "original_form": "제가 몇 명이었더라...",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.13",
          "form": "이거진짜 전무후무 아닌가요",
          "original_form": "이거진짜 전무후무 아닌가요",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.14",
          "form": "그럼 레포트도 절반만 받던가!",
          "original_form": "그럼 레포트도 절반만 받던가!",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026267.1.15",
          "form": "이거 증원해가지고 화실기 다 빠져서 월분반 폐강위기라던데ㅠㅠ",
          "original_form": "이거 증원해가지고 화실기 다 빠져서 월분반 폐강위기라던데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026267.1.16",
          "form": "와 진짜요?",
          "original_form": "와 진짜요?",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026267.1.17",
          "form": "이번에 졸업요건 바뀌어서",
          "original_form": "이번에 졸업요건 바뀌어서",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026267.1.18",
          "form": "다들 화실기 피할 수 있으면 피하려고",
          "original_form": "다들 화실기 피할 수 있으면 피하려고",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026267.1.19",
          "form": "하나보네요... 대박이다",
          "original_form": "하나보네요... 대박이다",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026267.1.20",
          "form": "그럼 욕먹을텐데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그럼 욕먹을텐데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026267.1.21",
          "form": "지금 제가 알기로는 30명 증원한거에요 ㅋㅋㅋㅋㅋ",
          "original_form": "지금 제가 알기로는 30명 증원한거에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026267.1.22",
          "form": "월~목 합쳐서요???",
          "original_form": "월~목 합쳐서요???",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026267.1.23",
          "form": "정확히는 모르겠는데ㅠ 어쨌든 그래서 지금 프리랩+랩리포트 한번에 받으면 레포트만 44개에욬ㅋㅋㅋ",
          "original_form": "정확히는 모르겠는데ㅠ 어쨌든 그래서 지금 프리랩+랩리포트 한번에 받으면 레포트만 44개에욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026267.1.24",
          "form": "지금 화수는 증원 확실하고 목분반도 증원했다는 말을 들었는데",
          "original_form": "지금 화수는 증원 확실하고 목분반도 증원했다는 말을 들었는데",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026267.1.25",
          "form": "와...?",
          "original_form": "와...?",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026267.1.26",
          "form": "아니 세상에 ㅋㅋㅋㅋㅋㅋ ;;;;",
          "original_form": "아니 세상에 ㅋㅋㅋㅋㅋㅋ ;;;;",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026267.1.27",
          "form": "다른 분반은 어떻게 됐는지 모르겠어요",
          "original_form": "다른 분반은 어떻게 됐는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026267.1.28",
          "form": "아니 레포트 채점 어떻게 다 해요?",
          "original_form": "아니 레포트 채점 어떻게 다 해요?",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026267.1.29",
          "form": "아니 교수님 너무",
          "original_form": "아니 교수님 너무",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026267.1.30",
          "form": "다른 과목에도 피해 주는거 아닌가요?",
          "original_form": "다른 과목에도 피해 주는거 아닌가요?",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026267.1.31",
          "form": "그러게요 교수님 왜 저희한테는 아무말씀 없으시다가ㅜㅜ",
          "original_form": "그러게요 교수님 왜 저희한테는 아무말씀 없으시다가ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026267.1.32",
          "form": "아니 그니까요",
          "original_form": "아니 그니까요",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026267.1.33",
          "form": "오늘 아침에 날벼락...ㅋㅋㅋㅋㅋ",
          "original_form": "오늘 아침에 날벼락...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:16"
        }
      ]
