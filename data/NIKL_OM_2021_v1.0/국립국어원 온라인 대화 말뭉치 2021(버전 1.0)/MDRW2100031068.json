      "utterance": [
        {
          "id": "MDRW2100031068.1.1",
          "form": "이럴수가 있는 그대로 곡물을 사랑해주라",
          "original_form": "이럴수가 있는 그대로 곡물을 사랑해주라",
          "speaker_id": "2",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031068.1.2",
          "form": "넌 아침 떡볶이 먹었니",
          "original_form": "넌 아침 떡볶이 먹었니",
          "speaker_id": "2",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031068.1.3",
          "form": "아니 동생이 떡볶이 먹는거 구경했어",
          "original_form": "아니 동생이 떡볶이 먹는거 구경했어",
          "speaker_id": "1",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031068.1.4",
          "form": "넌 상상떡볶이 먹었어?",
          "original_form": "넌 상상떡볶이 먹었어?",
          "speaker_id": "2",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100031068.1.5",
          "form": "뭐야 떡볶이 먹는 상상하는겨?",
          "original_form": "뭐야 떡볶이 먹는 상상하는겨?",
          "speaker_id": "1",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031068.1.6",
          "form": "응 ㅋㅋㅋㅋㅋ자린고비처럼",
          "original_form": "응 ㅋㅋㅋㅋㅋ자린고비처럼",
          "speaker_id": "2",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031068.1.7",
          "form": "어허 쫄깃하다~",
          "original_form": "어허 쫄깃하다~",
          "speaker_id": "2",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031068.1.8",
          "form": "요즘, 초딩들 사이에서 유행하는거니",
          "original_form": "요즘, 초딩들 사이에서 유행하는거니",
          "speaker_id": "1",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031068.1.9",
          "form": "아니 그냥 의식의 흐름대로 말한거야",
          "original_form": "아니 그냥 의식의 흐름대로 말한거야",
          "speaker_id": "2",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031068.1.10",
          "form": "나 졸지에 초딩 됨",
          "original_form": "나 졸지에 초딩 됨",
          "speaker_id": "2",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031068.1.11",
          "form": "어이가없네 자린고비아니걸랑",
          "original_form": "어이가없네 자린고비아니걸랑",
          "speaker_id": "1",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100031068.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럼 아침에는 뭐 먹었어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럼 아침에는 뭐 먹었어",
          "speaker_id": "2",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100031068.1.13",
          "form": "동생이 먹을래? 하는걸 아니 안먹을래 했어",
          "original_form": "동생이 먹을래? 하는걸 아니 안먹을래 했어",
          "speaker_id": "1",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100031068.1.14",
          "form": "아침 안먹었어",
          "original_form": "아침 안먹었어",
          "speaker_id": "1",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100031068.1.15",
          "form": "그래? 그럼 내 사랑이나 먹어",
          "original_form": "그래? 그럼 내 사랑이나 먹어",
          "speaker_id": "2",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100031068.1.16",
          "form": "배부르다 정말",
          "original_form": "배부르다 정말",
          "speaker_id": "1",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100031068.1.17",
          "form": "고마워",
          "original_form": "고마워",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100031068.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100031068.1.19",
          "form": "오늘은 더 자고싶어서",
          "original_form": "오늘은 더 자고싶어서",
          "speaker_id": "1",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100031068.1.20",
          "form": "아침밥 포기했어",
          "original_form": "아침밥 포기했어",
          "speaker_id": "1",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100031068.1.21",
          "form": "나도 주말이라 느긋하게 보내고 싶아서 시리얼 먹고 또 잤어",
          "original_form": "나도 주말이라 느긋하게 보내고 싶아서 시리얼 먹고 또 잤어",
          "speaker_id": "2",
