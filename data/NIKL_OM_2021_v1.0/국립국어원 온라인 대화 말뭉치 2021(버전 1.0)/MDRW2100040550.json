      "utterance": [
        {
          "id": "MDRW2100040550.1.1",
          "form": "직장 괜찮으신가요",
          "original_form": "직장 괜찮으신가요",
          "speaker_id": "1",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100040550.1.2",
          "form": "네 저 지금 백수라 완전 괜찮아요ㅋㅋㅋ 빈둥대고 있어용",
          "original_form": "네 저 지금 백수라 완전 괜찮아요ㅋㅋㅋ 빈둥대고 있어용",
          "speaker_id": "2",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100040550.1.3",
          "form": "잠깐 휴식기인가봐요",
          "original_form": "잠깐 휴식기인가봐요",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100040550.1.4",
          "form": "네 ㅎㅎㅎ 지쳤어가지구 지금은 쉬는데 집중하고 있어요",
          "original_form": "네 ㅎㅎㅎ 지쳤어가지구 지금은 쉬는데 집중하고 있어요",
          "speaker_id": "2",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040550.1.5",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040550.1.6",
          "form": "저는 다니고있어요",
          "original_form": "저는 다니고있어요",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040550.1.7",
          "form": "어느 직종에서 일하셨어요?",
          "original_form": "어느 직종에서 일하셨어요?",
          "speaker_id": "1",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100040550.1.8",
          "form": "저는 의류 브랜드에서 일했었어요! 재밌을거라고 생각했는데",
          "original_form": "저는 의류 브랜드에서 일했었어요! 재밌을거라고 생각했는데",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040550.1.9",
          "form": "재미없었어요",
          "original_form": "재미없었어요",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040550.1.10",
          "form": "그냥 회사는 다 똑같나봐요",
          "original_form": "그냥 회사는 다 똑같나봐요",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040550.1.11",
          "form": "뭘해도 재미없겠죠..?ㅠㅋㅋㅋㅋ",
          "original_form": "뭘해도 재미없겠죠..?ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040550.1.12",
          "form": "그쵸 ᆢ 회사가 다똑같아요 ᆢ",
          "original_form": "그쵸 ᆢ 회사가 다똑같아요 ᆢ",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040550.1.13",
          "form": "특히 사람땜시 ᆢ",
          "original_form": "특히 사람땜시 ᆢ",
          "speaker_id": "1",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040550.1.14",
          "form": "맞아요 항상 사람이 문제죠",
          "original_form": "맞아요 항상 사람이 문제죠",
          "speaker_id": "2",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040550.1.15",
          "form": "스트레스 심하세요?ㅠ",
          "original_form": "스트레스 심하세요?ㅠ",
          "speaker_id": "2",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040550.1.16",
          "form": "저는 한귀로 듣고 한귀로 흘리긴하는데도",
          "original_form": "저는 한귀로 듣고 한귀로 흘리긴하는데도",
          "speaker_id": "1",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040550.1.17",
          "form": "한대 때리고싶네요",
          "original_form": "한대 때리고싶네요",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040550.1.18",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040550.1.19",
          "form": "너무 진심같아요",
          "original_form": "너무 진심같아요",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040550.1.20",
          "form": "진심이시겠지만",
          "original_form": "진심이시겠지만",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040550.1.21",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040550.1.22",
          "form": "그만두신곳은 상사 괜찮으셨어요?",
          "original_form": "그만두신곳은 상사 괜찮으셨어요?",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040550.1.23",
          "form": "아니요..ㅋㅋ",
          "original_form": "아니요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040550.1.24",
          "form": "그래서 그만둔걸요",
          "original_form": "그래서 그만둔걸요",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040550.1.25",
          "form": "참 사람들이 왜 그럴까요",
          "original_form": "참 사람들이 왜 그럴까요",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040550.1.26",
          "form": "그러니까요ㅜㅜㅜ",
          "original_form": "그러니까요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040550.1.27",
          "form": "일적으로 혼내는건 그려려니하는데",
          "original_form": "일적으로 혼내는건 그려려니하는데",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040550.1.28",
          "form": "그외에거 트집잡으면 진짜 ᆢ",
          "original_form": "그외에거 트집잡으면 진짜 ᆢ",
          "speaker_id": "1",
          "time": "20210904 22:40"
        }
      ]
    }
