      "utterance": [
        {
          "id": "MDRW2100041100.1.1",
          "form": "정유민 출연작도 엄청 많음",
          "original_form": "정유민 출연작도 엄청 많음",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041100.1.2",
          "form": "검색해 보께요",
          "original_form": "검색해 보께요",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041100.1.3",
          "form": "꽃길만 걸어요 여기서 주연",
          "original_form": "꽃길만 걸어요 여기서 주연",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041100.1.4",
          "form": "진심 얼굴봐도 모르겠음요",
          "original_form": "진심 얼굴봐도 모르겠음요",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041100.1.5",
          "form": "고수랑 엄기준 나온 흉부외과에도 나오고",
          "original_form": "고수랑 엄기준 나온 흉부외과에도 나오고",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041100.1.6",
          "form": "임팩트나 없었나?  전혀 기억이 안나요",
          "original_form": "임팩트나 없었나?  전혀 기억이 안나요",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041100.1.7",
          "form": "이판사판 여기도 조연",
          "original_form": "이판사판 여기도 조연",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041100.1.8",
          "form": "이판사판 조금 봤는데",
          "original_form": "이판사판 조금 봤는데",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100041100.1.9",
          "form": "조연이라서 다 임팩트가 없었나보다",
          "original_form": "조연이라서 다 임팩트가 없었나보다",
          "speaker_id": "2",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100041100.1.10",
          "form": "조연이라도 개 성 있었음 기억 나는데",
          "original_form": "조연이라도 개 성 있었음 기억 나는데",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100041100.1.11",
          "form": "확 기억에 남는 얼굴은 아닌듯",
          "original_form": "확 기억에 남는 얼굴은 아닌듯",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041100.1.12",
          "form": "들리신나요, 초인가족에도 나옴",
          "original_form": "들리신나요, 초인가족에도 나옴",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041100.1.13",
          "form": "좀 흔한 얼굴이예요",
          "original_form": "좀 흔한 얼굴이예요",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041100.1.14",
          "form": "심지어 구르미 그린 달빛에도 조연 월희역에 나온다",
          "original_form": "심지어 구르미 그린 달빛에도 조연 월희역에 나온다",
          "speaker_id": "2",
