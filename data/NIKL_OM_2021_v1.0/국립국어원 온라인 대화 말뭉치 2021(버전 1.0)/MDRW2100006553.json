      "utterance": [
        {
          "id": "MDRW2100006553.1.1",
          "form": "진짜야 같이 하자",
          "original_form": "진짜야 같이 하자",
          "speaker_id": "2",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100006553.1.2",
          "form": "근데 옵치 포맷 바뀌어가지구 나는 이제 애쉬 거의 못해 ㅠㅠ",
          "original_form": "근데 옵치 포맷 바뀌어가지구 나는 이제 애쉬 거의 못해 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100006553.1.3",
          "form": "맞아 그렇더라",
          "original_form": "맞아 그렇더라",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100006553.1.4",
          "form": "하고싶은걸 잘 못하게 바뀌었더라고",
          "original_form": "하고싶은걸 잘 못하게 바뀌었더라고",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100006553.1.5",
          "form": "그치 하면 또 ,, ㅎ 늦게 잡히고 나는 지고 ㅠㅠ 그래서",
          "original_form": "그치 하면 또 ,, ㅎ 늦게 잡히고 나는 지고 ㅠㅠ 그래서",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100006553.1.6",
          "form": "아하 괜찮아괜찮아",
          "original_form": "아하 괜찮아괜찮아",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100006553.1.7",
          "form": "같이 하고 애쉬해",
          "original_form": "같이 하고 애쉬해",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100006553.1.8",
          "form": "ㅎ한번도 못이길걸",
          "original_form": "ㅎ한번도 못이길걸",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100006553.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100006553.1.10",
          "form": "게임인데 뭐 즐기면 되는거지",
          "original_form": "게임인데 뭐 즐기면 되는거지",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100006553.1.11",
          "form": "나 진짜 9레벨 애들이랑 할때가 제일 행복하던데?",
          "original_form": "나 진짜 9레벨 애들이랑 할때가 제일 행복하던데?",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100006553.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ나혼자 하니까 그런애들 걸리더라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ나혼자 하니까 그런애들 걸리더라",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100006553.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ그럴 수 있지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그럴 수 있지",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100006553.1.14",
          "form": "나 못한다고 화내지않아",
          "original_form": "나 못한다고 화내지않아",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100006553.1.15",
          "form": "음? 근데 후회할걸 속으로",
          "original_form": "음? 근데 후회할걸 속으로",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100006553.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100006553.1.17",
          "form": "모르겠다 아닐 것 같은데",
          "original_form": "모르겠다 아닐 것 같은데",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100006553.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅠ 통하지않네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅠ 통하지않네",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100006553.1.19",
          "form": "아아 싫으면 언제든 뭐....",
          "original_form": "아아 싫으면 언제든 뭐....",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100006553.1.20",
          "form": "애쉬 이벤트하면 한번 졸라봐",
          "original_form": "애쉬 이벤트하면 한번 졸라봐",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100006553.1.21",
          "form": "고민해볼게",
          "original_form": "고민해볼게",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100006553.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ원하면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ원하면",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100006553.1.23",
          "form": "애쉬스킨 아 시메도 좋아!",
          "original_form": "애쉬스킨 아 시메도 좋아!",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100006553.1.24",
          "form": "탈퇴했다 다시 가입하지 뭐",
          "original_form": "탈퇴했다 다시 가입하지 뭐",
          "speaker_id": "2",
          "time": "20210908 02:17"
        }
      ]
    }
