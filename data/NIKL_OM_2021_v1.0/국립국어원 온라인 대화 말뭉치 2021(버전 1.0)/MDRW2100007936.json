      "utterance": [
        {
          "id": "MDRW2100007936.1.1",
          "form": "당을 줄이려고 노력중이라... 걍 라떼는 오늘 만들어봐야겠어요",
          "original_form": "당을 줄이려고 노력중이라... 걍 라떼는 오늘 만들어봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007936.1.2",
          "form": "그래봤자 물대신 우유지만",
          "original_form": "그래봤자 물대신 우유지만",
          "speaker_id": "1",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007936.1.3",
          "form": "액상과당 문제죠",
          "original_form": "액상과당 문제죠",
          "speaker_id": "2",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007936.1.4",
          "form": "근데넘맛잇어요",
          "original_form": "근데넘맛잇어요",
          "speaker_id": "2",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007936.1.5",
          "form": "탄산도 커피도 당도",
          "original_form": "탄산도 커피도 당도",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007936.1.6",
          "form": "그러니까요 ㅠ 오래먹기 위해서 뭐든",
          "original_form": "그러니까요 ㅠ 오래먹기 위해서 뭐든",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007936.1.7",
          "form": "적당한게 중요한듯요",
          "original_form": "적당한게 중요한듯요",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007936.1.8",
          "form": "도넛도 진짜 오랜만인데 땡겼어요 크리스피..",
          "original_form": "도넛도 진짜 오랜만인데 땡겼어요 크리스피..",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007936.1.9",
          "form": "저도 님애기듣고먹고싶어서",
          "original_form": "저도 님애기듣고먹고싶어서",
          "speaker_id": "2",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007936.1.10",
          "form": "괴로워하는중입니다",
          "original_form": "괴로워하는중입니다",
          "speaker_id": "2",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007936.1.11",
          "form": "죄송하게 됐습니다...마켓컬리에서 3개입 팔더라고요ㅋㅋㅋ",
          "original_form": "죄송하게 됐습니다...마켓컬리에서 3개입 팔더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007936.1.12",
          "form": "아날로그휴먼이라",
          "original_form": "아날로그휴먼이라",
          "speaker_id": "2",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007936.1.13",
          "form": "그런거넘어려운듯해요",
          "original_form": "그런거넘어려운듯해요",
          "speaker_id": "2",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007936.1.14",
          "form": "배달음식 시키듯이...",
          "original_form": "배달음식 시키듯이...",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007936.1.15",
          "form": "하튼 엄청 다네요",
          "original_form": "하튼 엄청 다네요",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007936.1.16",
          "form": "처음먹엇을때",
          "original_form": "처음먹엇을때",
          "speaker_id": "2",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007936.1.17",
          "form": "엄청달아서놀랏엇어요",
          "original_form": "엄청달아서놀랏엇어요",
          "speaker_id": "2",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007936.1.18",
          "form": "진짜설탕이라 입에서 스르륵녹는",
          "original_form": "진짜설탕이라 입에서 스르륵녹는",
          "speaker_id": "2",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007936.1.19",
          "form": "완전요.. 당 충전에 기막히네요",
          "original_form": "완전요.. 당 충전에 기막히네요",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007936.1.20",
          "form": "하리보도 샀어요 땡겨서",
          "original_form": "하리보도 샀어요 땡겨서",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007936.1.21",
          "form": "당 줄인지 오랜데 어제 컨디션이 너무..",
          "original_form": "당 줄인지 오랜데 어제 컨디션이 너무..",
          "speaker_id": "1",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007936.1.22",
          "form": "두통까지 오니 확 죽을맛이었네요",
          "original_form": "두통까지 오니 확 죽을맛이었네요",
          "speaker_id": "1",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007936.1.23",
          "form": "잘하셧어요",
          "original_form": "잘하셧어요",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007936.1.24",
          "form": "리얼 삶의빛",
          "original_form": "리얼 삶의빛",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007936.1.25",
          "form": "그릭요거트도 샀어요 과일이랑 먹으려고",
          "original_form": "그릭요거트도 샀어요 과일이랑 먹으려고",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007936.1.26",
          "form": "민트초코 아이스크림도...",
          "original_form": "민트초코 아이스크림도...",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007936.1.27",
          "form": "민초 그렇게 좋아한건 아녔는데 훅 땡길때가 있어요",
          "original_form": "민초 그렇게 좋아한건 아녔는데 훅 땡길때가 있어요",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007936.1.28",
          "form": "복숭아그릭요거트",
          "original_form": "복숭아그릭요거트",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007936.1.29",
          "form": "해드셔보세요",
          "original_form": "해드셔보세요",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007936.1.30",
          "form": "오늘 과일 시킬까했는데 복숭아 시켜야겠어요",
          "original_form": "오늘 과일 시킬까했는데 복숭아 시켜야겠어요",
          "speaker_id": "1",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007936.1.31",
          "form": "딱복좋아하세요 물복좋아하세요",
          "original_form": "딱복좋아하세요 물복좋아하세요",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007936.1.32",
          "form": "참고로 전 돼지라 다좋아합니다",
          "original_form": "참고로 전 돼지라 다좋아합니다",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007936.1.33",
          "form": "저도 둘다 좋아합니다 달기만 하다면야",
          "original_form": "저도 둘다 좋아합니다 달기만 하다면야",
          "speaker_id": "1",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007936.1.34",
          "form": "충격을 받았네요",
          "original_form": "충격을 받았네요",
          "speaker_id": "1",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007936.1.35",
          "form": "엄마한테 말했더니 원래 맛있는 과일은 벌레가 먼저 먹는거라셔서",
          "original_form": "엄마한테 말했더니 원래 맛있는 과일은 벌레가 먼저 먹는거라셔서",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100007936.1.36",
          "form": "맛있긴했는데 뜻밖의 단백질섭취..",
          "original_form": "맛있긴했는데 뜻밖의 단백질섭취..",
          "speaker_id": "1",
          "time": "20210822 12:38"
        }
      ]
