      "utterance": [
        {
          "id": "MDRW2100009980.1.1",
          "form": "안녕하세요! 범사님은 최근에 어떤거 사셨나요?",
          "original_form": "안녕하세요! 범사님은 최근에 어떤거 사셨나요?",
          "speaker_id": "1",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009980.1.2",
          "form": "저 제일 최근에",
          "original_form": "저 제일 최근에",
          "speaker_id": "2",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009980.1.3",
          "form": "친구 선물 때문에 목걸이 샀어요!",
          "original_form": "친구 선물 때문에 목걸이 샀어요!",
          "speaker_id": "2",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009980.1.4",
          "form": "우와 어디에서 사셨나요??",
          "original_form": "우와 어디에서 사셨나요??",
          "speaker_id": "1",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009980.1.5",
          "form": "29cm라는 쇼핑몰에서 샀어요",
          "original_form": "29cm라는 쇼핑몰에서 샀어요",
          "speaker_id": "2",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009980.1.6",
          "form": "실버 목걸이~",
          "original_form": "실버 목걸이~",
          "speaker_id": "2",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009980.1.7",
          "form": "아하 인터넷으로 사셨군요",
          "original_form": "아하 인터넷으로 사셨군요",
          "speaker_id": "1",
          "time": "20210823 13:01"
        },
        {
          "id": "MDRW2100009980.1.8",
          "form": "저도 며칠전에 셔츠 몇벌 주문했어요!",
          "original_form": "저도 며칠전에 셔츠 몇벌 주문했어요!",
          "speaker_id": "1",
          "time": "20210823 13:02"
        },
        {
          "id": "MDRW2100009980.1.9",
          "form": "오 어떤 셔츠 사셨나요?",
          "original_form": "오 어떤 셔츠 사셨나요?",
          "speaker_id": "2",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100009980.1.10",
          "form": "그냥 포멀하게 입을 베이지색이랑 블랙 하나 샀어요!ㅎㅎ",
          "original_form": "그냥 포멀하게 입을 베이지색이랑 블랙 하나 샀어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100009980.1.11",
          "form": "옷 자주 사시나요??",
          "original_form": "옷 자주 사시나요??",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100009980.1.12",
          "form": "아뇨 저는 정말 거의 안 사요",
          "original_form": "아뇨 저는 정말 거의 안 사요",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100009980.1.13",
          "form": "저는... 당근마켓만 요즘",
          "original_form": "저는... 당근마켓만 요즘",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100009980.1.14",
          "form": "자주 하는 느낌요",
          "original_form": "자주 하는 느낌요",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100009980.1.15",
          "form": "오 당근으로 어떤거 사고팔아 보셨나요!ㅋㅋㅋ",
          "original_form": "오 당근으로 어떤거 사고팔아 보셨나요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100009980.1.16",
          "form": "저 집에 있는 문학책도 갖다 팔고",
          "original_form": "저 집에 있는 문학책도 갖다 팔고",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100009980.1.17",
          "form": "이것저것 살림 다 갖다 팔고 있어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이것저것 살림 다 갖다 팔고 있어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100009980.1.18",
          "form": "아하ㅋㅋㅋㅋㅋㅋㅋ 지금까지 진상 안만나고 잘 파셨나요??ㅠㅠㅠㅠ",
          "original_form": "아하ㅋㅋㅋㅋㅋㅋㅋ 지금까지 진상 안만나고 잘 파셨나요??ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100009980.1.19",
          "form": "저는 살 때 한 번? 진상 보고",
          "original_form": "저는 살 때 한 번? 진상 보고",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100009980.1.20",
          "form": "거의 없었어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "거의 없었어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100009980.1.21",
          "form": "헉 다행이네요! 저는 가끔 이상하게 네고하는 인간들 있더라고요ㅠㅠㅠㅠ",
