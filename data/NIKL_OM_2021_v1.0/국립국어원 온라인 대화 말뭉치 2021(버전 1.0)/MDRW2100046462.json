      "utterance": [
        {
          "id": "MDRW2100046462.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20211022 11:30"
        },
        {
          "id": "MDRW2100046462.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211022 14:12"
        },
        {
          "id": "MDRW2100046462.1.3",
          "form": "혹시 글램핑 좋아하세요?",
          "original_form": "혹시 글램핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20211022 14:12"
        },
        {
          "id": "MDRW2100046462.1.4",
          "form": "좋아하긴 하는데 자주 못가는 편입니다.",
          "original_form": "좋아하긴 하는데 자주 못가는 편입니다.",
          "speaker_id": "1",
          "time": "20211022 14:12"
        },
        {
          "id": "MDRW2100046462.1.5",
          "form": "오호 저는 딱 한 번 가봤는데",
          "original_form": "오호 저는 딱 한 번 가봤는데",
          "speaker_id": "2",
          "time": "20211022 14:13"
        },
        {
          "id": "MDRW2100046462.1.6",
          "form": "비 오는날이어서",
          "original_form": "비 오는날이어서",
          "speaker_id": "2",
          "time": "20211022 14:13"
        },
        {
          "id": "MDRW2100046462.1.7",
          "form": "망했다 싶었는데",
          "original_form": "망했다 싶었는데",
          "speaker_id": "2",
          "time": "20211022 14:13"
        },
        {
          "id": "MDRW2100046462.1.8",
          "form": "오히려 그게 신의 한수가되서 ㅎㅎ",
          "original_form": "오히려 그게 신의 한수가되서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211022 14:13"
        },
        {
          "id": "MDRW2100046462.1.9",
          "form": "맞아요~ 비오는날이 더 운치있죠",
          "original_form": "맞아요~ 비오는날이 더 운치있죠",
          "speaker_id": "1",
          "time": "20211022 14:13"
        },
        {
          "id": "MDRW2100046462.1.10",
          "form": "글램핑이 너무 좋아졌는데",
          "original_form": "글램핑이 너무 좋아졌는데",
          "speaker_id": "2",
          "time": "20211022 14:13"
        },
        {
          "id": "MDRW2100046462.1.11",
          "form": "시간이 없어서 그 뒤로는 못갔네요 흑흑",
          "original_form": "시간이 없어서 그 뒤로는 못갔네요 흑흑",
          "speaker_id": "2",
          "time": "20211022 14:13"
        },
        {
          "id": "MDRW2100046462.1.12",
          "form": "그러게요 정말 너무 귀중한 경험이었어요 ㅠㅠ",
          "original_form": "그러게요 정말 너무 귀중한 경험이었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211022 14:13"
        },
        {
          "id": "MDRW2100046462.1.13",
          "form": "글램핑 참 좋죠",
          "original_form": "글램핑 참 좋죠",
          "speaker_id": "1",
          "time": "20211022 14:17"
        },
        {
          "id": "MDRW2100046462.1.14",
          "form": "혹시 글램핑 명소 아시는 곳 있으시면 추천해주세요!",
          "original_form": "혹시 글램핑 명소 아시는 곳 있으시면 추천해주세요!",
          "speaker_id": "2",
          "time": "20211022 14:17"
        },
        {
          "id": "MDRW2100046462.1.15",
          "form": "저는 집이 경기북부쪽이라",
          "original_form": "저는 집이 경기북부쪽이라",
          "speaker_id": "1",
          "time": "20211022 14:18"
        },
        {
          "id": "MDRW2100046462.1.16",
          "form": "양주쪽이나",
          "original_form": "양주쪽이나",
          "speaker_id": "1",
          "time": "20211022 14:18"
        },
        {
          "id": "MDRW2100046462.1.17",
          "form": "연천쪽",
          "original_form": "연천쪽",
          "speaker_id": "1",
          "time": "20211022 14:18"
        },
        {
          "id": "MDRW2100046462.1.18",
          "form": "가곤 했어요.",
          "original_form": "가곤 했어요.",
          "speaker_id": "1",
          "time": "20211022 14:18"
        },
        {
          "id": "MDRW2100046462.1.19",
          "form": "아하 거기도 나중에 가봐야겠어용",
          "original_form": "아하 거기도 나중에 가봐야겠어용",
          "speaker_id": "2",
          "time": "20211022 14:18"
        },
        {
          "id": "MDRW2100046462.1.20",
          "form": "저는 가평만 가봤네요 ㅎㅎㅎ 초보티 팍팍",
          "original_form": "저는 가평만 가봤네요 ㅎㅎㅎ 초보티 팍팍",
          "speaker_id": "2",
          "time": "20211022 14:19"
        },
        {
          "id": "MDRW2100046462.1.21",
          "form": "혹시 글램핑 가시면",
          "original_form": "혹시 글램핑 가시면",
          "speaker_id": "2",
          "time": "20211022 14:19"
        },
        {
          "id": "MDRW2100046462.1.22",
          "form": "뭐 많이 드세요?!",
          "original_form": "뭐 많이 드세요?!",
          "speaker_id": "2",
          "time": "20211022 14:19"
        },
        {
          "id": "MDRW2100046462.1.23",
          "form": "보통 바베큐 많이 먹죠~",
          "original_form": "보통 바베큐 많이 먹죠~",
          "speaker_id": "1",
          "time": "20211022 14:19"
        },
        {
          "id": "MDRW2100046462.1.24",
          "form": "가는길에",
          "original_form": "가는길에",
          "speaker_id": "1",
          "time": "20211022 14:19"
        },
        {
          "id": "MDRW2100046462.1.25",
          "form": "하긴 ㅎㅎㅎㅎ 무조건 고기 먹어야죠!!",
          "original_form": "하긴 ㅎㅎㅎㅎ 무조건 고기 먹어야죠!!",
          "speaker_id": "2",
          "time": "20211022 14:19"
        },
        {
          "id": "MDRW2100046462.1.26",
          "form": "밀키트 사가서 찌개같은거도 먹고요 ㅎ",
          "original_form": "밀키트 사가서 찌개같은거도 먹고요 ㅎ",
          "speaker_id": "1",
          "time": "20211022 14:19"
        },
        {
          "id": "MDRW2100046462.1.27",
          "form": "크아~~~ 맞아여",
          "original_form": "크아~~~ 맞아여",
          "speaker_id": "2",
          "time": "20211022 14:19"
        },
        {
          "id": "MDRW2100046462.1.28",
          "form": "아침엔 라면 먹어줘야하는거 아시죠",
          "original_form": "아침엔 라면 먹어줘야하는거 아시죠",
          "speaker_id": "2",
          "time": "20211022 14:19"
        },
        {
          "id": "MDRW2100046462.1.29",
          "form": "그럼요 ㅎ",
          "original_form": "그럼요 ㅎ",
          "speaker_id": "1",
          "time": "20211022 14:20"
        },
        {
          "id": "MDRW2100046462.1.30",
          "form": "저는 새우 넣어서 먹었다가",
          "original_form": "저는 새우 넣어서 먹었다가",
          "speaker_id": "2",
          "time": "20211022 14:20"
        },
        {
          "id": "MDRW2100046462.1.31",
          "form": "아침에 소주 때릴뻔했습니다",
          "original_form": "아침에 소주 때릴뻔했습니다",
          "speaker_id": "2",
          "time": "20211022 14:20"
        },
        {
          "id": "MDRW2100046462.1.32",
          "form": "캬캬",
          "original_form": "캬캬",
          "speaker_id": "2",
          "time": "20211022 14:20"
        },
        {
          "id": "MDRW2100046462.1.33",
          "form": "ㅎㅎ 술 좋아하시나보네요~",
          "original_form": "ㅎㅎ 술 좋아하시나보네요~",
          "speaker_id": "1",
          "time": "20211022 14:20"
        },
        {
          "id": "MDRW2100046462.1.34",
          "form": "다음에 새우 넣고 라면 꼭 드셔보세요....죽음이에요 죽음",
          "original_form": "다음에 새우 넣고 라면 꼭 드셔보세요....죽음이에요 죽음",
          "speaker_id": "2",
          "time": "20211022 14:20"
        },
        {
          "id": "MDRW2100046462.1.35",
          "form": "그건 아닌데 약간 소주 생각이 뽝 나더라구욬ㅋㅋㅋㅋㅎㅎㅎ\r\n",
          "original_form": "그건 아닌데 약간 소주 생각이 뽝 나더라구욬ㅋㅋㅋㅋㅎㅎㅎ\r\n",
          "speaker_id": "2",
          "time": "20211022 14:20"
        }
      ]
