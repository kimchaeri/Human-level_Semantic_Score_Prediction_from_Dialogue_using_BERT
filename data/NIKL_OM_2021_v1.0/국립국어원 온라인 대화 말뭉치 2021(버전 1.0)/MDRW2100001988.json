      "utterance": [
        {
          "id": "MDRW2100001988.1.1",
          "form": "안녕하세요:) name2님ㅋㅋ",
          "original_form": "안녕하세요:) &name2&님ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001988.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100001988.1.3",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100001988.1.4",
          "form": "쇼핑 좋아하는데\r\n코로나 이후에 외출을 못하니",
          "original_form": "쇼핑 좋아하는데\r\n코로나 이후에 외출을 못하니",
          "speaker_id": "1",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100001988.1.5",
          "form": "카드값이 줄었다가\r\n지금은 카드값 난리네요ㅋㅋㅋ",
          "original_form": "카드값이 줄었다가\r\n지금은 카드값 난리네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100001988.1.6",
          "form": "오히려 가서 쇼핑하는것보다\r\n더 쇼핑을... ㅋㅋㅋ",
          "original_form": "오히려 가서 쇼핑하는것보다\r\n더 쇼핑을... ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100001988.1.7",
          "form": "맞아요 욕구해소를 못하니",
          "original_form": "맞아요 욕구해소를 못하니",
          "speaker_id": "2",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001988.1.8",
          "form": "더더 필요없는 화장품을 사고 있고",
          "original_form": "더더 필요없는 화장품을 사고 있고",
          "speaker_id": "2",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001988.1.9",
          "form": "오늘도 몇건 산거 같아여;;",
          "original_form": "오늘도 몇건 산거 같아여;;",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100001988.1.10",
          "form": "아ㅠㅠ 저만 그런게 아니네요ㅠㅠ",
          "original_form": "아ㅠㅠ 저만 그런게 아니네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001988.1.11",
          "form": "화장품은 뭐 사셨어요?",
          "original_form": "화장품은 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001988.1.12",
          "form": "마스크때문인지 피부 트러블이 심해서\r\n얼굴에 뭐 잘 안바르게 되요ㅠ",
          "original_form": "마스크때문인지 피부 트러블이 심해서\r\n얼굴에 뭐 잘 안바르게 되요ㅠ",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001988.1.13",
          "form": "포인트 눈화장",
          "original_form": "포인트 눈화장",
          "speaker_id": "2",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001988.1.14",
          "form": "눈에 신경 더 쓰는거 같아요",
          "original_form": "눈에 신경 더 쓰는거 같아요",
          "speaker_id": "2",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001988.1.15",
          "form": "오 그럼 섀도우? ㅋㅋ",
          "original_form": "오 그럼 섀도우? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001988.1.16",
          "form": "아이라이너? 마스카라?",
          "original_form": "아이라이너? 마스카라?",
          "speaker_id": "1",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001988.1.17",
          "form": "ㅠㅠ 마스크 주변 피부 트러블은 저도 구래여",
          "original_form": "ㅠㅠ 마스크 주변 피부 트러블은 저도 구래여",
          "speaker_id": "2",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001988.1.18",
          "form": "라이너 마스카라 섀도우 색조",
          "original_form": "라이너 마스카라 섀도우 색조",
          "speaker_id": "2",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100001988.1.19",
          "form": "ㅎㅎ 속눈썹 펌 재료도 샀네요",
          "original_form": "ㅎㅎ 속눈썹 펌 재료도 샀네요",
          "speaker_id": "2",
          "time": "20210819 22:59"
        },
        {
          "id": "MDRW2100001988.1.20",
          "form": "오 화장 잘하시나봐요 ㅋㅋ\r\n저는 똥손이라 아직 섀도우도\r\n해본적 없어여 ㅋㅋ",
          "original_form": "오 화장 잘하시나봐요 ㅋㅋ\r\n저는 똥손이라 아직 섀도우도\r\n해본적 없어여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:00"
        },
        {
          "id": "MDRW2100001988.1.21",
          "form": "속눈썹펌은 혼자 하기 어렵진 않나여?",
          "original_form": "속눈썹펌은 혼자 하기 어렵진 않나여?",
          "speaker_id": "1",
          "time": "20210819 23:00"
        },
        {
          "id": "MDRW2100001988.1.22",
          "form": "하다보면 느는게 손기술이더라구여",
          "original_form": "하다보면 느는게 손기술이더라구여",
          "speaker_id": "2",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100001988.1.23",
          "form": "보고 따라해보기도 하고",
          "original_form": "보고 따라해보기도 하고",
          "speaker_id": "2",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100001988.1.24",
          "form": "유툽같은거 보고 제품정보도 얻고\r\n구매도 하고 해보시나봐요ㅋ",
          "original_form": "유툽같은거 보고 제품정보도 얻고\r\n구매도 하고 해보시나봐요ㅋ",
          "speaker_id": "1",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100001988.1.25",
          "form": "집에만 있으니 심심해서요",
          "original_form": "집에만 있으니 심심해서요",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001988.1.26",
          "form": "해보고 기분전환하고",
          "original_form": "해보고 기분전환하고",
          "speaker_id": "2",
          "time": "20210819 23:08"
        },
        {
          "id": "MDRW2100001988.1.27",
          "form": "저도 한번 도전해봐야겠어영ㅋㅋ",
          "original_form": "저도 한번 도전해봐야겠어영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100001988.1.28",
          "form": "하시고  잘 지우세요",
          "original_form": "하시고  잘 지우세요",
          "speaker_id": "2",
          "time": "20210819 23:55"
        }
      ]
    }
