      "utterance": [
        {
          "id": "MDRW2100034772.1.1",
          "form": "ㅜㅜ저도요",
          "original_form": "ㅜㅜ저도요",
          "speaker_id": "2",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100034772.1.2",
          "form": "저 세시에마시면 새벽세시넘어도 못자요ㅜㅜㅜㅜㅜ",
          "original_form": "저 세시에마시면 새벽세시넘어도 못자요ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034772.1.3",
          "form": "하 심하시네요",
          "original_form": "하 심하시네요",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034772.1.4",
          "form": "저는 아무때나먹어도 아침7시에 잔답니다",
          "original_form": "저는 아무때나먹어도 아침7시에 잔답니다",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034772.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034772.1.6",
          "form": "ㅋㅋ아니ㅋㅋ왠찮으신건가요",
          "original_form": "ㅋㅋ아니ㅋㅋ왠찮으신건가요",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100034772.1.7",
          "form": "그래서 한달에 몇번 못먹는거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 한달에 몇번 못먹는거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034772.1.8",
          "form": "잘먹는 사람들이 대부분이던데 너무 부러워요 ㅠㅠ",
          "original_form": "잘먹는 사람들이 대부분이던데 너무 부러워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034772.1.9",
          "form": "아 ㅋㅋㅋ독극물이네",
          "original_form": "아 ㅋㅋㅋ독극물이네",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034772.1.10",
          "form": "카페인약하시네요",
          "original_form": "카페인약하시네요",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034772.1.11",
          "form": "진짜 그러네요 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 그러네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034772.1.12",
          "form": "심장 두근거린단 친구 있는데 그친구도 잠은 잘 자더라구요",
          "original_form": "심장 두근거린단 친구 있는데 그친구도 잠은 잘 자더라구요",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034772.1.13",
          "form": "억지로드시지마세요ㅜㅜㅜ",
          "original_form": "억지로드시지마세요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034772.1.14",
          "form": "그건 타고나는거같아요",
          "original_form": "그건 타고나는거같아요",
          "speaker_id": "2",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034772.1.15",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034772.1.16",
          "form": "그래서 가끔 디카페만 마시는데",
          "original_form": "그래서 가끔 디카페만 마시는데",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034772.1.17",
          "form": "파는 곳이 별로 없어요",
          "original_form": "파는 곳이 별로 없어요",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034772.1.18",
          "form": "아ㅜㅜㅜ디카페인 많은거같은데ㅜ",
          "original_form": "아ㅜㅜㅜ디카페인 많은거같은데ㅜ",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034772.1.19",
          "form": "프랜차이즈만 많더라구여 ㅠㅠㅠ",
          "original_form": "프랜차이즈만 많더라구여 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034772.1.20",
          "form": "글쿤요 콜드블루는 좀 적다던데",
          "original_form": "글쿤요 콜드블루는 좀 적다던데",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034772.1.21",
          "form": "카페인이",
          "original_form": "카페인이",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034772.1.22",
          "form": "오 그래요??",
          "original_form": "오 그래요??",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034772.1.23",
          "form": "참고할게요 ㅋㅋㅋ",
          "original_form": "참고할게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034772.1.24",
          "form": "하지만 디카페인은 아닙니당ㅎㅎ",
          "original_form": "하지만 디카페인은 아닙니당ㅎㅎ",
