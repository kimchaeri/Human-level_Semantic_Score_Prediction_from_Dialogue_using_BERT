      "utterance": [
        {
          "id": "MDRW2100010210.1.1",
          "form": "일 하시고 계신가요?",
          "original_form": "일 하시고 계신가요?",
          "speaker_id": "2",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100010210.1.2",
          "form": "아르바이트 하신 적 있으세요?",
          "original_form": "아르바이트 하신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100010210.1.3",
          "form": "피시방 알바해봤어요",
          "original_form": "피시방 알바해봤어요",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100010210.1.4",
          "form": "피시방 알바 어땠나요?",
          "original_form": "피시방 알바 어땠나요?",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100010210.1.5",
          "form": "음.. 별로였어요ㅎ",
          "original_form": "음.. 별로였어요ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100010210.1.6",
          "form": "ㅋㅋㅋㅋㅋ 너무 학생들이 많죠?",
          "original_form": "ㅋㅋㅋㅋㅋ 너무 학생들이 많죠?",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100010210.1.7",
          "form": "방학 때 좀 많고 학생들 많은 것보다",
          "original_form": "방학 때 좀 많고 학생들 많은 것보다",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100010210.1.8",
          "form": "음식을 만들어야 한다는 게 너무 힘들었어요",
          "original_form": "음식을 만들어야 한다는 게 너무 힘들었어요",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100010210.1.9",
          "form": "아 음식이 거의 카페 수준이더라두요...",
          "original_form": "아 음식이 거의 카페 수준이더라두요...",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100010210.1.10",
          "form": "보고 충격",
          "original_form": "보고 충격",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100010210.1.11",
          "form": "진짜 너무 힘들었어요",
          "original_form": "진짜 너무 힘들었어요",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100010210.1.12",
          "form": "이게 피시방 인지 음식점인지 모를 만큼",
          "original_form": "이게 피시방 인지 음식점인지 모를 만큼",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100010210.1.13",
          "form": "아르바이트해보셨나요",
          "original_form": "아르바이트해보셨나요",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100010210.1.14",
          "form": "네 저는 수영복 판매 해봤는데",
          "original_form": "네 저는 수영복 판매 해봤는데",
          "speaker_id": "2",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100010210.1.15",
          "form": "이게 나름 꿀이였어요 ㅋㅋ",
          "original_form": "이게 나름 꿀이였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100010210.1.16",
          "form": "수영복 판매요?",
          "original_form": "수영복 판매요?",
          "speaker_id": "1",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100010210.1.17",
          "form": "수영장 안에 있는 곳에서 판매 하셨나요",
          "original_form": "수영장 안에 있는 곳에서 판매 하셨나요",
          "speaker_id": "1",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100010210.1.18",
          "form": "ㅋㅋ 아니요 마트 안에 있는 곳이요!",
          "original_form": "ㅋㅋ 아니요 마트 안에 있는 곳이요!",
          "speaker_id": "2",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100010210.1.19",
          "form": "마트 안에  있는 곳이면 손님들이 많이 왔겠어요",
          "original_form": "마트 안에  있는 곳이면 손님들이 많이 왔겠어요",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100010210.1.20",
          "form": "수영복이라 그런지",
          "original_form": "수영복이라 그런지",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100010210.1.21",
          "form": "한두번 반짝하고는 널널했어요 ㅋㅋ",
          "original_form": "한두번 반짝하고는 널널했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100010210.1.22",
          "form": "여름철에 인기가 많았겠네요",
          "original_form": "여름철에 인기가 많았겠네요",
          "speaker_id": "1",
          "time": "20210824 16:15"
        }
      ]
