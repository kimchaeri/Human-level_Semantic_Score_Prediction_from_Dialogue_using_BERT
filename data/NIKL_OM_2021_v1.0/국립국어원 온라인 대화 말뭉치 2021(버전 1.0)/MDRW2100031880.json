      "utterance": [
        {
          "id": "MDRW2100031880.1.1",
          "form": "제주변에는 그런남자들 많이 없어요",
          "original_form": "제주변에는 그런남자들 많이 없어요",
          "speaker_id": "2",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100031880.1.2",
          "form": "그럼 어떤 사람들인데요?",
          "original_form": "그럼 어떤 사람들인데요?",
          "speaker_id": "1",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100031880.1.3",
          "form": "다들 겸손하고 배려하고 그렇더라구요",
          "original_form": "다들 겸손하고 배려하고 그렇더라구요",
          "speaker_id": "2",
          "time": "20210905 07:16"
        },
        {
          "id": "MDRW2100031880.1.4",
          "form": "그럼 좋네요",
          "original_form": "그럼 좋네요",
          "speaker_id": "1",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100031880.1.5",
          "form": "제 회사 동료들도 다 그렇고",
          "original_form": "제 회사 동료들도 다 그렇고",
          "speaker_id": "2",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100031880.1.6",
          "form": "제 동기들도",
          "original_form": "제 동기들도",
          "speaker_id": "2",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100031880.1.7",
          "form": "혹시 회사가 어떤 쪽인지 여쭤봐도 돼요?",
          "original_form": "혹시 회사가 어떤 쪽인지 여쭤봐도 돼요?",
          "speaker_id": "1",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100031880.1.8",
          "form": "음 연구 기술쪽이고 공공기관입니다",
          "original_form": "음 연구 기술쪽이고 공공기관입니다",
          "speaker_id": "2",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100031880.1.9",
          "form": "아 그러면 이과쪽 나오신거죠?",
          "original_form": "아 그러면 이과쪽 나오신거죠?",
          "speaker_id": "1",
          "time": "20210905 07:17"
        },
        {
          "id": "MDRW2100031880.1.10",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100031880.1.11",
          "form": "문과 전공인데",
          "original_form": "문과 전공인데",
          "speaker_id": "1",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100031880.1.12",
          "form": "남자들 찌질한 거 많이 겪었어요",
          "original_form": "남자들 찌질한 거 많이 겪었어요",
          "speaker_id": "1",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100031880.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100031880.1.14",
          "form": "남자는 원래 지질합니다 ㅋㅋ",
          "original_form": "남자는 원래 지질합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:18"
        },
        {
          "id": "MDRW2100031880.1.15",
          "form": "그러면 여자가 그 멘탈 케어를",
          "original_form": "그러면 여자가 그 멘탈 케어를",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100031880.1.16",
          "form": "다 해 줘야 되는데 저는 그게 노동으로 느껴져요",
          "original_form": "다 해 줘야 되는데 저는 그게 노동으로 느껴져요",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100031880.1.17",
          "form": "혹시 연애 몇번하셨을까요?",
          "original_form": "혹시 연애 몇번하셨을까요?",
          "speaker_id": "2",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100031880.1.18",
          "form": "정식 교제",
          "original_form": "정식 교제",
          "speaker_id": "2",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100031880.1.19",
          "form": "그렇게 많이는 안 했어요",
          "original_form": "그렇게 많이는 안 했어요",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100031880.1.20",
          "form": "서너번?",
          "original_form": "서너번?",
          "speaker_id": "1",
          "time": "20210905 07:19"
        },
        {
          "id": "MDRW2100031880.1.21",
          "form": "혹시 1년이상 사귄경험이있나요?",
