      "utterance": [
        {
          "id": "MDRW2100018643.1.1",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018643.1.2",
          "form": "아니요 알바해요! 님은요?",
          "original_form": "아니요 알바해요! 님은요?",
          "speaker_id": "2",
          "time": "20210827 09:51"
        },
        {
          "id": "MDRW2100018643.1.3",
          "form": "저는 취준중이에요!",
          "original_form": "저는 취준중이에요!",
          "speaker_id": "1",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018643.1.4",
          "form": "어떤 알바하세요?",
          "original_form": "어떤 알바하세요?",
          "speaker_id": "1",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018643.1.5",
          "form": "전 우체국에서 방학동안 일하고 있어요",
          "original_form": "전 우체국에서 방학동안 일하고 있어요",
          "speaker_id": "2",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018643.1.6",
          "form": "우와 어떤일을 하나요?",
          "original_form": "우와 어떤일을 하나요?",
          "speaker_id": "1",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018643.1.7",
          "form": "음 사무실에 장부같은걸 다 수거해서 싸인 받구요\n또 직원분들께 우편물도 나눠드려요!",
          "original_form": "음 사무실에 장부같은걸 다 수거해서 싸인 받구요\n또 직원분들께 우편물도 나눠드려요!",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018643.1.8",
          "form": "우체국에서 일하는건 처음들어봐서 신기해요!",
          "original_form": "우체국에서 일하는건 처음들어봐서 신기해요!",
          "speaker_id": "1",
          "time": "20210827 10:05"
        },
        {
          "id": "MDRW2100018643.1.9",
          "form": "좋은 알바 하고 계시네요~",
          "original_form": "좋은 알바 하고 계시네요~",
          "speaker_id": "1",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018643.1.10",
          "form": "그러게요 몸은 확실히 편하고 좋은거 같아요",
          "original_form": "그러게요 몸은 확실히 편하고 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210827 10:12"
        },
        {
          "id": "MDRW2100018643.1.11",
          "form": "어떤 직종으로 취업 준비하고 계세요?",
          "original_form": "어떤 직종으로 취업 준비하고 계세요?",
          "speaker_id": "2",
          "time": "20210827 10:13"
        },
        {
          "id": "MDRW2100018643.1.12",
          "form": "공무원이요!",
          "original_form": "공무원이요!",
          "speaker_id": "1",
          "time": "20210827 10:22"
        },
        {
          "id": "MDRW2100018643.1.13",
          "form": "다른일 하다가 아무래도 안정적인게 하고 싶다는 생각을 하네요..ㅠ",
          "original_form": "다른일 하다가 아무래도 안정적인게 하고 싶다는 생각을 하네요..ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:23"
        },
        {
          "id": "MDRW2100018643.1.14",
          "form": "아 네 보통 그렇죠ㅜ 그런데 제가 같이 일하는 공무원들은 정시 출퇴근을 하는걸 못보고 나름 힘든일도 많은거 같더라구요",
          "original_form": "아 네 보통 그렇죠ㅜ 그런데 제가 같이 일하는 공무원들은 정시 출퇴근을 하는걸 못보고 나름 힘든일도 많은거 같더라구요",
          "speaker_id": "2",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018643.1.15",
          "form": "아 그런가요?ㅠ",
          "original_form": "아 그런가요?ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018643.1.16",
          "form": "그래도 사기업에 비하면 쉬운 일이겠죠?ㅜㅜ\n취업하기 무섭네요",
          "original_form": "그래도 사기업에 비하면 쉬운 일이겠죠?ㅜㅜ\n취업하기 무섭네요",
          "speaker_id": "2",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018643.1.17",
          "form": "공무원도 편하다는건 예전말이네요. ㅠ",
          "original_form": "공무원도 편하다는건 예전말이네요. ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018643.1.18",
          "form": "맞아요ㅠ 코로나때문에 더 위축이..",
          "original_form": "맞아요ㅠ 코로나때문에 더 위축이..",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018643.1.19",
          "form": "어떤 직종 준비하세요?",
          "original_form": "어떤 직종 준비하세요?",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018643.1.20",
          "form": "네 맞아요ㅜㅜ 전 개발자요! 재택근무 하는게 꿈이라ㅎㅎ",
          "original_form": "네 맞아요ㅜㅜ 전 개발자요! 재택근무 하는게 꿈이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 10:27"
        }
      ]
