      "utterance": [
        {
          "id": "MDRW2100018411.1.1",
          "form": "안녕하세요! 혹시 공연 관람 좋아하세요...?",
          "original_form": "안녕하세요! 혹시 공연 관람 좋아하세요...?",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100018411.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100018411.1.3",
          "form": "저는 공연보다는 전시회 자주 가요",
          "original_form": "저는 공연보다는 전시회 자주 가요",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100018411.1.4",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100018411.1.5",
          "form": "저도 미술 전시회 좋아해요~",
          "original_form": "저도 미술 전시회 좋아해요~",
          "speaker_id": "2",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100018411.1.6",
          "form": "취미가 비슷하네요 어떤 미술가 좋아하세요?",
          "original_form": "취미가 비슷하네요 어떤 미술가 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018411.1.7",
          "form": "저는 앤디워홀이요ㅎㅎ",
          "original_form": "저는 앤디워홀이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018411.1.8",
          "form": "전에 친구랑 별 생각없이 갔었는데",
          "original_form": "전에 친구랑 별 생각없이 갔었는데",
          "speaker_id": "2",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018411.1.9",
          "form": "너무 인상 깊더라구요",
          "original_form": "너무 인상 깊더라구요",
          "speaker_id": "2",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018411.1.10",
          "form": "아 앤디워홀 저도 좋아해요",
          "original_form": "아 앤디워홀 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018411.1.11",
          "form": "앤디워홀의 심오한 작품세계가 흥미롭더라구요",
          "original_form": "앤디워홀의 심오한 작품세계가 흥미롭더라구요",
          "speaker_id": "1",
          "time": "20210827 01:26"
        },
        {
          "id": "MDRW2100018411.1.12",
          "form": "아까 아이돌 얘기하시던데 누구 좋아하세요?",
          "original_form": "아까 아이돌 얘기하시던데 누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 01:26"
        },
        {
          "id": "MDRW2100018411.1.13",
          "form": "저는 트와이스 좋아해요!",
          "original_form": "저는 트와이스 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 01:28"
        },
        {
          "id": "MDRW2100018411.1.14",
          "form": "같은 여자지만 멋있고 예쁘고 그렇더라고요",
          "original_form": "같은 여자지만 멋있고 예쁘고 그렇더라고요",
          "speaker_id": "2",
          "time": "20210827 01:28"
        },
        {
          "id": "MDRW2100018411.1.15",
          "form": "아 트와이스 저도 호감이에요 그래서 공연도 가본적 있어요",
          "original_form": "아 트와이스 저도 호감이에요 그래서 공연도 가본적 있어요",
          "speaker_id": "1",
          "time": "20210827 01:28"
        },
        {
          "id": "MDRW2100018411.1.16",
          "form": "진짜요?? 저도 콘서트 다녀온적 있어요",
          "original_form": "진짜요?? 저도 콘서트 다녀온적 있어요",
          "speaker_id": "2",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100018411.1.17",
          "form": "여러번 다녀온거 같아요ㅋㅋㅋㅋ",
          "original_form": "여러번 다녀온거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100018411.1.18",
          "form": "트와이스 공연이 다채로워서 재밌더라고요",
          "original_form": "트와이스 공연이 다채로워서 재밌더라고요",
          "speaker_id": "1",
          "time": "20210827 01:30"
        },
        {
          "id": "MDRW2100018411.1.19",
          "form": "곡도 유명하고",
          "original_form": "곡도 유명하고",
          "speaker_id": "1",
          "time": "20210827 01:30"
        },
        {
          "id": "MDRW2100018411.1.20",
          "form": "맞아요 그게 장점인거 같아요",
          "original_form": "맞아요 그게 장점인거 같아요",
          "speaker_id": "2",
          "time": "20210827 01:30"
        },
        {
          "id": "MDRW2100018411.1.21",
          "form": "곡이 유명해서 다들 따라부를 수 있으니까요",
          "original_form": "곡이 유명해서 다들 따라부를 수 있으니까요",
          "speaker_id": "2",
          "time": "20210827 01:30"
        },
        {
          "id": "MDRW2100018411.1.22",
          "form": "앤디워홀도 유명하다보니 지인들이랑 같이 가기도 좋더라구요",
          "original_form": "앤디워홀도 유명하다보니 지인들이랑 같이 가기도 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018411.1.23",
          "form": "맞아요!  전시회든 공연이든 어느정도 인지도가 있어야 좋은거 같아요",
          "original_form": "맞아요!  전시회든 공연이든 어느정도 인지도가 있어야 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018411.1.24",
          "form": "그래야 친구들이랑 관람하러 갈때 서로 편한거 같아요",
          "original_form": "그래야 친구들이랑 관람하러 갈때 서로 편한거 같아요",
          "speaker_id": "2",
          "time": "20210827 01:31"
        }
      ]
    }
