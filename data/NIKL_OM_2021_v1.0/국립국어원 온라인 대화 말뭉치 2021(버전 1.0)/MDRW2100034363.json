      "utterance": [
        {
          "id": "MDRW2100034363.1.1",
          "form": "네 초반만 좀 넘기면 진짜 최고예요",
          "original_form": "네 초반만 좀 넘기면 진짜 최고예요",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100034363.1.2",
          "form": "초반은 좀 루즈한가요?",
          "original_form": "초반은 좀 루즈한가요?",
          "speaker_id": "1",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100034363.1.3",
          "form": "루즈하진않은데 불륜얘기가 나와서 꺼려질수도있을거같아서요",
          "original_form": "루즈하진않은데 불륜얘기가 나와서 꺼려질수도있을거같아서요",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100034363.1.4",
          "form": "전체적으로 긴장감있고 재밌긴해요",
          "original_form": "전체적으로 긴장감있고 재밌긴해요",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100034363.1.5",
          "form": "아 막장을 하도 봐서 그런건 이제 아무렇지도 않더라구요 ㅋㅋㅋㅋ",
          "original_form": "아 막장을 하도 봐서 그런건 이제 아무렇지도 않더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100034363.1.6",
          "form": "긴장감있는게 몰입잘돼서  좋아요",
          "original_form": "긴장감있는게 몰입잘돼서  좋아요",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100034363.1.7",
          "form": "그렇다면 괜찮으시겠네요",
          "original_form": "그렇다면 괜찮으시겠네요",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100034363.1.8",
          "form": "네 참고할게요 ㅋㅋㅋ",
          "original_form": "네 참고할게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100034363.1.9",
          "form": "마자요 몰입 잘돼요",
          "original_form": "마자요 몰입 잘돼요",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100034363.1.10",
          "form": "유튜브는 뭐보세요?",
          "original_form": "유튜브는 뭐보세요?",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100034363.1.11",
          "form": "저는 요새 또  입덕해가지구 아이돌 영상봐요ㅋㅋㄱ",
          "original_form": "저는 요새 또  입덕해가지구 아이돌 영상봐요ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100034363.1.12",
          "form": "오 누구요??",
          "original_form": "오 누구요??",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100034363.1.13",
          "form": "드라마 메이킹도 보구요",
          "original_form": "드라마 메이킹도 보구요",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100034363.1.14",
          "form": "메이킹도 재밌어요 ㅋㅋㅋ",
          "original_form": "메이킹도 재밌어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100034363.1.15",
          "form": "요새 드림캐쳐 노래가 좋아서",
          "original_form": "요새 드림캐쳐 노래가 좋아서",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100034363.1.16",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100034363.1.17",
          "form": "찾아보다보니까 노래 잘하더라구요ㅠ",
          "original_form": "찾아보다보니까 노래 잘하더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100034363.1.18",
          "form": "저 드림캐쳐 최애노래 있어요 ㅋㅋㅋ",
          "original_form": "저 드림캐쳐 최애노래 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100034363.1.19",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100034363.1.20",
          "form": "네 ㅋㅋㅋ 최근 노래는 모르는데",
          "original_form": "네 ㅋㅋㅋ 최근 노래는 모르는데",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100034363.1.21",
          "form": "캐치미아세요?",
          "original_form": "캐치미아세요?",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100034363.1.22",
          "form": "오~ 뭔가요?",
          "original_form": "오~ 뭔가요?",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100034363.1.23",
          "form": "너무좋아요",
          "original_form": "너무좋아요",
          "speaker_id": "1",
          "time": "20210828 00:34"
        }
      ]
    }
