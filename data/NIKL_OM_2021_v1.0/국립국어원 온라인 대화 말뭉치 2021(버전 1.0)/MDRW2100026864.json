      "utterance": [
        {
          "id": "MDRW2100026864.1.1",
          "form": "?뭐지",
          "original_form": "?뭐지",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.2",
          "form": "반려뱀과 여우가 있어요",
          "original_form": "반려뱀과 여우가 있어요",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.3",
          "form": "평생 함께하기로 했어요",
          "original_form": "평생 함께하기로 했어요",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.4",
          "form": "저는 요즘 고양이가",
          "original_form": "저는 요즘 고양이가",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.5",
          "form": "좋다고 쓸랬는데",
          "original_form": "좋다고 쓸랬는데",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.6",
          "form": "이런 곳에서 오타쿠 티내지 마세요",
          "original_form": "이런 곳에서 오타쿠 티내지 마세요",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.7",
          "form": "그럼요그럼요",
          "original_form": "그럼요그럼요",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.8",
          "form": "name3는 잘 지내나요",
          "original_form": "&name3&는 잘 지내나요",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.9",
          "form": "지금 제 옆에서",
          "original_form": "지금 제 옆에서",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.10",
          "form": "?뭐지",
          "original_form": "?뭐지",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026864.1.11",
          "form": "님 제가 보낸 내용 보여요?",
          "original_form": "님 제가 보낸 내용 보여요?",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026864.1.12",
          "form": "name3",
          "original_form": "&name3&",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026864.1.13",
          "form": "네네 보여요",
          "original_form": "네네 보여요",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026864.1.14",
          "form": "나쁜말은 어쩌구도 보이시나용",
          "original_form": "나쁜말은 어쩌구도 보이시나용",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026864.1.15",
          "form": "네?? 아니요??",
          "original_form": "네?? 아니요??",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026864.1.16",
          "form": "헐....말도안대",
          "original_form": "헐....말도안대",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026864.1.17",
          "form": "name3 제옆에서 까까먹는중",
          "original_form": "&name3& 제옆에서 까까먹는중",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026864.1.18",
          "form": "님 나쁜말 했나요",
          "original_form": "님 나쁜말 했나요",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026864.1.19",
          "form": "name3야 맛잇니 너무 많이먹으면 뚠뚠냥이가 된단다",
          "original_form": "&name3&야 맛잇니 너무 많이먹으면 뚠뚠냥이가 된단다",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.20",
          "form": "아니!!! 까까를 츢ㄹㅡ 라고 썼을 때",
          "original_form": "아니!!! 까까를 츢ㄹㅡ 라고 썼을 때",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.21",
          "form": "나쁜말이라구떠요",
          "original_form": "나쁜말이라구떠요",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.22",
          "form": "뚠냥이도 귀엽지만요",
          "original_form": "뚠냥이도 귀엽지만요",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.23",
          "form": "어째서...",
          "original_form": "어째서...",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.24",
          "form": "그친구는 꼬리가 두개라 에너지 소비량도 두배라서",
          "original_form": "그친구는 꼬리가 두개라 에너지 소비량도 두배라서",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.25",
          "form": "많이먹어도 ㄱㅊ",
          "original_form": "많이먹어도 ㄱㅊ",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.26",
          "form": "기적의 계산법",
          "original_form": "기적의 계산법",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.27",
          "form": "저희집 뱀하고 여우도",
          "original_form": "저희집 뱀하고 여우도",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.28",
          "form": "아무래도...",
          "original_form": "아무래도...",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.29",
          "form": "잘먹어요",
          "original_form": "잘먹어요",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.30",
          "form": "우리집 반려여우는 요리도 해줘요",
          "original_form": "우리집 반려여우는 요리도 해줘요",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.31",
          "form": "뭘먹죠?",
          "original_form": "뭘먹죠?",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026864.1.32",
          "form": "와 부럽다",
          "original_form": "와 부럽다",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.33",
          "form": "콜라랑...유부초밥이랑....",
          "original_form": "콜라랑...유부초밥이랑....",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.34",
          "form": "저는 요즘 사슴 키우잖아요",
          "original_form": "저는 요즘 사슴 키우잖아요",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.35",
          "form": "벌크업사슴이요",
          "original_form": "벌크업사슴이요",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.36",
          "form": "엘크아녔나요??",
          "original_form": "엘크아녔나요??",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.37",
          "form": "네네...엘크",
          "original_form": "네네...엘크",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.38",
          "form": "겉보기엔 꽃사슴",
          "original_form": "겉보기엔 꽃사슴",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.39",
          "form": "눈이 즐거워요",
          "original_form": "눈이 즐거워요",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.40",
          "form": "아주 예쁘던데요",
          "original_form": "아주 예쁘던데요",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.41",
          "form": "님네 집에서 잘 자라고 잇는 것 같아서",
          "original_form": "님네 집에서 잘 자라고 잇는 것 같아서",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.42",
          "form": "아무래도 제가 좋아하려면",
          "original_form": "아무래도 제가 좋아하려면",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.43",
          "form": "기쁘네요...",
          "original_form": "기쁘네요...",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026864.1.44",
          "form": "그래야하는편이죠",
          "original_form": "그래야하는편이죠",
          "speaker_id": "2",
          "time": "20210902 00:48"
        }
      ]
    }
