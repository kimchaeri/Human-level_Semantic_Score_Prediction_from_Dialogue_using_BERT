      "utterance": [
        {
          "id": "MDRW2100041002.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100041002.1.2",
          "form": "오늘 점심 뭐드셨어요?",
          "original_form": "오늘 점심 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100041002.1.3",
          "form": "도미노 포테이토피자요! name1님은요?",
          "original_form": "도미노 포테이토피자요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100041002.1.4",
          "form": "와 맛있겠어요",
          "original_form": "와 맛있겠어요",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100041002.1.5",
          "form": "네 꿀맛이에요!",
          "original_form": "네 꿀맛이에요!",
          "speaker_id": "2",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100041002.1.6",
          "form": "어제 도미노 1만원세일했거든요ㅋㅋ",
          "original_form": "어제 도미노 1만원세일했거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100041002.1.7",
          "form": "와 피자 먹고 싶어지네요 ㅠㅠ",
          "original_form": "와 피자 먹고 싶어지네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100041002.1.8",
          "form": "헉 그랬어요???",
          "original_form": "헉 그랬어요???",
          "speaker_id": "1",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100041002.1.9",
          "form": "이미 세일 기간 지났나요?",
          "original_form": "이미 세일 기간 지났나요?",
          "speaker_id": "1",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100041002.1.10",
          "form": "요기요에서 오늘까지입니다~",
          "original_form": "요기요에서 오늘까지입니다~",
          "speaker_id": "2",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100041002.1.11",
          "form": "츄라이 츄라이~",
          "original_form": "츄라이 츄라이~",
          "speaker_id": "2",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100041002.1.12",
          "form": "와 저녁에 사먹어야겠어요",
          "original_form": "와 저녁에 사먹어야겠어요",
          "speaker_id": "1",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100041002.1.13",
          "form": "정보 감사해요!!",
          "original_form": "정보 감사해요!!",
          "speaker_id": "1",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100041002.1.14",
          "form": "전 아점으로 밥에 계란찜먹었는데 ㅠㅋㅋ",
          "original_form": "전 아점으로 밥에 계란찜먹었는데 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:06"
        },
        {
          "id": "MDRW2100041002.1.15",
          "form": "저녁에 피자 시켜야겠어요",
          "original_form": "저녁에 피자 시켜야겠어요",
          "speaker_id": "1",
          "time": "20210905 15:06"
        },
        {
          "id": "MDRW2100041002.1.16",
          "form": "ㅋㅋ계란찜도 훌륭한 점심입니당",
          "original_form": "ㅋㅋ계란찜도 훌륭한 점심입니당",
          "speaker_id": "2",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100041002.1.17",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100041002.1.18",
          "form": "계란찜 어떻게하셨어요?",
          "original_form": "계란찜 어떻게하셨어요?",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100041002.1.19",
          "form": "저는 백종원이 알려준 스타일대로해요",
          "original_form": "저는 백종원이 알려준 스타일대로해요",
          "speaker_id": "2",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041002.1.20",
          "form": "전 계란에 물이랑 다시다넣고 파,고추 넣어서 찜해먹어요!",
          "original_form": "전 계란에 물이랑 다시다넣고 파,고추 넣어서 찜해먹어요!",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100041002.1.21",
          "form": "와 제대로 맛나게하시네요!",
          "original_form": "와 제대로 맛나게하시네요!",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041002.1.22",
          "form": "저는 뜨거운 물과 간장만 겨우ㅋㅋ",
          "original_form": "저는 뜨거운 물과 간장만 겨우ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041002.1.23",
          "form": "심플해서 좋네요 ㅋㅋ",
          "original_form": "심플해서 좋네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100041002.1.24",
          "form": "간을 간장으로 하네요?",
          "original_form": "간을 간장으로 하네요?",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100041002.1.25",
          "form": "네 그냥 소금이 혹시 잘 안섞이면 씹힐까봐요!",
          "original_form": "네 그냥 소금이 혹시 잘 안섞이면 씹힐까봐요!",
          "speaker_id": "2",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100041002.1.26",
          "form": "국간장 조금 넣어요",
          "original_form": "국간장 조금 넣어요",
          "speaker_id": "2",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100041002.1.27",
          "form": "그 조리법이 백종원 스타일인가요?",
          "original_form": "그 조리법이 백종원 스타일인가요?",
          "speaker_id": "1",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100041002.1.28",
          "form": "아뇨 백종원 비법은 뜨거운물 넣기 에요",
          "original_form": "아뇨 백종원 비법은 뜨거운물 넣기 에요",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100041002.1.29",
          "form": "왜냐면 그냥 미적지근한물 넣으면 가운데는 덜익고 가에는 과하게 익는데",
          "original_form": "왜냐면 그냥 미적지근한물 넣으면 가운데는 덜익고 가에는 과하게 익는데",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100041002.1.30",
          "form": "뜨건물 넣으면 이미 조금 익어서 고루 촉촉하게 잘익어요",
          "original_form": "뜨건물 넣으면 이미 조금 익어서 고루 촉촉하게 잘익어요",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100041002.1.31",
          "form": "우와 그래서 였군요!!",
          "original_form": "우와 그래서 였군요!!",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100041002.1.32",
          "form": "백종원 유튜브 좋네요 ㅋㅋ",
          "original_form": "백종원 유튜브 좋네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100041002.1.33",
          "form": "저도 떡볶이 레시피 참고했었는데",
          "original_form": "저도 떡볶이 레시피 참고했었는데",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100041002.1.34",
          "form": "좋더라고요!",
          "original_form": "좋더라고요!",
          "speaker_id": "1",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100041002.1.35",
          "form": "ㅋㅋ네저는 박막례할머니도 좋아해요",
          "original_form": "ㅋㅋ네저는 박막례할머니도 좋아해요",
          "speaker_id": "2",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100041002.1.36",
          "form": "대용량으로 만드시는ㅋㅋ",
          "original_form": "대용량으로 만드시는ㅋㅋ",
