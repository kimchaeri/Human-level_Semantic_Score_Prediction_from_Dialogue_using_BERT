      "utterance": [
        {
          "id": "MDRW2100033546.1.1",
          "form": "name2님은 무슨일하세요?",
          "original_form": "&name2&님은 무슨일하세요?",
          "speaker_id": "1",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100033546.1.2",
          "form": "저 사무직이요. name1님은요",
          "original_form": "저 사무직이요. &name1&님은요",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100033546.1.3",
          "form": "저도 사무직이라해야되나  ㅎ",
          "original_form": "저도 사무직이라해야되나  ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100033546.1.4",
          "form": "회계 행정쪽은 아니고",
          "original_form": "회계 행정쪽은 아니고",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100033546.1.5",
          "form": "네.. 컴으로 사무 보면 사무직이죠 ㅎ",
          "original_form": "네.. 컴으로 사무 보면 사무직이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100033546.1.6",
          "form": "그럼 사무직이요ㅎㄹ",
          "original_form": "그럼 사무직이요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100033546.1.7",
          "form": "전 혼자라 이것저것 해요. 사장님 차 운전도 하고 ㅋ",
          "original_form": "전 혼자라 이것저것 해요. 사장님 차 운전도 하고 ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100033546.1.8",
          "form": "오 운전도요?ㅎ",
          "original_form": "오 운전도요?ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100033546.1.9",
          "form": "전 장롱면허",
          "original_form": "전 장롱면허",
          "speaker_id": "1",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100033546.1.10",
          "form": "술 달 깨셨을 때 가끔이요 ^^",
          "original_form": "술 달 깨셨을 때 가끔이요 ^^",
          "speaker_id": "2",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100033546.1.11",
          "form": "ㅎㅎ 그런것까지 다챙길려면 피곤하실듯",
          "original_form": "ㅎㅎ 그런것까지 다챙길려면 피곤하실듯",
          "speaker_id": "1",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100033546.1.12",
          "form": "아주 가끔이라 괜찮아요.",
          "original_form": "아주 가끔이라 괜찮아요.",
          "speaker_id": "2",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100033546.1.13",
          "form": "혼자겨심 심심하시겠어요",
          "original_form": "혼자겨심 심심하시겠어요",
          "speaker_id": "1",
          "time": "20210824 14:36"
        },
        {
          "id": "MDRW2100033546.1.14",
          "form": "말동무도 없고",
          "original_form": "말동무도 없고",
          "speaker_id": "1",
          "time": "20210824 14:36"
        },
        {
          "id": "MDRW2100033546.1.15",
          "form": "알바 말고 정규직은 여기가 첨 인데\r\n처음부터 혼자 일해서 넘 편해요",
          "original_form": "알바 말고 정규직은 여기가 첨 인데\r\n처음부터 혼자 일해서 넘 편해요",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100033546.1.16",
          "form": "일은 혼자가 편한거같아요",
          "original_form": "일은 혼자가 편한거같아요",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100033546.1.17",
          "form": "상사있음 스트레스 ㅎ 부하직원있어도 관리스트레스",
          "original_form": "상사있음 스트레스 ㅎ 부하직원있어도 관리스트레스",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100033546.1.18",
          "form": "맞아요..조금 심심한게 훨 좋죠",
          "original_form": "맞아요..조금 심심한게 훨 좋죠",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100033546.1.19",
          "form": "12년을 혼자 하니까 다른 직원 들어옴 불편할 거 같아요",
          "original_form": "12년을 혼자 하니까 다른 직원 들어옴 불편할 거 같아요",
          "speaker_id": "2",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100033546.1.20",
          "form": "오 오래하셨네요 한직장에서요?",
          "original_form": "오 오래하셨네요 한직장에서요?",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033546.1.21",
          "form": "네 ㅋ",
          "original_form": "네 ㅋ",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100033546.1.22",
          "form": "오 대단하셔요 ㅎ",
          "original_form": "오 대단하셔요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100033546.1.23",
          "form": "어찌 지내다 보니 세월이 그리됐더라구요..",
          "original_form": "어찌 지내다 보니 세월이 그리됐더라구요..",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100033546.1.24",
          "form": "전 두회사를 왔다갔다 반복했는데",
          "original_form": "전 두회사를 왔다갔다 반복했는데",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100033546.1.25",
          "form": "상사땜에 스트레스 받아서 때려쳤다가",
          "original_form": "상사땜에 스트레스 받아서 때려쳤다가",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100033546.1.26",
          "form": "다시 결국 돌아왔어요",
          "original_form": "다시 결국 돌아왔어요",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100033546.1.27",
          "form": "에고 상사 스트레스 겪어보진 않았지만 힘드셨을 거 같네요.",
          "original_form": "에고 상사 스트레스 겪어보진 않았지만 힘드셨을 거 같네요.",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100033546.1.28",
          "form": "맨날 싸웠어요ㅋ 제가 할말다하는스타일이라",
          "original_form": "맨날 싸웠어요ㅋ 제가 할말다하는스타일이라",
          "speaker_id": "1",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100033546.1.29",
          "form": "할말은 하고 살아야죠. 잘하셨어요. ㅎㅎ",
          "original_form": "할말은 하고 살아야죠. 잘하셨어요. ㅎㅎ",
