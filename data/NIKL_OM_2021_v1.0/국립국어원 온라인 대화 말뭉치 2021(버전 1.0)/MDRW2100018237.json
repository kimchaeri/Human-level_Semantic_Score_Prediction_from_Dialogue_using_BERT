      "utterance": [
        {
          "id": "MDRW2100018237.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018237.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018237.1.3",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018237.1.4",
          "form": "네 고양이요",
          "original_form": "네 고양이요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018237.1.5",
          "form": "3살 됐어요",
          "original_form": "3살 됐어요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018237.1.6",
          "form": "귀여울거 같아요",
          "original_form": "귀여울거 같아요",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018237.1.7",
          "form": "맞아요 벌써 3살인데 생긴거는 그대로에요",
          "original_form": "맞아요 벌써 3살인데 생긴거는 그대로에요",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018237.1.8",
          "form": "저 쓰러집니다 귀여워서",
          "original_form": "저 쓰러집니다 귀여워서",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018237.1.9",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018237.1.10",
          "form": "고양이들 보면 진짜 귀엽더라구요",
          "original_form": "고양이들 보면 진짜 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100018237.1.11",
          "form": "성격두 다 제각각이구",
          "original_form": "성격두 다 제각각이구",
          "speaker_id": "2",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100018237.1.12",
          "form": "맞아요 하하 우리 고양이는 본인이 배고플 때 애교를 부린답니다",
          "original_form": "맞아요 하하 우리 고양이는 본인이 배고플 때 애교를 부린답니다",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018237.1.13",
          "form": "씁쓸하긴 하지만 걍 귀여우니 봐주죠",
          "original_form": "씁쓸하긴 하지만 걍 귀여우니 봐주죠",
          "speaker_id": "1",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018237.1.14",
          "form": "털도 많아서 키우기 힘들것 같아요",
          "original_form": "털도 많아서 키우기 힘들것 같아요",
          "speaker_id": "2",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018237.1.15",
          "form": "맞아요 털 때문에 정말 힘들어요",
          "original_form": "맞아요 털 때문에 정말 힘들어요",
          "speaker_id": "1",
          "time": "20210827 09:05"
        },
        {
          "id": "MDRW2100018237.1.16",
          "form": "가끔 턱 밑을 뒷발로 긇을때면 한숨이 나와요",
          "original_form": "가끔 턱 밑을 뒷발로 긇을때면 한숨이 나와요",
          "speaker_id": "1",
          "time": "20210827 09:05"
        },
        {
          "id": "MDRW2100018237.1.17",
          "form": "털이 한뭉탱이가 털리거든요",
          "original_form": "털이 한뭉탱이가 털리거든요",
          "speaker_id": "1",
          "time": "20210827 09:05"
        },
        {
          "id": "MDRW2100018237.1.18",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "1",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100018237.1.19",
          "form": "그래도 귀여우니까 꼭 안아준답니다",
          "original_form": "그래도 귀여우니까 꼭 안아준답니다",
          "speaker_id": "1",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100018237.1.20",
          "form": "진짜 예뻐하시네요ㅋㅋ",
          "original_form": "진짜 예뻐하시네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:07"
        },
        {
          "id": "MDRW2100018237.1.21",
          "form": "고양이가 행복하겠어요",
          "original_form": "고양이가 행복하겠어요",
          "speaker_id": "2",
          "time": "20210827 09:07"
        },
        {
          "id": "MDRW2100018237.1.22",
          "form": "네 행복했으면 좋겠네요",
          "original_form": "네 행복했으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018237.1.23",
          "form": "고양이는 길게 20년을 살잖아요. 그 짧은 삶이 항상 행복했으면 좋겠더라구요",
          "original_form": "고양이는 길게 20년을 살잖아요. 그 짧은 삶이 항상 행복했으면 좋겠더라구요",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018237.1.24",
          "form": "그래서 잘해주려고 노력하는 것 같아요",
          "original_form": "그래서 잘해주려고 노력하는 것 같아요",
          "speaker_id": "1",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100018237.1.25",
          "form": "좋은 집사시네요ㅎㅎ",
          "original_form": "좋은 집사시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018237.1.26",
          "form": "전 키울 자신은 아직 없어서요",
          "original_form": "전 키울 자신은 아직 없어서요",
          "speaker_id": "2",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100018237.1.27",
          "form": "생각보다 신경쓸게 많다고 하더라구요",
          "original_form": "생각보다 신경쓸게 많다고 하더라구요",
          "speaker_id": "2",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018237.1.28",
          "form": "네 사실 저도 키울 생각이 없었어요. 끝까지 책임져야하고 비용도 많이 들어서요. 그럴만한 경제력은 없거든요",
          "original_form": "네 사실 저도 키울 생각이 없었어요. 끝까지 책임져야하고 비용도 많이 들어서요. 그럴만한 경제력은 없거든요",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018237.1.29",
          "form": "근데 우연찮게 키우게 되면서 정말 더 간절하게 느껴요",
          "original_form": "근데 우연찮게 키우게 되면서 정말 더 간절하게 느껴요",
          "speaker_id": "1",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018237.1.30",
          "form": "생명을 키우는거는 정말 큰 책임감이 들고, 함부로 키우면 안된다는 걸요",
          "original_form": "생명을 키우는거는 정말 큰 책임감이 들고, 함부로 키우면 안된다는 걸요",
          "speaker_id": "1",
          "time": "20210827 09:14"
        },
        {
          "id": "MDRW2100018237.1.31",
          "form": "저도 이 아이를 키우고 나면, 나중에 늙어서 여유가 생길 때 키우거나 아예 안키울 것 같아요",
          "original_form": "저도 이 아이를 키우고 나면, 나중에 늙어서 여유가 생길 때 키우거나 아예 안키울 것 같아요",
          "speaker_id": "1",
          "time": "20210827 09:14"
        },
        {
          "id": "MDRW2100018237.1.32",
          "form": "다들 같은 생각이면 좋겠어요. 관련 사건 볼때마다 한숨이 다 나와요",
          "original_form": "다들 같은 생각이면 좋겠어요. 관련 사건 볼때마다 한숨이 다 나와요",
          "speaker_id": "2",
          "time": "20210827 09:18"
        },
        {
          "id": "MDRW2100018237.1.33",
          "form": "아 맞아요 저는 잘 못보겠더라구요 다치는건",
          "original_form": "아 맞아요 저는 잘 못보겠더라구요 다치는건",
          "speaker_id": "1",
          "time": "20210827 09:20"
        },
        {
          "id": "MDRW2100018237.1.34",
          "form": "동물관련법이 더 강화 됐으면 좋겠어요",
          "original_form": "동물관련법이 더 강화 됐으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 09:20"
        }
      ]
    }
