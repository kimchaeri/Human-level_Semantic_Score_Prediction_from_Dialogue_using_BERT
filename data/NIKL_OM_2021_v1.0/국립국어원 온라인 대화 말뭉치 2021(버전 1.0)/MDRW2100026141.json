      "utterance": [
        {
          "id": "MDRW2100026141.1.1",
          "form": "안녕하세요 날이 추워졌네요",
          "original_form": "안녕하세요 날이 추워졌네요",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026141.1.2",
          "form": "안녕하세요^^ 비오니까 더 추워진 것 같아요~",
          "original_form": "안녕하세요^^ 비오니까 더 추워진 것 같아요~",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026141.1.3",
          "form": "네 ㅜㅜ 이제 이불 두꺼운거 꺼내야겟어요",
          "original_form": "네 ㅜㅜ 이제 이불 두꺼운거 꺼내야겟어요",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026141.1.4",
          "form": "그쵸 이불도 오늘 새로 샀고 저는 오늘 추워서 걸치는 옷도 꺼내놨어요",
          "original_form": "그쵸 이불도 오늘 새로 샀고 저는 오늘 추워서 걸치는 옷도 꺼내놨어요",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026141.1.5",
          "form": "아 진짜 잘하셨어요 안하면 감기걸릴거같아요",
          "original_form": "아 진짜 잘하셨어요 안하면 감기걸릴거같아요",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026141.1.6",
          "form": "어제는 뉴스 특보도 떴더라구요. 비 많이 온다구ㅠㅠ",
          "original_form": "어제는 뉴스 특보도 떴더라구요. 비 많이 온다구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026141.1.7",
          "form": "내일도 또 비 올 것 같은데 걱정이에요.",
          "original_form": "내일도 또 비 올 것 같은데 걱정이에요.",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026141.1.8",
          "form": "헐 비가 오나요",
          "original_form": "헐 비가 오나요",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100026141.1.9",
          "form": "여긴 아직 안오긴 하는데 흐리네요",
          "original_form": "여긴 아직 안오긴 하는데 흐리네요",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100026141.1.10",
          "form": "아 지금은 비 안와요!",
          "original_form": "아 지금은 비 안와요!",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100026141.1.11",
          "form": "오히려 쨍쨍해졌어요",
          "original_form": "오히려 쨍쨍해졌어요",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100026141.1.12",
          "form": "헐 다시 더워졋군요",
          "original_form": "헐 다시 더워졋군요",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100026141.1.13",
          "form": "네ㅠㅠ 그래도 햇볕나니까 상쾌해요~",
          "original_form": "네ㅠㅠ 그래도 햇볕나니까 상쾌해요~",
