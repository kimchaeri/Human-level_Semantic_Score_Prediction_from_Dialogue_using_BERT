      "utterance": [
        {
          "id": "MDRW2100028354.1.1",
          "form": "나도 레시피는 모르는데",
          "original_form": "나도 레시피는 모르는데",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028354.1.2",
          "form": "언니 집 놀러갔을 때 해줬어",
          "original_form": "언니 집 놀러갔을 때 해줬어",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028354.1.3",
          "form": "간장이랑 버터랑 버섯넣고 휘끼휘끼 해서 줬어",
          "original_form": "간장이랑 버터랑 버섯넣고 휘끼휘끼 해서 줬어",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028354.1.4",
          "form": "휘끼휘끼",
          "original_form": "휘끼휘끼",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028354.1.5",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028354.1.6",
          "form": "간장도 버터도좋아",
          "original_form": "간장도 버터도좋아",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028354.1.7",
          "form": "후추 좀 넣어도 괜찮을조합",
          "original_form": "후추 좀 넣어도 괜찮을조합",
          "speaker_id": "1",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100028354.1.8",
          "form": "후추 들어갔던거같기도 하고 기억이 안난다",
          "original_form": "후추 들어갔던거같기도 하고 기억이 안난다",
          "speaker_id": "2",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100028354.1.9",
          "form": "유튜브 검색하면 나올거같기도",
          "original_form": "유튜브 검색하면 나올거같기도",
          "speaker_id": "2",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100028354.1.10",
          "form": "뭐든 다 있을거같긴함",
          "original_form": "뭐든 다 있을거같긴함",
          "speaker_id": "1",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100028354.1.11",
          "form": "바리에이션도 많을듯~~",
          "original_form": "바리에이션도 많을듯~~",
          "speaker_id": "1",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100028354.1.12",
          "form": "간단하게 마늘연두 파스타도ㅜ좋은데",
          "original_form": "간단하게 마늘연두 파스타도ㅜ좋은데",
          "speaker_id": "1",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100028354.1.13",
          "form": "헐 맛있겠다",
          "original_form": "헐 맛있겠다",
          "speaker_id": "2",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100028354.1.14",
          "form": "완전 코리안 파스타네",
          "original_form": "완전 코리안 파스타네",
          "speaker_id": "2",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100028354.1.15",
          "form": "우리집엔 다진마늘이나 통마늘이 상시구비되어있지않구",
          "original_form": "우리집엔 다진마늘이나 통마늘이 상시구비되어있지않구",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028354.1.16",
          "form": "마늘파우더 양파파우더 쓰는데",
          "original_form": "마늘파우더 양파파우더 쓰는데",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028354.1.17",
          "form": "파우더는 신기하다 우린 파우더는 안써가지고",
          "original_form": "파우더는 신기하다 우린 파우더는 안써가지고",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028354.1.18",
          "form": "팬에 기름 잔뜩 두르고",
          "original_form": "팬에 기름 잔뜩 두르고",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028354.1.19",
          "form": "마늘파우더 탈탈해서 색 좀 나올때까지 볶다가 (너무 가열하면 탐",
          "original_form": "마늘파우더 탈탈해서 색 좀 나올때까지 볶다가 (너무 가열하면 탐",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028354.1.20",
          "form": "연두 한숟갈정도 휘리릭",
          "original_form": "연두 한숟갈정도 휘리릭",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028354.1.21",
          "form": "파스타 비비면 끝",
          "original_form": "파스타 비비면 끝",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028354.1.22",
          "form": "파우더 편해 조아",
          "original_form": "파우더 편해 조아",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028354.1.23",
          "form": "우리는 다진마늘이나 통마늘도 많이 있다보니까",
          "original_form": "우리는 다진마늘이나 통마늘도 많이 있다보니까",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028354.1.24",
          "form": "따로 파우더는 안사는거같애",
          "original_form": "따로 파우더는 안사는거같애",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028354.1.25",
          "form": "주기적으로 어디선가 농사해서 와",
          "original_form": "주기적으로 어디선가 농사해서 와",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028354.1.26",
          "form": "누군가가",
          "original_form": "누군가가",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028354.1.27",
          "form": "얽ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "얽ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028354.1.28",
          "form": "그럼 필요없을만 하겠다",
          "original_form": "그럼 필요없을만 하겠다",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028354.1.29",
          "form": "암ㅍ리그래도 갓 다진 마늘은 못이기지",
          "original_form": "암ㅍ리그래도 갓 다진 마늘은 못이기지",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028354.1.30",
          "form": "대신 누군가의 노동이 들어가는 일들이지",
          "original_form": "대신 누군가의 노동이 들어가는 일들이지",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028354.1.31",
          "form": "나는 내가 했던 최고의 노동은",
          "original_form": "나는 내가 했던 최고의 노동은",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028354.1.32",
          "form": "옥수수 알 분리하기..",
          "original_form": "옥수수 알 분리하기..",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028354.1.33",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028354.1.34",
          "form": "그거 레시피영상같은거보면 그냥 칼로 슥삭삭 하던데 ㅋㅋ큐ㅠㅜ",
          "original_form": "그거 레시피영상같은거보면 그냥 칼로 슥삭삭 하던데 ㅋㅋ큐ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028354.1.35",
          "form": "나도 다음에 칼로 할까봐...",
          "original_form": "나도 다음에 칼로 할까봐...",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028354.1.36",
          "form": "젓가락으로 꾹눌러도 잘 나오는데",
          "original_form": "젓가락으로 꾹눌러도 잘 나오는데",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028354.1.37",
          "form": "손바닥 멍들어서 2일간 아프더라",
          "original_form": "손바닥 멍들어서 2일간 아프더라",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028354.1.38",
          "form": "칼이 그건가?",
          "original_form": "칼이 그건가?",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028354.1.39",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028354.1.40",
          "form": "젓가락 같은게 더힘들지않아?? 힘받는 지점이 한군데라서",
          "original_form": "젓가락 같은게 더힘들지않아?? 힘받는 지점이 한군데라서",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028354.1.41",
          "form": "멍들만도",
          "original_form": "멍들만도",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028354.1.42",
