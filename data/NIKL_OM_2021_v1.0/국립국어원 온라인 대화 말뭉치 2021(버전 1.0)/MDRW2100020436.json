      "utterance": [
        {
          "id": "MDRW2100020436.1.1",
          "form": "남자친구 있으세용?",
          "original_form": "남자친구 있으세용?",
          "speaker_id": "1",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.2",
          "form": "넹 사실 얼마전에 생겨서",
          "original_form": "넹 사실 얼마전에 생겨서",
          "speaker_id": "2",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.3",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "1",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.4",
          "form": "오늘이 딱 일주일째",
          "original_form": "오늘이 딱 일주일째",
          "speaker_id": "2",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.5",
          "form": "어디서 만나셨어요?",
          "original_form": "어디서 만나셨어요?",
          "speaker_id": "1",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.7",
          "form": "저는 없어요ㅠㅠ",
          "original_form": "저는 없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.8",
          "form": "소개팅으로 만났어요",
          "original_form": "소개팅으로 만났어요",
          "speaker_id": "2",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.9",
          "form": "너무 좋으시겠다",
          "original_form": "너무 좋으시겠다",
          "speaker_id": "1",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.10",
          "form": "소개팅 설레요ㅋㅋ",
          "original_form": "소개팅 설레요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:37"
        },
        {
          "id": "MDRW2100020436.1.11",
          "form": "한 1년반의 공백이 잇다가 ㅋㅋㅋ",
          "original_form": "한 1년반의 공백이 잇다가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:38"
        },
        {
          "id": "MDRW2100020436.1.12",
          "form": "소개팅 엄청 했는데",
          "original_form": "소개팅 엄청 했는데",
          "speaker_id": "2",
          "time": "20210828 05:38"
        },
        {
          "id": "MDRW2100020436.1.13",
          "form": "와 소개팅으로 잘되기가 의외로 쉽지 않은데",
          "original_form": "와 소개팅으로 잘되기가 의외로 쉽지 않은데",
          "speaker_id": "1",
          "time": "20210828 05:38"
        },
        {
          "id": "MDRW2100020436.1.14",
          "form": "정말 인연인가봐요",
          "original_form": "정말 인연인가봐요",
          "speaker_id": "1",
          "time": "20210828 05:38"
        },
        {
          "id": "MDRW2100020436.1.15",
          "form": "겨우 맘에 드는 사람이 나타났어요",
          "original_form": "겨우 맘에 드는 사람이 나타났어요",
          "speaker_id": "2",
          "time": "20210828 05:38"
        },
        {
          "id": "MDRW2100020436.1.16",
          "form": "대박...",
          "original_form": "대박...",
          "speaker_id": "1",
          "time": "20210828 05:38"
        },
        {
          "id": "MDRW2100020436.1.17",
          "form": "네 좀 현타와서 소개팅 그만할까 했던",
          "original_form": "네 좀 현타와서 소개팅 그만할까 했던",
          "speaker_id": "2",
          "time": "20210828 05:38"
        },
        {
          "id": "MDRW2100020436.1.18",
          "form": "매일매일 눈뜨는게 행복할것같아요",
          "original_form": "매일매일 눈뜨는게 행복할것같아요",
          "speaker_id": "1",
          "time": "20210828 05:38"
        },
        {
          "id": "MDRW2100020436.1.19",
          "form": "ㅋㅋㅋㅋ아직은 어색해요",
          "original_form": "ㅋㅋㅋㅋ아직은 어색해요",
          "speaker_id": "2",
          "time": "20210828 05:38"
        }
