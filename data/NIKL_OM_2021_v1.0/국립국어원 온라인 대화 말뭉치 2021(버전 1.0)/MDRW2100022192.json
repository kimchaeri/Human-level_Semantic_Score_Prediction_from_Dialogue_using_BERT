      "utterance": [
        {
          "id": "MDRW2100022192.1.1",
          "form": "보통? 정도 인 것 같아요",
          "original_form": "보통? 정도 인 것 같아요",
          "speaker_id": "2",
          "time": "20210829 11:03"
        },
        {
          "id": "MDRW2100022192.1.2",
          "form": "그쵸 안쓰는 아이는 또 안쓴다고 그래서..",
          "original_form": "그쵸 안쓰는 아이는 또 안쓴다고 그래서..",
          "speaker_id": "2",
          "time": "20210829 11:03"
        },
        {
          "id": "MDRW2100022192.1.3",
          "form": "흠 더 고민해 봐야겠네요",
          "original_form": "흠 더 고민해 봐야겠네요",
          "speaker_id": "2",
          "time": "20210829 11:03"
        },
        {
          "id": "MDRW2100022192.1.4",
          "form": "근데 확실히 캣휠 매력적이에요",
          "original_form": "근데 확실히 캣휠 매력적이에요",
          "speaker_id": "1",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100022192.1.5",
          "form": "활동량 늘려서 운동시킬 수 있는 몇 언되는 제품",
          "original_form": "활동량 늘려서 운동시킬 수 있는 몇 언되는 제품",
          "speaker_id": "1",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100022192.1.6",
          "form": "캣휠 무료이용권같은거 있으면 좋겠어요",
          "original_form": "캣휠 무료이용권같은거 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022192.1.7",
          "form": "ㅜㅜ.. 그러니까요",
          "original_form": "ㅜㅜ.. 그러니까요",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022192.1.8",
          "form": "잘 타는지 안타는지 확인해보고 사고싶어서",
          "original_form": "잘 타는지 안타는지 확인해보고 사고싶어서",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022192.1.9",
          "form": "가격 만만치가 않잖아요ㅠ",
          "original_form": "가격 만만치가 않잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022192.1.10",
          "form": "저는 무료이용권 있다는 줄 알고 냅다 들어왔네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 무료이용권 있다는 줄 알고 냅다 들어왔네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022192.1.11",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022192.1.12",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022192.1.13",
          "form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022192.1.14",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100022192.1.15",
          "form": "예전엔 소음있다고 그랬는데",
          "original_form": "예전엔 소음있다고 그랬는데",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022192.1.16",
          "form": "요즘은 무소음 제품도 잘 나왔더고 하더라구요",
          "original_form": "요즘은 무소음 제품도 잘 나왔더고 하더라구요",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100022192.1.17",
          "form": "그러니까요!! 찾아보니까 엄청 많더라고요",
          "original_form": "그러니까요!! 찾아보니까 엄청 많더라고요",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100022192.1.18",
          "form": "뭘 사야할지 고민돼요ㅋㅋㅋㅋㅋ",
          "original_form": "뭘 사야할지 고민돼요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022192.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022192.1.20",
          "form": "진짜 무료 이용권 있었으면 좋겜ㅅ어요",
          "original_form": "진짜 무료 이용권 있었으면 좋겜ㅅ어요",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022192.1.21",
          "form": "그러니까요...ㅜ",
          "original_form": "그러니까요...ㅜ",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022192.1.22",
          "form": "혹시 사랑빗 쓰시나요?",
          "original_form": "혹시 사랑빗 쓰시나요?",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022192.1.23",
          "form": "저가 여러빗을 실패하고 사봤는데 이건 진짜 물건이더라고요",
          "original_form": "저가 여러빗을 실패하고 사봤는데 이건 진짜 물건이더라고요",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022192.1.24",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "1",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100022192.1.25",
          "form": "한번 찾아봐야겠어요",
          "original_form": "한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100022192.1.26",
          "form": "빗 빗는걸 싫어해서",
          "original_form": "빗 빗는걸 싫어해서",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022192.1.27",
          "form": "고민이었는데",
          "original_form": "고민이었는데",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022192.1.28",
          "form": "네 추천합니다!!",
          "original_form": "네 추천합니다!!",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022192.1.29",
          "form": "우리 고양이는 왜 이불 속을 좋아할까요 ㅋㅋㅋㅋ",
          "original_form": "우리 고양이는 왜 이불 속을 좋아할까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022192.1.30",
          "form": "계속 꿈질거리네요",
          "original_form": "계속 꿈질거리네요",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022192.1.31",
          "form": "저희고영이는",
          "original_form": "저희고영이는",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022192.1.32",
          "form": "이불 속 안좋아하는데",
          "original_form": "이불 속 안좋아하는데",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022192.1.33",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022192.1.34",
          "form": "근데 유튜브 보면 다른고양이들은 잘 들어가드라구요",
          "original_form": "근데 유튜브 보면 다른고양이들은 잘 들어가드라구요",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022192.1.35",
          "form": "그 안에서 꼼지락 거리는 고양이 너무 귀여울것같아요ㅜ",
          "original_form": "그 안에서 꼼지락 거리는 고양이 너무 귀여울것같아요ㅜ",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022192.1.36",
          "form": "ㅋㅋㅋㅋ맞아요 그루밍하느라 꼼지락 거리는데",
          "original_form": "ㅋㅋㅋㅋ맞아요 그루밍하느라 꼼지락 거리는데",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022192.1.37",
          "form": "확그냥 안아주고싶어요 헤헤",
          "original_form": "확그냥 안아주고싶어요 헤헤",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022192.1.38",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022192.1.39",
