      "utterance": [
        {
          "id": "MDRW2100033549.1.1",
          "form": "거리두기는 ㅇ아예 안돼요",
          "original_form": "거리두기는 ㅇ아예 안돼요",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100033549.1.2",
          "form": "아저씨들은 더 쫌 안지키시고요",
          "original_form": "아저씨들은 더 쫌 안지키시고요",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100033549.1.3",
          "form": "그래서 요새 잘 못가겠어요",
          "original_form": "그래서 요새 잘 못가겠어요",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033549.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033549.1.5",
          "form": "주말엔 남편하고 호수앞 스벅 가긴했어요",
          "original_form": "주말엔 남편하고 호수앞 스벅 가긴했어요",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033549.1.6",
          "form": "청도 거기간게 마지막이요",
          "original_form": "청도 거기간게 마지막이요",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033549.1.7",
          "form": "오랜만에 갔네요",
          "original_form": "오랜만에 갔네요",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033549.1.8",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033549.1.9",
          "form": "주변환경이 참좋으시네오ㅎ",
          "original_form": "주변환경이 참좋으시네오ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033549.1.10",
          "form": "드라이브스루는 하시죠?",
          "original_form": "드라이브스루는 하시죠?",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033549.1.11",
          "form": "신랑이랑 둘이 어디간게 언젠지 ㅋㅣㅣ",
          "original_form": "신랑이랑 둘이 어디간게 언젠지 ㅋㅣㅣ",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100033549.1.12",
          "form": "네 지방의 장점이죠",
          "original_form": "네 지방의 장점이죠",
          "speaker_id": "2",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100033549.1.13",
          "form": "저도 몇달만요",
          "original_form": "저도 몇달만요",
          "speaker_id": "2",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100033549.1.14",
          "form": "애들이 같이 안간다고 해서 샌드위치 머핀 먹고 왔네요",
          "original_form": "애들이 같이 안간다고 해서 샌드위치 머핀 먹고 왔네요",
          "speaker_id": "2",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100033549.1.15",
          "form": "머핀 맛집이예요 ㅎㅎ",
          "original_form": "머핀 맛집이예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100033549.1.16",
          "form": "머핀 먹고싶네요",
          "original_form": "머핀 먹고싶네요",
          "speaker_id": "1",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100033549.1.17",
          "form": "좀따 애 학원보내고 저는 카페가서 머핀 먹고 와야겠어요 ㅎ",
          "original_form": "좀따 애 학원보내고 저는 카페가서 머핀 먹고 와야겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100033549.1.18",
          "form": "쿠키머핀인데 블루베리 쨈이 가득",
          "original_form": "쿠키머핀인데 블루베리 쨈이 가득",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100033549.1.19",
          "form": "와 진짜 맛나요",
          "original_form": "와 진짜 맛나요",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100033549.1.20",
          "form": "네 그러세요",
          "original_form": "네 그러세요",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100033549.1.21",
          "form": "스콘도 정말 맛있어요",
          "original_form": "스콘도 정말 맛있어요",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100033549.1.22",
          "form": "한때 빠졌었어요",
          "original_form": "한때 빠졌었어요",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100033549.1.23",
          "form": "스콘은 오리지널?",
          "original_form": "스콘은 오리지널?",
          "speaker_id": "1",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100033549.1.24",
          "form": "따뜻하게 데워줄때 바로 먹어야",
          "original_form": "따뜻하게 데워줄때 바로 먹어야",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100033549.1.25",
          "form": "진짜더라구요",
          "original_form": "진짜더라구요",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100033549.1.26",
          "form": "그렇죠 ㅎㅎ데워줘서 맛난 거였네요",
          "original_form": "그렇죠 ㅎㅎ데워줘서 맛난 거였네요",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100033549.1.27",
          "form": "갓구운거 먹고싶네요",
          "original_form": "갓구운거 먹고싶네요",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100033549.1.28",
          "form": "지난번에 모르고 데워서 픽업해와서",
          "original_form": "지난번에 모르고 데워서 픽업해와서",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100033549.1.29",
          "form": "집에서 앙 베는순간 앗!",
          "original_form": "집에서 앙 베는순간 앗!",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100033549.1.30",
          "form": "틀리구나 싶더라구요",
          "original_form": "틀리구나 싶더라구요",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100033549.1.31",
          "form": "틀리죠 그맛이 정말 틀려요",
          "original_form": "틀리죠 그맛이 정말 틀려요",
          "speaker_id": "2",
          "time": "20210824 13:30"
        }
      ]
    }
  ]
