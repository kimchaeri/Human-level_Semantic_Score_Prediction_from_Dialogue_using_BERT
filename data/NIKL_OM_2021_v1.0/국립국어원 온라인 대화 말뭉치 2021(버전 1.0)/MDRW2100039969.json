      "utterance": [
        {
          "id": "MDRW2100039969.1.1",
          "form": "오!  그거 보셨군요",
          "original_form": "오!  그거 보셨군요",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039969.1.2",
          "form": "저는...보다 말았던",
          "original_form": "저는...보다 말았던",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039969.1.3",
          "form": "편집분만 봤네요ㅋㅋㅋㅋ",
          "original_form": "편집분만 봤네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039969.1.4",
          "form": "보다가 아... 뭔가 제 스타일이 아니었던 드라마",
          "original_form": "보다가 아... 뭔가 제 스타일이 아니었던 드라마",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039969.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ 학원물 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 학원물 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039969.1.6",
          "form": "전 재밌게봤어요 ㅋㅋㅋ",
          "original_form": "전 재밌게봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039969.1.7",
          "form": "좋아하는배우나와서 ㅋㅋㅋ",
          "original_form": "좋아하는배우나와서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039969.1.8",
          "form": "근데 저 지금 보니까 문빈님 특별출연한 웹드도봤네요... 노빠꾸 로맨스...",
          "original_form": "근데 저 지금 보니까 문빈님 특별출연한 웹드도봤네요... 노빠꾸 로맨스...",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039969.1.9",
          "form": "악ㅋㅋㅋㅋ그것도 보셨나요",
          "original_form": "악ㅋㅋㅋㅋ그것도 보셨나요",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039969.1.10",
          "form": "인어왕자ㅋㅋㅋ",
          "original_form": "인어왕자ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039969.1.11",
          "form": "저 그거 안봤어요",
          "original_form": "저 그거 안봤어요",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039969.1.12",
          "form": "키스신만 본 아이러니",
          "original_form": "키스신만 본 아이러니",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039969.1.13",
          "form": "ㅌㅋㅋㅌㅋㅋㅋ사실 제 돌이 흔히 말하는 망돌인데..",
          "original_form": "ㅌㅋㅋㅌㅋㅋㅋ사실 제 돌이 흔히 말하는 망돌인데..",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039969.1.14",
          "form": "거기나와서......",
          "original_form": "거기나와서......",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039969.1.15",
          "form": "슬픈이야기네요... 정말 드라마도 노빠꾸더라구요..",
          "original_form": "슬픈이야기네요... 정말 드라마도 노빠꾸더라구요..",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039969.1.16",
          "form": "누군데요????",
          "original_form": "누군데요????",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039969.1.17",
          "form": "윤산이라고...",
          "original_form": "윤산이라고...",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039969.1.18",
          "form": "오 찾아보고왔어여",
          "original_form": "오 찾아보고왔어여",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039969.1.19",
          "form": "핫샷",
          "original_form": "핫샷",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039969.1.20",
          "form": "네... 해체했어요...",
          "original_form": "네... 해체했어요...",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039969.1.21",
          "form": "언제해체해도 이상하지 않을 그룹이었죠...",
          "original_form": "언제해체해도 이상하지 않을 그룹이었죠...",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039969.1.22",
          "form": "아하ㅠ",
          "original_form": "아하ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039969.1.23",
          "form": "2월에 군대가셨네여",
          "original_form": "2월에 군대가셨네여",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039969.1.24",
          "form": "아직도 좋아하시나여?",
          "original_form": "아직도 좋아하시나여?",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039969.1.25",
          "form": "맞아요...군대에서 잘하고 있는지....",
          "original_form": "맞아요...군대에서 잘하고 있는지....",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039969.1.26",
          "form": "네 ㅜ",
          "original_form": "네 ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039969.1.27",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039969.1.28",
          "form": "그전에 망돌? 이라면 망돌인 그룹 좋아했었는데",
          "original_form": "그전에 망돌? 이라면 망돌인 그룹 좋아했었는데",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039969.1.29",
          "form": "지금 보면 그르트라고용",
          "original_form": "지금 보면 그르트라고용",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039969.1.30",
          "form": "어떤그룹인가요 ㅋㅋㅋ",
          "original_form": "어떤그룹인가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039969.1.31",
          "form": "B.A.P",
          "original_form": "B.A.P",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039969.1.32",
          "form": "아실까요",
          "original_form": "아실까요",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039969.1.33",
          "form": "알죠알죠 ㅋㅋㅋㅋ",
          "original_form": "알죠알죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039969.1.34",
          "form": "망돌아니죠이정도면 ㅋㅋㅋㅋ",
          "original_form": "망돌아니죠이정도면 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:55"
        }
