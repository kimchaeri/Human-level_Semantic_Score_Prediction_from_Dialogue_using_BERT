      "utterance": [
        {
          "id": "MDRW2100007680.1.1",
          "form": "안녕하세요 겨리 님!",
          "original_form": "안녕하세요 겨리 님!",
          "speaker_id": "1",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007680.1.2",
          "form": "최근에 여행 다녀오셨나요?",
          "original_form": "최근에 여행 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007680.1.3",
          "form": "최근에는 못갔어요ㅠㅠ",
          "original_form": "최근에는 못갔어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007680.1.4",
          "form": "19년이 마지막이었던 거 같아요",
          "original_form": "19년이 마지막이었던 거 같아요",
          "speaker_id": "2",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007680.1.5",
          "form": "어디 다녀오신 적 있으세요?",
          "original_form": "어디 다녀오신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007680.1.6",
          "form": "헉 꽤 오래됐네요",
          "original_form": "헉 꽤 오래됐네요",
          "speaker_id": "1",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007680.1.7",
          "form": "저도 최근에는 여행 못 갔어요ㅜㅜ",
          "original_form": "저도 최근에는 여행 못 갔어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007680.1.8",
          "form": "코로나 때문에..",
          "original_form": "코로나 때문에..",
          "speaker_id": "1",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007680.1.9",
          "form": "그쵸 요새는 좀ㅜㅜ",
          "original_form": "그쵸 요새는 좀ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007680.1.10",
          "form": "여행이 낙이었는데 마니 심심심해요",
          "original_form": "여행이 낙이었는데 마니 심심심해요",
          "speaker_id": "2",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007680.1.11",
          "form": "여행 간 곳 중에 어디가 젤 좋으셨어요?",
          "original_form": "여행 간 곳 중에 어디가 젤 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007680.1.12",
          "form": "정말 어릴 때 프랑스에 가봤어요",
          "original_form": "정말 어릴 때 프랑스에 가봤어요",
          "speaker_id": "1",
          "time": "20210822 09:40"
        },
        {
          "id": "MDRW2100007680.1.13",
          "form": "처음이자 마지막 해외여행이었는데 아직도 생생해요",
          "original_form": "처음이자 마지막 해외여행이었는데 아직도 생생해요",
          "speaker_id": "1",
          "time": "20210822 09:40"
        },
        {
          "id": "MDRW2100007680.1.14",
          "form": "오 프랑스 한 나라만 다녀오신 거에요?",
          "original_form": "오 프랑스 한 나라만 다녀오신 거에요?",
          "speaker_id": "2",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007680.1.15",
          "form": "넵 딱 한 번 다녀왔어요",
          "original_form": "넵 딱 한 번 다녀왔어요",
          "speaker_id": "1",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007680.1.16",
          "form": "혹시 프랑스 가보셨어요?",
          "original_form": "혹시 프랑스 가보셨어요?",
          "speaker_id": "1",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007680.1.17",
          "form": "싱기하다 보통 유럽은 묶어서 다녀오지 않아요?",
          "original_form": "싱기하다 보통 유럽은 묶어서 다녀오지 않아요?",
          "speaker_id": "2",
          "time": "20210822 09:43"
        },
        {
          "id": "MDRW2100007680.1.18",
          "form": "프랑스는 안가봤어요",
          "original_form": "프랑스는 안가봤어요",
          "speaker_id": "2",
          "time": "20210822 09:43"
        },
        {
          "id": "MDRW2100007680.1.19",
          "form": "유럽쪽은 로마랑 아테네만",
          "original_form": "유럽쪽은 로마랑 아테네만",
          "speaker_id": "2",
          "time": "20210822 09:43"
        },
        {
          "id": "MDRW2100007680.1.20",
          "form": "패키지 없이 자유여행으로 다녀왔어요ㅎㅎ",
          "original_form": "패키지 없이 자유여행으로 다녀왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100007680.1.21",
          "form": "로마랑 아테네는 어땠어요?? 너무 가보고 싶어요",
          "original_form": "로마랑 아테네는 어땠어요?? 너무 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100007680.1.22",
          "form": "로마는 마니 들으셨겠지만 더러워요ㅋㅋㅋ 인종차별 심하고",
          "original_form": "로마는 마니 들으셨겠지만 더러워요ㅋㅋㅋ 인종차별 심하고",
          "speaker_id": "2",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007680.1.23",
          "form": "볼 건 엄청 많아요!",
          "original_form": "볼 건 엄청 많아요!",
          "speaker_id": "2",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007680.1.24",
          "form": "전 아테네 넘 좋았어요. 볼 건 로마보다 덜 할 순 있는데 더 깨끗하고 조용",
          "original_form": "전 아테네 넘 좋았어요. 볼 건 로마보다 덜 할 순 있는데 더 깨끗하고 조용",
          "speaker_id": "2",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007680.1.25",
          "form": "아테네!! 코로나 끝나면 꼭 한 번 가보고 싶어요",
          "original_form": "아테네!! 코로나 끝나면 꼭 한 번 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007680.1.26",
          "form": "다른 덴 가보고 싶은 데 있으세요?",
          "original_form": "다른 덴 가보고 싶은 데 있으세요?",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007680.1.27",
          "form": "요샌 독일이랑 중국서안이랑 이집트요",
          "original_form": "요샌 독일이랑 중국서안이랑 이집트요",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007680.1.28",
          "form": "이 시국 끝나면 좋겠어요 빨리ㅋㅋ",
          "original_form": "이 시국 끝나면 좋겠어요 빨리ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007680.1.29",
          "form": "그쵸ㅋㅋㅜㅜ 너무 오래 가네요",
          "original_form": "그쵸ㅋㅋㅜㅜ 너무 오래 가네요",
          "speaker_id": "1",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007680.1.30",
          "form": "사실 저는 작년에 대학 입시가 끝나서",
          "original_form": "사실 저는 작년에 대학 입시가 끝나서",
          "speaker_id": "1",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007680.1.31",
          "form": "끝나고 여행가고 싶었는데",
          "original_form": "끝나고 여행가고 싶었는데",
          "speaker_id": "1",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007680.1.32",
          "form": "오왕 그럼 앞으로 갈 기회가 많겠어요",
          "original_form": "오왕 그럼 앞으로 갈 기회가 많겠어요",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007680.1.33",
          "form": "관심 있으시면 진짜 대학생때 마니 다니세요",
          "original_form": "관심 있으시면 진짜 대학생때 마니 다니세요",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007680.1.34",
          "form": "지금은 좀 아숩지만",
          "original_form": "지금은 좀 아숩지만",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007680.1.35",
          "form": "마음만 같아선 그러고 싶어요ㅜㅜ",
          "original_form": "마음만 같아선 그러고 싶어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 11:47"
        }
      ]
    }
