      "utterance": [
        {
          "id": "MDRW2100003795.1.1",
          "form": "기분도 약간 다운되네",
          "original_form": "기분도 약간 다운되네",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003795.1.2",
          "form": "난 멍해 ㅎㅎ",
          "original_form": "난 멍해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003795.1.3",
          "form": "비 보면서 멍때려야 하나 ㅋㅋ",
          "original_form": "비 보면서 멍때려야 하나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003795.1.4",
          "form": "아무 것도 하기 싫다",
          "original_form": "아무 것도 하기 싫다",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003795.1.5",
          "form": "아무 것도 하지마. 비오는 날 부침개나 부쳐먹고 푹 쉬자 ㅋㅋ",
          "original_form": "아무 것도 하지마. 비오는 날 부침개나 부쳐먹고 푹 쉬자 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003795.1.6",
          "form": "ㅎㅎ 기름 자글자글 좋~지",
          "original_form": "ㅎㅎ 기름 자글자글 좋~지",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003795.1.7",
          "form": "오늘은 운동도 못하겠네",
          "original_form": "오늘은 운동도 못하겠네",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003795.1.8",
          "form": "실내에서 해도 되긴 하는데 습해서 땀이 많이 나니깐",
          "original_form": "실내에서 해도 되긴 하는데 습해서 땀이 많이 나니깐",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003795.1.9",
          "form": "ㅎㅎ 그럼 진짜 하기 싫지~끈적이고",
          "original_form": "ㅎㅎ 그럼 진짜 하기 싫지~끈적이고",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003795.1.10",
          "form": "ㅇㅇ 비오는 날은 그냥 쉬는 걸로 ㅎㅎ",
          "original_form": "ㅇㅇ 비오는 날은 그냥 쉬는 걸로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003795.1.11",
          "form": "비가 너무 와서 배달도 힘들겠다 ㅠ",
          "original_form": "비가 너무 와서 배달도 힘들겠다 ㅠ",
          "speaker_id": "2",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003795.1.12",
          "form": "왜 뭐 먹고 싶어?",
          "original_form": "왜 뭐 먹고 싶어?",
          "speaker_id": "1",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003795.1.13",
          "form": "애들 방학 끝나서 맛있는 거 사주고 싶은데 시키기 미안한 날씨야 ㅠㅠ",
          "original_form": "애들 방학 끝나서 맛있는 거 사주고 싶은데 시키기 미안한 날씨야 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003795.1.14",
          "form": "그래도 팔아줘야 배달원들도 먹고 살지 ㅠ",
          "original_form": "그래도 팔아줘야 배달원들도 먹고 살지 ㅠ",
          "speaker_id": "1",
          "time": "20210821 12:16"
        }
      ]
