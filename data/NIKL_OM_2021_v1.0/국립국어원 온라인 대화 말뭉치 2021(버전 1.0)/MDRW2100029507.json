      "utterance": [
        {
          "id": "MDRW2100029507.1.1",
          "form": "발라드 모음같은게 있더라구요 그거 틀어놓거나 아이돌 댄스음악도 들어요",
          "original_form": "발라드 모음같은게 있더라구요 그거 틀어놓거나 아이돌 댄스음악도 들어요",
          "speaker_id": "2",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100029507.1.2",
          "form": "오 저는",
          "original_form": "오 저는",
          "speaker_id": "1",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100029507.1.3",
          "form": "지금 루한 리허설 보고있어요",
          "original_form": "지금 루한 리허설 보고있어요",
          "speaker_id": "1",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100029507.1.4",
          "form": "어떤 리허설인데요?신곡나왔나요",
          "original_form": "어떤 리허설인데요?신곡나왔나요",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029507.1.5",
          "form": "아니요 추억 여행중이에요",
          "original_form": "아니요 추억 여행중이에요",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029507.1.6",
          "form": "엑소꺼에요 ㅜㅜ",
          "original_form": "엑소꺼에요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029507.1.7",
          "form": "그렇군요 그래도 재밌겠어요",
          "original_form": "그렇군요 그래도 재밌겠어요",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029507.1.8",
          "form": "혼자 제일 열심히 춰요",
          "original_form": "혼자 제일 열심히 춰요",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029507.1.9",
          "form": "다른사람은 설렁설렁이에요",
          "original_form": "다른사람은 설렁설렁이에요",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029507.1.10",
          "form": "ㅋㅋ열심히하는모습이 보기좋겠어요 다른사람들은 설렁설렁하다니 초심을 잃었나보군요",
          "original_form": "ㅋㅋ열심히하는모습이 보기좋겠어요 다른사람들은 설렁설렁하다니 초심을 잃었나보군요",
          "speaker_id": "2",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029507.1.11",
          "form": "으르렁으로 잘되고 있을때라 그런가바요",
          "original_form": "으르렁으로 잘되고 있을때라 그런가바요",
          "speaker_id": "1",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100029507.1.12",
          "form": "으르렁 그때 인기 정말 많긴했죠",
          "original_form": "으르렁 그때 인기 정말 많긴했죠",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029507.1.13",
          "form": "지금은 방탄이랑 위치가 바뀌었다니",
          "original_form": "지금은 방탄이랑 위치가 바뀌었다니",
          "speaker_id": "1",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029507.1.14",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029507.1.15",
          "form": "그러게요 신기하네요 그때는 그럴줄몰랐으니",
          "original_form": "그러게요 신기하네요 그때는 그럴줄몰랐으니",
          "speaker_id": "2",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029507.1.16",
          "form": "지금 일하는곳에 사람이 많네요",
          "original_form": "지금 일하는곳에 사람이 많네요",
          "speaker_id": "1",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029507.1.17",
          "form": "노래가 안들려요",
          "original_form": "노래가 안들려요",
          "speaker_id": "1",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029507.1.18",
          "form": "시끄럽나봐요 조용해질때들어야겠네요",
          "original_form": "시끄럽나봐요 조용해질때들어야겠네요",
          "speaker_id": "2",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100029507.1.19",
          "form": "넷플릭스 추천작 있으신가요",
          "original_form": "넷플릭스 추천작 있으신가요",
          "speaker_id": "1",
          "time": "20210903 10:59"
        }
      ]
