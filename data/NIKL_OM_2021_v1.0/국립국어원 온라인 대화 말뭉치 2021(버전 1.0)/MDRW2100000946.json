      "utterance": [
        {
          "id": "MDRW2100000946.1.1",
          "form": "저는 최근에 가을옷 구매했어요",
          "original_form": "저는 최근에 가을옷 구매했어요",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000946.1.2",
          "form": "날 시원해져서 슬슬 옷 사야하는데~",
          "original_form": "날 시원해져서 슬슬 옷 사야하는데~",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100000946.1.3",
          "form": "인터넷으로 샀어요?",
          "original_form": "인터넷으로 샀어요?",
          "speaker_id": "2",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100000946.1.4",
          "form": "요즘은 인터넷 구매만 해요.",
          "original_form": "요즘은 인터넷 구매만 해요.",
          "speaker_id": "1",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100000946.1.5",
          "form": "인터넷쇼핑이 진짜 편하네요",
          "original_form": "인터넷쇼핑이 진짜 편하네요",
          "speaker_id": "1",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100000946.1.6",
          "form": "요즘은 배송도 빨리 해주고 배송비 무료인곳도 많아서 좋은 것 같아요",
          "original_form": "요즘은 배송도 빨리 해주고 배송비 무료인곳도 많아서 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100000946.1.7",
          "form": "배송비가 항상 아깝더라구요ㅎ",
          "original_form": "배송비가 항상 아깝더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100000946.1.8",
          "form": "맞아요 배송비 들면 뭔가 아쉽ㅋㅋ",
          "original_form": "맞아요 배송비 들면 뭔가 아쉽ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100000946.1.9",
          "form": "그래서 요즘은 당근도 많이 써요",
          "original_form": "그래서 요즘은 당근도 많이 써요",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100000946.1.10",
          "form": "배송비아끼려다가 금액맞춘다고 더 사게되요ㅎ",
          "original_form": "배송비아끼려다가 금액맞춘다고 더 사게되요ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100000946.1.11",
          "form": "아~뭔지 알아요ㅋㅋ",
          "original_form": "아~뭔지 알아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100000946.1.12",
          "form": "그리고 어떻게든 할인쿠폰 찾아서 적용하고ㅋㅋ",
          "original_form": "그리고 어떻게든 할인쿠폰 찾아서 적용하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100000946.1.13",
          "form": "할인쿠폰,카드할인 챙겨야해요ㅋ",
          "original_form": "할인쿠폰,카드할인 챙겨야해요ㅋ",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100000946.1.14",
          "form": "이것저것 할인 적용해서 배송 온 옷같은거 상상했던 그 핏 아니면 넘 슬퍼요",
          "original_form": "이것저것 할인 적용해서 배송 온 옷같은거 상상했던 그 핏 아니면 넘 슬퍼요",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100000946.1.15",
          "form": "완전 너무 슬퍼요ㅜㅜ",
          "original_form": "완전 너무 슬퍼요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100000946.1.16",
          "form": "인터넷으로 화장품 살때가 젤 어려운거같아요",
          "original_form": "인터넷으로 화장품 살때가 젤 어려운거같아요",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100000946.1.17",
          "form": "후기는 필수죠ㅎㅎ",
          "original_form": "후기는 필수죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:18"
        }
