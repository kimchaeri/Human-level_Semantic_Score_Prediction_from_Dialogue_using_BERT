      "utterance": [
        {
          "id": "MDRW2100011926.1.1",
          "form": "오늘은 무슨 업무를 하시나요",
          "original_form": "오늘은 무슨 업무를 하시나요",
          "speaker_id": "1",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100011926.1.2",
          "form": "어제 못한 일을 합니다...",
          "original_form": "어제 못한 일을 합니다...",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100011926.1.3",
          "form": "논문을 읽어야해요",
          "original_form": "논문을 읽어야해요",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100011926.1.4",
          "form": "아 그러시구나..",
          "original_form": "아 그러시구나..",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100011926.1.5",
          "form": "나는 급여를 보내고",
          "original_form": "나는 급여를 보내고",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100011926.1.6",
          "form": "종합소득세 신고를 두건 한다음에",
          "original_form": "종합소득세 신고를 두건 한다음에",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100011926.1.7",
          "form": "분기결산을 한군데 끝내고",
          "original_form": "분기결산을 한군데 끝내고",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100011926.1.8",
          "form": "지급명세서를 제출해야해",
          "original_form": "지급명세서를 제출해야해",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100011926.1.9",
          "form": "신난다",
          "original_form": "신난다",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100011926.1.10",
          "form": "뭔진 모르지만 어려워 보이네요",
          "original_form": "뭔진 모르지만 어려워 보이네요",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100011926.1.11",
          "form": "정말 일하기 싫다",
          "original_form": "정말 일하기 싫다",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100011926.1.12",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100011926.1.13",
          "form": "돈이나 많이 벌고싶은",
          "original_form": "돈이나 많이 벌고싶은",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100011926.1.14",
          "form": "업무량에 비해 너무나 귀엽기만 한 월급",
          "original_form": "업무량에 비해 너무나 귀엽기만 한 월급",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100011926.1.15",
          "form": "밥먹으면서도 업무스트레스를 받아야함",
          "original_form": "밥먹으면서도 업무스트레스를 받아야함",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100011926.1.16",
          "form": "체할뻔~",
          "original_form": "체할뻔~",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100011926.1.17",
          "form": "즉당히...즉당히 살고싶어요...",
          "original_form": "즉당히...즉당히 살고싶어요...",
          "speaker_id": "2",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100011926.1.18",
          "form": "월급은 딱 두배만 바랄게요",
          "original_form": "월급은 딱 두배만 바랄게요",
          "speaker_id": "1",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100011926.1.19",
          "form": "다시 취업한다면 무슨 일 하고싶니",
          "original_form": "다시 취업한다면 무슨 일 하고싶니",
          "speaker_id": "2",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100011926.1.20",
          "form": "아무것도하고싶지않아",
          "original_form": "아무것도하고싶지않아",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100011926.1.21",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100011926.1.22",
          "form": "전업투자자가 되는 것으로. .",
          "original_form": "전업투자자가 되는 것으로. .",
          "speaker_id": "2",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100011926.1.23",
          "form": "야구단 들어가면 재밌겠다",
          "original_form": "야구단 들어가면 재밌겠다",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100011926.1.24",
          "form": "홍보팀쪽",
          "original_form": "홍보팀쪽",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100011926.1.25",
          "form": "스펙 나이 흥망 모든걸떠나서",
          "original_form": "스펙 나이 흥망 모든걸떠나서",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100011926.1.26",
          "form": "sns마케팅같은거 재밌을거같아",
          "original_form": "sns마케팅같은거 재밌을거같아",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100011926.1.27",
          "form": "아니면 상품기획이나",
          "original_form": "아니면 상품기획이나",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100011926.1.28",
          "form": "여행 마케팅? 하는 지인있는데",
          "original_form": "여행 마케팅? 하는 지인있는데",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100011926.1.29",
          "form": "맨날 여행다니더라",
          "original_form": "맨날 여행다니더라",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100011926.1.30",
          "form": "너 마케팅 잘할듯",
          "original_form": "너 마케팅 잘할듯",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100011926.1.31",
          "form": "여행은 온전히 즐기고만싶어서 일로는 싫어",
          "original_form": "여행은 온전히 즐기고만싶어서 일로는 싫어",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100011926.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100011926.1.33",
          "form": "그리고 좋은정보는 나만알고싶음",
          "original_form": "그리고 좋은정보는 나만알고싶음",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100011926.1.34",
          "form": "난 sns귀찮아",
          "original_form": "난 sns귀찮아",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100011926.1.35",
          "form": "나도 트위터말고는 안하는데 업무면 해야지..",
          "original_form": "나도 트위터말고는 안하는데 업무면 해야지..",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100011926.1.36",
          "form": "지금 일도 싫은건 아닌데 너무 보람이 없음",
          "original_form": "지금 일도 싫은건 아닌데 너무 보람이 없음",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100011926.1.37",
          "form": "어서 퇴사기원을..",
          "original_form": "어서 퇴사기원을..",
          "speaker_id": "2",
          "time": "20210825 13:25"
        }
      ]
    }
