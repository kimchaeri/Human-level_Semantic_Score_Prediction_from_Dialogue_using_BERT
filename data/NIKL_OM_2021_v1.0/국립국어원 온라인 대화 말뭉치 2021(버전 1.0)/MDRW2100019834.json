      "utterance": [
        {
          "id": "MDRW2100019834.1.1",
          "form": "뭔가 스파이크 할때 속이 시원해질거같아요",
          "original_form": "뭔가 스파이크 할때 속이 시원해질거같아요",
          "speaker_id": "2",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019834.1.2",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019834.1.3",
          "form": "아니요 아직 못가봤어요ㅋㅋ",
          "original_form": "아니요 아직 못가봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019834.1.4",
          "form": "키큰 선수들이 스파이크날릴때 멋있어요",
          "original_form": "키큰 선수들이 스파이크날릴때 멋있어요",
          "speaker_id": "1",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019834.1.5",
          "form": "코로나가 좀 잠잠해져야 갈수있을텐데ㅠ",
          "original_form": "코로나가 좀 잠잠해져야 갈수있을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019834.1.6",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019834.1.7",
          "form": "그니까요 멋있더라구요",
          "original_form": "그니까요 멋있더라구요",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019834.1.8",
          "form": "코로나때문에 공연도 안 하고 슬퍼요 ㅠ",
          "original_form": "코로나때문에 공연도 안 하고 슬퍼요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019834.1.9",
          "form": "그니까요ㅠ\r\n코로나아니었으면",
          "original_form": "그니까요ㅠ\r\n코로나아니었으면",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019834.1.10",
          "form": "볼거 엄청 많았을텐데ㅠ",
          "original_form": "볼거 엄청 많았을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019834.1.11",
          "form": "맞아요 살맛안나요 ㅠㅠ",
          "original_form": "맞아요 살맛안나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019834.1.12",
          "form": "스포츠경기 직관은 그럼 가보신적 아예 없으세요??",
          "original_form": "스포츠경기 직관은 그럼 가보신적 아예 없으세요??",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019834.1.13",
          "form": "네 직관 갈 정도로 관심있진 않았나봐욪ㅋㅋㅋ",
          "original_form": "네 직관 갈 정도로 관심있진 않았나봐욪ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019834.1.14",
          "form": "저두여 ㅋㅋㅋ보기만해요",
          "original_form": "저두여 ㅋㅋㅋ보기만해요",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019834.1.15",
          "form": "저는 보는것보다 직접하는게 더 재밌더라구요",
          "original_form": "저는 보는것보다 직접하는게 더 재밌더라구요",
          "speaker_id": "2",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019834.1.16",
          "form": "와직접 하시기도하세요?",
          "original_form": "와직접 하시기도하세요?",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019834.1.17",
          "form": "전 몸쓰는건 다 못해서..ㅋㅋㅋ",
          "original_form": "전 몸쓰는건 다 못해서..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019834.1.18",
          "form": "대학교때 양궁 교양수업에서 배웠었는데",
          "original_form": "대학교때 양궁 교양수업에서 배웠었는데",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019834.1.19",
          "form": "재밌더라구요 그래서 양궁카페가서 종종해요ㅋㅋ",
          "original_form": "재밌더라구요 그래서 양궁카페가서 종종해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019834.1.20",
          "form": "와양궁카페도 있군요 저도 가봐야겠어요",
          "original_form": "와양궁카페도 있군요 저도 가봐야겠어요",
