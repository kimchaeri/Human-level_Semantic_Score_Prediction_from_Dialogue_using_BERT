        {
          "id": "MDRW2100007660.1.1",
          "form": "정모해요?",
          "original_form": "정모해요?",
          "speaker_id": "1",
          "time": "20210822 09:10"
        },
        {
          "id": "MDRW2100007660.1.2",
          "form": "정모가 뭐에요?",
          "original_form": "정모가 뭐에요?",
          "speaker_id": "2",
          "time": "20210822 09:13"
        },
        {
          "id": "MDRW2100007660.1.3",
          "form": "정기모임요",
          "original_form": "정기모임요",
          "speaker_id": "1",
          "time": "20210822 09:13"
        },
        {
          "id": "MDRW2100007660.1.4",
          "form": "동호회 이런 거요?",
          "original_form": "동호회 이런 거요?",
          "speaker_id": "2",
          "time": "20210822 09:13"
        },
        {
          "id": "MDRW2100007660.1.5",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210822 09:14"
        },
        {
          "id": "MDRW2100007660.1.6",
          "form": "아 예전엔 했었어요",
          "original_form": "아 예전엔 했었어요",
          "speaker_id": "2",
          "time": "20210822 09:15"
        },
        {
          "id": "MDRW2100007660.1.7",
          "form": "코로너전에요?",
          "original_form": "코로너전에요?",
          "speaker_id": "1",
          "time": "20210822 09:16"
        },
        {
          "id": "MDRW2100007660.1.8",
          "form": "네 한 삼사년 전쯤에요",
          "original_form": "네 한 삼사년 전쯤에요",
          "speaker_id": "2",
          "time": "20210822 09:16"
        },
        {
          "id": "MDRW2100007660.1.9",
          "form": "전 친구 계모림요",
          "original_form": "전 친구 계모림요",
          "speaker_id": "1",
          "time": "20210822 09:18"
        },
        {
          "id": "MDRW2100007660.1.10",
          "form": "아 친목으로 계모임 많이 하더라고요",
          "original_form": "아 친목으로 계모임 많이 하더라고요",
          "speaker_id": "2",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007660.1.11",
          "form": "요즘 모이기두 힘들죠",
          "original_form": "요즘 모이기두 힘들죠",
          "speaker_id": "1",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007660.1.12",
          "form": "맞아요 곗돈만 쌓여요",
          "original_form": "맞아요 곗돈만 쌓여요",
          "speaker_id": "2",
          "time": "20210822 09:20"
        },
        {
          "id": "MDRW2100007660.1.13",
          "form": "나중에 나누고 또 모아요",
