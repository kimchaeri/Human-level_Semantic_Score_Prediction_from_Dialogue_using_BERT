        },
        {
          "id": "MDRW2100045623.1.2",
          "form": "트레이더스 무슨 제품 추천해?",
          "original_form": "트레이더스 무슨 제품 추천해?",
          "speaker_id": "1",
          "time": "20210908 04:20"
        },
        {
          "id": "MDRW2100045623.1.3",
          "form": "이번 주말에 가려고?",
          "original_form": "이번 주말에 가려고?",
          "speaker_id": "2",
          "time": "20210908 04:21"
        },
        {
          "id": "MDRW2100045623.1.4",
          "form": "응 토욜에 쇼핑하려고",
          "original_form": "응 토욜에 쇼핑하려고",
          "speaker_id": "1",
          "time": "20210908 04:21"
        },
        {
          "id": "MDRW2100045623.1.5",
          "form": "거기 티라미슈 맛있대!",
          "original_form": "거기 티라미슈 맛있대!",
          "speaker_id": "2",
          "time": "20210908 04:21"
        },
        {
          "id": "MDRW2100045623.1.6",
          "form": "오 거기 빵 싸고 양도 많지",
          "original_form": "오 거기 빵 싸고 양도 많지",
          "speaker_id": "1",
          "time": "20210908 04:21"
        },
        {
          "id": "MDRW2100045623.1.7",
          "form": "맞아 맛도 있더라구",
          "original_form": "맞아 맛도 있더라구",
          "speaker_id": "2",
          "time": "20210908 04:21"
        },
        {
          "id": "MDRW2100045623.1.8",
          "form": "나는 거기가면 해산물 꼭 사",
          "original_form": "나는 거기가면 해산물 꼭 사",
          "speaker_id": "1",
          "time": "20210908 04:22"
        },
        {
          "id": "MDRW2100045623.1.9",
          "form": "냉동사오는거야?",
          "original_form": "냉동사오는거야?",
          "speaker_id": "2",
          "time": "20210908 04:22"
        },
        {
          "id": "MDRW2100045623.1.10",
          "form": "아니 광어회같은거 ㅋㅋ",
          "original_form": "아니 광어회같은거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:22"
        },
        {
          "id": "MDRW2100045623.1.11",
          "form": "가격이 괜찮나보네",
          "original_form": "가격이 괜찮나보네",
          "speaker_id": "2",
          "time": "20210908 04:22"
        },
        {
          "id": "MDRW2100045623.1.12",
          "form": "응 싸고 신선해",
          "original_form": "응 싸고 신선해",
          "speaker_id": "1",
          "time": "20210908 04:22"
        },
        {
          "id": "MDRW2100045623.1.13",
          "form": "거기 새우튀김 도 맛있는데",
          "original_form": "거기 새우튀김 도 맛있는데",
          "speaker_id": "2",
          "time": "20210908 04:23"
        },
        {
          "id": "MDRW2100045623.1.14",
          "form": "아 튀김 좋은생각이다",
          "original_form": "아 튀김 좋은생각이다",
          "speaker_id": "1",
          "time": "20210908 04:23"
