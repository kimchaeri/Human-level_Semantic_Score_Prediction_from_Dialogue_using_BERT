      "utterance": [
        {
          "id": "MDRW2100013718.1.1",
          "form": "마미 마라탕 시켜먹을까?",
          "original_form": "마미 마라탕 시켜먹을까?",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013718.1.2",
          "form": "그저께 먹었잖아",
          "original_form": "그저께 먹었잖아",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013718.1.3",
          "form": "또 먹고싶다 ㅠㅠ",
          "original_form": "또 먹고싶다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013718.1.4",
          "form": "살찐다",
          "original_form": "살찐다",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.5",
          "form": "살안찐다 마라탕은..",
          "original_form": "살안찐다 마라탕은..",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.6",
          "form": "무슨 헛소리",
          "original_form": "무슨 헛소리",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.7",
          "form": "아닌뎅!ㅋㅋ 당면만 좀 안먹으면 괜찮을껄",
          "original_form": "아닌뎅!ㅋㅋ 당면만 좀 안먹으면 괜찮을껄",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.8",
          "form": "지방이 얼마나 많은데",
          "original_form": "지방이 얼마나 많은데",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.9",
          "form": "탄수화물만 적게 먹으면 살안찜 ㅋ",
          "original_form": "탄수화물만 적게 먹으면 살안찜 ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.10",
          "form": "몰라 시키던지",
          "original_form": "몰라 시키던지",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.11",
          "form": "구래 ㅋㅋ 엄마 좋아하는 버섯 많이 넣어서 고고",
          "original_form": "구래 ㅋㅋ 엄마 좋아하는 버섯 많이 넣어서 고고",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.12",
          "form": "그래 집에 채소도 있으니까",
          "original_form": "그래 집에 채소도 있으니까",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.13",
          "form": "웅웅 다 넣어서 고고싱",
          "original_form": "웅웅 다 넣어서 고고싱",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013718.1.14",
          "form": "그러면 이따 1시간 뒤에 시켜라",
          "original_form": "그러면 이따 1시간 뒤에 시켜라",
          "speaker_id": "2",
          "time": "20210825 15:45"
