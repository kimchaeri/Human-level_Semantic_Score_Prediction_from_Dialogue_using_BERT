      "utterance": [
        {
          "id": "MDRW2100006947.1.1",
          "form": "오늘 뭐드셨어요?",
          "original_form": "오늘 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006947.1.2",
          "form": "저는 오늘저녁 소불고기 먹었어요.",
          "original_form": "저는 오늘저녁 소불고기 먹었어요.",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006947.1.3",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006947.1.4",
          "form": "저는 라면끓여먹었어요 ㅋㅋ",
          "original_form": "저는 라면끓여먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006947.1.5",
          "form": "배달음식은 드세요?",
          "original_form": "배달음식은 드세요?",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006947.1.6",
          "form": "저는요즘 배달음식때문에",
          "original_form": "저는요즘 배달음식때문에",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006947.1.7",
          "form": "좋아하죠ㅋㅋ",
          "original_form": "좋아하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006947.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006947.1.9",
          "form": "10키로 찐거같아요",
          "original_form": "10키로 찐거같아요",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006947.1.10",
          "form": "편의점음식도 너무먹어서",
          "original_form": "편의점음식도 너무먹어서",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006947.1.11",
          "form": "와 10키로ㅋㅋ",
          "original_form": "와 10키로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006947.1.12",
          "form": "하긴 ㅋㅋ저도 막 찌고있어요ㅋㅋ",
          "original_form": "하긴 ㅋㅋ저도 막 찌고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006947.1.13",
          "form": "맛있는게 너무많아요ㅋㅋ",
          "original_form": "맛있는게 너무많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006947.1.14",
          "form": "그냥 배가 안고픈데도",
          "original_form": "그냥 배가 안고픈데도",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006947.1.15",
          "form": "맛있어서 의무적으로 먹는ㅋㅋ",
          "original_form": "맛있어서 의무적으로 먹는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006947.1.16",
          "form": "이제 안그러려구요",
          "original_form": "이제 안그러려구요",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006947.1.17",
          "form": "ㅋㅋ전 배불러도",
          "original_form": "ㅋㅋ전 배불러도",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006947.1.18",
          "form": "계속먹게되서",
          "original_form": "계속먹게되서",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006947.1.19",
          "form": "그래서 살이쪘나봐요.",
          "original_form": "그래서 살이쪘나봐요.",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006947.1.20",
          "form": "그리고 기분이 안좋은걸 먹는거로 라도 해소하려니까",
          "original_form": "그리고 기분이 안좋은걸 먹는거로 라도 해소하려니까",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006947.1.21",
          "form": "배가아플때까지 먹다 토한적도있고",
          "original_form": "배가아플때까지 먹다 토한적도있고",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006947.1.22",
          "form": "헉 그러시구나",
          "original_form": "헉 그러시구나",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006947.1.23",
          "form": "그럼 속 안좋아지잖아요. ㅠ",
          "original_form": "그럼 속 안좋아지잖아요. ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006947.1.24",
          "form": "한번만그랬어용!",
          "original_form": "한번만그랬어용!",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006947.1.25",
          "form": "돈아까워서 하면안되겠더라",
          "original_form": "돈아까워서 하면안되겠더라",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006947.1.26",
          "form": "좋아하는음식은 뭔가요?",
          "original_form": "좋아하는음식은 뭔가요?",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006947.1.27",
          "form": "저는 다 좋아해요.",
          "original_form": "저는 다 좋아해요.",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006947.1.28",
          "form": "옛날엔 회를 못먹었는데",
          "original_form": "옛날엔 회를 못먹었는데",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006947.1.29",
          "form": "점점 나이드니ㅋㅋ",
          "original_form": "점점 나이드니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006947.1.30",
          "form": "못먹는게 없어지더라구요",
          "original_form": "못먹는게 없어지더라구요",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006947.1.31",
          "form": "회도 좋아해요.ㅋ",
          "original_form": "회도 좋아해요.ㅋ",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006947.1.32",
          "form": "저는아직 회 못먹는데ㅋㅋ",
          "original_form": "저는아직 회 못먹는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006947.1.33",
          "form": "아 그러시구나ㅋㅋ",
          "original_form": "아 그러시구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:14"
        }
      ]
    }
