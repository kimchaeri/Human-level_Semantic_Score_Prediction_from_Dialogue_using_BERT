      "utterance": [
        {
          "id": "MDRW2100003509.1.1",
          "form": "지금도 유튜브 보고 있어?",
          "original_form": "지금도 유튜브 보고 있어?",
          "speaker_id": "2",
          "time": "20210821 01:24"
        },
        {
          "id": "MDRW2100003509.1.2",
          "form": "응 잠이 안 와서 영상 몇개 보다 자려고~",
          "original_form": "응 잠이 안 와서 영상 몇개 보다 자려고~",
          "speaker_id": "1",
          "time": "20210821 01:24"
        },
        {
          "id": "MDRW2100003509.1.3",
          "form": "성시경 킬링 보이스 보고 있어",
          "original_form": "성시경 킬링 보이스 보고 있어",
          "speaker_id": "1",
          "time": "20210821 01:25"
        },
        {
          "id": "MDRW2100003509.1.4",
          "form": "노래 부르는 거",
          "original_form": "노래 부르는 거",
          "speaker_id": "1",
          "time": "20210821 01:25"
        },
        {
          "id": "MDRW2100003509.1.5",
          "form": "엄마 예전에 그거 봤어.",
          "original_form": "엄마 예전에 그거 봤어.",
          "speaker_id": "2",
          "time": "20210821 01:25"
        },
        {
          "id": "MDRW2100003509.1.6",
          "form": "나는 넷플릭스 보려고 했는데 뭘 봐야 될지 모르겠내.",
          "original_form": "나는 넷플릭스 보려고 했는데 뭘 봐야 될지 모르겠내.",
          "speaker_id": "2",
          "time": "20210821 01:26"
        },
        {
          "id": "MDRW2100003509.1.7",
          "form": "너 요즘 뭐 보니?",
          "original_form": "너 요즘 뭐 보니?",
          "speaker_id": "2",
          "time": "20210821 01:26"
        },
        {
          "id": "MDRW2100003509.1.8",
          "form": "난 항상 보는 거ㅋㅋㅋㅋ",
          "original_form": "난 항상 보는 거ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003509.1.9",
          "form": "모팸 빅뱅이론 프렌즈 브나나",
          "original_form": "모팸 빅뱅이론 프렌즈 브나나",
          "speaker_id": "1",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003509.1.10",
          "form": "다 미국 시트콤이야",
          "original_form": "다 미국 시트콤이야",
          "speaker_id": "1",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003509.1.11",
          "form": "모던패밀리 재밌어 추천",
          "original_form": "모던패밀리 재밌어 추천",
          "speaker_id": "1",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003509.1.12",
          "form": "그거 네가 저번에 보여줬잖아",
          "original_form": "그거 네가 저번에 보여줬잖아",
          "speaker_id": "2",
          "time": "20210821 01:28"
        },
        {
          "id": "MDRW2100003509.1.13",
          "form": "아 그랬나?ㅋㅋㅋ 근데 다 안 봤잖아~",
          "original_form": "아 그랬나?ㅋㅋㅋ 근데 다 안 봤잖아~",
          "speaker_id": "1",
          "time": "20210821 01:29"
        },
        {
          "id": "MDRW2100003509.1.14",
          "form": "시즌 11까지 있음!!",
          "original_form": "시즌 11까지 있음!!",
          "speaker_id": "1",
          "time": "20210821 01:29"
        },
        {
          "id": "MDRW2100003509.1.15",
          "form": "처음부터 정주행해",
          "original_form": "처음부터 정주행해",
          "speaker_id": "1",
          "time": "20210821 01:29"
        },
        {
          "id": "MDRW2100003509.1.16",
          "form": "한국드라마는 없어?",
          "original_form": "한국드라마는 없어?",
          "speaker_id": "2",
          "time": "20210821 01:30"
        },
        {
          "id": "MDRW2100003509.1.17",
          "form": "그거 봐 괴물",
          "original_form": "그거 봐 괴물",
          "speaker_id": "1",
          "time": "20210821 01:30"
        },
        {
          "id": "MDRW2100003509.1.18",
          "form": "신하균이랑 여진구 나오는 거",
          "original_form": "신하균이랑 여진구 나오는 거",
          "speaker_id": "1",
          "time": "20210821 01:30"
        },
        {
          "id": "MDRW2100003509.1.19",
          "form": "그거 넷플에 있을걸?",
          "original_form": "그거 넷플에 있을걸?",
          "speaker_id": "1",
          "time": "20210821 01:30"
        },
        {
          "id": "MDRW2100003509.1.20",
          "form": "무슨 내용인데",
          "original_form": "무슨 내용인데",
          "speaker_id": "2",
          "time": "20210821 01:30"
        },
        {
          "id": "MDRW2100003509.1.21",
          "form": "추리물이야 나도 끝까지는 안 봤어",
          "original_form": "추리물이야 나도 끝까지는 안 봤어",
          "speaker_id": "1",
          "time": "20210821 01:31"
        },
        {
          "id": "MDRW2100003509.1.22",
          "form": "아직 5화까지박에 안 봤음",
          "original_form": "아직 5화까지박에 안 봤음",
          "speaker_id": "1",
          "time": "20210821 01:31"
        },
        {
          "id": "MDRW2100003509.1.23",
          "form": "어 검색해보니까 있네.",
          "original_form": "어 검색해보니까 있네.",
          "speaker_id": "2",
          "time": "20210821 01:32"
        },
        {
          "id": "MDRW2100003509.1.24",
          "form": "넷플릭스 결제해놓고 자주 안 보게되네",
          "original_form": "넷플릭스 결제해놓고 자주 안 보게되네",
          "speaker_id": "2",
          "time": "20210821 01:33"
        },
        {
          "id": "MDRW2100003509.1.25",
          "form": "돈 아까워.",
          "original_form": "돈 아까워.",
          "speaker_id": "2",
          "time": "20210821 01:33"
        },
        {
          "id": "MDRW2100003509.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ내가 매일 쓰니까 아까워하지 마",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ내가 매일 쓰니까 아까워하지 마",
          "speaker_id": "1",
          "time": "20210821 01:33"
        },
        {
          "id": "MDRW2100003509.1.27",
          "form": "암튼 괴물 재밌어",
          "original_form": "암튼 괴물 재밌어",
          "speaker_id": "1",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003509.1.28",
          "form": "신하균 연기 진짜 잘해",
          "original_form": "신하균 연기 진짜 잘해",
          "speaker_id": "1",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003509.1.29",
          "form": "신하균이야 원래 연기 잘하지",
          "original_form": "신하균이야 원래 연기 잘하지",
          "speaker_id": "2",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003509.1.30",
          "form": "얼마 전에 복수는 나의 것 다시 봤는데 거기서도 연기 너무 잘하더라.",
          "original_form": "얼마 전에 복수는 나의 것 다시 봤는데 거기서도 연기 너무 잘하더라.",
          "speaker_id": "2",
          "time": "20210821 01:36"
        },
        {
          "id": "MDRW2100003509.1.31",
          "form": "박찬욱 꺼?",
          "original_form": "박찬욱 꺼?",
          "speaker_id": "1",
          "time": "20210821 01:36"
        },
        {
          "id": "MDRW2100003509.1.32",
          "form": "거의 20년 된 영화 아녀",
          "original_form": "거의 20년 된 영화 아녀",
          "speaker_id": "1",
          "time": "20210821 01:37"
        },
        {
          "id": "MDRW2100003509.1.33",
          "form": "그정도 됐지",
          "original_form": "그정도 됐지",
          "speaker_id": "2",
          "time": "20210821 01:37"
        },
        {
          "id": "MDRW2100003509.1.34",
          "form": "이천년대 초에 개봉한 거니까.",
          "original_form": "이천년대 초에 개봉한 거니까.",
          "speaker_id": "2",
          "time": "20210821 01:37"
        },
        {
          "id": "MDRW2100003509.1.35",
          "form": "그때 여진구는 거의 유치원생이었겠는데??ㅋㅋㅋㅋㅋ",
          "original_form": "그때 여진구는 거의 유치원생이었겠는데??ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 01:38"
        },
        {
          "id": "MDRW2100003509.1.36",
          "form": "나이차가 그렇게 많이 나나",
          "original_form": "나이차가 그렇게 많이 나나",
          "speaker_id": "2",
          "time": "20210821 01:38"
        },
        {
          "id": "MDRW2100003509.1.37",
          "form": "그치... 거의 스무살은 차이날걸??",
          "original_form": "그치... 거의 스무살은 차이날걸??",
          "speaker_id": "1",
          "time": "20210821 01:39"
        },
        {
          "id": "MDRW2100003509.1.38",
          "form": "괴물에서 여진구도 연기 진짜 잘해",
          "original_form": "괴물에서 여진구도 연기 진짜 잘해",
          "speaker_id": "1",
          "time": "20210821 01:39"
        },
        {
          "id": "MDRW2100003509.1.39",
          "form": "신하균한테 안 밀림",
          "original_form": "신하균한테 안 밀림",
          "speaker_id": "1",
          "time": "20210821 01:40"
        },
        {
          "id": "MDRW2100003509.1.40",
          "form": "재밌다니까 봐야겠네 괴물.",
          "original_form": "재밌다니까 봐야겠네 괴물.",
          "speaker_id": "2",
          "time": "20210821 01:40"
        },
        {
          "id": "MDRW2100003509.1.41",
          "form": "근데 무서울 수도 있으니까 자기 전에는 웃긴 거 봐",
          "original_form": "근데 무서울 수도 있으니까 자기 전에는 웃긴 거 봐",
          "speaker_id": "1",
          "time": "20210821 01:42"
        },
        {
          "id": "MDRW2100003509.1.42",
          "form": "아니면 다큐멘터리",
          "original_form": "아니면 다큐멘터리",
          "speaker_id": "1",
          "time": "20210821 01:42"
        },
        {
          "id": "MDRW2100003509.1.43",
          "form": "얼마전에 넷플릭스에서 씨스파라시 봤어.",
          "original_form": "얼마전에 넷플릭스에서 씨스파라시 봤어.",
          "speaker_id": "2",
          "time": "20210821 01:42"
        },
        {
          "id": "MDRW2100003509.1.44",
          "form": "네가 전에 보라고 추천해준거,",
          "original_form": "네가 전에 보라고 추천해준거,",
          "speaker_id": "2",
          "time": "20210821 01:42"
        },
        {
          "id": "MDRW2100003509.1.45",
          "form": "오 잘했어 잘했어~~",
          "original_form": "오 잘했어 잘했어~~",
          "speaker_id": "1",
          "time": "20210821 01:43"
        },
        {
          "id": "MDRW2100003509.1.46",
          "form": "넷플이 다큐를 진짜 잘 만드는 것 같아",
          "original_form": "넷플이 다큐를 진짜 잘 만드는 것 같아",
          "speaker_id": "1",
          "time": "20210821 01:43"
        },
        {
          "id": "MDRW2100003509.1.47",
          "form": "그거 보니까 고래랑 상어랑 물고기들이 너무 불쌍하더라.",
          "original_form": "그거 보니까 고래랑 상어랑 물고기들이 너무 불쌍하더라.",
          "speaker_id": "2",
          "time": "20210821 01:43"
        },
        {
          "id": "MDRW2100003509.1.48",
          "form": "그치... 나도 그거 보고 생선 적게 먹어야겠다고 생각했어",
          "original_form": "그치... 나도 그거 보고 생선 적게 먹어야겠다고 생각했어",
          "speaker_id": "1",
          "time": "20210821 01:44"
        },
        {
          "id": "MDRW2100003509.1.49",
          "form": "맞아. 바다에서 그렇게 많은 생물들이 죽임을 당하는지 몰랐어.",
          "original_form": "맞아. 바다에서 그렇게 많은 생물들이 죽임을 당하는지 몰랐어.",
          "speaker_id": "2",
          "time": "20210821 01:46"
        },
        {
          "id": "MDRW2100003509.1.50",
          "form": "나도 오늘 넷플릭스에서 다큐 보고 자야겠다",
          "original_form": "나도 오늘 넷플릭스에서 다큐 보고 자야겠다",
          "speaker_id": "1",
          "time": "20210821 01:46"
        },
        {
          "id": "MDRW2100003509.1.51",
          "form": "그래. 유튜브 그만 보고 자.",
          "original_form": "그래. 유튜브 그만 보고 자.",
          "speaker_id": "2",
          "time": "20210821 01:47"
        },
        {
          "id": "MDRW2100003509.1.52",
          "form": "알겠어 엄마도 넷플 재밌게 봐~~~",
          "original_form": "알겠어 엄마도 넷플 재밌게 봐~~~",
          "speaker_id": "1",
          "time": "20210821 01:47"
        },
        {
          "id": "MDRW2100003509.1.53",
          "form": "저번에 넷플에서 괴물 보라고 했던 건 봤어?",
          "original_form": "저번에 넷플에서 괴물 보라고 했던 건 봤어?",
