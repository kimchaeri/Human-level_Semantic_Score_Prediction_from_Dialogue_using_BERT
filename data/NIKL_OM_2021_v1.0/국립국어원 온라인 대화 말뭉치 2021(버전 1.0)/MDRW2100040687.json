      "utterance": [
        {
          "id": "MDRW2100040687.1.1",
          "form": "와 이거 어렵네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 이거 어렵네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040687.1.2",
          "form": "야 이번엔 너가맞춰봐 ㅋㅋㅋㅋ",
          "original_form": "야 이번엔 너가맞춰봐 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040687.1.3",
          "form": "ㅋㅋㅋ진짜어려움ㅠ",
          "original_form": "ㅋㅋㅋ진짜어려움ㅠ",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040687.1.4",
          "form": "맞춰봐 ㅋㅋㅋ 난생각했어 ㅋㅋ",
          "original_form": "맞춰봐 ㅋㅋㅋ 난생각했어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040687.1.5",
          "form": "봉투야상자야?",
          "original_form": "봉투야상자야?",
          "speaker_id": "2",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100040687.1.6",
          "form": "상자야! 상자과자야",
          "original_form": "상자야! 상자과자야",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040687.1.7",
          "form": "초코맛이야?",
          "original_form": "초코맛이야?",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040687.1.8",
          "form": "아니아니 초코맛 1도안나",
          "original_form": "아니아니 초코맛 1도안나",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040687.1.9",
          "form": "바삭해?",
          "original_form": "바삭해?",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040687.1.10",
          "form": "어 바삭바삭해!!",
          "original_form": "어 바삭바삭해!!",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040687.1.11",
          "form": "개별포장이야?",
          "original_form": "개별포장이야?",
          "speaker_id": "2",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040687.1.12",
          "form": "어..  아니?",
          "original_form": "어..  아니?",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040687.1.13",
          "form": "상자안에 한봉지!",
          "original_form": "상자안에 한봉지!",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040687.1.14",
          "form": "예감!",
          "original_form": "예감!",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100040687.1.15",
          "form": "땡이야 내가 질문에서 깔려했는데 봐준다",
          "original_form": "땡이야 내가 질문에서 깔려했는데 봐준다",
          "speaker_id": "1",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100040687.1.16",
          "form": "예감맛있지만 당신은 선택되지 않았습니다",
          "original_form": "예감맛있지만 당신은 선택되지 않았습니다",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040687.1.17",
          "form": "버터맛이야?",
          "original_form": "버터맛이야?",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040687.1.18",
          "form": "아니 전혀 안나 버터맛 ㅋㅋㅋ",
          "original_form": "아니 전혀 안나 버터맛 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100040687.1.19",
          "form": "흠 어렵다 ㅋㅋㅋㅋ",
          "original_form": "흠 어렵다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100040687.1.20",
          "form": "너모야?",
          "original_form": "너모야?",
          "speaker_id": "2",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100040687.1.21",
          "form": "네모ㅋㅋ",
          "original_form": "네모ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100040687.1.22",
          "form": "아니 ㅋㅋㅋ",
          "original_form": "아니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100040687.1.23",
          "form": "네모도 동그라미도 아니야",
          "original_form": "네모도 동그라미도 아니야",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100040687.1.24",
          "form": "구운감자!",
          "original_form": "구운감자!",
          "speaker_id": "2",
          "time": "20210905 00:28"
        },
        {
          "id": "MDRW2100040687.1.25",
          "form": "땡! 고래밥임 ㅋㅋㅋㅋㅋ",
          "original_form": "땡! 고래밥임 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:29"
        },
        {
          "id": "MDRW2100040687.1.26",
          "form": "우리진짜못한다",
          "original_form": "우리진짜못한다",
          "speaker_id": "1",
          "time": "20210905 00:29"
        },
        {
          "id": "MDRW2100040687.1.27",
          "form": "정답은 고래밥이었어 ㅋㅋㅋ",
          "original_form": "정답은 고래밥이었어 ㅋㅋㅋ",
