      "utterance": [
        {
          "id": "MDRW2100031918.1.1",
          "form": "고민이 많으시겟어요?ㅎ",
          "original_form": "고민이 많으시겟어요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100031918.1.2",
          "form": "저도 고민 엄청많은데ㅠ",
          "original_form": "저도 고민 엄청많은데ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100031918.1.3",
          "form": "맞아요 ㅠㅁ ㅠ",
          "original_form": "맞아요 ㅠㅁ ㅠ",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100031918.1.4",
          "form": "어린이집 첨 갓을때",
          "original_form": "어린이집 첨 갓을때",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100031918.1.5",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031918.1.6",
          "form": "선생님한테 밥먹을때 한번 같이 먹어볼까 권해서",
          "original_form": "선생님한테 밥먹을때 한번 같이 먹어볼까 권해서",
          "speaker_id": "1",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031918.1.7",
          "form": "아아ㅎ 그래서요?",
          "original_form": "아아ㅎ 그래서요?",
          "speaker_id": "2",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031918.1.8",
          "form": "안먹는다면 주지 말라고 해요",
          "original_form": "안먹는다면 주지 말라고 해요",
          "speaker_id": "1",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031918.1.9",
          "form": "못먹는거 먹이면 토해버려서",
          "original_form": "못먹는거 먹이면 토해버려서",
          "speaker_id": "1",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031918.1.10",
          "form": "아이고ㅠㅠ",
          "original_form": "아이고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 08:40"
        },
        {
          "id": "MDRW2100031918.1.11",
          "form": "일부러 토하는군요ㅠ",
          "original_form": "일부러 토하는군요ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:40"
        },
        {
          "id": "MDRW2100031918.1.12",
          "form": "속상하셨겟네요ㅠ",
          "original_form": "속상하셨겟네요ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:40"
        },
        {
          "id": "MDRW2100031918.1.13",
          "form": "일부러는 아닌데",
          "original_form": "일부러는 아닌데",
          "speaker_id": "1",
          "time": "20210905 08:40"
        },
        {
          "id": "MDRW2100031918.1.14",
          "form": "뱉는데 아니라 바로  토해 버리더라구요",
          "original_form": "뱉는데 아니라 바로  토해 버리더라구요",
          "speaker_id": "1",
          "time": "20210905 08:40"
        },
        {
          "id": "MDRW2100031918.1.15",
          "form": "저희아이도 그랬어요ㅠ",
          "original_form": "저희아이도 그랬어요ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031918.1.16",
          "form": "뱉어버리고 토해버리고",
          "original_form": "뱉어버리고 토해버리고",
          "speaker_id": "2",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031918.1.17",
          "form": "입이 예민 해서 그런다고",
          "original_form": "입이 예민 해서 그런다고",
          "speaker_id": "1",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031918.1.18",
          "form": "에고에고ㅠ",
          "original_form": "에고에고ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031918.1.19",
          "form": "공감하네요ㅠ",
          "original_form": "공감하네요ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031918.1.20",
          "form": "그나마  자장은 아주 조사서 ㅋㅋㅋㅋ 만들면 ㅋㅋ",
          "original_form": "그나마  자장은 아주 조사서 ㅋㅋㅋㅋ 만들면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031918.1.21",
          "form": "그니깐ㅎㅎㅎㅎㅎ",
          "original_form": "그니깐ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100031918.1.22",
          "form": "조 ㅅ ㅏ ㅅ ㅓ",
          "original_form": "조 ㅅ ㅏ ㅅ ㅓ",
          "speaker_id": "1",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100031918.1.23",
          "form": "이게  ㅋㅋㅋ",
          "original_form": "이게  ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100031918.1.24",
          "form": "짜장은 엄청좋아해요ㅎ 근데 웃긴건 배달음식은 또 안먹어요 ㅡㅎ 몇번실패했는지ㅠ",
          "original_form": "짜장은 엄청좋아해요ㅎ 근데 웃긴건 배달음식은 또 안먹어요 ㅡㅎ 몇번실패했는지ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100031918.1.25",
          "form": "조사",
          "original_form": "조사",
          "speaker_id": "2",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100031918.1.26",
          "form": "불량단어네 ㅋㅋ",
          "original_form": "불량단어네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100031918.1.27",
          "form": "나쁜말인가보네요ㅎㅎㅎ",
          "original_form": "나쁜말인가보네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100031918.1.28",
          "form": "잘게 부슨다  뜻인데 ㅋㅋㅋ",
          "original_form": "잘게 부슨다  뜻인데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031918.1.29",
          "form": "아 저희앤 배달도 먹는집만!!",
          "original_form": "아 저희앤 배달도 먹는집만!!",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031918.1.30",
          "form": "그니깐요ㅎㅎ",
          "original_form": "그니깐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031918.1.31",
          "form": "사투리라 그런가보네요ㅎ",
          "original_form": "사투리라 그런가보네요ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031918.1.32",
          "form": "그러게영 ㅋㅋ",
          "original_form": "그러게영 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031918.1.33",
          "form": "아아 그럼식비 엄청들겟는데요?",
          "original_form": "아아 그럼식비 엄청들겟는데요?",
          "speaker_id": "2",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031918.1.34",
          "form": "아 별루요",
          "original_form": "아 별루요",
          "speaker_id": "1",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031918.1.35",
          "form": "딱 먹을거만 사고 만들어서",
          "original_form": "딱 먹을거만 사고 만들어서",
          "speaker_id": "1",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031918.1.36",
          "form": "아아아아ㅎㅎ",
          "original_form": "아아아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031918.1.37",
          "form": "맞아 아아님은 고수지ㅎㅎ 순간 깜빡했음요ㅎ",
          "original_form": "맞아 아아님은 고수지ㅎㅎ 순간 깜빡했음요ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031918.1.38",
          "form": "ㅎㅎ re",
          "original_form": "ㅎㅎ re",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031918.1.39",
          "form": "제출?",
          "original_form": "제출?",
          "speaker_id": "2",
