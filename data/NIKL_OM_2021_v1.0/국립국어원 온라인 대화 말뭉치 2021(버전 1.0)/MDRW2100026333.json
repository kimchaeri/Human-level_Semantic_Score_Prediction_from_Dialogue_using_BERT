      "utterance": [
        {
          "id": "MDRW2100026333.1.1",
          "form": "님은 뭐먹으석에ㅛㅇ",
          "original_form": "님은 뭐먹으석에ㅛㅇ",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026333.1.2",
          "form": "머먹었냐고요?",
          "original_form": "머먹었냐고요?",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026333.1.3",
          "form": "뭐먹을거에요",
          "original_form": "뭐먹을거에요",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026333.1.4",
          "form": "근데 뭐먹엇어요",
          "original_form": "근데 뭐먹엇어요",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026333.1.5",
          "form": "둘다 물어봐여지",
          "original_form": "둘다 물어봐여지",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026333.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026333.1.7",
          "form": "아까는..양념고기랑 카레먹음",
          "original_form": "아까는..양념고기랑 카레먹음",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026333.1.8",
          "form": "만약 또 밥읓 먹는다면 또먹을듯",
          "original_form": "만약 또 밥읓 먹는다면 또먹을듯",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026333.1.9",
          "form": "므ㅏㅅ난거 먹엇다",
          "original_form": "므ㅏㅅ난거 먹엇다",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026333.1.10",
          "form": "아 나커리먹고싶음",
          "original_form": "아 나커리먹고싶음",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026333.1.11",
          "form": "카레말거 난이랑 같이",
          "original_form": "카레말거 난이랑 같이",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026333.1.12",
          "form": "커리",
          "original_form": "커리",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026333.1.13",
          "form": "하 나도 ㄹㅇ",
          "original_form": "하 나도 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026333.1.14",
          "form": "먹고싶다",
          "original_form": "먹고싶다",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026333.1.15",
          "form": "안먹은지 오래되서 맛이 기억에서 소멸함",
          "original_form": "안먹은지 오래되서 맛이 기억에서 소멸함",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026333.1.16",
          "form": "커리는 배달없나",
          "original_form": "커리는 배달없나",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026333.1.17",
          "form": "보기라도해야지",
          "original_form": "보기라도해야지",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026333.1.18",
          "form": "헐..있으면좋겠다",
          "original_form": "헐..있으면좋겠다",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026333.1.19",
          "form": "저번에 막 커리",
          "original_form": "저번에 막 커리",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026333.1.20",
          "form": "인터넷주문 막 돌던데",
          "original_form": "인터넷주문 막 돌던데",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026333.1.21",
          "form": "흑",
          "original_form": "흑",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026333.1.22",
          "form": "없음",
          "original_form": "없음",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026333.1.23",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026333.1.24",
          "form": "주뱜에",
          "original_form": "주뱜에",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026333.1.25",
          "form": "타코가게인데",
          "original_form": "타코가게인데",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026333.1.26",
          "form": "배달비 1천원이고",
          "original_form": "배달비 1천원이고",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026333.1.27",
          "form": "최소금액 7천원인데",
          "original_form": "최소금액 7천원인데",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026333.1.28",
          "form": "별점 5점인곳잇음",
          "original_form": "별점 5점인곳잇음",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026333.1.29",
          "form": "다음에 시켜봐야지",
          "original_form": "다음에 시켜봐야지",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026333.1.30",
          "form": "뎅~",
          "original_form": "뎅~",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026333.1.31",
          "form": "맛잇겟다 타코",
          "original_form": "맛잇겟다 타코",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026333.1.32",
          "form": "타코 아니고 타코야키 가게엿고",
          "original_form": "타코 아니고 타코야키 가게엿고",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026333.1.33",
          "form": "아웃겨",
          "original_form": "아웃겨",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026333.1.34",
          "form": "도랏니?",
          "original_form": "도랏니?",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026333.1.35",
          "form": "타코 인 카페라매",
          "original_form": "타코 인 카페라매",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026333.1.36",
          "form": "속앗다..",
          "original_form": "속앗다..",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026333.1.37",
          "form": "다른데는 배달비 3천원이여 너무해",
          "original_form": "다른데는 배달비 3천원이여 너무해",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026333.1.38",
          "form": "비싸다",
          "original_form": "비싸다",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026333.1.39",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026333.1.40",
          "form": "삼천원 너무 비싸",
          "original_form": "삼천원 너무 비싸",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026333.1.41",
          "form": "다이소가면 과자 세개나 삼",
          "original_form": "다이소가면 과자 세개나 삼",
          "speaker_id": "1",
          "time": "20210901 21:43"
        }
      ]
