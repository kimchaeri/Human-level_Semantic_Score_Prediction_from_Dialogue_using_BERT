      "utterance": [
        {
          "id": "MDRW2100044623.1.1",
          "form": "헐 맞아요!! 그릭복숭아 !!!!",
          "original_form": "헐 맞아요!! 그릭복숭아 !!!!",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044623.1.2",
          "form": "네 그릭복숭아",
          "original_form": "네 그릭복숭아",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044623.1.3",
          "form": "저도 그거 해먹어보고싶은데 올해 복숭아를 별로 안먹었네요 그러고보니 ㅋㅋㅋㅋ",
          "original_form": "저도 그거 해먹어보고싶은데 올해 복숭아를 별로 안먹었네요 그러고보니 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044623.1.4",
          "form": "저도 이번 여름엔 복숭아 안먹었어요",
          "original_form": "저도 이번 여름엔 복숭아 안먹었어요",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044623.1.5",
          "form": "원래 복숭아킬러인데",
          "original_form": "원래 복숭아킬러인데",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044623.1.6",
          "form": "헐 저두요!!!! 저도 원래 복숭아 진짜좋아하는데",
          "original_form": "헐 저두요!!!! 저도 원래 복숭아 진짜좋아하는데",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044623.1.7",
          "form": "올해는 그냥 수박 외길걸었네요 ㅋㅋㅋㅋ",
          "original_form": "올해는 그냥 수박 외길걸었네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044623.1.8",
          "form": "아 저는 자두랑",
          "original_form": "아 저는 자두랑",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044623.1.9",
          "form": "스테비아토마토에 빠져서",
          "original_form": "스테비아토마토에 빠져서",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044623.1.10",
          "form": "와 스테비아 토마토가 그렇게 맛있다던ㄷ9",
          "original_form": "와 스테비아 토마토가 그렇게 맛있다던ㄷ9",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044623.1.11",
          "form": "못먹어봣어요!!! ㅠㅠ",
          "original_form": "못먹어봣어요!!! ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044623.1.12",
          "form": "네 진짜 한번 빠지면",
          "original_form": "네 진짜 한번 빠지면",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044623.1.13",
          "form": "일반토마토 못먹어요",
          "original_form": "일반토마토 못먹어요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044623.1.14",
          "form": "스테비아토마토 덕에 아이스크림이나 다른 과일 생각이 안났어요",
          "original_form": "스테비아토마토 덕에 아이스크림이나 다른 과일 생각이 안났어요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044623.1.15",
          "form": "와 그정도에요?!?!?!?!세상에나",
          "original_form": "와 그정도에요?!?!?!?!세상에나",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044623.1.16",
          "form": "저도 진작 먹어봤어야하는디...",
          "original_form": "저도 진작 먹어봤어야하는디...",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044623.1.17",
          "form": "이젠 저희 마트엔 안나오더라고요",
          "original_form": "이젠 저희 마트엔 안나오더라고요",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044623.1.18",
          "form": "여름에 잠깐 나왔나봐요ㅜ",
          "original_form": "여름에 잠깐 나왔나봐요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044623.1.19",
          "form": "으헝 ㅠㅠ 내년을 기약해야겠군요",
          "original_form": "으헝 ㅠㅠ 내년을 기약해야겠군요",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044623.1.20",
          "form": "칼로리나 당은 괜찮나요?",
          "original_form": "칼로리나 당은 괜찮나요?",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044623.1.21",
          "form": "네 스테비아가 당뇨환자에게도 권하는 당류라더라고요",
          "original_form": "네 스테비아가 당뇨환자에게도 권하는 당류라더라고요",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044623.1.22",
          "form": "오오 ㅋㅋㅋ다행이네요 그럼 마음놓고 먹는걸로...!!!",
