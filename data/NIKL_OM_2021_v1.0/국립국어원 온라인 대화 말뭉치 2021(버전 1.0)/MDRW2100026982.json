      "utterance": [
        {
          "id": "MDRW2100026982.1.1",
          "form": "아참. 패딩 산 거는 배송 왔어?",
          "original_form": "아참. 패딩 산 거는 배송 왔어?",
          "speaker_id": "1",
          "time": "20210902 02:34"
        },
        {
          "id": "MDRW2100026982.1.2",
          "form": "응 아까 낮에 왔어~",
          "original_form": "응 아까 낮에 왔어~",
          "speaker_id": "2",
          "time": "20210902 02:34"
        },
        {
          "id": "MDRW2100026982.1.3",
          "form": "어때. 맘에 들어?",
          "original_form": "어때. 맘에 들어?",
          "speaker_id": "1",
          "time": "20210902 02:34"
        },
        {
          "id": "MDRW2100026982.1.4",
          "form": "어 생각보다 엄청 가벼워",
          "original_form": "어 생각보다 엄청 가벼워",
          "speaker_id": "2",
          "time": "20210902 02:34"
        },
        {
          "id": "MDRW2100026982.1.5",
          "form": "다행이네. 사이즈는 잘 맞고?",
          "original_form": "다행이네. 사이즈는 잘 맞고?",
          "speaker_id": "1",
          "time": "20210902 02:35"
        },
        {
          "id": "MDRW2100026982.1.6",
          "form": "응 여성용이라 그런지 엄청 크지는 않더라",
          "original_form": "응 여성용이라 그런지 엄청 크지는 않더라",
          "speaker_id": "2",
          "time": "20210902 02:35"
        },
        {
          "id": "MDRW2100026982.1.7",
          "form": "너무 크면 오히려 안 좋아. 맘에 든다니 다행이네.",
          "original_form": "너무 크면 오히려 안 좋아. 맘에 든다니 다행이네.",
          "speaker_id": "1",
          "time": "20210902 02:35"
        },
        {
          "id": "MDRW2100026982.1.8",
          "form": "ㅋㅋㅋ 근데 모자 탈부착 되는 줄 알았더니 아니더라",
          "original_form": "ㅋㅋㅋ 근데 모자 탈부착 되는 줄 알았더니 아니더라",
          "speaker_id": "2",
          "time": "20210902 02:36"
        },
        {
          "id": "MDRW2100026982.1.9",
          "form": "세일 가격이라 손해본 느낌은 아닌데 정가였으면 절대 안 샀을 것 같아",
          "original_form": "세일 가격이라 손해본 느낌은 아닌데 정가였으면 절대 안 샀을 것 같아",
          "speaker_id": "2",
          "time": "20210902 02:36"
        },
        {
          "id": "MDRW2100026982.1.10",
          "form": "아니야 그정도면 싸게 산 거야. 패딩인데.",
          "original_form": "아니야 그정도면 싸게 산 거야. 패딩인데.",
          "speaker_id": "1",
          "time": "20210902 02:37"
        },
        {
          "id": "MDRW2100026982.1.11",
          "form": "그렇긴 하지 세일 많이 했으니까ㅋㅋㅋㅋ",
          "original_form": "그렇긴 하지 세일 많이 했으니까ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 02:37"
        },
        {
          "id": "MDRW2100026982.1.12",
          "form": "네가 제작년에 사준 엄마 패딩은 아직까지도 잘 입고 다니잖아.",
          "original_form": "네가 제작년에 사준 엄마 패딩은 아직까지도 잘 입고 다니잖아.",
          "speaker_id": "1",
          "time": "20210902 02:38"
        },
        {
          "id": "MDRW2100026982.1.13",
          "form": "그건 좀 더 비쌌지",
          "original_form": "그건 좀 더 비쌌지",
          "speaker_id": "2",
          "time": "20210902 02:38"
        },
        {
          "id": "MDRW2100026982.1.14",
          "form": "엄마 생일 선물이었는데",
          "original_form": "엄마 생일 선물이었는데",
          "speaker_id": "2",
          "time": "20210902 02:38"
        },
        {
          "id": "MDRW2100026982.1.15",
          "form": "딸이 준 선물이었으니까 올해도 잘 입고 다녀야지.",
          "original_form": "딸이 준 선물이었으니까 올해도 잘 입고 다녀야지.",
          "speaker_id": "1",
          "time": "20210902 02:39"
        },
        {
          "id": "MDRW2100026982.1.16",
          "form": "ㅎㅎㅎ 암튼 모자 탈부착 안되는 것 빼곤 맘에 들어. 올 겨울 외투 쇼핑은 끝이야",
          "original_form": "ㅎㅎㅎ 암튼 모자 탈부착 안되는 것 빼곤 맘에 들어. 올 겨울 외투 쇼핑은 끝이야",
          "speaker_id": "2",
          "time": "20210902 02:40"
        }
      ]
