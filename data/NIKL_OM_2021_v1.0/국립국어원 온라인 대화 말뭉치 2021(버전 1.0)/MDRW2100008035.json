        {
          "id": "MDRW2100008035.1.1",
          "form": "전 안좋아해요ㅜ",
          "original_form": "전 안좋아해요ㅜ",
          "speaker_id": "2",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100008035.1.2",
          "form": "name1님은 좋아하세요?",
          "original_form": "&name1&님은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100008035.1.3",
          "form": "가끔 마시면 맛있더라고요",
          "original_form": "가끔 마시면 맛있더라고요",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100008035.1.4",
          "form": "와인 드셔보셨나요?",
          "original_form": "와인 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100008035.1.5",
          "form": "아뇨ㅎ\r\n와인 좋아하세요?",
          "original_form": "아뇨ㅎ\r\n와인 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008035.1.6",
          "form": "저도 스파클링 와인만 몇 번 마셔봤어요",
          "original_form": "저도 스파클링 와인만 몇 번 마셔봤어요",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008035.1.7",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008035.1.8",
          "form": "술을 특별히 안 좋아하는 이유가 있으신가요?",
          "original_form": "술을 특별히 안 좋아하는 이유가 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008035.1.9",
          "form": "알콜 맛? 냄새?",
          "original_form": "알콜 맛? 냄새?",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008035.1.10",
          "form": "이게 저랑 안맞더라구요ㅎㅎㅎ",
          "original_form": "이게 저랑 안맞더라구요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008035.1.11",
          "form": "아 뭔지 알 거 같아요",
          "original_form": "아 뭔지 알 거 같아요",
          "speaker_id": "1",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008035.1.12",
          "form": "그래서 달달한",
          "original_form": "그래서 달달한",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008035.1.13",
          "form": "건 좋아해요ㅎㅎㅎ",
          "original_form": "건 좋아해요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008035.1.14",
          "form": "저도 처음에는 알콜솜 먹는 거 같다 막 그랬어요",
          "original_form": "저도 처음에는 알콜솜 먹는 거 같다 막 그랬어요",
          "speaker_id": "1",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008035.1.15",
          "form": "그럼 칵테일은 좋아하시나요?",
          "original_form": "그럼 칵테일은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008035.1.16",
          "form": "칵테일은 먹어본적이 없어요ㅜ",
          "original_form": "칵테일은 먹어본적이 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008035.1.17",
          "form": "맛있나요??ㅜ",
          "original_form": "맛있나요??ㅜ",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008035.1.18",
          "form": "달달한 것도 있고 상큼한 것도 있어요",
          "original_form": "달달한 것도 있고 상큼한 것도 있어요",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008035.1.19",
          "form": "도수 낮고 음료 같은 것 술 싫어하는 친구도 맛있게 마시더라고요",
          "original_form": "도수 낮고 음료 같은 것 술 싫어하는 친구도 맛있게 마시더라고요",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008035.1.20",
          "form": "다음에 기회되면 먹어봐야겠어요!!·",
          "original_form": "다음에 기회되면 먹어봐야겠어요!!·",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008035.1.21",
          "form": "칵테일바 대부분이 분위기도 좋아서 친구 분들이랑 한번 가보세요!",
          "original_form": "칵테일바 대부분이 분위기도 좋아서 친구 분들이랑 한번 가보세요!",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008035.1.22",
          "form": "하나도 몰라서 가기 어려웠는데 도전해볼께요!!!ㅎㅎ",
