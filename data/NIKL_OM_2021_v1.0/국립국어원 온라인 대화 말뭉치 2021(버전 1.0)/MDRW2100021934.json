      "utterance": [
        {
          "id": "MDRW2100021934.1.1",
          "form": "여름휴가잘다녀오셨어요?",
          "original_form": "여름휴가잘다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021934.1.2",
          "form": "코시국이라 \r\n이번 여름 휴가는 패스했어요",
          "original_form": "코시국이라 \r\n이번 여름 휴가는 패스했어요",
          "speaker_id": "2",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021934.1.3",
          "form": "그럼 집콕하셨겟네요ㅎ",
          "original_form": "그럼 집콕하셨겟네요ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021934.1.4",
          "form": "name1님은 휴가때\r\n어디 다녀오셨나요?",
          "original_form": "&name1&님은 휴가때\r\n어디 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021934.1.5",
          "form": "코로나 끝나면 여행은 해외로ㅡ?",
          "original_form": "코로나 끝나면 여행은 해외로ㅡ?",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021934.1.6",
          "form": "네:) 집콕윽 연속이죠",
          "original_form": "네:) 집콕윽 연속이죠",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021934.1.7",
          "form": "아ㅎ 저는 당일치키로 갔다왔어요ㅎㅎ",
          "original_form": "아ㅎ 저는 당일치키로 갔다왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021934.1.8",
          "form": "장흥으로ㅎㅎ",
          "original_form": "장흥으로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021934.1.9",
          "form": "해외도 가고싶죠@",
          "original_form": "해외도 가고싶죠@",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021934.1.10",
          "form": "오 장흥 어땠나요?",
          "original_form": "오 장흥 어땠나요?",
          "speaker_id": "2",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021934.1.11",
          "form": "좋았어요ㅎㅎ",
          "original_form": "좋았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021934.1.12",
          "form": "뭐가 젤 기억에 남아요?",
          "original_form": "뭐가 젤 기억에 남아요?",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021934.1.13",
          "form": "가서 저희도 코로나라 차에서내려서 잠깐보고 했지만",
          "original_form": "가서 저희도 코로나라 차에서내려서 잠깐보고 했지만",
          "speaker_id": "1",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021934.1.14",
          "form": "괜찮았어요ㅎㅎ",
          "original_form": "괜찮았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021934.1.15",
          "form": "장흥에는 뭐가많이있어서ㅎ 식당가서 삼합도먹었구요",
          "original_form": "장흥에는 뭐가많이있어서ㅎ 식당가서 삼합도먹었구요",
          "speaker_id": "1",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021934.1.16",
          "form": "엄청 외진대서 차세워놓고 놀고 그랬어요ㅎ",
          "original_form": "엄청 외진대서 차세워놓고 놀고 그랬어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021934.1.17",
          "form": "즐거운 여행이셨겠어요:)",
          "original_form": "즐거운 여행이셨겠어요:)",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021934.1.18",
          "form": "가족여행이었나요?",
          "original_form": "가족여행이었나요?",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021934.1.19",
          "form": "네네ㅎㅎ 가족여행갔었어요ㅎ",
          "original_form": "네네ㅎㅎ 가족여행갔었어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021934.1.20",
          "form": "가서 조그만한 동네에 바닷가가있어서ㅎㅎ",
          "original_form": "가서 조그만한 동네에 바닷가가있어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021934.1.21",
          "form": "딸이랑 재밌게 당일로 잘 놀다왔어요ㅎㅎ",
          "original_form": "딸이랑 재밌게 당일로 잘 놀다왔어요ㅎㅎ",
          "speaker_id": "1",
