      "utterance": [
        {
          "id": "MDRW2100035943.1.1",
          "form": "갖고싶은 물건 있으세요?",
          "original_form": "갖고싶은 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210830 15:24"
        },
        {
          "id": "MDRW2100035943.1.2",
          "form": "저 신발, 가방이용!ㅋㅋ",
          "original_form": "저 신발, 가방이용!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:56"
        },
        {
          "id": "MDRW2100035943.1.3",
          "form": "크으 좋죠ㅋㅋㅋ",
          "original_form": "크으 좋죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100035943.1.4",
          "form": "저도 신발 하나 사고 싶네용",
          "original_form": "저도 신발 하나 사고 싶네용",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100035943.1.5",
          "form": "애들이랑 다니면서 물,물티슈,휴대폰",
          "original_form": "애들이랑 다니면서 물,물티슈,휴대폰",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100035943.1.6",
          "form": "넣고 다닐려고 작은 손가방하나",
          "original_form": "넣고 다닐려고 작은 손가방하나",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100035943.1.7",
          "form": "보고있거든요",
          "original_form": "보고있거든요",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100035943.1.8",
          "form": "마르헨제이 아세요 ㅣㅎㅎ",
          "original_form": "마르헨제이 아세요 ㅣㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100035943.1.9",
          "form": "방금 보고왔어요!",
          "original_form": "방금 보고왔어요!",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100035943.1.10",
          "form": "복주머니 너무 귀엽네요?!",
          "original_form": "복주머니 너무 귀엽네요?!",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100035943.1.11",
          "form": "그쵸  너무귀여운거 많아서",
          "original_form": "그쵸  너무귀여운거 많아서",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100035943.1.12",
          "form": "뭘구매하지 계속 보고 또보고있어욤ㅎㅎ",
          "original_form": "뭘구매하지 계속 보고 또보고있어욤ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100035943.1.13",
          "form": "고민중이에요! 빨리사야하는뎈ㅋ",
          "original_form": "고민중이에요! 빨리사야하는뎈ㅋ",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100035943.1.14",
          "form": "고민될만해요ㅋㅋㅋ 저도 잠깐보는데도 뒤로 계속 넘기고 있었어요ㅋㅋ",
          "original_form": "고민될만해요ㅋㅋㅋ 저도 잠깐보는데도 뒤로 계속 넘기고 있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100035943.1.15",
          "form": "맘같아선 종류별로",
          "original_form": "맘같아선 종류별로",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100035943.1.16",
          "form": "마음에드는거 한개씩 사고싶네요^^;;",
          "original_form": "마음에드는거 한개씩 사고싶네요^^;;",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100035943.1.17",
          "form": "진짜요ㅋㅋ",
          "original_form": "진짜요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100035943.1.18",
          "form": "아 날도 추워지니 가디건도 하나 갖고싶어요",
          "original_form": "아 날도 추워지니 가디건도 하나 갖고싶어요",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100035943.1.19",
          "form": "전 애들가디건도 사주고싶은데",
          "original_form": "전 애들가디건도 사주고싶은데",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100035943.1.20",
          "form": "애들껀 왜이렇게 비싼지모르겠어요ㅜㅜ",
          "original_form": "애들껀 왜이렇게 비싼지모르겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100035943.1.21",
          "form": "가디건에 금가루뿌렸나.ㅠㅜ  싸구려는 너무싼티나고 디자인도 별로구요",
          "original_form": "가디건에 금가루뿌렸나.ㅠㅜ  싸구려는 너무싼티나고 디자인도 별로구요",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100035943.1.22",
          "form": "진짜요ㅠ 요즘엔 애들게 더 비싸요",
          "original_form": "진짜요ㅠ 요즘엔 애들게 더 비싸요",
          "speaker_id": "1",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100035943.1.23",
          "form": "원단도 더 적게드는데 말이죠",
          "original_form": "원단도 더 적게드는데 말이죠",
          "speaker_id": "1",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100035943.1.24",
          "form": "그쵸~~ 예쁜거일수록 비싸서ㅠㅠ",
          "original_form": "그쵸~~ 예쁜거일수록 비싸서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100035943.1.25",
          "form": "사고싶은거 다 살수있으면 넘 행복할거같아요~ㅎㅎㅎ",
          "original_form": "사고싶은거 다 살수있으면 넘 행복할거같아요~ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100035943.1.26",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100035943.1.27",
          "form": "그러면 소원이 없겠어요",
