      "utterance": [
        {
          "id": "MDRW2100008359.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008359.1.2",
          "form": "올림픽 보셨어요?",
          "original_form": "올림픽 보셨어요?",
          "speaker_id": "1",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008359.1.3",
          "form": "배구 양궁 펜싱 골프 조금씩 봤어요",
          "original_form": "배구 양궁 펜싱 골프 조금씩 봤어요",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008359.1.4",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008359.1.5",
          "form": "그래도 많이 보셨네요",
          "original_form": "그래도 많이 보셨네요",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008359.1.6",
          "form": "저는 양궁만 좀보고",
          "original_form": "저는 양궁만 좀보고",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008359.1.7",
          "form": "배구 좀 보다 껐어요 ㅎㅎ",
          "original_form": "배구 좀 보다 껐어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008359.1.8",
          "form": "양궁 심박수나오는거 보셨어요?",
          "original_form": "양궁 심박수나오는거 보셨어요?",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008359.1.9",
          "form": "양궁 정말 군더더기 없이 최고로 잘해요",
          "original_form": "양궁 정말 군더더기 없이 최고로 잘해요",
          "speaker_id": "2",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008359.1.10",
          "form": "배구는 속상해서 응원했어요 ㅜㅜ",
          "original_form": "배구는 속상해서 응원했어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008359.1.11",
          "form": "너무신기하지않던가요?",
          "original_form": "너무신기하지않던가요?",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008359.1.12",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008359.1.13",
          "form": "어쩜그리 평온한지...",
          "original_form": "어쩜그리 평온한지...",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008359.1.14",
          "form": "요새 참 대단해요",
          "original_form": "요새 참 대단해요",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008359.1.15",
          "form": "강심장들같았어요",
          "original_form": "강심장들같았어요",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008359.1.16",
          "form": "그런서보니 더 대단해보여요",
          "original_form": "그런서보니 더 대단해보여요",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008359.1.17",
          "form": "저라면 손부터 덜덜했을듯요",
          "original_form": "저라면 손부터 덜덜했을듯요",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008359.1.18",
          "form": "다른나라선수들은 100넘어가는데",
          "original_form": "다른나라선수들은 100넘어가는데",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008359.1.19",
          "form": "우리나라선수들만 80  60 이러더라구요",
          "original_form": "우리나라선수들만 80  60 이러더라구요",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008359.1.20",
          "form": "네 아 진짜 타고났어요",
          "original_form": "네 아 진짜 타고났어요",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008359.1.21",
          "form": "우리나라 최강입니다",
          "original_form": "우리나라 최강입니다",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008359.1.22",
          "form": "존경스럽더라구요",
          "original_form": "존경스럽더라구요",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008359.1.23",
          "form": "앞으로도넘사벽 일것 같아요",
          "original_form": "앞으로도넘사벽 일것 같아요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008359.1.24",
          "form": "어린 안산선수도 넘대단하구요",
          "original_form": "어린 안산선수도 넘대단하구요",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008359.1.25",
          "form": "네 어린아이에 보통 정신력 아니예요",
          "original_form": "네 어린아이에 보통 정신력 아니예요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008359.1.26",
          "form": "텐텐텐 꽂는 김진우? 그 선수도 대단해요",
          "original_form": "텐텐텐 꽂는 김진우? 그 선수도 대단해요",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008359.1.27",
          "form": "옛날 올림픽 카메라 렌즈 막깨고 ㅋㅋ",
          "original_form": "옛날 올림픽 카메라 렌즈 막깨고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008359.1.28",
          "form": "인터뷰하는데 정말 침착하게 말도 잘하더라구요",
          "original_form": "인터뷰하는데 정말 침착하게 말도 잘하더라구요",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008359.1.29",
          "form": "그때도 장난아니였는데 요 ㅋㅋ",
          "original_form": "그때도 장난아니였는데 요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008359.1.30",
          "form": "그러니 잘하나봐요",
          "original_form": "그러니 잘하나봐요",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008359.1.31",
          "form": "렌즈도 깼어요?",
          "original_form": "렌즈도 깼어요?",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008359.1.32",
          "form": "몰랐네요 ㅎ",
          "original_form": "몰랐네요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:34"
        }
      ]
