      "utterance": [
        {
          "id": "MDRW2100038904.1.1",
          "form": "인사이더라는 드라마도 있어",
          "original_form": "인사이더라는 드라마도 있어",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038904.1.2",
          "form": "드라마가 많아 다 보 지 도 못하겠어요",
          "original_form": "드라마가 많아 다 보 지 도 못하겠어요",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038904.1.3",
          "form": "그러게 지금하는것도 다 못보고 있는데",
          "original_form": "그러게 지금하는것도 다 못보고 있는데",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038904.1.4",
          "form": "지금  뭐하는지도 거의 몰라요",
          "original_form": "지금  뭐하는지도 거의 몰라요",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038904.1.5",
          "form": "이 드라마는 잠입수사를 하던 사법연수생이 나락으로",
          "original_form": "이 드라마는 잠입수사를 하던 사법연수생이 나락으로",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038904.1.6",
          "form": "나락으로 빠지는 드라마?",
          "original_form": "나락으로 빠지는 드라마?",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038904.1.7",
          "form": "교도소 도박판에서 고군분투하는 액션",
          "original_form": "교도소 도박판에서 고군분투하는 액션",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038904.1.8",
          "form": "교도소 도박판? ㅋㅋ",
          "original_form": "교도소 도박판? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038904.1.9",
          "form": "강하늘이 사법연수생",
          "original_form": "강하늘이 사법연수생",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038904.1.10",
          "form": "내용이 좀 조잡하다",
          "original_form": "내용이 좀 조잡하다",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038904.1.11",
          "form": "이유영이 ㅋㅋ 강남 화류계의 대마담",
          "original_form": "이유영이 ㅋㅋ 강남 화류계의 대마담",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038904.1.12",
          "form": "아 근데 강하늘이 나오네요",
          "original_form": "아 근데 강하늘이 나오네요",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038904.1.13",
          "form": "ㅋㅋㅋ 뭐지요?",
          "original_form": "ㅋㅋㅋ 뭐지요?",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038904.1.14",
          "form": "엉 강하늘이 교도소 재소자",
          "original_form": "엉 강하늘이 교도소 재소자",
          "speaker_id": "2",
