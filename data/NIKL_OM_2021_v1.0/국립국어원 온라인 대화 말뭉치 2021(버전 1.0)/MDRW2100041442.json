      "utterance": [
        {
          "id": "MDRW2100041442.1.1",
          "form": "요즘도 게임하나?",
          "original_form": "요즘도 게임하나?",
          "speaker_id": "2",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100041442.1.2",
          "form": "고럼고럼",
          "original_form": "고럼고럼",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100041442.1.3",
          "form": "방금까지 꿈의 정원 했어",
          "original_form": "방금까지 꿈의 정원 했어",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100041442.1.4",
          "form": "엄마는 꿈의 집 하는데",
          "original_form": "엄마는 꿈의 집 하는데",
          "speaker_id": "2",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100041442.1.5",
          "form": "너무 어려워",
          "original_form": "너무 어려워",
          "speaker_id": "2",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100041442.1.6",
          "form": "그치?? 나도 그거 하다가 레벨 깨기가 너무 어려워서 꿈의 정원으로 갈아 탐...",
          "original_form": "그치?? 나도 그거 하다가 레벨 깨기가 너무 어려워서 꿈의 정원으로 갈아 탐...",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100041442.1.7",
          "form": "게임인데 너무 어렵네.. 다른 게임 할 거 없나.",
          "original_form": "게임인데 너무 어렵네.. 다른 게임 할 거 없나.",
          "speaker_id": "2",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100041442.1.8",
          "form": "꿈의 정원 해~~ 내가 만든 팀에 가입하면 캐시 드림..",
          "original_form": "꿈의 정원 해~~ 내가 만든 팀에 가입하면 캐시 드림..",
          "speaker_id": "1",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100041442.1.9",
          "form": "그 게임은 이벤트 뭐 있는데?",
          "original_form": "그 게임은 이벤트 뭐 있는데?",
          "speaker_id": "2",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100041442.1.10",
          "form": "꿈의 집이랑 거의 비슷하지ㅋㅋㅋ",
          "original_form": "꿈의 집이랑 거의 비슷하지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100041442.1.11",
          "form": "요즘 보물 상자 이벤트 해",
          "original_form": "요즘 보물 상자 이벤트 해",
          "speaker_id": "1",
          "time": "20210906 00:39"
        },
        {
          "id": "MDRW2100041442.1.12",
          "form": "레벨 깨서 상자 열면 아이템 줌",
          "original_form": "레벨 깨서 상자 열면 아이템 줌",
          "speaker_id": "1",
          "time": "20210906 00:39"
        },
        {
          "id": "MDRW2100041442.1.13",
          "form": "그것도 하다 지겨워서 어플 지웠는데",
          "original_form": "그것도 하다 지겨워서 어플 지웠는데",
          "speaker_id": "2",
          "time": "20210906 00:39"
        },
        {
          "id": "MDRW2100041442.1.14",
          "form": "다시 설치해야겠네",
          "original_form": "다시 설치해야겠네",
          "speaker_id": "2",
          "time": "20210906 00:40"
        },
        {
          "id": "MDRW2100041442.1.15",
          "form": "요즘 이벤트 많이 하니까 다시 시작해ㅋㅋㅋ",
          "original_form": "요즘 이벤트 많이 하니까 다시 시작해ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:40"
        },
        {
          "id": "MDRW2100041442.1.16",
          "form": "난 또 만렙 됨^^v",
          "original_form": "난 또 만렙 됨^^v",
          "speaker_id": "1",
          "time": "20210906 00:40"
        },
        {
          "id": "MDRW2100041442.1.17",
          "form": "맨날 게임만 하는 거 아냐?",
          "original_form": "맨날 게임만 하는 거 아냐?",
          "speaker_id": "2",
          "time": "20210906 00:41"
        },
        {
          "id": "MDRW2100041442.1.18",
          "form": "게임 중독이야 너.",
          "original_form": "게임 중독이야 너.",
          "speaker_id": "2",
          "time": "20210906 00:41"
        },
        {
          "id": "MDRW2100041442.1.19",
          "form": "엄마두 매일 리지니 하면서 뭘ㅋㅋㅋ",
          "original_form": "엄마두 매일 리지니 하면서 뭘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:41"
        },
        {
          "id": "MDRW2100041442.1.20",
          "form": "난 메이플 스토리 안 하는 것만으로도 게임 중독은 아니야",
          "original_form": "난 메이플 스토리 안 하는 것만으로도 게임 중독은 아니야",
          "speaker_id": "1",
          "time": "20210906 00:42"
        },
        {
          "id": "MDRW2100041442.1.21",
          "form": "그러고 보니 요즘 메이플을 안 하는 구나",
          "original_form": "그러고 보니 요즘 메이플을 안 하는 구나",
          "speaker_id": "2",
          "time": "20210906 00:42"
        },
        {
          "id": "MDRW2100041442.1.22",
          "form": "그것도 레벨 높았잖아",
          "original_form": "그것도 레벨 높았잖아",
          "speaker_id": "2",
          "time": "20210906 00:42"
        },
        {
          "id": "MDRW2100041442.1.23",
          "form": "메이플 도대체 언제적 얘기야",
          "original_form": "메이플 도대체 언제적 얘기야",
          "speaker_id": "1",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100041442.1.24",
          "form": "초딩 때 하던 게임이야...",
          "original_form": "초딩 때 하던 게임이야...",
