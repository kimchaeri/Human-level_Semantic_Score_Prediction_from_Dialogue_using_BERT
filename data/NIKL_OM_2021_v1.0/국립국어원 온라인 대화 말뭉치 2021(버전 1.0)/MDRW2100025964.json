      "utterance": [
        {
          "id": "MDRW2100025964.1.1",
          "form": "나 케이크 커피빈 안가고 부산대 앞에서 살라고",
          "original_form": "나 케이크 커피빈 안가고 부산대 앞에서 살라고",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100025964.1.2",
          "form": "왜 커피빈 케이크 먹고 싶다며",
          "original_form": "왜 커피빈 케이크 먹고 싶다며",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100025964.1.3",
          "form": "멀리까지 가서 샀는데 케잌 맛 없음 두런두런할거 아니야",
          "original_form": "멀리까지 가서 샀는데 케잌 맛 없음 두런두런할거 아니야",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100025964.1.4",
          "form": "ㅋㅋㅋ그렇긴하지",
          "original_form": "ㅋㅋㅋ그렇긴하지",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100025964.1.5",
          "form": "그런소리듣기 싫어서",
          "original_form": "그런소리듣기 싫어서",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100025964.1.6",
          "form": "내 생일 케이큰데 말 많을거잖아",
          "original_form": "내 생일 케이큰데 말 많을거잖아",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100025964.1.7",
          "form": "듣기싫음",
          "original_form": "듣기싫음",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100025964.1.8",
          "form": "부산대 앞에가 주차하기 좋아",
          "original_form": "부산대 앞에가 주차하기 좋아",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100025964.1.9",
          "form": "하여튼 이집 사람들 말도 많고 골치아픔",
          "original_form": "하여튼 이집 사람들 말도 많고 골치아픔",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100025964.1.10",
          "form": "독립해라",
          "original_form": "독립해라",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100025964.1.11",
          "form": "할거거든ㅡㅡ",
          "original_form": "할거거든ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100025964.1.12",
          "form": "부산대 앞으로 가니까 케이크는 나 먹고 싶은 걸로 할거야",
          "original_form": "부산대 앞으로 가니까 케이크는 나 먹고 싶은 걸로 할거야",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100025964.1.13",
          "form": "그래라 먹고 싶은 거 먹어",
          "original_form": "그래라 먹고 싶은 거 먹어",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100025964.1.14",
          "form": "ㅇㅇ초코 케잌해도 됨?",
          "original_form": "ㅇㅇ초코 케잌해도 됨?",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100025964.1.15",
          "form": "어쩌다 한번인데 먹고싶은 거 해",
          "original_form": "어쩌다 한번인데 먹고싶은 거 해",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100025964.1.16",
          "form": "맨날 그렇게 말하고 답정너인데 이번엔 내가 고른다 진짜",
          "original_form": "맨날 그렇게 말하고 답정너인데 이번엔 내가 고른다 진짜",
