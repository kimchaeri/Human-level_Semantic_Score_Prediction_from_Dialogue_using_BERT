      "utterance": [
        {
          "id": "MDRW2100004180.1.1",
          "form": "우울할 때 먹는 소울푸드 있어?",
          "original_form": "우울할 때 먹는 소울푸드 있어?",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004180.1.2",
          "form": "달달한 초콜릿이나 아이스크림 먹으면 기분 좋아져",
          "original_form": "달달한 초콜릿이나 아이스크림 먹으면 기분 좋아져",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004180.1.3",
          "form": "초콜릿 맛있지",
          "original_form": "초콜릿 맛있지",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004180.1.4",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004180.1.5",
          "form": "나는 매운 음식",
          "original_form": "나는 매운 음식",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004180.1.6",
          "form": "아 맞다 매운 거 먹으면 확 풀리지",
          "original_form": "아 맞다 매운 거 먹으면 확 풀리지",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004180.1.7",
          "form": "땀 쭉 빼고 나면",
          "original_form": "땀 쭉 빼고 나면",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004180.1.8",
          "form": "기분 좋아져",
          "original_form": "기분 좋아져",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004180.1.9",
          "form": "떡볶이 불닭볶음면 그런거",
          "original_form": "떡볶이 불닭볶음면 그런거",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004180.1.10",
          "form": "응응 맞아",
          "original_form": "응응 맞아",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004180.1.11",
          "form": "불닭도",
          "original_form": "불닭도",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004180.1.12",
          "form": "닭발도 불닭발",
          "original_form": "닭발도 불닭발",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004180.1.13",
          "form": "불족발도 있다",
          "original_form": "불족발도 있다",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004180.1.14",
          "form": "매운 거 먹으면 왜 기분이 좋아지지 ㅋㅋ",
          "original_form": "매운 거 먹으면 왜 기분이 좋아지지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004180.1.15",
