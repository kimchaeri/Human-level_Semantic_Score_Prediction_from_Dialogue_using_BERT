      "utterance": [
        {
          "id": "MDRW2100036832.1.1",
          "form": "점심 먹으니 후식이 땡기네용",
          "original_form": "점심 먹으니 후식이 땡기네용",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036832.1.2",
          "form": "점심 맛있게드셨어요?",
          "original_form": "점심 맛있게드셨어요?",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036832.1.3",
          "form": "저는 간단히 챙겨먹었어요",
          "original_form": "저는 간단히 챙겨먹었어요",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036832.1.4",
          "form": "넹! 어떤거 드셨어오??",
          "original_form": "넹! 어떤거 드셨어오??",
          "speaker_id": "2",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100036832.1.5",
          "form": "그냥 집에 있는 밥에 계란말이랑 잔파랑 먹었어요",
          "original_form": "그냥 집에 있는 밥에 계란말이랑 잔파랑 먹었어요",
          "speaker_id": "1",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100036832.1.6",
          "form": "우왕 계란말이 맛있으셨겠어요",
          "original_form": "우왕 계란말이 맛있으셨겠어요",
          "speaker_id": "2",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036832.1.7",
          "form": "계란말이 맛있게하는 법 있나요?!",
          "original_form": "계란말이 맛있게하는 법 있나요?!",
          "speaker_id": "2",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036832.1.8",
          "form": "그냥저냥했어요",
          "original_form": "그냥저냥했어요",
          "speaker_id": "1",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036832.1.9",
          "form": "모르겠어요 엄마가한거여서요",
          "original_form": "모르겠어요 엄마가한거여서요",
          "speaker_id": "1",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036832.1.10",
          "form": "아항 엄마표는 진짜 뭐든 맛있어요",
          "original_form": "아항 엄마표는 진짜 뭐든 맛있어요",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100036832.1.11",
          "form": "후식 뭐드시고싶으세요?",
          "original_form": "후식 뭐드시고싶으세요?",
          "speaker_id": "1",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100036832.1.12",
          "form": "달달한게 먹고싶었는데 수박먹었어오!",
          "original_form": "달달한게 먹고싶었는데 수박먹었어오!",
          "speaker_id": "2",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100036832.1.13",
          "form": "오 수박 맛있겠어요!!!",
          "original_form": "오 수박 맛있겠어요!!!",
          "speaker_id": "1",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100036832.1.14",
          "form": "넹! 끝자락이라 맛이 뛰어나진 않아도 꽤 맛있더라구용",
          "original_form": "넹! 끝자락이라 맛이 뛰어나진 않아도 꽤 맛있더라구용",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100036832.1.15",
          "form": "올해 수박 많이 드셨어요?",
          "original_form": "올해 수박 많이 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100036832.1.16",
          "form": "아뇨 두세번 먹은게 다에요 ㅜㅜ",
          "original_form": "아뇨 두세번 먹은게 다에요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:50"
        },
