        {
          "id": "MDRW2100026204.1.3",
          "form": "나도 배추 좋아",
          "original_form": "나도 배추 좋아",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026204.1.4",
          "form": "청경채도 좋아해",
          "original_form": "청경채도 좋아해",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026204.1.5",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026204.1.6",
          "form": "나도 숙주는 좀 빼고",
          "original_form": "나도 숙주는 좀 빼고",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026204.1.7",
          "form": "숙주 안 좋아하니?",
          "original_form": "숙주 안 좋아하니?",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026204.1.8",
          "form": "좋긴한대",
          "original_form": "좋긴한대",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026204.1.9",
          "form": "그 배보다는 단배추로",
          "original_form": "그 배보다는 단배추로",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026204.1.10",
          "form": "배채우고싶어",
          "original_form": "배채우고싶어",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026204.1.11",
          "form": "ㅋㅋㅋ그렇구만 난 대부분의 야채가 다 좋아",
          "original_form": "ㅋㅋㅋ그렇구만 난 대부분의 야채가 다 좋아",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026204.1.12",
          "form": "넌 피쉬볼 먹니",
          "original_form": "넌 피쉬볼 먹니",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026204.1.13",
          "form": "난 어묵안먹어",
          "original_form": "난 어묵안먹어",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026204.1.14",
          "form": "나도 안 먹어",
          "original_form": "나도 안 먹어",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026204.1.15",
          "form": "만두도 안 먹니",
          "original_form": "만두도 안 먹니",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026204.1.16",
          "form": "마라탕에도",
          "original_form": "마라탕에도",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026204.1.17",
          "form": "용납불가",
          "original_form": "용납불가",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026204.1.18",
          "form": "나도 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛이 따로 놀아",
          "original_form": "나도 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛이 따로 놀아",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026204.1.19",
          "form": "버섯도 먹니",
          "original_form": "버섯도 먹니",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026204.1.20",
          "form": "버섯...팽이버섯정도?",
          "original_form": "버섯...팽이버섯정도?",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026204.1.21",
          "form": "면적이크면",
          "original_form": "면적이크면",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026204.1.22",
          "form": "별로더라",
          "original_form": "별로더라",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026204.1.23",
          "form": "난 버섯까지는 그래도 용납 가능",
          "original_form": "난 버섯까지는 그래도 용납 가능",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026204.1.24",
          "form": "피쉬볼이랑 만두는 뭔가 걸리적거려",
          "original_form": "피쉬볼이랑 만두는 뭔가 걸리적거려",
          "speaker_id": "1",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026204.1.25",
          "form": "맞아 담백한게 좋은데",
          "original_form": "맞아 담백한게 좋은데",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026204.1.26",
          "form": "너무 많은맛이라",
          "original_form": "너무 많은맛이라",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026204.1.27",
          "form": "맞아 그냥 소고기가 최고야",
          "original_form": "맞아 그냥 소고기가 최고야",
          "speaker_id": "1",
          "time": "20210901 20:57"
        }
      ]
