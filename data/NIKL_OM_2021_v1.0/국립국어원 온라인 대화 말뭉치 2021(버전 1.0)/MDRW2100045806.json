      "utterance": [
        {
          "id": "MDRW2100045806.1.1",
          "form": "지금 날씨가 어떤가요",
          "original_form": "지금 날씨가 어떤가요",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045806.1.2",
          "form": "흐리네요",
          "original_form": "흐리네요",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100045806.1.3",
          "form": "오늘도 비 올 거 같아요.",
          "original_form": "오늘도 비 올 거 같아요.",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100045806.1.4",
          "form": "비가 그치고 흐린 날 이네요",
          "original_form": "비가 그치고 흐린 날 이네요",
          "speaker_id": "1",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100045806.1.5",
          "form": "비 좀 그만 왔으면 좋셌어요",
          "original_form": "비 좀 그만 왔으면 좋셌어요",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045806.1.6",
          "form": "이번주는 그만 온다고 하는것 같네요",
          "original_form": "이번주는 그만 온다고 하는것 같네요",
          "speaker_id": "1",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045806.1.7",
          "form": "다행이네요.",
          "original_form": "다행이네요.",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045806.1.8",
          "form": "비가 계속오니 맘도 우울하네요",
          "original_form": "비가 계속오니 맘도 우울하네요",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045806.1.9",
          "form": "그쵸.쨍한 날 보고 싶어요.",
          "original_form": "그쵸.쨍한 날 보고 싶어요.",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045806.1.10",
          "form": "해가 넘 그립네요",
          "original_form": "해가 넘 그립네요",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045806.1.11",
          "form": "여긴 엄청 흐린 날씨네요",
          "original_form": "여긴 엄청 흐린 날씨네요",
          "speaker_id": "1",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100045806.1.12",
          "form": "비올거 같나요?",
          "original_form": "비올거 같나요?",
          "speaker_id": "2",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100045806.1.13",
          "form": "비는 그쳤지만 또 올것같은 날씨네요",
          "original_form": "비는 그쳤지만 또 올것같은 날씨네요",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045806.1.14",
          "form": "여기는 맑아 질거 같아요",
          "original_form": "여기는 맑아 질거 같아요",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045806.1.15",
          "form": "맑은 하늘을 볼수 있겠네요",
          "original_form": "맑은 하늘을 볼수 있겠네요",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045806.1.16",
