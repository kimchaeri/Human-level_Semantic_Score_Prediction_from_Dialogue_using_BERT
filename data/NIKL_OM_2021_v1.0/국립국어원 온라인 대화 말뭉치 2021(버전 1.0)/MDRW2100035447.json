      "utterance": [
        {
          "id": "MDRW2100035447.1.1",
          "form": "점심은 뭐 드셨어요?",
          "original_form": "점심은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100035447.1.2",
          "form": "돼지국밥 먹었어요!",
          "original_form": "돼지국밥 먹었어요!",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100035447.1.3",
          "form": "점심 뭐 드셨어요?",
          "original_form": "점심 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100035447.1.4",
          "form": "오 ! 국밥 맛있는데 ㅎㅎ",
          "original_form": "오 ! 국밥 맛있는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100035447.1.5",
          "form": "전 짬뽕이요ㅎㅎ",
          "original_form": "전 짬뽕이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100035447.1.6",
          "form": "아아ㅏ아 짬뽕 먹고싶어요...",
          "original_form": "아아ㅏ아 짬뽕 먹고싶어요...",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100035447.1.7",
          "form": "밖에 비 와서 배달 시키기 좀 그러네요ㅠㅠ",
          "original_form": "밖에 비 와서 배달 시키기 좀 그러네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100035447.1.8",
          "form": "아 비와요??? 여기는 아예 안와요 !!",
          "original_form": "아 비와요??? 여기는 아예 안와요 !!",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100035447.1.9",
          "form": "아까는 비 안왔는데 가자기 오네요ㅠ",
          "original_form": "아까는 비 안왔는데 가자기 오네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100035447.1.10",
          "form": "짬뽕 먹고싶은데ㅠ ㅋㅋㅋㅋㅋ",
          "original_form": "짬뽕 먹고싶은데ㅠ ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100035447.1.11",
          "form": "저녁은 뭘 드실 예정인가요?",
          "original_form": "저녁은 뭘 드실 예정인가요?",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100035447.1.12",
          "form": "아직까지 배가 불러서 ㅋㅋ 가볍게 먹으려구요 과일같은거 !",
          "original_form": "아직까지 배가 불러서 ㅋㅋ 가볍게 먹으려구요 과일같은거 !",
          "speaker_id": "1",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100035447.1.13",
          "form": "저녁은 뭐 드실꺼에요?",
          "original_form": "저녁은 뭐 드실꺼에요?",
          "speaker_id": "1",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100035447.1.14",
          "form": "아직 고민중이에요 ㅎㅎ",
          "original_form": "아직 고민중이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100035447.1.15",
          "form": "저 아까 복숭아 먹었는데 복숭아 요즘 마무리 철이라서 달고 맛있었어요!",
          "original_form": "저 아까 복숭아 먹었는데 복숭아 요즘 마무리 철이라서 달고 맛있었어요!",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100035447.1.16",
          "form": "저 복숭아 엄청 좋아해요 ㅠㅠㅡ !!",
          "original_form": "저 복숭아 엄청 좋아해요 ㅠㅠㅡ !!",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100035447.1.17",
          "form": "이번 여름에 진짜 하루에 하나씩은 꼭 먹었는데 ㅋㅋ",
          "original_form": "이번 여름에 진짜 하루에 하나씩은 꼭 먹었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100035447.1.18",
          "form": "지금도 마무리철인데 괜찮군요???!",
          "original_form": "지금도 마무리철인데 괜찮군요???!",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100035447.1.19",
          "form": "더 달달하고 맛있는 것 같아요!",
          "original_form": "더 달달하고 맛있는 것 같아요!",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035447.1.20",
          "form": "오 마트 한번 들려야겠네요 ㅋㅋ 또 내년까지 못먹으니까ㅜㅜ",
          "original_form": "오 마트 한번 들려야겠네요 ㅋㅋ 또 내년까지 못먹으니까ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035447.1.21",
          "form": "과일 좋아하세요?",
          "original_form": "과일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035447.1.22",
          "form": "좋아해요!",
          "original_form": "좋아해요!",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100035447.1.23",
          "form": "과일 좋아하세요?",
          "original_form": "과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100035447.1.24",
          "form": "네 ㅎㅎ 사랑합니다 과일 ㅎㅎ",
          "original_form": "네 ㅎㅎ 사랑합니다 과일 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100035447.1.25",
          "form": "어떤 과일 젤 좋아하세요?",
          "original_form": "어떤 과일 젤 좋아하세요?",
