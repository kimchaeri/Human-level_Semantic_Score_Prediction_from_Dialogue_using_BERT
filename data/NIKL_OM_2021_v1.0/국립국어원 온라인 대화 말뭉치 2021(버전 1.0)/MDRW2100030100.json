      "utterance": [
        {
          "id": "MDRW2100030100.1.1",
          "form": "독서 좋아하시나요?",
          "original_form": "독서 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030100.1.2",
          "form": "일주일에 한권은 읽으려고 노력하고 있어요!",
          "original_form": "일주일에 한권은 읽으려고 노력하고 있어요!",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030100.1.3",
          "form": "책자주읽으세요?",
          "original_form": "책자주읽으세요?",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030100.1.4",
          "form": "아니요ㅠ저는 사실 안읽어요 ㅋㅋㅋ",
          "original_form": "아니요ㅠ저는 사실 안읽어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100030100.1.5",
          "form": "도서관을 애용하시나요?",
          "original_form": "도서관을 애용하시나요?",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100030100.1.6",
          "form": "아뇨ㅎㅎㅎ\r\n밀리이용권끊었다가 괜찮은 책은 직접사서봐요ㅎ",
          "original_form": "아뇨ㅎㅎㅎ\r\n밀리이용권끊었다가 괜찮은 책은 직접사서봐요ㅎ",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100030100.1.7",
          "form": "아 밀리 좋죠 ㅋㅋㅋㅋ 보통 밀리 플랫폼에서 구매하시나요???",
          "original_form": "아 밀리 좋죠 ㅋㅋㅋㅋ 보통 밀리 플랫폼에서 구매하시나요???",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030100.1.8",
          "form": "네ㅎㅎ 한달 기본이용료로 사용중인데",
          "original_form": "네ㅎㅎ 한달 기본이용료로 사용중인데",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030100.1.9",
          "form": "잘안써지더라구요ㅎㅎ",
          "original_form": "잘안써지더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030100.1.10",
          "form": "종이책이 확실히 좋네요ㅋ",
          "original_form": "종이책이 확실히 좋네요ㅋ",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030100.1.11",
          "form": "아 가독성이 좀 떨어지죠",
          "original_form": "아 가독성이 좀 떨어지죠",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030100.1.12",
          "form": "소설은 읽기 좋던데요",
          "original_form": "소설은 읽기 좋던데요",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030100.1.13",
          "form": "보통 어떤 계열을 읽으세요?",
          "original_form": "보통 어떤 계열을 읽으세요?",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030100.1.14",
          "form": "사회문화와 재태크요",
          "original_form": "사회문화와 재태크요",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030100.1.15",
          "form": "최근보는건 살아남은 아이라고",
          "original_form": "최근보는건 살아남은 아이라고",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030100.1.16",
          "form": "형제복지원피해자가 쓴 책이고",
          "original_form": "형제복지원피해자가 쓴 책이고",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030100.1.17",
          "form": "요한 씨돌 용현이라고",
          "original_form": "요한 씨돌 용현이라고",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030100.1.18",
          "form": "하는책 보고 있어요ㅎㅎ",
          "original_form": "하는책 보고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030100.1.19",
          "form": "아 다큐멘터리로 본 적 있는 것 같아요",
          "original_form": "아 다큐멘터리로 본 적 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030100.1.20",
          "form": "사회이슈에 관심이 많으시군요 ㅇ0ㅇ",
          "original_form": "사회이슈에 관심이 많으시군요 ㅇ0ㅇ",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030100.1.21",
          "form": "읽고나면 오랫동안 후유증이 남을 것 같아요",
          "original_form": "읽고나면 오랫동안 후유증이 남을 것 같아요",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030100.1.22",
          "form": "맞아요ㅜㅜ\r\n아직 치유되지 않은 부분이라..읽으면서도 감정조절이 잘 안되더라구요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ\r\n아직 치유되지 않은 부분이라..읽으면서도 감정조절이 잘 안되더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030100.1.23",
          "form": "그래서 고양이 유튭으로 힐링하고 다시봐요...ㅋㅋㅋ",
          "original_form": "그래서 고양이 유튭으로 힐링하고 다시봐요...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100030100.1.24",
          "form": "아 조절 잘하시네요ㅠㅠㅠㅠ",
          "original_form": "아 조절 잘하시네요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030100.1.25",
          "form": "너무 화가 나는 내용이라ㅠㅠ",
          "original_form": "너무 화가 나는 내용이라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030100.1.26",
          "form": "정신적으로 너무 갉아먹히는 것 같아서 힘들죠ㅠㅠ",
          "original_form": "정신적으로 너무 갉아먹히는 것 같아서 힘들죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030100.1.27",
          "form": "오래된 일도 아니라서 더요...",
          "original_form": "오래된 일도 아니라서 더요...",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030100.1.28",
          "form": "네..근데 안보려고 해도 한글귀가 계속 보이더라구요",
          "original_form": "네..근데 안보려고 해도 한글귀가 계속 보이더라구요",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030100.1.29",
          "form": "어떤 글귀요...?",
          "original_form": "어떤 글귀요...?",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030100.1.30",
          "form": "제 얘기좀 들어주세요 라는문구가",
          "original_form": "제 얘기좀 들어주세요 라는문구가",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030100.1.31",
          "form": "그냥 아파서 안볼 수 없더라구요",
          "original_form": "그냥 아파서 안볼 수 없더라구요",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030100.1.32",
          "form": "아ㅠㅠㅠㅠㅠ",
          "original_form": "아ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030100.1.33",
          "form": "마음이 아프네요ㅠㅠㅠ",
          "original_form": "마음이 아프네요ㅠㅠㅠ",
