      "utterance": [
        {
          "id": "MDRW2100047183.1.1",
          "form": "네... 호로요이였나 일본 술 한모금 하고 뻗었어요ㅠ",
          "original_form": "네... 호로요이였나 일본 술 한모금 하고 뻗었어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100047183.1.2",
          "form": "와우ㅋㅋㅋ",
          "original_form": "와우ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100047183.1.3",
          "form": "저보다 더하시네요?!",
          "original_form": "저보다 더하시네요?!",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100047183.1.4",
          "form": "name1님도 술 못하세요?",
          "original_form": "&name1&님도 술 못하세요?",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100047183.1.5",
          "form": "전혀요ㅋㅋㅋㅋ",
          "original_form": "전혀요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100047183.1.6",
          "form": "단술이좋더라고요 그나마",
          "original_form": "단술이좋더라고요 그나마",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100047183.1.7",
          "form": "맞아요 호로요이 분명 달고 맛있었는데... 쓰읍",
          "original_form": "맞아요 호로요이 분명 달고 맛있었는데... 쓰읍",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100047183.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100047183.1.9",
          "form": "알쓰들의 대화군요",
          "original_form": "알쓰들의 대화군요",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100047183.1.10",
          "form": "저 사실 저 말고 다른 알쓰 처음 봐요",
          "original_form": "저 사실 저 말고 다른 알쓰 처음 봐요",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100047183.1.11",
          "form": "주변 사람들은 다 잘 마셔서ㅠ",
          "original_form": "주변 사람들은 다 잘 마셔서ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100047183.1.12",
          "form": "ㅋㅋㅋㅋ요즘",
          "original_form": "ㅋㅋㅋㅋ요즘",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100047183.1.13",
          "form": "진짜다들",
          "original_form": "진짜다들",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100047183.1.14",
          "form": "잘마시긴하더라고요",
          "original_form": "잘마시긴하더라고요",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100047183.1.15",
          "form": "술찌들이랑만 다니고 싶어요",
          "original_form": "술찌들이랑만 다니고 싶어요",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047183.1.16",
          "form": "다들 만나면 술마시자고 해서...",
          "original_form": "다들 만나면 술마시자고 해서...",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047183.1.17",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:08"
        },
        {
          "id": "MDRW2100047183.1.18",
          "form": "저네요!",
          "original_form": "저네요!",
          "speaker_id": "1",
          "time": "20210827 00:08"
        },
        {
          "id": "MDRW2100047183.1.19",
          "form": "술 안마시고 노는 문화가 생겼으면 좋겠어요",
          "original_form": "술 안마시고 노는 문화가 생겼으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100047183.1.20",
          "form": "우리나라는",
          "original_form": "우리나라는",
          "speaker_id": "1",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100047183.1.21",
          "form": "그런게 참 안되어있는거같아요",
          "original_form": "그런게 참 안되어있는거같아요",
          "speaker_id": "1",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100047183.1.22",
          "form": "맨정신으로도 술 텐션 만들 수 있는데...",
          "original_form": "맨정신으로도 술 텐션 만들 수 있는데...",
          "speaker_id": "2",
          "time": "20210827 00:12"
        }
