      "utterance": [
        {
          "id": "MDRW2100018628.1.1",
          "form": "어제 저녁메뉴는 뭔가요?",
          "original_form": "어제 저녁메뉴는 뭔가요?",
          "speaker_id": "1",
          "time": "20210827 09:23"
        },
        {
          "id": "MDRW2100018628.1.2",
          "form": "떡볶이 먹었어요",
          "original_form": "떡볶이 먹었어요",
          "speaker_id": "2",
          "time": "20210827 09:23"
        },
        {
          "id": "MDRW2100018628.1.3",
          "form": "우와 떡볶이 맛있겠네요",
          "original_form": "우와 떡볶이 맛있겠네요",
          "speaker_id": "1",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100018628.1.4",
          "form": "배달 시켜드신건가요?",
          "original_form": "배달 시켜드신건가요?",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100018628.1.5",
          "form": "아뇨 시장에서 사먹었어요",
          "original_form": "아뇨 시장에서 사먹었어요",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100018628.1.6",
          "form": "오 시장 떡볶이 좋죠",
          "original_form": "오 시장 떡볶이 좋죠",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100018628.1.7",
          "form": "오늘 오랜만에 치킨 먹었는데 너무 맛있었어요",
          "original_form": "오늘 오랜만에 치킨 먹었는데 너무 맛있었어요",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100018628.1.8",
          "form": "님은 최근에 드신 것 중에 맛있었던거 있나요?",
          "original_form": "님은 최근에 드신 것 중에 맛있었던거 있나요?",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100018628.1.9",
          "form": "노랑통닭이요",
          "original_form": "노랑통닭이요",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100018628.1.10",
          "form": "맛있어서 요새 자주 먹어요",
          "original_form": "맛있어서 요새 자주 먹어요",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100018628.1.11",
          "form": "노랑통닭 진짜 맛있죠",
          "original_form": "노랑통닭 진짜 맛있죠",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100018628.1.12",
          "form": "노랑통닭에서 파는 떡볶이 드셔보셨나요",
          "original_form": "노랑통닭에서 파는 떡볶이 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100018628.1.13",
          "form": "국물에 찍어먹으면 느끼한게 싹 사라져요. 너무맛있어요",
          "original_form": "국물에 찍어먹으면 느끼한게 싹 사라져요. 너무맛있어요",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100018628.1.14",
          "form": "몰랐어요",
          "original_form": "몰랐어요",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100018628.1.15",
          "form": "치킨만 시켜먹어서ㅋㅋㅋ",
          "original_form": "치킨만 시켜먹어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100018628.1.16",
          "form": "7천원이에요 비싼데 한번 여유되실 때 드셔보세요..\n추천합니다",
          "original_form": "7천원이에요 비싼데 한번 여유되실 때 드셔보세요..\n추천합니다",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100018628.1.17",
          "form": "오... 뭔가 기대되네요. 한번 먹어볼게요ㅋㅋ",
          "original_form": "오... 뭔가 기대되네요. 한번 먹어볼게요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100018628.1.18",
          "form": "네 하하\n저도 맛있는거 추천해주세요~",
          "original_form": "네 하하\n저도 맛있는거 추천해주세요~",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100018628.1.19",
          "form": "요새 알볼로 피자 자주 먹는데 맛있더라구요",
          "original_form": "요새 알볼로 피자 자주 먹는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100018628.1.20",
          "form": "알볼로 피자 맛있겠네요. 거기는 어떤 메뉴가 맛있나요?",
          "original_form": "알볼로 피자 맛있겠네요. 거기는 어떤 메뉴가 맛있나요?",
          "speaker_id": "1",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100018628.1.21",
          "form": "메뉴가 되게 다양하던데 즐겨먹는 메뉴 있으신가요?",
          "original_form": "메뉴가 되게 다양하던데 즐겨먹는 메뉴 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100018628.1.22",
          "form": "저는 옥수수 피자요. 콘치즈 좋아하는데 잔뜩 올라가서 맛있어요",
          "original_form": "저는 옥수수 피자요. 콘치즈 좋아하는데 잔뜩 올라가서 맛있어요",
          "speaker_id": "2",
          "time": "20210828 10:04"
        }
      ]
    }
