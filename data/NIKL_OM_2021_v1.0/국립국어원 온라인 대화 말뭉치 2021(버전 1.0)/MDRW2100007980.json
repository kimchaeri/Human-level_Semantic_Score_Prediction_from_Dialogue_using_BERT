      "utterance": [
        {
          "id": "MDRW2100007980.1.1",
          "form": "첫학기 비대면으로 결정남?",
          "original_form": "첫학기 비대면으로 결정남?",
          "speaker_id": "1",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007980.1.2",
          "form": "아직 모르겠어 난 개강일도 몰라",
          "original_form": "아직 모르겠어 난 개강일도 몰라",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007980.1.3",
          "form": "뭔소리야",
          "original_form": "뭔소리야",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007980.1.4",
          "form": "홈페이지 들어가봐",
          "original_form": "홈페이지 들어가봐",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007980.1.5",
          "form": "...다음주에 오티한다니까 그때들어봐야지",
          "original_form": "...다음주에 오티한다니까 그때들어봐야지",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007980.1.6",
          "form": "오티를 비대면으로 해?",
          "original_form": "오티를 비대면으로 해?",
          "speaker_id": "1",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007980.1.7",
          "form": "웅웅 줌으로한대",
          "original_form": "웅웅 줌으로한대",
          "speaker_id": "2",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100007980.1.8",
          "form": "와 이거 많이튕기는구나",
          "original_form": "와 이거 많이튕기는구나",
          "speaker_id": "2",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100007980.1.9",
          "form": "너네 과 몇 명인지는 알아?",
          "original_form": "너네 과 몇 명인지는 알아?",
          "speaker_id": "1",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100007980.1.10",
          "form": "놉",
          "original_form": "놉",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007980.1.11",
          "form": "아무것도 아는게 없당",
          "original_form": "아무것도 아는게 없당",
          "speaker_id": "2",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100007980.1.12",
          "form": "그럼 수강신청은 함?",
          "original_form": "그럼 수강신청은 함?",
          "speaker_id": "1",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100007980.1.13",
          "form": "하긴했는데",
          "original_form": "하긴했는데",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100007980.1.14",
          "form": "오티듣고 재신청해야할걸",
          "original_form": "오티듣고 재신청해야할걸",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100007980.1.15",
          "form": "난 담주 토요일 개강ㅠ",
          "original_form": "난 담주 토요일 개강ㅠ",
          "speaker_id": "1",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100007980.1.16",
          "form": "담주토욜이면 28일??",
          "original_form": "담주토욜이면 28일??",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100007980.1.17",
          "form": "나 30일이사해",
          "original_form": "나 30일이사해",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100007980.1.18",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100007980.1.19",
          "form": "개 강 이 왜 나 쁜 말?",
          "original_form": "개 강 이 왜 나 쁜 말?",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100007980.1.20",
          "form": "학교근처로 가고싶었는데",
          "original_form": "학교근처로 가고싶었는데",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100007980.1.21",
          "form": "개강 맞춰서 이사",
          "original_form": "개강 맞춰서 이사",
