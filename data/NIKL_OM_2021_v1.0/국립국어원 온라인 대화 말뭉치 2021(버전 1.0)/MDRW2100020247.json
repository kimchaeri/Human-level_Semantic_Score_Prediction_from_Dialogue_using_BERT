          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.2",
          "form": "아무튼",
          "original_form": "아무튼",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.3",
          "form": "얼마전에",
          "original_form": "얼마전에",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.4",
          "form": "트위터에서 누가",
          "original_form": "트위터에서 누가",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.5",
          "form": "15평 정도 되는 집에 독립해서",
          "original_form": "15평 정도 되는 집에 독립해서",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.6",
          "form": "한달에 한번 뮤짘컬보러가고",
          "original_form": "한달에 한번 뮤짘컬보러가고",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.7",
          "form": "1년에 한번 여행가고",
          "original_form": "1년에 한번 여행가고",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.8",
          "form": "도랐네",
          "original_form": "도랐네",
          "speaker_id": "1",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.9",
          "form": "과일먹고 주말마다 영화보는",
          "original_form": "과일먹고 주말마다 영화보는",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.10",
          "form": "그런 평범한 삶을",
          "original_form": "그런 평범한 삶을",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020247.1.11",
          "form": "살고싶은데 그게 너무 어렵다고",
          "original_form": "살고싶은데 그게 너무 어렵다고",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020247.1.12",
          "form": "하는데 공감",
          "original_form": "하는데 공감",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020247.1.13",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020247.1.14",
          "form": "그게 평범으로",
          "original_form": "그게 평범으로",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020247.1.15",
          "form": "그려지는것도 새삼 참",
          "original_form": "그려지는것도 새삼 참",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020247.1.16",
          "form": "이상한거같다",
          "original_form": "이상한거같다",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020247.1.17",
          "form": "평범이 맞긴하나",
          "original_form": "평범이 맞긴하나",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020247.1.18",
          "form": "우리나라에선 솔직히",
          "original_form": "우리나라에선 솔직히",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020247.1.19",
          "form": "평범아닌데",
          "original_form": "평범아닌데",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020247.1.20",
          "form": "사람들이 말하는 기본적인 삶이",
          "original_form": "사람들이 말하는 기본적인 삶이",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.21",
          "form": "저 수준이라서",
          "original_form": "저 수준이라서",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.22",
          "form": "국가에갸도 막",
          "original_form": "국가에갸도 막",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.23",
          "form": "문화의날 이런거 다지면서",
          "original_form": "문화의날 이런거 다지면서",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.24",
          "form": "공연이나 영화관람 이렁거 챙기게하는데",
          "original_form": "공연이나 영화관람 이렁거 챙기게하는데",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.25",
          "form": "실제론 그거하는갸도ㅠ어려운 사람 개만아",
          "original_form": "실제론 그거하는갸도ㅠ어려운 사람 개만아",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.26",
          "form": "그것도",
          "original_form": "그것도",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.27",
          "form": "누릴 여유가되야",
          "original_form": "누릴 여유가되야",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.28",
          "form": "누리지",
          "original_form": "누리지",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.29",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.30",
          "form": "나 오줌타임",
          "original_form": "나 오줌타임",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.31",
          "form": "사치라고 느끼는 사람이 얼마나 많은데",
          "original_form": "사치라고 느끼는 사람이 얼마나 많은데",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.32",
          "form": "ㅇㅋ",
          "original_form": "ㅇㅋ",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020247.1.33",
          "form": "시원",
          "original_form": "시원",
          "speaker_id": "2",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100020247.1.34",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020247.1.35",
          "form": "30 중반 되기전에",
          "original_form": "30 중반 되기전에",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020247.1.36",
          "form": "독립하믄게 꿈이다",
          "original_form": "독립하믄게 꿈이다",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020247.1.37",
          "form": "자가가 잇는것이...",
          "original_form": "자가가 잇는것이...",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020247.1.38",
          "form": "하 ㄹㅇ독립하고싶다",
          "original_form": "하 ㄹㅇ독립하고싶다",
          "speaker_id": "1",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020247.1.39",
          "form": "님은 독립하게 되면",
          "original_form": "님은 독립하게 되면",
          "speaker_id": "1",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020247.1.40",
          "form": "하고싶은 게 있나요",
          "original_form": "하고싶은 게 있나요",
          "speaker_id": "1",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020247.1.41",
          "form": "독립할때만 할 수 있는걸 말하는지",
          "original_form": "독립할때만 할 수 있는걸 말하는지",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020247.1.42",
          "form": "나 독립하면",
          "original_form": "나 독립하면",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020247.1.43",
          "form": "집꾸미기랑",
          "original_form": "집꾸미기랑",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020247.1.44",
          "form": "음..걍 하면 하고싶은거",
          "original_form": "음..걍 하면 하고싶은거",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020247.1.45",
          "form": "게임기사기",
          "original_form": "게임기사기",
          "speaker_id": "2",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020247.1.46",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020247.1.47",
          "form": "오 자가가 생긴다면(월전세 안돼)",
          "original_form": "오 자가가 생긴다면(월전세 안돼)",
          "speaker_id": "2",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020247.1.48",
          "form": "저는 배우고 싶은거 배울라고요",
          "original_form": "저는 배우고 싶은거 배울라고요",
          "speaker_id": "2",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020247.1.49",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020247.1.50",
          "form": "음.. 그리고",
          "original_form": "음.. 그리고",
          "speaker_id": "2",
          "time": "20210828 00:47"
        }
      ]
    }
