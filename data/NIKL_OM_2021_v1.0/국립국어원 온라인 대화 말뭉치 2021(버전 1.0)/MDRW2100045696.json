      "utterance": [
        {
          "id": "MDRW2100045696.1.1",
          "form": "튀김도 국물도 아닙니다",
          "original_form": "튀김도 국물도 아닙니다",
          "speaker_id": "2",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100045696.1.2",
          "form": "쌀도 아니예요",
          "original_form": "쌀도 아니예요",
          "speaker_id": "2",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100045696.1.3",
          "form": "찜도 아니예요 ㅡㅋㅋㅋㅋㅋ",
          "original_form": "찜도 아니예요 ㅡㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100045696.1.4",
          "form": "회인가요? ㅋㅋㅋㅋ",
          "original_form": "회인가요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045696.1.5",
          "form": "아뇨! 해산물은 들어가지만 회는 아니예요",
          "original_form": "아뇨! 해산물은 들어가지만 회는 아니예요",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100045696.1.6",
          "form": "날 것이 아니예용",
          "original_form": "날 것이 아니예용",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100045696.1.7",
          "form": "우와 어렵네요 ㅋㅋㅋㅋㅋ",
          "original_form": "우와 어렵네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045696.1.8",
          "form": "ㅋㅋㅌㅋㅋ아시면 아! 하실걸요",
          "original_form": "ㅋㅋㅌㅋㅋ아시면 아! 하실걸요",
          "speaker_id": "2",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045696.1.9",
          "form": "조리법 나머지 다 물어보시는 걸 추천합니다",
          "original_form": "조리법 나머지 다 물어보시는 걸 추천합니다",
          "speaker_id": "2",
          "time": "20210908 05:40"
        },
        {
          "id": "MDRW2100045696.1.10",
          "form": "건어물인가요? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "건어물인가요? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:40"
        },
        {
          "id": "MDRW2100045696.1.11",
          "form": "음 다 조리하고 그 위에 건어물이 약간 올라가요",
          "original_form": "음 다 조리하고 그 위에 건어물이 약간 올라가요",
          "speaker_id": "2",
          "time": "20210908 05:41"
        },
        {
          "id": "MDRW2100045696.1.12",
          "form": "이번에는 감이 안오네요 ㅋㅋㅋㅋ",
          "original_form": "이번에는 감이 안오네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:41"
        },
        {
          "id": "MDRW2100045696.1.13",
