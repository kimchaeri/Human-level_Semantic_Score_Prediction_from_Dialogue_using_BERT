      "utterance": [
        {
          "id": "MDRW2100039370.1.1",
          "form": "그래도 언니 진짜 잘한다~",
          "original_form": "그래도 언니 진짜 잘한다~",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100039370.1.2",
          "form": "투는 미안하지뭐 \r\n발음도 그렇고 인지도 그렇고",
          "original_form": "투는 미안하지뭐 \r\n발음도 그렇고 인지도 그렇고",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100039370.1.3",
          "form": "내가 좀 더 신경썼으면",
          "original_form": "내가 좀 더 신경썼으면",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100039370.1.4",
          "form": "진짜 언니는 다섯 낳아도 잘 키울듯..",
          "original_form": "진짜 언니는 다섯 낳아도 잘 키울듯..",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100039370.1.5",
          "form": "느리지 않았을텐데",
          "original_form": "느리지 않았을텐데",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100039370.1.6",
          "form": "영상만 보여주고 방치해서",
          "original_form": "영상만 보여주고 방치해서",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100039370.1.7",
          "form": "지금 잡을라니 힘들다;;;",
          "original_form": "지금 잡을라니 힘들다;;;",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100039370.1.8",
          "form": "그렇게 생각하지마요~",
          "original_form": "그렇게 생각하지마요~",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100039370.1.9",
          "form": "이미 뇌가 자리잡은듯해서 걱정도 되고;;;",
          "original_form": "이미 뇌가 자리잡은듯해서 걱정도 되고;;;",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100039370.1.10",
          "form": "아직 어린데 가능성은 무궁무진 합니다~",
          "original_form": "아직 어린데 가능성은 무궁무진 합니다~",
          "speaker_id": "2",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100039370.1.11",
          "form": "그래 지금이라도 영상을 좀 덜 보여주고",
          "original_form": "그래 지금이라도 영상을 좀 덜 보여주고",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100039370.1.12",
          "form": "좋아하는 역할도 많이 해주려고",
          "original_form": "좋아하는 역할도 많이 해주려고",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100039370.1.13",
          "form": "원한테도 투가 지금 발달이 느려서",
          "original_form": "원한테도 투가 지금 발달이 느려서",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100039370.1.14",
          "form": "센터도 다니고 공부도 해야 되니까",
          "original_form": "센터도 다니고 공부도 해야 되니까",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100039370.1.15",
          "form": "멋있어여 언니~",
          "original_form": "멋있어여 언니~",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.16",
          "form": "집에서도 폰 사용하지 말자고",
          "original_form": "집에서도 폰 사용하지 말자고",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.17",
          "form": "얘기해 놨는데",
          "original_form": "얘기해 놨는데",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.18",
          "form": "언니는요;;",
          "original_form": "언니는요;;",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.19",
          "form": "일단 원이 휴대폰을 계속 보니까",
          "original_form": "일단 원이 휴대폰을 계속 보니까",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.20",
          "form": "쫌 잘 안 된다",
          "original_form": "쫌 잘 안 된다",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.22",
          "form": "이제 원들은 폰이 비중이큰데ㅠ",
          "original_form": "이제 원들은 폰이 비중이큰데ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.23",
          "form": "그래도 어제는 폰 안 보더라고 ㅋㅋㅋㅋ",
          "original_form": "그래도 어제는 폰 안 보더라고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.24",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.25",
          "form": "6시까지 휴대폰을 풀어놨거든",
          "original_form": "6시까지 휴대폰을 풀어놨거든",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.26",
          "form": "근데 요새는 비 오니까 놀이터에서 안 놀고 바로 집에 오잖아",
          "original_form": "근데 요새는 비 오니까 놀이터에서 안 놀고 바로 집에 오잖아",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.27",
          "form": "우리도 뭐 폰아님 티비니깐..",
          "original_form": "우리도 뭐 폰아님 티비니깐..",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039370.1.28",
          "form": "그니까 집에 와서 6시까지 폰을 보는거야",
          "original_form": "그니까 집에 와서 6시까지 폰을 보는거야",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039370.1.29",
          "form": "그러면 투가 또 지도 유튜브를 보여달라 그래",
          "original_form": "그러면 투가 또 지도 유튜브를 보여달라 그래",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039370.1.30",
          "form": "악순환",
          "original_form": "악순환",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039370.1.31",
          "form": "글쵸..위에보면 자기도 보곱지..",
          "original_form": "글쵸..위에보면 자기도 보곱지..",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039370.1.32",
          "form": "근데 놀아주는건 너무 힘들고ㅠ",
          "original_form": "근데 놀아주는건 너무 힘들고ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039370.1.33",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039370.1.34",
          "form": "난도 요즘 매장 일 너무 바빠가 갔다오면 녹촌데.",
          "original_form": "난도 요즘 매장 일 너무 바빠가 갔다오면 녹촌데.",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039370.1.35",
          "form": "놀아달라하면 짜증이 빡..ㅠ",
          "original_form": "놀아달라하면 짜증이 빡..ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039370.1.36",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039370.1.37",
          "form": "니 너무 바쁘다",
          "original_form": "니 너무 바쁘다",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039370.1.38",
          "form": "얘기할 시간이 없네",
          "original_form": "얘기할 시간이 없네",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039370.1.39",
          "form": "나 담주까지 ㅠ",
          "original_form": "나 담주까지 ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039370.1.40",
          "form": "안캐도",
          "original_form": "안캐도",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039370.1.41",
          "form": "그래 놀아달라고 하면 힘들지",
          "original_form": "그래 놀아달라고 하면 힘들지",
          "speaker_id": "1",
          "time": "20210903 16:52"
        }
      ]
