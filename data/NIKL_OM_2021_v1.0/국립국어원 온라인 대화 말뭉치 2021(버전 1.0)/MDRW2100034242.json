      "utterance": [
        {
          "id": "MDRW2100034242.1.1",
          "form": "혹시 콘서트 가보셨나요?",
          "original_form": "혹시 콘서트 가보셨나요?",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100034242.1.2",
          "form": "네 아이돌 콘서트 많이 가봤어요 ㅋㅋㅋ",
          "original_form": "네 아이돌 콘서트 많이 가봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100034242.1.3",
          "form": "가보셨어요?",
          "original_form": "가보셨어요?",
          "speaker_id": "2",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100034242.1.4",
          "form": "오~ 네 저도 아이돌 콘서트 몇번 가봤어요!",
          "original_form": "오~ 네 저도 아이돌 콘서트 몇번 가봤어요!",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034242.1.5",
          "form": "누구 콘서트 가셨어요?",
          "original_form": "누구 콘서트 가셨어요?",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034242.1.6",
          "form": "저는 엑소팬이라 ㅋㅋㅋ 엑소랑 드림콘서트같은 여러아이돌하는거요",
          "original_form": "저는 엑소팬이라 ㅋㅋㅋ 엑소랑 드림콘서트같은 여러아이돌하는거요",
          "speaker_id": "2",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034242.1.7",
          "form": "아하! 엑소팬이시군요",
          "original_form": "아하! 엑소팬이시군요",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100034242.1.8",
          "form": "그럼 드림콘서트도 가보신거에요?",
          "original_form": "그럼 드림콘서트도 가보신거에요?",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100034242.1.9",
          "form": "네 ㅋㅋㅋ예전에요",
          "original_form": "네 ㅋㅋㅋ예전에요",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100034242.1.10",
          "form": "누구 보셨었어요?",
          "original_form": "누구 보셨었어요?",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100034242.1.11",
          "form": "와 드림콘서트 가기 힘들지 않나요?",
          "original_form": "와 드림콘서트 가기 힘들지 않나요?",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100034242.1.12",
          "form": "저는 빅스랑 마마무랑 데이식스 콘서트 가봤어요!",
          "original_form": "저는 빅스랑 마마무랑 데이식스 콘서트 가봤어요!",
          "speaker_id": "1",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034242.1.13",
          "form": "그게 은근히 공연장이 넓어서 가기좋더라구요",
          "original_form": "그게 은근히 공연장이 넓어서 가기좋더라구요",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034242.1.14",
          "form": "오 많이 가보셨네요",
          "original_form": "오 많이 가보셨네요",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034242.1.15",
          "form": "경쟁률은 안쌨나요?",
          "original_form": "경쟁률은 안쌨나요?",
          "speaker_id": "1",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034242.1.16",
          "form": "자리욕심만 없다면 괜찮더라구요 ㅋㅋㅋ",
          "original_form": "자리욕심만 없다면 괜찮더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034242.1.17",
          "form": "네 여러가수 가봤네요ㅋㅋ",
          "original_form": "네 여러가수 가봤네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034242.1.18",
          "form": "아 그러네요 뒷자리는 널널하겠어요",
          "original_form": "아 그러네요 뒷자리는 널널하겠어요",
          "speaker_id": "1",
          "time": "20210827 21:55"
        }
      ]
