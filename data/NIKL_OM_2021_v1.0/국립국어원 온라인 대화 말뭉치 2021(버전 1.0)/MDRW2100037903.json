      "utterance": [
        {
          "id": "MDRW2100037903.1.1",
          "form": "혹시 건조기 있으세요?",
          "original_form": "혹시 건조기 있으세요?",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037903.1.2",
          "form": "최근에구매했는데 좋더라구요ㅎㅎ",
          "original_form": "최근에구매했는데 좋더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037903.1.3",
          "form": "저 있어요 ㅋㅋ 없으면 안 돼요",
          "original_form": "저 있어요 ㅋㅋ 없으면 안 돼요",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037903.1.4",
          "form": "필수템이에요 요즘은~!",
          "original_form": "필수템이에요 요즘은~!",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037903.1.5",
          "form": "오ㅋㅋ 그니깐요ㅋㅋ",
          "original_form": "오ㅋㅋ 그니깐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037903.1.6",
          "form": "예전에는 날씨보고 세탁기 돌렸는데",
          "original_form": "예전에는 날씨보고 세탁기 돌렸는데",
          "speaker_id": "2",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037903.1.7",
          "form": "이제는 상관없어요!",
          "original_form": "이제는 상관없어요!",
          "speaker_id": "2",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037903.1.8",
          "form": "전 이제야 써봤네요ㅋ",
          "original_form": "전 이제야 써봤네요ㅋ",
          "speaker_id": "2",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037903.1.9",
          "form": "저도 산지는 얼마 안돼요ㅋㅋㅋ",
          "original_form": "저도 산지는 얼마 안돼요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037903.1.10",
          "form": "건조기도 그렇고 요즘에 가전제품 좋은 거 많더라고요",
          "original_form": "건조기도 그렇고 요즘에 가전제품 좋은 거 많더라고요",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037903.1.11",
          "form": "식기세척기도 사고 싶어요",
          "original_form": "식기세척기도 사고 싶어요",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037903.1.12",
          "form": "저 식세기 있어요!",
          "original_form": "저 식세기 있어요!",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037903.1.13",
          "form": "건조기만큼 신세계는 아닌데",
          "original_form": "건조기만큼 신세계는 아닌데",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037903.1.14",
          "form": "그래도 설거지하기싫을때 편해요.",
          "original_form": "그래도 설거지하기싫을때 편해요.",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037903.1.15",
          "form": "역시 식세기도 사야겠어요ㅋㅋㅋ",
          "original_form": "역시 식세기도 사야겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037903.1.16",
          "form": "근데 아무래도 애벌세척도해야하고,",
          "original_form": "근데 아무래도 애벌세척도해야하고,",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037903.1.17",
          "form": "안에서 테트리스도해야하고",
          "original_form": "안에서 테트리스도해야하고",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037903.1.18",
          "form": "손이 좀 가긴하는데ㅋㅋ 편하긴해요",
          "original_form": "손이 좀 가긴하는데ㅋㅋ 편하긴해요",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037903.1.19",
          "form": "저 미생물 음쓰 처리기도 샀어요!",
          "original_form": "저 미생물 음쓰 처리기도 샀어요!",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037903.1.20",
          "form": "오 괜찮네요 그거",
          "original_form": "오 괜찮네요 그거",
          "speaker_id": "1",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037903.1.21",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037903.1.22",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "1",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037903.1.23",
          "form": "이것도 편해요ㅋㅋ 음쓰 없어져서 신기하기도하고",
          "original_form": "이것도 편해요ㅋㅋ 음쓰 없어져서 신기하기도하고",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037903.1.24",
          "form": "근데 수박 껍질이나 메론껍질은 완전히 안없어지더라고요",
          "original_form": "근데 수박 껍질이나 메론껍질은 완전히 안없어지더라고요",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037903.1.25",
          "form": "나무젓기락으로 건져줘요ㅋㅋ",
          "original_form": "나무젓기락으로 건져줘요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037903.1.26",
          "form": "아 그런거는 수동으로 없애주나보네요",
          "original_form": "아 그런거는 수동으로 없애주나보네요",
          "speaker_id": "1",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037903.1.27",
          "form": "그래도 완전 편할 듯요!",
          "original_form": "그래도 완전 편할 듯요!",
          "speaker_id": "1",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037903.1.28",
          "form": "원래 냉동실에 얼렸는데",
          "original_form": "원래 냉동실에 얼렸는데",
          "speaker_id": "2",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037903.1.29",
          "form": "미생물로 처리되는 거면 흙으로 되어있어요?",
          "original_form": "미생물로 처리되는 거면 흙으로 되어있어요?",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037903.1.30",
          "form": "네 흙 같아요",
          "original_form": "네 흙 같아요",
          "speaker_id": "2",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037903.1.31",
          "form": "분갈이용흙  같아요",
          "original_form": "분갈이용흙  같아요",
          "speaker_id": "2",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037903.1.32",
          "form": "저도엄청고민하고샀는데",
          "original_form": "저도엄청고민하고샀는데",
          "speaker_id": "2",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037903.1.33",
          "form": "음쓰버리러 안가도되니 좋아요",
          "original_form": "음쓰버리러 안가도되니 좋아요",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037903.1.34",
          "form": "저도 지금 얼려두고 있어요 ㅋㅋㅋㅋ",
          "original_form": "저도 지금 얼려두고 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037903.1.35",
          "form": "사람 사는거 다 비슷하네요 ㅋㅋㅋ",
          "original_form": "사람 사는거 다 비슷하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100037903.1.36",
          "form": "아 갑자기 뽐뿌오네요",
          "original_form": "아 갑자기 뽐뿌오네요",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100037903.1.37",
          "form": "그쵸ㅋㅋ",
          "original_form": "그쵸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100037903.1.38",
          "form": "가족들하고사세요?",
          "original_form": "가족들하고사세요?",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100037903.1.39",
          "form": "저는2인가족이라서",
          "original_form": "저는2인가족이라서",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100037903.1.40",
          "form": "네~ 저희도 2인 가족이에요 ㅋㅋ 신기방기",
          "original_form": "네~ 저희도 2인 가족이에요 ㅋㅋ 신기방기",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037903.1.41",
          "form": "음쓰가 많이나오지는않아서",
          "original_form": "음쓰가 많이나오지는않아서",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037903.1.42",
          "form": "관리만잘하면 계속 미생물키워서 쓸수있대요",
          "original_form": "관리만잘하면 계속 미생물키워서 쓸수있대요",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037903.1.43",
          "form": "딱 좋네요!",
          "original_form": "딱 좋네요!",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037903.1.44",
          "form": "친환경적이고 간편하고",
          "original_form": "친환경적이고 간편하고",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037903.1.45",
          "form": "ㅋㅋ 좋아요!",
          "original_form": "ㅋㅋ 좋아요!",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037903.1.46",
          "form": "음식물 쓰레기 봉투도 환경에 안 좋잖아요",
          "original_form": "음식물 쓰레기 봉투도 환경에 안 좋잖아요",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037903.1.47",
          "form": "이거 뭔가 얘기하다보면 생활에 필요한 장비들을 구매하고 싶어지네요 ㅋㅋㅋ",
          "original_form": "이거 뭔가 얘기하다보면 생활에 필요한 장비들을 구매하고 싶어지네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037903.1.48",
          "form": "그쵸ㅋㅋ",
          "original_form": "그쵸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:46"
        }
      ]
