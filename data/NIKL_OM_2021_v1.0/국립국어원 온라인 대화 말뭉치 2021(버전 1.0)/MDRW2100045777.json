        {
          "id": "MDRW2100045777.1.1",
          "form": "안녕하세요,  떡볶이 좋아하시나요?",
          "original_form": "안녕하세요,  떡볶이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045777.1.2",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045777.1.3",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045777.1.4",
          "form": "네 떡볶이 너무 맛있죠ㅠㅠ",
          "original_form": "네 떡볶이 너무 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045777.1.5",
          "form": "즉떡은 천국입니다",
          "original_form": "즉떡은 천국입니다",
          "speaker_id": "1",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045777.1.6",
          "form": "전 시장떡볶이파예요",
          "original_form": "전 시장떡볶이파예요",
          "speaker_id": "2",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045777.1.7",
          "form": "꾸덕꾸덕한 쌀떡!!",
          "original_form": "꾸덕꾸덕한 쌀떡!!",
          "speaker_id": "2",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045777.1.8",
          "form": "오오 저도 쌀떡이요ㅠㅠㅠ",
          "original_form": "오오 저도 쌀떡이요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045777.1.9",
          "form": "매콤~~~하고 꾸덕꾸덕한...",
          "original_form": "매콤~~~하고 꾸덕꾸덕한...",
          "speaker_id": "1",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045777.1.10",
          "form": "저도저도!!",
          "original_form": "저도저도!!",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045777.1.11",
          "form": "아 먹고싶네요 아침부터 ㅋㅋㅋㅋㅋ",
          "original_form": "아 먹고싶네요 아침부터 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045777.1.12",
          "form": "저두요ㅋㅋㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045777.1.13",
          "form": "에잇 점심은 떡볶이 먹어야겠어요ㅋㅋ",
          "original_form": "에잇 점심은 떡볶이 먹어야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045777.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ아침은 뭐드실거예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아침은 뭐드실거예요",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045777.1.15",
          "form": "저 시리얼요ㅠㅠㅠ 대충 때워야해요ㅠㅠ",
          "original_form": "저 시리얼요ㅠㅠㅠ 대충 때워야해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100045777.1.16",
          "form": "무슨 시리얼 드세여?",
          "original_form": "무슨 시리얼 드세여?",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045777.1.17",
          "form": "포스트 그래놀라요!",
          "original_form": "포스트 그래놀라요!",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045777.1.18",
          "form": "앗 포스트 그래놀라 맛잇죠 ㅎㅎ",
          "original_form": "앗 포스트 그래놀라 맛잇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045777.1.19",
          "form": "간단히 때우기 좋은 거 같아요ㅋㅋ",
          "original_form": "간단히 때우기 좋은 거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100045777.1.20",
          "form": "맞아요 ㅋㅋㅋㅋㅋ간편하고 적당히 고소하고 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ간편하고 적당히 고소하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100045777.1.21",
          "form": "아침엔 간편한 게 최고죠!ㅋㅋㅋ",
