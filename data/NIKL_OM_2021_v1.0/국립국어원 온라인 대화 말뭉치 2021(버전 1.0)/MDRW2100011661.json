      "utterance": [
        {
          "id": "MDRW2100011661.1.1",
          "form": "좋았던여행지 있으세요?",
          "original_form": "좋았던여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011661.1.2",
          "form": "대만이요!",
          "original_form": "대만이요!",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011661.1.3",
          "form": "와! 몇박으로 가셨나요?",
          "original_form": "와! 몇박으로 가셨나요?",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011661.1.4",
          "form": "저 대만 아직 안가봐서요ㅎㅎ",
          "original_form": "저 대만 아직 안가봐서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100011661.1.5",
          "form": "3박4일이요ㅎㅎ 코로나 끝나면 다시 가보고싶네요ㅠ",
          "original_form": "3박4일이요ㅎㅎ 코로나 끝나면 다시 가보고싶네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011661.1.6",
          "form": "오 정말 좋으셨나봐요!",
          "original_form": "오 정말 좋으셨나봐요!",
          "speaker_id": "2",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011661.1.7",
          "form": "음식은 입에 맞으셨어요?",
          "original_form": "음식은 입에 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011661.1.8",
          "form": "향신료 냄새만 빼면 나름 괜찮았어요ㅎㅎ",
          "original_form": "향신료 냄새만 빼면 나름 괜찮았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011661.1.9",
          "form": "향신료ㅋㅋ큐ㅠㅜ",
          "original_form": "향신료ㅋㅋ큐ㅠㅜ",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011661.1.10",
          "form": "국내여행도 자주 다니시나요?",
          "original_form": "국내여행도 자주 다니시나요?",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011661.1.11",
          "form": "전 강릉이 좋았어요ㅎㅎ",
          "original_form": "전 강릉이 좋았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011661.1.12",
          "form": "오 강릉 제주도 좋아요!",
          "original_form": "오 강릉 제주도 좋아요!",
          "speaker_id": "1",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011661.1.13",
          "form": "오 제주도도 좋아요!",
          "original_form": "오 제주도도 좋아요!",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011661.1.14",
          "form": "빨리코로나 끝났으면 좋겠네요",
          "original_form": "빨리코로나 끝났으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011661.1.15",
          "form": "섭지코지 진짜 경치가 짱이에요",
          "original_form": "섭지코지 진짜 경치가 짱이에요",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011661.1.16",
          "form": "오오 맞아요 미쳤어요",
          "original_form": "오오 맞아요 미쳤어요",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011661.1.17",
          "form": "성산일출봉도 좋았어요",
          "original_form": "성산일출봉도 좋았어요",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011661.1.18",
          "form": "오 맞아요~~ 등산 한시간도 안걸리던데",
