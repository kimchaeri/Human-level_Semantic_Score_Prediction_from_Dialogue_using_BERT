      "utterance": [
        {
          "id": "MDRW2100024265.1.1",
          "form": "이제 주무신데요",
          "original_form": "이제 주무신데요",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024265.1.2",
          "form": "피곤 하신가봐요",
          "original_form": "피곤 하신가봐요",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024265.1.3",
          "form": "ㅎㄹㄹㄹㄹ",
          "original_form": "ㅎㄹㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024265.1.4",
          "form": "아내가 자라고 했대요ㅎ",
          "original_form": "아내가 자라고 했대요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024265.1.5",
          "form": "name3님네 아이가 많아서 보긴 좋더라구요 ㅎㅎ",
          "original_form": "&name3&님네 아이가 많아서 보긴 좋더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024265.1.6",
          "form": "맞아요 ㅠ 부러워요ㅠ",
          "original_form": "맞아요 ㅠ 부러워요ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024265.1.7",
          "form": "제가 동생들이 2명이라 어릴때부터 지금까지",
          "original_form": "제가 동생들이 2명이라 어릴때부터 지금까지",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024265.1.8",
          "form": "엄청 좋았는데ㅎ 그래서 저도 기회가되면 애기 셋까지는 낳고싶었어요ㅎ 물론 제욕심이지만ㅎ",
          "original_form": "엄청 좋았는데ㅎ 그래서 저도 기회가되면 애기 셋까지는 낳고싶었어요ㅎ 물론 제욕심이지만ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024265.1.9",
          "form": "ㅎㅎㅎ 저도 꿈은 ㅎㅎ",
          "original_form": "ㅎㅎㅎ 저도 꿈은 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024265.1.10",
          "form": "현실은 아니더라구요",
          "original_form": "현실은 아니더라구요",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024265.1.11",
          "form": "맞아요ㅎ 그현실 때문에ㅠ",
          "original_form": "맞아요ㅎ 그현실 때문에ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024265.1.12",
          "form": "진짜 맞벌이안하면 힘들긴해요ㅎㅎ",
          "original_form": "진짜 맞벌이안하면 힘들긴해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024265.1.13",
          "form": "그래서 name3님이 부러워요ㅎ",
          "original_form": "그래서 &name3&님이 부러워요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024265.1.14",
          "form": "ㅎㅎㅎ 돈뿐만 아니라",
          "original_form": "ㅎㅎㅎ 돈뿐만 아니라",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024265.1.15",
          "form": "물론 힘도 드시겟지만ㅠ",
          "original_form": "물론 힘도 드시겟지만ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024265.1.16",
          "form": "ㅎㅎ이것저것 복합적이죠",
          "original_form": "ㅎㅎ이것저것 복합적이죠",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024265.1.17",
          "form": "체력도 그렇죠ㅠ",
          "original_form": "체력도 그렇죠ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024265.1.18",
          "form": "맞는것같애요ㅎ",
          "original_form": "맞는것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024265.1.19",
          "form": "현실적인것도 있고",
          "original_form": "현실적인것도 있고",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024265.1.20",
          "form": "서로 의견도 맞아야하고ㅎ",
          "original_form": "서로 의견도 맞아야하고ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024265.1.21",
          "form": "name1님은 둘째는? 생각있어요?",
          "original_form": "&name1&님은 둘째는? 생각있어요?",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024265.1.22",
          "form": "없어요 ㅎㅎ",
          "original_form": "없어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024265.1.23",
          "form": "ㅎㅎ 맞아요ㅎ 없는게 더 낫을듯ㅎ",
          "original_form": "ㅎㅎ 맞아요ㅎ 없는게 더 낫을듯ㅎ",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024265.1.24",
          "form": "전 어짜피 이제 생각도없지만ㅎㅎ",
          "original_form": "전 어짜피 이제 생각도없지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024265.1.25",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024265.1.26",
          "form": "상황도 안되고ㅎㅎ 이혼하면 이제는 혼자 아이랑 같이 놀면서 살아야죠ㄹㅎ",
          "original_form": "상황도 안되고ㅎㅎ 이혼하면 이제는 혼자 아이랑 같이 놀면서 살아야죠ㄹㅎ",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024265.1.27",
          "form": "아이랑 같이 여행도 자주가고ㅎ 캠핑도가고ㅎㅎ",
          "original_form": "아이랑 같이 여행도 자주가고ㅎ 캠핑도가고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024265.1.28",
          "form": "그게 더 아이한테 괜찮은 선택이라면",
          "original_form": "그게 더 아이한테 괜찮은 선택이라면",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024265.1.29",
          "form": "그러는게 좋죠",
          "original_form": "그러는게 좋죠",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024265.1.30",
          "form": "아참 벌써 8월말ㅠㅠ 오늘또 알아봐야하는데ㅍ",
          "original_form": "아참 벌써 8월말ㅠㅠ 오늘또 알아봐야하는데ㅍ",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024265.1.31",
          "form": "어떤걸요?",
          "original_form": "어떤걸요?",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024265.1.32",
          "form": "출첵이요ㅍ",
          "original_form": "출첵이요ㅍ",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024265.1.33",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024265.1.34",
          "form": "돈돈모에서 떠먹여줬으묜ㅠㅠ",
          "original_form": "돈돈모에서 떠먹여줬으묜ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024265.1.35",
          "form": "어렵죺뭐 ㅎㅎㅎ",
          "original_form": "어렵죺뭐 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024265.1.36",
          "form": "한번에 다되면  좋은데 안되니 힘듦 ㅠㅜ",
          "original_form": "한번에 다되면  좋은데 안되니 힘듦 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024265.1.37",
          "form": "낼하루 시간있으니깐 ㅎㅎ 일하면서 틈틈히 알아봐야겟어요ㅎㅎ",
          "original_form": "낼하루 시간있으니깐 ㅎㅎ 일하면서 틈틈히 알아봐야겟어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024265.1.38",
          "form": "요즘 딸하고 여행많이가는데",
          "original_form": "요즘 딸하고 여행많이가는데",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024265.1.39",
          "form": "네넴",
          "original_form": "네넴",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024265.1.40",
          "form": "엄청 좋아해요ㅎ 막 둘이서 소리지르고",
          "original_form": "엄청 좋아해요ㅎ 막 둘이서 소리지르고",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024265.1.41",
          "form": "ㅎㅎ 재밋을거 같아요",
          "original_form": "ㅎㅎ 재밋을거 같아요",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024265.1.42",
          "form": "노래부르고ㅎㄹ그땐 유튜브도 필요업ㄱ어요ㅎ",
          "original_form": "노래부르고ㅎㄹ그땐 유튜브도 필요업ㄱ어요ㅎ",
