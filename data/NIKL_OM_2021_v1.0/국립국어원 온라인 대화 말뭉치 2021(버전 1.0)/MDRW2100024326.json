      "utterance": [
        {
          "id": "MDRW2100024326.1.1",
          "form": "두끼먹어도배안고프더라구요",
          "original_form": "두끼먹어도배안고프더라구요",
          "speaker_id": "2",
          "time": "20210831 00:50"
        },
        {
          "id": "MDRW2100024326.1.2",
          "form": "ㅋㅋㄱ대체당은 아무래도 비싸니깐...ㅋ",
          "original_form": "ㅋㅋㄱ대체당은 아무래도 비싸니깐...ㅋ",
          "speaker_id": "1",
          "time": "20210831 00:50"
        },
        {
          "id": "MDRW2100024326.1.3",
          "form": "단백질쉐이크만 두끼요?",
          "original_form": "단백질쉐이크만 두끼요?",
          "speaker_id": "1",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024326.1.4",
          "form": "두끼먹고 저녁 마라탕",
          "original_form": "두끼먹고 저녁 마라탕",
          "speaker_id": "2",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024326.1.5",
          "form": "대단해요...ㅠㅠ저는 한끼만 굶어도 힘들어요",
          "original_form": "대단해요...ㅠㅠ저는 한끼만 굶어도 힘들어요",
          "speaker_id": "1",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024326.1.6",
          "form": "놀이터에서 감자샐러드샌드위치얻어먹었네요",
          "original_form": "놀이터에서 감자샐러드샌드위치얻어먹었네요",
          "speaker_id": "2",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024326.1.7",
          "form": "설탕쨈발라주심",
          "original_form": "설탕쨈발라주심",
          "speaker_id": "2",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024326.1.8",
          "form": "ㅋㅋㅋ마라탕은ㅋㅋㅋㅋ조리법 자체가 엄함요ㅋㅋ",
          "original_form": "ㅋㅋㅋ마라탕은ㅋㅋㅋㅋ조리법 자체가 엄함요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024326.1.9",
          "form": "맛있어서 거절못함",
          "original_form": "맛있어서 거절못함",
          "speaker_id": "2",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024326.1.10",
          "form": "달달이가 맛있는건 진리죠ㅋㅋ",
          "original_form": "달달이가 맛있는건 진리죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024326.1.11",
          "form": "사람성의가있지 어떻게 거절한담요",
          "original_form": "사람성의가있지 어떻게 거절한담요",
          "speaker_id": "2",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024326.1.12",
          "form": "마라탕도집에서 만들어 먹어요?",
          "original_form": "마라탕도집에서 만들어 먹어요?",
          "speaker_id": "1",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024326.1.13",
          "form": "고모께서직접만드신 딸기쨈이래요",
          "original_form": "고모께서직접만드신 딸기쨈이래요",
          "speaker_id": "2",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024326.1.14",
          "form": "마라소스만 넣으면 마라탕되요",
          "original_form": "마라소스만 넣으면 마라탕되요",
          "speaker_id": "2",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024326.1.15",
          "form": "정성가득한 음식이네여 수제쨈ㅋㅋ",
          "original_form": "정성가득한 음식이네여 수제쨈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024326.1.16",
          "form": "땅콩소스없어서 카야잼에 콩가루 마요네즈 간장섞었는데",
          "original_form": "땅콩소스없어서 카야잼에 콩가루 마요네즈 간장섞었는데",
          "speaker_id": "2",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024326.1.17",
          "form": "아ㅋㅋ신기하다ㅋㅋ마라소스가 파는군요",
          "original_form": "아ㅋㅋ신기하다ㅋㅋ마라소스가 파는군요",
          "speaker_id": "1",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100024326.1.18",
          "form": "땅콩소스같았어요 순간",
          "original_form": "땅콩소스같았어요 순간",
          "speaker_id": "2",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100024326.1.19",
          "form": "내가음식천재인가싶더라구요",
          "original_form": "내가음식천재인가싶더라구요",
          "speaker_id": "2",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100024326.1.20",
          "form": "저는 마라탕 안먹어봐서 몰랐어요",
          "original_form": "저는 마라탕 안먹어봐서 몰랐어요",
          "speaker_id": "1",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100024326.1.21",
          "form": "안좋아할꺼같아요",
          "original_form": "안좋아할꺼같아요",
          "speaker_id": "2",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100024326.1.22",
          "form": "ㅋㅋㅋㅋ편식심하니까요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ편식심하니까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100024326.1.23",
          "form": "마라탕 소스한개만넣었는데 너무매운데",
          "original_form": "마라탕 소스한개만넣었는데 너무매운데",
          "speaker_id": "2",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100024326.1.24",
          "form": "향신료듬뿍이니까요",
          "original_form": "향신료듬뿍이니까요",
          "speaker_id": "1",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100024326.1.25",
          "form": "저녁식사만 한시간반했어요",
          "original_form": "저녁식사만 한시간반했어요",
          "speaker_id": "2",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100024326.1.26",
          "form": "신랑이 평일저녁에먹기엔 너무 해비한거 아니냐며",
          "original_form": "신랑이 평일저녁에먹기엔 너무 해비한거 아니냐며",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024326.1.27",
          "form": "대박ㅋㅋㅋ정말푸짐한한끼였네요",
          "original_form": "대박ㅋㅋㅋ정말푸짐한한끼였네요",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024326.1.28",
          "form": "먹고 또 운동하면 되는것이지요ㅋㅋㅋ",
          "original_form": "먹고 또 운동하면 되는것이지요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024326.1.29",
          "form": "야채랑 버섯가득인데 어디가해비하다는건지",
          "original_form": "야채랑 버섯가득인데 어디가해비하다는건지",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024326.1.30",
          "form": "잘먹어야 운동도 잘해요ㅋㅋ",
          "original_form": "잘먹어야 운동도 잘해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024326.1.31",
          "form": "저녁메뉴 선택 굿ㅋㅋㅋ",
          "original_form": "저녁메뉴 선택 굿ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024326.1.32",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
