      "utterance": [
        {
          "id": "MDRW2100011138.1.1",
          "form": "난 행크티비 구독해",
          "original_form": "난 행크티비 구독해",
          "speaker_id": "2",
          "time": "20210824 01:34"
        },
        {
          "id": "MDRW2100011138.1.2",
          "form": "오 그건 어떤 채널이야?",
          "original_form": "오 그건 어떤 채널이야?",
          "speaker_id": "1",
          "time": "20210824 01:34"
        },
        {
          "id": "MDRW2100011138.1.3",
          "form": "부동산 채널이야",
          "original_form": "부동산 채널이야",
          "speaker_id": "2",
          "time": "20210824 01:37"
        },
        {
          "id": "MDRW2100011138.1.4",
          "form": "방금 구독했어 ㅎㅎㅎ",
          "original_form": "방금 구독했어 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011138.1.5",
          "form": "나는 핸담 채널 즐겨봐 그것도 부동산이야!",
          "original_form": "나는 핸담 채널 즐겨봐 그것도 부동산이야!",
          "speaker_id": "1",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011138.1.6",
          "form": "나도 찾아볼게",
          "original_form": "나도 찾아볼게",
          "speaker_id": "2",
          "time": "20210824 01:42"
        },
        {
          "id": "MDRW2100011138.1.7",
          "form": "월급쟁이 재테크 얘기하는구나",
          "original_form": "월급쟁이 재테크 얘기하는구나",
          "speaker_id": "2",
          "time": "20210824 01:43"
        },
        {
          "id": "MDRW2100011138.1.8",
          "form": "맞아~ 그 사람 쉽게 설명해줘서 좋아 ㅎㅎ",
          "original_form": "맞아~ 그 사람 쉽게 설명해줘서 좋아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:44"
        },
        {
          "id": "MDRW2100011138.1.9",
          "form": "응응 잘 들어볼게",
          "original_form": "응응 잘 들어볼게",
          "speaker_id": "2",
          "time": "20210824 01:44"
        },
        {
          "id": "MDRW2100011138.1.10",
          "form": "나도 언니가 알려준거 구독 했어~",
          "original_form": "나도 언니가 알려준거 구독 했어~",
          "speaker_id": "1",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011138.1.11",
          "form": "또 어떤 유튜브 봐?",
          "original_form": "또 어떤 유튜브 봐?",
          "speaker_id": "1",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011138.1.12",
          "form": "모두의 부동산 봐",
          "original_form": "모두의 부동산 봐",
          "speaker_id": "2",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011138.1.13",
          "form": "그것도 할게 ㅋㅋㅋㅋㅋ!!!",
          "original_form": "그것도 할게 ㅋㅋㅋㅋㅋ!!!",
          "speaker_id": "1",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011138.1.14",
          "form": "꼭 부동산 대박날거야 언니는",
          "original_form": "꼭 부동산 대박날거야 언니는",
          "speaker_id": "1",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011138.1.15",
          "form": "ㅋㅋㅋㅋ 우리 같이 대박나자",
          "original_form": "ㅋㅋㅋㅋ 우리 같이 대박나자",
          "speaker_id": "2",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011138.1.16",
          "form": "나도 대박날게 ㅎㅎㅎㅎ 같이 놀러다니자 평생~~",
          "original_form": "나도 대박날게 ㅎㅎㅎㅎ 같이 놀러다니자 평생~~",
          "speaker_id": "1",
