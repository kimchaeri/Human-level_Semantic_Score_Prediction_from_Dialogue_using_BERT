      "utterance": [
        {
          "id": "MDRW2100033408.1.1",
          "form": "나올사람은 나오겠죵 각자 중요임무를 가진채",
          "original_form": "나올사람은 나오겠죵 각자 중요임무를 가진채",
          "speaker_id": "1",
          "time": "20210907 07:05"
        },
        {
          "id": "MDRW2100033408.1.2",
          "form": "스포츠도 좋아하시나요?",
          "original_form": "스포츠도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 07:40"
        },
        {
          "id": "MDRW2100033408.1.3",
          "form": "넹 축구좋아해용",
          "original_form": "넹 축구좋아해용",
          "speaker_id": "1",
          "time": "20210907 07:41"
        },
        {
          "id": "MDRW2100033408.1.4",
          "form": "아! 직관도 가시나요??",
          "original_form": "아! 직관도 가시나요??",
          "speaker_id": "2",
          "time": "20210907 07:42"
        },
        {
          "id": "MDRW2100033408.1.5",
          "form": "직접가서 보지는 않고 티비로만봐요",
          "original_form": "직접가서 보지는 않고 티비로만봐요",
          "speaker_id": "1",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100033408.1.6",
          "form": "응원하는 팀 있으세요??",
          "original_form": "응원하는 팀 있으세요??",
          "speaker_id": "2",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100033408.1.7",
          "form": "월드컵때만 열심히 응원해요",
          "original_form": "월드컵때만 열심히 응원해요",
          "speaker_id": "1",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100033408.1.8",
          "form": "아ㅋㅋ 저도ㅋㅋ",
          "original_form": "아ㅋㅋ 저도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:44"
        },
        {
          "id": "MDRW2100033408.1.9",
          "form": "이번 올림픽 보셨어요?",
          "original_form": "이번 올림픽 보셨어요?",
          "speaker_id": "2",
          "time": "20210907 07:44"
        },
        {
          "id": "MDRW2100033408.1.10",
          "form": "월드컵때만 열심히 응원하고싶더라구요",
          "original_form": "월드컵때만 열심히 응원하고싶더라구요",
          "speaker_id": "1",
          "time": "20210907 07:44"
        },
        {
          "id": "MDRW2100033408.1.11",
          "form": "넹 봤어용",
          "original_form": "넹 봤어용",
          "speaker_id": "1",
          "time": "20210907 07:44"
        },
        {
          "id": "MDRW2100033408.1.12",
          "form": "집에서 응원했었죵",
          "original_form": "집에서 응원했었죵",
          "speaker_id": "1",
          "time": "20210907 07:44"
        },
        {
          "id": "MDRW2100033408.1.13",
          "form": "ㅋㅋ저도",
          "original_form": "ㅋㅋ저도",
          "speaker_id": "2",
          "time": "20210907 07:45"
        },
        {
          "id": "MDRW2100033408.1.14",
          "form": "근데 제가 보면 지는 느낌이라 방에서 기사만....",
          "original_form": "근데 제가 보면 지는 느낌이라 방에서 기사만....",
          "speaker_id": "2",
          "time": "20210907 07:45"
        },
        {
          "id": "MDRW2100033408.1.15",
          "form": "앜ㅋㅋ저도 그런게 좀 있어요",
          "original_form": "앜ㅋㅋ저도 그런게 좀 있어요",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100033408.1.16",
          "form": "끝나면 나가서 하이라이트만 봐요",
          "original_form": "끝나면 나가서 하이라이트만 봐요",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100033408.1.17",
          "form": "그것도좋아요!!",
          "original_form": "그것도좋아요!!",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100033408.1.18",
          "form": "잼있어서",
          "original_form": "잼있어서",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100033408.1.19",
          "form": "ㅋㅋ 쫄깃한건 남의 일일때만 즐깁니다",
          "original_form": "ㅋㅋ 쫄깃한건 남의 일일때만 즐깁니다",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100033408.1.20",
          "form": "김연아도 항상 끝나고 봤어요",
          "original_form": "김연아도 항상 끝나고 봤어요",
          "speaker_id": "2",
