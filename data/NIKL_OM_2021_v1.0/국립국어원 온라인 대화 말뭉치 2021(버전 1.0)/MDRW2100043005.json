      "utterance": [
        {
          "id": "MDRW2100043005.1.1",
          "form": "엉엉 패션도 조아해",
          "original_form": "엉엉 패션도 조아해",
          "speaker_id": "2",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100043005.1.2",
          "form": "젊을 때 이쁜거 마니 입어보자 싶어소",
          "original_form": "젊을 때 이쁜거 마니 입어보자 싶어소",
          "speaker_id": "2",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100043005.1.3",
          "form": "너도 조아해?",
          "original_form": "너도 조아해?",
          "speaker_id": "2",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100043005.1.4",
          "form": "나도 좋아해 그래서 살이 많이쪄서 예쁜옷입으려고 살빼는중야",
          "original_form": "나도 좋아해 그래서 살이 많이쪄서 예쁜옷입으려고 살빼는중야",
          "speaker_id": "1",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100043005.1.5",
          "form": "우앙 그렇구나",
          "original_form": "우앙 그렇구나",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100043005.1.6",
          "form": "어떤거 사고싶어???",
          "original_form": "어떤거 사고싶어???",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100043005.1.7",
          "form": "나는 크롭티 ~ 너는?",
          "original_form": "나는 크롭티 ~ 너는?",
          "speaker_id": "1",
          "time": "20210907 13:14"
        },
        {
          "id": "MDRW2100043005.1.8",
          "form": "우와 크롭티",
          "original_form": "우와 크롭티",
          "speaker_id": "2",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043005.1.9",
          "form": "이제 가을이라 크롭 니트도 이쁠것같아",
          "original_form": "이제 가을이라 크롭 니트도 이쁠것같아",
          "speaker_id": "2",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043005.1.10",
          "form": "난 뱃살 때문에 못입어ㅠㅠ",
          "original_form": "난 뱃살 때문에 못입어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043005.1.11",
          "form": "나두양 .. 그래서 운덩즁 ㅠ",
          "original_form": "나두양 .. 그래서 운덩즁 ㅠ",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043005.1.12",
          "form": "니트 사고싶당",
          "original_form": "니트 사고싶당",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043005.1.13",
          "form": "맞아 니트 이쁜거 사고파",
          "original_form": "맞아 니트 이쁜거 사고파",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100043005.1.14",
          "form": "니트는 질도 중요하자너 따가우면 안되니까",
          "original_form": "니트는 질도 중요하자너 따가우면 안되니까",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100043005.1.15",
          "form": "맞아 질좋은거사면",
          "original_form": "맞아 질좋은거사면",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043005.1.16",
          "form": "진짜 입을때마다 기분좋아",
          "original_form": "진짜 입을때마다 기분좋아",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043005.1.17",
          "form": "맞아 부들부들 하자나",
          "original_form": "맞아 부들부들 하자나",
          "speaker_id": "2",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100043005.1.18",
          "form": "하루 코디가 기분에 영향을 주기도 해",
          "original_form": "하루 코디가 기분에 영향을 주기도 해",
          "speaker_id": "2",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100043005.1.19",
          "form": "성형엔 관심많아?",
          "original_form": "성형엔 관심많아?",
          "speaker_id": "1",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100043005.1.20",
          "form": "응 나 코 조금 매뷰리라서 코 성형 하고싶어",
          "original_form": "응 나 코 조금 매뷰리라서 코 성형 하고싶어",
          "speaker_id": "2",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100043005.1.21",
          "form": "계획은 있어?",
          "original_form": "계획은 있어?",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043005.1.22",
          "form": "아 있는데 아마 고민만 하다가",
          "original_form": "아 있는데 아마 고민만 하다가",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043005.1.23",
          "form": "아마 안할거같아",
          "original_form": "아마 안할거같아",
          "speaker_id": "2",
          "time": "20210907 16:31"
        }
