      "utterance": [
        {
          "id": "MDRW2100024554.1.1",
          "form": "점심은 뭐 드셨어요?",
          "original_form": "점심은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100024554.1.2",
          "form": "고기먹었어요!ㅋㅋ",
          "original_form": "고기먹었어요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100024554.1.3",
          "form": "백신맞고 기운이 영 없어서 고기로 기운차리려고요ㅎ",
          "original_form": "백신맞고 기운이 영 없어서 고기로 기운차리려고요ㅎ",
          "speaker_id": "2",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100024554.1.4",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100024554.1.5",
          "form": "저도 소불고기 먹었어요!",
          "original_form": "저도 소불고기 먹었어요!",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100024554.1.6",
          "form": "백신 맞았을때는 잘 먹어야 한대요",
          "original_form": "백신 맞았을때는 잘 먹어야 한대요",
          "speaker_id": "1",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024554.1.7",
          "form": "저녁에도 고기 드세요~",
          "original_form": "저녁에도 고기 드세요~",
          "speaker_id": "1",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024554.1.8",
          "form": "ㅎㅎㅎ아침 점심 저녁",
          "original_form": "ㅎㅎㅎ아침 점심 저녁",
          "speaker_id": "2",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024554.1.9",
          "form": "고기반찬최고죠!ㅋㄱ",
          "original_form": "고기반찬최고죠!ㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024554.1.10",
          "form": "소불고기 맛나겠네요!",
          "original_form": "소불고기 맛나겠네요!",
          "speaker_id": "2",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024554.1.11",
          "form": "네.. 맛있었어요..ㅎㅎ 이런 비오는 날엔 전 부쳐먹고 싶은데 이미 얼마전에 먹어서요",
          "original_form": "네.. 맛있었어요..ㅎㅎ 이런 비오는 날엔 전 부쳐먹고 싶은데 이미 얼마전에 먹어서요",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100024554.1.12",
          "form": "비오나 봐요",
          "original_form": "비오나 봐요",
          "speaker_id": "2",
          "time": "20210831 14:46"
        },
        {
          "id": "MDRW2100024554.1.13",
          "form": "여긴 흐리기만하네요",
          "original_form": "여긴 흐리기만하네요",
          "speaker_id": "2",
          "time": "20210831 14:46"
        },
        {
          "id": "MDRW2100024554.1.14",
          "form": "전은 또 먹어도 맛있죠ㅎ",
          "original_form": "전은 또 먹어도 맛있죠ㅎ",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024554.1.15",
          "form": "맞아요.. 밀가루와 기름의 조화는 환상이죠...ㅎㅎ",
          "original_form": "맞아요.. 밀가루와 기름의 조화는 환상이죠...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100024554.1.16",
          "form": "너무 환상이라 친해지고 싶더라구요ㅋㅋ",
          "original_form": "너무 환상이라 친해지고 싶더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100024554.1.17",
          "form": "근데 살이랑은 절교하고 싶어서.. 참 고민이죠...ㅋ",
          "original_form": "근데 살이랑은 절교하고 싶어서.. 참 고민이죠...ㅋ",
          "speaker_id": "1",
          "time": "20210831 15:00"
        },
        {
          "id": "MDRW2100024554.1.18",
          "form": "고기는 어떻게 해드시는 거 좋아해요?",
          "original_form": "고기는 어떻게 해드시는 거 좋아해요?",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024554.1.19",
          "form": "굽는거? 끓이는거? 양념하는거?",
          "original_form": "굽는거? 끓이는거? 양념하는거?",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024554.1.20",
          "form": "세상에...고기는 뭘해도 맛있죠",
          "original_form": "세상에...고기는 뭘해도 맛있죠",
          "speaker_id": "2",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100024554.1.21",
          "form": "심지어 육회도.'",
          "original_form": "심지어 육회도.'",
          "speaker_id": "2",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100024554.1.22",
          "form": "ㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100024554.1.23",
          "form": "안가리시는 군요..ㅎㅎㅎ",
          "original_form": "안가리시는 군요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100024554.1.24",
          "form": "저도 다 좋은데.. 어떤 사람이 물에 빠진 고기 싫다고 해서 신기했어요",
          "original_form": "저도 다 좋은데.. 어떤 사람이 물에 빠진 고기 싫다고 해서 신기했어요",
          "speaker_id": "1",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100024554.1.25",
          "form": "갈비탕이나 삼계탕이나.. 설렁탕 같은게 얼마나 맛있는데",
          "original_form": "갈비탕이나 삼계탕이나.. 설렁탕 같은게 얼마나 맛있는데",
          "speaker_id": "1",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100024554.1.26",
          "form": "아..물에 빠진 고기면",
          "original_form": "아..물에 빠진 고기면",
          "speaker_id": "2",
          "time": "20210831 15:08"
        },
        {
          "id": "MDRW2100024554.1.27",
          "form": "얼마나 깨끗하겠어요ㅋㅋㅋ",
          "original_form": "얼마나 깨끗하겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:08"
        },
        {
          "id": "MDRW2100024554.1.28",
          "form": "훨씬 담백하고 좋죠 ㅋㅋ",
          "original_form": "훨씬 담백하고 좋죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024554.1.29",
          "form": "경쟁자가 줄어서 좋네요ㅋㅋ",
          "original_form": "경쟁자가 줄어서 좋네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024554.1.30",
          "form": "ㅎㅎㅎ 식당 고를때 좀 귀찮아요.. 그런데 빼고 가야하니까",
          "original_form": "ㅎㅎㅎ 식당 고를때 좀 귀찮아요.. 그런데 빼고 가야하니까",
          "speaker_id": "1",
          "time": "20210831 15:11"
        }
      ]
    }
