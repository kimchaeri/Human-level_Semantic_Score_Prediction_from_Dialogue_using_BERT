      "utterance": [
        {
          "id": "MDRW2100021032.1.1",
          "form": "어제까지 비가 와서 추웠는데",
          "original_form": "어제까지 비가 와서 추웠는데",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021032.1.2",
          "form": "오늘은 안 오네요ㅎㅎ",
          "original_form": "오늘은 안 오네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021032.1.3",
          "form": "오늘은 오랜만에 햇빛이 드네요!",
          "original_form": "오늘은 오랜만에 햇빛이 드네요!",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021032.1.4",
          "form": "기분 좋아지는 날씨예요",
          "original_form": "기분 좋아지는 날씨예요",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021032.1.5",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021032.1.6",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021032.1.7",
          "form": "오늘 날씨가 참 좋아요",
          "original_form": "오늘 날씨가 참 좋아요",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021032.1.8",
          "form": "그죠 그동안 빨래안말라서",
          "original_form": "그죠 그동안 빨래안말라서",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100021032.1.9",
          "form": "고민이였는데 덕분에 오늘 빨래널었어요",
          "original_form": "고민이였는데 덕분에 오늘 빨래널었어요",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100021032.1.10",
          "form": "곧 가을이려나봐요",
          "original_form": "곧 가을이려나봐요",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100021032.1.11",
          "form": "그렇게 덥지도 않네요",
          "original_form": "그렇게 덥지도 않네요",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100021032.1.12",
          "form": "어우 저도요",
          "original_form": "어우 저도요",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100021032.1.13",
          "form": "비가계속와서",
          "original_form": "비가계속와서",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100021032.1.14",
          "form": "빨래도 못했는데",
          "original_form": "빨래도 못했는데",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100021032.1.15",
          "form": "오늘은 해도 될 거 같아요ㅎㅎ",
          "original_form": "오늘은 해도 될 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100021032.1.16",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100021032.1.17",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021032.1.18",
          "form": "사계절 중에 제일 좋죠",
          "original_form": "사계절 중에 제일 좋죠",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021032.1.19",
          "form": "황사도없고",
          "original_form": "황사도없고",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021032.1.20",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021032.1.21",
          "form": "덥지도춥지도않고",
          "original_form": "덥지도춥지도않고",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021032.1.22",
          "form": "딱좋아요",
          "original_form": "딱좋아요",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021032.1.23",
          "form": "코로나 시국이라 여행못가겠지만",
          "original_form": "코로나 시국이라 여행못가겠지만",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021032.1.24",
          "form": "여행가기 좋은 날씨죠",
          "original_form": "여행가기 좋은 날씨죠",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021032.1.25",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021032.1.26",
          "form": "오맞아요!!",
          "original_form": "오맞아요!!",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021032.1.27",
          "form": "여행도 못 간 지 너무 오래됐네요ㅠㅠ",
          "original_form": "여행도 못 간 지 너무 오래됐네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021032.1.28",
          "form": "그죠ㅠㅠ 가을에 산책이라도 열심히 갈려구요",
          "original_form": "그죠ㅠㅠ 가을에 산책이라도 열심히 갈려구요",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021032.1.29",
          "form": "산책좋죠~",
          "original_form": "산책좋죠~",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100021032.1.30",
          "form": "이럴 때일수록",
          "original_form": "이럴 때일수록",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100021032.1.31",
          "form": "면역력 증진을 위해",
          "original_form": "면역력 증진을 위해",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100021032.1.32",
          "form": "운동을열심히해야죵ㅎㅎㅎ",
          "original_form": "운동을열심히해야죵ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:46"
        }
      ]
