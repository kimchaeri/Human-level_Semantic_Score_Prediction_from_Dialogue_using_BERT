      "utterance": [
        {
          "id": "MDRW2100032471.1.1",
          "form": "피자 토핑 마음껏 올릴 수 있다면 뭐 고를 거야?",
          "original_form": "피자 토핑 마음껏 올릴 수 있다면 뭐 고를 거야?",
          "speaker_id": "1",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100032471.1.2",
          "form": "일단 치즈 듬뿍!",
          "original_form": "일단 치즈 듬뿍!",
          "speaker_id": "2",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100032471.1.3",
          "form": "나도 치즈 좋아 ㅋㅋㅋㅋ",
          "original_form": "나도 치즈 좋아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100032471.1.4",
          "form": "베이컨하고 소세지 페퍼로니 가득!",
          "original_form": "베이컨하고 소세지 페퍼로니 가득!",
          "speaker_id": "2",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100032471.1.5",
          "form": "나는 뭐든 토핑 많으면 좋을 것 같기도 하고",
          "original_form": "나는 뭐든 토핑 많으면 좋을 것 같기도 하고",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100032471.1.6",
          "form": "오 새우도 좋다 치킨도 좋고",
          "original_form": "오 새우도 좋다 치킨도 좋고",
          "speaker_id": "2",
          "time": "20210905 18:53"
        },
        {
          "id": "MDRW2100032471.1.7",
          "form": "갑자기 피자 생각이 났어 ㅋㅋㅋ",
          "original_form": "갑자기 피자 생각이 났어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:53"
        },
        {
          "id": "MDRW2100032471.1.8",
          "form": "토핑 아끼면 섭하긴 하지",
          "original_form": "토핑 아끼면 섭하긴 하지",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100032471.1.9",
          "form": "시카고 피자도 좋아해?",
          "original_form": "시카고 피자도 좋아해?",
          "speaker_id": "1",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100032471.1.10",
          "form": "그럼그럼 치즈는 다 좋아",
          "original_form": "그럼그럼 치즈는 다 좋아",
          "speaker_id": "2",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100032471.1.11",
          "form": "나도 ㅋㅋㅋ",
          "original_form": "나도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100032471.1.12",
          "form": "토마토 소스와 치즈의 조합은 정말 환상인듯",
          "original_form": "토마토 소스와 치즈의 조합은 정말 환상인듯",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100032471.1.13",
          "form": "맞아 누가 개발했는지 대단해",
          "original_form": "맞아 누가 개발했는지 대단해",
          "speaker_id": "2",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100032471.1.14",
          "form": "고구마 피자도 좋아",
          "original_form": "고구마 피자도 좋아",
          "speaker_id": "1",
          "time": "20210905 18:57"
        }
