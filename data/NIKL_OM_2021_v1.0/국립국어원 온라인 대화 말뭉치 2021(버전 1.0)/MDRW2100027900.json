      "utterance": [
        {
          "id": "MDRW2100027900.1.1",
          "form": "안녕하세요 백신 맞으셨나요",
          "original_form": "안녕하세요 백신 맞으셨나요",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027900.1.2",
          "form": "넵 맞은지 이제 열흘? 정도됐네요 ㅋㅋㅋ",
          "original_form": "넵 맞은지 이제 열흘? 정도됐네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100027900.1.3",
          "form": "오 저랑 비슷하세요",
          "original_form": "오 저랑 비슷하세요",
          "speaker_id": "1",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027900.1.4",
          "form": "저는 보름전 맞았어요",
          "original_form": "저는 보름전 맞았어요",
          "speaker_id": "1",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027900.1.5",
          "form": "백신 모더나요",
          "original_form": "백신 모더나요",
          "speaker_id": "1",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100027900.1.6",
          "form": "저두요! 아프진 않으셨어요? 저는 삼일정도 근육통오더라구요",
          "original_form": "저두요! 아프진 않으셨어요? 저는 삼일정도 근육통오더라구요",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027900.1.7",
          "form": "저도 팔이 퉁퉁부었어요",
          "original_form": "저도 팔이 퉁퉁부었어요",
          "speaker_id": "1",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027900.1.8",
          "form": "일주일 후 부작용이 왔어요",
          "original_form": "일주일 후 부작용이 왔어요",
          "speaker_id": "1",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027900.1.9",
          "form": "겨드랑이 통증이요",
          "original_form": "겨드랑이 통증이요",
          "speaker_id": "1",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027900.1.10",
          "form": "일주일뒤에 간지럽고 붓는다는거 듣긴한거같아요 저는 다행이게도 괜찮긴한데ㅠㅠ 2차가 좀 걱정되네요 ㅋㅋㅋㅋ",
          "original_form": "일주일뒤에 간지럽고 붓는다는거 듣긴한거같아요 저는 다행이게도 괜찮긴한데ㅠㅠ 2차가 좀 걱정되네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100027900.1.11",
          "form": "여자는 가슴이 발달해서 림프절이 붓는게 흔하대요",
          "original_form": "여자는 가슴이 발달해서 림프절이 붓는게 흔하대요",
          "speaker_id": "1",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100027900.1.12",
          "form": "그래서 겨드랑이가 아팠어요",
          "original_form": "그래서 겨드랑이가 아팠어요",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027900.1.13",
          "form": "2차가 저도 걱정돼요",
          "original_form": "2차가 저도 걱정돼요",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027900.1.14",
          "form": "담달초에 맞아야해요",
          "original_form": "담달초에 맞아야해요",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027900.1.15",
          "form": "저는 담달 말쯤인데 생리하는 쯤이라 더 걱정되네요ㅠㅠ 첫둘째날은아닐거같아서ㅜ다행이긴한데ㅠㅠ",
          "original_form": "저는 담달 말쯤인데 생리하는 쯤이라 더 걱정되네요ㅠㅠ 첫둘째날은아닐거같아서ㅜ다행이긴한데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100027900.1.16",
          "form": "괜찮아요. 이상있음 상담처도 있구요.",
          "original_form": "괜찮아요. 이상있음 상담처도 있구요.",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027900.1.17",
          "form": "다만 며칠 푹 쉬시면 될거에요.",
          "original_form": "다만 며칠 푹 쉬시면 될거에요.",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027900.1.18",
          "form": "백신 완료하면 든든하지요",
          "original_form": "백신 완료하면 든든하지요",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100027900.1.19",
          "form": "ㅋㅋㅋㅋ 맞아요! 그래서 사실 그녕 빨리 2차 맞아버리고 싶기도 해요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 맞아요! 그래서 사실 그녕 빨리 2차 맞아버리고 싶기도 해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100027900.1.20",
          "form": "접종끝난분들 부럽더라구요 ㅠㅠ",
          "original_form": "접종끝난분들 부럽더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100027900.1.21",
          "form": "네 1차만 맞아도 좀안심돼오ㅎ",
          "original_form": "네 1차만 맞아도 좀안심돼오ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100027900.1.22",
          "form": "얼른 국민들 다 맞길요",
          "original_form": "얼른 국민들 다 맞길요",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100027900.1.23",
          "form": "아직 백신부작용 걱정하는 사람도 많아요",
          "original_form": "아직 백신부작용 걱정하는 사람도 많아요",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100027900.1.24",
          "form": "맞아요ㅠㅠ 얼른 다 맞았으면 ㅠㅠ 곧 추석이기두 하구 !",
          "original_form": "맞아요ㅠㅠ 얼른 다 맞았으면 ㅠㅠ 곧 추석이기두 하구 !",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100027900.1.25",
          "form": "타이레놀같은거 챙겨놓으심 괜찮아요",
          "original_form": "타이레놀같은거 챙겨놓으심 괜찮아요",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100027900.1.26",
          "form": "모든약은 부작용있으니 백신도 특별하지않다고 생각해요",
          "original_form": "모든약은 부작용있으니 백신도 특별하지않다고 생각해요",
          "speaker_id": "1",
          "time": "20210902 17:52"
        }
      ]
    }
