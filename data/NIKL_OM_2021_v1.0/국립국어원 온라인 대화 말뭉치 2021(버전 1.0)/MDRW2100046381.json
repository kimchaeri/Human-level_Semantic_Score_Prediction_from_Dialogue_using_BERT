      "utterance": [
        {
          "id": "MDRW2100046381.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211020 16:14"
        },
        {
          "id": "MDRW2100046381.1.2",
          "form": "안녕하세요! 즐겨 보시는 콘텐츠 있으신가요~?",
          "original_form": "안녕하세요! 즐겨 보시는 콘텐츠 있으신가요~?",
          "speaker_id": "2",
          "time": "20211020 16:14"
        },
        {
          "id": "MDRW2100046381.1.3",
          "form": "넷플요",
          "original_form": "넷플요",
          "speaker_id": "1",
          "time": "20211020 16:15"
        },
        {
          "id": "MDRW2100046381.1.4",
          "form": "넷플에서 재밌게 보셨던 드라마나 영화 추천해주세요!",
          "original_form": "넷플에서 재밌게 보셨던 드라마나 영화 추천해주세요!",
          "speaker_id": "2",
          "time": "20211020 16:15"
        },
        {
          "id": "MDRW2100046381.1.5",
          "form": "오징어게임 너무 재밌게 봤어요",
          "original_form": "오징어게임 너무 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20211020 16:15"
        },
        {
          "id": "MDRW2100046381.1.6",
          "form": "역시 요즘 오징어게임이 대세긴한가봐요",
          "original_form": "역시 요즘 오징어게임이 대세긴한가봐요",
          "speaker_id": "2",
          "time": "20211020 16:16"
        },
        {
          "id": "MDRW2100046381.1.7",
          "form": "그렇죠^^",
          "original_form": "그렇죠^^",
          "speaker_id": "1",
          "time": "20211020 16:16"
        },
        {
          "id": "MDRW2100046381.1.8",
          "form": "막 외국에서도 달고나가 유행이라던데",
          "original_form": "막 외국에서도 달고나가 유행이라던데",
          "speaker_id": "2",
          "time": "20211020 16:16"
        },
        {
          "id": "MDRW2100046381.1.9",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "1",
          "time": "20211020 16:16"
        },
        {
          "id": "MDRW2100046381.1.10",
          "form": "네네 달고나 만들기도 인기고",
          "original_form": "네네 달고나 만들기도 인기고",
          "speaker_id": "2",
          "time": "20211020 16:17"
        },
        {
          "id": "MDRW2100046381.1.11",
          "form": "만드는 영상 올리는 유튜버들도 많더라구요!",
          "original_form": "만드는 영상 올리는 유튜버들도 많더라구요!",
          "speaker_id": "2",
          "time": "20211020 16:17"
        },
        {
          "id": "MDRW2100046381.1.12",
          "form": "추억의 달고나 ㅋㅋ",
          "original_form": "추억의 달고나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:17"
        },
        {
          "id": "MDRW2100046381.1.13",
          "form": "소싯적에 국자좀 태워보셨나요~?ㅋㅋ",
          "original_form": "소싯적에 국자좀 태워보셨나요~?ㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 16:17"
        },
        {
          "id": "MDRW2100046381.1.14",
          "form": "다시 한번 해봐야겠어요 ㅋㅋ",
          "original_form": "다시 한번 해봐야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:17"
        },
        {
          "id": "MDRW2100046381.1.15",
          "form": "오징어 게임에 나오는 게임중에 정작 오징어게임은",
          "original_form": "오징어 게임에 나오는 게임중에 정작 오징어게임은",
          "speaker_id": "2",
          "time": "20211020 16:18"
        },
        {
          "id": "MDRW2100046381.1.16",
          "form": "사람들이 잘 모르는 것 같던데",
          "original_form": "사람들이 잘 모르는 것 같던데",
          "speaker_id": "2",
          "time": "20211020 16:18"
        },
        {
          "id": "MDRW2100046381.1.17",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:18"
        },
        {
          "id": "MDRW2100046381.1.18",
          "form": "혹시 이 게임 잘 아시나요",
          "original_form": "혹시 이 게임 잘 아시나요",
          "speaker_id": "2",
          "time": "20211020 16:18"
        },
        {
          "id": "MDRW2100046381.1.19",
          "form": "네 잘 알죠 ㅋ",
          "original_form": "네 잘 알죠 ㅋ",
          "speaker_id": "1",
          "time": "20211020 16:18"
        },
        {
          "id": "MDRW2100046381.1.20",
          "form": "오오 제 주변에는 이 게임을 아는 사람이 없어서",
          "original_form": "오오 제 주변에는 이 게임을 아는 사람이 없어서",
          "speaker_id": "2",
          "time": "20211020 16:19"
        },
        {
          "id": "MDRW2100046381.1.21",
          "form": "드라마 볼 때 생소했다고 하더라구요",
          "original_form": "드라마 볼 때 생소했다고 하더라구요",
          "speaker_id": "2",
          "time": "20211020 16:19"
        },
        {
          "id": "MDRW2100046381.1.22",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20211020 16:20"
        },
        {
          "id": "MDRW2100046381.1.23",
          "form": "그래도 이렇게 한국 콘텐츠가 알려지는건 좋은 현상 같아요 ㅎㅎ",
          "original_form": "그래도 이렇게 한국 콘텐츠가 알려지는건 좋은 현상 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 16:21"
        }
      ]
