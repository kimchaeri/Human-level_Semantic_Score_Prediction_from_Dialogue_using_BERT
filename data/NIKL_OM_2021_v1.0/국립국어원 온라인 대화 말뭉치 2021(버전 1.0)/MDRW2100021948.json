      "utterance": [
        {
          "id": "MDRW2100021948.1.1",
          "form": "네 ! 전 꼭 할꺼에요 ㅋㅋㅋ",
          "original_form": "네 ! 전 꼭 할꺼에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021948.1.2",
          "form": "지금도 인스타같은거 ㅎㅎ 좋아해서요",
          "original_form": "지금도 인스타같은거 ㅎㅎ 좋아해서요",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021948.1.3",
          "form": "싸이월드 다시 시작하면 옛날이랑 좀 다르겠죠?ㅋㅋㅋ",
          "original_form": "싸이월드 다시 시작하면 옛날이랑 좀 다르겠죠?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021948.1.4",
          "form": "그래도 재밌을 거 같아요",
          "original_form": "그래도 재밌을 거 같아요",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021948.1.5",
          "form": "확실히 다를거같아요ㅠㅜ 그 감성은 없지않을까요 ㅋㅋ",
          "original_form": "확실히 다를거같아요ㅠㅜ 그 감성은 없지않을까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021948.1.6",
          "form": "싸이월드 하실건가요! ㅋㅋ",
          "original_form": "싸이월드 하실건가요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021948.1.7",
          "form": "그쵸ㅠㅠ 오픈하면 어떻게 바뀔지 궁금하네요",
          "original_form": "그쵸ㅠㅠ 오픈하면 어떻게 바뀔지 궁금하네요",
          "speaker_id": "2",
          "time": "20210829 00:41"
        },
        {
          "id": "MDRW2100021948.1.8",
          "form": "궁금해서 해볼 거 같아요ㅋㅋㅋㄱ",
          "original_form": "궁금해서 해볼 거 같아요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210829 00:41"
        },
        {
          "id": "MDRW2100021948.1.9",
          "form": "완전 유행할거 같아요 ㅋㅋㅋ",
          "original_form": "완전 유행할거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100021948.1.10",
          "form": "부활하는게 왜 이렇게 오래걸리는진 모르겠지만 ㅋㅋㅋ",
          "original_form": "부활하는게 왜 이렇게 오래걸리는진 모르겠지만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100021948.1.11",
          "form": "그니까요 말 나온지 꽤 됐는데 서비스 시작한다는 소식은 없네요ㅋㅋㄱㅋ",
          "original_form": "그니까요 말 나온지 꽤 됐는데 서비스 시작한다는 소식은 없네요ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210829 14:46"
        },
        {
          "id": "MDRW2100021948.1.12",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:49"
        },
        {
          "id": "MDRW2100021948.1.13",
          "form": "사람들도 그냥 흑역사 지우기만 하던데ㅋㅋㅋ",
          "original_form": "사람들도 그냥 흑역사 지우기만 하던데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:49"
        },
        {
          "id": "MDRW2100021948.1.14",
          "form": "ㅋㅋㅋ사람들이 추억의 서버가 부활해서 관심 가진 거 아니라",
          "original_form": "ㅋㅋㅋ사람들이 추억의 서버가 부활해서 관심 가진 거 아니라",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021948.1.15",
          "form": "흑역사 때문에 우왕좌왕하는 느낌이긴 했어요ㅋㅋㅋㄱ",
          "original_form": "흑역사 때문에 우왕좌왕하는 느낌이긴 했어요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021948.1.16",
          "form": "진짜요 ㅋㅋㅋ 사람들도 갑자기 왜 부활하냐고 ㅋㅋ",
          "original_form": "진짜요 ㅋㅋㅋ 사람들도 갑자기 왜 부활하냐고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:57"
        },
        {
          "id": "MDRW2100021948.1.17",
          "form": "흑역사는 지우고 부활시키라고 ㅋㅋ",
          "original_form": "흑역사는 지우고 부활시키라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:57"
        },
        {
          "id": "MDRW2100021948.1.18",
          "form": "저도 놀래서 들어갔어요 ㅋㅋㅋ",
          "original_form": "저도 놀래서 들어갔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:57"
        },
        {
          "id": "MDRW2100021948.1.19",
          "form": "맞아요ㅋㅋㅋㅋㅋ 근데 그래놓고 아직 소식이 없어... ㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ 근데 그래놓고 아직 소식이 없어... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100021948.1.20",
          "form": "오픈하면 재밌겠어요ㅎㅎ",
          "original_form": "오픈하면 재밌겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100021948.1.21",
          "form": "진짜요 ㅎㅎ 지금 인스타는 하시나요?",
          "original_form": "진짜요 ㅎㅎ 지금 인스타는 하시나요?",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100021948.1.22",
          "form": "지금은 공부해서 그냥 구독계만 하나 있어요!",
          "original_form": "지금은 공부해서 그냥 구독계만 하나 있어요!",
          "speaker_id": "2",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100021948.1.23",
          "form": "인스타 자주 하세요?",
          "original_form": "인스타 자주 하세요?",
          "speaker_id": "2",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100021948.1.24",
          "form": "저는 눈팅 하는거 좋아해요 ㅋㅋ ! 이것저것",
          "original_form": "저는 눈팅 하는거 좋아해요 ㅋㅋ ! 이것저것",
          "speaker_id": "1",
          "time": "20210829 15:09"
        }
      ]
