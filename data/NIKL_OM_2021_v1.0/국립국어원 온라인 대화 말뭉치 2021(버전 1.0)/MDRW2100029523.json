      "utterance": [
        {
          "id": "MDRW2100029523.1.1",
          "form": "이제 9월이니까 9월 모평 보겠어요",
          "original_form": "이제 9월이니까 9월 모평 보겠어요",
          "speaker_id": "1",
          "time": "20210903 12:30"
        },
        {
          "id": "MDRW2100029523.1.2",
          "form": "오 벌써 시간이 그렇게 됐네요?",
          "original_form": "오 벌써 시간이 그렇게 됐네요?",
          "speaker_id": "2",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100029523.1.3",
          "form": "올해는 안밀리고 그대로 11월 수능이겠죠?",
          "original_form": "올해는 안밀리고 그대로 11월 수능이겠죠?",
          "speaker_id": "2",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100029523.1.4",
          "form": "그렇지 않을까요?",
          "original_form": "그렇지 않을까요?",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100029523.1.5",
          "form": "천재지변에 없는 한 ㅋㅋㅋ",
          "original_form": "천재지변에 없는 한 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100029523.1.6",
          "form": "수능 출제 위원 썰 들어보셨나요?",
          "original_form": "수능 출제 위원 썰 들어보셨나요?",
          "speaker_id": "1",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100029523.1.7",
          "form": "이제부터.. 통조림의 시간인가요!",
          "original_form": "이제부터.. 통조림의 시간인가요!",
          "speaker_id": "2",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100029523.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100029523.1.9",
          "form": "통조림 기간 동안 심심할까봐 멸치 한 박스 가져간대요ㅋㅋㅋㅋㅋ",
          "original_form": "통조림 기간 동안 심심할까봐 멸치 한 박스 가져간대요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100029523.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ통조림 중에 주식 떨어지는 소식에 피눈물 흘렸다는 우스갯소리도 있더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ통조림 중에 주식 떨어지는 소식에 피눈물 흘렸다는 우스갯소리도 있더라구요",
          "speaker_id": "2",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029523.1.11",
          "form": "아ㅠㅠㅠ",
          "original_form": "아ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029523.1.12",
          "form": "저는 병원 간 출제의원 썰 좋아해요",
          "original_form": "저는 병원 간 출제의원 썰 좋아해요",
          "speaker_id": "1",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100029523.1.13",
          "form": "혹시 아세요?",
          "original_form": "혹시 아세요?",
          "speaker_id": "1",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100029523.1.14",
          "form": "헉 그건 뭐에요?",
          "original_form": "헉 그건 뭐에요?",
          "speaker_id": "2",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100029523.1.15",
          "form": "응급실인가 실려 갔는데 간호사가 환자분 이름이 뭐에요? 하니까",
          "original_form": "응급실인가 실려 갔는데 간호사가 환자분 이름이 뭐에요? 하니까",
          "speaker_id": "1",
          "time": "20210903 14:25"
        },
        {
          "id": "MDRW2100029523.1.16",
          "form": "경호원분이 알면 안 돼는 분입니다 하면서 제지했다는 썰이요",
          "original_form": "경호원분이 알면 안 돼는 분입니다 하면서 제지했다는 썰이요",
