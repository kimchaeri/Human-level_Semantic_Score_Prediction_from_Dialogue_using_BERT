      "utterance": [
        {
          "id": "MDRW2100002125.1.1",
          "form": "저 내일 모가디슈 보러가려구요 !",
          "original_form": "저 내일 모가디슈 보러가려구요 !",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002125.1.2",
          "form": "그거 재밌대요",
          "original_form": "그거 재밌대요",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002125.1.3",
          "form": "총 쏘고 신파도없고 후기가좋더라구요",
          "original_form": "총 쏘고 신파도없고 후기가좋더라구요",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002125.1.4",
          "form": "영화보는거 좋아하세요?",
          "original_form": "영화보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002125.1.5",
          "form": "오 후기가 좋더라구요",
          "original_form": "오 후기가 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002125.1.6",
          "form": "네네 보는거 좋아해용 ㅎㅎ지원님ㅇ은여 ?",
          "original_form": "네네 보는거 좋아해용 ㅎㅎ지원님ㅇ은여 ?",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002125.1.7",
          "form": "저도 시간 날 때 한 번씩 보는 편이에요",
          "original_form": "저도 시간 날 때 한 번씩 보는 편이에요",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002125.1.8",
          "form": "장르는 딱히 안가리시나요?",
          "original_form": "장르는 딱히 안가리시나요?",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002125.1.9",
          "form": "전 무서운걸 못봐서",
          "original_form": "전 무서운걸 못봐서",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002125.1.10",
          "form": "네네 근데 막",
          "original_form": "네네 근데 막",
          "speaker_id": "2",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002125.1.11",
          "form": "징그러운건 잘못봐요 ㅠ",
          "original_form": "징그러운건 잘못봐요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002125.1.12",
          "form": "좀비 이런거?",
          "original_form": "좀비 이런거?",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002125.1.13",
          "form": "음 막 사실적으로 몸이 잘린다던지 그런거요ㅋㅋ 소름돋아요",
          "original_form": "음 막 사실적으로 몸이 잘린다던지 그런거요ㅋㅋ 소름돋아요",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002125.1.14",
          "form": "뮤지컬도 좋아하세요?",
          "original_form": "뮤지컬도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002125.1.15",
          "form": "으",
          "original_form": "으",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002125.1.16",
          "form": "너무싫어",
          "original_form": "너무싫어",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002125.1.17",
          "form": "뮤지컬은 좋아요",
          "original_form": "뮤지컬은 좋아요",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002125.1.18",
          "form": "노래하고 뭔가 그 현장에 있는게 좋아요",
          "original_form": "노래하고 뭔가 그 현장에 있는게 좋아요",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002125.1.19",
          "form": "저도요 뮤지컬 정말좋아해요 ㅎㅎ",
          "original_form": "저도요 뮤지컬 정말좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002125.1.20",
          "form": "넘 멋져요",
          "original_form": "넘 멋져요",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002125.1.21",
          "form": "기억에 남는 뮤지컬이 있으신가요",
          "original_form": "기억에 남는 뮤지컬이 있으신가요",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002125.1.22",
          "form": "스위니토드요 !",
          "original_form": "스위니토드요 !",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002125.1.23",
          "form": "담에 하면 한번보세용",
          "original_form": "담에 하면 한번보세용",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002125.1.24",
          "form": "아직도 그 장면이랑 노래가 머릿속에 남아있어여",
          "original_form": "아직도 그 장면이랑 노래가 머릿속에 남아있어여",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002125.1.25",
          "form": "기회가 되면 꼭 한번 보고싶어요",
          "original_form": "기회가 되면 꼭 한번 보고싶어요",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002125.1.26",
          "form": "노래 구절 어디부분인지 알려주실 수 있나요",
          "original_form": "노래 구절 어디부분인지 알려주실 수 있나요",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002125.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ들어는봤나 스위니토드 ~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ들어는봤나 스위니토드 ~",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002125.1.28",
          "form": "모가디슈 보러가려구용",
          "original_form": "모가디슈 보러가려구용",
          "speaker_id": "2",
