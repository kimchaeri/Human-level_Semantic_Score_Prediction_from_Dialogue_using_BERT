      "utterance": [
        {
          "id": "MDRW2100018499.1.1",
          "form": "ㅋㅋㅋ아침은 드셨어요?",
          "original_form": "ㅋㅋㅋ아침은 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 06:40"
        },
        {
          "id": "MDRW2100018499.1.2",
          "form": "아직이용ㅋㅋㅋ",
          "original_form": "아직이용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:40"
        },
        {
          "id": "MDRW2100018499.1.3",
          "form": "그 요즘 요기요에서",
          "original_form": "그 요즘 요기요에서",
          "speaker_id": "1",
          "time": "20210827 06:40"
        },
        {
          "id": "MDRW2100018499.1.4",
          "form": "파리바게트 할인하는거 아세요?",
          "original_form": "파리바게트 할인하는거 아세요?",
          "speaker_id": "1",
          "time": "20210827 06:40"
        },
        {
          "id": "MDRW2100018499.1.5",
          "form": "아! 아직두 해요?",
          "original_form": "아! 아직두 해요?",
          "speaker_id": "2",
          "time": "20210827 06:41"
        },
        {
          "id": "MDRW2100018499.1.6",
          "form": "네네! 이번주 내내해요",
          "original_form": "네네! 이번주 내내해요",
          "speaker_id": "1",
          "time": "20210827 06:41"
        },
        {
          "id": "MDRW2100018499.1.7",
          "form": "5000원 할인 맞죠?",
          "original_form": "5000원 할인 맞죠?",
          "speaker_id": "2",
          "time": "20210827 06:41"
        },
        {
          "id": "MDRW2100018499.1.8",
          "form": "넹넹ㅎㅎ 그래서 이거 오픈하면",
          "original_form": "넹넹ㅎㅎ 그래서 이거 오픈하면",
          "speaker_id": "1",
          "time": "20210827 06:41"
        },
        {
          "id": "MDRW2100018499.1.9",
          "form": "늦은 아점 먹을까해용",
          "original_form": "늦은 아점 먹을까해용",
          "speaker_id": "1",
          "time": "20210827 06:41"
        },
        {
          "id": "MDRW2100018499.1.10",
          "form": "오! 어떤빵 사오실껀가요?",
          "original_form": "오! 어떤빵 사오실껀가요?",
          "speaker_id": "2",
          "time": "20210827 06:41"
        },
        {
          "id": "MDRW2100018499.1.11",
          "form": "파리바게트에 파는 빵중에",
          "original_form": "파리바게트에 파는 빵중에",
          "speaker_id": "1",
          "time": "20210827 06:41"
        },
        {
          "id": "MDRW2100018499.1.12",
          "form": "토종효모빵이랑",
          "original_form": "토종효모빵이랑",
          "speaker_id": "1",
          "time": "20210827 06:41"
        },
        {
          "id": "MDRW2100018499.1.13",
          "form": "꿀토스트 진짜 맛있어요!",
          "original_form": "꿀토스트 진짜 맛있어요!",
          "speaker_id": "1",
          "time": "20210827 06:42"
        },
        {
          "id": "MDRW2100018499.1.14",
          "form": "꿀토스트....세상에",
          "original_form": "꿀토스트....세상에",
          "speaker_id": "2",
          "time": "20210827 06:42"
        },
        {
          "id": "MDRW2100018499.1.15",
          "form": "저는 슴슴한 빵 좋아해서 이런거 위주로 먹는데 진짜 맛있어요",
          "original_form": "저는 슴슴한 빵 좋아해서 이런거 위주로 먹는데 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210827 06:42"
        },
        {
          "id": "MDRW2100018499.1.16",
          "form": "저는 빵 다 좋아하거든요",
          "original_form": "저는 빵 다 좋아하거든요",
          "speaker_id": "2",
          "time": "20210827 06:42"
        },
        {
          "id": "MDRW2100018499.1.17",
          "form": "그리고 파는곳은 많이는 없지만 메론빵도 추천해용",
          "original_form": "그리고 파는곳은 많이는 없지만 메론빵도 추천해용",
          "speaker_id": "1",
          "time": "20210827 06:42"
        },
        {
          "id": "MDRW2100018499.1.18",
          "form": "모닝빵의 기본맛도 좋아하고 단것도 좋아하고!",
          "original_form": "모닝빵의 기본맛도 좋아하고 단것도 좋아하고!",
          "speaker_id": "2",
          "time": "20210827 06:42"
        },
        {
          "id": "MDRW2100018499.1.19",
          "form": "메론빵 맛있어요!",
          "original_form": "메론빵 맛있어요!",
          "speaker_id": "2",
          "time": "20210827 06:42"
        },
        {
          "id": "MDRW2100018499.1.20",
          "form": "단거 넘 맛있죠ㅠㅜ",
          "original_form": "단거 넘 맛있죠ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 06:42"
        },
        {
          "id": "MDRW2100018499.1.21",
          "form": "단팥빵은 좋아하세요?",
          "original_form": "단팥빵은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 06:42"
        },
        {
          "id": "MDRW2100018499.1.22",
          "form": "맥날 마늘버거 드셔보셨어요?",
          "original_form": "맥날 마늘버거 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210827 06:43"
        }
      ]
