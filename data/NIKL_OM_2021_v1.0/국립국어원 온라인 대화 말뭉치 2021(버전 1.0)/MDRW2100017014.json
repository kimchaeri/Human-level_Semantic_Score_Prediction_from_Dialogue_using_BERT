      "utterance": [
        {
          "id": "MDRW2100017014.1.1",
          "form": "마라탕 뭐 넣어 먹어?",
          "original_form": "마라탕 뭐 넣어 먹어?",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017014.1.2",
          "form": "나 중당 옥수수면 치즈떡",
          "original_form": "나 중당 옥수수면 치즈떡",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017014.1.3",
          "form": "고구마떡 분모자 청경체 배추",
          "original_form": "고구마떡 분모자 청경체 배추",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017014.1.4",
          "form": "팽이버섯",
          "original_form": "팽이버섯",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017014.1.5",
          "form": "버섯",
          "original_form": "버섯",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017014.1.6",
          "form": "감자",
          "original_form": "감자",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017014.1.7",
          "form": "이렇게 넣는듯",
          "original_form": "이렇게 넣는듯",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017014.1.8",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017014.1.9",
          "form": "와 감자도 있어?",
          "original_form": "와 감자도 있어?",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017014.1.10",
          "form": "내가 사먹는데는 감자 없음",
          "original_form": "내가 사먹는데는 감자 없음",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017014.1.11",
          "form": "감자 맛있어?",
          "original_form": "감자 맛있어?",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017014.1.12",
          "form": "엉 존맛",
          "original_form": "엉 존맛",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017014.1.13",
          "form": "어떻게 썰려서 와?",
          "original_form": "어떻게 썰려서 와?",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017014.1.14",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017014.1.15",
          "form": "슬라이스 감자!",
          "original_form": "슬라이스 감자!",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017014.1.16",
          "form": "근데 난 보통 시켜먹는게 아니라",
          "original_form": "근데 난 보통 시켜먹는게 아니라",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017014.1.17",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100017014.1.18",
          "form": "매장?",
          "original_form": "매장?",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017014.1.19",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017014.1.20",
          "form": "난 매장감",
          "original_form": "난 매장감",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017014.1.21",
          "form": "안먹는거 많아서",
          "original_form": "안먹는거 많아서",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017014.1.22",
          "form": "아 맞네 너 목이버섯 안 먹지",
          "original_form": "아 맞네 너 목이버섯 안 먹지",
          "speaker_id": "1",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100017014.1.23",
          "form": "마장",
          "original_form": "마장",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100017014.1.24",
          "form": "맛없어",
          "original_form": "맛없어",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100017014.1.25",
          "form": "나는야 편식쟝이",
          "original_form": "나는야 편식쟝이",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100017014.1.26",
          "form": "으헿",
          "original_form": "으헿",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100017014.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100017014.1.28",
          "form": "너무 시러",
          "original_form": "너무 시러",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017014.1.29",
          "form": "맛없어",
          "original_form": "맛없어",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017014.1.30",
          "form": "나는 딱히 안 먹는 거 없음",
          "original_form": "나는 딱히 안 먹는 거 없음",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017014.1.31",
          "form": "동두부? 얼린 두부 그거 빼고 다 먹음",
          "original_form": "동두부? 얼린 두부 그거 빼고 다 먹음",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017014.1.32",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017014.1.33",
          "form": "그것도 안먹어",
          "original_form": "그것도 안먹어",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017014.1.34",
          "form": "마라탕 먹고 싶어ㅠ",
          "original_form": "마라탕 먹고 싶어ㅠ",
          "speaker_id": "1",
          "time": "20210826 17:02"
