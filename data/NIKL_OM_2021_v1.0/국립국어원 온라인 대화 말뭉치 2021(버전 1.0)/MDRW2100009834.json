      "utterance": [
        {
          "id": "MDRW2100009834.1.1",
          "form": "요새 극장가면 팝콘이 싸요",
          "original_form": "요새 극장가면 팝콘이 싸요",
          "speaker_id": "2",
          "time": "20210823 10:45"
        },
        {
          "id": "MDRW2100009834.1.2",
          "form": "들어가서 못먹고 싸가야해서그렇대요",
          "original_form": "들어가서 못먹고 싸가야해서그렇대요",
          "speaker_id": "2",
          "time": "20210823 10:45"
        },
        {
          "id": "MDRW2100009834.1.3",
          "form": "오 그렇구나 영화볼때 먹어야 제맛인데ㅠ",
          "original_form": "오 그렇구나 영화볼때 먹어야 제맛인데ㅠ",
          "speaker_id": "1",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100009834.1.4",
          "form": "극장힘든데 팝콘은 팔아야겠고ㅋㅋ 안쓰러운 맴~",
          "original_form": "극장힘든데 팝콘은 팔아야겠고ㅋㅋ 안쓰러운 맴~",
          "speaker_id": "2",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100009834.1.5",
          "form": "ㅋㅋ최근에 영화보러갔는데 사람 거의 없더라구요",
          "original_form": "ㅋㅋ최근에 영화보러갔는데 사람 거의 없더라구요",
          "speaker_id": "1",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009834.1.6",
          "form": "네 전세낸거같은 기분이었어요",
          "original_form": "네 전세낸거같은 기분이었어요",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009834.1.7",
          "form": "모가디슈보고왔거든요",
          "original_form": "모가디슈보고왔거든요",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009834.1.8",
          "form": "오 볼까 고민중인데 재밌나요? 블랙위도우가 마지막으로 본거라..",
          "original_form": "오 볼까 고민중인데 재밌나요? 블랙위도우가 마지막으로 본거라..",
          "speaker_id": "1",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009834.1.9",
          "form": "헌혈하니까 영화티켓 두장주더라구요",
          "original_form": "헌혈하니까 영화티켓 두장주더라구요",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009834.1.10",
          "form": "블위도봤어요",
          "original_form": "블위도봤어요",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009834.1.11",
          "form": "모가디슈 실제사건 기반이라 재밌어요",
          "original_form": "모가디슈 실제사건 기반이라 재밌어요",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009834.1.12",
          "form": "봐야겠네요ㅎㅎ 다들 재밌다고 해서ㅎㅎ",
          "original_form": "봐야겠네요ㅎㅎ 다들 재밌다고 해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100009834.1.13",
          "form": "지금 아프가니스탄에서 한국대사관 탈출했어서",
          "original_form": "지금 아프가니스탄에서 한국대사관 탈출했어서",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009834.1.14",
          "form": "시기적절한 영화같아요",
          "original_form": "시기적절한 영화같아요",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009834.1.15",
          "form": "맞아요 그렇다고 하더라구요 신기하게 시기가",
          "original_form": "맞아요 그렇다고 하더라구요 신기하게 시기가",
          "speaker_id": "1",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009834.1.16",
          "form": "그냥 기사만 읽는게아니라 영화생각나서 더 입체적으로 느껴지네요",
          "original_form": "그냥 기사만 읽는게아니라 영화생각나서 더 입체적으로 느껴지네요",
          "speaker_id": "2",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009834.1.17",
          "form": "역사는 반복된다 그런걸 느끼게 해주는 영화같기도 하구ㅎㅎ",
          "original_form": "역사는 반복된다 그런걸 느끼게 해주는 영화같기도 하구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100009834.1.18",
          "form": "근데 배워서 답습안하면 반복되기만할뿐ㅜㅡ",
          "original_form": "근데 배워서 답습안하면 반복되기만할뿐ㅜㅡ",
          "speaker_id": "2",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100009834.1.19",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 11:00"
        }
      ]
