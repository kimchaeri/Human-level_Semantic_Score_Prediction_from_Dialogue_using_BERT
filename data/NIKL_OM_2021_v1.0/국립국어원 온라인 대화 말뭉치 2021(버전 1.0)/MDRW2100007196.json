      "utterance": [
        {
          "id": "MDRW2100007196.1.1",
          "form": "술안먹음",
          "original_form": "술안먹음",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007196.1.2",
          "form": "자주 먹잖아",
          "original_form": "자주 먹잖아",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007196.1.3",
          "form": "조만간 닭발집가서 소주하실?",
          "original_form": "조만간 닭발집가서 소주하실?",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007196.1.4",
          "form": "아니됐다",
          "original_form": "아니됐다",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007196.1.5",
          "form": "과음만안하면 괜찮음",
          "original_form": "과음만안하면 괜찮음",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007196.1.6",
          "form": "무슨소리",
          "original_form": "무슨소리",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007196.1.7",
          "form": "닭발집가서 소주 예전에 설날처럼 해야제",
          "original_form": "닭발집가서 소주 예전에 설날처럼 해야제",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007196.1.8",
          "form": "그래 다음 설날에 가자",
          "original_form": "그래 다음 설날에 가자",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007196.1.9",
          "form": "추석에가든동",
          "original_form": "추석에가든동",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007196.1.10",
          "form": "그래도 술은 노노 술이 무슨맛임?",
          "original_form": "그래도 술은 노노 술이 무슨맛임?",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007196.1.11",
          "form": "1병은 오히려 몸에좋음",
          "original_form": "1병은 오히려 몸에좋음",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007196.1.12",
          "form": "그냥 기분풀리는 맛으로먹는거지",
          "original_form": "그냥 기분풀리는 맛으로먹는거지",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007196.1.13",
          "form": "이상한 소리",
          "original_form": "이상한 소리",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007196.1.14",
          "form": "왜 맞는데",
          "original_form": "왜 맞는데",
