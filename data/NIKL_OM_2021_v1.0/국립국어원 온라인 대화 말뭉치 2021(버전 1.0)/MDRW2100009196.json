        {
          "id": "MDRW2100009196.1.1",
          "form": "무슨음식 좋아해요",
          "original_form": "무슨음식 좋아해요",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009196.1.2",
          "form": "저 크림파스타 조아해요",
          "original_form": "저 크림파스타 조아해요",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009196.1.3",
          "form": "오 저도 파스타 환장합니다",
          "original_form": "오 저도 파스타 환장합니다",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009196.1.4",
          "form": "다른건요?",
          "original_form": "다른건요?",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009196.1.5",
          "form": "마라탕이욤!!",
          "original_form": "마라탕이욤!!",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009196.1.6",
          "form": "마라탕 맛있죠 ㅠ",
          "original_form": "마라탕 맛있죠 ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009196.1.7",
          "form": "맛있는거만 좋아하시네용 ㅋㅋ역쉬",
          "original_form": "맛있는거만 좋아하시네용 ㅋㅋ역쉬",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009196.1.8",
          "form": "오오 name1님은 좋아하시는 음식 더잇어요!?",
          "original_form": "오오 &name1&님은 좋아하시는 음식 더잇어요!?",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009196.1.9",
          "form": "저는 해산물류 좋아해요",
          "original_form": "저는 해산물류 좋아해요",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009196.1.10",
          "form": "초밥 해물찜이런거 ㅎㅎ",
          "original_form": "초밥 해물찜이런거 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009196.1.11",
          "form": "오 전 초밥중에 연어초밥조아해요",
          "original_form": "오 전 초밥중에 연어초밥조아해요",
          "speaker_id": "2",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009196.1.12",
          "form": "미식가시네요 ㅋㅋㅋㅋ연어초밥 진짜맛나죠",
          "original_form": "미식가시네요 ㅋㅋㅋㅋ연어초밥 진짜맛나죠",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009196.1.13",
          "form": "ㅋㅋㅋ name1님도 미식가신데요? 저랑 입맛 완전 비슷해요",
          "original_form": "ㅋㅋㅋ &name1&님도 미식가신데요? 저랑 입맛 완전 비슷해요",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009196.1.14",
          "form": "한식은 안좋아하세요?",
          "original_form": "한식은 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009196.1.15",
          "form": "저 김치찌개 조아해요!",
          "original_form": "저 김치찌개 조아해요!",
