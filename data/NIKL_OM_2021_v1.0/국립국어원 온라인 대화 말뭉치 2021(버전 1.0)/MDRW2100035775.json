      "utterance": [
        {
          "id": "MDRW2100035775.1.1",
          "form": "방금 이차돌 가서 받았어요 ㅎㅎ",
          "original_form": "방금 이차돌 가서 받았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:30"
        },
        {
          "id": "MDRW2100035775.1.2",
          "form": "쿠폰은 일단 쓸지 안쓸지 모르지만 무조건 받고 봐야되요 ㅎㅎ",
          "original_form": "쿠폰은 일단 쓸지 안쓸지 모르지만 무조건 받고 봐야되요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100035775.1.3",
          "form": "ㅋㅋ 그죠",
          "original_form": "ㅋㅋ 그죠",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100035775.1.4",
          "form": "전에 60계인가 하는 곳",
          "original_form": "전에 60계인가 하는 곳",
          "speaker_id": "2",
          "time": "20210830 01:35"
        },
        {
          "id": "MDRW2100035775.1.5",
          "form": "만원 할인쿠폰을 받았어요 그거 기프티스타에 팔았어요",
          "original_form": "만원 할인쿠폰을 받았어요 그거 기프티스타에 팔았어요",
          "speaker_id": "2",
          "time": "20210830 01:36"
        },
        {
          "id": "MDRW2100035775.1.6",
          "form": "오!!저도 그거 했는데 장동민이하는그거",
          "original_form": "오!!저도 그거 했는데 장동민이하는그거",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100035775.1.7",
          "form": "저도그래서 만원받았는데",
          "original_form": "저도그래서 만원받았는데",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100035775.1.8",
          "form": "저는먹을라고ㅋㅋ 한장을 더사서 ㅋㅋㅋ",
          "original_form": "저는먹을라고ㅋㅋ 한장을 더사서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:38"
        },
        {
          "id": "MDRW2100035775.1.9",
          "form": "60계치킨드셔보셨어요?",
          "original_form": "60계치킨드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210830 01:39"
        },
        {
          "id": "MDRW2100035775.1.10",
          "form": "아뇨 안먹어봤어요",
          "original_form": "아뇨 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100035775.1.11",
          "form": "아 그렇게 쿠폰 두장으로 시킬 수 있는거군요",
          "original_form": "아 그렇게 쿠폰 두장으로 시킬 수 있는거군요",
          "speaker_id": "2",
          "time": "20210830 01:40"
        },
        {
          "id": "MDRW2100035775.1.12",
          "form": "될것같아요 페이즈쿠폰이라",
          "original_form": "될것같아요 페이즈쿠폰이라",
          "speaker_id": "1",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100035775.1.13",
          "form": "금액권ㅎㅎ",
          "original_form": "금액권ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100035775.1.14",
          "form": "페이즈쿠폰이 금액권이란 뜻이에요?",
          "original_form": "페이즈쿠폰이 금액권이란 뜻이에요?",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100035775.1.15",
          "form": "페이즈는 기프티콘 이런거처럼 저런 쿠폰발행업체구요. 디지털상품권만원적혀있어서",
          "original_form": "페이즈는 기프티콘 이런거처럼 저런 쿠폰발행업체구요. 디지털상품권만원적혀있어서",
          "speaker_id": "1",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100035775.1.16",
          "form": "60계치킨에서",
          "original_form": "60계치킨에서",
          "speaker_id": "1",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100035775.1.17",
          "form": "먹는거 그금액만큼되는쿠폰 일거에요~",
          "original_form": "먹는거 그금액만큼되는쿠폰 일거에요~",
          "speaker_id": "1",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100035775.1.18",
          "form": "아아 그렇군요!",
          "original_form": "아아 그렇군요!",
          "speaker_id": "2",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100035775.1.19",
          "form": "아침드셨나요?ㅎ",
          "original_form": "아침드셨나요?ㅎ",
          "speaker_id": "1",
          "time": "20210830 10:46"
        },
        {
          "id": "MDRW2100035775.1.20",
          "form": "아뇨 안먹었어요 ㅎㅎ",
          "original_form": "아뇨 안먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 10:47"
        },
        {
          "id": "MDRW2100035775.1.21",
          "form": "저는 어제 국에대충 말아먹었어요~",
          "original_form": "저는 어제 국에대충 말아먹었어요~",
          "speaker_id": "1",
          "time": "20210830 10:58"
        },
        {
          "id": "MDRW2100035775.1.22",
          "form": "점심은 먹었어요",
          "original_form": "점심은 먹었어요",
