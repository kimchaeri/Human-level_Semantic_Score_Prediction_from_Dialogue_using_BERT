          "form": "으엌",
          "original_form": "으엌",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015361.1.7",
          "form": "고로케좋아요",
          "original_form": "고로케좋아요",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015361.1.8",
