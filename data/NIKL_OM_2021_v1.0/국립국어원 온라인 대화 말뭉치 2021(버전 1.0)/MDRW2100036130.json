      "utterance": [
        {
          "id": "MDRW2100036130.1.1",
          "form": "벌써 저녁이네요",
          "original_form": "벌써 저녁이네요",
          "speaker_id": "2",
          "time": "20210830 18:06"
        },
        {
          "id": "MDRW2100036130.1.2",
          "form": "네 name2님 저녁뭐드실거에요?",
          "original_form": "네 &name2&님 저녁뭐드실거에요?",
          "speaker_id": "1",
          "time": "20210830 18:31"
        },
        {
          "id": "MDRW2100036130.1.3",
          "form": "저 피자 시켰어요!",
          "original_form": "저 피자 시켰어요!",
          "speaker_id": "2",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036130.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036130.1.5",
          "form": "전 4시에짜장면먹었는데아직도 배불러요ㅜ",
          "original_form": "전 4시에짜장면먹었는데아직도 배불러요ㅜ",
          "speaker_id": "1",
          "time": "20210830 19:31"
        },
        {
          "id": "MDRW2100036130.1.6",
          "form": "어디피자시키셨어요?",
          "original_form": "어디피자시키셨어요?",
          "speaker_id": "1",
          "time": "20210830 19:31"
        },
        {
          "id": "MDRW2100036130.1.7",
          "form": "점심 늦게 드셨군요ㅠ",
          "original_form": "점심 늦게 드셨군요ㅠ",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100036130.1.8",
          "form": "피자마루요!!",
          "original_form": "피자마루요!!",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100036130.1.9",
          "form": "피자마루 어떤거드셨어요?",
          "original_form": "피자마루 어떤거드셨어요?",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036130.1.10",
          "form": "전 고구마피자맛있더라구여",
          "original_form": "전 고구마피자맛있더라구여",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036130.1.11",
          "form": "우와 저 꿀고구마요",
          "original_form": "우와 저 꿀고구마요",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100036130.1.12",
          "form": "아시는구나",
          "original_form": "아시는구나",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100036130.1.13",
          "form": "아시는분이 나타나셨어욧!!",
          "original_form": "아시는분이 나타나셨어욧!!",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100036130.1.14",
          "form": "피자마루 고구마피자 유명한거아녔나요! 완전 가성비좋고 맛있는데",
          "original_form": "피자마루 고구마피자 유명한거아녔나요! 완전 가성비좋고 맛있는데",
          "speaker_id": "1",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100036130.1.15",
          "form": "피자마루 안드시는 분들이 계시더라구욥ㅠ",
          "original_form": "피자마루 안드시는 분들이 계시더라구욥ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100036130.1.16",
          "form": "오늘 완전 전하고 있어요!!",
          "original_form": "오늘 완전 전하고 있어요!!",
          "speaker_id": "2",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100036130.1.17",
          "form": "요새 피자마루가 잘안보이더라구여ㅜㅜ아쉽",
          "original_form": "요새 피자마루가 잘안보이더라구여ㅜㅜ아쉽",
          "speaker_id": "1",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100036130.1.18",
          "form": "오 피자마루전도사",
          "original_form": "오 피자마루전도사",
          "speaker_id": "1",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100036130.1.19",
          "form": "맞아요ㅠ 가성비 갑인데..",
          "original_form": "맞아요ㅠ 가성비 갑인데..",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036130.1.20",
          "form": "여기 대화하시는분들이 피자마룰 드시는 마음으로..!",
          "original_form": "여기 대화하시는분들이 피자마룰 드시는 마음으로..!",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036130.1.21",
          "form": "ㅋㅋㅋㅋㅋ맞아요 피자마루 또 맛있는거 추천해주실만한거있나요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 피자마루 또 맛있는거 추천해주실만한거있나요",
