      "utterance": [
        {
          "id": "MDRW2100023843.1.1",
          "form": "안녕하세요 저녁은 드셨어요~?",
          "original_form": "안녕하세요 저녁은 드셨어요~?",
          "speaker_id": "1",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100023843.1.2",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023843.1.3",
          "form": "name1은 뭐드셨어요?",
          "original_form": "&name1&은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023843.1.4",
          "form": "저는 카레 먹었어요! 좋아하시나요~?",
          "original_form": "저는 카레 먹었어요! 좋아하시나요~?",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023843.1.5",
          "form": "가끔 먹어요!",
          "original_form": "가끔 먹어요!",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023843.1.6",
          "form": "어떤 카레 드셨어요?",
          "original_form": "어떤 카레 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023843.1.7",
          "form": "저는 고기 듬~뿍 넣어서 먹어용ㅎㅎㅎ",
          "original_form": "저는 고기 듬~뿍 넣어서 먹어용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023843.1.8",
          "form": "고기 좋아하세요?",
          "original_form": "고기 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023843.1.9",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100023843.1.10",
          "form": "소고기카레 맛있죠 :)",
          "original_form": "소고기카레 맛있죠 :)",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100023843.1.11",
          "form": "소고기도 카레에 넣으면 맛있나용??",
          "original_form": "소고기도 카레에 넣으면 맛있나용??",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023843.1.12",
          "form": "전 돼지고기 먹었어용!",
          "original_form": "전 돼지고기 먹었어용!",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023843.1.13",
          "form": "네 짱맛있어요 :)",
          "original_form": "네 짱맛있어요 :)",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023843.1.14",
          "form": "돼지고기 어떤부위 넣으셨어요?",
          "original_form": "돼지고기 어떤부위 넣으셨어요?",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023843.1.15",
          "form": "헐 소고기 넣을 생각은 한번도 안해봤는데ㅋㅋㅋㅋ 도전해볼게요",
          "original_form": "헐 소고기 넣을 생각은 한번도 안해봤는데ㅋㅋㅋㅋ 도전해볼게요",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023843.1.16",
          "form": "목살넣어요",
          "original_form": "목살넣어요",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023843.1.17",
          "form": "좀 의외죠?ㅋㅋㅋ",
          "original_form": "좀 의외죠?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023843.1.18",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023843.1.19",
          "form": "소고기 불고기거리 넣음 맛있어요 ㅎㅎ",
          "original_form": "소고기 불고기거리 넣음 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023843.1.20",
          "form": "아아 그 얇은거 오호",
          "original_form": "아아 그 얇은거 오호",
          "speaker_id": "1",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023843.1.21",
          "form": "지방적은부위가 맛있는것 같아요 :)",
          "original_form": "지방적은부위가 맛있는것 같아요 :)",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023843.1.22",
          "form": "꿀팁 감사합니당!ㅎㅎ",
          "original_form": "꿀팁 감사합니당!ㅎㅎ",
          "speaker_id": "1",
