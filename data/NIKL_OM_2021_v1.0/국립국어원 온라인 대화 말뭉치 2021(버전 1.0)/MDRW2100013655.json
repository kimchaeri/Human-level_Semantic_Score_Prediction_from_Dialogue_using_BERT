      "utterance": [
        {
          "id": "MDRW2100013655.1.1",
          "form": "최근에 뭐샀어?",
          "original_form": "최근에 뭐샀어?",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013655.1.2",
          "form": "크림치즈 샀어",
          "original_form": "크림치즈 샀어",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013655.1.3",
          "form": "빵에 바르는거얌?",
          "original_form": "빵에 바르는거얌?",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013655.1.4",
          "form": "응 빵에 발라먹는 거",
          "original_form": "응 빵에 발라먹는 거",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013655.1.5",
          "form": "필라델피아 크림치즈 샀어",
          "original_form": "필라델피아 크림치즈 샀어",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013655.1.6",
          "form": "넌 최근에 뭐 샀어?",
          "original_form": "넌 최근에 뭐 샀어?",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013655.1.7",
          "form": "나는 꼬죽칩",
          "original_form": "나는 꼬죽칩",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013655.1.8",
          "form": "꼬북칩.. ㅋㅋㅋ 엄청 사먹어",
          "original_form": "꼬북칩.. ㅋㅋㅋ 엄청 사먹어",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013655.1.9",
          "form": "ㅋㅋㅋㅋㅋ 꼬북칩 맛있지",
          "original_form": "ㅋㅋㅋㅋㅋ 꼬북칩 맛있지",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013655.1.10",
          "form": "난 딸기쨈도 사고 시리얼도 샀어",
          "original_form": "난 딸기쨈도 사고 시리얼도 샀어",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013655.1.11",
          "form": "아침으로 먹으려고",
          "original_form": "아침으로 먹으려고",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013655.1.12",
          "form": "그 외에 쇼핑한 건 뭐 있어?",
          "original_form": "그 외에 쇼핑한 건 뭐 있어?",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013655.1.13",
          "form": "소주 빼고? ㅋㅋㅋ",
          "original_form": "소주 빼고? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013655.1.14",
          "form": "넌 제일 비싸게 산게 뭐야?",
          "original_form": "넌 제일 비싸게 산게 뭐야?",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013655.1.15",
          "form": "최근에 비싸게 산 거 휴대폰?",
          "original_form": "최근에 비싸게 산 거 휴대폰?",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013655.1.16",
          "form": "신형은 아니고 중고로 샀어",
          "original_form": "신형은 아니고 중고로 샀어",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013655.1.17",
          "form": "뭐삿엉? 아이뽕?",
          "original_form": "뭐삿엉? 아이뽕?",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013655.1.18",
          "form": "갤 울트라 그거",
          "original_form": "갤 울트라 그거",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013655.1.19",
          "form": "엄청 큰거 샀네... 안무거워?",
          "original_form": "엄청 큰거 샀네... 안무거워?",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013655.1.20",
          "form": "무겁더라... 그래도 화면도 크고 그래서 산 보람이 있어 ㅋㅋ",
          "original_form": "무겁더라... 그래도 화면도 크고 그래서 산 보람이 있어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013655.1.21",
          "form": "나도 최신 저나기 사고 싶다..",
          "original_form": "나도 최신 저나기 사고 싶다..",
          "speaker_id": "1",
          "time": "20210825 15:42"
        }
      ]
