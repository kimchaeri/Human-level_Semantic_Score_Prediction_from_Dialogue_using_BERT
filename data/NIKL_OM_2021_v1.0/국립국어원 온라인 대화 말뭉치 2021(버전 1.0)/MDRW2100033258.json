      "utterance": [
        {
          "id": "MDRW2100033258.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100033258.1.2",
          "form": "네 미역국이랑 먹었어요",
          "original_form": "네 미역국이랑 먹었어요",
          "speaker_id": "2",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100033258.1.3",
          "form": "잘드셨네요",
          "original_form": "잘드셨네요",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033258.1.4",
          "form": "저는 방금 먹고왔어요",
          "original_form": "저는 방금 먹고왔어요",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033258.1.5",
          "form": "퇴근하고 집에 늦게와서",
          "original_form": "퇴근하고 집에 늦게와서",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033258.1.6",
          "form": "배고프셨겠어요. 뭐 드셨어요?",
          "original_form": "배고프셨겠어요. 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100033258.1.7",
          "form": "전 궁중떡볶이에 밥먹었어요 ㅎㅎ",
          "original_form": "전 궁중떡볶이에 밥먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100033258.1.8",
          "form": "늦게먹으면 안되는데ㅜㅜ",
          "original_form": "늦게먹으면 안되는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100033258.1.9",
          "form": "궁중떡볶이! 어디서 파는 건가요? 아님 집에서 만드셨나요?",
          "original_form": "궁중떡볶이! 어디서 파는 건가요? 아님 집에서 만드셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033258.1.10",
          "form": "집에서 만든거요 ㅋㅋ",
          "original_form": "집에서 만든거요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033258.1.11",
          "form": "직접 만드셨어요?",
          "original_form": "직접 만드셨어요?",
          "speaker_id": "2",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100033258.1.12",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210907 06:23"
        },
        {
          "id": "MDRW2100033258.1.13",
          "form": "엄마가 만들어주신거",
          "original_form": "엄마가 만들어주신거",
          "speaker_id": "1",
          "time": "20210907 06:23"
        },
        {
          "id": "MDRW2100033258.1.14",
          "form": "엄마표가 맛있죠 ㅎㅎ",
          "original_form": "엄마표가 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100033258.1.15",
          "form": "그럼요 ㅋㅋ",
          "original_form": "그럼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100033258.1.16",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100033258.1.17",
          "form": "네 소시지야채볶음이랑 다른 반찬들해서 먹었어요",
          "original_form": "네 소시지야채볶음이랑 다른 반찬들해서 먹었어요",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100033258.1.18",
          "form": "자취하세요?",
          "original_form": "자취하세요?",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100033258.1.19",
          "form": "아뇨ㅎㅎ 제가 먼저 와서 쏘야 하고 나머지는 있는 반찬 꺼냈어요",
          "original_form": "아뇨ㅎㅎ 제가 먼저 와서 쏘야 하고 나머지는 있는 반찬 꺼냈어요",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100033258.1.20",
          "form": "쏘야 맛있었겠네요",
          "original_form": "쏘야 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210907 20:00"
        }
      ]
