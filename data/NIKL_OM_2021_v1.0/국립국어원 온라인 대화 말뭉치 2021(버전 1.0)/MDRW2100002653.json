      "utterance": [
        {
          "id": "MDRW2100002653.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002653.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002653.1.3",
          "form": "집안일 하시나요?",
          "original_form": "집안일 하시나요?",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002653.1.4",
          "form": "동생이랑 둘이살아서 집안일은 나눠서 하고 잇어요",
          "original_form": "동생이랑 둘이살아서 집안일은 나눠서 하고 잇어요",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002653.1.5",
          "form": "아~어떤거 맡으셨나요~?",
          "original_form": "아~어떤거 맡으셨나요~?",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002653.1.6",
          "form": "나눠서 한다고는 해도 딱 어떤걸 담당하는건 아니고요",
          "original_form": "나눠서 한다고는 해도 딱 어떤걸 담당하는건 아니고요",
          "speaker_id": "2",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002653.1.7",
          "form": "아~시간 나는 사람이 하나요?",
          "original_form": "아~시간 나는 사람이 하나요?",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002653.1.8",
          "form": "일찍퇴근한 사람이 저녁하면 다른사람이 뒷정리",
          "original_form": "일찍퇴근한 사람이 저녁하면 다른사람이 뒷정리",
          "speaker_id": "2",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002653.1.9",
          "form": "이런식이요",
          "original_form": "이런식이요",
          "speaker_id": "2",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002653.1.10",
          "form": "아~그렇군요~ㅎ 저는 주로 제가 해요",
          "original_form": "아~그렇군요~ㅎ 저는 주로 제가 해요",
          "speaker_id": "1",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002653.1.11",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002653.1.12",
          "form": "네~결혼 했어요~ㅎ",
          "original_form": "네~결혼 했어요~ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002653.1.13",
          "form": "결혼하신분들이 혼자서 다하는 분들 많더라고요",
          "original_form": "결혼하신분들이 혼자서 다하는 분들 많더라고요",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002653.1.14",
          "form": "저희 언니도 그렇고요",
          "original_form": "저희 언니도 그렇고요",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002653.1.15",
          "form": "결혼하고 처음에 잘 정리해야 할것 같아요",
          "original_form": "결혼하고 처음에 잘 정리해야 할것 같아요",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002653.1.16",
          "form": "맞아요 보니까 다들 그것땜에 싸움나더라고요",
          "original_form": "맞아요 보니까 다들 그것땜에 싸움나더라고요",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002653.1.17",
          "form": "네 엄청 많이 싸웠어요",
          "original_form": "네 엄청 많이 싸웠어요",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002653.1.18",
          "form": "싸우셨는데도 거의 혼자하시는 거세요",
          "original_form": "싸우셨는데도 거의 혼자하시는 거세요",
          "speaker_id": "2",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002653.1.19",
          "form": "네~제가 일찍 퇴근하고 주말에는 쉬니까 그렇게 되버렸어요ㅠㅠ",
          "original_form": "네~제가 일찍 퇴근하고 주말에는 쉬니까 그렇게 되버렸어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002653.1.20",
          "form": "저희언니보니까 형부가 연차쓰고 쉬는데도 저녁에 퇴근해서 밥하더라고요",
          "original_form": "저희언니보니까 형부가 연차쓰고 쉬는데도 저녁에 퇴근해서 밥하더라고요",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002653.1.21",
          "form": "헛ㅠㅠ 그건 너무 하네요ㅠ",
          "original_form": "헛ㅠㅠ 그건 너무 하네요ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:43"
        }
      ]
