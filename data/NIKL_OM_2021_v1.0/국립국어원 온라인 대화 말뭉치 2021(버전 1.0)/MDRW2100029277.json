      "utterance": [
        {
          "id": "MDRW2100029277.1.1",
          "form": "여행은 좋아하세요?",
          "original_form": "여행은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029277.1.2",
          "form": "코로나 때문에 해외여행도 못가고ㅠ",
          "original_form": "코로나 때문에 해외여행도 못가고ㅠ",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029277.1.3",
          "form": "그러게욧 ㅜㅡ 못나가니 더 가고 싶어지네요",
          "original_form": "그러게욧 ㅜㅡ 못나가니 더 가고 싶어지네요",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029277.1.4",
          "form": "저번에 제주도 갔다왔는데 너무 좋더라구요 ㅎㅎ",
          "original_form": "저번에 제주도 갔다왔는데 너무 좋더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029277.1.5",
          "form": "헛 제주도도 또 가고싶어지네요 ㅎㅎㅎ",
          "original_form": "헛 제주도도 또 가고싶어지네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029277.1.6",
          "form": "몇월에 다녀오셨어요?",
          "original_form": "몇월에 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029277.1.7",
          "form": "4월쯤이요 ㅋㅋ",
          "original_form": "4월쯤이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029277.1.8",
          "form": "ㅎ 4월이면 적당히 안붐비면서도 좋을때 다녀오셨네요",
          "original_form": "ㅎ 4월이면 적당히 안붐비면서도 좋을때 다녀오셨네요",
          "speaker_id": "2",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029277.1.9",
          "form": "가을엔 또 여행가고 싶네요 ㅋㅋ",
          "original_form": "가을엔 또 여행가고 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029277.1.10",
          "form": "가을엔 진짜 여행가면 좋죠 ㅎㅎㅎㅎ",
          "original_form": "가을엔 진짜 여행가면 좋죠 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029277.1.11",
          "form": "날씨도 좋고 단풍까지 ㅎ",
          "original_form": "날씨도 좋고 단풍까지 ㅎ",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029277.1.12",
          "form": "전남에 여행갈만한데 있을까요?",
          "original_form": "전남에 여행갈만한데 있을까요?",
          "speaker_id": "1",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029277.1.13",
          "form": "흠 여수는 가보셨어요?",
          "original_form": "흠 여수는 가보셨어요?",
          "speaker_id": "2",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029277.1.14",
          "form": "여수도 좋죠 ㅋㅋ",
          "original_form": "여수도 좋죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029277.1.15",
          "form": "전 산보단 바닷가가 좋아서요 ㅎㅎㅎㅎ",
          "original_form": "전 산보단 바닷가가 좋아서요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029277.1.16",
          "form": "고흥도 몇번 가봤는데 좋더라구요",
          "original_form": "고흥도 몇번 가봤는데 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029277.1.17",
          "form": "진도 쏠비치 한번 가봐야하는데 ㅎㅎ",
          "original_form": "진도 쏠비치 한번 가봐야하는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029277.1.18",
          "form": "거기가 유명하던데 ㅎ 한번 가보곤 싶네요 ㅎㅎㅎㅎ",
          "original_form": "거기가 유명하던데 ㅎ 한번 가보곤 싶네요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:34"
        }
