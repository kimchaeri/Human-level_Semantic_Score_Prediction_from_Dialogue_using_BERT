      "utterance": [
        {
          "id": "MDRW2100002467.1.1",
          "form": "ㅋㅋ 키우면 가족이죠^^ 한가족~~",
          "original_form": "ㅋㅋ 키우면 가족이죠^^ 한가족~~",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002467.1.2",
          "form": "그리고 시간지날수록강아지들도",
          "original_form": "그리고 시간지날수록강아지들도",
          "speaker_id": "1",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002467.1.3",
          "form": "사람같아지지않나욬ㅋㅋ",
          "original_form": "사람같아지지않나욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002467.1.4",
          "form": "맞아욬ㅋㅋ 사람같아요",
          "original_form": "맞아욬ㅋㅋ 사람같아요",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002467.1.5",
          "form": "날 좋으니까 슬슬 가을옷 사고싶네요",
          "original_form": "날 좋으니까 슬슬 가을옷 사고싶네요",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002467.1.6",
          "form": "이미  쇼핑몰에는 가을옷 나오더라구요^^",
          "original_form": "이미  쇼핑몰에는 가을옷 나오더라구요^^",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002467.1.7",
          "form": "역시.... 참 옷은 사도사도 입을것이 없는 기분이예요",
          "original_form": "역시.... 참 옷은 사도사도 입을것이 없는 기분이예요",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002467.1.8",
          "form": "분명 한달전에 여름셔츠 사입었는데...",
          "original_form": "분명 한달전에 여름셔츠 사입었는데...",
          "speaker_id": "2",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002467.1.9",
          "form": "ㅋㅋ맞아요 항상그렇죠",
          "original_form": "ㅋㅋ맞아요 항상그렇죠",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100002467.1.10",
          "form": "계절이 바뀌고나면  작년엔 벗고다녔나 싶을만큼ㅋㅋㅋ",
          "original_form": "계절이 바뀌고나면  작년엔 벗고다녔나 싶을만큼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100002467.1.11",
          "form": "맞아요 이렇게 날씨가 애매해지니까 가을옷을 구매하고싶어져요!ㅎ.ㅎ",
          "original_form": "맞아요 이렇게 날씨가 애매해지니까 가을옷을 구매하고싶어져요!ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100002467.1.12",
          "form": "최근에 쇼핑한건 뭐있으세요?ㅎㅎ",
          "original_form": "최근에 쇼핑한건 뭐있으세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100002467.1.13",
          "form": "전 지플립3를 구매했답니다!ㅎㅎㅎ",
          "original_form": "전 지플립3를 구매했답니다!ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100002467.1.14",
          "form": "아직배송은 안됐지만요^^;",
          "original_form": "아직배송은 안됐지만요^^;",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100002467.1.15",
          "form": "배송을 기다리고있어요!!",
          "original_form": "배송을 기다리고있어요!!",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100002467.1.16",
          "form": "오늘은 생수가 배송왔네요 ㅋ 삼다수!",
          "original_form": "오늘은 생수가 배송왔네요 ㅋ 삼다수!",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100002467.1.17",
          "form": "최근에 쇼핑한건 슈요!",
          "original_form": "최근에 쇼핑한건 슈요!",
          "speaker_id": "2",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100002467.1.18",
          "form": "밀푀유슈랑 카야슈? 사서 친구 사다줫어요!",
          "original_form": "밀푀유슈랑 카야슈? 사서 친구 사다줫어요!",
          "speaker_id": "2",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100002467.1.19",
          "form": "아 그리고 복숭아요!",
          "original_form": "아 그리고 복숭아요!",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100002467.1.20",
          "form": "배송받는건 늘 좋아요",
          "original_form": "배송받는건 늘 좋아요",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100002467.1.21",
          "form": "우와 맛있는거 많이 주문하시네요~",
          "original_form": "우와 맛있는거 많이 주문하시네요~",
          "speaker_id": "1",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100002467.1.22",
          "form": "과일 배송받아도 괜찮던가요?",
          "original_form": "과일 배송받아도 괜찮던가요?",
          "speaker_id": "1",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100002467.1.23",
          "form": "전 실패한이후로  구매를 못하겠어요",
          "original_form": "전 실패한이후로  구매를 못하겠어요",
          "speaker_id": "1",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100002467.1.24",
          "form": "다른건 전부 배송시키는데 아주작은물품조차",
          "original_form": "다른건 전부 배송시키는데 아주작은물품조차",
          "speaker_id": "1",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100002467.1.25",
          "form": "어 저도 이번에 시도했는데 나쁘지않았어요!",
          "original_form": "어 저도 이번에 시도했는데 나쁘지않았어요!",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100002467.1.26",
          "form": "운이 좋았던건가! ㅎ.ㅎ",
          "original_form": "운이 좋았던건가! ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100002467.1.27",
          "form": "지플립3 언제 배송예정인가요? ㅎ.ㅎ설레겠어요",
          "original_form": "지플립3 언제 배송예정인가요? ㅎ.ㅎ설레겠어요",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100002467.1.28",
          "form": "우왕 다행이네요 ㅋㅋ  쇼핑성공!",
          "original_form": "우왕 다행이네요 ㅋㅋ  쇼핑성공!",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100002467.1.29",
          "form": "배송출발해서 오늘올줄알았는데ㅠㅠ",
          "original_form": "배송출발해서 오늘올줄알았는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100002467.1.30",
          "form": "내일오려나봐요~~",
          "original_form": "내일오려나봐요~~",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100002467.1.31",
          "form": "앗...배송 기다리시는거 듣고 생각났는데",
          "original_form": "앗...배송 기다리시는거 듣고 생각났는데",
          "speaker_id": "2",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100002467.1.32",
          "form": "저 일리커피머신 기다리고있어요",
          "original_form": "저 일리커피머신 기다리고있어요",
          "speaker_id": "2",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100002467.1.33",
          "form": "어제 출발했다는데 아직도...입하..중",
          "original_form": "어제 출발했다는데 아직도...입하..중",
          "speaker_id": "2",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100002467.1.34",
          "form": "애타게 기다리는건 꼭 늦게오는거같아요..",
          "original_form": "애타게 기다리는건 꼭 늦게오는거같아요..",
          "speaker_id": "2",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100002467.1.35",
          "form": "우왕 일리^^",
          "original_form": "우왕 일리^^",
          "speaker_id": "1",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100002467.1.36",
          "form": "커피좋아하시나봐요~",
          "original_form": "커피좋아하시나봐요~",
          "speaker_id": "1",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100002467.1.37",
          "form": "저는 카페인에 엄청 민감해서 못마시는데 향만 맡아도 좋네용",
          "original_form": "저는 카페인에 엄청 민감해서 못마시는데 향만 맡아도 좋네용",
          "speaker_id": "1",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100002467.1.38",
          "form": "얼른 배송왔으면 좋겠어요~",
          "original_form": "얼른 배송왔으면 좋겠어요~",
          "speaker_id": "1",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100002467.1.39",
          "form": "좋아해요! 식도염때문에 카페인 쎄게는 못마시는데 좋아해서ㅠㅠ",
          "original_form": "좋아해요! 식도염때문에 카페인 쎄게는 못마시는데 좋아해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100002467.1.40",
          "form": "카페인에 민감하시구나..ㅠㅠ",
          "original_form": "카페인에 민감하시구나..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100002467.1.41",
          "form": "아..식도염이 있으시구나ㅜㅜ 약하게 드셔야겠네요",
          "original_form": "아..식도염이 있으시구나ㅜㅜ 약하게 드셔야겠네요",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100002467.1.42",
          "form": "지플립 케이스나 이런건 구매하셨어요?",
          "original_form": "지플립 케이스나 이런건 구매하셨어요?",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100002467.1.43",
          "form": "네 저는 아예 잠을못자요 흑흑",
          "original_form": "네 저는 아예 잠을못자요 흑흑",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100002467.1.44",
          "form": "아직 케이스 구매못했는데 계속 구경중이에요 ㅎㅎ 신나네요",
          "original_form": "아직 케이스 구매못했는데 계속 구경중이에요 ㅎㅎ 신나네요",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100002467.1.45",
          "form": "쇼핑은 즐거워요~ 돈쓰는게 제일즐겁죠 ㅋㅋ",
          "original_form": "쇼핑은 즐거워요~ 돈쓰는게 제일즐겁죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100002467.1.46",
          "form": "투명케이스도 넘 예쁠꺼같아요!",
          "original_form": "투명케이스도 넘 예쁠꺼같아요!",
          "speaker_id": "2",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100002467.1.47",
          "form": "지플립자체가 너무 예뻐서",
          "original_form": "지플립자체가 너무 예뻐서",
