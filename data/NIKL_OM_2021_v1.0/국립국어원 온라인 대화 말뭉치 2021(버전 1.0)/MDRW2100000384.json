      "utterance": [
        {
          "id": "MDRW2100000384.1.1",
          "form": "싸이월드 하셨나요?",
          "original_form": "싸이월드 하셨나요?",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000384.1.2",
          "form": "많이 했죠",
          "original_form": "많이 했죠",
          "speaker_id": "2",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000384.1.3",
          "form": "추억의 싸이월드",
          "original_form": "추억의 싸이월드",
          "speaker_id": "2",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000384.1.4",
          "form": "아이디 찾으셨어요?",
          "original_form": "아이디 찾으셨어요?",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000384.1.5",
          "form": "아직 사진 많이 있나요?",
          "original_form": "아직 사진 많이 있나요?",
          "speaker_id": "2",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000384.1.6",
          "form": "전 아직이요",
          "original_form": "전 아직이요",
          "speaker_id": "2",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000384.1.7",
          "form": "사느라 바빠서 다시 들어가볼 생각을 못했네요 흑역사라 ㅎㅎ",
          "original_form": "사느라 바빠서 다시 들어가볼 생각을 못했네요 흑역사라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000384.1.8",
          "form": "저는 아이디 찾기까지 했어요 비공개로 감춰둔 앨범 다시 꺼내고싶네요",
          "original_form": "저는 아이디 찾기까지 했어요 비공개로 감춰둔 앨범 다시 꺼내고싶네요",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000384.1.9",
          "form": "저도 옛날 사진 다시 보고 싶네요.",
          "original_form": "저도 옛날 사진 다시 보고 싶네요.",
          "speaker_id": "2",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000384.1.10",
          "form": "1촌 파도타기도 ㅎㅎ",
          "original_form": "1촌 파도타기도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000384.1.11",
          "form": "도토리도 많이 있었는데 빨리 나오면 좋겠네요",
          "original_form": "도토리도 많이 있었는데 빨리 나오면 좋겠네요",
          "speaker_id": "1",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000384.1.12",
          "form": "도토리는 환불 안해줬나요",
          "original_form": "도토리는 환불 안해줬나요",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000384.1.13",
          "form": "싸이월드 검색해서 들어가면 해주더라구요",
          "original_form": "싸이월드 검색해서 들어가면 해주더라구요",
          "speaker_id": "1",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000384.1.14",
          "form": "저도 사놓은 노래 많았는데 듣고 싶네요",
          "original_form": "저도 사놓은 노래 많았는데 듣고 싶네요",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000384.1.15",
          "form": "노래도 미니미도 추억이네요 빨리 부활해줬으면..",
          "original_form": "노래도 미니미도 추억이네요 빨리 부활해줬으면..",
          "speaker_id": "1",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000384.1.16",
          "form": "미니미 고르는 거 재미있었는데",
          "original_form": "미니미 고르는 거 재미있었는데",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000384.1.17",
          "form": "언제쯤 예전처럼 이용할 수 있을까요",
          "original_form": "언제쯤 예전처럼 이용할 수 있을까요",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000384.1.18",
          "form": "검색해보니 아직은 안되네요",
          "original_form": "검색해보니 아직은 안되네요",
          "speaker_id": "1",
          "time": "20210819 15:56"
        }
