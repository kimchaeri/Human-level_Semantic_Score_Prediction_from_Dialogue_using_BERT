        {
          "id": "MDRW2100001821.1.1",
          "form": "요즘 미세먼지 없어서 하늘이 엄청 깨끗하더라",
          "original_form": "요즘 미세먼지 없어서 하늘이 엄청 깨끗하더라",
          "speaker_id": "2",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001821.1.2",
          "form": "맞아 너무 쾌청해",
          "original_form": "맞아 너무 쾌청해",
          "speaker_id": "1",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001821.1.3",
          "form": "사진찍어 두고 싶은 하늘",
          "original_form": "사진찍어 두고 싶은 하늘",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001821.1.4",
          "form": "난 너무 예뻐서 찍어뒀엉 ㅎㅎ",
          "original_form": "난 너무 예뻐서 찍어뒀엉 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001821.1.5",
          "form": "ㅎㅎㅎ 역시나",
          "original_form": "ㅎㅎㅎ 역시나",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001821.1.6",
          "form": "달도 엄청 밝게 보여",
          "original_form": "달도 엄청 밝게 보여",
          "speaker_id": "1",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001821.1.7",
          "form": "미세먼지 없으니 살 것 같아",
          "original_form": "미세먼지 없으니 살 것 같아",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001821.1.8",
          "form": "공기청정기 돌아갈 일이 없엉 ㅎㅎ",
          "original_form": "공기청정기 돌아갈 일이 없엉 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001821.1.9",
          "form": "마스크만 안쓰면 딱인데",
          "original_form": "마스크만 안쓰면 딱인데",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001821.1.10",
          "form": "곧 그렇게 될거야",
          "original_form": "곧 그렇게 될거야",
          "speaker_id": "2",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001821.1.11",
          "form": "다음주엔 비온다던데",
          "original_form": "다음주엔 비온다던데",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001821.1.12",
          "form": "요즘 먹구름이 자주 끼더만",
          "original_form": "요즘 먹구름이 자주 끼더만",
          "speaker_id": "2",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001821.1.13",
          "form": "날씨가 변덕이 심해",
          "original_form": "날씨가 변덕이 심해",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001821.1.14",
          "form": "동남아처럼 갑자기 비오고 ㅎㅎ",
          "original_form": "동남아처럼 갑자기 비오고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001821.1.15",
          "form": "맞아 우산 항상 가지고 다녀야 됑",
          "original_form": "맞아 우산 항상 가지고 다녀야 됑",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001821.1.16",
          "form": "일기예보 잘 안맞아서 항상 가지고 다니는 수밖에 ㅎㅎ",
          "original_form": "일기예보 잘 안맞아서 항상 가지고 다니는 수밖에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001821.1.17",
          "form": "진짜 일기예보 잘 안맞긴 하더라",
          "original_form": "진짜 일기예보 잘 안맞긴 하더라",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001821.1.18",
          "form": "저번주 날씨도 거의 안맞음 ㅋ",
          "original_form": "저번주 날씨도 거의 안맞음 ㅋ",
          "speaker_id": "2",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001821.1.19",
          "form": "최신 컴퓨터 쓴다는데 왜그런지 ㅠ",
          "original_form": "최신 컴퓨터 쓴다는데 왜그런지 ㅠ",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100001821.1.20",
          "form": "귀뚜라미 우네.. 이제 가을인가봐",
