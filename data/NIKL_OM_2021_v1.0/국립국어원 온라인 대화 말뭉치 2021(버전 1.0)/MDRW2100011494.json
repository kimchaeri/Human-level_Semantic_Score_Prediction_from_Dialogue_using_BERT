      "utterance": [
        {
          "id": "MDRW2100011494.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100011494.1.2",
          "form": "안녕하세요 반려동물을 키우시나요?",
          "original_form": "안녕하세요 반려동물을 키우시나요?",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100011494.1.3",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100011494.1.4",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "1",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100011494.1.5",
          "form": "저도요. 혹시 한 번도 키워 보신 적 없으신 건가요?",
          "original_form": "저도요. 혹시 한 번도 키워 보신 적 없으신 건가요?",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011494.1.6",
          "form": "아니요 저도요",
          "original_form": "아니요 저도요",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100011494.1.7",
          "form": "강아지 좋아하세요?",
          "original_form": "강아지 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100011494.1.8",
          "form": "강아지 귀엽죠 ㅋㅋ",
          "original_form": "강아지 귀엽죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100011494.1.9",
          "form": "고양이는요?",
          "original_form": "고양이는요?",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100011494.1.10",
          "form": "고양이도 좋아해요. 근데 반려동물 키우지는 못할 것 같아요",
          "original_form": "고양이도 좋아해요. 근데 반려동물 키우지는 못할 것 같아요",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100011494.1.11",
          "form": "ㅠㅠ 왜죠?",
          "original_form": "ㅠㅠ 왜죠?",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100011494.1.12",
          "form": "아무래도 동물을 키우는 데는 책임감이 필요하잖아요",
          "original_form": "아무래도 동물을 키우는 데는 책임감이 필요하잖아요",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100011494.1.13",
          "form": "알러지가 있기도 하고요. 고양이 키우는 집에 갔더니 콧물이 자꾸 나더라구요 ㅋㅋㅋ",
          "original_form": "알러지가 있기도 하고요. 고양이 키우는 집에 갔더니 콧물이 자꾸 나더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100011494.1.14",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100011494.1.15",
          "form": "저는 강아지 진짜키우고싶어요",
          "original_form": "저는 강아지 진짜키우고싶어요",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100011494.1.16",
          "form": "지금은 키울 수 있는 상황이 아닌가요?",
          "original_form": "지금은 키울 수 있는 상황이 아닌가요?",
          "speaker_id": "2",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100011494.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100011494.1.18",
          "form": "강아지도 오래혼자두면",
          "original_form": "강아지도 오래혼자두면",
          "speaker_id": "1",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100011494.1.19",
          "form": "학대잖아요ㅠ",
          "original_form": "학대잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100011494.1.20",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100011494.1.21",
          "form": "신경써야 할 점이 참 많더라구요",
          "original_form": "신경써야 할 점이 참 많더라구요",
          "speaker_id": "2",
          "time": "20210825 00:14"
        }
      ]
