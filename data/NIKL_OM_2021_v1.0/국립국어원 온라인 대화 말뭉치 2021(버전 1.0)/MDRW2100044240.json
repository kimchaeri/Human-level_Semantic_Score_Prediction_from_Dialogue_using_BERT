      "utterance": [
        {
          "id": "MDRW2100044240.1.1",
          "form": "오늘 작업은 잘 끝났나??",
          "original_form": "오늘 작업은 잘 끝났나??",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044240.1.2",
          "form": "웅~ 완벽하데 끝냈지~~~~",
          "original_form": "웅~ 완벽하데 끝냈지~~~~",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044240.1.3",
          "form": "수고했네~",
          "original_form": "수고했네~",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044240.1.4",
          "form": "아예 끝낭거야??",
          "original_form": "아예 끝낭거야??",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044240.1.5",
          "form": "일단은~ 끝!!!",
          "original_form": "일단은~ 끝!!!",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044240.1.6",
          "form": "근데 또 수정사항 주면.. 해야하고..",
          "original_form": "근데 또 수정사항 주면.. 해야하고..",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044240.1.7",
          "form": "끝이여도 꿑이 아니네",
          "original_form": "끝이여도 꿑이 아니네",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044240.1.8",
          "form": "어쩔슈없이.. 회사일이라는게...ㅠ",
          "original_form": "어쩔슈없이.. 회사일이라는게...ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044240.1.9",
          "form": "다른 일은 없는 거야???",
          "original_form": "다른 일은 없는 거야???",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044240.1.10",
          "form": "다른 일 바로 시작하는데...",
          "original_form": "다른 일 바로 시작하는데...",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044240.1.11",
          "form": "겹치면 어쩔수없는 거고..ㅠ",
          "original_form": "겹치면 어쩔수없는 거고..ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044240.1.12",
          "form": "아이고. 일이많으면 야근인가??",
          "original_form": "아이고. 일이많으면 야근인가??",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044240.1.13",
          "form": "그러겠지... 어쩔수없이..",
          "original_form": "그러겠지... 어쩔수없이..",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044240.1.14",
          "form": "고생이네 우리딸..ㅠ",
          "original_form": "고생이네 우리딸..ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044240.1.15",
          "form": "야근비는 없는가??",
          "original_form": "야근비는 없는가??",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044240.1.16",
          "form": "당연하게도... 없다..",
          "original_form": "당연하게도... 없다..",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044240.1.17",
          "form": "이제부터는 52시간?? 그거라서 야근수당 챙겨줘야한다던데",
          "original_form": "이제부터는 52시간?? 그거라서 야근수당 챙겨줘야한다던데",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044240.1.18",
          "form": "법은 그런데 직업특성상 어쩔 수앖지 않냐며..",
          "original_form": "법은 그런데 직업특성상 어쩔 수앖지 않냐며..",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044240.1.19",
          "form": "대표님이 그냥 넘겨버리심..ㅠ",
          "original_form": "대표님이 그냥 넘겨버리심..ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044240.1.20",
          "form": "아이고.... 어떡하나",
          "original_form": "아이고.... 어떡하나",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044240.1.21",
          "form": "어쩔수없는 거지",
          "original_form": "어쩔수없는 거지",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044240.1.22",
          "form": "그냥 무상으로 야근해주는 수밖에..ㅠ",
