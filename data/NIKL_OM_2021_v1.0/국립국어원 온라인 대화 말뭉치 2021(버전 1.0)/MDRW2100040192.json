      "utterance": [
        {
          "id": "MDRW2100040192.1.1",
          "form": "최근 잘 산템 있나요",
          "original_form": "최근 잘 산템 있나요",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100040192.1.2",
          "form": "구매하고픈거 있어세요",
          "original_form": "구매하고픈거 있어세요",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100040192.1.3",
          "form": "글쎄요..",
          "original_form": "글쎄요..",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100040192.1.4",
          "form": "딱히 사고싶은건없네요",
          "original_form": "딱히 사고싶은건없네요",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100040192.1.5",
          "form": "오 저도 규매욕 사라져서 돈 아끼고 싶어요",
          "original_form": "오 저도 규매욕 사라져서 돈 아끼고 싶어요",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100040192.1.6",
          "form": "ㅎㅎ 아가씨면 사고싶은거많으실텐데~~",
          "original_form": "ㅎㅎ 아가씨면 사고싶은거많으실텐데~~",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100040192.1.7",
          "form": "맞아요 옷이랑 구두 사도사도 모자라요",
          "original_form": "맞아요 옷이랑 구두 사도사도 모자라요",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100040192.1.8",
          "form": "가장예쁠때니까 많이꾸미세요",
          "original_form": "가장예쁠때니까 많이꾸미세요",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100040192.1.9",
          "form": "오 그게 젤 좋겠죠?,",
          "original_form": "오 그게 젤 좋겠죠?,",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100040192.1.10",
          "form": "나중에 돈은 아끼게되겠죠?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나중에 돈은 아끼게되겠죠?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100040192.1.11",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100040192.1.12",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "2",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100040192.1.13",
          "form": "나를위해 쓰세요",
          "original_form": "나를위해 쓰세요",
          "speaker_id": "2",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100040192.1.14",
          "form": "한번뿐인 인생인데 절약하다 가기는~~ㅎㅎ",
          "original_form": "한번뿐인 인생인데 절약하다 가기는~~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100040192.1.15",
          "form": "젊음은 다시오지 않으니~~",
          "original_form": "젊음은 다시오지 않으니~~",
          "speaker_id": "2",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100040192.1.16",
          "form": "우와 정말 멋져요ㅠㅠㅠㅠ 사진으로 많이 남겨놔야겠어요",
          "original_form": "우와 정말 멋져요ㅠㅠㅠㅠ 사진으로 많이 남겨놔야겠어요",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100040192.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100040192.1.18",
          "form": "즐기세요",
          "original_form": "즐기세요",
          "speaker_id": "2",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100040192.1.19",
          "form": "네네열심히 즐겨야죠",
          "original_form": "네네열심히 즐겨야죠",
          "speaker_id": "1",
          "time": "20210904 14:34"
        },
        {
          "id": "MDRW2100040192.1.20",
          "form": "젊음을 허비하능것같아요",
          "original_form": "젊음을 허비하능것같아요",
          "speaker_id": "1",
          "time": "20210904 14:34"
        },
        {
          "id": "MDRW2100040192.1.21",
          "form": "시간이금방가더라구요",
          "original_form": "시간이금방가더라구요",
          "speaker_id": "2",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100040192.1.22",
          "form": "그쵸 너무 일상에 치여서만 사는거같네요",
          "original_form": "그쵸 너무 일상에 치여서만 사는거같네요",
          "speaker_id": "1",
          "time": "20210904 14:36"
        },
        {
          "id": "MDRW2100040192.1.23",
          "form": "저도 실컷놀은것같은데 다시가고싶네요..젊었을때로",
          "original_form": "저도 실컷놀은것같은데 다시가고싶네요..젊었을때로",
          "speaker_id": "2",
