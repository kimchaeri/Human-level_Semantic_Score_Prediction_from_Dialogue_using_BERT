      "utterance": [
        {
          "id": "MDRW2100018653.1.1",
          "form": "난 처음에 아진 소각로 증설 하는 걸로 자꾸 소송하길래",
          "original_form": "난 처음에 아진 소각로 증설 하는 걸로 자꾸 소송하길래",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018653.1.2",
          "form": "근데 그게 눈으로 확인할 수 있는게 아니니",
          "original_form": "근데 그게 눈으로 확인할 수 있는게 아니니",
          "speaker_id": "1",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018653.1.3",
          "form": "왜 여기 코앞에 있는 거는 안 건드리고",
          "original_form": "왜 여기 코앞에 있는 거는 안 건드리고",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018653.1.4",
          "form": "저기에 만 그러는지 싶었는데",
          "original_form": "저기에 만 그러는지 싶었는데",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018653.1.5",
          "form": "여기 세화제 지인가 여기는 나름 대기업이고",
          "original_form": "여기 세화제 지인가 여기는 나름 대기업이고",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018653.1.6",
          "form": "그래서 못건들은 줄 알았는데",
          "original_form": "그래서 못건들은 줄 알았는데",
          "speaker_id": "2",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018653.1.7",
          "form": "아진 증설하고 나면 여기도",
          "original_form": "아진 증설하고 나면 여기도",
          "speaker_id": "2",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018653.1.8",
          "form": "증설 할 거기 때문에 절대 하면 안 된다 하더라구요",
          "original_form": "증설 할 거기 때문에 절대 하면 안 된다 하더라구요",
          "speaker_id": "2",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018653.1.9",
          "form": "세화제 지는 진짜 너무 가까워요",
          "original_form": "세화제 지는 진짜 너무 가까워요",
          "speaker_id": "2",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018653.1.10",
          "form": "아하 그렇구만",
          "original_form": "아하 그렇구만",
          "speaker_id": "1",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100018653.1.11",
          "form": "그래 진짜 너무 가깝지",
          "original_form": "그래 진짜 너무 가깝지",
          "speaker_id": "1",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100018653.1.12",
          "form": "엄마집에서 연기나오는것만 봐도 불안함",
          "original_form": "엄마집에서 연기나오는것만 봐도 불안함",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018653.1.13",
          "form": "수증기라고는 하지만",
          "original_form": "수증기라고는 하지만",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018653.1.14",
          "form": "알수있나 뭐...",
          "original_form": "알수있나 뭐...",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018653.1.15",
          "form": "맞아요 근데 수 중근은 아닌 듯",
          "original_form": "맞아요 근데 수 중근은 아닌 듯",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018653.1.16",
          "form": "수준기",
          "original_form": "수준기",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018653.1.17",
          "form": "수증기",
          "original_form": "수증기",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018653.1.18",
          "form": "힘들고 만요",
          "original_form": "힘들고 만요",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018653.1.19",
          "form": "저번에 굴뚝을 청소하는지 쾅쾅거리길래 보니까",
          "original_form": "저번에 굴뚝을 청소하는지 쾅쾅거리길래 보니까",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018653.1.20",
          "form": "진짜 갈색 먼지가 팍팍 날려요",
          "original_form": "진짜 갈색 먼지가 팍팍 날려요",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018653.1.21",
          "form": "보고 엄청 놀랬어요",
          "original_form": "보고 엄청 놀랬어요",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018653.1.22",
          "form": "그래 그냥 수증기는 아닐듯",
          "original_form": "그래 그냥 수증기는 아닐듯",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018653.1.23",
          "form": "그러기엔 너무 진하다고 해야되나??",
          "original_form": "그러기엔 너무 진하다고 해야되나??",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018653.1.24",
          "form": "탁한느낌이 있음",
          "original_form": "탁한느낌이 있음",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018653.1.25",
          "form": "약 타서 그냥 하얗게 만들어서 날리는 걸 듯",
          "original_form": "약 타서 그냥 하얗게 만들어서 날리는 걸 듯",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018653.1.26",
          "form": "조사 함 해볼까요",
          "original_form": "조사 함 해볼까요",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018653.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018653.1.28",
          "form": "그래 시작해라 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래 시작해라 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018653.1.29",
          "form": "비온다 아들 우산 안가져갔는데",
          "original_form": "비온다 아들 우산 안가져갔는데",
          "speaker_id": "1",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018653.1.30",
          "form": "예비용 쓰고 오겠지",
          "original_form": "예비용 쓰고 오겠지",
          "speaker_id": "1",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018653.1.31",
          "form": "첫찌 우산 가져갔나??",
          "original_form": "첫찌 우산 가져갔나??",
          "speaker_id": "1",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018653.1.32",
          "form": "우산 가방에 넣어줬어요 신랑이ㅋㅋㅋㅋㅋ",
          "original_form": "우산 가방에 넣어줬어요 신랑이ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018653.1.33",
          "form": "엄마는 아무것도 모름ㅋㅋㅋ",
          "original_form": "엄마는 아무것도 모름ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018653.1.34",
          "form": "나는 비안 올 줄 알았는데",
          "original_form": "나는 비안 올 줄 알았는데",
          "speaker_id": "2",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018653.1.35",
          "form": "오 신랑센스있네",
          "original_form": "오 신랑센스있네",
          "speaker_id": "1",
          "time": "20210827 10:50"
        },
        {
          "id": "MDRW2100018653.1.36",
          "form": "학교에 삼단 우산 있는데",
          "original_form": "학교에 삼단 우산 있는데",
          "speaker_id": "1",
          "time": "20210827 10:50"
        },
        {
          "id": "MDRW2100018653.1.37",
          "form": "접는 게 불편하니 싫다더라고",
          "original_form": "접는 게 불편하니 싫다더라고",
          "speaker_id": "1",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018653.1.38",
          "form": "근데 긴우산은 매일 잃어버리고 옴",
          "original_form": "근데 긴우산은 매일 잃어버리고 옴",
          "speaker_id": "1",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018653.1.39",
          "form": "긴 투명 우산 하나도 지금 반도에서 돌아다니고 있을 듯",
          "original_form": "긴 투명 우산 하나도 지금 반도에서 돌아다니고 있을 듯",
          "speaker_id": "1",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018653.1.40",
          "form": "삼단 우산 선생님이 챙겨주면 참 좋을 텐데",
          "original_form": "삼단 우산 선생님이 챙겨주면 참 좋을 텐데",
          "speaker_id": "2",
          "time": "20210827 10:53"
        },
        {
          "id": "MDRW2100018653.1.41",
          "form": "긴 우산은 진짜 잘 잃어버려요",
          "original_form": "긴 우산은 진짜 잘 잃어버려요",
          "speaker_id": "2",
          "time": "20210827 10:53"
        },
        {
          "id": "MDRW2100018653.1.42",
          "form": "지금은 비 안오나 봐요 우산 안 쓰고 다니네요",
          "original_form": "지금은 비 안오나 봐요 우산 안 쓰고 다니네요",
          "speaker_id": "2",
          "time": "20210827 10:54"
        },
        {
          "id": "MDRW2100018653.1.43",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "1",
          "time": "20210827 10:56"
        },
        {
          "id": "MDRW2100018653.1.44",
          "form": "하교때까지 비 안오기를...",
          "original_form": "하교때까지 비 안오기를...",
          "speaker_id": "1",
          "time": "20210827 10:57"
        },
        {
          "id": "MDRW2100018653.1.45",
          "form": "주말에 비 안옴으로 바껴서",
          "original_form": "주말에 비 안옴으로 바껴서",
          "speaker_id": "1",
          "time": "20210827 10:57"
        },
        {
          "id": "MDRW2100018653.1.46",
          "form": "캠핑잡아놨는데 비가오네...화가나는군...",
          "original_form": "캠핑잡아놨는데 비가오네...화가나는군...",
          "speaker_id": "1",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018653.1.47",
          "form": "텐트 새건데 ㅠ",
          "original_form": "텐트 새건데 ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018653.1.48",
          "form": "우중캠핑 멋있지요",
          "original_form": "우중캠핑 멋있지요",
          "speaker_id": "2",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018653.1.49",
          "form": "새거 게 시 하는 겁니까",
          "original_form": "새거 게 시 하는 겁니까",
          "speaker_id": "2",
          "time": "20210827 10:58"
        }
