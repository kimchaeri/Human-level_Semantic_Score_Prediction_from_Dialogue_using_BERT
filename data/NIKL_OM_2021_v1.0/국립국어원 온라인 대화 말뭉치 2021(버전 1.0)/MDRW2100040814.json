      "utterance": [
        {
          "id": "MDRW2100040814.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100040814.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100040814.1.3",
          "form": "좋아하는 가수 있으세요?",
          "original_form": "좋아하는 가수 있으세요?",
          "speaker_id": "1",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040814.1.4",
          "form": "있다가 탈덕ㅋㅋ중이에요ㅋㅋ",
          "original_form": "있다가 탈덕ㅋㅋ중이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040814.1.5",
          "form": "ㅋㅋㅋ누구좋아하셨어요?",
          "original_form": "ㅋㅋㅋ누구좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040814.1.6",
          "form": "ㅋㅋ그 친구 잘못이 없어서ㅋㅋ익명으로 하겠습니다",
          "original_form": "ㅋㅋ그 친구 잘못이 없어서ㅋㅋ익명으로 하겠습니다",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040814.1.7",
          "form": "좋아하는 가수있으세요?",
          "original_form": "좋아하는 가수있으세요?",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040814.1.8",
          "form": "ㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋ네",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040814.1.9",
          "form": "누구요???",
          "original_form": "누구요???",
          "speaker_id": "2",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040814.1.10",
          "form": "악뮤 좋아해요",
          "original_form": "악뮤 좋아해요",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040814.1.11",
          "form": "아! ㅋㅋ저도 라이트하게 좋아해요 양다일하고",
          "original_form": "아! ㅋㅋ저도 라이트하게 좋아해요 양다일하고",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040814.1.12",
          "form": "좋아하는 노래 장르는 어떻게 되세요?",
          "original_form": "좋아하는 노래 장르는 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100040814.1.13",
          "form": "전 발라드 알엔비 클래식ㅋㅋ",
          "original_form": "전 발라드 알엔비 클래식ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040814.1.14",
          "form": "가리지않아요ㅋㅋ",
          "original_form": "가리지않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040814.1.15",
          "form": "그렇군요!!",
          "original_form": "그렇군요!!",
          "speaker_id": "1",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100040814.1.16",
          "form": "알엔비중엔 뭐가 좋으세요?",
          "original_form": "알엔비중엔 뭐가 좋으세요?",
          "speaker_id": "1",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100040814.1.17",
          "form": "저 새로운 노래 잘못 찾는데 유투버들이 머머할때 듣는 곡 이런거 쭉 듣다 찾아들어요!",
          "original_form": "저 새로운 노래 잘못 찾는데 유투버들이 머머할때 듣는 곡 이런거 쭉 듣다 찾아들어요!",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100040814.1.18",
          "form": "플레이리스트 좋은거 진짜 많죠",
          "original_form": "플레이리스트 좋은거 진짜 많죠",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100040814.1.19",
          "form": "한국인들 천재에요",
          "original_form": "한국인들 천재에요",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100040814.1.20",
          "form": "나른하게 뽀송뽀송 이렇게 자동검색되는 알앤비팝리스트 좋아해요",
          "original_form": "나른하게 뽀송뽀송 이렇게 자동검색되는 알앤비팝리스트 좋아해요",
