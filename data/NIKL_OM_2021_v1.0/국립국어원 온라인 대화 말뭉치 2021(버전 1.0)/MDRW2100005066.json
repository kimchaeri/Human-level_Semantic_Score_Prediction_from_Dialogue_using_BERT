      "utterance": [
        {
          "id": "MDRW2100005066.1.1",
          "form": "그럼 애기랑 공연도",
          "original_form": "그럼 애기랑 공연도",
          "speaker_id": "2",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005066.1.2",
          "form": "많이 보러 가시나요~?ㅎㅎ",
          "original_form": "많이 보러 가시나요~?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005066.1.3",
          "form": "공연은 코로나때문에ㅠㅠ",
          "original_form": "공연은 코로나때문에ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005066.1.4",
          "form": "최근에 보스베이비 영화봤어요~",
          "original_form": "최근에 보스베이비 영화봤어요~",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005066.1.5",
          "form": "연극 엄청 보고싶은데..",
          "original_form": "연극 엄청 보고싶은데..",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005066.1.6",
          "form": "지방에 살아서 공연이 잘 없네요ㅠ",
          "original_form": "지방에 살아서 공연이 잘 없네요ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005066.1.7",
          "form": "아ㅠㅠ 코로나ㅠㅠ 맞죠",
          "original_form": "아ㅠㅠ 코로나ㅠㅠ 맞죠",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005066.1.8",
          "form": "보스베이비 아이랑 보기에 괜찮던가요??",
          "original_form": "보스베이비 아이랑 보기에 괜찮던가요??",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005066.1.9",
          "form": "재미있어하는데 좀 길더라구요;; 잠들뻔 했네요 ㅋㅋ",
          "original_form": "재미있어하는데 좀 길더라구요;; 잠들뻔 했네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005066.1.10",
          "form": "콘서트 가본적 있으셔요??",
          "original_form": "콘서트 가본적 있으셔요??",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005066.1.11",
          "form": "네~~ ㅎㅎ 저는 좋아하는 가수가 있어서",
          "original_form": "네~~ ㅎㅎ 저는 좋아하는 가수가 있어서",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005066.1.12",
          "form": "코로나 전에는 많이 갔어요 ^^",
          "original_form": "코로나 전에는 많이 갔어요 ^^",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005066.1.13",
          "form": "어떤 가수 좋아하셔요??",
          "original_form": "어떤 가수 좋아하셔요??",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005066.1.14",
          "form": "앗 아시려나...ㅎㅎ",
          "original_form": "앗 아시려나...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005066.1.15",
          "form": "샤이니의 태민이라구...ㅎㅎ",
          "original_form": "샤이니의 태민이라구...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005066.1.16",
          "form": "아실 것 같은데",
          "original_form": "아실 것 같은데",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005066.1.17",
          "form": "혹시.. 별로 안 유명한가...ㅜㅜ",
          "original_form": "혹시.. 별로 안 유명한가...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005066.1.18",
          "form": "알죠알죠^^",
          "original_form": "알죠알죠^^",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005066.1.19",
          "form": "태민이 대구사람이잖아요^^",
          "original_form": "태민이 대구사람이잖아요^^",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005066.1.20",
          "form": "저도 대구 ㅋㅋㅋ",
          "original_form": "저도 대구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005066.1.21",
          "form": "코로나 없어지면 콘서트도 하겠죠??",
          "original_form": "코로나 없어지면 콘서트도 하겠죠??",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005066.1.22",
          "form": "앗 ㅎㅎㅎㅎㅎ",
          "original_form": "앗 ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005066.1.23",
          "form": "요즘은 코로나 있어두",
          "original_form": "요즘은 코로나 있어두",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005066.1.24",
          "form": "비대면으로 콘서트하고 그러더라고요 ㅎㅎ",
          "original_form": "비대면으로 콘서트하고 그러더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005066.1.25",
          "form": "그래도 역시 콘서트는 직접 보는 맛인데..ㅜㅜ",
          "original_form": "그래도 역시 콘서트는 직접 보는 맛인데..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005066.1.26",
          "form": "그쵸그쵸~",
          "original_form": "그쵸그쵸~",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005066.1.27",
          "form": "콘서트는 가서 봐야죠ㅠㅠ",
          "original_form": "콘서트는 가서 봐야죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005066.1.28",
          "form": "가서 같이 노래부르고 막 ㅎㅎ",
          "original_form": "가서 같이 노래부르고 막 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005066.1.29",
          "form": "얼른 가서 보고싳네요 ㅎㅎㅎ",
          "original_form": "얼른 가서 보고싳네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005066.1.30",
          "form": "가본 적 있으세요 ㅎ?",
          "original_form": "가본 적 있으세요 ㅎ?",
