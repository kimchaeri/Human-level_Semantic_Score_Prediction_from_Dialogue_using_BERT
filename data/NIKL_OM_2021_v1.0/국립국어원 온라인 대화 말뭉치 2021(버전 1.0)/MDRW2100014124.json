      "utterance": [
        {
          "id": "MDRW2100014124.1.1",
          "form": "싸이버거!!!불싸이버거!!!!",
          "original_form": "싸이버거!!!불싸이버거!!!!",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014124.1.2",
          "form": "불싸이버거는 안먹어봤어요! 맛있나요?",
          "original_form": "불싸이버거는 안먹어봤어요! 맛있나요?",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014124.1.3",
          "form": "매워요ㅋㅋㄱㅋ",
          "original_form": "매워요ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014124.1.4",
          "form": "많이 매운가요?",
          "original_form": "많이 매운가요?",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014124.1.5",
          "form": "저는 좀 매웠어요",
          "original_form": "저는 좀 매웠어요",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014124.1.6",
          "form": "매운거잘먹는사람은",
          "original_form": "매운거잘먹는사람은",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014124.1.7",
          "form": "안매울수있겠죠",
          "original_form": "안매울수있겠죠",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014124.1.8",
          "form": "하긴 매운맛은 주관적이라 좀 어렵네요 ㅎㅎ",
          "original_form": "하긴 매운맛은 주관적이라 좀 어렵네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014124.1.9",
          "form": "전 맵찔이라서 매울거같아요 ㅋㅋ",
          "original_form": "전 맵찔이라서 매울거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014124.1.10",
          "form": "저는 둘다 다좋아해요",
          "original_form": "저는 둘다 다좋아해요",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014124.1.11",
          "form": "싸이치킨도 나왔던데 싸이버거만큼 맛있을지 궁금하네요",
          "original_form": "싸이치킨도 나왔던데 싸이버거만큼 맛있을지 궁금하네요",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014124.1.12",
          "form": "싸이치킨도 왠지 맛있어 보이는것같아요",
          "original_form": "싸이치킨도 왠지 맛있어 보이는것같아요",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014124.1.13",
          "form": "닭다리살이면 뭐든 오케이입니다!",
          "original_form": "닭다리살이면 뭐든 오케이입니다!",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014124.1.14",
          "form": "당연하죠!!!닭다리살",
          "original_form": "당연하죠!!!닭다리살",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014124.1.15",
          "form": "얘기하다보니 저도 싸이버거 땡기네요 ㅋㅋㅋ",
          "original_form": "얘기하다보니 저도 싸이버거 땡기네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014124.1.16",
          "form": "싸이버거 먹고싶은데",
          "original_form": "싸이버거 먹고싶은데",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014124.1.17",
          "form": "냠남ㅋㅋㄱㄱ",
          "original_form": "냠남ㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014124.1.18",
          "form": "맘스터치는 싸이버거도 맛있지만 감자튀김도 맛있어요!",
          "original_form": "맘스터치는 싸이버거도 맛있지만 감자튀김도 맛있어요!",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014124.1.19",
          "form": "맞아요 감자도맛있어요!!!",
          "original_form": "맞아요 감자도맛있어요!!!",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014124.1.20",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210825 17:31"
        }
      ]
