      "utterance": [
        {
          "id": "MDRW2100026802.1.1",
          "form": "아 그리고 설산은",
          "original_form": "아 그리고 설산은",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026802.1.2",
          "form": "좀 나중에 가",
          "original_form": "좀 나중에 가",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026802.1.3",
          "form": "나도 거긴 애들 렙 60찍고 갔어",
          "original_form": "나도 거긴 애들 렙 60찍고 갔어",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026802.1.4",
          "form": "설산 근처도 아직 안가봤지만 기억해두겠슴다",
          "original_form": "설산 근처도 아직 안가봤지만 기억해두겠슴다",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.5",
          "form": "설산은 다음 장으로 넘어가야 있지 않을까",
          "original_form": "설산은 다음 장으로 넘어가야 있지 않을까",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.6",
          "form": "아 그거 말인데",
          "original_form": "아 그거 말인데",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.7",
          "form": "내 애들은 아직도 렙 10인데()",
          "original_form": "내 애들은 아직도 렙 10인데()",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.8",
          "form": "지도에 어두운 부분에",
          "original_form": "지도에 어두운 부분에",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.9",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.10",
          "form": "동그란거 쪽으러 가서",
          "original_form": "동그란거 쪽으러 가서",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.11",
          "form": "신상 찍으면",
          "original_form": "신상 찍으면",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.12",
          "form": "지도 열려",
          "original_form": "지도 열려",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.13",
          "form": "점점이 찍힌 그런ㄱ 오",
          "original_form": "점점이 찍힌 그런ㄱ 오",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026802.1.14",
          "form": "사실 지금도 리월 가려면 갈수잇어",
          "original_form": "사실 지금도 리월 가려면 갈수잇어",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026802.1.15",
          "form": "아냐 무서워서 안갈래()",
          "original_form": "아냐 무서워서 안갈래()",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026802.1.16",
          "form": "천천히... 갈게......",
          "original_form": "천천히... 갈게......",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026802.1.17",
          "form": "ㅋㅋㅋㅋㅋ리월정도는 괜찮아",
          "original_form": "ㅋㅋㅋㅋㅋ리월정도는 괜찮아",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026802.1.18",
          "form": "얘네가 쪼렙이한테는",
          "original_form": "얘네가 쪼렙이한테는",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026802.1.19",
          "form": "ㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026802.1.20",
          "form": "쪼랩애들 일단 좀 키우고..",
          "original_form": "쪼랩애들 일단 좀 키우고..",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026802.1.21",
          "form": "월드 전체 레벨이 있거든?",
          "original_form": "월드 전체 레벨이 있거든?",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026802.1.22",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.23",
          "form": "나는 5렙이라",
          "original_form": "나는 5렙이라",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.24",
          "form": "몬드 필드에 있는 잡몹 5-60렙이야",
          "original_form": "몬드 필드에 있는 잡몹 5-60렙이야",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.25",
          "form": "그니까 너는 리월가도",
          "original_form": "그니까 너는 리월가도",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.26",
          "form": "난 체감상 0.1랩이겠는걸...?",
          "original_form": "난 체감상 0.1랩이겠는걸...?",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.27",
          "form": "ㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.28",
          "form": "글케 엄청ㄴㅏㄴ 적은",
          "original_form": "글케 엄청ㄴㅏㄴ 적은",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.29",
          "form": "안나온단 뜻이야",
          "original_form": "안나온단 뜻이야",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.30",
          "form": "큰 애들은 안나온다은",
          "original_form": "큰 애들은 안나온다은",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.31",
          "form": "는",
          "original_form": "는",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.32",
          "form": "머 힘들면 걍",
          "original_form": "머 힘들면 걍",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.33",
          "form": "그런 말이구나",
          "original_form": "그런 말이구나",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026802.1.34",
          "form": "빠르게 듀오 가능한 렙만 찍어",
          "original_form": "빠르게 듀오 가능한 렙만 찍어",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026802.1.35",
          "form": "내가 보스 뺑뺑이 돌아줄게",
          "original_form": "내가 보스 뺑뺑이 돌아줄게",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026802.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026802.1.37",
          "form": "이것이 버스기사다",
          "original_form": "이것이 버스기사다",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026802.1.38",
          "form": "ㅋㅋㅋ 버스기사님 제가 아직 돈이 없어서 버스에 못 타요()",
          "original_form": "ㅋㅋㅋ 버스기사님 제가 아직 돈이 없어서 버스에 못 타요()",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026802.1.39",
          "form": "일단 시간이..나야.. 같이 돌지..주말..힘내겠슴다..",
          "original_form": "일단 시간이..나야.. 같이 돌지..주말..힘내겠슴다..",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026802.1.40",
          "form": "ㅇㅇ파이팅",
          "original_form": "ㅇㅇ파이팅",
          "speaker_id": "2",
          "time": "20210902 00:23"
        }
      ]
    }
