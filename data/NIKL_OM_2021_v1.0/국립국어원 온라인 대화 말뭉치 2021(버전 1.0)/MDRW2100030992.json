      "utterance": [
        {
          "id": "MDRW2100030992.1.1",
          "form": "밥솥에 10분에서 15분 정도만 찌면 되거든요",
          "original_form": "밥솥에 10분에서 15분 정도만 찌면 되거든요",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030992.1.2",
          "form": "꼭 드셔보세요 진짜 최근 최애 만두에요",
          "original_form": "꼭 드셔보세요 진짜 최근 최애 만두에요",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030992.1.3",
          "form": "오 조아요 새우 만두 조아요",
          "original_form": "오 조아요 새우 만두 조아요",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030992.1.4",
          "form": "사실 맛있는거 되게 좋아하는데 맛집을 잘 몰라서 못 찾아 가는 경향도 있어요 ㅠ",
          "original_form": "사실 맛있는거 되게 좋아하는데 맛집을 잘 몰라서 못 찾아 가는 경향도 있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030992.1.5",
          "form": "새우만두 좋아하시면 강추에요 ㅋㅋ",
          "original_form": "새우만두 좋아하시면 강추에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030992.1.6",
          "form": "우왕 조타",
          "original_form": "우왕 조타",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030992.1.7",
          "form": "어케아셧어요",
          "original_form": "어케아셧어요",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030992.1.8",
          "form": "카페도 자주 가시나요?",
          "original_form": "카페도 자주 가시나요?",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030992.1.9",
          "form": "넹넹 지금도 카페",
          "original_form": "넹넹 지금도 카페",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100030992.1.10",
          "form": "라떼마시는중",
          "original_form": "라떼마시는중",
          "speaker_id": "1",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100030992.1.11",
          "form": "우왕 저도 아인슈페너 마시는 중이에요",
          "original_form": "우왕 저도 아인슈페너 마시는 중이에요",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100030992.1.12",
          "form": "커피 좋아하세요? 아니면 비 커피류를 좋아하시나요?",
          "original_form": "커피 좋아하세요? 아니면 비 커피류를 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100030992.1.13",
          "form": "커피요 근데 카페인 마니못받아요ㅠ",
          "original_form": "커피요 근데 카페인 마니못받아요ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100030992.1.14",
          "form": "아쉽겠어요 ㅠ",
          "original_form": "아쉽겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030992.1.15",
          "form": "좋아하는데 많이 못 먹는 것 만큼 ㅠㅠ",
          "original_form": "좋아하는데 많이 못 먹는 것 만큼 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030992.1.16",
          "form": "넹 마싯는데 ㅠ",
          "original_form": "넹 마싯는데 ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100030992.1.17",
          "form": "어떤 커피 좋아하세요?",
          "original_form": "어떤 커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030992.1.18",
          "form": "라떼용..",
          "original_form": "라떼용..",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030992.1.19",
          "form": "라떼 좋아용",
          "original_form": "라떼 좋아용",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100030992.1.20",
          "form": "우유 들어가서 부드럽게 먹기 좋더라구요",
          "original_form": "우유 들어가서 부드럽게 먹기 좋더라구요",
