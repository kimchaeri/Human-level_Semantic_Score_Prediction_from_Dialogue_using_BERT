        },
        {
          "id": "MDRW2100040478.1.2",
          "form": "먹었어용 ㅋㅋ",
          "original_form": "먹었어용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100040478.1.3",
          "form": "내 것도 좀 가져와라",
          "original_form": "내 것도 좀 가져와라",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100040478.1.4",
          "form": "알았다~",
          "original_form": "알았다~",
          "speaker_id": "1",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100040478.1.5",
          "form": "물을 많이 마셨나",
          "original_form": "물을 많이 마셨나",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100040478.1.6",
          "form": "웅웅 물 조금으론 못 삼키겠다 ㅋ",
          "original_form": "웅웅 물 조금으론 못 삼키겠다 ㅋ",
          "speaker_id": "1",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100040478.1.7",
          "form": "약 먹을 때 물 많이 마셔야 한다",
          "original_form": "약 먹을 때 물 많이 마셔야 한다",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100040478.1.8",
          "form": "웅웅 목에 걸리니까 아프더라",
          "original_form": "웅웅 목에 걸리니까 아프더라",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100040478.1.9",
          "form": "그래 잘 먹어야 된다",
          "original_form": "그래 잘 먹어야 된다",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100040478.1.10",
          "form": "웅 비타민 꼬박꼬박 챙겨먹자",
          "original_form": "웅 비타민 꼬박꼬박 챙겨먹자",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100040478.1.11",
          "form": "그래 집에서 빨리 먹어라",
