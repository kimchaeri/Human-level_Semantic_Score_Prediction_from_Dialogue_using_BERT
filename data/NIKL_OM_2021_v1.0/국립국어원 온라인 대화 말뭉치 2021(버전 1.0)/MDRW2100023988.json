      "utterance": [
        {
          "id": "MDRW2100023988.1.1",
          "form": "여긴 간만에 해가 떴어요",
          "original_form": "여긴 간만에 해가 떴어요",
          "speaker_id": "1",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100023988.1.2",
          "form": "이게 얼마만인지",
          "original_form": "이게 얼마만인지",
          "speaker_id": "1",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100023988.1.3",
          "form": "좋아하는 계절 있나요?^^",
          "original_form": "좋아하는 계절 있나요?^^",
          "speaker_id": "1",
          "time": "20210906 08:41"
        },
        {
          "id": "MDRW2100023988.1.4",
          "form": "전 봄이랑 가을 좋아해요~",
          "original_form": "전 봄이랑 가을 좋아해요~",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100023988.1.5",
          "form": "정말 좋은 계절이죠",
          "original_form": "정말 좋은 계절이죠",
          "speaker_id": "1",
          "time": "20210907 08:33"
        },
        {
          "id": "MDRW2100023988.1.6",
          "form": "전 가을이 좋은데 단풍이 좋기 떄문이에요",
          "original_form": "전 가을이 좋은데 단풍이 좋기 떄문이에요",
          "speaker_id": "1",
          "time": "20210907 08:33"
        },
        {
          "id": "MDRW2100023988.1.7",
          "form": "단풍 이쁘죠~ 알록달록하니~",
          "original_form": "단풍 이쁘죠~ 알록달록하니~",
          "speaker_id": "2",
          "time": "20210907 08:44"
        },
        {
          "id": "MDRW2100023988.1.8",
          "form": "네 그래서 해마다 단풍구경 갔었는데요",
          "original_form": "네 그래서 해마다 단풍구경 갔었는데요",
          "speaker_id": "1",
          "time": "20210907 08:45"
        },
        {
          "id": "MDRW2100023988.1.9",
          "form": "작년부터 못가서 너무 아쉬워요",
          "original_form": "작년부터 못가서 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210907 08:45"
        },
        {
          "id": "MDRW2100023988.1.10",
          "form": "학창시절에 예쁜 단풍잎 코팅해 두고 했었는데~\r\n갑자기 추억돋네요~ㅎ",
          "original_form": "학창시절에 예쁜 단풍잎 코팅해 두고 했었는데~\r\n갑자기 추억돋네요~ㅎ",
          "speaker_id": "2",
          "time": "20210907 08:46"
        },
        {
          "id": "MDRW2100023988.1.11",
          "form": "그러게요.. 그럴 떄가 있었네요",
          "original_form": "그러게요.. 그럴 떄가 있었네요",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100023988.1.12",
          "form": "조금 있으면 단풍 이쁘게 물들겠네요~",
          "original_form": "조금 있으면 단풍 이쁘게 물들겠네요~",
          "speaker_id": "2",
          "time": "20210907 10:43"
        },
        {
          "id": "MDRW2100023988.1.13",
          "form": "네 정말 기대돼요",
          "original_form": "네 정말 기대돼요",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100023988.1.14",
          "form": "그 전이 이 비도 좀 그쳤으면 좋겠구요",
          "original_form": "그 전이 이 비도 좀 그쳤으면 좋겠구요",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100023988.1.15",
          "form": "이번 초가을은 비가 정말 많이 오네요",
          "original_form": "이번 초가을은 비가 정말 많이 오네요",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100023988.1.16",
          "form": "제가 있는 곳은 비 예보는 있는데 아직 비는 안오네요~",
          "original_form": "제가 있는 곳은 비 예보는 있는데 아직 비는 안오네요~",
          "speaker_id": "2",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100023988.1.17",
          "form": "일기예보가 안맞는가?ㅎ",
          "original_form": "일기예보가 안맞는가?ㅎ",
          "speaker_id": "2",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100023988.1.18",
          "form": "그래요? 요새는 예보도 그때 돼봐야 아는 거라서요",
          "original_form": "그래요? 요새는 예보도 그때 돼봐야 아는 거라서요",
          "speaker_id": "1",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100023988.1.19",
          "form": "비가 그치고 제대로 된 가을을 맛보고 싶네요",
          "original_form": "비가 그치고 제대로 된 가을을 맛보고 싶네요",
          "speaker_id": "1",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100023988.1.20",
          "form": "오늘이 절기상 맑은 이슬이 내린다는 백로라네요~",
          "original_form": "오늘이 절기상 맑은 이슬이 내린다는 백로라네요~",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100023988.1.21",
          "form": "오늘 내리는 비는 맑은 이슬인걸로~ㅎ",
          "original_form": "오늘 내리는 비는 맑은 이슬인걸로~ㅎ",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100023988.1.22",
          "form": "오늘이 백로였군요",
          "original_form": "오늘이 백로였군요",
          "speaker_id": "1",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100023988.1.23",
          "form": "방금 일기예보 다시 본다고 보니 백로라네요~ㅎ",
          "original_form": "방금 일기예보 다시 본다고 보니 백로라네요~ㅎ",
          "speaker_id": "2",
          "time": "20210907 14:36"
        },
        {
          "id": "MDRW2100023988.1.24",
          "form": "절기상으로는 가을이 성큼 다가왔네요",
          "original_form": "절기상으로는 가을이 성큼 다가왔네요",
          "speaker_id": "2",
          "time": "20210907 14:36"
        }
      ]
    }
