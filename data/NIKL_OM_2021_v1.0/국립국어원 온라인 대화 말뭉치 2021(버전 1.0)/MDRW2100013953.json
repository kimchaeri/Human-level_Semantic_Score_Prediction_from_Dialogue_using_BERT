      "utterance": [
        {
          "id": "MDRW2100013953.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013953.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013953.1.3",
          "form": "혹시 직장인이세요?",
          "original_form": "혹시 직장인이세요?",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013953.1.4",
          "form": "아니요 취준생이에요ㅠ",
          "original_form": "아니요 취준생이에요ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013953.1.5",
          "form": "아하 ㅠ 저도 아직 대학생이에요",
          "original_form": "아하 ㅠ 저도 아직 대학생이에요",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013953.1.6",
          "form": "앗 그럼 아르바이트 해본 적 있으세요?",
          "original_form": "앗 그럼 아르바이트 해본 적 있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013953.1.7",
          "form": "네네ㅎㅎ 제가 미술 쪽 배우는중이라",
          "original_form": "네네ㅎㅎ 제가 미술 쪽 배우는중이라",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013953.1.8",
          "form": "미술학원 한번 해봤어요!",
          "original_form": "미술학원 한번 해봤어요!",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013953.1.9",
          "form": "오 그럼 애들 알려주는거에요?",
          "original_form": "오 그럼 애들 알려주는거에요?",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013953.1.10",
          "form": "ㅎㅎ 그렇긴 한데 몇달 안 하고 그만두게 돼서 ㅠ",
          "original_form": "ㅎㅎ 그렇긴 한데 몇달 안 하고 그만두게 돼서 ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013953.1.11",
          "form": "일심향님도 취준생이면 이전에 알바 해보셨나요?",
          "original_form": "일심향님도 취준생이면 이전에 알바 해보셨나요?",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013953.1.12",
          "form": "네ㅋㅋㅋ 단기알바도 몇 개하고 길게도 해봤어요",
          "original_form": "네ㅋㅋㅋ 단기알바도 몇 개하고 길게도 해봤어요",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013953.1.13",
          "form": "아하 저도 알바를... 평범한 걸 좀 해 봐야 하는데 ㅠㅋㅋ",
          "original_form": "아하 저도 알바를... 평범한 걸 좀 해 봐야 하는데 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013953.1.14",
          "form": "그런 경험이 없어서 엄마한테 맨날 잔소리 들어요ㅋㅋ",
          "original_form": "그런 경험이 없어서 엄마한테 맨날 잔소리 들어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013953.1.15",
          "form": "진로랑 관련된 알바가 더 좋죠ㅋㅋㄱㅋ 전 그냥 어중히 떠중히라",
          "original_form": "진로랑 관련된 알바가 더 좋죠ㅋㅋㄱㅋ 전 그냥 어중히 떠중히라",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013953.1.16",
          "form": "ㅋㅋㅋ ㅠㅠ 그래두 그런 평범한 경험이 중요하더라구요",
          "original_form": "ㅋㅋㅋ ㅠㅠ 그래두 그런 평범한 경험이 중요하더라구요",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013953.1.17",
          "form": "그럼 하고싶은 알바는 있으세요?!",
          "original_form": "그럼 하고싶은 알바는 있으세요?!",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013953.1.18",
          "form": "저는 카페 한번 해보고싶어요....ㅎㅎ",
          "original_form": "저는 카페 한번 해보고싶어요....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013953.1.19",
          "form": "오 까페 좋죠ㅋㅋㅋㅋ 근데 까페 일 하다보면 뭔가 아메리카노는 사먹기 아깝게 느껴질때가 있는 것 같아요ㅋㅋㅋㅋ",
          "original_form": "오 까페 좋죠ㅋㅋㅋㅋ 근데 까페 일 하다보면 뭔가 아메리카노는 사먹기 아깝게 느껴질때가 있는 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013953.1.20",
          "form": "앜ㅋㅋㅋ 뭔지 알거같아요 집에 커피머신 하나 두니까",
          "original_form": "앜ㅋㅋㅋ 뭔지 알거같아요 집에 커피머신 하나 두니까",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013953.1.21",
          "form": "하나에 삼사천원 하는 아메리카노 너무...... ㅋㅋㅋ",
          "original_form": "하나에 삼사천원 하는 아메리카노 너무...... ㅋㅋㅋ",
          "speaker_id": "1",
