      "utterance": [
        {
          "id": "MDRW2100019004.1.1",
          "form": "오늘 점심메뉴는 뭐였나요",
          "original_form": "오늘 점심메뉴는 뭐였나요",
          "speaker_id": "1",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100019004.1.2",
          "form": "샐러드에 크로와상  먹었어요",
          "original_form": "샐러드에 크로와상  먹었어요",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019004.1.3",
          "form": "맛있었겠어요",
          "original_form": "맛있었겠어요",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019004.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019004.1.5",
          "form": "전 김치찌개에 불고기먹었어요",
          "original_form": "전 김치찌개에 불고기먹었어요",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019004.1.6",
          "form": "맛있으셨겠어요ㅠㅠㅜ",
          "original_form": "맛있으셨겠어요ㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100019004.1.7",
          "form": "좋아하는 간식이 뭐예용?",
          "original_form": "좋아하는 간식이 뭐예용?",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100019004.1.8",
          "form": "저는 달달구리 좋아해요",
          "original_form": "저는 달달구리 좋아해요",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019004.1.9",
          "form": "갑자기 뚱카롱 먹고싶네요ㅜㅜ",
          "original_form": "갑자기 뚱카롱 먹고싶네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019004.1.10",
          "form": "뚱카롱 좋죠ㅠㅠ 몇 구 세트로 많이 사서 먹고싶어요",
          "original_form": "뚱카롱 좋죠ㅠㅠ 몇 구 세트로 많이 사서 먹고싶어요",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019004.1.11",
          "form": "저두요ㅜㅜ 와구와구 먹고싶네요",
          "original_form": "저두요ㅜㅜ 와구와구 먹고싶네요",
          "speaker_id": "1",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019004.1.12",
          "form": "주변에 맛있는 곳이 있으신가요?",
          "original_form": "주변에 맛있는 곳이 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019004.1.13",
          "form": "잘모르겠어요",
          "original_form": "잘모르겠어요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019004.1.14",
          "form": "배고프네용",
          "original_form": "배고프네용",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019004.1.15",
          "form": "먹는 얘기만한게 없죵!",
          "original_form": "먹는 얘기만한게 없죵!",
          "speaker_id": "1",
