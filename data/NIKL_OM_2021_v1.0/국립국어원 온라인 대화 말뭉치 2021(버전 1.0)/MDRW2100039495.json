      "utterance": [
        {
          "id": "MDRW2100039495.1.1",
          "form": "어떤과목 좋아하셨나요",
          "original_form": "어떤과목 좋아하셨나요",
          "speaker_id": "1",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039495.1.2",
          "form": "역사랑 사회과목 좋아했어요",
          "original_form": "역사랑 사회과목 좋아했어요",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039495.1.3",
          "form": "어떤 과목을 좋아하셨나요?",
          "original_form": "어떤 과목을 좋아하셨나요?",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039495.1.4",
          "form": "역사 너무 어려워요ㅋ 전 한문이요",
          "original_form": "역사 너무 어려워요ㅋ 전 한문이요",
          "speaker_id": "1",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100039495.1.5",
          "form": "앗 혹시 한문 안배우셨다거나ㅋㅋㅋ",
          "original_form": "앗 혹시 한문 안배우셨다거나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039495.1.6",
          "form": "아니요 한문배운세대입니당",
          "original_form": "아니요 한문배운세대입니당",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039495.1.7",
          "form": "한시해석도 배웠었는데 ㅋㅋ",
          "original_form": "한시해석도 배웠었는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039495.1.8",
          "form": "와 기억난다 맞아요ㄲㅋ",
          "original_form": "와 기억난다 맞아요ㄲㅋ",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039495.1.9",
          "form": "역사가 어렵긴한데 흐름을 알면 재밌었요 ㅋㅋㅋ",
          "original_form": "역사가 어렵긴한데 흐름을 알면 재밌었요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039495.1.10",
          "form": "그건그런데 그게 외워지지는 안더라고요ㅋ 그게문제",
          "original_form": "그건그런데 그게 외워지지는 안더라고요ㅋ 그게문제",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039495.1.11",
          "form": "외워야 할게 많긴 하죠.. 그래서 세계사는 너무 힘들었어요ㅋㅋㄱ",
          "original_form": "외워야 할게 많긴 하죠.. 그래서 세계사는 너무 힘들었어요ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039495.1.12",
          "form": "아 세계사 진짜 힘들었죠",
          "original_form": "아 세계사 진짜 힘들었죠",
          "speaker_id": "1",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039495.1.13",
          "form": "쉬운과목이 없엇어요",
          "original_form": "쉬운과목이 없엇어요",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039495.1.14",
          "form": "맞아요.. 공부는 쉬운게 없죠 ㅠ",
          "original_form": "맞아요.. 공부는 쉬운게 없죠 ㅠ",
          "speaker_id": "2",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100039495.1.15",
          "form": "네 진짜 공부는 아무나 못해요",
          "original_form": "네 진짜 공부는 아무나 못해요",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100039495.1.16",
          "form": "지금도 자격증시험 준비중인데 너무 하기싫으네요",
          "original_form": "지금도 자격증시험 준비중인데 너무 하기싫으네요",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039495.1.17",
          "form": "공부하려면 갑자기 딴게 막보이죠",
          "original_form": "공부하려면 갑자기 딴게 막보이죠",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100039495.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ \r\n네네 그렇더라구요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ \r\n네네 그렇더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039495.1.19",
          "form": "하고잇는 공부가 있나요?",
          "original_form": "하고잇는 공부가 있나요?",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039495.1.20",
          "form": "전 지금은 하는거 없어요\r\n나이드니까 하기싫어서ㅋㅋ",
          "original_form": "전 지금은 하는거 없어요\r\n나이드니까 하기싫어서ㅋㅋ",
