      "utterance": [
        {
          "id": "MDRW2100030508.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 02:27"
        },
        {
          "id": "MDRW2100030508.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030508.1.3",
          "form": "부모님 두분 다 모더나??",
          "original_form": "부모님 두분 다 모더나??",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100030508.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 02:35"
        },
        {
          "id": "MDRW2100030508.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210904 02:35"
        },
        {
          "id": "MDRW2100030508.1.6",
          "form": "좋은거 맞으셧네요",
          "original_form": "좋은거 맞으셧네요",
          "speaker_id": "1",
          "time": "20210904 02:35"
        },
        {
          "id": "MDRW2100030508.1.7",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "2",
          "time": "20210904 02:38"
        },
        {
          "id": "MDRW2100030508.1.8",
          "form": "그렇죠.",
          "original_form": "그렇죠.",
          "speaker_id": "1",
          "time": "20210904 02:39"
        },
        {
          "id": "MDRW2100030508.1.9",
          "form": "열은 안나나요?",
          "original_form": "열은 안나나요?",
          "speaker_id": "1",
          "time": "20210904 02:39"
        },
        {
          "id": "MDRW2100030508.1.10",
          "form": "접종받은 부위 근처로 열이 나고 있어요",
          "original_form": "접종받은 부위 근처로 열이 나고 있어요",
          "speaker_id": "2",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100030508.1.11",
          "form": "열심히 반응하고잇나봐요..",
          "original_form": "열심히 반응하고잇나봐요..",
          "speaker_id": "1",
          "time": "20210904 02:40"
        },
        {
          "id": "MDRW2100030508.1.12",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "2",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030508.1.13",
          "form": "그동안 맞앗돈 백신중에 비교해보면 어때요",
          "original_form": "그동안 맞앗돈 백신중에 비교해보면 어때요",
          "speaker_id": "1",
          "time": "20210904 02:48"
        },
        {
          "id": "MDRW2100030508.1.14",
          "form": "작년에 독감을 맞았었는데,\r\n확실히 그 때보다 통증이 훨씬 강해요!",
          "original_form": "작년에 독감을 맞았었는데,\r\n확실히 그 때보다 통증이 훨씬 강해요!",
          "speaker_id": "2",
          "time": "20210904 02:49"
        },
        {
          "id": "MDRW2100030508.1.15",
          "form": "그렇죠 ㅎㅎㅎㅎ",
          "original_form": "그렇죠 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100030508.1.16",
          "form": "역대급?",
          "original_form": "역대급?",
          "speaker_id": "1",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100030508.1.17",
          "form": "네 근데 다른 점은",
          "original_form": "네 근데 다른 점은",
          "speaker_id": "2",
          "time": "20210904 02:50"
        },
        {
          "id": "MDRW2100030508.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100030508.1.19",
          "form": "독감때는 팔 통증은 덜했고, 감기몸살 기운이 오래 지속되었어요",
          "original_form": "독감때는 팔 통증은 덜했고, 감기몸살 기운이 오래 지속되었어요",
          "speaker_id": "2",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100030508.1.20",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210904 02:51"
        },
        {
          "id": "MDRW2100030508.1.21",
          "form": "지금은 (아직까지 상황으로)",
          "original_form": "지금은 (아직까지 상황으로)",
          "speaker_id": "2",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100030508.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100030508.1.23",
          "form": "몸살기운은 없으나 팔의 통증이나 무게감이 생각했던것보다 훨씬 센편이에요",
          "original_form": "몸살기운은 없으나 팔의 통증이나 무게감이 생각했던것보다 훨씬 센편이에요",
          "speaker_id": "2",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100030508.1.24",
          "form": "그쳐..",
          "original_form": "그쳐..",
          "speaker_id": "1",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100030508.1.25",
          "form": "비슷하네요",
          "original_form": "비슷하네요",
          "speaker_id": "1",
          "time": "20210904 02:53"
        }
      ]
    }
