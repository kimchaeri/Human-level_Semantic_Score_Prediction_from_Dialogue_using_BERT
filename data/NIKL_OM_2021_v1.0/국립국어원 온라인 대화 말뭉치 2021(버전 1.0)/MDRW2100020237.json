      "utterance": [
        {
          "id": "MDRW2100020237.1.1",
          "form": "뭐 몇점이상은",
          "original_form": "뭐 몇점이상은",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.2",
          "form": "동상 은상",
          "original_form": "동상 은상",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.3",
          "form": "일케줌",
          "original_form": "일케줌",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.4",
          "form": "와 님은",
          "original_form": "와 님은",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.5",
          "form": "점수로 나눠준거같아",
          "original_form": "점수로 나눠준거같아",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.6",
          "form": "금상 받았갰내요",
          "original_form": "금상 받았갰내요",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.7",
          "form": "저는 동상인가 은상받앗어요",
          "original_form": "저는 동상인가 은상받앗어요",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.8",
          "form": "일등했으니까",
          "original_form": "일등했으니까",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.9",
          "form": "우리반에 금상이 없엇음",
          "original_form": "우리반에 금상이 없엇음",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.10",
          "form": "헐..글쿠나대박이네",
          "original_form": "헐..글쿠나대박이네",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020237.1.11",
          "form": "나랑 똑같은 점수가 한명 더 잇엇나??",
          "original_form": "나랑 똑같은 점수가 한명 더 잇엇나??",
          "speaker_id": "2",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100020237.1.12",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100020237.1.13",
          "form": "한개틀이면 은상이고",
          "original_form": "한개틀이면 은상이고",
          "speaker_id": "2",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100020237.1.14",
          "form": "두개틀리면 ㄱ동상이고 그랬나",
          "original_form": "두개틀리면 ㄱ동상이고 그랬나",
          "speaker_id": "2",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100020237.1.15",
          "form": "대단하네..",
          "original_form": "대단하네..",
          "speaker_id": "1",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100020237.1.16",
          "form": "다 어릴때의 일이다..",
          "original_form": "다 어릴때의 일이다..",
          "speaker_id": "2",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100020237.1.17",
          "form": "지금 그래도",
          "original_form": "지금 그래도",
          "speaker_id": "1",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100020237.1.18",
          "form": "계산 잘하잖아요",
          "original_form": "계산 잘하잖아요",
          "speaker_id": "1",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100020237.1.19",
          "form": "사는데 지장업ㅎ는만큼은 하는거같아요",
          "original_form": "사는데 지장업ㅎ는만큼은 하는거같아요",
          "speaker_id": "2",
          "time": "20210828 00:37"
        }
      ]
