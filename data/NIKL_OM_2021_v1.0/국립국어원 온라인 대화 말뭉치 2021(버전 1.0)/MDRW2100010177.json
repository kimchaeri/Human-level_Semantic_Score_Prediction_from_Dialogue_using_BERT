      "utterance": [
        {
          "id": "MDRW2100010177.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 17:54"
        },
        {
          "id": "MDRW2100010177.1.2",
          "form": "콘서트 가고싶은데 못가고있어요",
          "original_form": "콘서트 가고싶은데 못가고있어요",
          "speaker_id": "2",
          "time": "20210823 17:54"
        },
        {
          "id": "MDRW2100010177.1.3",
          "form": "다녀오신 콘서트 있으세요?",
          "original_form": "다녀오신 콘서트 있으세요?",
          "speaker_id": "1",
          "time": "20210823 17:54"
        },
        {
          "id": "MDRW2100010177.1.4",
          "form": "아직요ㅋㅋ 아이유  콘서트 가고싶어요ㅜㅜ",
          "original_form": "아직요ㅋㅋ 아이유  콘서트 가고싶어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 17:55"
        },
        {
          "id": "MDRW2100010177.1.5",
          "form": "아 ㅠㅠ저도 아이유 콘서트는 꼭 가보고싶어요",
          "original_form": "아 ㅠㅠ저도 아이유 콘서트는 꼭 가보고싶어요",
          "speaker_id": "1",
          "time": "20210823 17:55"
        },
        {
          "id": "MDRW2100010177.1.6",
          "form": "싸이 흠뻑쇼도 에너지 장난아니래서 가보고싶어요",
          "original_form": "싸이 흠뻑쇼도 에너지 장난아니래서 가보고싶어요",
          "speaker_id": "2",
          "time": "20210823 17:55"
        },
        {
          "id": "MDRW2100010177.1.7",
          "form": "저 가봤는데 진짜 재밌고 진짜 지쳐요ㅋㅋ",
          "original_form": "저 가봤는데 진짜 재밌고 진짜 지쳐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:56"
        },
        {
          "id": "MDRW2100010177.1.8",
          "form": "싸이님 체력이 엄청나서 공연시간이 계속 늘어난다고 하더라구요ㅋㅋ",
          "original_form": "싸이님 체력이 엄청나서 공연시간이 계속 늘어난다고 하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:56"
        },
        {
          "id": "MDRW2100010177.1.9",
          "form": "진짜 가수분들 체력 엄청나요 존경심 들어요",
          "original_form": "진짜 가수분들 체력 엄청나요 존경심 들어요",
          "speaker_id": "1",
          "time": "20210823 17:56"
        },
        {
          "id": "MDRW2100010177.1.10",
          "form": "방탄소년단 콘서트도 가보고싶어요",
          "original_form": "방탄소년단 콘서트도 가보고싶어요",
          "speaker_id": "2",
          "time": "20210823 17:57"
        },
        {
          "id": "MDRW2100010177.1.11",
          "form": "앗 저랑 취향이 비슷하신가봐욥",
          "original_form": "앗 저랑 취향이 비슷하신가봐욥",
          "speaker_id": "1",
          "time": "20210823 17:57"
        },
        {
          "id": "MDRW2100010177.1.12",
          "form": "방탄콘서트는 진짜 수강신청 보다 어렵다던데..",
          "original_form": "방탄콘서트는 진짜 수강신청 보다 어렵다던데..",
          "speaker_id": "1",
          "time": "20210823 17:57"
        },
        {
          "id": "MDRW2100010177.1.13",
          "form": "제 지인들은 방탄 콘서트 디게 자주 가시더라구요 엄청 재밌대요",
          "original_form": "제 지인들은 방탄 콘서트 디게 자주 가시더라구요 엄청 재밌대요",
          "speaker_id": "2",
          "time": "20210823 17:58"
        },
        {
          "id": "MDRW2100010177.1.14",
          "form": "콘서트 넘 가고싶어요ㅜㅜ",
          "original_form": "콘서트 넘 가고싶어요ㅜㅜ",
