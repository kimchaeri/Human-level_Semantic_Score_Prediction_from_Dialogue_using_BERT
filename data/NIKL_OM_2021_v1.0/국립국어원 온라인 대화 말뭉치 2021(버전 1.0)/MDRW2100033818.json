      "utterance": [
        {
          "id": "MDRW2100033818.1.1",
          "form": "재밌게 본 드라마가 있나요?",
          "original_form": "재밌게 본 드라마가 있나요?",
          "speaker_id": "1",
          "time": "20210825 08:42"
        },
        {
          "id": "MDRW2100033818.1.2",
          "form": "요즘에는 슬기로운 의사생활,펜트하우스요",
          "original_form": "요즘에는 슬기로운 의사생활,펜트하우스요",
          "speaker_id": "2",
          "time": "20210825 08:43"
        },
        {
          "id": "MDRW2100033818.1.3",
          "form": "저번 주 팬트하우스 보셨나요?",
          "original_form": "저번 주 팬트하우스 보셨나요?",
          "speaker_id": "1",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100033818.1.4",
          "form": "네 봤어요, ㅜㅜ",
          "original_form": "네 봤어요, ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100033818.1.5",
          "form": "펜트하우스 잘보세요?",
          "original_form": "펜트하우스 잘보세요?",
          "speaker_id": "2",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100033818.1.6",
          "form": "저 지금 다 봤어요 저번 주 거 너무 충격이라 재방송을 못 보고 있어요",
          "original_form": "저 지금 다 봤어요 저번 주 거 너무 충격이라 재방송을 못 보고 있어요",
          "speaker_id": "1",
          "time": "20210825 08:44"
        },
        {
          "id": "MDRW2100033818.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.8",
          "form": "지난주꺼는",
          "original_form": "지난주꺼는",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.9",
          "form": "마지막장면이",
          "original_form": "마지막장면이",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.10",
          "form": "진짜 역대급",
          "original_form": "진짜 역대급",
          "speaker_id": "1",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.11",
          "form": "너무 충격이더라구요",
          "original_form": "너무 충격이더라구요",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.12",
          "form": "무서웠어요. ㅜㅋㅋ",
          "original_form": "무서웠어요. ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.13",
          "form": "너무 잔인하고 너무 자극적이고",
          "original_form": "너무 잔인하고 너무 자극적이고",
          "speaker_id": "1",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.14",
          "form": "그러니깐요",
          "original_form": "그러니깐요",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.15",
          "form": "캐릭터들이 벌 받기는 바랬는데 저런 식으로 벌 받기를 원하지는 않았어요",
          "original_form": "캐릭터들이 벌 받기는 바랬는데 저런 식으로 벌 받기를 원하지는 않았어요",
          "speaker_id": "1",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.16",
          "form": "말많더라구요",
          "original_form": "말많더라구요",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.17",
          "form": "맞아요.ㅜㅜ",
          "original_form": "맞아요.ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100033818.1.18",
          "form": "죽을줄이야ㅜ",
          "original_form": "죽을줄이야ㅜ",
          "speaker_id": "2",
          "time": "20210825 08:46"
        },
        {
          "id": "MDRW2100033818.1.19",
          "form": "근데 좀 진짜 샹들리에가 떨어진다는 게 너무 충격적이고 그리고 그걸 맞고 죽는다는 것도 너무 충격적이고",
          "original_form": "근데 좀 진짜 샹들리에가 떨어진다는 게 너무 충격적이고 그리고 그걸 맞고 죽는다는 것도 너무 충격적이고",
          "speaker_id": "1",
          "time": "20210825 08:46"
        },
        {
          "id": "MDRW2100033818.1.20",
          "form": "그러니깐요ㅜ",
          "original_form": "그러니깐요ㅜ",
          "speaker_id": "2",
          "time": "20210825 08:46"
        },
        {
          "id": "MDRW2100033818.1.21",
          "form": "저렇게까지 해야 하나 싶은..",
          "original_form": "저렇게까지 해야 하나 싶은..",
          "speaker_id": "1",
          "time": "20210825 08:46"
        }
      ]
