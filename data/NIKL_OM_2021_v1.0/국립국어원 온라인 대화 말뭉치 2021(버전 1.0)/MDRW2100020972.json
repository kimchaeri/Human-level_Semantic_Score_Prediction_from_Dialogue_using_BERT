      "utterance": [
        {
          "id": "MDRW2100020972.1.1",
          "form": "아 ㅋㅋㄱ 뭔가 귀여워요 ㅋㅋ",
          "original_form": "아 ㅋㅋㄱ 뭔가 귀여워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020972.1.2",
          "form": "우리나라에서 바나나라니 !",
          "original_form": "우리나라에서 바나나라니 !",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020972.1.3",
          "form": "진짜 지구온난화 계속 가속화 되면",
          "original_form": "진짜 지구온난화 계속 가속화 되면",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020972.1.4",
          "form": "바나나 재배하는 날 올거 같지 않아요?ㅠㅠ",
          "original_form": "바나나 재배하는 날 올거 같지 않아요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020972.1.5",
          "form": "맞아요 ㅠㅠ 갈수록 여름이 길어져서",
          "original_form": "맞아요 ㅠㅠ 갈수록 여름이 길어져서",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020972.1.6",
          "form": "4월부터 여름일듯한 느낌..이에요",
          "original_form": "4월부터 여름일듯한 느낌..이에요",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020972.1.7",
          "form": "이러다가 나중엔 보호복 같은 것도 생길듯요 ㅋㅋ",
          "original_form": "이러다가 나중엔 보호복 같은 것도 생길듯요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020972.1.8",
          "form": "에어컨 내장된 옷 그런거",
          "original_form": "에어컨 내장된 옷 그런거",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020972.1.9",
          "form": "와 ㅋㅋ 뭔가 상상되는걸요 ㅋㅋㅋ",
          "original_form": "와 ㅋㅋ 뭔가 상상되는걸요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020972.1.10",
          "form": "그 약간 우주복처럼 ..",
          "original_form": "그 약간 우주복처럼 ..",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020972.1.11",
          "form": "그게 지구온난화 끝판왕 같아요 ㅋㅋㅋㅋ",
          "original_form": "그게 지구온난화 끝판왕 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020972.1.12",
          "form": "에어컨 내장된 옷이라니 ㅋㅋ",
          "original_form": "에어컨 내장된 옷이라니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020972.1.13",
          "form": "ㅋㅋㅋㅋㅋ그쵸그쵸",
          "original_form": "ㅋㅋㅋㅋㅋ그쵸그쵸",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020972.1.14",
          "form": "지금 사람들 마스크 끼고 다니는 것도 대기오염 심하면 그렇게 된다고 그랬는데",
          "original_form": "지금 사람들 마스크 끼고 다니는 것도 대기오염 심하면 그렇게 된다고 그랬는데",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020972.1.15",
          "form": "충분히 일어날 수 있을 것 같아요",
          "original_form": "충분히 일어날 수 있을 것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020972.1.16",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020972.1.17",
          "form": "원래 마스크 미세먼지때문에 끼고 다니랬는데 ㅠㅜ",
          "original_form": "원래 마스크 미세먼지때문에 끼고 다니랬는데 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020972.1.18",
          "form": "근데 그때도 전 안끼고 다녔거든요",
          "original_form": "근데 그때도 전 안끼고 다녔거든요",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100020972.1.19",
          "form": "마스크 비싼 것 같아서... 그리고 예민한 분 아니면 잘 끼지도 않았었잖아요",
          "original_form": "마스크 비싼 것 같아서... 그리고 예민한 분 아니면 잘 끼지도 않았었잖아요",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100020972.1.20",
          "form": "맞아요 ! 저도 그냥 쌩얼 가릴때만 일회용 썼었는데 ..",
          "original_form": "맞아요 ! 저도 그냥 쌩얼 가릴때만 일회용 썼었는데 ..",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100020972.1.21",
          "form": "헐 그런 용도로 쓸 수도 있네요 ㅋㅋㅋ대박",
          "original_form": "헐 그런 용도로 쓸 수도 있네요 ㅋㅋㅋ대박",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100020972.1.22",
          "form": "턱에 걸치는 친구들도 많았는데.. 얼굴 작아보인다구 ㅎㅎ",
          "original_form": "턱에 걸치는 친구들도 많았는데.. 얼굴 작아보인다구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100020972.1.23",
          "form": "아 맞아요 ㅋㅋ !",
          "original_form": "아 맞아요 ㅋㅋ !",
          "speaker_id": "1",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100020972.1.24",
          "form": "전 가끔 얼굴 시릴때도 썼어요 ㅋㅋㅋ",
          "original_form": "전 가끔 얼굴 시릴때도 썼어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100020972.1.25",
          "form": "ㅋㅋㅋㅋ아 되게 다양하게 잘 활용하시네요",
          "original_form": "ㅋㅋㅋㅋ아 되게 다양하게 잘 활용하시네요",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100020972.1.26",
          "form": "그래도 젤 좋은건 마스크 벗는 날이 오는 것이죠 ㅜㅜ",
          "original_form": "그래도 젤 좋은건 마스크 벗는 날이 오는 것이죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100020972.1.27",
          "form": "맞아요 ㅠㅠㅠ 내년은 가능할까요?",
          "original_form": "맞아요 ㅠㅠㅠ 내년은 가능할까요?",
          "speaker_id": "1",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100020972.1.28",
          "form": "여름에 쓰는게 젤 싫어요 ㅜㅜ",
          "original_form": "여름에 쓰는게 젤 싫어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100020972.1.29",
          "form": "ㅋㅋ 올해 여름 진짜 쪄죽는 줄 알았어요",
          "original_form": "ㅋㅋ 올해 여름 진짜 쪄죽는 줄 알았어요",
