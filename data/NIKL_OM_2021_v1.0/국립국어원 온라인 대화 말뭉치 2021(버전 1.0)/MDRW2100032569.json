      "utterance": [
        {
          "id": "MDRW2100032569.1.1",
          "form": "광자매 혹시 보니?",
          "original_form": "광자매 혹시 보니?",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032569.1.2",
          "form": "아니 ㅋㅋㅋ근데 저번주인가?",
          "original_form": "아니 ㅋㅋㅋ근데 저번주인가?",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032569.1.3",
          "form": "본 것 같다",
          "original_form": "본 것 같다",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032569.1.4",
          "form": "난자냉동하는데 남편 안오고 막",
          "original_form": "난자냉동하는데 남편 안오고 막",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032569.1.5",
          "form": "아 드라마 내용 엉망임ㅋㅋㅋㅋ",
          "original_form": "아 드라마 내용 엉망임ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032569.1.6",
          "form": "딱 두번 봤는데",
          "original_form": "딱 두번 봤는데",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.7",
          "form": "시어머니 진짜 문제많은데",
          "original_form": "시어머니 진짜 문제많은데",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.8",
          "form": "복덩엄마 죽은 날이랑",
          "original_form": "복덩엄마 죽은 날이랑",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.9",
          "form": "난자냉동 ㅋㅋㅋ",
          "original_form": "난자냉동 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.10",
          "form": "오늘 갑자기 천사됨 전개 개이상함",
          "original_form": "오늘 갑자기 천사됨 전개 개이상함",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.11",
          "form": "?? 시어먀 말투 진짜",
          "original_form": "?? 시어먀 말투 진짜",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.13",
          "form": "노답이었는데",
          "original_form": "노답이었는데",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.14",
          "form": "갑자기? ㅋㅋㅋ",
          "original_form": "갑자기? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.15",
          "form": "ㅇㅇ근데갑자기 천사되서 이상해 오늘 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ근데갑자기 천사되서 이상해 오늘 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032569.1.16",
          "form": "끝날때됐는데 어칼지 몰라서",
          "original_form": "끝날때됐는데 어칼지 몰라서",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032569.1.17",
          "form": "걍 바꾼건가 ㅋㅋㅋ",
          "original_form": "걍 바꾼건가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032569.1.18",
          "form": "진짜 그런느낌임ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 그런느낌임ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032569.1.19",
          "form": "근데 거기 발암 또 잇자나",
          "original_form": "근데 거기 발암 또 잇자나",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032569.1.20",
          "form": "막내딸 친아빠?",
          "original_form": "막내딸 친아빠?",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032569.1.21",
          "form": "맞아 막장임ㅋㅋㅋㅋ",
          "original_form": "맞아 막장임ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100032569.1.22",
          "form": "ㅋㅋㅋㅋ근디 역시 주말은 막장이지",
          "original_form": "ㅋㅋㅋㅋ근디 역시 주말은 막장이지",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032569.1.23",
          "form": "그래도 순옥킴보다는 덜하지 뭐...",
          "original_form": "그래도 순옥킴보다는 덜하지 뭐...",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032569.1.24",
          "form": "아냐 뭔가...그래도 순옥킴은 전개 안답답하니가...",
          "original_form": "아냐 뭔가...그래도 순옥킴은 전개 안답답하니가...",
