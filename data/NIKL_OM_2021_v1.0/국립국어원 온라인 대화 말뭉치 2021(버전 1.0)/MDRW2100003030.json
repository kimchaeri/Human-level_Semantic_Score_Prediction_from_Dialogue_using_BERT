      "utterance": [
        {
          "id": "MDRW2100003030.1.1",
          "form": "요즘에 사신 것중에 좋은 물건 있으신가요?",
          "original_form": "요즘에 사신 것중에 좋은 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003030.1.2",
          "form": "음 저 최근에 테이프클리너 구입했어요",
          "original_form": "음 저 최근에 테이프클리너 구입했어요",
          "speaker_id": "2",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003030.1.3",
          "form": "아 돌돌이?",
          "original_form": "아 돌돌이?",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003030.1.4",
          "form": "네  그거요",
          "original_form": "네  그거요",
          "speaker_id": "2",
          "time": "20210820 23:09"
        },
        {
          "id": "MDRW2100003030.1.5",
          "form": "맞아요 돌돌이도 중독이지요. 고로고로쓰세요?",
          "original_form": "맞아요 돌돌이도 중독이지요. 고로고로쓰세요?",
          "speaker_id": "1",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100003030.1.6",
          "form": "저 그거 썼는데 비싸서 저렴이로 갈아탔어요",
          "original_form": "저 그거 썼는데 비싸서 저렴이로 갈아탔어요",
          "speaker_id": "2",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100003030.1.7",
          "form": "그래도 고로고로만한게 없어서 다시 돌아갈까 생각중이에요ㅠ",
          "original_form": "그래도 고로고로만한게 없어서 다시 돌아갈까 생각중이에요ㅠ",
          "speaker_id": "2",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100003030.1.8",
          "form": "저렴이 다이소꺼 써봤는데",
          "original_form": "저렴이 다이소꺼 써봤는데",
          "speaker_id": "1",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100003030.1.9",
          "form": "성질버려요ㅠ",
          "original_form": "성질버려요ㅠ",
          "speaker_id": "1",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100003030.1.10",
          "form": "그건 진짜..",
          "original_form": "그건 진짜..",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100003030.1.11",
          "form": "못쓰겠더라구요",
          "original_form": "못쓰겠더라구요",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100003030.1.12",
          "form": "약간 돌돌이라는 이름도 아까운..",
          "original_form": "약간 돌돌이라는 이름도 아까운..",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100003030.1.13",
          "form": "최근에 구매하신 상품 있으신가요",
          "original_form": "최근에 구매하신 상품 있으신가요",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100003030.1.14",
          "form": "저는 무알콜 맥주에 중독되었답니다ㅎㅎ",
          "original_form": "저는 무알콜 맥주에 중독되었답니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100003030.1.15",
          "form": "택배도 되어서",
          "original_form": "택배도 되어서",
          "speaker_id": "1",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100003030.1.16",
          "form": "오 선호하는 브랜드가있나오?",
          "original_form": "오 선호하는 브랜드가있나오?",
          "speaker_id": "2",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100003030.1.17",
          "form": "하이트 새로나온거 괜찮더라고요",
          "original_form": "하이트 새로나온거 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100003030.1.18",
          "form": "맛이 깔끔한가요",
          "original_form": "맛이 깔끔한가요",
          "speaker_id": "2",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100003030.1.19",
          "form": "칼로리도 적답니다ㅋ",
          "original_form": "칼로리도 적답니다ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100003030.1.20",
          "form": "칼로리도 적군요",
          "original_form": "칼로리도 적군요",
          "speaker_id": "2",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100003030.1.21",
          "form": "네 제가 느끼기에는 그래요ㅋㅋ",
          "original_form": "네 제가 느끼기에는 그래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100003030.1.22",
          "form": "맥주는 시원하다가도 마시고 나면 더워서ㅠ",
          "original_form": "맥주는 시원하다가도 마시고 나면 더워서ㅠ",
          "speaker_id": "1",
          "time": "20210823 10:52"
        }
      ]
