      "utterance": [
        {
          "id": "MDRW2100047388.1.1",
          "form": "넷플보셔요?",
          "original_form": "넷플보셔요?",
          "speaker_id": "2",
          "time": "20210827 00:33"
        },
        {
          "id": "MDRW2100047388.1.2",
          "form": "네:) 보긴하는데\r\n슬의생만 보구있어요",
          "original_form": "네:) 보긴하는데\r\n슬의생만 보구있어요",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100047388.1.3",
          "form": "name2님도 넷플 보세요?",
          "original_form": "&name2&님도 넷플 보세요?",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100047388.1.4",
          "form": "슬의생 사랑합니다",
          "original_form": "슬의생 사랑합니다",
          "speaker_id": "2",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100047388.1.5",
          "form": "넷플과 티빙의 행벅",
          "original_form": "넷플과 티빙의 행벅",
          "speaker_id": "2",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100047388.1.6",
          "form": "아 티빙도 보시는구나",
          "original_form": "아 티빙도 보시는구나",
          "speaker_id": "1",
          "time": "20210827 00:42"
        },
        {
          "id": "MDRW2100047388.1.7",
          "form": "저 유툽에서 환승연애봤는데\r\n넘 재밌어서 티빙도 가입해야되나\r\n고민중이에여ㅋㅋ",
          "original_form": "저 유툽에서 환승연애봤는데\r\n넘 재밌어서 티빙도 가입해야되나\r\n고민중이에여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:42"
        },
        {
          "id": "MDRW2100047388.1.8",
          "form": "네이버플러스로 꼭 보세요 티빙도ㅋㅋ",
          "original_form": "네이버플러스로 꼭 보세요 티빙도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:43"
        },
        {
          "id": "MDRW2100047388.1.9",
          "form": "그 넷플에도 있어요 돌싱글즈와 체인지 어쩌고",
          "original_form": "그 넷플에도 있어요 돌싱글즈와 체인지 어쩌고",
          "speaker_id": "2",
          "time": "20210827 00:43"
        },
        {
          "id": "MDRW2100047388.1.10",
          "form": "아..... 저 넷플 다 보고있네여ㅋㅋㅈ",
          "original_form": "아..... 저 넷플 다 보고있네여ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210827 00:45"
        },
        {
          "id": "MDRW2100047388.1.11",
          "form": "말씀하신것들도 다 봤어여ㅋㅋㅋ",
          "original_form": "말씀하신것들도 다 봤어여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:45"
        },
        {
          "id": "MDRW2100047388.1.12",
          "form": "요즘 연예인들 나오는거보다 그런게 더 재밌네요!",
          "original_form": "요즘 연예인들 나오는거보다 그런게 더 재밌네요!",
          "speaker_id": "2",
          "time": "20210827 00:45"
        },
        {
          "id": "MDRW2100047388.1.13",
          "form": "슬의생은 혼자조용히보고 흐믓하게 웃는 방송!ㅎㅎ",
          "original_form": "슬의생은 혼자조용히보고 흐믓하게 웃는 방송!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:46"
        },
        {
          "id": "MDRW2100047388.1.14",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:46"
        },
        {
          "id": "MDRW2100047388.1.15",
          "form": "슬의생 담주 결방 이라던데...ㅜㅜ",
          "original_form": "슬의생 담주 결방 이라던데...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 00:46"
        },
        {
          "id": "MDRW2100047388.1.16",
          "form": "헉 왜 결방이에요 아...",
          "original_form": "헉 왜 결방이에요 아...",
          "speaker_id": "2",
          "time": "20210827 00:46"
        },
        {
          "id": "MDRW2100047388.1.17",
          "form": "다른거봐야지ㅋㅋㅋ",
          "original_form": "다른거봐야지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:47"
        },
        {
          "id": "MDRW2100047388.1.18",
          "form": "축구경기 중계하나보더라구요 ㅜㅜㅋ",
          "original_form": "축구경기 중계하나보더라구요 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210827 00:47"
        },
        {
          "id": "MDRW2100047388.1.19",
          "form": "2주를 어떻게 기다리죵",
          "original_form": "2주를 어떻게 기다리죵",
          "speaker_id": "1",
          "time": "20210827 00:47"
        },
        {
          "id": "MDRW2100047388.1.20",
          "form": "이런 축구",
          "original_form": "이런 축구",
          "speaker_id": "2",
          "time": "20210827 00:47"
        },
        {
          "id": "MDRW2100047388.1.21",
          "form": "넷플에 미드도 많습니다",
          "original_form": "넷플에 미드도 많습니다",
          "speaker_id": "2",
          "time": "20210827 00:47"
        },
        {
          "id": "MDRW2100047388.1.22",
          "form": "미드 추천 부탁드려요",
          "original_form": "미드 추천 부탁드려요",
