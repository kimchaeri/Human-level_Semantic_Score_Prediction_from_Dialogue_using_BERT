      "utterance": [
        {
          "id": "MDRW2100017000.1.1",
          "form": "얼마전에 넥플릭스에서 퀴즈 풀어서 선물주는 프로?에 샤이니가 나왔어요",
          "original_form": "얼마전에 넥플릭스에서 퀴즈 풀어서 선물주는 프로?에 샤이니가 나왔어요",
          "speaker_id": "2",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100017000.1.2",
          "form": "아 그거 봤어요 퍼준다는거ㅋㅋ",
          "original_form": "아 그거 봤어요 퍼준다는거ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017000.1.3",
          "form": "진짜 퍼주던데ㅋㅋ",
          "original_form": "진짜 퍼주던데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100017000.1.4",
          "form": "그게 사전에 뭐갖고 싶은지 알아보나봐요",
          "original_form": "그게 사전에 뭐갖고 싶은지 알아보나봐요",
          "speaker_id": "2",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017000.1.5",
          "form": "아 스메그도요?",
          "original_form": "아 스메그도요?",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017000.1.6",
          "form": "그래서 온유군은 캠핑셋트 달라고 하고 민호군이 애플왓치 달라고 했었는데",
          "original_form": "그래서 온유군은 캠핑셋트 달라고 하고 민호군이 애플왓치 달라고 했었는데",
          "speaker_id": "2",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100017000.1.7",
          "form": "맞아요ㅋㅋ 근데 원래 캠핑 했던가요?",
          "original_form": "맞아요ㅋㅋ 근데 원래 캠핑 했던가요?",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017000.1.8",
          "form": "캠핑용품 은근 비싸요",
          "original_form": "캠핑용품 은근 비싸요",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017000.1.9",
          "form": "경품이 바꿔서 됐는데 온유군이 애플왓치 너 써 하고 민호군 줬는데 안 받더니 뭐 뺏어오긴가? 그런 게임 걸러서 온유군 왓치가 결국은 민호군한테",
          "original_form": "경품이 바꿔서 됐는데 온유군이 애플왓치 너 써 하고 민호군 줬는데 안 받더니 뭐 뺏어오긴가? 그런 게임 걸러서 온유군 왓치가 결국은 민호군한테",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017000.1.10",
          "form": "ㅋㅋㅋ줄거같았어요ㅋㅋ",
          "original_form": "ㅋㅋㅋ줄거같았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017000.1.11",
          "form": "온유 애플이긴 하져?",
          "original_form": "온유 애플이긴 하져?",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017000.1.12",
          "form": "근데 민호군이 끝나고 캠핑셋트 줬대요\r\n민호군 폰이 업댓이 안됐나해서 왓치 연결 아직 못 했고",
          "original_form": "근데 민호군이 끝나고 캠핑셋트 줬대요\r\n민호군 폰이 업댓이 안됐나해서 왓치 연결 아직 못 했고",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017000.1.13",
          "form": "애플이긴 할거예요 근데 엄청 오래 쓰고 있는거 같던데 ㅋㅋㅋ",
          "original_form": "애플이긴 할거예요 근데 엄청 오래 쓰고 있는거 같던데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017000.1.14",
          "form": "키군이 형 걍 쓰라고 충전하지 말고 끼고 다니라고 ㅋㅋㅋㅋ",
          "original_form": "키군이 형 걍 쓰라고 충전하지 말고 끼고 다니라고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017000.1.15",
          "form": "왜 충전하지 말래요ㅋㅋ",
          "original_form": "왜 충전하지 말래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017000.1.16",
          "form": "이번에 유툽 콘텐츠에 캠핑할 거 같긴 한데",
          "original_form": "이번에 유툽 콘텐츠에 캠핑할 거 같긴 한데",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017000.1.17",
          "form": "저 그거 봤는데",
          "original_form": "저 그거 봤는데",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017000.1.18",
          "form": "흐름이 기억 안나요",
          "original_form": "흐름이 기억 안나요",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017000.1.19",
          "form": "안쓴다니까 걍 끼고만 다니라고 ㅋㅋㅋ",
          "original_form": "안쓴다니까 걍 끼고만 다니라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017000.1.20",
          "form": "태연 플립을 산건지 받은건지 어제 인스타에 올렸는데",
          "original_form": "태연 플립을 산건지 받은건지 어제 인스타에 올렸는데",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017000.1.21",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017000.1.22",
          "form": "돌들한테 플립 좀 뿌렸음 좋겠네요",
          "original_form": "돌들한테 플립 좀 뿌렸음 좋겠네요",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017000.1.23",
          "form": "글게요",
          "original_form": "글게요",
          "speaker_id": "2",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100017000.1.24",
          "form": "온유군 원래 갤럭시 썼었는데",
          "original_form": "온유군 원래 갤럭시 썼었는데",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017000.1.25",
          "form": "어차피 세컨도 있을거 아니에요",
          "original_form": "어차피 세컨도 있을거 아니에요",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017000.1.26",
          "form": "일단 브이라이브는 갤럭시가 나아요",
          "original_form": "일단 브이라이브는 갤럭시가 나아요",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017000.1.27",
          "form": "ㅇㅇ 글게요",
          "original_form": "ㅇㅇ 글게요",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017000.1.28",
          "form": "아 지코가 갤럭시인데",
          "original_form": "아 지코가 갤럭시인데",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017000.1.29",
          "form": "이유가 컴맹이라 음원파일을 옮길수가 없어서라고ㅋㅋ",
          "original_form": "이유가 컴맹이라 음원파일을 옮길수가 없어서라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017000.1.30",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017000.1.31",
          "form": "덕질은 안드로이드로",
          "original_form": "덕질은 안드로이드로",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017000.1.32",
          "form": "넷플에 산드라오 드라마 재밌어 보이는데 볼 시간이 없네요",
          "original_form": "넷플에 산드라오 드라마 재밌어 보이는데 볼 시간이 없네요",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017000.1.33",
          "form": "예능 보느라ㅋㅋ",
          "original_form": "예능 보느라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017000.1.34",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017000.1.35",
          "form": "요즘 전 케이블 만으로도 ㅋㅋ",
          "original_form": "요즘 전 케이블 만으로도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017000.1.36",
          "form": "예고만 봤었는데 대학 학과장인가 총장인가되는 코미디던데",
          "original_form": "예고만 봤었는데 대학 학과장인가 총장인가되는 코미디던데",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017000.1.37",
          "form": "오늘 슬의는 긴게 확정이라 내용이나 재밌었음 좋겠어요",
          "original_form": "오늘 슬의는 긴게 확정이라 내용이나 재밌었음 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017000.1.38",
          "form": "운동하면서 볼건데",
          "original_form": "운동하면서 볼건데",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017000.1.39",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017000.1.40",
          "form": "세계사 그거요 화욜이죠?",
          "original_form": "세계사 그거요 화욜이죠?",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017000.1.41",
          "form": "거꾸로보는 세계사",
          "original_form": "거꾸로보는 세계사",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017000.1.42",
          "form": "운동!",
          "original_form": "운동!",
          "speaker_id": "2",
          "time": "20210826 17:25"
        }
      ]
