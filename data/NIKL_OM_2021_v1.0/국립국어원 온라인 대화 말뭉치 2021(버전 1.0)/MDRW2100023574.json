      "utterance": [
        {
          "id": "MDRW2100023574.1.1",
          "form": "미용실 자주 가십니까",
          "original_form": "미용실 자주 가십니까",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100023574.1.2",
          "form": "사촌언니가 미용실을 오픈했어요",
          "original_form": "사촌언니가 미용실을 오픈했어요",
          "speaker_id": "1",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023574.1.3",
          "form": "어쩔 수 없이 자주 가게 되었어요",
          "original_form": "어쩔 수 없이 자주 가게 되었어요",
          "speaker_id": "1",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023574.1.4",
          "form": "건강을 위해 따로 하시는 운동이 있으신가요",
          "original_form": "건강을 위해 따로 하시는 운동이 있으신가요",
          "speaker_id": "1",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023574.1.5",
          "form": "숨쉬기 운동 하구요",
          "original_form": "숨쉬기 운동 하구요",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023574.1.6",
          "form": "그리고 철봉얻은거 있어서",
          "original_form": "그리고 철봉얻은거 있어서",
          "speaker_id": "2",
          "time": "20210830 11:47"
        },
        {
          "id": "MDRW2100023574.1.7",
          "form": "그걸로 조금했더니만 몸짱 되었습니다",
          "original_form": "그걸로 조금했더니만 몸짱 되었습니다",
          "speaker_id": "2",
          "time": "20210830 11:47"
        },
        {
          "id": "MDRW2100023574.1.8",
          "form": "빨래를 복근에서 하고있네요",
          "original_form": "빨래를 복근에서 하고있네요",
          "speaker_id": "2",
          "time": "20210830 11:47"
        },
        {
          "id": "MDRW2100023574.1.9",
          "form": "철봉 빌려드릴까요?",
          "original_form": "철봉 빌려드릴까요?",
          "speaker_id": "2",
          "time": "20210830 11:48"
        },
        {
          "id": "MDRW2100023574.1.10",
          "form": "폰이 제 말을 잘 인식 못하네요",
          "original_form": "폰이 제 말을 잘 인식 못하네요",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023574.1.11",
          "form": "그네도 건강에 도움이됩니다",
          "original_form": "그네도 건강에 도움이됩니다",
          "speaker_id": "2",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023574.1.12",
          "form": "개떡같이 말해도 찰떡같이 알아들어 주세요",
          "original_form": "개떡같이 말해도 찰떡같이 알아들어 주세요",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023574.1.13",
          "form": "말을 잘못하시는것 같아요",
          "original_form": "말을 잘못하시는것 같아요",
          "speaker_id": "2",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023574.1.14",
          "form": "사투리를 잘못 알아듣네요",
          "original_form": "사투리를 잘못 알아듣네요",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023574.1.15",
          "form": "아 사투리 쓰시나보네요?",
          "original_form": "아 사투리 쓰시나보네요?",
          "speaker_id": "2",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023574.1.16",
          "form": "아 모르셨나 봐요~",
          "original_form": "아 모르셨나 봐요~",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023574.1.17",
          "form": "경상도 사투리로 펌은 뭐라고 하나요?",
          "original_form": "경상도 사투리로 펌은 뭐라고 하나요?",
          "speaker_id": "2",
          "time": "20210830 11:50"
        },
