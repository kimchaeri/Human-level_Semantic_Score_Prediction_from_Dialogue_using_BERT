      "utterance": [
        {
          "id": "MDRW2100002660.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002660.1.2",
          "form": "네 여행엄청좋아해요 name2님은여",
          "original_form": "네 여행엄청좋아해요 &name2&님은여",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002660.1.3",
          "form": "저도 좋아하는 편인데 자주가진 못하네요 ㅠㅠ",
          "original_form": "저도 좋아하는 편인데 자주가진 못하네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002660.1.4",
          "form": "그쳐 아무래도 코로나라ㅜ",
          "original_form": "그쳐 아무래도 코로나라ㅜ",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002660.1.5",
          "form": "가장좋았던 여행지는 어디에요",
          "original_form": "가장좋았던 여행지는 어디에요",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002660.1.6",
          "form": "저는 국내에서는 경주랑 제주요!",
          "original_form": "저는 국내에서는 경주랑 제주요!",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002660.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002660.1.8",
          "form": "저도 제주도 엄청좋아해요 ㅋㅋㅋ",
          "original_form": "저도 제주도 엄청좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002660.1.9",
          "form": "해외여행은어디가 가장좋으셨어요",
          "original_form": "해외여행은어디가 가장좋으셨어요",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002660.1.10",
          "form": "가보고싶은여행지나",
          "original_form": "가보고싶은여행지나",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002660.1.11",
          "form": "해외여행은 태국이요! 해와여행을 많이가보진 못했지만..",
          "original_form": "해외여행은 태국이요! 해와여행을 많이가보진 못했지만..",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002660.1.12",
          "form": "태국은 다시가보고싶어요!",
          "original_form": "태국은 다시가보고싶어요!",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002660.1.13",
          "form": "name1님은 어디가 가장좋았나요?",
          "original_form": "&name1&님은 어디가 가장좋았나요?",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002660.1.14",
          "form": "태국 좋죠 ㅜㅜ전 코로나가기 직전에 베트남갔다왔는데",
          "original_form": "태국 좋죠 ㅜㅜ전 코로나가기 직전에 베트남갔다왔는데",
          "speaker_id": "1",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100002660.1.15",
          "form": "최근에 그래도간곳이라그런지 젤 기억이나요",
          "original_form": "최근에 그래도간곳이라그런지 젤 기억이나요",
          "speaker_id": "1",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100002660.1.16",
          "form": "베트남 정말 가보규 싶었는데 ㅠㅠ 코로나전에 가볼껄 너무 아쉬워요 ㅠㅠㅋㅋㅋ",
          "original_form": "베트남 정말 가보규 싶었는데 ㅠㅠ 코로나전에 가볼껄 너무 아쉬워요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100002660.1.17",
          "form": "코로나끝나먼 어디 제일 가고싶으세오?",
          "original_form": "코로나끝나먼 어디 제일 가고싶으세오?",
          "speaker_id": "2",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100002660.1.18",
          "form": "저 휴양지요ㅜ하와이한번가보고싶어요",
          "original_form": "저 휴양지요ㅜ하와이한번가보고싶어요",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100002660.1.19",
          "form": "하와이 상상만해도 너뮤 행복하네여 ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "하와이 상상만해도 너뮤 행복하네여 ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100002660.1.20",
          "form": "저도 휴양지가서 딱 누워서 바다보규 맛있는거 먹고 쉬고싶어요",
          "original_form": "저도 휴양지가서 딱 누워서 바다보규 맛있는거 먹고 쉬고싶어요",
          "speaker_id": "2",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100002660.1.21",
          "form": "저는 발리가서 서핑해보고싶어요!!!",
          "original_form": "저는 발리가서 서핑해보고싶어요!!!",
          "speaker_id": "2",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100002660.1.22",
          "form": "서핑도 한번배워보고싶네요",
          "original_form": "서핑도 한번배워보고싶네요",
          "speaker_id": "1",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100002660.1.23",
          "form": "저도 못하지만 꼭 배워보려구요 ㅋㅋㅋ 여행가면 기념품은 꼭 사오시나요?",
          "original_form": "저도 못하지만 꼭 배워보려구요 ㅋㅋㅋ 여행가면 기념품은 꼭 사오시나요?",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100002660.1.24",
          "form": "저도 운동신경은없지만 배워보고싶네요 ㅋㅋㅋ",
          "original_form": "저도 운동신경은없지만 배워보고싶네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100002660.1.25",
          "form": "기념품 소소한걸루",
          "original_form": "기념품 소소한걸루",
