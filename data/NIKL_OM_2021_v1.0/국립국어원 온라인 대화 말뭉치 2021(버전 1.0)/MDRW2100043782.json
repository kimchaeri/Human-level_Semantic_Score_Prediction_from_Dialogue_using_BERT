      "utterance": [
        {
          "id": "MDRW2100043782.1.1",
          "form": "학생에게 관심이 많은 좋은 교수님이셨군요",
          "original_form": "학생에게 관심이 많은 좋은 교수님이셨군요",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043782.1.2",
          "form": "그쵸ㅋㅋ 모든 학생의 얼굴과 이름을 외우셨어요",
          "original_form": "그쵸ㅋㅋ 모든 학생의 얼굴과 이름을 외우셨어요",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043782.1.3",
          "form": "120명쯤 듣는 수업에서 출석체크 들어오시면",
          "original_form": "120명쯤 듣는 수업에서 출석체크 들어오시면",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043782.1.4",
          "form": "한번 왼쪽에서 오른쪽 쓱 보시고는",
          "original_form": "한번 왼쪽에서 오른쪽 쓱 보시고는",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043782.1.5",
          "form": "없는 학생 이름만 부르셨어요",
          "original_form": "없는 학생 이름만 부르셨어요",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043782.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043782.1.7",
          "form": "절대 도망치지 못하는 수업..",
          "original_form": "절대 도망치지 못하는 수업..",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043782.1.8",
          "form": "와와 그건 진짜 대단하네요",
          "original_form": "와와 그건 진짜 대단하네요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043782.1.9",
          "form": "그런 대형강에서!",
          "original_form": "그런 대형강에서!",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043782.1.10",
          "form": "무슨 그 영화에 나오는 천재교수님 같아요",
          "original_form": "무슨 그 영화에 나오는 천재교수님 같아요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043782.1.11",
          "form": "다만 관대하시게도",
          "original_form": "다만 관대하시게도",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043782.1.12",
          "form": "3번 아무이유없이 수업자체휴강권을주셨죠",
          "original_form": "3번 아무이유없이 수업자체휴강권을주셨죠",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043782.1.13",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043782.1.14",
          "form": "어메리칸 스타일이시네요",
          "original_form": "어메리칸 스타일이시네요",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043782.1.15",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043782.1.16",
          "form": "한번 수업에 안가면 따라가기 힘들어서",
          "original_form": "한번 수업에 안가면 따라가기 힘들어서",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043782.1.17",
          "form": "잘 쓰지는 못했어요ㅋㅋ",
          "original_form": "잘 쓰지는 못했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043782.1.18",
          "form": "ㅋㅋㅋ그런 부분까지 고려한 관대함이셨군요",
          "original_form": "ㅋㅋㅋ그런 부분까지 고려한 관대함이셨군요",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043782.1.19",
          "form": "따라올테면 따라와라",
          "original_form": "따라올테면 따라와라",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043782.1.20",
          "form": "네 갈테면 가보렴^^",
          "original_form": "네 갈테면 가보렴^^",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043782.1.21",
          "form": "요녀석 도망칠수없단다",
          "original_form": "요녀석 도망칠수없단다",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043782.1.22",
          "form": "혼자 해봐 힘들지? 안나와도 되지만 뭐 네가 꼭 수업에 나오겠다면 말리진 않으리",
          "original_form": "혼자 해봐 힘들지? 안나와도 되지만 뭐 네가 꼭 수업에 나오겠다면 말리진 않으리",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043782.1.23",
          "form": "난 휴강권 줬다~",
          "original_form": "난 휴강권 줬다~",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043782.1.24",
          "form": "그렇죠ㅋㅋ",
          "original_form": "그렇죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043782.1.25",
          "form": "쓸테면 써보렴",
          "original_form": "쓸테면 써보렴",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043782.1.26",
          "form": "멋진 분이시네요 ㅋㅋㅋ 저도 배우고 싶은",
          "original_form": "멋진 분이시네요 ㅋㅋㅋ 저도 배우고 싶은",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043782.1.27",
          "form": "청강으로요",
          "original_form": "청강으로요",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043782.1.28",
          "form": "하지만 그건 교수님 본인이 너무 힘든 강의스타일이세요ㅋㅋ",
