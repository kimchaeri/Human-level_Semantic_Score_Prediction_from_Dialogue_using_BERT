          "speaker_id": "1",
          "time": "20210908 06:40"
        },
        {
          "id": "MDRW2100045745.1.17",
          "form": "한 가지 맛인가요?",
          "original_form": "한 가지 맛인가요?",
          "speaker_id": "2",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045745.1.18",
