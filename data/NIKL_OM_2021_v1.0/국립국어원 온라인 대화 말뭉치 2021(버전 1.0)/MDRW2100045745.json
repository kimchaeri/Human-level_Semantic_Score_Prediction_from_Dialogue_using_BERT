        },
        {
          "id": "MDRW2100045745.1.11",
          "form": "아닙니다!",
          "original_form": "아닙니다!",
          "speaker_id": "1",
          "time": "20210908 06:39"
        },
        {
          "id": "MDRW2100045745.1.12",
          "form": "이럴수가 그럼 통에 들은건가요?",
          "original_form": "이럴수가 그럼 통에 들은건가요?",
          "speaker_id": "2",
          "time": "20210908 06:39"
        },
