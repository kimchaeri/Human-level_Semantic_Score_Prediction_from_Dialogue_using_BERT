          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045745.1.7",
          "form": "오키요!!",
          "original_form": "오키요!!",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045745.1.8",
          "form": "막대 아이스인가요?",
          "original_form": "막대 아이스인가요?",
