      "utterance": [
        {
          "id": "MDRW2100010458.1.1",
          "form": "진짜 달달한 로코입니다",
          "original_form": "진짜 달달한 로코입니다",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010458.1.2",
          "form": "껄껄",
          "original_form": "껄껄",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010458.1.3",
          "form": "ㅎ...니예니예",
          "original_form": "ㅎ...니예니예",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010458.1.4",
          "form": "책 읽는거 좋아해요?",
          "original_form": "책 읽는거 좋아해요?",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010458.1.5",
          "form": "예전엔 많이 읽었어요",
          "original_form": "예전엔 많이 읽었어요",
          "speaker_id": "1",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010458.1.6",
          "form": "더글러스 케네디나 기욤 뮈소",
          "original_form": "더글러스 케네디나 기욤 뮈소",
          "speaker_id": "1",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010458.1.7",
          "form": "오...저는 책읽은지 꽤 되었는데ㅋㅋ",
          "original_form": "오...저는 책읽은지 꽤 되었는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010458.1.8",
          "form": "몸의 양식이나 마음의 양식이나 많이 먹는 편이어서 돼진가봐요",
          "original_form": "몸의 양식이나 마음의 양식이나 많이 먹는 편이어서 돼진가봐요",
          "speaker_id": "1",
          "time": "20210823 19:37"
        },
        {
          "id": "MDRW2100010458.1.9",
          "form": "김리...?ㅋㅋㅋ",
          "original_form": "김리...?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010458.1.10",
          "form": "전 영문과다보니 학교 다닐 때 위대한 개츠비랑 시련, 제인 오스틴, 오만과 편견 이런거 읽었었는데",
          "original_form": "전 영문과다보니 학교 다닐 때 위대한 개츠비랑 시련, 제인 오스틴, 오만과 편견 이런거 읽었었는데",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010458.1.11",
          "form": "그 때 치여서 그런가 읽고 싶지 않아요....ㅜㅜ",
          "original_form": "그 때 치여서 그런가 읽고 싶지 않아요....ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010458.1.12",
          "form": "제목만 들어도 토가나오네요",
          "original_form": "제목만 들어도 토가나오네요",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010458.1.13",
          "form": "제가 내용있는 책 알러지가있어서",
          "original_form": "제가 내용있는 책 알러지가있어서",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010458.1.14",
          "form": "어 아니구나 재밌는책인데",
          "original_form": "어 아니구나 재밌는책인데",
          "speaker_id": "1",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010458.1.15",
          "form": "다 원어겠네요",
          "original_form": "다 원어겠네요",
          "speaker_id": "1",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010458.1.16",
          "form": "어후 끔찍쓰",
          "original_form": "어후 끔찍쓰",
          "speaker_id": "1",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010458.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ끔찍쓰....뭐...그래도 재밌긴했어요 내용이.",
          "original_form": "ㅋㅋㅋㅋㅋㅋ끔찍쓰....뭐...그래도 재밌긴했어요 내용이.",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010458.1.18",
          "form": "저는 저거 고등학교 다닐 때 읽었어요",
          "original_form": "저는 저거 고등학교 다닐 때 읽었어요",
          "speaker_id": "1",
          "time": "20210824 09:06"
        },
        {
          "id": "MDRW2100010458.1.19",
          "form": "한글로",
          "original_form": "한글로",
          "speaker_id": "1",
          "time": "20210824 09:06"
        },
        {
          "id": "MDRW2100010458.1.20",
          "form": "근데 지금까지 기억 남는책은",
          "original_form": "근데 지금까지 기억 남는책은",
          "speaker_id": "1",
          "time": "20210824 09:07"
        },
        {
          "id": "MDRW2100010458.1.21",
          "form": "상실의시대랑 빅픽쳐",
          "original_form": "상실의시대랑 빅픽쳐",
          "speaker_id": "1",
          "time": "20210824 09:07"
        },
        {
          "id": "MDRW2100010458.1.22",
          "form": "무슨내용인가여",
          "original_form": "무슨내용인가여",
          "speaker_id": "2",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100010458.1.23",
          "form": "상실의시대는 생략하고",
          "original_form": "상실의시대는 생략하고",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100010458.1.24",
          "form": "빅픽쳐는 그",
          "original_form": "빅픽쳐는 그",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100010458.1.25",
          "form": "사진에 관한건데",
          "original_form": "사진에 관한건데",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100010458.1.26",
          "form": "사진작가의 인생 제 2막에 관한 내용입니다",
          "original_form": "사진작가의 인생 제 2막에 관한 내용입니다",
          "speaker_id": "1",
          "time": "20210824 10:14"
        },
        {
          "id": "MDRW2100010458.1.27",
          "form": "음...나중에 서점가면 빅픽쳐 한 번 봐봐야겠네요ㅋ",
          "original_form": "음...나중에 서점가면 빅픽쳐 한 번 봐봐야겠네요ㅋ",
          "speaker_id": "2",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100010458.1.28",
          "form": "한번 읽어보세요. 사진 관심 있으시면 공감 많이 가는 소설일거예요",
          "original_form": "한번 읽어보세요. 사진 관심 있으시면 공감 많이 가는 소설일거예요",
          "speaker_id": "1",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100010458.1.29",
          "form": "오...알겠어요!",
          "original_form": "오...알겠어요!",
          "speaker_id": "2",
          "time": "20210824 13:05"
        }
      ]
    }
  ]
