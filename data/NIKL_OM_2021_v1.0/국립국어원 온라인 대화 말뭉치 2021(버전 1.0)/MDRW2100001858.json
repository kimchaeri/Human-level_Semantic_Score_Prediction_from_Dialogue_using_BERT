      "utterance": [
        {
          "id": "MDRW2100001858.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001858.1.2",
          "form": "안녕하세요 ^^",
          "original_form": "안녕하세요 ^^",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001858.1.3",
          "form": "혹시 믿으시는 종교 있으세요?",
          "original_form": "혹시 믿으시는 종교 있으세요?",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001858.1.4",
          "form": "저는 원래천주교인데",
          "original_form": "저는 원래천주교인데",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001858.1.5",
          "form": "무교에 가까워요",
          "original_form": "무교에 가까워요",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001858.1.6",
          "form": "아 저는 무교인데",
          "original_form": "아 저는 무교인데",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001858.1.7",
          "form": "요즘 종교에 관심 생기더라구요",
          "original_form": "요즘 종교에 관심 생기더라구요",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001858.1.8",
          "form": "불교쪽에",
          "original_form": "불교쪽에",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001858.1.9",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001858.1.10",
          "form": "뭔가 맘이 편안한 느낌?",
          "original_form": "뭔가 맘이 편안한 느낌?",
          "speaker_id": "2",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001858.1.11",
          "form": "불교가 아무래도",
          "original_form": "불교가 아무래도",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001858.1.12",
          "form": "신을 믿기보다는",
          "original_form": "신을 믿기보다는",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001858.1.13",
          "form": "삶을 바라보는 방식에대해 고찰하는 느낌?",
          "original_form": "삶을 바라보는 방식에대해 고찰하는 느낌?",
          "speaker_id": "1",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001858.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001858.1.15",
          "form": "템플스테이 같은거 해보고싶어요",
          "original_form": "템플스테이 같은거 해보고싶어요",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001858.1.16",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001858.1.17",
          "form": "또 사찰쪽이 이쁘기도 하고 가보고싶어요",
          "original_form": "또 사찰쪽이 이쁘기도 하고 가보고싶어요",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001858.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001858.1.19",
          "form": "근데 전 큰 불상을 보면 좀 무섭기도하고 ㅎㅎ",
          "original_form": "근데 전 큰 불상을 보면 좀 무섭기도하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001858.1.20",
          "form": "동상공포증이 있어서요",
          "original_form": "동상공포증이 있어서요",
          "speaker_id": "2",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001858.1.21",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001858.1.22",
          "form": "저는 분위기가 참 좋던데",
          "original_form": "저는 분위기가 참 좋던데",
          "speaker_id": "1",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001858.1.23",
          "form": "저는 불상만아니면 ㅎㅎ",
          "original_form": "저는 불상만아니면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001858.1.24",
          "form": "근데 티비에서 보고있음 또 맘편하고 괜찮아요",
          "original_form": "근데 티비에서 보고있음 또 맘편하고 괜찮아요",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001858.1.25",
          "form": "되게 신비한 느낌이 나더라구요",
          "original_form": "되게 신비한 느낌이 나더라구요",
          "speaker_id": "1",
          "time": "20210819 22:39"
        }
      ]
