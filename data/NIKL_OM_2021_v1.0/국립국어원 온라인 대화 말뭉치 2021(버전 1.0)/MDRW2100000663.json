      "utterance": [
        {
          "id": "MDRW2100000663.1.1",
          "form": "운동하시는거 있으세요?",
          "original_form": "운동하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000663.1.2",
          "form": "그냥 집에서 혈액순환 잘 되게 \n팔 다리 움직이고 산책정도 해요",
          "original_form": "그냥 집에서 혈액순환 잘 되게 \n팔 다리 움직이고 산책정도 해요",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000663.1.3",
          "form": "혹시 헬스장 다니세요?",
          "original_form": "혹시 헬스장 다니세요?",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000663.1.4",
          "form": "저는 필라테스 다녀요 ㅎ",
          "original_form": "저는 필라테스 다녀요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000663.1.5",
          "form": "아~  필라테스\n여자분들이 많이 하지요",
          "original_form": "아~  필라테스\n여자분들이 많이 하지요",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000663.1.6",
          "form": "요즘은 남자회원분들도 많이 오시더라구요",
          "original_form": "요즘은 남자회원분들도 많이 오시더라구요",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000663.1.7",
          "form": "개인아니면 커플수업으로",
          "original_form": "개인아니면 커플수업으로",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000663.1.8",
          "form": "아~ \n요가보다 어렵나요?",
          "original_form": "아~ \n요가보다 어렵나요?",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000663.1.9",
          "form": "힘이 조금더 들어가죠 근력강화에 좋아요",
          "original_form": "힘이 조금더 들어가죠 근력강화에 좋아요",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000663.1.10",
          "form": "네 코어능력 강화에 좋다고 하네요",
          "original_form": "네 코어능력 강화에 좋다고 하네요",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000663.1.11",
          "form": "전 가끔 플랭크만 좀 했었는데...",
          "original_form": "전 가끔 플랭크만 좀 했었는데...",
          "speaker_id": "2",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000663.1.12",
          "form": "맞아요 필라테스는 코어가 젤 중요해요",
          "original_form": "맞아요 필라테스는 코어가 젤 중요해요",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000663.1.13",
          "form": "그죠  코어가 강해야 척추관절도 튼튼해지고",
          "original_form": "그죠  코어가 강해야 척추관절도 튼튼해지고",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000663.1.14",
          "form": "특히 오래 앉아 있는 사무직은\n신경써줘야죠",
          "original_form": "특히 오래 앉아 있는 사무직은\n신경써줘야죠",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000663.1.15",
          "form": "허리아팠는데 이거하고 허리아픈게 없어져서 계속하고 있어요",
          "original_form": "허리아팠는데 이거하고 허리아픈게 없어져서 계속하고 있어요",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000663.1.16",
          "form": "좋은거같아요",
          "original_form": "좋은거같아요",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000663.1.17",
          "form": "그러시구나\n효과를 보셨으니 필라테스 한 보람이 있으셨겠네요^^",
          "original_form": "그러시구나\n효과를 보셨으니 필라테스 한 보람이 있으셨겠네요^^",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000663.1.18",
          "form": "네네 주변에 추천도하는데 안하시는분들은 비싼운동이라는 인식이 강하시더라구요",
          "original_form": "네네 주변에 추천도하는데 안하시는분들은 비싼운동이라는 인식이 강하시더라구요",
          "speaker_id": "1",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000663.1.19",
          "form": "그래도 건강 생각하면\n그렇게 비싼건 아닐 수도...",
          "original_form": "그래도 건강 생각하면\n그렇게 비싼건 아닐 수도...",
          "speaker_id": "2",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000663.1.20",
          "form": "맞아요 저는 좋아서 계속한지 2년되가는거같아요",
          "original_form": "맞아요 저는 좋아서 계속한지 2년되가는거같아요",
          "speaker_id": "1",
          "time": "20210819 17:47"
        }
      ]
