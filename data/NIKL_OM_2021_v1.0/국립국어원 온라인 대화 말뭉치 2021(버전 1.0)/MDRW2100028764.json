      "utterance": [
        {
          "id": "MDRW2100028764.1.1",
          "form": "그거 알아?",
          "original_form": "그거 알아?",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028764.1.2",
          "form": "\" 메이플스토리 \"",
          "original_form": "\" 메이플스토리 \"",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028764.1.3",
          "form": "뭔데?",
          "original_form": "뭔데?",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028764.1.4",
          "form": "< 메이플 >",
          "original_form": "< 메이플 >",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028764.1.5",
          "form": "《 메 이 플 스 토 리 》",
          "original_form": "《 메 이 플 스 토 리 》",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.6",
          "form": "아마 우리나라에서 메이플스토리를",
          "original_form": "아마 우리나라에서 메이플스토리를",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.7",
          "form": "모르는 사람은",
          "original_form": "모르는 사람은",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.8",
          "form": "없을거야",
          "original_form": "없을거야",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.9",
          "form": "다 어릴때 한번씩 해봣겟지",
          "original_form": "다 어릴때 한번씩 해봣겟지",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.10",
          "form": "나도 그렇게생각해",
          "original_form": "나도 그렇게생각해",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.11",
          "form": "너는요즘해?",
          "original_form": "너는요즘해?",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.12",
          "form": "게임 순위가 뭐라고생각해?",
          "original_form": "게임 순위가 뭐라고생각해?",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.13",
          "form": "당근이지",
          "original_form": "당근이지",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.14",
          "form": "요즘 10대 청소년 들한테는",
          "original_form": "요즘 10대 청소년 들한테는",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.15",
          "form": "잘 모르겟어",
          "original_form": "잘 모르겟어",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.16",
          "form": "메이플이 원탑이라구",
          "original_form": "메이플이 원탑이라구",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028764.1.17",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.18",
          "form": "난 메이플이 1등이라고 생각해",
          "original_form": "난 메이플이 1등이라고 생각해",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.19",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.20",
          "form": "나두 동의해 ㅎㅎ",
          "original_form": "나두 동의해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.21",
          "form": "2등으로는 뭐  ....",
          "original_form": "2등으로는 뭐  ....",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.22",
          "form": "메이플같은 횡스크롤게임은 이제 우리나라에",
          "original_form": "메이플같은 횡스크롤게임은 이제 우리나라에",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.23",
          "form": "메이플밖에없다더라..",
          "original_form": "메이플밖에없다더라..",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.24",
          "form": "어쩐지..",
          "original_form": "어쩐지..",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.25",
          "form": "메이플은 대체게임이 없어",
          "original_form": "메이플은 대체게임이 없어",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.26",
          "form": "그런것같어 ㅎㅎ",
          "original_form": "그런것같어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.27",
          "form": "암튼 2등으로는 뭐",
          "original_form": "암튼 2등으로는 뭐",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.28",
          "form": "오버워치같은게 있겠지 ㅎㅎ",
          "original_form": "오버워치같은게 있겠지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028764.1.29",
          "form": "오버워치 재밋어?",
          "original_form": "오버워치 재밋어?",
          "speaker_id": "2",
          "time": "20210902 21:55"
        }
      ]
    }
  ]
