      "utterance": [
        {
          "id": "MDRW2100006737.1.1",
          "form": "영양제는 어디서 구입하세요?",
          "original_form": "영양제는 어디서 구입하세요?",
          "speaker_id": "1",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006737.1.2",
          "form": "저는 그냥 약국에서 사요",
          "original_form": "저는 그냥 약국에서 사요",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006737.1.3",
          "form": "근데 동생은 인터넷으로 잘 사더라고요",
          "original_form": "근데 동생은 인터넷으로 잘 사더라고요",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006737.1.4",
          "form": "비맥스는 인터넷에 없죠?",
          "original_form": "비맥스는 인터넷에 없죠?",
          "speaker_id": "1",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100006737.1.5",
          "form": "네 근데 종로에서 사면 더 싼가봐요",
          "original_form": "네 근데 종로에서 사면 더 싼가봐요",
          "speaker_id": "2",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100006737.1.6",
          "form": "아 맞아요 그런 이야기 들었어요",
          "original_form": "아 맞아요 그런 이야기 들었어요",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100006737.1.7",
          "form": "근데 약도 약국마다 가격이 다르니 너무 어려운 것 같아요ㅋㅋ",
          "original_form": "근데 약도 약국마다 가격이 다르니 너무 어려운 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006737.1.8",
          "form": "맞아요 같은 제품도 각기 다르더라구요",
          "original_form": "맞아요 같은 제품도 각기 다르더라구요",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100006737.1.9",
          "form": "다 균일가면 편할텐데요ㅠㅠ",
          "original_form": "다 균일가면 편할텐데요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100006737.1.10",
          "form": "그러게요 가격비교할필요도없이요",
          "original_form": "그러게요 가격비교할필요도없이요",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100006737.1.11",
          "form": "그러니까요. 괜히 비교하느라 힘든데ㅜㅜ",
          "original_form": "그러니까요. 괜히 비교하느라 힘든데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006737.1.12",
          "form": "기억애 남는 선물 있나요",
          "original_form": "기억애 남는 선물 있나요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006737.1.13",
          "form": "음 향수요",
          "original_form": "음 향수요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006737.1.14",
          "form": "아 좋아하는 향이셨나요",
          "original_form": "아 좋아하는 향이셨나요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006737.1.15",
          "form": "네 꽃향이었는데 자주 손이 가더라고요",
          "original_form": "네 꽃향이었는데 자주 손이 가더라고요",
