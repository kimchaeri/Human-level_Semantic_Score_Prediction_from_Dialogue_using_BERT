      "utterance": [
        {
          "id": "MDRW2100022212.1.1",
          "form": "오늘 아침은 드셨어요?",
          "original_form": "오늘 아침은 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100022212.1.2",
          "form": "아직이요 뭐 드셨어요?",
          "original_form": "아직이요 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022212.1.3",
          "form": "저 아직 아침 못먹었어요 ㅜㅜ",
          "original_form": "저 아직 아침 못먹었어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022212.1.4",
          "form": "그래서 건너뛰고 점심 때 밥 먹으려구요",
          "original_form": "그래서 건너뛰고 점심 때 밥 먹으려구요",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022212.1.5",
          "form": "그렇군요ㅠㅠ 저도 그럴것같아요",
          "original_form": "그렇군요ㅠㅠ 저도 그럴것같아요",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022212.1.6",
          "form": "배달은 질리고 해먹는건 귀찮고 밥을 포기...",
          "original_form": "배달은 질리고 해먹는건 귀찮고 밥을 포기...",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022212.1.7",
          "form": "점심 뭐 먹을지 생각해놨어요?",
          "original_form": "점심 뭐 먹을지 생각해놨어요?",
          "speaker_id": "1",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022212.1.8",
          "form": "아니요 ㅋㅋㅋ아무생각없네용 뭐드실건가요",
          "original_form": "아니요 ㅋㅋㅋ아무생각없네용 뭐드실건가요",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022212.1.9",
          "form": "저 참치뽈찜 먹으려고 어제 해동시켜놨어요 ㅎㅎ",
          "original_form": "저 참치뽈찜 먹으려고 어제 해동시켜놨어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100022212.1.10",
          "form": "참치뽈찜첨 들어요 신기해용",
          "original_form": "참치뽈찜첨 들어요 신기해용",
          "speaker_id": "2",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100022212.1.11",
          "form": "저도 엊그제 처음 들어봤어요..",
          "original_form": "저도 엊그제 처음 들어봤어요..",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022212.1.12",
          "form": "방금 먹어봤는데 이거 맛있네요!",
          "original_form": "방금 먹어봤는데 이거 맛있네요!",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022212.1.13",
          "form": "시키신거에요?",
          "original_form": "시키신거에요?",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022212.1.14",
          "form": "네네 배달은 아니고 택배로요 ㅎㅎ",
          "original_form": "네네 배달은 아니고 택배로요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100022212.1.15",
          "form": "아 택배로 받으신거구너",
          "original_form": "아 택배로 받으신거구너",
          "speaker_id": "2",
          "time": "20210830 07:45"
        },
        {
          "id": "MDRW2100022212.1.16",
          "form": "벌써아침이네요ㅠ아침드셨어요?",
          "original_form": "벌써아침이네요ㅠ아침드셨어요?",
          "speaker_id": "2",
          "time": "20210830 07:46"
        },
        {
          "id": "MDRW2100022212.1.17",
          "form": "어우 출근하느라 아침도 못먹었네요 ㅎㅎ",
          "original_form": "어우 출근하느라 아침도 못먹었네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100022212.1.18",
          "form": "방금 저녁 먹고 오는 길이에요~",
          "original_form": "방금 저녁 먹고 오는 길이에요~",
          "speaker_id": "1",
          "time": "20210830 19:16"
        }
      ]
