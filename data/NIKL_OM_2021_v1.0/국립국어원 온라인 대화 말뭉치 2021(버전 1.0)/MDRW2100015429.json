      "utterance": [
        {
          "id": "MDRW2100015429.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015429.1.2",
          "form": "안녕하세요 스포츠좋아하세요?",
          "original_form": "안녕하세요 스포츠좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015429.1.3",
          "form": "배구 좋아해요",
          "original_form": "배구 좋아해요",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015429.1.4",
          "form": "저도 이번 올림픽계기로 배구 좋아하게 되었는더",
          "original_form": "저도 이번 올림픽계기로 배구 좋아하게 되었는더",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015429.1.5",
          "form": "반갑네용",
          "original_form": "반갑네용",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015429.1.6",
          "form": "저랑 계기가 같네요",
          "original_form": "저랑 계기가 같네요",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015429.1.7",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015429.1.8",
          "form": "그럼 혹시 김연경님 팬이 되기도?",
          "original_form": "그럼 혹시 김연경님 팬이 되기도?",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015429.1.9",
          "form": "당근이죠",
          "original_form": "당근이죠",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015429.1.10",
          "form": "ㅋㅋㅋ한국인의 대동단결ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ한국인의 대동단결ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015429.1.11",
          "form": "연경선수는 없겠지만 배구직관가보고 싶어요",
          "original_form": "연경선수는 없겠지만 배구직관가보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015429.1.12",
          "form": "저도 직관 가고싶네요",
          "original_form": "저도 직관 가고싶네요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015429.1.13",
          "form": "그쵸 재밌을거 같아요",
          "original_form": "그쵸 재밌을거 같아요",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015429.1.14",
          "form": "응원하는 팀 있으세요?",
          "original_form": "응원하는 팀 있으세요?",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015429.1.15",
          "form": "아직이요!",
          "original_form": "아직이요!",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015429.1.16",
          "form": "저도 아직이에요 혹시 다른 좋아하는 스포츠있어요?",
          "original_form": "저도 아직이에요 혹시 다른 좋아하는 스포츠있어요?",
          "speaker_id": "2",
