      "utterance": [
        {
          "id": "MDRW2100018119.1.1",
          "form": "최근에 어떤 책 읽으셨나요?",
          "original_form": "최근에 어떤 책 읽으셨나요?",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018119.1.2",
          "form": "280일이요",
          "original_form": "280일이요",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018119.1.3",
          "form": "소설인가요? 어떤 내용이에요?",
          "original_form": "소설인가요? 어떤 내용이에요?",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018119.1.4",
          "form": "책소개는",
          "original_form": "책소개는",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018119.1.5",
          "form": "아무도말해주지않은 임신의 힘든점",
          "original_form": "아무도말해주지않은 임신의 힘든점",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018119.1.6",
          "form": "이래서 호기심에 읽었는데",
          "original_form": "이래서 호기심에 읽었는데",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018119.1.7",
          "form": "사회비판적인 내용이라 읽기 불편해서",
          "original_form": "사회비판적인 내용이라 읽기 불편해서",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018119.1.8",
          "form": "읽다말았어요",
          "original_form": "읽다말았어요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018119.1.9",
          "form": "아 280일이 임신기간 얘기하는 거였군요",
          "original_form": "아 280일이 임신기간 얘기하는 거였군요",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018119.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018119.1.11",
          "form": "찾아보니 소설이네요",
          "original_form": "찾아보니 소설이네요",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018119.1.12",
          "form": "영 불편해서 참고 읽어야하나 \n덮어야하나 고민하다가 안읽기로했어요",
          "original_form": "영 불편해서 참고 읽어야하나 \n덮어야하나 고민하다가 안읽기로했어요",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018119.1.13",
          "form": "뭐 꼭 끝까지 다 볼 필요는 없으니까요",
          "original_form": "뭐 꼭 끝까지 다 볼 필요는 없으니까요",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018119.1.14",
          "form": "그렇죠?? 다른분들은 참고 읽으시나 궁금했어요",
          "original_form": "그렇죠?? 다른분들은 참고 읽으시나 궁금했어요",
          "speaker_id": "2",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018119.1.15",
          "form": "아니다 싶은 건 빠르게 그만두는 것도 좋더라구요 ㅋㅋ",
          "original_form": "아니다 싶은 건 빠르게 그만두는 것도 좋더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018119.1.16",
          "form": "그래야겠어요. ㅋㅋ 혹시 추천해주실만한 책 있으신가요",
          "original_form": "그래야겠어요. ㅋㅋ 혹시 추천해주실만한 책 있으신가요",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018119.1.17",
          "form": "저는 정재승의 열두발자국 읽기 시작했는데 볼만한 것 같아요",
