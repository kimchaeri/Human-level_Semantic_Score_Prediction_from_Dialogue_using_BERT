      "utterance": [
        {
          "id": "MDRW2100033150.1.1",
          "form": "온라인 커뮤니티는 내 전문이지",
          "original_form": "온라인 커뮤니티는 내 전문이지",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100033150.1.2",
          "form": "온라인 커뮤니티에서 만난 친구를 직접 만나보시적이있나요?",
          "original_form": "온라인 커뮤니티에서 만난 친구를 직접 만나보시적이있나요?",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100033150.1.3",
          "form": "물론 너도 전문이지",
          "original_form": "물론 너도 전문이지",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100033150.1.4",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100033150.1.5",
          "form": "아니라고 ㅋㅋ",
          "original_form": "아니라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100033150.1.6",
          "form": "온라인 친구...? 만난 적 없음",
          "original_form": "온라인 친구...? 만난 적 없음",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100033150.1.7",
          "form": "가족을 만난 적은 있지",
          "original_form": "가족을 만난 적은 있지",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100033150.1.8",
          "form": "아.. 커뮤니티하는 사람은 가족이다?",
          "original_form": "아.. 커뮤니티하는 사람은 가족이다?",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100033150.1.9",
          "form": "인정이지",
          "original_form": "인정이지",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100033150.1.10",
          "form": "그들은...온라인 따위의 친구가 아니야...!",
          "original_form": "그들은...온라인 따위의 친구가 아니야...!",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100033150.1.11",
          "form": "그치.. 컴퓨터를 사이로 만나는 일종의 헤어진... 이산가족",
          "original_form": "그치.. 컴퓨터를 사이로 만나는 일종의 헤어진... 이산가족",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100033150.1.12",
          "form": "나 왤케 온라인 커뮤니티 많이 하는 사람처럼 말하냐",
          "original_form": "나 왤케 온라인 커뮤니티 많이 하는 사람처럼 말하냐",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100033150.1.13",
          "form": "나 이제 트위터 줄이고 갓생 살려고",
          "original_form": "나 이제 트위터 줄이고 갓생 살려고",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100033150.1.14",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.15",
          "form": "트위터 탈퇴하겠습니다",
          "original_form": "트위터 탈퇴하겠습니다",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.16",
          "form": "나진짜이제 탈퇴함",
          "original_form": "나진짜이제 탈퇴함",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.17",
          "form": "사이좋게계폭하는거야",
          "original_form": "사이좋게계폭하는거야",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.18",
          "form": "나도임 ㅋ",
          "original_form": "나도임 ㅋ",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.19",
          "form": "하나",
          "original_form": "하나",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.20",
          "form": "둘",
          "original_form": "둘",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.21",
          "form": "셋",
          "original_form": "셋",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.22",
          "form": "왜안하는데",
          "original_form": "왜안하는데",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.23",
          "form": "나 이거 끝나규 바로 탈퇴할거임",
          "original_form": "나 이거 끝나규 바로 탈퇴할거임",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.24",
          "form": "가족들이기다려 학생",
          "original_form": "가족들이기다려 학생",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.25",
          "form": "블락하고 탈퇴하는 척 하기",
          "original_form": "블락하고 탈퇴하는 척 하기",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033150.1.26",
          "form": "고도의 블락기술",
          "original_form": "고도의 블락기술",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100033150.1.27",
          "form": "갓반인돼야지 탈출하자 온라인 중독",
          "original_form": "갓반인돼야지 탈출하자 온라인 중독",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100033150.1.28",
          "form": "근데 이제 이걸 트위터에 쓰고",
          "original_form": "근데 이제 이걸 트위터에 쓰고",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100033150.1.29",
