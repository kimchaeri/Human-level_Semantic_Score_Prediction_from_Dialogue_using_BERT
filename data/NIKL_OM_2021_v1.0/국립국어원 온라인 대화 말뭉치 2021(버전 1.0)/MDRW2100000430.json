      "utterance": [
        {
          "id": "MDRW2100000430.1.1",
          "form": "어제 name2님 지역도 비왔나요??",
          "original_form": "어제 &name2&님 지역도 비왔나요??",
          "speaker_id": "1",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000430.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000430.1.3",
          "form": "아침부터 천둥번개",
          "original_form": "아침부터 천둥번개",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000430.1.4",
          "form": "우르릉쾅",
          "original_form": "우르릉쾅",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000430.1.5",
          "form": "출근 전에 멍멍이\r\n산책",
          "original_form": "출근 전에 멍멍이\r\n산책",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000430.1.6",
          "form": "시켜야 했는데",
          "original_form": "시켜야 했는데",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000430.1.7",
          "form": "비오면 자기도 못 나가는 거 알고 우울해해요",
          "original_form": "비오면 자기도 못 나가는 거 알고 우울해해요",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000430.1.8",
          "form": "name1님은 수해 입은 지역이라든가 그런 곳인가요 혹시",
          "original_form": "&name1&님은 수해 입은 지역이라든가 그런 곳인가요 혹시",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000430.1.9",
          "form": "다행하게도 아니긴하지만 ㅠㅠㅠㅠ은근 지방쪽이 수해가 심했었더라구요",
          "original_form": "다행하게도 아니긴하지만 ㅠㅠㅠㅠ은근 지방쪽이 수해가 심했었더라구요",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000430.1.10",
          "form": "날씨문제로 그러는거 몰랐었어요ㅠㅠ",
          "original_form": "날씨문제로 그러는거 몰랐었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000430.1.11",
          "form": "그니까요 ㅠ",
          "original_form": "그니까요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000430.1.12",
          "form": "전지구적으로 홍수 많이 나는 거 보니",
          "original_form": "전지구적으로 홍수 많이 나는 거 보니",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000430.1.13",
          "form": "언제 내 차례가 되어도 모르겠더라고요",
          "original_form": "언제 내 차례가 되어도 모르겠더라고요",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000430.1.14",
          "form": "아ㅠㅠ이번에 그 도쿄올림픽에 어떤나라 선수",
          "original_form": "아ㅠㅠ이번에 그 도쿄올림픽에 어떤나라 선수",
          "speaker_id": "1",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000430.1.15",
          "form": "자기 나라가 침수위기라",
          "original_form": "자기 나라가 침수위기라",
          "speaker_id": "1",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000430.1.16",
          "form": "알리기위해 출전한거보고 울컥햌ㅅ어요",
          "original_form": "알리기위해 출전한거보고 울컥햌ㅅ어요",
          "speaker_id": "1",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000430.1.17",
          "form": "아 맞아요 ㅠㅜ",
          "original_form": "아 맞아요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000430.1.18",
          "form": "지구온난화로 기상악화가 진짜..넘나 무섭구",
          "original_form": "지구온난화로 기상악화가 진짜..넘나 무섭구",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000430.1.19",
          "form": "ㅠ 긍까요",
          "original_form": "ㅠ 긍까요",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000430.1.20",
          "form": "나중에 일본은 아예 침몰가능성도있다던데 ㄷㄷ",
          "original_form": "나중에 일본은 아예 침몰가능성도있다던데 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000430.1.21",
          "form": "ㄷㄷ",
          "original_form": "ㄷㄷ",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000430.1.22",
          "form": "오늘 아침에",
          "original_form": "오늘 아침에",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000430.1.23",
          "form": "인도네시안가 거긴 이미 진행 중이라며",
          "original_form": "인도네시안가 거긴 이미 진행 중이라며",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000430.1.24",
          "form": "그런 기사를 본 거 같은에",
          "original_form": "그런 기사를 본 거 같은에",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000430.1.25",
          "form": "헉....무섭다",
          "original_form": "헉....무섭다",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000430.1.26",
          "form": "우리나라도 삼면이 바다라",
          "original_form": "우리나라도 삼면이 바다라",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000430.1.27",
          "form": "걱정되네욥..",
          "original_form": "걱정되네욥..",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000430.1.28",
          "form": "산으로 올라가야 하나",
          "original_form": "산으로 올라가야 하나",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000430.1.29",
          "form": "늘 고민하게 되네요",
          "original_form": "늘 고민하게 되네요",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000430.1.30",
          "form": "층고 높은 곳에 살아야하나...",
          "original_form": "층고 높은 곳에 살아야하나...",
          "speaker_id": "1",
          "time": "20210819 16:23"
        }
      ]
