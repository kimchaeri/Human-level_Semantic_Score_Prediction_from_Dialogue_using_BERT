      "utterance": [
        {
          "id": "MDRW2100025197.1.1",
          "form": "스포츠 좋아하는 거 있으심가요?",
          "original_form": "스포츠 좋아하는 거 있으심가요?",
          "speaker_id": "1",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100025197.1.2",
          "form": "저는 주로 야구 봐요~ name2은 좋아하시는 스포츠 있으세요?",
          "original_form": "저는 주로 야구 봐요~ &name2&은 좋아하시는 스포츠 있으세요?",
          "speaker_id": "2",
          "time": "20210901 00:42"
        },
        {
          "id": "MDRW2100025197.1.3",
          "form": "음 저는 스포츠 잘은 안봤는데 저번 올림픽 때 보게 됐어요",
          "original_form": "음 저는 스포츠 잘은 안봤는데 저번 올림픽 때 보게 됐어요",
          "speaker_id": "1",
          "time": "20210901 00:44"
        },
        {
          "id": "MDRW2100025197.1.4",
          "form": "저는 배구가 재미있더라구요",
          "original_form": "저는 배구가 재미있더라구요",
          "speaker_id": "1",
          "time": "20210901 00:44"
        },
        {
          "id": "MDRW2100025197.1.5",
          "form": "저도 올림픽 때 배구의 매력에 빠졌어요",
          "original_form": "저도 올림픽 때 배구의 매력에 빠졌어요",
          "speaker_id": "2",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025197.1.6",
          "form": "코로나만 아니면 직관도 가고 싶은데 아쉬워요ㅠㅠ",
          "original_form": "코로나만 아니면 직관도 가고 싶은데 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 00:47"
        },
        {
          "id": "MDRW2100025197.1.7",
          "form": "그러니까요~ㅎㅎ",
          "original_form": "그러니까요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100025197.1.8",
          "form": "직관이 진짜 재밌을 것 같아요",
          "original_form": "직관이 진짜 재밌을 것 같아요",
          "speaker_id": "1",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100025197.1.9",
          "form": "저는 한 번도 스포츠 직관을 안가봐서요",
          "original_form": "저는 한 번도 스포츠 직관을 안가봐서요",
          "speaker_id": "1",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100025197.1.10",
          "form": "현장감도 느껴보고 싶네요",
          "original_form": "현장감도 느껴보고 싶네요",
          "speaker_id": "1",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100025197.1.11",
          "form": "직관에 빠지면 답도 없는 것 같아요ㅋㅋㅋ",
          "original_form": "직관에 빠지면 답도 없는 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100025197.1.12",
          "form": "야구가 특히 인기 많은 것 같아요",
          "original_form": "야구가 특히 인기 많은 것 같아요",
          "speaker_id": "1",
          "time": "20210901 00:53"
        },
        {
          "id": "MDRW2100025197.1.13",
          "form": "직관 스포츠로",
          "original_form": "직관 스포츠로",
          "speaker_id": "1",
          "time": "20210901 00:53"
        },
        {
          "id": "MDRW2100025197.1.14",
          "form": "그쳐 한국에서는 관중들도 제일 많구요ㅋㅋㅋ",
          "original_form": "그쳐 한국에서는 관중들도 제일 많구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:53"
        },
        {
          "id": "MDRW2100025197.1.15",
          "form": "직관 가면 열기가 장난 아니에요ㅋㅋㅋㅋ",
          "original_form": "직관 가면 열기가 장난 아니에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025197.1.16",
          "form": "치킨 먹는 거 재밌어 보여서 한 번 야구장 가보고 싶다는",
          "original_form": "치킨 먹는 거 재밌어 보여서 한 번 야구장 가보고 싶다는",
          "speaker_id": "1",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025197.1.17",
          "form": "막연한 생각이 있었는데",
          "original_form": "막연한 생각이 있었는데",
          "speaker_id": "1",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025197.1.18",
          "form": "아직 특별히 응원하는 팀은 없으신 거예요?",
          "original_form": "아직 특별히 응원하는 팀은 없으신 거예요?",
          "speaker_id": "2",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025197.1.19",
          "form": "코로나 때문에 생각만 해요..ㅋㅋ",
          "original_form": "코로나 때문에 생각만 해요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025197.1.20",
          "form": "음 아버지가",
          "original_form": "음 아버지가",
          "speaker_id": "1",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025197.1.21",
          "form": "한화를 자주 보세요",
          "original_form": "한화를 자주 보세요",
          "speaker_id": "1",
          "time": "20210901 00:56"
        },
        {
          "id": "MDRW2100025197.1.22",
          "form": "한화ㅠㅠㅠ 저도 연고지 때문에 한화 경기 자주 보는데..ㅎㅎ",
          "original_form": "한화ㅠㅠㅠ 저도 연고지 때문에 한화 경기 자주 보는데..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100025197.1.23",
          "form": "아버지께서 한화가 가장 경기가 재미있다고 하더라구요",
          "original_form": "아버지께서 한화가 가장 경기가 재미있다고 하더라구요",
          "speaker_id": "1",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100025197.1.24",
          "form": "그래도 오늘은 이겼답니다^^",
          "original_form": "그래도 오늘은 이겼답니다^^",
          "speaker_id": "2",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100025197.1.25",
          "form": "한화가 거의 꼴찌 아닌가여?",
          "original_form": "한화가 거의 꼴찌 아닌가여?",
          "speaker_id": "1",
