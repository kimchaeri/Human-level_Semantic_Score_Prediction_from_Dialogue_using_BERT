      "utterance": [
        {
          "id": "MDRW2100031013.1.1",
          "form": "아가들은 초등학생인가요?",
          "original_form": "아가들은 초등학생인가요?",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100031013.1.2",
          "form": "중학생과고등학생요",
          "original_form": "중학생과고등학생요",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100031013.1.3",
          "form": "앗 한창 공부할 나이네요",
          "original_form": "앗 한창 공부할 나이네요",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100031013.1.4",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031013.1.5",
          "form": "키크라고 산건데..",
          "original_form": "키크라고 산건데..",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031013.1.6",
          "form": "뭔가 남자아이들같은 느낌인데 맞나요?",
          "original_form": "뭔가 남자아이들같은 느낌인데 맞나요?",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031013.1.7",
          "form": "딸둘에아들하나요",
          "original_form": "딸둘에아들하나요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031013.1.8",
          "form": "고등학생되니 여자애가안커서",
          "original_form": "고등학생되니 여자애가안커서",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031013.1.9",
          "form": "아 본인도 고민이겠네요",
          "original_form": "아 본인도 고민이겠네요",
          "speaker_id": "1",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100031013.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031013.1.11",
          "form": "요즘애들 다크던데",
          "original_form": "요즘애들 다크던데",
          "speaker_id": "2",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031013.1.12",
          "form": "요즘애들 너무커서 옆에가기 싫어요",
          "original_form": "요즘애들 너무커서 옆에가기 싫어요",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031013.1.13",
          "form": "초딩때 잘먹일껄 후회돼요",
          "original_form": "초딩때 잘먹일껄 후회돼요",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100031013.1.14",
          "form": "초딩애들있으면 막먹이세요",
          "original_form": "초딩애들있으면 막먹이세요",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100031013.1.15",
          "form": "줄넘기시키고",
          "original_form": "줄넘기시키고",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100031013.1.16",
          "form": "많이 움직이는 애들이",
          "original_form": "많이 움직이는 애들이",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100031013.1.17",
          "form": "확실히 잘 크더라구요",
          "original_form": "확실히 잘 크더라구요",
          "speaker_id": "1",
          "time": "20210904 15:35"
        }
      ]
