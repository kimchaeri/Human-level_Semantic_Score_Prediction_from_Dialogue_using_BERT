      "utterance": [
        {
          "id": "MDRW2100036350.1.1",
          "form": "바 갔을때",
          "original_form": "바 갔을때",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100036350.1.2",
          "form": "아이스카빙하는거",
          "original_form": "아이스카빙하는거",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100036350.1.3",
          "form": "실제로 본적이 있는데",
          "original_form": "실제로 본적이 있는데",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100036350.1.4",
          "form": "그건뭐랴요?!",
          "original_form": "그건뭐랴요?!",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036350.1.5",
          "form": "진짜 마술? 묘기? 그런거 보는것같아요",
          "original_form": "진짜 마술? 묘기? 그런거 보는것같아요",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036350.1.6",
          "form": "그 바텐더님이",
          "original_form": "그 바텐더님이",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036350.1.7",
          "form": "그냥 막 얼음을",
          "original_form": "그냥 막 얼음을",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036350.1.8",
          "form": "오~~~",
          "original_form": "오~~~",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036350.1.9",
          "form": "잔에 들어갈크기의 큰",
          "original_form": "잔에 들어갈크기의 큰",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036350.1.10",
          "form": "구 형태로 얼음 조각하시는거에용",
          "original_form": "구 형태로 얼음 조각하시는거에용",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036350.1.11",
          "form": "조각칼 같은걸로 얼음을 샥샥샤샥 해요ㅋㅋㅋㅋ",
          "original_form": "조각칼 같은걸로 얼음을 샥샥샤샥 해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036350.1.12",
          "form": "와~~ 진짜 능력자네요",
          "original_form": "와~~ 진짜 능력자네요",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036350.1.13",
          "form": "빠르게해야할텐데!!! 기술짱 ㅋㅋ",
          "original_form": "빠르게해야할텐데!!! 기술짱 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100036350.1.14",
          "form": "이게 술에는 얼음을 보통 양주 아니고서야",
          "original_form": "이게 술에는 얼음을 보통 양주 아니고서야",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100036350.1.15",
          "form": "안넣어서 먹잖아용?",
          "original_form": "안넣어서 먹잖아용?",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100036350.1.16",
          "form": "그쵸 !? 양주나 보드카",
          "original_form": "그쵸 !? 양주나 보드카",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100036350.1.17",
          "form": "근데 양주 마실때는 얼음이 빨리녹는데",
          "original_form": "근데 양주 마실때는 얼음이 빨리녹는데",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100036350.1.18",
          "form": "이건 면적이 적어서",
          "original_form": "이건 면적이 적어서",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100036350.1.19",
          "form": "정~말 천천히 녹아서",
          "original_form": "정~말 천천히 녹아서",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100036350.1.20",
          "form": "술의 맛을 안해치더라구요",
          "original_form": "술의 맛을 안해치더라구요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100036350.1.21",
          "form": "오! 아이디아가좋앗네요",
          "original_form": "오! 아이디아가좋앗네요",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036350.1.22",
          "form": "맞아요 요샌 왕얼음틀로",
          "original_form": "맞아요 요샌 왕얼음틀로",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036350.1.23",
          "form": "큰구슬얼음만들어서 여름에 아아마셧느데 ㅋㅋㅋ",
          "original_form": "큰구슬얼음만들어서 여름에 아아마셧느데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036350.1.24",
          "form": "나중에 바 가시면 한번 부탁드려보세용ㅎㅎㅎ 서비스로 해주시는것같아요",
          "original_form": "나중에 바 가시면 한번 부탁드려보세용ㅎㅎㅎ 서비스로 해주시는것같아요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036350.1.25",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036350.1.26",
          "form": "오~~~!!!",
          "original_form": "오~~~!!!",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036350.1.27",
          "form": "요즘 lg전자 냉장고에서",
          "original_form": "요즘 lg전자 냉장고에서",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036350.1.28",
          "form": "기억해볼께요!!",
          "original_form": "기억해볼께요!!",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036350.1.29",
          "form": "이런 얼음 나오는 얼음정수기냉장고 내놨더라구요",
          "original_form": "이런 얼음 나오는 얼음정수기냉장고 내놨더라구요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036350.1.30",
          "form": "양주 좋아하는분들은 눈이 번쩍 뜨일ㅋㅋㅋㅋ",
          "original_form": "양주 좋아하는분들은 눈이 번쩍 뜨일ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036350.1.31",
          "form": "왕얼음이 나와요!?!",
          "original_form": "왕얼음이 나와요!?!",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036350.1.32",
          "form": "네네!",
          "original_form": "네네!",
