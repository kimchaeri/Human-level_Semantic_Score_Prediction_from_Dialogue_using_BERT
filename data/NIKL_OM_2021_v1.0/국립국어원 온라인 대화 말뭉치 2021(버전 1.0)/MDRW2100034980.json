      "utterance": [
        {
          "id": "MDRW2100034980.1.1",
          "form": "그거 아직 나와요??",
          "original_form": "그거 아직 나와요??",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034980.1.2",
          "form": "ㅋㅋㅋ 긍가봐요",
          "original_form": "ㅋㅋㅋ 긍가봐요",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034980.1.3",
          "form": "꼬꼬면은 봣는데 나가사키짬뽕은 못봣어요 ㅋㅋㅋㅋㅋ",
          "original_form": "꼬꼬면은 봣는데 나가사키짬뽕은 못봣어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034980.1.4",
          "form": "꼬꼬면도 같이 있었는데 암튼 맛있었어요",
          "original_form": "꼬꼬면도 같이 있었는데 암튼 맛있었어요",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100034980.1.5",
          "form": "시원한국물 라면 매력적이에요",
          "original_form": "시원한국물 라면 매력적이에요",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100034980.1.6",
          "form": "맞아요 깔끔 칼칼 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 깔끔 칼칼 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100034980.1.7",
          "form": "저는 그 참깨라면도 좋아요",
          "original_form": "저는 그 참깨라면도 좋아요",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100034980.1.8",
          "form": "참깨라면도 좋아해요 고소해서 ㅋㅋㅋㅋㅋ",
          "original_form": "참깨라면도 좋아해요 고소해서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034980.1.9",
          "form": "네 고소한매력도 좋더라구요",
          "original_form": "네 고소한매력도 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100034980.1.10",
          "form": "맞아요 참깨라면이 끌릴 때가 잇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 참깨라면이 끌릴 때가 잇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034980.1.11",
          "form": "얼큰한것도 많은데 고소 시원한것도 가끔 땡기죠",
          "original_form": "얼큰한것도 많은데 고소 시원한것도 가끔 땡기죠",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034980.1.12",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100034980.1.13",
          "form": "아 불닭볶음면도 좋아해요",
          "original_form": "아 불닭볶음면도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100034980.1.14",
          "form": "아좋죠",
          "original_form": "아좋죠",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100034980.1.15",
          "form": "첨에 나왔을때 인기가 대단했어요",
          "original_form": "첨에 나왔을때 인기가 대단했어요",
