      "utterance": [
        {
          "id": "MDRW2100013451.1.1",
          "form": "즐겨보는 책이 있나요",
          "original_form": "즐겨보는 책이 있나요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013451.1.2",
          "form": "즐겨보진 않지만 강아지 관련 책을봐요",
          "original_form": "즐겨보진 않지만 강아지 관련 책을봐요",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013451.1.3",
          "form": "강아지 키우시나봐요",
          "original_form": "강아지 키우시나봐요",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013451.1.4",
          "form": "주로 어떤거에 관련된 책을 보시나요",
          "original_form": "주로 어떤거에 관련된 책을 보시나요",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013451.1.5",
          "form": "애견 건강같은거 찾아보시는건가요",
          "original_form": "애견 건강같은거 찾아보시는건가요",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013451.1.6",
          "form": "교육과 훈련이요",
          "original_form": "교육과 훈련이요",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013451.1.7",
          "form": "아 교육과 훈련이구나",
          "original_form": "아 교육과 훈련이구나",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013451.1.8",
          "form": "저도 허스키 한마리 키우고있어서 책 추천해주실ㅈ수 있나요",
          "original_form": "저도 허스키 한마리 키우고있어서 책 추천해주실ㅈ수 있나요",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013451.1.9",
          "form": "요즘 보는건",
          "original_form": "요즘 보는건",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013451.1.10",
          "form": "냊강아지 스트레스없이 행복한 75가지 놀이 방법이요",
          "original_form": "냊강아지 스트레스없이 행복한 75가지 놀이 방법이요",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013451.1.11",
          "form": "저에게 꼭 필요한 책인거같아요",
          "original_form": "저에게 꼭 필요한 책인거같아요",
          "speaker_id": "2",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013451.1.12",
          "form": "꼭 읽어보세요",
          "original_form": "꼭 읽어보세요",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013451.1.13",
          "form": "그 안에서 가장 기억에 남는 놀이법은 무엇이셨나요",
          "original_form": "그 안에서 가장 기억에 남는 놀이법은 무엇이셨나요",
          "speaker_id": "2",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013451.1.14",
          "form": "집에서 하는 놀이들 이요",
          "original_form": "집에서 하는 놀이들 이요",
          "speaker_id": "1",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100013451.1.15",
          "form": "코로나와 날씨때문에 자주 못나가니",
          "original_form": "코로나와 날씨때문에 자주 못나가니",
          "speaker_id": "1",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100013451.1.16",
          "form": "도움됩니다",
          "original_form": "도움됩니다",
          "speaker_id": "1",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100013451.1.17",
          "form": "꼭 한 번 구매해서 읽어봐야겠어요",
          "original_form": "꼭 한 번 구매해서 읽어봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100013451.1.18",
          "form": "75가지나 된다니 진짜 유용할거같습니다.",
          "original_form": "75가지나 된다니 진짜 유용할거같습니다.",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100013451.1.19",
          "form": "혹시 다른 거 하나 더 추천해주실 수 있으십니까",
          "original_form": "혹시 다른 거 하나 더 추천해주실 수 있으십니까",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013451.1.20",
          "form": "훈련이 잘못됐습니다 저자 알렉스",
          "original_form": "훈련이 잘못됐습니다 저자 알렉스",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013451.1.21",
          "form": "그 책도 너무 좋아보입니다",
          "original_form": "그 책도 너무 좋아보입니다",
          "speaker_id": "2",
