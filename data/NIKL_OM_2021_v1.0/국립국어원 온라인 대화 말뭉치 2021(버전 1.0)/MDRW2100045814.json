      "utterance": [
        {
          "id": "MDRW2100045814.1.1",
          "form": "안녕하세요, 떡볶이 좋아하시나요?",
          "original_form": "안녕하세요, 떡볶이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045814.1.2",
          "form": "떡볶이좋아해오ㅡ",
          "original_form": "떡볶이좋아해오ㅡ",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100045814.1.3",
          "form": "어떤 떡볶이 좋아하세요?",
          "original_form": "어떤 떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100045814.1.4",
          "form": "저는 로제떡볶이 좋아해요",
          "original_form": "저는 로제떡볶이 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100045814.1.5",
          "form": "로제 맛있죠! 저는 즉떡 좋아해요",
          "original_form": "로제 맛있죠! 저는 즉떡 좋아해요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100045814.1.6",
          "form": "저도 즉떡 좋아해요! 신당동떡볶이",
          "original_form": "저도 즉떡 좋아해요! 신당동떡볶이",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100045814.1.7",
          "form": "오 신당동 떡볶이 맛있죠! 즉떡은 볶음밥도 진짜 맛있잖아요ㅎㅎ",
          "original_form": "오 신당동 떡볶이 맛있죠! 즉떡은 볶음밥도 진짜 맛있잖아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100045814.1.8",
          "form": "맞아요 맞아요! 사리추가하는재미도있어요",
          "original_form": "맞아요 맞아요! 사리추가하는재미도있어요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100045814.1.9",
          "form": "맞아요! 당면 너무 맛있죠",
          "original_form": "맞아요! 당면 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100045814.1.10",
          "form": "전 쫄면사리나 만두 꼭 추가해요 ㅎㅎㅎ",
          "original_form": "전 쫄면사리나 만두 꼭 추가해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100045814.1.11",
          "form": "만두 너무 맛있죠ㅋㅋㅋ바삭한데 양념이랑 해서 먹으면 또 다른 느낌이구ㅜㅜ",
          "original_form": "만두 너무 맛있죠ㅋㅋㅋ바삭한데 양념이랑 해서 먹으면 또 다른 느낌이구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100045814.1.12",
          "form": "맞아요 ㅠㅠㅠ 아 점심시간멀었는데 배고프네요",
          "original_form": "맞아요 ㅠㅠㅠ 아 점심시간멀었는데 배고프네요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100045814.1.13",
          "form": "그니까요ㅠㅠㅠ 점심에 뭐 드실 거예요??",
          "original_form": "그니까요ㅠㅠㅠ 점심에 뭐 드실 거예요??",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100045814.1.14",
          "form": "점심에 순두부찌개먹을까해요",
          "original_form": "점심에 순두부찌개먹을까해요",
          "speaker_id": "2",
          "time": "20210908 10:50"
        }
      ]
