      "utterance": [
        {
          "id": "MDRW2100015921.1.1",
          "form": "요즘 쇼핑하신거 있으세요?",
          "original_form": "요즘 쇼핑하신거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015921.1.2",
          "form": "네 최근에 모자하나샀어요 ㅋㅋㅋ",
          "original_form": "네 최근에 모자하나샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100015921.1.3",
          "form": "저도 오늘 h패션몰에서 적립금 털고 옷이나골라봐야겠어요",
          "original_form": "저도 오늘 h패션몰에서 적립금 털고 옷이나골라봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100015921.1.4",
          "form": "아 거기 저도 이용중이에요 ㅋㅋㅋㅋ적립금 많이주더라구요",
          "original_form": "아 거기 저도 이용중이에요 ㅋㅋㅋㅋ적립금 많이주더라구요",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100015921.1.5",
          "form": "네 만근해서 2만포줬는데",
          "original_form": "네 만근해서 2만포줬는데",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100015921.1.6",
          "form": "룰렛이 안도와주네요",
          "original_form": "룰렛이 안도와주네요",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100015921.1.7",
          "form": "아 맞아요 ㅠㅠ 저는 500포인트 나왓어요 ㅎ..",
          "original_form": "아 맞아요 ㅠㅠ 저는 500포인트 나왓어요 ㅎ..",
          "speaker_id": "2",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100015921.1.8",
          "form": "전아예쿠폰이요ㅎ",
          "original_form": "전아예쿠폰이요ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100015921.1.9",
          "form": "실망스러운ㅎ",
          "original_form": "실망스러운ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100015921.1.10",
          "form": "그쵸 ㅠㅠ 저는 거기서 타미 모자하나삿어요!",
          "original_form": "그쵸 ㅠㅠ 저는 거기서 타미 모자하나삿어요!",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100015921.1.11",
          "form": "엄마꺼로 주말에 dkny후드집업하나샀는데",
          "original_form": "엄마꺼로 주말에 dkny후드집업하나샀는데",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100015921.1.12",
          "form": "괜찮더라구요",
          "original_form": "괜찮더라구요",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100015921.1.13",
          "form": "오오 저도 참고해볼게요",
          "original_form": "오오 저도 참고해볼게요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100015921.1.14",
          "form": "적립금 마저 털어야되서! 양말이나 살까했고든요",
          "original_form": "적립금 마저 털어야되서! 양말이나 살까했고든요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100015921.1.15",
          "form": "비싸요ㅎ h포인트 탈탈털고사서ㅋ",
          "original_form": "비싸요ㅎ h포인트 탈탈털고사서ㅋ",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100015921.1.16",
          "form": "그냥적립금만으로 실결제가 많으니",
          "original_form": "그냥적립금만으로 실결제가 많으니",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100015921.1.17",
          "form": "티셔츠쪽으로",
          "original_form": "티셔츠쪽으로",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100015921.1.18",
          "form": "그렇군요 지금 적립금 만얼마 남아서 정말 애매해요 ㅋㅋㅋ",
          "original_form": "그렇군요 지금 적립금 만얼마 남아서 정말 애매해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100015921.1.19",
          "form": "다들 양말사시던데요",
          "original_form": "다들 양말사시던데요",
          "speaker_id": "1",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100015921.1.20",
          "form": "네네 ㅋㅋㅋㅋ 고려중이에요",
          "original_form": "네네 ㅋㅋㅋㅋ 고려중이에요",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100015921.1.21",
          "form": "포인트 오늘소멸이죠?",
          "original_form": "포인트 오늘소멸이죠?",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100015921.1.22",
          "form": "네 오늘까지!",
          "original_form": "네 오늘까지!",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100015921.1.23",
          "form": "예쁜거 품절 될거같은데 얼른사세용",
          "original_form": "예쁜거 품절 될거같은데 얼른사세용",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100015921.1.24",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "1",
          "time": "20210826 11:06"
        }
      ]
    }
