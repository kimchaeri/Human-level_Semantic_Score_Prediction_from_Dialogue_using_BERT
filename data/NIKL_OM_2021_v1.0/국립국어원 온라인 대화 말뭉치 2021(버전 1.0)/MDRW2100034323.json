      "utterance": [
        {
          "id": "MDRW2100034323.1.1",
          "form": "운동 주 몇회 가세요?!",
          "original_form": "운동 주 몇회 가세요?!",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100034323.1.2",
          "form": "저 주2회로 등록했어요!",
          "original_form": "저 주2회로 등록했어요!",
          "speaker_id": "1",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100034323.1.3",
          "form": "그리고 등산도 좀 자주 가려구요",
          "original_form": "그리고 등산도 좀 자주 가려구요",
          "speaker_id": "1",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100034323.1.4",
          "form": "주식이 아니라 건강에 돈을 투자해야 할 것 같다는 생각이..ㅋㅋㅋㅋ",
          "original_form": "주식이 아니라 건강에 돈을 투자해야 할 것 같다는 생각이..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100034323.1.5",
          "form": "ㅋㅋㅋㅋㅋ오 등산 좋죠",
          "original_form": "ㅋㅋㅋㅋㅋ오 등산 좋죠",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100034323.1.6",
          "form": "등산 저도 가고싶네요",
          "original_form": "등산 저도 가고싶네요",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100034323.1.7",
          "form": "등산 좋아하시나요",
          "original_form": "등산 좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100034323.1.8",
          "form": "얼마전에 저는 한라산에 다녀왔어요",
          "original_form": "얼마전에 저는 한라산에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100034323.1.9",
          "form": "네 예전에는 좀 갔어요",
          "original_form": "네 예전에는 좀 갔어요",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100034323.1.10",
          "form": "너무 좋죠?",
          "original_form": "너무 좋죠?",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100034323.1.11",
          "form": "네 뿌듯하고 운동되고",
          "original_form": "네 뿌듯하고 운동되고",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100034323.1.12",
          "form": "근데 예전에 등산갓다가 무릎나갓어서 좀 조심해요 ㅋㅋ",
          "original_form": "근데 예전에 등산갓다가 무릎나갓어서 좀 조심해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100034323.1.13",
          "form": "무릎보호대 착용하고",
          "original_form": "무릎보호대 착용하고",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100034323.1.14",
          "form": "가셔야해요 ㅠㅠ",
          "original_form": "가셔야해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100034323.1.15",
          "form": "저 테이핑 요새 엄청 열심히해요 ㅋㅋㅋㅋ",
          "original_form": "저 테이핑 요새 엄청 열심히해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100034323.1.16",
          "form": "ㅋㅋㅋㅋㅋ 꼭 하셔야해요",
          "original_form": "ㅋㅋㅋㅋㅋ 꼭 하셔야해요",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100034323.1.17",
          "form": "근데 하면 다름을 느끼나요?!",
          "original_form": "근데 하면 다름을 느끼나요?!",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100034323.1.18",
          "form": "테이핑은 좀 덜 다치는 것 같긴 햇어요! 기분탓일수도잇는데 ㅋㅋㅋ",
          "original_form": "테이핑은 좀 덜 다치는 것 같긴 햇어요! 기분탓일수도잇는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100034323.1.19",
          "form": "ㅋㅋㅋㅋ그럼 다행니네녀!!",
          "original_form": "ㅋㅋㅋㅋ그럼 다행니네녀!!",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100034323.1.20",
          "form": "보호대도 좋을거같은데 여름에 너무 더워요",
          "original_form": "보호대도 좋을거같은데 여름에 너무 더워요",
          "speaker_id": "1",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100034323.1.21",
          "form": "맞아요 덥죠 ㅠㅠㅠㅠ",
          "original_form": "맞아요 덥죠 ㅠㅠㅠㅠ",
