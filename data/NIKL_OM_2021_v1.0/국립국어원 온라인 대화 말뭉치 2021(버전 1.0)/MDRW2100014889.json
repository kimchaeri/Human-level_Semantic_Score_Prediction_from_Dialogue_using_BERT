      "utterance": [
        {
          "id": "MDRW2100014889.1.1",
          "form": "name1님은 먹는 단밤을 좋아하세요?",
          "original_form": "&name1&님은 먹는 단밤을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014889.1.2",
          "form": "음 아뇨 ㅋㅋ",
          "original_form": "음 아뇨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014889.1.3",
          "form": "이태원 클라쓰~",
          "original_form": "이태원 클라쓰~",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014889.1.4",
          "form": "앜ㅋㅋ 죄송해요 제가 그 드라마를 못봐서 몰랐네여 ㅜㅠ",
          "original_form": "앜ㅋㅋ 죄송해요 제가 그 드라마를 못봐서 몰랐네여 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014889.1.5",
          "form": "맛있는 음식 좋아하세요?",
          "original_form": "맛있는 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014889.1.6",
          "form": "ㅎㅎ 괘아나여",
          "original_form": "ㅎㅎ 괘아나여",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014889.1.7",
          "form": "네 좋아하죠",
          "original_form": "네 좋아하죠",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014889.1.8",
          "form": "제일 좋아하는 음식이 뭐예요?",
          "original_form": "제일 좋아하는 음식이 뭐예요?",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014889.1.9",
          "form": "곱창 막창 &  해산물 요리요 ㅎㅎ",
          "original_form": "곱창 막창 &  해산물 요리요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014889.1.10",
          "form": "다 맛있는 음식이네요!! 저도 다 좋아해요",
          "original_form": "다 맛있는 음식이네요!! 저도 다 좋아해요",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014889.1.11",
          "form": "해산물 요리는 어떤걸까요? 회? 해물찜?",
          "original_form": "해산물 요리는 어떤걸까요? 회? 해물찜?",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014889.1.12",
          "form": "찜요 ㅎㅎㅎ",
          "original_form": "찜요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014889.1.13",
          "form": "캬 ㅋㅋㅋ 가격만큼 맛도 있지요 저는 딱 한번 먹어봤는데 너무 맛있었어요",
          "original_form": "캬 ㅋㅋㅋ 가격만큼 맛도 있지요 저는 딱 한번 먹어봤는데 너무 맛있었어요",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014889.1.14",
          "form": "그럼 술도 곁들이나요?",
          "original_form": "그럼 술도 곁들이나요?",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014889.1.15",
          "form": "아 노노놉 술을 못해요",
          "original_form": "아 노노놉 술을 못해요",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014889.1.16",
          "form": "그렇군요 술은 꼭 필요한 건 아니라고 생각해요 ㅎㅎ",
          "original_form": "그렇군요 술은 꼭 필요한 건 아니라고 생각해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014889.1.17",
          "form": "저도 잘 못하는 편이라",
          "original_form": "저도 잘 못하는 편이라",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014889.1.18",
          "form": "그러면 커피나 차 같은 음료는 좋아하세요?",
          "original_form": "그러면 커피나 차 같은 음료는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014889.1.19",
          "form": "아아 좋아해요 ㅎㅎ",
          "original_form": "아아 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014889.1.20",
          "form": "아아 좋죠 ㅋㅋㅋㅋ 저도 식사후에 마시면 입안이 깔끔해지는 것 같아서 좋더라고요",
          "original_form": "아아 좋죠 ㅋㅋㅋㅋ 저도 식사후에 마시면 입안이 깔끔해지는 것 같아서 좋더라고요",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014889.1.21",
          "form": "전 우유들어간걸 안좋아해서ㅠㅋㅋ",
          "original_form": "전 우유들어간걸 안좋아해서ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014889.1.22",
          "form": "역시 아아 낄끔",
          "original_form": "역시 아아 낄끔",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014889.1.23",
          "form": "맞아요ㅋㅋㅋ 왠지 소화도 잘되는 것 같아요",
          "original_form": "맞아요ㅋㅋㅋ 왠지 소화도 잘되는 것 같아요",
