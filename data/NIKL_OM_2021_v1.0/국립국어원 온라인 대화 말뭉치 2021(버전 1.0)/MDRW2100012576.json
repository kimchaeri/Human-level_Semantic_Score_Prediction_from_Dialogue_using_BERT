      "utterance": [
        {
          "id": "MDRW2100012576.1.1",
          "form": "건강을 위해 하는 운동은 무엇 인가요",
          "original_form": "건강을 위해 하는 운동은 무엇 인가요",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100012576.1.2",
          "form": "한 두달 좀넘게? 70일정도 매일 걸었어요 2만보씩",
          "original_form": "한 두달 좀넘게? 70일정도 매일 걸었어요 2만보씩",
          "speaker_id": "2",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100012576.1.3",
          "form": "저도 산책로 걷기운동 해요",
          "original_form": "저도 산책로 걷기운동 해요",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100012576.1.4",
          "form": "오 얼마나 걸으세요?",
          "original_form": "오 얼마나 걸으세요?",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100012576.1.5",
          "form": "하루 1시간 빨리 걷기를 해요",
          "original_form": "하루 1시간 빨리 걷기를 해요",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100012576.1.6",
          "form": "요즘 비올땐 어떻게 하세요?ㅜ",
          "original_form": "요즘 비올땐 어떻게 하세요?ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100012576.1.7",
          "form": "비가 조금 오면 그냥 걸어요",
          "original_form": "비가 조금 오면 그냥 걸어요",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100012576.1.8",
          "form": "지금걷고 들어왔어요 비조금 맞고요",
          "original_form": "지금걷고 들어왔어요 비조금 맞고요",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100012576.1.9",
          "form": "오 매일 거르지 않으시군요 멋져요!",
          "original_form": "오 매일 거르지 않으시군요 멋져요!",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100012576.1.10",
          "form": "저는 2만보씩 걸었는데 최근 소홀해졌어요ㅜ",
          "original_form": "저는 2만보씩 걸었는데 최근 소홀해졌어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100012576.1.11",
          "form": "거의 매일 걸으려고 노력합니다",
          "original_form": "거의 매일 걸으려고 노력합니다",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100012576.1.12",
          "form": "어젠 비가 넘 많이와서 쉬었어요",
          "original_form": "어젠 비가 넘 많이와서 쉬었어요",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100012576.1.13",
          "form": "저 좀반성되네요 월욜부터 식단조절하니 힘들어서 이러면서 버스타고 댕겼는디ㅜ",
          "original_form": "저 좀반성되네요 월욜부터 식단조절하니 힘들어서 이러면서 버스타고 댕겼는디ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100012576.1.14",
          "form": "걷기를 못할때 따로 하는운동이 있나요",
          "original_form": "걷기를 못할때 따로 하는운동이 있나요",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100012576.1.15",
          "form": "한때는 홈트도했는데ㅜ 또 흐지부지 ㅋㅋ",
          "original_form": "한때는 홈트도했는데ㅜ 또 흐지부지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100012576.1.16",
          "form": "진짜 낼은  꼭 해야겠어요 운동ㅜ",
          "original_form": "진짜 낼은  꼭 해야겠어요 운동ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100012576.1.17",
          "form": "홈트도 요즘 많이들 하는것 같아요",
          "original_form": "홈트도 요즘 많이들 하는것 같아요",
          "speaker_id": "1",
          "time": "20210901 22:46"
        }
      ]
