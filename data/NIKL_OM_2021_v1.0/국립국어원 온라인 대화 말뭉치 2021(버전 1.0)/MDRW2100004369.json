      "utterance": [
        {
          "id": "MDRW2100004369.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004369.1.2",
          "form": "최근에 공연이나 전시회에 자주 가셨나요?",
          "original_form": "최근에 공연이나 전시회에 자주 가셨나요?",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004369.1.3",
          "form": "안녕하세요!!!",
          "original_form": "안녕하세요!!!",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004369.1.4",
          "form": "요즘 코로나가 심해서 공연이랑 전시회간 지 오래됐어요",
          "original_form": "요즘 코로나가 심해서 공연이랑 전시회간 지 오래됐어요",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004369.1.5",
          "form": "최근에 보신게 있으신가요",
          "original_form": "최근에 보신게 있으신가요",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004369.1.6",
          "form": "저도 코로나 때문에 한참 못가다가",
          "original_form": "저도 코로나 때문에 한참 못가다가",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004369.1.7",
          "form": "지난 7월에 뮤지컬 공연 하나 봤어요",
          "original_form": "지난 7월에 뮤지컬 공연 하나 봤어요",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004369.1.8",
          "form": "오 뭐 보셨어요!!!",
          "original_form": "오 뭐 보셨어요!!!",
          "speaker_id": "2",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004369.1.9",
          "form": "뮤지컬 재밌= 보셨나요!",
          "original_form": "뮤지컬 재밌={emoji:☆} 보셨나요!",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004369.1.10",
          "form": "재밌게",
          "original_form": "재밌게",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004369.1.11",
          "form": "티켓 이벤트 당첨이 돼서",
          "original_form": "티켓 이벤트 당첨이 돼서",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004369.1.12",
          "form": "서울예술단 <윤동주 달을 쏘다> 봤는데",
          "original_form": "서울예술단 <윤동주 달을 쏘다> 봤는데",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004369.1.13",
          "form": "앙상블이 특히 좋았어요",
          "original_form": "앙상블이 특히 좋았어요",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004369.1.14",
          "form": "배우분들 합이 되게 좋았나보네용ㅋㅋㅋ",
          "original_form": "배우분들 합이 되게 좋았나보네용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004369.1.15",
          "form": "네 아무래도 예술단원들이 앙상블 하다보니",
          "original_form": "네 아무래도 예술단원들이 앙상블 하다보니",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004369.1.16",
          "form": "같이 연습을 많이 해서인지 합이 잘 맞더라고요",
          "original_form": "같이 연습을 많이 해서인지 합이 잘 맞더라고요",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004369.1.17",
          "form": "오오 같이 많이 맞춰보셔서 더 잘 맞나보네요",
          "original_form": "오오 같이 많이 맞춰보셔서 더 잘 맞나보네요",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004369.1.18",
          "form": "네 그런 것 같아요",
          "original_form": "네 그런 것 같아요",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004369.1.19",
          "form": "뮤지컬 연습하는 거 방송에 나온 거 보면 옴청 빡세더라구요",
          "original_form": "뮤지컬 연습하는 거 방송에 나온 거 보면 옴청 빡세더라구요",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004369.1.20",
          "form": "전 뮤지컬 보는 것 좋아하는데",
          "original_form": "전 뮤지컬 보는 것 좋아하는데",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004369.1.21",
          "form": "요즘은 코로나 때문에 거의 못봐요ㅜ",
          "original_form": "요즘은 코로나 때문에 거의 못봐요ㅜ",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004369.1.22",
          "form": "그쵸 아쉬우시겠어요ㅠㅠㅠ",
          "original_form": "그쵸 아쉬우시겠어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004369.1.23",
          "form": "네 근데 요즘은",
          "original_form": "네 근데 요즘은",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004369.1.24",
          "form": "막 자리도 두칸앉고 한칸씩 떨어져앉더라구요",
          "original_form": "막 자리도 두칸앉고 한칸씩 떨어져앉더라구요",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004369.1.25",
          "form": "온라인으로 프레스콜이나 공연 보여주기도 해서",
          "original_form": "온라인으로 프레스콜이나 공연 보여주기도 해서",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004369.1.26",
          "form": "그건 또 좋기도 해요",
          "original_form": "그건 또 좋기도 해요",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004369.1.27",
          "form": "공연장 안가도 집에서 볼 수 있는 건",
          "original_form": "공연장 안가도 집에서 볼 수 있는 건",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004369.1.28",
          "form": "아맞아요 그리고 무료공연도 있더라구요",
          "original_form": "아맞아요 그리고 무료공연도 있더라구요",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004369.1.29",
          "form": "어서 코로나가 끝나서 공연도 많이 볼 수 있었으면 좋겠어요",
          "original_form": "어서 코로나가 끝나서 공연도 많이 볼 수 있었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004369.1.30",
          "form": "뮤지컬 보신 거 중에 가장 재밌게 보신 건 어떤거예요",
          "original_form": "뮤지컬 보신 거 중에 가장 재밌게 보신 건 어떤거예요",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004369.1.31",
          "form": "네네 빨리 코로나가 잠잠해져서 공연장 맘놓고 갈 수 있기를",
          "original_form": "네네 빨리 코로나가 잠잠해져서 공연장 맘놓고 갈 수 있기를",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004369.1.32",
          "form": "아 저는 전에 뉴욕 갔을 때 브로드웨이  간 적 있는데",
          "original_form": "아 저는 전에 뉴욕 갔을 때 브로드웨이  간 적 있는데",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004369.1.33",
          "form": "그때 본 위키드가 좋았어요!",
          "original_form": "그때 본 위키드가 좋았어요!",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004369.1.34",
          "form": "오 위키드!",
          "original_form": "오 위키드!",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004369.1.35",
          "form": "저는 런던에서 위키드봤었어요",
          "original_form": "저는 런던에서 위키드봤었어요",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004369.1.36",
          "form": "위키드 한국에서도 하지 않았었나요",
          "original_form": "위키드 한국에서도 하지 않았었나요",
          "speaker_id": "2",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004369.1.37",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004369.1.38",
          "form": "최근에도 옥주현 손승연.. 주연으로 했더라고요",
          "original_form": "최근에도 옥주현 손승연.. 주연으로 했더라고요",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004369.1.39",
          "form": "한국에선 아직 못봤어요",
          "original_form": "한국에선 아직 못봤어요",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004369.1.40",
          "form": "기사본거같아요 옥주현 환불 그런 기사요",
          "original_form": "기사본거같아요 옥주현 환불 그런 기사요",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004369.1.41",
          "form": "저는 뮤지컬을 잘 몰라서 가끔 재밌다고 하는 것만 봐요!",
          "original_form": "저는 뮤지컬을 잘 몰라서 가끔 재밌다고 하는 것만 봐요!",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004369.1.42",
          "form": "저도 잘 알지는 못하고",
          "original_form": "저도 잘 알지는 못하고",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004369.1.43",
          "form": "그냥 오 멋있다",
          "original_form": "그냥 오 멋있다",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004369.1.44",
          "form": "노래 좋다",
          "original_form": "노래 좋다",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004369.1.45",
          "form": "이 정도ㅎㅎ",
          "original_form": "이 정도ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004369.1.46",
          "form": "ㅎㅎ문화인! 아까말씀해주신 뮤지컬도 한 번 찾아봐야겠어요",
          "original_form": "ㅎㅎ문화인! 아까말씀해주신 뮤지컬도 한 번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 16:50"
        }
      ]
