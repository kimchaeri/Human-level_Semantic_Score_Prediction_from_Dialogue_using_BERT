      "utterance": [
        {
          "id": "MDRW2100025152.1.1",
          "form": "가족관계가 어떻게되니",
          "original_form": "가족관계가 어떻게되니",
          "speaker_id": "1",
          "time": "20210831 23:49"
        },
        {
          "id": "MDRW2100025152.1.2",
          "form": "부모님이랑 삼남매",
          "original_form": "부모님이랑 삼남매",
          "speaker_id": "2",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100025152.1.3",
          "form": "합쳐서 5명",
          "original_form": "합쳐서 5명",
          "speaker_id": "2",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100025152.1.4",
          "form": "오 난 외동이야",
          "original_form": "오 난 외동이야",
          "speaker_id": "1",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100025152.1.5",
          "form": "나는 외동이 부럽더라",
          "original_form": "나는 외동이 부럽더라",
          "speaker_id": "2",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100025152.1.6",
          "form": "솔직히 나도 외동이 좋아",
          "original_form": "솔직히 나도 외동이 좋아",
          "speaker_id": "1",
          "time": "20210831 23:51"
        },
        {
          "id": "MDRW2100025152.1.7",
          "form": "먹을거 다 내꺼거든",
          "original_form": "먹을거 다 내꺼거든",
          "speaker_id": "1",
          "time": "20210831 23:51"
        },
        {
          "id": "MDRW2100025152.1.8",
          "form": "그게 제일 부러워",
          "original_form": "그게 제일 부러워",
          "speaker_id": "2",
          "time": "20210831 23:51"
        },
        {
          "id": "MDRW2100025152.1.9",
          "form": "그래도 남매면",
          "original_form": "그래도 남매면",
          "speaker_id": "1",
          "time": "20210831 23:51"
        },
        {
          "id": "MDRW2100025152.1.10",
          "form": "우리 맨날 맛있는 반찬으로 싸웠거든",
          "original_form": "우리 맨날 맛있는 반찬으로 싸웠거든",
          "speaker_id": "2",
          "time": "20210831 23:51"
        },
        {
          "id": "MDRW2100025152.1.11",
          "form": "음식 시켜먹을때 돈 많이 안들겠다",
          "original_form": "음식 시켜먹을때 돈 많이 안들겠다",
          "speaker_id": "1",
          "time": "20210831 23:52"
        },
        {
          "id": "MDRW2100025152.1.12",
          "form": "아니 오히려 더 많이 들어",
          "original_form": "아니 오히려 더 많이 들어",
          "speaker_id": "2",
          "time": "20210831 23:52"
        },
        {
          "id": "MDRW2100025152.1.13",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210831 23:52"
        },
        {
          "id": "MDRW2100025152.1.14",
          "form": "합심해서 맨날 시켜먹거든",
          "original_form": "합심해서 맨날 시켜먹거든",
          "speaker_id": "2",
          "time": "20210831 23:52"
        },
        {
          "id": "MDRW2100025152.1.15",
          "form": "요리를 아무도 할 줄 모르니까",
          "original_form": "요리를 아무도 할 줄 모르니까",
          "speaker_id": "2",
          "time": "20210831 23:53"
        },
        {
          "id": "MDRW2100025152.1.16",
          "form": "장점같기도하고 단점같기도하네",
          "original_form": "장점같기도하고 단점같기도하네",
          "speaker_id": "1",
          "time": "20210831 23:53"
        }
      ]
    }
