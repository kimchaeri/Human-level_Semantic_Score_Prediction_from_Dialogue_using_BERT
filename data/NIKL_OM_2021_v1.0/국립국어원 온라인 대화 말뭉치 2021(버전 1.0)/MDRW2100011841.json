      "utterance": [
        {
          "id": "MDRW2100011841.1.1",
          "form": "근데 무관중이라",
          "original_form": "근데 무관중이라",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011841.1.2",
          "form": "그치 그래서 못가지",
          "original_form": "그치 그래서 못가지",
          "speaker_id": "1",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011841.1.3",
          "form": "야구장 안간지 오래되지 않았나?",
          "original_form": "야구장 안간지 오래되지 않았나?",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011841.1.4",
          "form": "진짜 오래됐어",
          "original_form": "진짜 오래됐어",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011841.1.5",
          "form": "근데 두산 땜에 안보잖아",
          "original_form": "근데 두산 땜에 안보잖아",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011841.1.6",
          "form": "아니 이제 응원하는 팀 없이 그냥 보는 거지",
          "original_form": "아니 이제 응원하는 팀 없이 그냥 보는 거지",
          "speaker_id": "1",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011841.1.7",
          "form": "이기는 팀 우리 팀?ㅋㅋㅋㅋ",
          "original_form": "이기는 팀 우리 팀?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011841.1.8",
          "form": "원래 그런 마음가짐으로 볼 때 제일 재밌는 거야",
          "original_form": "원래 그런 마음가짐으로 볼 때 제일 재밌는 거야",
          "speaker_id": "1",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011841.1.9",
          "form": "그렇긴 해",
          "original_form": "그렇긴 해",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011841.1.10",
          "form": "근데 선수들을 몰라서 그게 좀 걱정되긴 해",
          "original_form": "근데 선수들을 몰라서 그게 좀 걱정되긴 해",
          "speaker_id": "1",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011841.1.11",
          "form": "천천히 알아가는거지",
          "original_form": "천천히 알아가는거지",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011841.1.12",
          "form": "근데 잘 안  외워지더라고 응원가도 그렇고",
          "original_form": "근데 잘 안  외워지더라고 응원가도 그렇고",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011841.1.13",
          "form": "난 그냥 보는 편임",
          "original_form": "난 그냥 보는 편임",
          "speaker_id": "2",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011841.1.14",
          "form": "그래도 응원하는 팀이 있잖아",
          "original_form": "그래도 응원하는 팀이 있잖아",
          "speaker_id": "1",
          "time": "20210824 16:36"
        },
        {
          "id": "MDRW2100011841.1.15",
