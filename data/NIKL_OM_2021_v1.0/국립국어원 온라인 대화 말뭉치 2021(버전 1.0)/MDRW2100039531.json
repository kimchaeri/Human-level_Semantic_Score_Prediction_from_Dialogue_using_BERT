      "utterance": [
        {
          "id": "MDRW2100039531.1.1",
          "form": "고영... 여전히 누워있네요",
          "original_form": "고영... 여전히 누워있네요",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039531.1.2",
          "form": "집고양이들의 삶 뭔가 부러워요...",
          "original_form": "집고양이들의 삶 뭔가 부러워요...",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039531.1.3",
          "form": "진짜요,, 뭔가.. 저도 고양이할래요ㅜㅋㅋㅋ",
          "original_form": "진짜요,, 뭔가.. 저도 고양이할래요ㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039531.1.4",
          "form": "ㅠㅠ누군가 밥 챙겨주고 귀여워해주는 삶...",
          "original_form": "ㅠㅠ누군가 밥 챙겨주고 귀여워해주는 삶...",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039531.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ진짜요!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ진짜요!!!",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039531.1.6",
          "form": "다음생애엔 꼭 부잣집 고양이로.",
          "original_form": "다음생애엔 꼭 부잣집 고양이로.",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039531.1.7",
          "form": "털 긴 고양이는 왠지 부자집에 살거 같은 느낌이 있어요ㅋㅋ",
          "original_form": "털 긴 고양이는 왠지 부자집에 살거 같은 느낌이 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039531.1.8",
          "form": "앜ㅋㅋㅋㅋㅋㅋ진짜요! 뭔가 흰고양이일거같네요ㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ진짜요! 뭔가 흰고양이일거같네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100039531.1.9",
          "form": "페르시안 고양이 같은... 털 완전 풍성하고요ㅋㅋㅋ",
          "original_form": "페르시안 고양이 같은... 털 완전 풍성하고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 09:24"
        },
        {
          "id": "MDRW2100039531.1.10",
          "form": "진짜요ㅠㅠ근데 너무 귀엽고 도도해요",
          "original_form": "진짜요ㅠㅠ근데 너무 귀엽고 도도해요",
          "speaker_id": "2",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100039531.1.11",
          "form": "볼살 두터운 애들은 뚱해보이는 표정이라 그게 너무 귀여워요ㅋㅋㅋㅋ",
          "original_form": "볼살 두터운 애들은 뚱해보이는 표정이라 그게 너무 귀여워요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100039531.1.12",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ털도 다 쳐져서 뚜웅..이러고 잇는게ㅜㅜ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ털도 다 쳐져서 뚜웅..이러고 잇는게ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100039531.1.13",
          "form": "털 쓰담쓰담 해보고싶네요",
          "original_form": "털 쓰담쓰담 해보고싶네요",
          "speaker_id": "1",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100039531.1.14",
          "form": "저두요ㅜㅜ 장모는 어떤 느낌일지 궁금해요",
          "original_form": "저두요ㅜㅜ 장모는 어떤 느낌일지 궁금해요",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100039531.1.15",
          "form": "장모는 어떤 느낌일까요..",
          "original_form": "장모는 어떤 느낌일까요..",
