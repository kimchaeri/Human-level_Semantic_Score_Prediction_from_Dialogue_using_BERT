      "utterance": [
        {
          "id": "MDRW2100005484.1.1",
          "form": "학생때 장래희망이 뭐였어?",
          "original_form": "학생때 장래희망이 뭐였어?",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005484.1.2",
          "form": "음 초둥학생땐 의사!",
          "original_form": "음 초둥학생땐 의사!",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005484.1.3",
          "form": "우와 그렇구나",
          "original_form": "우와 그렇구나",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005484.1.4",
          "form": "고등학교 때는?",
          "original_form": "고등학교 때는?",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005484.1.5",
          "form": "크게 사고났었는데 다리 수술해서 잘 걸어다니고 있거든",
          "original_form": "크게 사고났었는데 다리 수술해서 잘 걸어다니고 있거든",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005484.1.6",
          "form": "그거 멋져서ㅋㅋ의사되겠다 맨날 써서 냈었지ㅋㅋ",
          "original_form": "그거 멋져서ㅋㅋ의사되겠다 맨날 써서 냈었지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005484.1.7",
          "form": "고등학교땐 음 의약학계열? ㅋㅋㅋㅋ",
          "original_form": "고등학교땐 음 의약학계열? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005484.1.8",
          "form": "이과인데 자연과학은 어려울거 같아서ㅋㅋㅋ현실적인이유지뭐ㅋㅋㅋ 너는?",
          "original_form": "이과인데 자연과학은 어려울거 같아서ㅋㅋㅋ현실적인이유지뭐ㅋㅋㅋ 너는?",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005484.1.9",
          "form": "그렇구나 너무 다행이야 ㅠㅠㅠ 의약학 계열이 계속 꿈이었구나",
          "original_form": "그렇구나 너무 다행이야 ㅠㅠㅠ 의약학 계열이 계속 꿈이었구나",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005484.1.10",
          "form": "나는 초등학교 때부터 초등교사가 꿈이었고",
          "original_form": "나는 초등학교 때부터 초등교사가 꿈이었고",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005484.1.11",
          "form": "지금도 똑같아 ㅎㅎㅎㅎ",
          "original_form": "지금도 똑같아 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005484.1.12",
          "form": "현실적인 이유 지만 나중에 그 멋졌던 감정이 좋게 다가 올거야!",
          "original_form": "현실적인 이유 지만 나중에 그 멋졌던 감정이 좋게 다가 올거야!",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005484.1.13",
          "form": "그렇게 될수 있게 노력해야지ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇게 될수 있게 노력해야지ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005484.1.14",
          "form": "오 학생들 가르치고 싶었어?",
          "original_form": "오 학생들 가르치고 싶었어?",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005484.1.15",
          "form": "응응 ㅎㅎㅎ 그냥 자연스레 꿈이 그렇게 되더라고?",
          "original_form": "응응 ㅎㅎㅎ 그냥 자연스레 꿈이 그렇게 되더라고?",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005484.1.16",
          "form": "오 내 주변에도 초등학교 선생님들 있어",
          "original_form": "오 내 주변에도 초등학교 선생님들 있어",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005484.1.17",
          "form": "뭔가 중고등학교 선생님보다도 좀 더? 사명감 이런게 있는거 같더라고 가래서 멋지더라..ㅎㅎ",
          "original_form": "뭔가 중고등학교 선생님보다도 좀 더? 사명감 이런게 있는거 같더라고 가래서 멋지더라..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005484.1.18",
          "form": "그렇구나 ㅎㅎㅎ 좋은 사람들인 가봐",
          "original_form": "그렇구나 ㅎㅎㅎ 좋은 사람들인 가봐",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005484.1.19",
          "form": "나도 내년에 임용인데 잘 됐으면 좋겠어",
          "original_form": "나도 내년에 임용인데 잘 됐으면 좋겠어",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005484.1.20",
          "form": "대단하다.. 난 임용 어떻게 공부하는지 듣고 난 시험 못봤겠구나...했어ㅋㅋㅋㅋ",
          "original_form": "대단하다.. 난 임용 어떻게 공부하는지 듣고 난 시험 못봤겠구나...했어ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005484.1.21",
          "form": "공부하는데 힘들지 않아?",
          "original_form": "공부하는데 힘들지 않아?",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005484.1.22",
          "form": "나는 내년에 시작이라 아직 시작을 안했는데",
          "original_form": "나는 내년에 시작이라 아직 시작을 안했는데",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005484.1.23",
          "form": "남친 하는거 보니까 힘들어보이더라ㅠㅠㅠ",
          "original_form": "남친 하는거 보니까 힘들어보이더라ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005484.1.24",
          "form": "오 과선후배인가보다!",
          "original_form": "오 과선후배인가보다!",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005484.1.25",
          "form": "임용합격하면 취업은 어떻게 하는거야?",
          "original_form": "임용합격하면 취업은 어떻게 하는거야?",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005484.1.26",
          "form": "음 지역에 응시하고 붙으면 교육청에서 발령을 내줘!",
          "original_form": "음 지역에 응시하고 붙으면 교육청에서 발령을 내줘!",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005484.1.27",
          "form": "부산이면 부산 안 초등학교!",
          "original_form": "부산이면 부산 안 초등학교!",
          "speaker_id": "1",
          "time": "20210821 22:50"
        }
      ]
