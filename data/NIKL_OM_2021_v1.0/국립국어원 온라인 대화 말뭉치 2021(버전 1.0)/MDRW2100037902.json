      "utterance": [
        {
          "id": "MDRW2100037902.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037902.1.2",
          "form": "집안일 자주 하세요?",
          "original_form": "집안일 자주 하세요?",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037902.1.3",
          "form": "아뇨 자주 안해요 name1님은 어떠세요?",
          "original_form": "아뇨 자주 안해요 &name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037902.1.4",
          "form": "전 집안 일 엄청 하기 싫어요ㅋㅋㅋㅋ",
          "original_form": "전 집안 일 엄청 하기 싫어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100037902.1.5",
          "form": "애 때문에 억지로 합니다ㅋㅋ",
          "original_form": "애 때문에 억지로 합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100037902.1.6",
          "form": "자녀가있으시군요",
          "original_form": "자녀가있으시군요",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100037902.1.7",
          "form": "저는 아직 결혼안해서요",
          "original_form": "저는 아직 결혼안해서요",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100037902.1.8",
          "form": "이런 말 그렇지만.. 혼자가 좋죠ㅋㅋ",
          "original_form": "이런 말 그렇지만.. 혼자가 좋죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037902.1.9",
          "form": "특히 하기 싫은 집안 일 있으세요?",
          "original_form": "특히 하기 싫은 집안 일 있으세요?",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100037902.1.10",
          "form": "설거지 하기싫어요",
          "original_form": "설거지 하기싫어요",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037902.1.11",
          "form": "name1님은 어떤일이 싫으세요?",
          "original_form": "&name1&님은 어떤일이 싫으세요?",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037902.1.12",
          "form": "저도 설거지 싫어요ㅋㅋ",
          "original_form": "저도 설거지 싫어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037902.1.13",
          "form": "그리고 빨래 게는 거;",
          "original_form": "그리고 빨래 게는 거;",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037902.1.14",
          "form": "굳이 개어야 하나싶어요",
          "original_form": "굳이 개어야 하나싶어요",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037902.1.15",
          "form": "혼자 살면",
          "original_form": "혼자 살면",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037902.1.16",
          "form": "말려서 입엇죠",
          "original_form": "말려서 입엇죠",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037902.1.17",
          "form": "맞아요 건조대에서 바로 입기~",
          "original_form": "맞아요 건조대에서 바로 입기~",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037902.1.18",
          "form": "결혼할 생각은 없는데",
          "original_form": "결혼할 생각은 없는데",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037902.1.19",
          "form": "살면서 불이익받을거같아요",
          "original_form": "살면서 불이익받을거같아요",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037902.1.20",
          "form": "요즘은 자녀가 부의상징이라네요",
          "original_form": "요즘은 자녀가 부의상징이라네요",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100037902.1.21",
          "form": "음.. 전 솔직히 요즘 요즘 애들한테 미안해요ㅠ",
          "original_form": "음.. 전 솔직히 요즘 요즘 애들한테 미안해요ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037902.1.22",
          "form": "애들이 편하게 살 수 있는 세상은 아닌 것 같아서ㅠ",
          "original_form": "애들이 편하게 살 수 있는 세상은 아닌 것 같아서ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037902.1.23",
          "form": "네 저도 그런생각때문이긴 한데요",
          "original_form": "네 저도 그런생각때문이긴 한데요",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100037902.1.24",
          "form": "사실 제 이기심이긴 해요",
          "original_form": "사실 제 이기심이긴 해요",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100037902.1.25",
          "form": "여자가 얻는 보상이 별로 없어보여서요",
          "original_form": "여자가 얻는 보상이 별로 없어보여서요",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100037902.1.26",
          "form": "맞아요 여자가 얻는 보상도 없지만..",
          "original_form": "맞아요 여자가 얻는 보상도 없지만..",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100037902.1.27",
          "form": "신랑도 많이 짠해요ㅠ",
          "original_form": "신랑도 많이 짠해요ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100037902.1.28",
          "form": "열심히 벌어서",
          "original_form": "열심히 벌어서",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100037902.1.29",
          "form": "다 애들 밑으로 들어가죠ㅠ",
          "original_form": "다 애들 밑으로 들어가죠ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:58"
        }
      ]
    }
