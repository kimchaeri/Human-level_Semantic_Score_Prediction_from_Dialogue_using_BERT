      "utterance": [
        {
          "id": "MDRW2100032991.1.1",
          "form": "청귤청 가져와서 집에서 히트쳤엉ㅅ",
          "original_form": "청귤청 가져와서 집에서 히트쳤엉ㅅ",
          "speaker_id": "2",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032991.1.2",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032991.1.3",
          "form": "숙성도 다 안됐는데 ㅋㅋ",
          "original_form": "숙성도 다 안됐는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032991.1.4",
          "form": "한짐 가득 챙겨 가셨네요 ㅎ",
          "original_form": "한짐 가득 챙겨 가셨네요 ㅎ",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032991.1.5",
          "form": "추석 맞이 선물인가요",
          "original_form": "추석 맞이 선물인가요",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100032991.1.6",
          "form": "제주도는 웬만한 건 다 비싸더라고요",
          "original_form": "제주도는 웬만한 건 다 비싸더라고요",
          "speaker_id": "1",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100032991.1.7",
          "form": "아 엄마가 갈치어딪냐고 하시던데요 ㅎㅎ",
          "original_form": "아 엄마가 갈치어딪냐고 하시던데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100032991.1.8",
          "form": "추석때 명상할거라 못 가서",
          "original_form": "추석때 명상할거라 못 가서",
          "speaker_id": "2",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100032991.1.9",
          "form": "갈치 엄청 엄청 비싸요",
          "original_form": "갈치 엄청 엄청 비싸요",
          "speaker_id": "1",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100032991.1.10",
          "form": "미리갔는데 돌아가서 준문해야겠어요",
          "original_form": "미리갔는데 돌아가서 준문해야겠어요",
          "speaker_id": "2",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100032991.1.11",
          "form": "제주에서 저렴한 건 딱 한 가지더라고요",
          "original_form": "제주에서 저렴한 건 딱 한 가지더라고요",
          "speaker_id": "1",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100032991.1.12",
          "form": "요즘 비싸다더라구요",
          "original_form": "요즘 비싸다더라구요",
          "speaker_id": "2",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100032991.1.13",
          "form": "삼다수 ㅡㅡㅋ",
          "original_form": "삼다수 ㅡㅡㅋ",
          "speaker_id": "1",
          "time": "20210906 12:26"
        },
        {
          "id": "MDRW2100032991.1.14",
          "form": "ㅎㅎㅎ 그마저도 브리타쓰는데 가장 맛있는 걸 싸게파네요",
          "original_form": "ㅎㅎㅎ 그마저도 브리타쓰는데 가장 맛있는 걸 싸게파네요",
          "speaker_id": "2",
          "time": "20210906 12:26"
        },
        {
          "id": "MDRW2100032991.1.15",
          "form": "네 이것만 싸요",
          "original_form": "네 이것만 싸요",
          "speaker_id": "1",
          "time": "20210906 12:26"
        },
        {
          "id": "MDRW2100032991.1.16",
          "form": "물만 먹을까여",
          "original_form": "물만 먹을까여",
          "speaker_id": "1",
          "time": "20210906 12:26"
        },
        {
          "id": "MDRW2100032991.1.17",
          "form": "ㅎㅎ 명상때는 좋은 물만 먹어도 좋은데",
          "original_form": "ㅎㅎ 명상때는 좋은 물만 먹어도 좋은데",
          "speaker_id": "2",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032991.1.18",
          "form": "채소 과일이 비싼게 흠이예요",
          "original_form": "채소 과일이 비싼게 흠이예요",
          "speaker_id": "2",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032991.1.19",
          "form": "채식선호자거든요",
          "original_form": "채식선호자거든요",
          "speaker_id": "2",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032991.1.20",
          "form": "추석앞이라 진짜 비싸요 전부",
          "original_form": "추석앞이라 진짜 비싸요 전부",
          "speaker_id": "1",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032991.1.21",
          "form": "재난지원금 나오믄 사야겠어요",
          "original_form": "재난지원금 나오믄 사야겠어요",
          "speaker_id": "1",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032991.1.22",
          "form": "아 ㅎㅎ 어제 아버지 대상자 아니라고 나왔던거 격노하셨음요",
          "original_form": "아 ㅎㅎ 어제 아버지 대상자 아니라고 나왔던거 격노하셨음요",
          "speaker_id": "2",
          "time": "20210906 12:30"
        },
        {
          "id": "MDRW2100032991.1.23",
          "form": "저도 채식으로 가고있어요",
          "original_form": "저도 채식으로 가고있어요",
          "speaker_id": "1",
          "time": "20210906 12:31"
        },
        {
          "id": "MDRW2100032991.1.24",
          "form": "앗 대상아니시군요ㅜ",
          "original_form": "앗 대상아니시군요ㅜ",
          "speaker_id": "1",
          "time": "20210906 12:31"
        }
      ]
