      "utterance": [
        {
          "id": "MDRW2100039724.1.1",
          "form": "쭈꾸미 배부르게 먹엇네요\r\n계란찜이 답이엇어여ㅋ",
          "original_form": "쭈꾸미 배부르게 먹엇네요\r\n계란찜이 답이엇어여ㅋ",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039724.1.2",
          "form": "다행이예요 ㅎㅎ",
          "original_form": "다행이예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039724.1.3",
          "form": "간만에 과식이네요ㅋㅋ",
          "original_form": "간만에 과식이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039724.1.4",
          "form": "저는 쭈꾸미 정보 덕분에 언제한번 메뉴로 한번 해보려구요",
          "original_form": "저는 쭈꾸미 정보 덕분에 언제한번 메뉴로 한번 해보려구요",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039724.1.5",
          "form": "네 양배추같은거 넣으면 좀덜매워요",
          "original_form": "네 양배추같은거 넣으면 좀덜매워요",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039724.1.6",
          "form": "아! 꿀팁이네요",
          "original_form": "아! 꿀팁이네요",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039724.1.7",
          "form": "담주말에 한번 시도해봐야겠어요",
          "original_form": "담주말에 한번 시도해봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039724.1.8",
          "form": "아이들이 아직 매운걸 못먹으니 고추장들어간 반찬을 안해본거 같아요 ㅎㅎ",
          "original_form": "아이들이 아직 매운걸 못먹으니 고추장들어간 반찬을 안해본거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039724.1.9",
          "form": "애기들있으면 그렇져",
          "original_form": "애기들있으면 그렇져",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039724.1.10",
          "form": "아이들위주의 식단을 짜니까",
          "original_form": "아이들위주의 식단을 짜니까",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039724.1.11",
          "form": "달달한거 메추리알 멸치볶음 미역국 이런것만 먹은거 같아요 ㅋㅋ",
          "original_form": "달달한거 메추리알 멸치볶음 미역국 이런것만 먹은거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039724.1.12",
          "form": "저희언니는 보니까 두가지 따로하더라고요",
          "original_form": "저희언니는 보니까 두가지 따로하더라고요",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039724.1.13",
          "form": "반찬을 두가지 종류로요??",
          "original_form": "반찬을 두가지 종류로요??",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039724.1.14",
          "form": "네 국도요",
          "original_form": "네 국도요",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039724.1.15",
          "form": "그럴 실력이 안되서 ㅋㅋㅋ",
          "original_form": "그럴 실력이 안되서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039724.1.16",
          "form": "하나 하는것도 벅차요 ㅎ",
          "original_form": "하나 하는것도 벅차요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039724.1.17",
          "form": "지난번엔 카레만들다가.....",
          "original_form": "지난번엔 카레만들다가.....",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039724.1.18",
          "form": "밍밍하게 먹는거도 질렷나봐여ㅋㅋ",
          "original_form": "밍밍하게 먹는거도 질렷나봐여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039724.1.19",
          "form": "감자 양파 당근 썰고나니 목디스크 오는거 같더라구요 ㅋ",
          "original_form": "감자 양파 당근 썰고나니 목디스크 오는거 같더라구요 ㅋ",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039724.1.20",
          "form": "아 전 그래서 채칼 그런거",
          "original_form": "아 전 그래서 채칼 그런거",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039724.1.21",
          "form": "사고싶더라고요",
          "original_form": "사고싶더라고요",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039724.1.22",
          "form": "만능 채칼",
          "original_form": "만능 채칼",
          "speaker_id": "1",
          "time": "20210903 21:38"
        }
      ]
    }
  ]
