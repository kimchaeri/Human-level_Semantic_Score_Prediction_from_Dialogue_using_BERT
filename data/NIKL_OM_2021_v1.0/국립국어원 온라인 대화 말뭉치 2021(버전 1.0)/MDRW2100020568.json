      "utterance": [
        {
          "id": "MDRW2100020568.1.1",
          "form": "역시 할인할 땐 비싼거 사야해요",
          "original_form": "역시 할인할 땐 비싼거 사야해요",
          "speaker_id": "2",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020568.1.2",
          "form": "그 저는 파바가면 그거 꼭 먹어요 뽀드득 그릴소시지",
          "original_form": "그 저는 파바가면 그거 꼭 먹어요 뽀드득 그릴소시지",
          "speaker_id": "1",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020568.1.3",
          "form": "저는 급 치즈타르트가 먹고 싶네요",
          "original_form": "저는 급 치즈타르트가 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020568.1.4",
          "form": "아 이름부터 맛있어요 ㅠ",
          "original_form": "아 이름부터 맛있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020568.1.5",
          "form": "아 치즈타르트 너무좋아",
          "original_form": "아 치즈타르트 너무좋아",
          "speaker_id": "1",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020568.1.6",
          "form": "저거 드셔보셨나요",
          "original_form": "저거 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020568.1.7",
          "form": "사진을 보고 오겠어요",
          "original_form": "사진을 보고 오겠어요",
          "speaker_id": "2",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020568.1.8",
          "form": "소시지가 다른 빵의 소시지랑달라서 저건 찐이에요",
          "original_form": "소시지가 다른 빵의 소시지랑달라서 저건 찐이에요",
          "speaker_id": "1",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020568.1.9",
          "form": "다른빵은 얇고 한빵 쓰는데 저건 좀 비싼빵을써서 꼭 드서보세요",
          "original_form": "다른빵은 얇고 한빵 쓰는데 저건 좀 비싼빵을써서 꼭 드서보세요",
          "speaker_id": "1",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020568.1.10",
          "form": "네 먹어봤죠 당근 ㅠ",
          "original_form": "네 먹어봤죠 당근 ㅠ",
          "speaker_id": "2",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020568.1.11",
          "form": "나름 피자빵은 클래식이라",
          "original_form": "나름 피자빵은 클래식이라",
          "speaker_id": "2",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020568.1.12",
          "form": "가끔 안사면 섭섭해요",
          "original_form": "가끔 안사면 섭섭해요",
          "speaker_id": "2",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020568.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020568.1.14",
          "form": "하나씩 담게되는",
          "original_form": "하나씩 담게되는",
          "speaker_id": "1",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020568.1.15",
          "form": "통통한 소시지",
          "original_form": "통통한 소시지",
          "speaker_id": "2",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020568.1.16",
          "form": "그리고 뭔가 뀐아망 좋아하실거같아요",
          "original_form": "그리고 뭔가 뀐아망 좋아하실거같아요",
          "speaker_id": "1",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020568.1.17",
          "form": "혹시 크루아상 좋아하시나요",
          "original_form": "혹시 크루아상 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020568.1.18",
          "form": "훗 드디어 빗나가셨네요",
          "original_form": "훗 드디어 빗나가셨네요",
          "speaker_id": "2",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020568.1.19",
          "form": "저는 사실",
          "original_form": "저는 사실",
          "speaker_id": "2",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020568.1.20",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020568.1.21",
          "form": "페스츄리류를 썩 안좋아한답니다!",
          "original_form": "페스츄리류를 썩 안좋아한답니다!",
          "speaker_id": "2",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020568.1.22",
          "form": "헉 이럴수가",
          "original_form": "헉 이럴수가",
          "speaker_id": "1",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020568.1.23",
          "form": "저 샌드위치에서만 좋아했죠!",
          "original_form": "저 샌드위치에서만 좋아했죠!",
          "speaker_id": "2",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020568.1.24",
          "form": "저의 미스테이크..!",
          "original_form": "저의 미스테이크..!",
          "speaker_id": "1",
          "time": "20210828 09:58"
        }
      ]
