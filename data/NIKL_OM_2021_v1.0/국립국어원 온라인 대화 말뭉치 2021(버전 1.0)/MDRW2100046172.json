      "utterance": [
        {
          "id": "MDRW2100046172.1.1",
          "form": "요즘에 사신 물건 있으신가요??",
          "original_form": "요즘에 사신 물건 있으신가요??",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046172.1.2",
          "form": "당근하세요?",
          "original_form": "당근하세요?",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046172.1.3",
          "form": "저 한번도 해본적 없어요 잘 하시나요??",
          "original_form": "저 한번도 해본적 없어요 잘 하시나요??",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046172.1.4",
          "form": "저는 주로 사고 팔고해요 ㅎ",
          "original_form": "저는 주로 사고 팔고해요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046172.1.5",
          "form": "어제는 버즈2 샀어요",
          "original_form": "어제는 버즈2 샀어요",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046172.1.6",
          "form": "당근통해서",
          "original_form": "당근통해서",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046172.1.7",
          "form": "오 괜찮으신가보네요 자주 하시는걸보니!!",
          "original_form": "오 괜찮으신가보네요 자주 하시는걸보니!!",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046172.1.8",
          "form": "직거래우선이니 믿을수 있어서요",
          "original_form": "직거래우선이니 믿을수 있어서요",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046172.1.9",
          "form": "중나는 사기꾼들도 많고",
          "original_form": "중나는 사기꾼들도 많고",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046172.1.10",
          "form": "맞아요 중고나라 같은곳 생각하면",
          "original_form": "맞아요 중고나라 같은곳 생각하면",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046172.1.11",
          "form": "벽돌 보내진 않겠어요ㅋㅋㅋㅋ",
          "original_form": "벽돌 보내진 않겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046172.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046172.1.13",
          "form": "그 사건 보고 깜놀했어요",
          "original_form": "그 사건 보고 깜놀했어요",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046172.1.14",
          "form": "그런 사례 너무 많은거 같더라고요",
          "original_form": "그런 사례 너무 많은거 같더라고요",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046172.1.15",
          "form": "저는 사기당할뻔했어요 ㅎ",
          "original_form": "저는 사기당할뻔했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046172.1.16",
          "form": "진짜여??",
          "original_form": "진짜여??",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046172.1.17",
          "form": "어떤일로",
          "original_form": "어떤일로",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046172.1.18",
          "form": "아이책산다고",
          "original_form": "아이책산다고",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046172.1.19",
          "form": "연락했다가요",
          "original_form": "연락했다가요",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046172.1.20",
          "form": "아 부피도 큰건데!!",
          "original_form": "아 부피도 큰건데!!",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046172.1.21",
          "form": "입금도 안전결제로 하라면서",
          "original_form": "입금도 안전결제로 하라면서",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046172.1.22",
          "form": "링크도 보내주더라구요",
          "original_form": "링크도 보내주더라구요",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046172.1.23",
          "form": "근데 입금자명도 이상한거부터",
          "original_form": "근데 입금자명도 이상한거부터",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046172.1.24",
          "form": "느킴이 쐐하더라구요",
          "original_form": "느킴이 쐐하더라구요",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046172.1.25",
          "form": "아 역시 티나나봐요",
          "original_form": "아 역시 티나나봐요",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046172.1.26",
          "form": "그래도 잘 거르셨네요",
          "original_form": "그래도 잘 거르셨네요",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046172.1.27",
          "form": "그래서 여쭈어보니",
          "original_form": "그래서 여쭈어보니",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046172.1.28",
          "form": "안심결제라서 그렇다고하시고",
          "original_form": "안심결제라서 그렇다고하시고",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046172.1.29",
          "form": "수수료를 포함해서보내라는거예요",
          "original_form": "수수료를 포함해서보내라는거예요",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046172.1.30",
          "form": "참 사기꾼은 부지런해여",
          "original_form": "참 사기꾼은 부지런해여",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046172.1.31",
          "form": "자동차감이 안되길래 물어보니",
          "original_form": "자동차감이 안되길래 물어보니",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046172.1.32",
          "form": "그리고 댁은 거제인데 은행은 광주",
          "original_form": "그리고 댁은 거제인데 은행은 광주",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046172.1.33",
          "form": "안걸릴거라 생각하나",
          "original_form": "안걸릴거라 생각하나",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046172.1.34",
          "form": "먼가 안맞더라구요 ㅎㅎ",
          "original_form": "먼가 안맞더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046172.1.35",
          "form": "아 그럼 백퍼네요",
          "original_form": "아 그럼 백퍼네요",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046172.1.36",
          "form": "경상도와 전라도 ㅋㅋ",
          "original_form": "경상도와 전라도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046172.1.37",
          "form": "그런 사기꾼은 좀",
          "original_form": "그런 사기꾼은 좀",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046172.1.38",
          "form": "벌을 줘야 안그럴거 같은데",
          "original_form": "벌을 줘야 안그럴거 같은데",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046172.1.39",
          "form": "처벌이 약하댔어요",
          "original_form": "처벌이 약하댔어요",
          "speaker_id": "2",
          "time": "20210908 11:09"
        }
      ]
