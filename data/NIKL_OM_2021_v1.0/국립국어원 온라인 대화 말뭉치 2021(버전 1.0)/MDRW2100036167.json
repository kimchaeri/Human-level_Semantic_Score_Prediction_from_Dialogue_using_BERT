      "utterance": [
        {
          "id": "MDRW2100036167.1.1",
          "form": "안녕하세요 토마토 좋아하시나요",
          "original_form": "안녕하세요 토마토 좋아하시나요",
          "speaker_id": "1",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100036167.1.2",
          "form": "안녕하세요?ㅋㅋ 토마토 예 좋아해요 ㅋㅋ",
          "original_form": "안녕하세요?ㅋㅋ 토마토 예 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036167.1.3",
          "form": "맛있고 요리에 쓰기 좋아서요",
          "original_form": "맛있고 요리에 쓰기 좋아서요",
          "speaker_id": "2",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036167.1.4",
          "form": "좋아하는 야채가 있으신가요?",
          "original_form": "좋아하는 야채가 있으신가요?",
          "speaker_id": "2",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036167.1.5",
          "form": "저는 양파를 좋아해요",
          "original_form": "저는 양파를 좋아해요",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036167.1.6",
          "form": "달달 볶아서 갈색되면 진짜 맛있어요",
          "original_form": "달달 볶아서 갈색되면 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036167.1.7",
          "form": "양파가 올해는 특히 싸죠",
          "original_form": "양파가 올해는 특히 싸죠",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036167.1.8",
          "form": "전 양파를 다지고 볶아서 인도식 카레가루랑 섞어서만든 카레를 좋아해요",
          "original_form": "전 양파를 다지고 볶아서 인도식 카레가루랑 섞어서만든 카레를 좋아해요",
          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036167.1.9",
          "form": "아 정말 맛있겠네요",
          "original_form": "아 정말 맛있겠네요",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036167.1.10",
          "form": "양파 풍미가 장난아니겠어요",
          "original_form": "양파 풍미가 장난아니겠어요",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036167.1.11",
          "form": "토마토카레 좋아하세요?",
          "original_form": "토마토카레 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100036167.1.12",
          "form": "좋아하는데 생토마토보단 퓨레넣는게 편해요",
          "original_form": "좋아하는데 생토마토보단 퓨레넣는게 편해요",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100036167.1.13",
          "form": "그래서 퓨레로 끓인적이 있어요",
          "original_form": "그래서 퓨레로 끓인적이 있어요",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100036167.1.14",
          "form": "아 맛있겠네요 요리 좋아하시나요",
          "original_form": "아 맛있겠네요 요리 좋아하시나요",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036167.1.15",
          "form": "좋아하진 않는데 집밥 먹으려면 해야죠 ㅋㅋname1님은요?",
          "original_form": "좋아하진 않는데 집밥 먹으려면 해야죠 ㅋㅋ&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036167.1.16",
          "form": "저는 요리 잘 안해요 해도 파스타 대충해서 먹곤한답니다",
          "original_form": "저는 요리 잘 안해요 해도 파스타 대충해서 먹곤한답니다",
          "speaker_id": "1",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100036167.1.17",
          "form": "카레도 한번하면 오래먹으니 좋네요",
          "original_form": "카레도 한번하면 오래먹으니 좋네요",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036167.1.18",
          "form": "파스타 소스는 사드시나요?",
          "original_form": "파스타 소스는 사드시나요?",
          "speaker_id": "2",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100036167.1.19",
          "form": "네 거의 ㅋㅋㅋㅋ 면삶고 냉장고에 야채 털고 소스부어서 먹어요..",
          "original_form": "네 거의 ㅋㅋㅋㅋ 면삶고 냉장고에 야채 털고 소스부어서 먹어요..",
          "speaker_id": "1",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100036167.1.20",
          "form": "밀가루좀 줄여야할텐데 너무 편하네요",
          "original_form": "밀가루좀 줄여야할텐데 너무 편하네요",
          "speaker_id": "1",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100036167.1.21",
          "form": "아항,, 저도 그래요 토마토 소스파인가요?",
          "original_form": "아항,, 저도 그래요 토마토 소스파인가요?",
          "speaker_id": "2",
          "time": "20210830 18:03"
        },
        {
          "id": "MDRW2100036167.1.22",
          "form": "네 ㅋㅋㅋ 크림파스타는 그냥 우유에 치즈넣고 대충해먹고",
          "original_form": "네 ㅋㅋㅋ 크림파스타는 그냥 우유에 치즈넣고 대충해먹고",
          "speaker_id": "1",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100036167.1.23",
          "form": "소스는 파스타 주로 사게되더라구요",
          "original_form": "소스는 파스타 주로 사게되더라구요",
          "speaker_id": "1",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100036167.1.24",
          "form": "아니 토마토요 ㅋㅋㅋㅋ",
          "original_form": "아니 토마토요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100036167.1.25",
          "form": "토마토 소스가 야채재료 쓰기 좋아요",
          "original_form": "토마토 소스가 야채재료 쓰기 좋아요",
          "speaker_id": "2",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100036167.1.26",
          "form": "크림은 좀 ㅠ 아무거나 넣음 맛없어요",
          "original_form": "크림은 좀 ㅠ 아무거나 넣음 맛없어요",
          "speaker_id": "2",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100036167.1.27",
          "form": "맞아요 ㅋㅋㅋㅋ 저도 맛없는거 많이먹엇어요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 저도 맛없는거 많이먹엇어요",
          "speaker_id": "1",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100036167.1.28",
          "form": "크림은 버섯 양파 정도가 무난하구,,",
          "original_form": "크림은 버섯 양파 정도가 무난하구,,",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100036167.1.29",
          "form": "해물도 잘못넣음 비리구 그래요",
          "original_form": "해물도 잘못넣음 비리구 그래요",
          "speaker_id": "2",
          "time": "20210830 18:08"
        }
      ]
