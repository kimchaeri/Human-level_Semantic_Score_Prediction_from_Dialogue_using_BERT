      "utterance": [
        {
          "id": "MDRW2100006863.1.1",
          "form": "너무 많고 다 달라요",
          "original_form": "너무 많고 다 달라요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006863.1.2",
          "form": "저도 몇곳 다녀보았지요",
          "original_form": "저도 몇곳 다녀보았지요",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006863.1.3",
          "form": "저는 영진국밥요",
          "original_form": "저는 영진국밥요",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006863.1.4",
          "form": "거긴 어디에 있어요?",
          "original_form": "거긴 어디에 있어요?",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006863.1.5",
          "form": "신평이라고 일호선이요",
          "original_form": "신평이라고 일호선이요",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006863.1.6",
          "form": "수백도 있나요? 수육백반",
          "original_form": "수백도 있나요? 수육백반",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006863.1.7",
          "form": "그집이 가장 유명한게 수백이죠",
          "original_form": "그집이 가장 유명한게 수백이죠",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006863.1.8",
          "form": "그렇군요! 수백 좋아요 거길 가바야겠네요",
          "original_form": "그렇군요! 수백 좋아요 거길 가바야겠네요",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006863.1.9",
          "form": "본전도 유명해요",
          "original_form": "본전도 유명해요",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006863.1.10",
          "form": "본전은 부산역 쪽이지요 저도 알아요",
          "original_form": "본전은 부산역 쪽이지요 저도 알아요",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006863.1.11",
          "form": "네 여행객 맛집요",
          "original_form": "네 여행객 맛집요",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006863.1.12",
          "form": "거기 맛있던데요 좀 좁지만 전통있더라구요",
          "original_form": "거기 맛있던데요 좀 좁지만 전통있더라구요",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006863.1.13",
          "form": "급 국밥이 당깁니다",
          "original_form": "급 국밥이 당깁니다",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006863.1.14",
          "form": "국밥은 진짜 다양해요 부산은 돼지국밥이지만",
          "original_form": "국밥은 진짜 다양해요 부산은 돼지국밥이지만",
