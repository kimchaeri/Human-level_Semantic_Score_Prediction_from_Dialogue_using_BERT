      "utterance": [
        {
          "id": "MDRW2100025833.1.1",
          "form": "나 요새 방 꾸미는 거ㅋ큐ㅠㅠ 땜에 뭘 많이 사긴 했는데",
          "original_form": "나 요새 방 꾸미는 거ㅋ큐ㅠㅠ 땜에 뭘 많이 사긴 했는데",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025833.1.2",
          "form": "어제는 그거 샀다 그... 그 뭐지 방청소하는 건데",
          "original_form": "어제는 그거 샀다 그... 그 뭐지 방청소하는 건데",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025833.1.3",
          "form": "옹옹",
          "original_form": "옹옹",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025833.1.4",
          "form": "먼지를 일케 쓱 하면 먼지가 다 딸려오더라구",
          "original_form": "먼지를 일케 쓱 하면 먼지가 다 딸려오더라구",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025833.1.5",
          "form": "스무고개야???",
          "original_form": "스무고개야???",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025833.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025833.1.7",
          "form": "돌돌이?",
          "original_form": "돌돌이?",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025833.1.8",
          "form": "아니 돌돌이는 아니고",
          "original_form": "아니 돌돌이는 아니고",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025833.1.9",
          "form": "그 스위퍼?? 라는 거였는데",
          "original_form": "그 스위퍼?? 라는 거였는데",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025833.1.10",
          "form": "상품명이 정확하지 않다....",
          "original_form": "상품명이 정확하지 않다....",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025833.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아니 난 빵 산 거",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아니 난 빵 산 거",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025833.1.12",
          "form": "오늘 4시부터 6시까지 배송 예정이라는디",
          "original_form": "오늘 4시부터 6시까지 배송 예정이라는디",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025833.1.13",
          "form": "오질 않는댜...",
          "original_form": "오질 않는댜...",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025833.1.14",
          "form": "마따 빵 샀댓지 그 엘뤼게이러파이",
          "original_form": "마따 빵 샀댓지 그 엘뤼게이러파이",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025833.1.15",
          "form": "마싯겟다",
          "original_form": "마싯겟다",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025833.1.16",
          "form": "마자 그거 엘리게이터파이",
          "original_form": "마자 그거 엘리게이터파이",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025833.1.17",
          "form": "!!!!! 밤만쥬랑",
          "original_form": "!!!!! 밤만쥬랑",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025833.1.18",
          "form": "마자!!",
          "original_form": "마자!!",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025833.1.19",
          "form": "밤만쥬!!!!",
          "original_form": "밤만쥬!!!!",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025833.1.20",
          "form": "존맛탱이래서 기대중",
          "original_form": "존맛탱이래서 기대중",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025833.1.21",
          "form": "바...",
          "original_form": "바...",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025833.1.22",
          "form": "나 침흐른다",
          "original_form": "나 침흐른다",
