      "utterance": [
        {
          "id": "MDRW2100041370.1.1",
          "form": "먹을 얘기를 하니 끝이 없네요",
          "original_form": "먹을 얘기를 하니 끝이 없네요",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100041370.1.2",
          "form": "저는 지금 설렁탕을 먹고 있습니다",
          "original_form": "저는 지금 설렁탕을 먹고 있습니다",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100041370.1.3",
          "form": "국물이 끝내줘요",
          "original_form": "국물이 끝내줘요",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100041370.1.4",
          "form": "뭐 먹고 계신가요?",
          "original_form": "뭐 먹고 계신가요?",
          "speaker_id": "1",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100041370.1.5",
          "form": "음성인식이 갑자기 안돼서",
          "original_form": "음성인식이 갑자기 안돼서",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100041370.1.6",
          "form": "핸드폰 껐다 켰어요",
          "original_form": "핸드폰 껐다 켰어요",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100041370.1.7",
          "form": "아이고 괜찮으신가요",
          "original_form": "아이고 괜찮으신가요",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100041370.1.8",
          "form": "오늘 저녁은 뭘로 드셨나요",
          "original_form": "오늘 저녁은 뭘로 드셨나요",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100041370.1.9",
          "form": "점심에 점심에 너무 맛있게 먹었어요",
          "original_form": "점심에 점심에 너무 맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041370.1.10",
          "form": "그래서 저녁은 간단하게 먹을려고 했는데",
          "original_form": "그래서 저녁은 간단하게 먹을려고 했는데",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041370.1.11",
          "form": "실패했어요 저녁도 너무 맛있게 먹었어요",
          "original_form": "실패했어요 저녁도 너무 맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041370.1.12",
          "form": "점심은 뭘로 드셨는데요",
          "original_form": "점심은 뭘로 드셨는데요",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041370.1.13",
          "form": "점심에는 라볶이 해 먹었어요",
          "original_form": "점심에는 라볶이 해 먹었어요",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041370.1.14",
          "form": "아 진짜 맛있었겠다",
          "original_form": "아 진짜 맛있었겠다",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041370.1.15",
          "form": "저도 라볶이 좋아하는데",
          "original_form": "저도 라볶이 좋아하는데",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100041370.1.16",
          "form": "오뎅 중에 면처럼 길게 되어있는 오뎅이 있는데 그걸로 오뎅 국을 끓여 먹거나",
          "original_form": "오뎅 중에 면처럼 길게 되어있는 오뎅이 있는데 그걸로 오뎅 국을 끓여 먹거나",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100041370.1.17",
          "form": "홈플러스에서 세일해서 2990원 하더라구요",
          "original_form": "홈플러스에서 세일해서 2990원 하더라구요",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100041370.1.18",
          "form": "떡볶이 해 먹으면 좋은 맛입니다",
          "original_form": "떡볶이 해 먹으면 좋은 맛입니다",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100041370.1.19",
          "form": "금요일날 홈플러스에서 뭔가 많이 사왔어요",
          "original_form": "금요일날 홈플러스에서 뭔가 많이 사왔어요",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100041370.1.20",
          "form": "먹을 것이 많으셨네요",
          "original_form": "먹을 것이 많으셨네요",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100041370.1.21",
          "form": "아 그런 오뎅이 있군요",
          "original_form": "아 그런 오뎅이 있군요",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041370.1.22",
          "form": "근데 다 먹었어요",
          "original_form": "근데 다 먹었어요",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041370.1.23",
          "form": "이제 옥수수만 남았어요",
          "original_form": "이제 옥수수만 남았어요",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041370.1.24",
          "form": "초당옥수수만 남은건가요",
          "original_form": "초당옥수수만 남은건가요",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041370.1.25",
          "form": "저 어제 전어 시켜 먹었잖아요",
          "original_form": "저 어제 전어 시켜 먹었잖아요",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041370.1.26",
          "form": "일할 때 저녁으로 한 개씩  먹으려구요",
          "original_form": "일할 때 저녁으로 한 개씩  먹으려구요",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041370.1.27",
          "form": "전어회가 너무 고소하고 맛이 있어서 수리 벌컥 벌컥 들어갔어요",
          "original_form": "전어회가 너무 고소하고 맛이 있어서 수리 벌컥 벌컥 들어갔어요",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041370.1.28",
          "form": "그렇죠 즐거운 주말을 보냈네요",
          "original_form": "그렇죠 즐거운 주말을 보냈네요",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041370.1.29",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210905 22:46"
        }
      ]
    }
