      "utterance": [
        {
          "id": "MDRW2100004771.1.1",
          "form": "티비봐요?",
          "original_form": "티비봐요?",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004771.1.2",
          "form": "티비는 안보고 넷플릭스나 왓챠를 즐겨봐요.",
          "original_form": "티비는 안보고 넷플릭스나 왓챠를 즐겨봐요.",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004771.1.3",
          "form": "즐겨보는 프로그램있으세요?",
          "original_form": "즐겨보는 프로그램있으세요?",
          "speaker_id": "2",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004771.1.4",
          "form": "런닝맨요",
          "original_form": "런닝맨요",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004771.1.5",
          "form": "저는 대탈출을 자주봐요 본적있어요?",
          "original_form": "저는 대탈출을 자주봐요 본적있어요?",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004771.1.6",
          "form": "잼나요?",
          "original_form": "잼나요?",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004771.1.7",
          "form": "네 방탈출프로그램인데 재미있어요 ㅎ",
          "original_form": "네 방탈출프로그램인데 재미있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004771.1.8",
          "form": "ㅎㅎ 그런가여?",
          "original_form": "ㅎㅎ 그런가여?",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004771.1.9",
          "form": "유투브로도 볼수있어요",
          "original_form": "유투브로도 볼수있어요",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004771.1.10",
          "form": "네 한번봐야지요",
          "original_form": "네 한번봐야지요",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004771.1.11",
          "form": "혹시 영화좋아하시나요?",
          "original_form": "혹시 영화좋아하시나요?",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004771.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004771.1.13",
          "form": "추천해주실것이있나요",
          "original_form": "추천해주실것이있나요",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004771.1.14",
          "form": "모가디슈 못봤어요 하지만 들었죠",
          "original_form": "모가디슈 못봤어요 하지만 들었죠",
