      "utterance": [
        {
          "id": "MDRW2100042196.1.1",
          "form": "밥은먹었어?",
          "original_form": "밥은먹었어?",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042196.1.2",
          "form": "응! 오늘 저녁은 치킨 마요 덮밥이었어",
          "original_form": "응! 오늘 저녁은 치킨 마요 덮밥이었어",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042196.1.3",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042196.1.4",
          "form": "나는순대볶음먹었어",
          "original_form": "나는순대볶음먹었어",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042196.1.5",
          "form": "사먹은거야?",
          "original_form": "사먹은거야?",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042196.1.6",
          "form": "아니아니 기숙사 식단!",
          "original_form": "아니아니 기숙사 식단!",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042196.1.7",
          "form": "크 오늘 치킨 튀김이 아주 빠삭빠삭하니 맛있더라",
          "original_form": "크 오늘 치킨 튀김이 아주 빠삭빠삭하니 맛있더라",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042196.1.8",
          "form": "기숙사사는구낭",
          "original_form": "기숙사사는구낭",
          "speaker_id": "1",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042196.1.9",
          "form": "오 당긴다",
          "original_form": "오 당긴다",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042196.1.10",
          "form": "밥잘나와?",
          "original_form": "밥잘나와?",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042196.1.11",
          "form": "나름...? 일단 긱사 치곤 잘 나와",
          "original_form": "나름...? 일단 긱사 치곤 잘 나와",
          "speaker_id": "2",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042196.1.12",
          "form": "크 뭣버다 배달음식이 짱인데",
          "original_form": "크 뭣버다 배달음식이 짱인데",
          "speaker_id": "2",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100042196.1.13",
          "form": "ㅎㅎㅎ뭐좋아해",
          "original_form": "ㅎㅎㅎ뭐좋아해",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042196.1.14",
          "form": "난.. 음 치킨도 좋지만 마라탕........",
          "original_form": "난.. 음 치킨도 좋지만 마라탕........",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042196.1.15",
          "form": "마라탕은 배달로 시켜먹기보단 가서먹는 게 짱이지만 말야",
          "original_form": "마라탕은 배달로 시켜먹기보단 가서먹는 게 짱이지만 말야",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042196.1.16",
          "form": "그치 고를수있지",
          "original_form": "그치 고를수있지",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042196.1.17",
          "form": "당긴다..",
          "original_form": "당긴다..",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042196.1.18",
          "form": "하 맛있지 마라탕...",
          "original_form": "하 맛있지 마라탕...",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042196.1.19",
          "form": "넌 뭐 좋아하는데?",
          "original_form": "넌 뭐 좋아하는데?",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042196.1.20",
          "form": "난 초밥이나",
          "original_form": "난 초밥이나",
          "speaker_id": "1",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042196.1.21",
          "form": "샐러드?ㅎㅎ토핑많은거",
          "original_form": "샐러드?ㅎㅎ토핑많은거",
          "speaker_id": "1",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042196.1.22",
          "form": "아님디저트",
          "original_form": "아님디저트",
          "speaker_id": "1",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042196.1.23",
          "form": "헐 샐러드 좋아해??",
          "original_form": "헐 샐러드 좋아해??",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042196.1.24",
          "form": "하긴 요즘 많이 먹드라....",
          "original_form": "하긴 요즘 많이 먹드라....",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042196.1.25",
          "form": "나두 디저트 좋아해 난 특히 앙버터",
          "original_form": "나두 디저트 좋아해 난 특히 앙버터",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042196.1.26",
          "form": "아이 앙버터 들어간 다쿠아즈",
          "original_form": "아이 앙버터 들어간 다쿠아즈",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042196.1.27",
          "form": "우리 동네에 다쿠아즈 맛집이 있거든",
          "original_form": "우리 동네에 다쿠아즈 맛집이 있거든",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042196.1.28",
          "form": "오 맛있지",
          "original_form": "오 맛있지",
          "speaker_id": "1",
          "time": "20210906 21:16"
        }
      ]
    }
