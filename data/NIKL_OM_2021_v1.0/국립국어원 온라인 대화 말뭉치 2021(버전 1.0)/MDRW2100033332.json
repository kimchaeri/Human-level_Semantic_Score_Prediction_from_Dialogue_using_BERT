      "utterance": [
        {
          "id": "MDRW2100033332.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033332.1.2",
          "form": "애인 있으신가요",
          "original_form": "애인 있으신가요",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033332.1.3",
          "form": "저스틴이라구 비버예요",
          "original_form": "저스틴이라구 비버예요",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033332.1.4",
          "form": "저도 비버인데",
          "original_form": "저도 비버인데",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033332.1.5",
          "form": "운명인가봐요",
          "original_form": "운명인가봐요",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033332.1.6",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100033332.1.7",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100033332.1.8",
          "form": "전 저한테만따뜻한 짐승남이 좋아요",
          "original_form": "전 저한테만따뜻한 짐승남이 좋아요",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033332.1.9",
          "form": "외적으로는요?",
          "original_form": "외적으로는요?",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033332.1.10",
          "form": "눈썹이 진한 사람이요",
          "original_form": "눈썹이 진한 사람이요",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033332.1.11",
          "form": "그렇게 그윽해보이더라고요",
          "original_form": "그렇게 그윽해보이더라고요",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100033332.1.12",
          "form": "최근 미팅상대로는 이상형을 만나지 못하셨나요?",
          "original_form": "최근 미팅상대로는 이상형을 만나지 못하셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100033332.1.13",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100033332.1.14",
          "form": "주선해 주시게요?",
          "original_form": "주선해 주시게요?",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100033332.1.15",
          "form": "아뇨... 저도 이상형을 못 만난 지라 여유가 없네요",
          "original_form": "아뇨... 저도 이상형을 못 만난 지라 여유가 없네요",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100033332.1.16",
          "form": "요즘은 비버랑 데이트만 하고 있어요",
          "original_form": "요즘은 비버랑 데이트만 하고 있어요",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100033332.1.17",
          "form": "비버씨는 이상형이 아닌가요?",
          "original_form": "비버씨는 이상형이 아닌가요?",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100033332.1.18",
          "form": "네 아무래도 제 이상형과는 거리가 좀 있더라구요",
          "original_form": "네 아무래도 제 이상형과는 거리가 좀 있더라구요",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033332.1.19",
          "form": "전 키 큰 사람이 좋거든요",
          "original_form": "전 키 큰 사람이 좋거든요",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033332.1.20",
          "form": "취향이",
          "original_form": "취향이",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033332.1.21",
          "form": "기린남",
          "original_form": "기린남",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033332.1.22",
          "form": "이신가봐요",
          "original_form": "이신가봐요",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033332.1.23",
          "form": "목 긴 사람은 벌로구.. 약간 타조?남이요",
          "original_form": "목 긴 사람은 벌로구.. 약간 타조?남이요",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100033332.1.24",
          "form": "다리가 길었으면 좋겠어요",
          "original_form": "다리가 길었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100033332.1.25",
          "form": "타조도",
          "original_form": "타조도",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100033332.1.26",
          "form": "목이 길지않나요?",
          "original_form": "목이 길지않나요?",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100033332.1.27",
          "form": "대머리같고..",
          "original_form": "대머리같고..",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100033332.1.28",
          "form": "왜 제 상상을 깨트리세요...",
          "original_form": "왜 제 상상을 깨트리세요...",
          "speaker_id": "2",
          "time": "20210906 22:31"
        }
      ]
    }
