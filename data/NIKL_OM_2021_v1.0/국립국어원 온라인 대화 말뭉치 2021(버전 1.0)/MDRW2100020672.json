      "utterance": [
        {
          "id": "MDRW2100020672.1.1",
          "form": "여고추리반 흥해서",
          "original_form": "여고추리반 흥해서",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020672.1.2",
          "form": "시즌 투도 나온다고 하더라구여",
          "original_form": "시즌 투도 나온다고 하더라구여",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020672.1.3",
          "form": "헐 너무 좋아요 !!!",
          "original_form": "헐 너무 좋아요 !!!",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020672.1.4",
          "form": "멤버도 그대로 가는걸까요 ??",
          "original_form": "멤버도 그대로 가는걸까요 ??",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020672.1.5",
          "form": "멤버 아마 그대로 가는것같아용!",
          "original_form": "멤버 아마 그대로 가는것같아용!",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020672.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020672.1.7",
          "form": "대탈출 또 재밌었던 편이",
          "original_form": "대탈출 또 재밌었던 편이",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020672.1.8",
          "form": "태양여고도 재밌었어용",
          "original_form": "태양여고도 재밌었어용",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.9",
          "form": "좋네요 !!",
          "original_form": "좋네요 !!",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.10",
          "form": "와 태양여고 !",
          "original_form": "와 태양여고 !",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.11",
          "form": "저 그거는 보다 말은 것 같아요",
          "original_form": "저 그거는 보다 말은 것 같아요",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.12",
          "form": "끝까지 봐야겠네요",
          "original_form": "끝까지 봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.13",
          "form": "이것도 나중에 또 연장선으로",
          "original_form": "이것도 나중에 또 연장선으로",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.14",
          "form": "그건 많이 안무섭나요 ...??",
          "original_form": "그건 많이 안무섭나요 ...??",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.15",
          "form": "이어지는 회차가 나오더라구용",
          "original_form": "이어지는 회차가 나오더라구용",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.16",
          "form": "근데 결말을 이미 다 알아서 ...",
          "original_form": "근데 결말을 이미 다 알아서 ...",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.17",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.18",
          "form": "이거는 약간",
          "original_form": "이거는 약간",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.19",
          "form": "오 진짜요 ??",
          "original_form": "오 진짜요 ??",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.20",
          "form": "사이비쪽이라",
          "original_form": "사이비쪽이라",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.21",
          "form": "무섭다기보다는",
          "original_form": "무섭다기보다는",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.22",
          "form": "이상하다? 느낌이에요ㅋㅋㅋ",
          "original_form": "이상하다? 느낌이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020672.1.23",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020672.1.24",
          "form": "그 교장이 짱이라면서요",
          "original_form": "그 교장이 짱이라면서요",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020672.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020672.1.26",
          "form": "무슨 교죠",
          "original_form": "무슨 교죠",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020672.1.27",
          "form": "계수상 선생님",
          "original_form": "계수상 선생님",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020672.1.28",
          "form": "개수상한 계수상 선생님ㅋㅋㅋㅋ",
          "original_form": "개수상한 계수상 선생님ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020672.1.29",
          "form": "npc 이름도 잘짓는것같아요",
          "original_form": "npc 이름도 잘짓는것같아요",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020672.1.30",
          "form": "아 ㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌ 아 미쳐욭ㅌㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌㅌ 아 미쳐욭ㅌㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020672.1.31",
          "form": "진짜 천재예요",
          "original_form": "진짜 천재예요",
          "speaker_id": "2",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020672.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ 비슷한 계열로",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 비슷한 계열로",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020672.1.33",
          "form": "크라임씬도 재밌게 봤는데",
          "original_form": "크라임씬도 재밌게 봤는데",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020672.1.34",
          "form": "헝 저두요",
          "original_form": "헝 저두요",
          "speaker_id": "2",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020672.1.35",
          "form": "존버단만 늘어나고",
          "original_form": "존버단만 늘어나고",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020672.1.36",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020672.1.37",
          "form": "크라임씬은 다봣ㄱ어요",
          "original_form": "크라임씬은 다봣ㄱ어요",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.38",
          "form": "박강남편이 제일 소름이었어요",
          "original_form": "박강남편이 제일 소름이었어요",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.39",
          "form": "그러게요 .. 언제 낭ㅅ려나 ...",
          "original_form": "그러게요 .. 언제 낭ㅅ려나 ...",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.40",
          "form": "와 진짜 인정요",
          "original_form": "와 진짜 인정요",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.41",
          "form": "그 감독님이 추리를 완벽하게하셔서",
          "original_form": "그 감독님이 추리를 완벽하게하셔서",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.42",
          "form": "볼줄 아시는 분이시군요!ㅋㅋㅋㅋ",
          "original_form": "볼줄 아시는 분이시군요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.43",
          "form": "진짜 더 재밌었어요",
          "original_form": "진짜 더 재밌었어요",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.44",
          "form": "맞아요 이거보고 아 장진감독님은 꼭 나오셔야겠다",
          "original_form": "맞아요 이거보고 아 장진감독님은 꼭 나오셔야겠다",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.45",
          "form": "생각했어요",
          "original_form": "생각했어요",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.46",
          "form": "박지윤님도 연기 너무 잘하셔서 ...",
          "original_form": "박지윤님도 연기 너무 잘하셔서 ...",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.47",
          "form": "맞아요 진짜 ...",
          "original_form": "맞아요 진짜 ...",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020672.1.48",
          "form": "여고추리반에서도 하드캐리해요ㅋㅋㅋㅋㅋ",
          "original_form": "여고추리반에서도 하드캐리해요ㅋㅋㅋㅋㅋ",
