      "utterance": [
        {
          "id": "MDRW2100006109.1.1",
          "form": "치킨먹었습니다 ㅎㅎ",
          "original_form": "치킨먹었습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006109.1.2",
          "form": "부럽네요...맛있는치킨..어디걸로??",
          "original_form": "부럽네요...맛있는치킨..어디걸로??",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006109.1.3",
          "form": "푸라닭이요",
          "original_form": "푸라닭이요",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006109.1.4",
          "form": "블랙마요",
          "original_form": "블랙마요",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006109.1.5",
          "form": "먹었습니다",
          "original_form": "먹었습니다",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006109.1.6",
          "form": "아..궁금한 맛인데 아직 못먹어봤거든요..",
          "original_form": "아..궁금한 맛인데 아직 못먹어봤거든요..",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006109.1.7",
          "form": "한번 드셔보세요",
          "original_form": "한번 드셔보세요",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006109.1.8",
          "form": "맛있습니다",
          "original_form": "맛있습니다",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006109.1.9",
          "form": "네 추천감사합니다 평소에도 푸라닭을 즐겨드시나요?",
          "original_form": "네 추천감사합니다 평소에도 푸라닭을 즐겨드시나요?",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100006109.1.10",
          "form": "네 즐겨먹구",
          "original_form": "네 즐겨먹구",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006109.1.11",
          "form": "교촌도 즐겨먹고",
          "original_form": "교촌도 즐겨먹고",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100006109.1.12",
          "form": "가끔 bnc뿌링클?",
          "original_form": "가끔 bnc뿌링클?",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100006109.1.13",
          "form": "도 먹어욯ㅎ",
          "original_form": "도 먹어욯ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100006109.1.14",
          "form": "저도 교촌 허니콤보도 좋아합니다 ㅎㅎ",
          "original_form": "저도 교촌 허니콤보도 좋아합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100006109.1.15",
          "form": "ㅋㅋㅋ 국민메뉴죠 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 국민메뉴죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006109.1.16",
          "form": "싫어하는사람 거의 없을거에요",
          "original_form": "싫어하는사람 거의 없을거에요",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006109.1.17",
          "form": "교촌을 즐겨드세요?",
          "original_form": "교촌을 즐겨드세요?",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006109.1.18",
          "form": "자주먹으면 물리는 맛이라..ㅋ 최애?는 비비큐 후라이드예요.....ㅎㅎ",
          "original_form": "자주먹으면 물리는 맛이라..ㅋ 최애?는 비비큐 후라이드예요.....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006109.1.19",
          "form": "아 그냥 후라이드 좋아하시는구나",
          "original_form": "아 그냥 후라이드 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006109.1.20",
          "form": "단백한거",
          "original_form": "단백한거",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006109.1.21",
          "form": "네 다른것들은 첫맛은 맛있는데 먹다보면 질리더라구요",
          "original_form": "네 다른것들은 첫맛은 맛있는데 먹다보면 질리더라구요",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100006109.1.22",
          "form": "후라이드는 계속 먹다보면",
          "original_form": "후라이드는 계속 먹다보면",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006109.1.23",
          "form": "느끼하지 않나요?",
          "original_form": "느끼하지 않나요?",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006109.1.24",
          "form": "전 느끼할까봐",
