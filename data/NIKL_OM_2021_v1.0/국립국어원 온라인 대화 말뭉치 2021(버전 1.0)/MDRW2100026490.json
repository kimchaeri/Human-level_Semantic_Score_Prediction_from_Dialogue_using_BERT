      "utterance": [
        {
          "id": "MDRW2100026490.1.1",
          "form": "노티드도넛 드셔보셨어요?",
          "original_form": "노티드도넛 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026490.1.2",
          "form": "저는 저번주에 처음 먹어봤어요",
          "original_form": "저는 저번주에 처음 먹어봤어요",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026490.1.3",
          "form": "아니요 줄이 항상 어마오마해서",
          "original_form": "아니요 줄이 항상 어마오마해서",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026490.1.4",
          "form": "엄듀를 못냇어요ㅠㅠ",
          "original_form": "엄듀를 못냇어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026490.1.5",
          "form": "오때요",
          "original_form": "오때요",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026490.1.6",
          "form": "와 저 포장줄로 한시간 더걸렸어요",
          "original_form": "와 저 포장줄로 한시간 더걸렸어요",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026490.1.7",
          "form": "평일 낮이었는데",
          "original_form": "평일 낮이었는데",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026490.1.8",
          "form": "훠...진짜 ㅋㅋㅋㅋ 인기많을만한가요",
          "original_form": "훠...진짜 ㅋㅋㅋㅋ 인기많을만한가요",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026490.1.9",
          "form": "먹고싶으면 배달이 빠를거 같고 그시간들여서는 안먹을래여",
          "original_form": "먹고싶으면 배달이 빠를거 같고 그시간들여서는 안먹을래여",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026490.1.10",
          "form": "여섯개 샀는데 이만원 걍넘더라구여",
          "original_form": "여섯개 샀는데 이만원 걍넘더라구여",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026490.1.11",
          "form": "ㅋㅋㅋ 배달도 하긴 하나보네요",
          "original_form": "ㅋㅋㅋ 배달도 하긴 하나보네요",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026490.1.12",
          "form": "도넛주제에",
          "original_form": "도넛주제에",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026490.1.13",
          "form": "ㅠㅠ넘비싸",
          "original_form": "ㅠㅠ넘비싸",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026490.1.14",
          "form": "배달땜에 웨이팅이 긴거같아요",
          "original_form": "배달땜에 웨이팅이 긴거같아요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026490.1.15",
          "form": "계산하면 수령까지 20분 걸린대여ㅋㅋ",
          "original_form": "계산하면 수령까지 20분 걸린대여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026490.1.16",
          "form": "아 진짴ㅋㅋㅋㅋ",
          "original_form": "아 진짴ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026490.1.17",
          "form": "뭐하러배달까지하지 ㅋㅋㅋ",
          "original_form": "뭐하러배달까지하지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026490.1.18",
          "form": "ㄱ거기사장 부럽네요",
          "original_form": "ㄱ거기사장 부럽네요",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026490.1.19",
          "form": "걍 던킨먹는게ㅋㅋㅋ",
          "original_form": "걍 던킨먹는게ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026490.1.20",
          "form": "ㅋㅋㅋㅋㅋ꽤됏는데 인기가 안식는게 신기하네요",
          "original_form": "ㅋㅋㅋㅋㅋ꽤됏는데 인기가 안식는게 신기하네요",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026490.1.21",
          "form": "저도 놀랬어요",
          "original_form": "저도 놀랬어요",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026490.1.22",
          "form": "다음엔 랜디스를 먹어봐야겠어요ㅋ",
          "original_form": "다음엔 랜디스를 먹어봐야겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026490.1.23",
          "form": "혹시 노티드 드실거면 배달 추천드려요",
          "original_form": "혹시 노티드 드실거면 배달 추천드려요",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100026490.1.24",
          "form": "언젠가 헌번 가봐야지ㅠㅠ",
          "original_form": "언젠가 헌번 가봐야지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026490.1.25",
          "form": "다들 먹는데 진심인가바여",
          "original_form": "다들 먹는데 진심인가바여",
          "speaker_id": "1",
          "time": "20210901 22:45"
        }
      ]
    }
  ]
