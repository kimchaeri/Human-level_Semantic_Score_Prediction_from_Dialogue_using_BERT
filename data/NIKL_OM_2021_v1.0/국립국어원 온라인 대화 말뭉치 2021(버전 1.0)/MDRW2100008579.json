      "utterance": [
        {
          "id": "MDRW2100008579.1.1",
          "form": "영화 보는거 좋아하시나요",
          "original_form": "영화 보는거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008579.1.2",
          "form": "영화 보는거 좋아해요",
          "original_form": "영화 보는거 좋아해요",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008579.1.3",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008579.1.4",
          "form": "스릴리물이나 액션 좋아해요",
          "original_form": "스릴리물이나 액션 좋아해요",
          "speaker_id": "2",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008579.1.5",
          "form": "저는 히어로물 좋아해요 아무 생각 없이 보기 좋아서 ㅋㅋ",
          "original_form": "저는 히어로물 좋아해요 아무 생각 없이 보기 좋아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008579.1.6",
          "form": "그렇군요 히어로 물에서는 역시나 mcu가 최고 아닐까요",
          "original_form": "그렇군요 히어로 물에서는 역시나 mcu가 최고 아닐까요",
          "speaker_id": "2",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008579.1.7",
          "form": "그런 것 같아요 덕후 까지는 아니지만 개봉하면 보게 되는 그런 ㅋㅋ",
          "original_form": "그런 것 같아요 덕후 까지는 아니지만 개봉하면 보게 되는 그런 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008579.1.8",
          "form": "코로나 때문에 보기 힘든것이 문제지만요",
          "original_form": "코로나 때문에 보기 힘든것이 문제지만요",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008579.1.9",
          "form": "맞아요 코로나 이후로 영화 한번도 못 봤어요",
          "original_form": "맞아요 코로나 이후로 영화 한번도 못 봤어요",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008579.1.10",
          "form": "그러죠 그래도 블랙윈도우 개봉했으니 다행이에요",
          "original_form": "그러죠 그래도 블랙윈도우 개봉했으니 다행이에요",
          "speaker_id": "2",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008579.1.11",
          "form": "마블 영화는 극장에서 보는 맛이 있는데..",
          "original_form": "마블 영화는 극장에서 보는 맛이 있는데..",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008579.1.12",
          "form": "아이맥스 보신 적 있으세요?",
          "original_form": "아이맥스 보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008579.1.13",
          "form": "네 본적있죠",
          "original_form": "네 본적있죠",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008579.1.14",
          "form": "오 저는 한번도 본적이 없어서 ㅋㅋ 확실히 다른가요",
          "original_form": "오 저는 한번도 본적이 없어서 ㅋㅋ 확실히 다른가요",
          "speaker_id": "1",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008579.1.15",
          "form": "네 확실히 달라요",
          "original_form": "네 확실히 달라요",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008579.1.16",
          "form": "가서 직접 보시면  알거에요",
          "original_form": "가서 직접 보시면  알거에요",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008579.1.17",
          "form": "아이맥스 관 자체가 별로 없어서 ㅠ",
          "original_form": "아이맥스 관 자체가 별로 없어서 ㅠ",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008579.1.18",
          "form": "고생하시네요",
          "original_form": "고생하시네요",
          "speaker_id": "2",
