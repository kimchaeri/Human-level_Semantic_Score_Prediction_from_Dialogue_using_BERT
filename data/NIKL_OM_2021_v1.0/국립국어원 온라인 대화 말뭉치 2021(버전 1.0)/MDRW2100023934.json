      "utterance": [
        {
          "id": "MDRW2100023934.1.1",
          "form": "버블티 좋아하세요?",
          "original_form": "버블티 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023934.1.2",
          "form": "버블티 좋아요 ㅋㅋㅋ",
          "original_form": "버블티 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023934.1.3",
          "form": "공차가시면 커스텀 어떻게 해드세용?",
          "original_form": "공차가시면 커스텀 어떻게 해드세용?",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023934.1.4",
          "form": "전 타로에 펄 추가하고 얼음적게 당도 70으로 해서먹어용!",
          "original_form": "전 타로에 펄 추가하고 얼음적게 당도 70으로 해서먹어용!",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023934.1.5",
          "form": "타피오카 펄을 두번 추가하는게 포인트입니당",
          "original_form": "타피오카 펄을 두번 추가하는게 포인트입니당",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023934.1.6",
          "form": "ㅋㅋㅋ구체적인 디테일이네용",
          "original_form": "ㅋㅋㅋ구체적인 디테일이네용",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023934.1.7",
          "form": "저는 허니밀크티 얼음보통 당도 30으로요 ㅎㅎ",
          "original_form": "저는 허니밀크티 얼음보통 당도 30으로요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023934.1.8",
          "form": "허니밀크티는 한번도 안마셔봤는대",
          "original_form": "허니밀크티는 한번도 안마셔봤는대",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023934.1.9",
          "form": "가끔 알로에펄 추가해 먹어요",
          "original_form": "가끔 알로에펄 추가해 먹어요",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023934.1.10",
          "form": "무슨맛이에요?",
          "original_form": "무슨맛이에요?",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023934.1.11",
          "form": "그냥 홍차맛이에요:)",
          "original_form": "그냥 홍차맛이에요:)",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023934.1.12",
          "form": "설탕대신 꿀로 당도 맞춘거요!",
          "original_form": "설탕대신 꿀로 당도 맞춘거요!",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023934.1.13",
          "form": "아하 달달한 홍차맛이구나",
          "original_form": "아하 달달한 홍차맛이구나",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023934.1.14",
          "form": "맛이 없을수가 없네요ㅋㅋㅋ",
          "original_form": "맛이 없을수가 없네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023934.1.15",
          "form": "갑자기 배고파지네요 ㅋㅋㅋㅋㅋㅋ디저트배",
          "original_form": "갑자기 배고파지네요 ㅋㅋㅋㅋㅋㅋ디저트배",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023934.1.16",
          "form": "그러게요ㅋㅋㅋ",
          "original_form": "그러게요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023934.1.17",
          "form": "달달한거 먹고싶어요ㅋㅋㅋ",
          "original_form": "달달한거 먹고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023934.1.18",
          "form": "디저트맛집 좋아하는곳 있으세요?",
          "original_form": "디저트맛집 좋아하는곳 있으세요?",
          "speaker_id": "2",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023934.1.19",
          "form": "저는 동네 디저트집인데",
          "original_form": "저는 동네 디저트집인데",
          "speaker_id": "1",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100023934.1.20",
          "form": "뚱가롱 맛집이라 종종 사먹어용ㅎㅎ",
          "original_form": "뚱가롱 맛집이라 종종 사먹어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100023934.1.21",
          "form": "name2님은용?",
          "original_form": "&name2&님은용?",
          "speaker_id": "1",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100023934.1.22",
          "form": "저는 스콘 좋아해요 ㅎㅎ",
          "original_form": "저는 스콘 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023934.1.23",
          "form": "아쉽게도 저희동네엔 스콘 맛집이 없더라구요....",
          "original_form": "아쉽게도 저희동네엔 스콘 맛집이 없더라구요....",
