        },
        {
          "id": "MDRW2100018792.1.3",
          "form": "못들죠 일단",
          "original_form": "못들죠 일단",
          "speaker_id": "1",
          "time": "20210827 12:10"
        },
        {
          "id": "MDRW2100018792.1.4",
          "form": "그렇긴해요",
          "original_form": "그렇긴해요",
          "speaker_id": "1",
          "time": "20210827 12:10"
        },
        {
          "id": "MDRW2100018792.1.5",
          "form": "그쵸 이미 그 아이도",
          "original_form": "그쵸 이미 그 아이도",
          "speaker_id": "2",
          "time": "20210827 12:10"
        },
        {
          "id": "MDRW2100018792.1.6",
          "form": "하지만 애기인걸요",
          "original_form": "하지만 애기인걸요",
          "speaker_id": "2",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018792.1.7",
          "form": "몇살이죠",
          "original_form": "몇살이죠",
          "speaker_id": "2",
          "time": "20210827 12:11"
        },
        {
