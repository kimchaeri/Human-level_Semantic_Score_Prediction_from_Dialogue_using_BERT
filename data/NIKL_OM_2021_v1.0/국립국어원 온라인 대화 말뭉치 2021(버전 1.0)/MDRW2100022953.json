        },
        {
          "id": "MDRW2100022953.1.3",
          "form": "name1은요??",
          "original_form": "&name1&은요??",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022953.1.4",
          "form": "아 당근..",
          "original_form": "아 당근..",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022953.1.5",
          "form": "저는 닭발이랑 곱창 같은거 ㅋㅋㅋ",
          "original_form": "저는 닭발이랑 곱창 같은거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022953.1.6",
          "form": "아 내장같은거?",
          "original_form": "아 내장같은거?",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022953.1.7",
          "form": "네 맞아요 ㅋㅋㅋ 알탕의 알은 또 좋아하는데",
          "original_form": "네 맞아요 ㅋㅋㅋ 알탕의 알은 또 좋아하는데",
          "speaker_id": "1",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022953.1.8",
          "form": "미끌미끌한 애들은 약해요 ㅋㅋㅋ",
          "original_form": "미끌미끌한 애들은 약해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022953.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022953.1.10",
          "form": "맞아요..못드시는분들 사실 엄청많더라구요",
          "original_form": "맞아요..못드시는분들 사실 엄청많더라구요",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022953.1.11",
          "form": "미끌미끌~",
          "original_form": "미끌미끌~",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022953.1.12",
          "form": "잘 드시나요?? ㅋㅋㅋ 곱창 근데 구워진거보면 뭔가 맛있게 보이기는 해요 ㅋㅋㅋ",
          "original_form": "잘 드시나요?? ㅋㅋㅋ 곱창 근데 구워진거보면 뭔가 맛있게 보이기는 해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022953.1.13",
          "form": "닭껍질도안드세요?",
          "original_form": "닭껍질도안드세요?",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022953.1.14",
          "form": "저는다잘먹어요...",
          "original_form": "저는다잘먹어요...",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022953.1.15",
          "form": "아 닭껍질은 괜찮아요 바삭하면 ㅋㅋㅋ",
          "original_form": "아 닭껍질은 괜찮아요 바삭하면 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022953.1.16",
          "form": "ㅋㅋㅋㅋ백숙",
          "original_form": "ㅋㅋㅋㅋ백숙",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022953.1.17",
          "form": "아 백숙 좋죠 크으",
          "original_form": "아 백숙 좋죠 크으",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022953.1.18",
          "form": "구수한거 좋아해요 우러나는거 ㅋㅋㅋㅋ",
          "original_form": "구수한거 좋아해요 우러나는거 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022953.1.19",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022953.1.20",
          "form": "ㅋㅋㅋㅋㅋ하...",
          "original_form": "ㅋㅋㅋㅋㅋ하...",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022953.1.21",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022953.1.22",
          "form": "아 이제 먹어야되는데 귀찮네요",
          "original_form": "아 이제 먹어야되는데 귀찮네요",
          "speaker_id": "1",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022953.1.23",
          "form": "드셨나요",
          "original_form": "드셨나요",
          "speaker_id": "1",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022953.1.24",
          "form": "ㅋㅋㅋㅋ저도 딱 그상태에요",
          "original_form": "ㅋㅋㅋㅋ저도 딱 그상태에요",
          "speaker_id": "2",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022953.1.25",
          "form": "여름에 입맛이 없더라고요 ㅠㅠ",
          "original_form": "여름에 입맛이 없더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022953.1.26",
          "form": "올여름에너무더웠죠 ..ㅠㅠ",
          "original_form": "올여름에너무더웠죠 ..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022953.1.27",
          "form": "그니까요.. 그냥 물말아서 오이지 얹어먹고 그랬네요 휴",
          "original_form": "그니까요.. 그냥 물말아서 오이지 얹어먹고 그랬네요 휴",
          "speaker_id": "1",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022953.1.28",
          "form": "아..그거라도먹을까요",
          "original_form": "아..그거라도먹을까요",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022953.1.29",
          "form": "일단전뭐가먹고싶긴한데 처리기가귀찬ㅅ아요..",
          "original_form": "일단전뭐가먹고싶긴한데 처리기가귀찬ㅅ아요..",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022953.1.30",
