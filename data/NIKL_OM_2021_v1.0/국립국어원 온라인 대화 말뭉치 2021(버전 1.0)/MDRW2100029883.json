        },
        {
          "id": "MDRW2100029883.1.3",
          "form": "김밥만삿을리가엄는데요",
          "original_form": "김밥만삿을리가엄는데요",
          "speaker_id": "1",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029883.1.4",
          "form": "김밥하고 커피샀어요",
          "original_form": "김밥하고 커피샀어요",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029883.1.5",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029883.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029883.1.7",
          "form": "저는 단백질쉐이크먹엇어욤",
          "original_form": "저는 단백질쉐이크먹엇어욤",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029883.1.8",
          "form": "퇴근하고",
          "original_form": "퇴근하고",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029883.1.9",
          "form": "묭실가여",
          "original_form": "묭실가여",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029883.1.10",
          "form": "맛있나요",
          "original_form": "맛있나요",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029883.1.11",
          "form": "혼자갈수있나요",
          "original_form": "혼자갈수있나요",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029883.1.12",
          "form": "걍그래요 \r\n혼자못가는데",
          "original_form": "걍그래요 \r\n혼자못가는데",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029883.1.13",
          "form": "남친이 방해하믄안대요",
          "original_form": "남친이 방해하믄안대요",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029883.1.14",
          "form": "중요한시험앞두고잇서여",
          "original_form": "중요한시험앞두고잇서여",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029883.1.15",
          "form": "혼자해야해요",
          "original_form": "혼자해야해요",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029883.1.16",
          "form": "슬프지만",
          "original_form": "슬프지만",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029883.1.17",
          "form": "고고합니다",
          "original_form": "고고합니다",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029883.1.18",
          "form": "이쁘네요",
          "original_form": "이쁘네요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029883.1.19",
          "form": "내일 머드실건가요",
          "original_form": "내일 머드실건가요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029883.1.20",
          "form": "내일",
          "original_form": "내일",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029883.1.21",
          "form": "남친이가",
          "original_form": "남친이가",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029883.1.22",
          "form": "정해준데용",
          "original_form": "정해준데용",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029883.1.23",
          "form": "저는 name2가 먹는거 다 좋아해욤",
          "original_form": "저는 &name2&가 먹는거 다 좋아해욤",
          "speaker_id": "1",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029883.1.24",
          "form": "고민되겠네요",
          "original_form": "고민되겠네요",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100029883.1.25",
          "form": "생각나는거 없어요?",
          "original_form": "생각나는거 없어요?",
          "speaker_id": "2",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029883.1.26",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029883.1.27",
          "form": "남친님이 정해주신답니다",
          "original_form": "남친님이 정해주신답니다",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029883.1.28",
