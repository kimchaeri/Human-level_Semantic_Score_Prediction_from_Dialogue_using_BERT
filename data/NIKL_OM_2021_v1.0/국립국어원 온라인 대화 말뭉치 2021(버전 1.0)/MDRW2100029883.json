          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029883.1.36",
          "form": "남친이장금이고",
          "original_form": "남친이장금이고",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029883.1.37",
          "form": "저는 맛잇게먹는담당이에욤",
          "original_form": "저는 맛잇게먹는담당이에욤",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029883.1.38",
          "form": "몸무게 재보세욥",
          "original_form": "몸무게 재보세욥",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100029883.1.39",
          "form": "잘드시나요",
          "original_form": "잘드시나요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029883.1.40",
          "form": "머좋아하세요",
          "original_form": "머좋아하세요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029883.1.41",
          "form": "수육 피자",
          "original_form": "수육 피자",
          "speaker_id": "1",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029883.1.42",
