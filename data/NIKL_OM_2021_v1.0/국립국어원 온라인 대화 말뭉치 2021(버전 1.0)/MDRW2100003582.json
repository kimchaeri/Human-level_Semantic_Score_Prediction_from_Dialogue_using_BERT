      "utterance": [
        {
          "id": "MDRW2100003582.1.1",
          "form": "최근에 구매한 물건이 있나요?",
          "original_form": "최근에 구매한 물건이 있나요?",
          "speaker_id": "1",
          "time": "20210821 02:42"
        },
        {
          "id": "MDRW2100003582.1.2",
          "form": "저 얼마전에 파자마 샀어요!",
          "original_form": "저 얼마전에 파자마 샀어요!",
          "speaker_id": "2",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003582.1.3",
          "form": "우와 파자마 어디서 사셨어요?",
          "original_form": "우와 파자마 어디서 사셨어요?",
          "speaker_id": "1",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003582.1.4",
          "form": "잠옷 잘 챙겨 입으면 기분이 좋더라구요",
          "original_form": "잠옷 잘 챙겨 입으면 기분이 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003582.1.5",
          "form": "전 무신사에서 로퍼 샀어요 ㅋㅋ",
          "original_form": "전 무신사에서 로퍼 샀어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003582.1.6",
          "form": "자주에서 구입했어요",
          "original_form": "자주에서 구입했어요",
          "speaker_id": "2",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003582.1.7",
          "form": "맞아요 잠옷이 귀여우면 잘때 기분 좋아요",
          "original_form": "맞아요 잠옷이 귀여우면 잘때 기분 좋아요",
          "speaker_id": "1",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003582.1.8",
          "form": "저도 무신사 자주 이용해요",
          "original_form": "저도 무신사 자주 이용해요",
          "speaker_id": "2",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003582.1.9",
          "form": "오 자주거 좋은데 !!",
          "original_form": "오 자주거 좋은데 !!",
          "speaker_id": "1",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003582.1.10",
          "form": "처음엔 잠옷에 몇만원 쓰기 망설여졌었는데 한두벌 사다보니까 계속 사게 되네요ㅎㅎ",
          "original_form": "처음엔 잠옷에 몇만원 쓰기 망설여졌었는데 한두벌 사다보니까 계속 사게 되네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003582.1.11",
          "form": "구매한 로퍼는 어때요?",
          "original_form": "구매한 로퍼는 어때요?",
          "speaker_id": "2",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003582.1.12",
          "form": "저도 잠옷 이번에 자주에서 사봐야겠어요",
          "original_form": "저도 잠옷 이번에 자주에서 사봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003582.1.13",
          "form": "로퍼는 그냥 기본로퍼에요!",
          "original_form": "로퍼는 그냥 기본로퍼에요!",
          "speaker_id": "1",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003582.1.14",
          "form": "근데 조금 커요 ㅠㅠ",
          "original_form": "근데 조금 커요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003582.1.15",
          "form": "아 온라인 쇼핑하면 사이즈 미스나는 일이 많아서 아쉬워요",
          "original_form": "아 온라인 쇼핑하면 사이즈 미스나는 일이 많아서 아쉬워요",
          "speaker_id": "2",
          "time": "20210821 02:47"
        },
        {
          "id": "MDRW2100003582.1.16",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210821 02:47"
        },
        {
          "id": "MDRW2100003582.1.17",
          "form": "교환하기에는 배송비때문에 좀 구렇구..",
          "original_form": "교환하기에는 배송비때문에 좀 구렇구..",
          "speaker_id": "2",
          "time": "20210821 02:47"
        },
        {
          "id": "MDRW2100003582.1.18",
          "form": "신발은 신어보고 사야하나봐요",
          "original_form": "신발은 신어보고 사야하나봐요",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003582.1.19",
          "form": "물론 옷도 입어보고 사야하지만..ㅋ큐ㅠㅠ",
          "original_form": "물론 옷도 입어보고 사야하지만..ㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003582.1.20",
          "form": "배송비 5000원 너무 아까워요",
          "original_form": "배송비 5000원 너무 아까워요",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003582.1.21",
          "form": "교환하면 5000원 ㅠㅠ",
          "original_form": "교환하면 5000원 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003582.1.22",
          "form": "맞아요ㅠㅠ 저도 얼마전 무신사에서 산 운동화가 좀 큰데 그냥 신고 있어요",
          "original_form": "맞아요ㅠㅠ 저도 얼마전 무신사에서 산 운동화가 좀 큰데 그냥 신고 있어요",
          "speaker_id": "2",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003582.1.23",
          "form": "무신사에서 20퍼 할인 쿠폰 뿌려서 좋더라구요",
          "original_form": "무신사에서 20퍼 할인 쿠폰 뿌려서 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003582.1.24",
          "form": "아 20퍼 쿠폰 뿌렷나요??",
          "original_form": "아 20퍼 쿠폰 뿌렷나요??",
          "speaker_id": "1",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003582.1.25",
          "form": "아 맞다 뿌렸다",
          "original_form": "아 맞다 뿌렸다",
          "speaker_id": "1",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003582.1.26",
          "form": "그래서 개이득하면서 샀던거 기억나네요 ㅋㅋㅋㅋ",
          "original_form": "그래서 개이득하면서 샀던거 기억나네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:50"
        },
        {
          "id": "MDRW2100003582.1.27",
          "form": "근데 이번 달이 마지막이래요ㅠㅠ",
          "original_form": "근데 이번 달이 마지막이래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 02:50"
        },
        {
          "id": "MDRW2100003582.1.28",
          "form": "쿠폰 없이 사면 왠지 손해보는 느낌...아쉬워요",
          "original_form": "쿠폰 없이 사면 왠지 손해보는 느낌...아쉬워요",
          "speaker_id": "2",
          "time": "20210821 02:51"
        },
        {
          "id": "MDRW2100003582.1.29",
          "form": "아 진짜",
          "original_form": "아 진짜",
          "speaker_id": "1",
          "time": "20210821 02:51"
        },
        {
          "id": "MDRW2100003582.1.30",
          "form": "너무 아쉽네요",
          "original_form": "너무 아쉽네요",
          "speaker_id": "1",
          "time": "20210821 02:51"
        }
      ]
    }
