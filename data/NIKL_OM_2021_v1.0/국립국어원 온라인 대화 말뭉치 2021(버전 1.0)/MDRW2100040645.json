      "utterance": [
        {
          "id": "MDRW2100040645.1.1",
          "form": "내일 뭐해먹을까",
          "original_form": "내일 뭐해먹을까",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040645.1.2",
          "form": "내일 밥 사 먹어야지",
          "original_form": "내일 밥 사 먹어야지",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040645.1.3",
          "form": "오 왜?",
          "original_form": "오 왜?",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040645.1.4",
          "form": "내일 체력 보충을 해야 한다",
          "original_form": "내일 체력 보충을 해야 한다",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040645.1.5",
          "form": "오 머 먹어 그럼",
          "original_form": "오 머 먹어 그럼",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040645.1.6",
          "form": "염소탕 사 먹자",
          "original_form": "염소탕 사 먹자",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040645.1.7",
          "form": "오 염소탕 ㅋ 좋다",
          "original_form": "오 염소탕 ㅋ 좋다",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040645.1.8",
          "form": "그래 점심에 먹으러 간다",
          "original_form": "그래 점심에 먹으러 간다",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040645.1.9",
          "form": "맛나겠다~ 염소탕 넘 든든함",
          "original_form": "맛나겠다~ 염소탕 넘 든든함",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040645.1.10",
          "form": "그래 먹으면 체력이 좋아진다",
          "original_form": "그래 먹으면 체력이 좋아진다",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040645.1.11",
          "form": "웅웅 느껴짐 ㅋㅋ",
          "original_form": "웅웅 느껴짐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
