      "utterance": [
        {
          "id": "MDRW2100025652.1.1",
          "form": "싸이월드 하셨나요",
          "original_form": "싸이월드 하셨나요",
          "speaker_id": "1",
          "time": "20210901 15:24"
        },
        {
          "id": "MDRW2100025652.1.2",
          "form": "네 싸이월드했어요",
          "original_form": "네 싸이월드했어요",
          "speaker_id": "2",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100025652.1.3",
          "form": "추억이있어요?",
          "original_form": "추억이있어요?",
          "speaker_id": "1",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100025652.1.4",
          "form": "전 딱히 없어요",
          "original_form": "전 딱히 없어요",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025652.1.5",
          "form": "그냥 했다만 기억나네요",
          "original_form": "그냥 했다만 기억나네요",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025652.1.6",
          "form": "님은 어떠세요?",
          "original_form": "님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025652.1.7",
          "form": "저도 딱히 추억은 없는거같아요",
          "original_form": "저도 딱히 추억은 없는거같아요",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100025652.1.8",
          "form": "도토리사는것도 약간 돈아까운기분이었고",
          "original_form": "도토리사는것도 약간 돈아까운기분이었고",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100025652.1.9",
          "form": "맞아요 돈 아까운 기분 맞아요 저도 그랬어요",
          "original_form": "맞아요 돈 아까운 기분 맞아요 저도 그랬어요",
          "speaker_id": "2",
          "time": "20210901 15:34"
        },
        {
          "id": "MDRW2100025652.1.10",
          "form": "막 특유의 그 추억 느낌이 있다는데",
          "original_form": "막 특유의 그 추억 느낌이 있다는데",
          "speaker_id": "2",
          "time": "20210901 15:34"
        },
        {
          "id": "MDRW2100025652.1.11",
          "form": "전 뭐 그런것도 없네요",
          "original_form": "전 뭐 그런것도 없네요",
          "speaker_id": "2",
          "time": "20210901 15:34"
        },
        {
          "id": "MDRW2100025652.1.12",
          "form": "흑역사랄까",
          "original_form": "흑역사랄까",
          "speaker_id": "2",
          "time": "20210901 15:34"
        },
        {
          "id": "MDRW2100025652.1.13",
          "form": "ㅋㅋㅋ그래도열리면한번은들어갈거같아요",
          "original_form": "ㅋㅋㅋ그래도열리면한번은들어갈거같아요",
          "speaker_id": "1",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025652.1.14",
          "form": "뭐...탈퇴하러 들어갈거같긴해요.. 귀찮아서 그대로 뒀거든요",
          "original_form": "뭐...탈퇴하러 들어갈거같긴해요.. 귀찮아서 그대로 뒀거든요",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025652.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ탈퇴ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ탈퇴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025652.1.16",
          "form": "싸이가울거같아요",
          "original_form": "싸이가울거같아요",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025652.1.17",
          "form": "ㅎㅎ 탈퇴입니다 탈퇴",
          "original_form": "ㅎㅎ 탈퇴입니다 탈퇴",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025652.1.18",
          "form": "ㅋㅋㅋ울라고 해야지요.. 내 추억이없으니 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ울라고 해야지요.. 내 추억이없으니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025652.1.19",
          "form": "하늘님싸이월드",
          "original_form": "하늘님싸이월드",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025652.1.20",
