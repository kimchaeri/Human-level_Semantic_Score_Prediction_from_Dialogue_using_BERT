        },
        {
          "id": "MDRW2100025652.1.23",
          "form": "헉... 저도 기억안나는데... 과연....",
          "original_form": "헉... 저도 기억안나는데... 과연....",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025652.1.24",
          "form": "저도 구경을 가긴해야겠네요 ㅋㅋ",
          "original_form": "저도 구경을 가긴해야겠네요 ㅋㅋ",
