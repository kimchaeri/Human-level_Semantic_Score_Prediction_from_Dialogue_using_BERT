      "utterance": [
        {
          "id": "MDRW2100045134.1.1",
          "form": "뭐예요?",
          "original_form": "뭐예요?",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045134.1.2",
          "form": "앙리가 대머리인거",
          "original_form": "앙리가 대머리인거",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045134.1.3",
          "form": "왕이 대머리예요 ;",
          "original_form": "왕이 대머리예요 ;",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045134.1.4",
          "form": "심각하죠?",
          "original_form": "심각하죠?",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045134.1.5",
          "form": "와중에 무능력자예요",
          "original_form": "와중에 무능력자예요",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045134.1.6",
          "form": "대머리 혐오내요",
          "original_form": "대머리 혐오내요",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045134.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045134.1.8",
          "form": "그건 분명히",
          "original_form": "그건 분명히",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045134.1.9",
          "form": "생각이 디스거스팅해서 헤어가 런어웨이 한거라구오",
          "original_form": "생각이 디스거스팅해서 헤어가 런어웨이 한거라구오",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045134.1.10",
          "form": "지 딸을 도구로 쓰는 왕이",
          "original_form": "지 딸을 도구로 쓰는 왕이",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045134.1.11",
          "form": "테런에는 있습니다",
          "original_form": "테런에는 있습니다",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045134.1.12",
          "form": "근대 좀 이상하긴 해요",
          "original_form": "근대 좀 이상하긴 해요",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045134.1.13",
          "form": "스토리",
          "original_form": "스토리",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045134.1.14",
          "form": "맞아요 ; 테런 스토리 뭣됐어요",
          "original_form": "맞아요 ; 테런 스토리 뭣됐어요",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045134.1.15",
          "form": ".. 하지마세요 ㅠ",
          "original_form": ".. 하지마세요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045134.1.16",
          "form": "네 안해요",
          "original_form": "네 안해요",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045134.1.17",
          "form": "하지만 엘소드는 같이 해봐요",
          "original_form": "하지만 엘소드는 같이 해봐요",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045134.1.18",
          "form": "갑분 엘소드?",
          "original_form": "갑분 엘소드?",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045134.1.19",
          "form": "하지만... 이브 4라인이 나왔는걸요",
          "original_form": "하지만... 이브 4라인이 나왔는걸요",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045134.1.20",
          "form": "이브.. 좋아하세요?",
          "original_form": "이브.. 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045134.1.21",
          "form": "네 사랑합니다 이번엔 진심이라구요",
          "original_form": "네 사랑합니다 이번엔 진심이라구요",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045134.1.22",
          "form": "그렇다면 저랑 한번 해보시겠어요?",
          "original_form": "그렇다면 저랑 한번 해보시겠어요?",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045134.1.23",
          "form": "고인물 세상으로 함께 떠나자 !",
          "original_form": "고인물 세상으로 함께 떠나자 !",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045134.1.24",
          "form": "저랑 같아쓰시잖아요",
          "original_form": "저랑 같아쓰시잖아요",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045134.1.25",
          "form": "샅은 아이디란 뜻 ㅎ",
          "original_form": "샅은 아이디란 뜻 ㅎ",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045134.1.26",
          "form": "예.",
          "original_form": "예.",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045134.1.27",
          "form": "그게 같이 떠나는거죠",
          "original_form": "그게 같이 떠나는거죠",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045134.1.28",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210908 01:06"
        }
      ]
    }
