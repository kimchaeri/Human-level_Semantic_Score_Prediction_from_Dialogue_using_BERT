        {
          "id": "MDRW2100024263.1.4",
          "form": "휴..",
          "original_form": "휴..",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024263.1.5",
          "form": "오늘급여날인데...급여가ㅠㅠ",
          "original_form": "오늘급여날인데...급여가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024263.1.6",
          "form": "급여는 통장을 스칠뿐ㅋㅋ",
          "original_form": "급여는 통장을 스칠뿐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024263.1.7",
          "form": "맞아요ㅠㅠ세금장난아님",
          "original_form": "맞아요ㅠㅠ세금장난아님",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024263.1.8",
          "form": "월급받고싶다",
          "original_form": "월급받고싶다",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024263.1.9",
          "form": "주부는 슬퍼요ㅠㅠ",
          "original_form": "주부는 슬퍼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024263.1.10",
          "form": "급여쥐꼬리예요ㅠㅠ",
          "original_form": "급여쥐꼬리예요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024263.1.11",
          "form": "차라리부업이나을듯",
          "original_form": "차라리부업이나을듯",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024263.1.12",
          "form": "블로그부업들어보셨어요?",
          "original_form": "블로그부업들어보셨어요?",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024263.1.13",
          "form": "요새는 디지털 노마드로 부업한대요",
          "original_form": "요새는 디지털 노마드로 부업한대요",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024263.1.14",
          "form": "오ㅋㅋ그게뭐예요?",
          "original_form": "오ㅋㅋ그게뭐예요?",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024263.1.15",
          "form": "저는 온라인폐지줍기해요ㅠ",
          "original_form": "저는 온라인폐지줍기해요ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024263.1.16",
          "form": "저만의부업",
          "original_form": "저만의부업",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024263.1.17",
          "form": "유튜브 블러그로 광고수익을 먹는거랍니다",
          "original_form": "유튜브 블러그로 광고수익을 먹는거랍니다",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024263.1.18",
          "form": "블로그 이번달 도전했는데",
          "original_form": "블로그 이번달 도전했는데",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024263.1.19",
          "form": "애기분유값벌었어요",
          "original_form": "애기분유값벌었어요",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024263.1.20",
          "form": "재밌을듯ㅋㅋ업무스트레스는 적으신가요?",
          "original_form": "재밌을듯ㅋㅋ업무스트레스는 적으신가요?",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024263.1.21",
          "form": "오늘포스팅해야되는데 스트레스가심해요",
          "original_form": "오늘포스팅해야되는데 스트레스가심해요",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024263.1.22",
          "form": "에고ㅠㅠ뭐든지 업무는 힘들죠",
          "original_form": "에고ㅠㅠ뭐든지 업무는 힘들죠",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024263.1.23",
          "form": "취미가 직업되면 힘든듯",
          "original_form": "취미가 직업되면 힘든듯",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024263.1.24",
          "form": "아르바이트하는게 낫겠어요",
          "original_form": "아르바이트하는게 낫겠어요",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024263.1.25",
          "form": "구직활동을 해야하나ㅡㅡ",
          "original_form": "구직활동을 해야하나ㅡㅡ",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024263.1.26",
          "form": "코로나땜시ㅠㅠ이직구직힘들죠",
          "original_form": "코로나땜시ㅠㅠ이직구직힘들죠",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024263.1.27",
          "form": "신랑은 이직한다고 영어공부한다더니",
          "original_form": "신랑은 이직한다고 영어공부한다더니",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
