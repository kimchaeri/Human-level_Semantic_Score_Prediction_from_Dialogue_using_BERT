        {
          "id": "MDRW2100034232.1.1",
          "form": "그러고보니 어제인가 좀 머쓱하달까",
          "original_form": "그러고보니 어제인가 좀 머쓱하달까",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100034232.1.2",
          "form": "돌려까기당함",
          "original_form": "돌려까기당함",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100034232.1.3",
          "form": "찡꾸는 좀 이상한 방언같군",
          "original_form": "찡꾸는 좀 이상한 방언같군",
          "speaker_id": "1",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100034232.1.4",
          "form": "보통 엄마는 저럴때 찡꾸먹었다고 하는데",
          "original_form": "보통 엄마는 저럴때 찡꾸먹었다고 하는데",
          "speaker_id": "1",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100034232.1.5",
          "form": "왜 수업시간 머했는디",
          "original_form": "왜 수업시간 머했는디",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100034232.1.6",
          "form": "내가 수업시간에 계속 깜빡 깜빡 졸았음",
          "original_form": "내가 수업시간에 계속 깜빡 깜빡 졸았음",
          "speaker_id": "1",
          "time": "20210828 09:09"
        },
        {
          "id": "MDRW2100034232.1.7",
          "form": "컴터로하는 수업이니 뒤로가서 서있지도 못하고",
          "original_form": "컴터로하는 수업이니 뒤로가서 서있지도 못하고",
          "speaker_id": "1",
          "time": "20210828 09:10"
        },
        {
          "id": "MDRW2100034232.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 09:10"
        },
        {
          "id": "MDRW2100034232.1.9",
          "form": "서있는구나 졸리면",
          "original_form": "서있는구나 졸리면",
          "speaker_id": "2",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100034232.1.10",
          "form": "아니 교실이면 책들고 뒤로가서 서있잖아요",
          "original_form": "아니 교실이면 책들고 뒤로가서 서있잖아요",
          "speaker_id": "1",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100034232.1.11",
          "form": "여긴 그게안돼ㅜ",
          "original_form": "여긴 그게안돼ㅜ",
          "speaker_id": "1",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100034232.1.12",
          "form": "ㅜㅜㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100034232.1.13",
          "form": "실습해야해서 자리 떠날수없믐",
          "original_form": "실습해야해서 자리 떠날수없믐",
          "speaker_id": "2",
          "time": "20210828 09:12"
        },
        {
          "id": "MDRW2100034232.1.14",
          "form": "찬 물 마시면서 깨고 싶은데",
          "original_form": "찬 물 마시면서 깨고 싶은데",
          "speaker_id": "1",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100034232.1.15",
          "form": "코로나라서 그것도 눈치보임",
          "original_form": "코로나라서 그것도 눈치보임",
          "speaker_id": "1",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100034232.1.16",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100034232.1.17",
          "form": "어케해야 잠깨지 나는 자버림ㅜㅜ",
          "original_form": "어케해야 잠깨지 나는 자버림ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100034232.1.18",
          "form": "그래서 그냥 졸았는데",
          "original_form": "그래서 그냥 졸았는데",
          "speaker_id": "1",
          "time": "20210828 09:36"
