      "utterance": [
        {
          "id": "MDRW2100014431.1.1",
          "form": "오늘 수확이 좋으세요 ㅎㅎ",
          "original_form": "오늘 수확이 좋으세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014431.1.2",
          "form": "제가 팔았어요",
          "original_form": "제가 팔았어요",
          "speaker_id": "1",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014431.1.3",
          "form": "공짜피자",
          "original_form": "공짜피자",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014431.1.4",
          "form": "당근판매",
          "original_form": "당근판매",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.5",
          "form": "아이 퍼즐이요 ㅎ",
          "original_form": "아이 퍼즐이요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.6",
          "form": "큰돈 버셨는데요",
          "original_form": "큰돈 버셨는데요",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.8",
          "form": "잘 파셨어요?",
          "original_form": "잘 파셨어요?",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.9",
          "form": "비싸게?",
          "original_form": "비싸게?",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.10",
          "form": "그러게요 오늘은 소비없이 수입만 ㅋㅋ",
          "original_form": "그러게요 오늘은 소비없이 수입만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.11",
          "form": "그런날이 최고죠",
          "original_form": "그런날이 최고죠",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.12",
          "form": "행복의 제로소비",
          "original_form": "행복의 제로소비",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.13",
          "form": "올린지 좀되서 가격다운하다 더이상은 내릴수없어서",
          "original_form": "올린지 좀되서 가격다운하다 더이상은 내릴수없어서",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.14",
          "form": "가장원하는 삶이죠",
          "original_form": "가장원하는 삶이죠",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.15",
          "form": "그냥 방치했더니",
          "original_form": "그냥 방치했더니",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014431.1.16",
          "form": "그럼 팔려요",
          "original_form": "그럼 팔려요",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014431.1.17",
          "form": "오늘연락와서 있냐고 묻더라구요",
          "original_form": "오늘연락와서 있냐고 묻더라구요",
          "speaker_id": "1",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014431.1.18",
          "form": "오 그럴때 넘 행복해요",
          "original_form": "오 그럴때 넘 행복해요",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014431.1.19",
          "form": "그래서 팔았어요",
          "original_form": "그래서 팔았어요",
          "speaker_id": "1",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014431.1.20",
          "form": "생각지도 않을때 팔리면",
          "original_form": "생각지도 않을때 팔리면",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014431.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014431.1.22",
          "form": "꽁돈같고",
          "original_form": "꽁돈같고",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014431.1.23",
          "form": "추카추카 합니다 ㅎㅎ",
          "original_form": "추카추카 합니다 ㅎㅎ",
