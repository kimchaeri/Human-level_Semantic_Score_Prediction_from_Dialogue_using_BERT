      "utterance": [
        {
          "id": "MDRW2100011237.1.1",
          "form": "우리신랑은 요새 텐트바꾸고",
          "original_form": "우리신랑은 요새 텐트바꾸고",
          "speaker_id": "2",
          "time": "20210824 09:40"
        },
        {
          "id": "MDRW2100011237.1.2",
          "form": "또 타프사고",
          "original_form": "또 타프사고",
          "speaker_id": "2",
          "time": "20210824 09:40"
        },
        {
          "id": "MDRW2100011237.1.3",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210824 09:40"
        },
        {
          "id": "MDRW2100011237.1.4",
          "form": "타프옆에 가림막 사고",
          "original_form": "타프옆에 가림막 사고",
          "speaker_id": "2",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.5",
          "form": "타프없었나?ㅋㅋ",
          "original_form": "타프없었나?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.6",
          "form": "지름신이 왔음 ㅋㅋ",
          "original_form": "지름신이 왔음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.7",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.8",
          "form": "아 웃겨ㅋㅋㅋ",
          "original_form": "아 웃겨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.9",
          "form": "장난아니네 ㅋㅋ",
          "original_form": "장난아니네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.10",
          "form": "텐트 앞에 펼치면 타프 있어서 그걸로 썼는데",
          "original_form": "텐트 앞에 펼치면 타프 있어서 그걸로 썼는데",
          "speaker_id": "2",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.11",
          "form": "울신랑은 텐트하나 팔아라해도",
          "original_form": "울신랑은 텐트하나 팔아라해도",
          "speaker_id": "1",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.12",
          "form": "꺼내놓기만ㅋㅋ",
          "original_form": "꺼내놓기만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.13",
          "form": "여름에는 택도 없네",
          "original_form": "여름에는 택도 없네",
          "speaker_id": "2",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.15",
          "form": "ㅋㅋㅋㅋ그렇지ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그렇지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.16",
          "form": "그래 파는게 쉬운게 아님",
          "original_form": "그래 파는게 쉬운게 아님",
          "speaker_id": "2",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.17",
          "form": "맞어 진짜 쉽지않더라ㅋㅋ",
          "original_form": "맞어 진짜 쉽지않더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.18",
          "form": "팔고 또 사면",
          "original_form": "팔고 또 사면",
          "speaker_id": "1",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100011237.1.19",
          "form": "그래도 언니 낫다ㅋㅋ",
          "original_form": "그래도 언니 낫다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100011237.1.20",
          "form": "엉 처음에 샀던 캠핑장비도 바꾸고 싶은데",
          "original_form": "엉 처음에 샀던 캠핑장비도 바꾸고 싶은데",
          "speaker_id": "2",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100011237.1.21",
          "form": "우린 나가지도 않았는데 또 들어왔잖아ㅋㅋ",
          "original_form": "우린 나가지도 않았는데 또 들어왔잖아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100011237.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱ 맞다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱ 맞다",
          "speaker_id": "2",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100011237.1.23",
          "form": "갑자기 급 사왔지",
          "original_form": "갑자기 급 사왔지",
