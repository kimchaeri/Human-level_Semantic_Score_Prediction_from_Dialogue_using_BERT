          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011605.1.3",
          "form": "나혼자산다 ㅋㅋㅋㅋ",
          "original_form": "나혼자산다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011605.1.4",
          "form": "맞네맞네ㅋ",
          "original_form": "맞네맞네ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011605.1.5",
          "form": "독립하고싶음 ㅋㅋㅋㅋㅋ",
          "original_form": "독립하고싶음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011605.1.6",
          "form": "해방타운  잼있지ㅋㅋ",
          "original_form": "해방타운  잼있지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011605.1.7",
          "form": "독립하고싶은 마음이 강하네ㅋㅋ",
          "original_form": "독립하고싶은 마음이 강하네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011605.1.8",
          "form": "안그래도 사람들 넷플릭스 윽시보데~",
          "original_form": "안그래도 사람들 넷플릭스 윽시보데~",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011605.1.9",
          "form": "맞아 넷플릭스 많이보더라ㅋㅋ",
          "original_form": "맞아 넷플릭스 많이보더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011605.1.10",
          "form": "나혼자산다 잼있지ㅋㅋ",
          "original_form": "나혼자산다 잼있지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011605.1.11",
          "form": "나도 놀면뭐하니랑ㅋㅋ펜트하우스",
          "original_form": "나도 놀면뭐하니랑ㅋㅋ펜트하우스",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011605.1.12",
          "form": "난 그런건 할줄 모르겠음 ㅋㅋㅋㅋ",
          "original_form": "난 그런건 할줄 모르겠음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011605.1.13",
          "form": "그리고 슬기로운 의사생활",
          "original_form": "그리고 슬기로운 의사생활",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011605.1.14",
          "form": "즐겨본다ㅋㅋㅋ",
          "original_form": "즐겨본다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011605.1.15",
          "form": "드라마는 나 일찍자니 챙겨볼수가 없고ㅠ",
          "original_form": "드라마는 나 일찍자니 챙겨볼수가 없고ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011605.1.16",
          "form": "나도 넷플릭스로 영화한편봄ㅋ",
          "original_form": "나도 넷플릭스로 영화한편봄ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011605.1.17",
          "form": "예능은 끊어서 봐도 되니 ㅋㅋㅋㅋ",
