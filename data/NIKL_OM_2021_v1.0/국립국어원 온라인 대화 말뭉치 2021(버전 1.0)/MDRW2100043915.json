      "utterance": [
        {
          "id": "MDRW2100043915.1.1",
          "form": "음식에 대해서 얘기나눴습니닿",
          "original_form": "음식에 대해서 얘기나눴습니닿",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043915.1.2",
          "form": "어제고 오늘이고 비가 와서 그런지 쌀쌀해졌어요",
          "original_form": "어제고 오늘이고 비가 와서 그런지 쌀쌀해졌어요",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043915.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043915.1.4",
          "form": "얼마전에는 좀 덥더니",
          "original_form": "얼마전에는 좀 덥더니",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043915.1.5",
          "form": "갑자기 쌀쌀해졌더라구요",
          "original_form": "갑자기 쌀쌀해졌더라구요",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043915.1.6",
          "form": "낼은 또 다시 더워진다네요 뉴스보니까",
          "original_form": "낼은 또 다시 더워진다네요 뉴스보니까",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043915.1.7",
          "form": "날씨가 오락가락해요ㅜ",
          "original_form": "날씨가 오락가락해요ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043915.1.8",
          "form": "또 더워지나요 ..",
          "original_form": "또 더워지나요 ..",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043915.1.9",
          "form": "옷을 뭘 입을지 감이안오네요ㅋㅋㅋ",
          "original_form": "옷을 뭘 입을지 감이안오네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043915.1.10",
          "form": "저도 그래서 요즘 반팔입고 겉옷 들고다녀요ㅋㅋ",
          "original_form": "저도 그래서 요즘 반팔입고 겉옷 들고다녀요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043915.1.11",
          "form": "그쵸 겉옷들고다녀야하는 계절이 되어버렸어요",
          "original_form": "그쵸 겉옷들고다녀야하는 계절이 되어버렸어요",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043915.1.12",
          "form": "영 귀찮달지..",
          "original_form": "영 귀찮달지..",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043915.1.13",
          "form": "ㅋㅋㅋㅋㅋ어느 계절 제일 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋㅋ어느 계절 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043915.1.14",
          "form": "저는 겨울이요ㅎㅎ",
          "original_form": "저는 겨울이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043915.1.15",
          "form": "옷입기가 편해서 넘 좋아요ㅋㅋㅋㅋ",
          "original_form": "옷입기가 편해서 넘 좋아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043915.1.16",
          "form": "저는 옷상관안하면 가을이요!!!",
          "original_form": "저는 옷상관안하면 가을이요!!!",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043915.1.17",
          "form": "옷입는거 생각하면 여름!",
          "original_form": "옷입는거 생각하면 여름!",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043915.1.18",
          "form": "겨울에 롱패딩에 후드가 넘 편해서 4계절 내내 입고다니고싶어요ㅋㅋㅋ",
          "original_form": "겨울에 롱패딩에 후드가 넘 편해서 4계절 내내 입고다니고싶어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043915.1.19",
          "form": "저는 여름에 원피스하나로 그냥 훅 입고 훅 벗으니 그게 편해서 여름을 택했어요ㅋㅋㅋㅋ",
          "original_form": "저는 여름에 원피스하나로 그냥 훅 입고 훅 벗으니 그게 편해서 여름을 택했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043915.1.20",
          "form": "후드도 짱 편하죠ㅜ",
          "original_form": "후드도 짱 편하죠ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043915.1.21",
          "form": "원피스가 진짜 편하다고 하긴 하더라구요ㅋㅋㅋ",
          "original_form": "원피스가 진짜 편하다고 하긴 하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043915.1.22",
          "form": "다들 한번입으면 다른거 입기 귀찮다고 그러던..ㅋㅋㅋ",
          "original_form": "다들 한번입으면 다른거 입기 귀찮다고 그러던..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043915.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 원피스도 입기 편한거로 입고요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 원피스도 입기 편한거로 입고요",
          "speaker_id": "1",
          "time": "20210907 21:14"
        }
      ]
