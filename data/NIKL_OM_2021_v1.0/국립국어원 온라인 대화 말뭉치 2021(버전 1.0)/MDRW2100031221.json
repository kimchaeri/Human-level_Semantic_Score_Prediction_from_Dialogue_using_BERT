      "utterance": [
        {
          "id": "MDRW2100031221.1.1",
          "form": "name2님은 직장인이신가요?",
          "original_form": "&name2&님은 직장인이신가요?",
          "speaker_id": "1",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100031221.1.2",
          "form": "아뇨 저는 백수입니당",
          "original_form": "아뇨 저는 백수입니당",
          "speaker_id": "2",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100031221.1.3",
          "form": "name1님은용?",
          "original_form": "&name1&님은용?",
          "speaker_id": "2",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100031221.1.4",
          "form": "저는 재택 프리랜서예요",
          "original_form": "저는 재택 프리랜서예요",
          "speaker_id": "1",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100031221.1.5",
          "form": "오 멋져요...",
          "original_form": "오 멋져요...",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100031221.1.6",
          "form": "능력있는 사람만 하는거 아닌가요?",
          "original_form": "능력있는 사람만 하는거 아닌가요?",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100031221.1.7",
          "form": "아잇...그 정도는 아니구 운이 좋아서 어떻게 하고 있네요",
          "original_form": "아잇...그 정도는 아니구 운이 좋아서 어떻게 하고 있네요",
          "speaker_id": "1",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100031221.1.8",
          "form": "겸손하시네요ㅎㅎㅎ 자세하게는 말구 곤란하지 않으신 범위에서 어떤 일 하고 계시는지 알려주실수 있나요?",
          "original_form": "겸손하시네요ㅎㅎㅎ 자세하게는 말구 곤란하지 않으신 범위에서 어떤 일 하고 계시는지 알려주실수 있나요?",
          "speaker_id": "2",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100031221.1.9",
          "form": "저는 교정교열이랑 한자번역 하고 있어요ㅎㅎ",
          "original_form": "저는 교정교열이랑 한자번역 하고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100031221.1.10",
          "form": "교정교열 들어보셨어요??",
          "original_form": "교정교열 들어보셨어요??",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100031221.1.11",
          "form": "오 들어봤어요! 엄청 어려운 일 아닌가요",
          "original_form": "오 들어봤어요! 엄청 어려운 일 아닌가요",
          "speaker_id": "2",
          "time": "20210904 18:34"
        },
        {
          "id": "MDRW2100031221.1.12",
          "form": "저처럼 덜렁이는 상상도 못해요ㅠ ㅋㅋㅋㅋ",
          "original_form": "저처럼 덜렁이는 상상도 못해요ㅠ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:35"
        },
        {
          "id": "MDRW2100031221.1.13",
          "form": "꼼꼼해야 하는 일이긴 해요ㅋㅋㅋ 그래서 저도 원고 몇번씩 다시 본다는",
          "original_form": "꼼꼼해야 하는 일이긴 해요ㅋㅋㅋ 그래서 저도 원고 몇번씩 다시 본다는",
          "speaker_id": "1",
          "time": "20210904 18:35"
        },
        {
          "id": "MDRW2100031221.1.14",
          "form": "신경을 되게 많이 쓰시겠어요",
          "original_form": "신경을 되게 많이 쓰시겠어요",
          "speaker_id": "2",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031221.1.15",
          "form": "그럼 일하다가 스트레스 받으면 어떻게 하세요?",
          "original_form": "그럼 일하다가 스트레스 받으면 어떻게 하세요?",
          "speaker_id": "2",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031221.1.16",
          "form": "인터넷 서핑, 독서, 음악감상, 또는 하고있던 일말고 다른일이요ㅋㅋㅋ",
          "original_form": "인터넷 서핑, 독서, 음악감상, 또는 하고있던 일말고 다른일이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031221.1.17",
          "form": "ㅋㅋㅋㅋㅋ 그럼 재택이시니까 노래 크게 틀기도 하고 그러세요?",
          "original_form": "ㅋㅋㅋㅋㅋ 그럼 재택이시니까 노래 크게 틀기도 하고 그러세요?",
          "speaker_id": "2",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031221.1.18",
          "form": "오, 당근이죵ㅋㅋㅋ",
          "original_form": "오, 당근이죵ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:47"
        },
        {
          "id": "MDRW2100031221.1.19",
          "form": "노동요는 업무 필수요소입니다",
          "original_form": "노동요는 업무 필수요소입니다",
          "speaker_id": "1",
          "time": "20210904 18:47"
        },
        {
          "id": "MDRW2100031221.1.20",
          "form": "오 너무 좋겠다ㅋㅋㅋ 노동요로는 어떤 취향이신가요 신나는곡? 투지가 불타오르는 곡?",
          "original_form": "오 너무 좋겠다ㅋㅋㅋ 노동요로는 어떤 취향이신가요 신나는곡? 투지가 불타오르는 곡?",
          "speaker_id": "2",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100031221.1.21",
          "form": "그날그날 기분따라 다릅니다",
          "original_form": "그날그날 기분따라 다릅니다",
          "speaker_id": "1",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100031221.1.22",
          "form": "제이팝 틀때도 있고",
          "original_form": "제이팝 틀때도 있고",
          "speaker_id": "1",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100031221.1.23",
          "form": "재즈나 케이팝이나 지브리나 피아노곡이나",
          "original_form": "재즈나 케이팝이나 지브리나 피아노곡이나",
          "speaker_id": "1",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100031221.1.24",
          "form": "장르별로 트는것 같아요ㅎㅎ",
          "original_form": "장르별로 트는것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100031221.1.25",
          "form": "오 되게 장르를 안가리시네요",
          "original_form": "오 되게 장르를 안가리시네요",
          "speaker_id": "2",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100031221.1.26",
          "form": "혹시 신나는곡 틀면 실수가 늘어나나요?",
          "original_form": "혹시 신나는곡 틀면 실수가 늘어나나요?",
          "speaker_id": "2",
          "time": "20210904 18:51"
        },
        {
          "id": "MDRW2100031221.1.27",
          "form": "궁금해요ㅋㅋㅋㅋ",
          "original_form": "궁금해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:51"
        },
        {
          "id": "MDRW2100031221.1.28",
          "form": "ㅋㅋㅋㅋㅋ 꼭 그렇진 않아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 꼭 그렇진 않아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:00"
        },
        {
          "id": "MDRW2100031221.1.29",
          "form": "name2님은 하고 싶은 일 있으세요?",
          "original_form": "&name2&님은 하고 싶은 일 있으세요?",
          "speaker_id": "1",
          "time": "20210904 19:01"
        },
        {
