      "utterance": [
        {
          "id": "MDRW2100024769.1.1",
          "form": "안녕하세요 유자좋아하세요?",
          "original_form": "안녕하세요 유자좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024769.1.2",
          "form": "네 유자차 좋아해요..ㅋㅋㅋ",
          "original_form": "네 유자차 좋아해요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024769.1.3",
          "form": "name1님는 무슨 음료 좋아하시나요",
          "original_form": "&name1&님는 무슨 음료 좋아하시나요",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024769.1.4",
          "form": "전 아아 좋아해요!!",
          "original_form": "전 아아 좋아해요!!",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024769.1.5",
          "form": "하루에 한번은 꼭 먹어요",
          "original_form": "하루에 한번은 꼭 먹어요",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024769.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100024769.1.7",
          "form": "저도 아아 좋아요",
          "original_form": "저도 아아 좋아요",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100024769.1.8",
          "form": "무조건 투샷으로 ㅎㅎ",
          "original_form": "무조건 투샷으로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100024769.1.9",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210901 07:25"
        },
        {
          "id": "MDRW2100024769.1.10",
          "form": "아아는 진하게 먹어야죸ㅋㅋ",
          "original_form": "아아는 진하게 먹어야죸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 07:25"
        },
        {
          "id": "MDRW2100024769.1.11",
          "form": "정신 바짝 들겤ㅋㅋ",
          "original_form": "정신 바짝 들겤ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 07:25"
        },
        {
          "id": "MDRW2100024769.1.12",
          "form": "그렇습니다",
          "original_form": "그렇습니다",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100024769.1.13",
          "form": "저는 우유를 못먹어서 두유나 귀리우유 넣고",
          "original_form": "저는 우유를 못먹어서 두유나 귀리우유 넣고",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100024769.1.14",
          "form": "라떼도 많이 먹어요!",
          "original_form": "라떼도 많이 먹어요!",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100024769.1.15",
          "form": "와 저 두유라떼 좋아해요",
          "original_form": "와 저 두유라떼 좋아해요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100024769.1.16",
          "form": "!!!",
          "original_form": "!!!",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100024769.1.17",
          "form": "아몬드브리즈에 넣은것도 맛있더라고요",
          "original_form": "아몬드브리즈에 넣은것도 맛있더라고요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100024769.1.18",
          "form": "저도여ㅎㅎ  아몬드브리즈도 좋죠",
          "original_form": "저도여ㅎㅎ  아몬드브리즈도 좋죠",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100024769.1.19",
          "form": "이번에 두부 압축기? 두부 프레스를 샀는데",
          "original_form": "이번에 두부 압축기? 두부 프레스를 샀는데",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100024769.1.20",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100024769.1.21",
          "form": "두부 물 쫙 빼주니까 편하네요~",
          "original_form": "두부 물 쫙 빼주니까 편하네요~",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100024769.1.22",
          "form": "와 신기해요",
          "original_form": "와 신기해요",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100024769.1.23",
          "form": "신기핫 물건들이 많네요ㅋㅋ",
          "original_form": "신기핫 물건들이 많네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100024769.1.24",
          "form": "ㅋㅋㅋㅋ 그러게요",
          "original_form": "ㅋㅋㅋㅋ 그러게요",
          "speaker_id": "2",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100024769.1.25",
          "form": "방금 매일유업에서 새로나온 귀리우유 산거 처음 먹었는데",
          "original_form": "방금 매일유업에서 새로나온 귀리우유 산거 처음 먹었는데",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100024769.1.26",
          "form": "빨대가 종이빨대예요 ㄷㄷ",
          "original_form": "빨대가 종이빨대예요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100024769.1.27",
          "form": "와 점점 환경 생각하는 그런게 많아지네요",
          "original_form": "와 점점 환경 생각하는 그런게 많아지네요",
          "speaker_id": "2",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100024769.1.28",
          "form": "요즘 편의점에 옥수수전분 빨대 비치하기도하고",
          "original_form": "요즘 편의점에 옥수수전분 빨대 비치하기도하고",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100024769.1.29",
          "form": "그러더라구요",
          "original_form": "그러더라구요",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100024769.1.30",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100024769.1.31",
          "form": "그건 한번도 못봤내요",
          "original_form": "그건 한번도 못봤내요",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100024769.1.32",
          "form": "사실 종이빨대 불호인데 환경 생각하면 뭐 어쩔 수 없죠 ㅎㅎ",
          "original_form": "사실 종이빨대 불호인데 환경 생각하면 뭐 어쩔 수 없죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100024769.1.33",
          "form": "종이빨대는 흐물흐물해지는데",
          "original_form": "종이빨대는 흐물흐물해지는데",
          "speaker_id": "1",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100024769.1.34",
          "form": "옥수수빨대는 계속 튼튼해요!",
          "original_form": "옥수수빨대는 계속 튼튼해요!",
          "speaker_id": "1",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100024769.1.35",
          "form": "옥수수빨대는 안흐물거려요!",
          "original_form": "옥수수빨대는 안흐물거려요!",
          "speaker_id": "1",
          "time": "20210903 09:17"
        }
      ]
