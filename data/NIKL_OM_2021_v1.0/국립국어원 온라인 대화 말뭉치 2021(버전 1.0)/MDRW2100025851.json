          "original_form": "&name1&",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025851.1.5",
          "form": "오늘 언제 퇴근하시나용",
          "original_form": "오늘 언제 퇴근하시나용",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025851.1.6",
          "form": "그저그래요 일이죠뭐",
          "original_form": "그저그래요 일이죠뭐",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025851.1.7",
          "form": "6시에요 칼퇴해요",
          "original_form": "6시에요 칼퇴해요",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025851.1.8",
          "form": "우와 부러우어ㅛ",
          "original_form": "우와 부러우어ㅛ",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025851.1.9",
          "form": "name1는요?",
          "original_form": "&name1&는요?",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025851.1.10",
          "form": "부러워요 전8시에 퇴근해요",
          "original_form": "부러워요 전8시에 퇴근해요",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025851.1.11",
          "form": "8시가 칼퇴랍니다",
          "original_form": "8시가 칼퇴랍니다",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025851.1.12",
          "form": "출근이 늦은거 아녀요?",
          "original_form": "출근이 늦은거 아녀요?",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025851.1.13",
          "form": "출근은 10시까지긴 해용 몇시 출근하세요",
          "original_form": "출근은 10시까지긴 해용 몇시 출근하세요",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025851.1.14",
          "form": "8시40분에 해요",
          "original_form": "8시40분에 해요",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025851.1.15",
          "form": "우와 몇시에 일어나세요 그럼",
          "original_form": "우와 몇시에 일어나세요 그럼",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025851.1.16",
          "form": "출근 힘들겠어요",
          "original_form": "출근 힘들겠어요",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025851.1.17",
          "form": "8시10분이요 ㅋㅋ",
          "original_form": "8시10분이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025851.1.18",
          "form": "오 그래도 숙소랑 가까운가보네요",
          "original_form": "오 그래도 숙소랑 가까운가보네요",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025851.1.19",
          "form": "이직생각은 없으신가요",
          "original_form": "이직생각은 없으신가요",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025851.1.20",
          "form": "일찍 일어나야 하는데 넘 딱 맞춰 일어나서 사람 구실만 하고 출근하네요",
          "original_form": "일찍 일어나야 하는데 넘 딱 맞춰 일어나서 사람 구실만 하고 출근하네요",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025851.1.21",
          "form": "차로 5분거리",
          "original_form": "차로 5분거리",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025851.1.22",
          "form": "그래도 출근할 수 있어서 다행입니다",
          "original_form": "그래도 출근할 수 있어서 다행입니다",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025851.1.23",
          "form": "이직은 비밀 ㅋㅋ",
          "original_form": "이직은 비밀 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025851.1.24",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025851.1.25",
          "form": "감사한 마음으로 다니겠습니다",
          "original_form": "감사한 마음으로 다니겠습니다",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025851.1.26",
          "form": "직장은 맘에 드시나요",
          "original_form": "직장은 맘에 드시나요",
          "speaker_id": "1",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025851.1.27",
          "form": "맘에 들어요 연말 인사이동 후엔 어떻게 될지 모르겠지만",
          "original_form": "맘에 들어요 연말 인사이동 후엔 어떻게 될지 모르겠지만",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025851.1.28",
          "form": "아직은 만족합니다",
          "original_form": "아직은 만족합니다",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025851.1.29",
          "form": "name1는 대구 자주 가나요?",
          "original_form": "&name1&는 대구 자주 가나요?",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025851.1.30",
          "form": "직업적으로",
          "original_form": "직업적으로",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025851.1.31",
