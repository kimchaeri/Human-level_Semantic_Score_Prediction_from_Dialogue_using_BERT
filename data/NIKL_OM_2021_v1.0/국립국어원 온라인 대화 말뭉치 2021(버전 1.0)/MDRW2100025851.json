        },
        {
          "id": "MDRW2100025851.1.2",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025851.1.3",
          "form": "내다",
          "original_form": "내다",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025851.1.4",
