      "utterance": [
        {
          "id": "MDRW2100036626.1.1",
          "form": "저 김치 사먹는데",
          "original_form": "저 김치 사먹는데",
          "speaker_id": "1",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.2",
          "form": "ㅎㅎ마침딱있던거라",
          "original_form": "ㅎㅎ마침딱있던거라",
          "speaker_id": "2",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.3",
          "form": "혹시 직접 담궈드시나요?",
          "original_form": "혹시 직접 담궈드시나요?",
          "speaker_id": "1",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.4",
          "form": "맞아요 ㅋㅋㅋ 딱 갖고계셨네요",
          "original_form": "맞아요 ㅋㅋㅋ 딱 갖고계셨네요",
          "speaker_id": "1",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.5",
          "form": "어머니가 담아주시구여",
          "original_form": "어머니가 담아주시구여",
          "speaker_id": "2",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.6",
          "form": "반은사먹어요",
          "original_form": "반은사먹어요",
          "speaker_id": "2",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.7",
          "form": "와아",
          "original_form": "와아",
          "speaker_id": "1",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.8",
          "form": "사드실땐 어디꺼 드세요?",
          "original_form": "사드실땐 어디꺼 드세요?",
          "speaker_id": "1",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.9",
          "form": "저 그 청정원거",
          "original_form": "저 그 청정원거",
          "speaker_id": "2",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.10",
          "form": "최근에 비비고 포기김치를 샀는데 너무 매워서요ㅠㅠ",
          "original_form": "최근에 비비고 포기김치를 샀는데 너무 매워서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.11",
          "form": "종갓집 말씀하시는구나~",
          "original_form": "종갓집 말씀하시는구나~",
          "speaker_id": "1",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.12",
          "form": "저도 매운거잘못먹는데ㅜ",
          "original_form": "저도 매운거잘못먹는데ㅜ",
          "speaker_id": "2",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.13",
          "form": "아맞아요",
          "original_form": "아맞아요",
          "speaker_id": "2",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100036626.1.14",
          "form": "종갓집이 비싸긴해도 김치 전반적으로",
          "original_form": "종갓집이 비싸긴해도 김치 전반적으로",
          "speaker_id": "1",
          "time": "20210831 02:41"
        },
        {
          "id": "MDRW2100036626.1.15",
          "form": "종갓집 갑자기생각이안났네요 ㅋㅋㅋㅋㅋ",
          "original_form": "종갓집 갑자기생각이안났네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:41"
        },
        {
          "id": "MDRW2100036626.1.16",
          "form": "깔끔한 맛이더라구요",
          "original_form": "깔끔한 맛이더라구요",
          "speaker_id": "1",
          "time": "20210831 02:41"
        },
        {
          "id": "MDRW2100036626.1.17",
          "form": "시원하고 맛있더라구여",
          "original_form": "시원하고 맛있더라구여",
          "speaker_id": "2",
          "time": "20210831 02:41"
        },
        {
          "id": "MDRW2100036626.1.18",
          "form": "아ㅋㅋㅋㅋ 그러실수 있죵!",
          "original_form": "아ㅋㅋㅋㅋ 그러실수 있죵!",
          "speaker_id": "1",
          "time": "20210831 02:41"
        },
        {
          "id": "MDRW2100036626.1.19",
          "form": "청정원에서 할인많이할때",
          "original_form": "청정원에서 할인많이할때",
          "speaker_id": "2",
          "time": "20210831 02:41"
        },
        {
          "id": "MDRW2100036626.1.20",
          "form": "김치사요 ㅎㅎ",
          "original_form": "김치사요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 02:41"
        },
        {
          "id": "MDRW2100036626.1.21",
          "form": "네네 저도 젓갈 들어간 김치는 안좋아해서ㅠㅠ",
          "original_form": "네네 저도 젓갈 들어간 김치는 안좋아해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 02:41"
        }
      ]
