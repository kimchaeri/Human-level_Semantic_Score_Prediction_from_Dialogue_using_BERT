      "utterance": [
        {
          "id": "MDRW2100025937.1.1",
          "form": "네 즉석에서 구워줘요!",
          "original_form": "네 즉석에서 구워줘요!",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025937.1.2",
          "form": "위생은...조금 의심스럽지만요ㅋㅋㅋ",
          "original_form": "위생은...조금 의심스럽지만요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025937.1.3",
          "form": "책 좋아하세요??",
          "original_form": "책 좋아하세요??",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100025937.1.4",
          "form": "읽으려고 노력중이에요",
          "original_form": "읽으려고 노력중이에요",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025937.1.5",
          "form": "책 좋아하세요???",
          "original_form": "책 좋아하세요???",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025937.1.6",
          "form": "좋아하는데 요즘은 전혀 못 읽고있어요ㅠ",
          "original_form": "좋아하는데 요즘은 전혀 못 읽고있어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100025937.1.7",
          "form": "좋아하는 장르 있나요?",
          "original_form": "좋아하는 장르 있나요?",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100025937.1.8",
          "form": "소설이 제일 잘 읽히는거 같아요",
          "original_form": "소설이 제일 잘 읽히는거 같아요",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100025937.1.9",
          "form": "사실 한권 도서관에서 빌려왔는데 3주째 못읽고 있긴해요ㅋㅋㅋ",
          "original_form": "사실 한권 도서관에서 빌려왔는데 3주째 못읽고 있긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100025937.1.10",
          "form": "뭐 빌려 오셨나요 ㅎㅎ",
          "original_form": "뭐 빌려 오셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100025937.1.11",
          "form": "3주면 연체된거 아닌가요 ㅋㅋㅋㅋㅋ",
          "original_form": "3주면 연체된거 아닌가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100025937.1.12",
          "form": "연장했어요ㅎㅎ",
          "original_form": "연장했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100025937.1.13",
          "form": "구병모작가 신작 빌려왔어요!",
          "original_form": "구병모작가 신작 빌려왔어요!",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100025937.1.14",
          "form": "바늘과 가죽인가요??",
          "original_form": "바늘과 가죽인가요??",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100025937.1.15",
          "form": "구병모 작가님 책 재밌어요ㅠㅠ!",
          "original_form": "구병모 작가님 책 재밌어요ㅠㅠ!",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100025937.1.16",
          "form": "오 맞아요!",
          "original_form": "오 맞아요!",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025937.1.17",
          "form": "초반이지만 재밌어서 얼른 읽어야하는데 시간이...",
          "original_form": "초반이지만 재밌어서 얼른 읽어야하는데 시간이...",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025937.1.18",
          "form": "약간 넷플 올드가드랑 비슷한건가 싶기도 하구요!",
          "original_form": "약간 넷플 올드가드랑 비슷한건가 싶기도 하구요!",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100025937.1.19",
          "form": "구병모 작가님책이 취향에 맞으신다면",
          "original_form": "구병모 작가님책이 취향에 맞으신다면",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100025937.1.20",
          "form": "파과 를 꼭 추천 드립니다..!",
          "original_form": "파과 를 꼭 추천 드립니다..!",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100025937.1.21",
          "form": "살인청부업자를 업으로 삼고있는 할머니가 주인공인데 정말 재밌어요!",
          "original_form": "살인청부업자를 업으로 삼고있는 할머니가 주인공인데 정말 재밌어요!",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100025937.1.22",
          "form": "파과 읽었어요!",
          "original_form": "파과 읽었어요!",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100025937.1.23",
          "form": "그거 읽고 관심가게 돼서 빌려본거에요ㅎㅎㅎㅎ",
          "original_form": "그거 읽고 관심가게 돼서 빌려본거에요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100025937.1.24",
          "form": "읽으셨군요! 재밌죠ㅠㅠㅠㅠㅠㅠ",
          "original_form": "읽으셨군요! 재밌죠ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100025937.1.25",
          "form": "저도 얼른 그 신작 읽어봐야겠네요!",
          "original_form": "저도 얼른 그 신작 읽어봐야겠네요!",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100025937.1.26",
          "form": "만약에 읽기 시작하신다면 저보다 먼저 읽으실수도...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "만약에 읽기 시작하신다면 저보다 먼저 읽으실수도...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100025937.1.27",
          "form": "연장기간 끝나기 전에 읽으셔야죠! ㅎㅎ",
          "original_form": "연장기간 끝나기 전에 읽으셔야죠! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:36"
        }
      ]
    }
