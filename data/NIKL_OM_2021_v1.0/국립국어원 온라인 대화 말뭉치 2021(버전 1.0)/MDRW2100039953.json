      "utterance": [
        {
          "id": "MDRW2100039953.1.1",
          "form": "요즘 꽂힌 음식 있으신가요?!",
          "original_form": "요즘 꽂힌 음식 있으신가요?!",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039953.1.2",
          "form": "저는 그릭복숭아에 꽂혔어요!",
          "original_form": "저는 그릭복숭아에 꽂혔어요!",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039953.1.3",
          "form": "우와?!",
          "original_form": "우와?!",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039953.1.4",
          "form": "혹시 그",
          "original_form": "혹시 그",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039953.1.5",
          "form": "트위터에서 핫하다는 그",
          "original_form": "트위터에서 핫하다는 그",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039953.1.6",
          "form": "정석대로 하면 복숭아 속 비워서 그릭요거트를 채우는 거라던데",
          "original_form": "정석대로 하면 복숭아 속 비워서 그릭요거트를 채우는 거라던데",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039953.1.7",
          "form": "그거 맞나요?! 막례쓰도",
          "original_form": "그거 맞나요?! 막례쓰도",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039953.1.8",
          "form": "네네 만들어 드신!!!",
          "original_form": "네네 만들어 드신!!!",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039953.1.9",
          "form": "넵 맞아요 그거요!",
          "original_form": "넵 맞아요 그거요!",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039953.1.10",
          "form": "전 손재주가 없어서 속을 그렇게 파진 못했고ㅋㅋㅋㅋ",
          "original_form": "전 손재주가 없어서 속을 그렇게 파진 못했고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039953.1.11",
          "form": "대충 잘라서 그릭요거트랑 같이 먹었어요",
          "original_form": "대충 잘라서 그릭요거트랑 같이 먹었어요",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039953.1.12",
          "form": "오오 저도 궁금하네요!",
          "original_form": "오오 저도 궁금하네요!",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039953.1.13",
          "form": "다른 분들 드시는거 구경만 했는데...",
          "original_form": "다른 분들 드시는거 구경만 했는데...",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039953.1.14",
          "form": "복숭아랑 요거트 하니까 생각난건데",
          "original_form": "복숭아랑 요거트 하니까 생각난건데",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039953.1.15",
          "form": "파리바게트에서 파는 아이스크림 드셔본 적 있나요?",
          "original_form": "파리바게트에서 파는 아이스크림 드셔본 적 있나요?",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039953.1.16",
          "form": "오 파바에서 뭔가 새로 나왔나요?",
          "original_form": "오 파바에서 뭔가 새로 나왔나요?",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100039953.1.17",
          "form": "얼마전에 민초 케이크가 너무 먹어보고 싶어서 사오긴 했는데 아이스크림코너는 못봤거든요ㅠㅠ",
          "original_form": "얼마전에 민초 케이크가 너무 먹어보고 싶어서 사오긴 했는데 아이스크림코너는 못봤거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039953.1.18",
          "form": "오 민초단이시군요!!!!",
          "original_form": "오 민초단이시군요!!!!",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039953.1.19",
          "form": "저도 완전 민초단이거든요!!!!",
          "original_form": "저도 완전 민초단이거든요!!!!",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039953.1.20",
          "form": "사실 민초는 아이스크림으로 먹는 게 베스트긴해요",
          "original_form": "사실 민초는 아이스크림으로 먹는 게 베스트긴해요",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039953.1.21",
          "form": "맞아요ㅠㅠ 그 특유의 시원한 향과 맛이 차갑게 먹었을때 제일 잘 어울리는 것 같아요",
          "original_form": "맞아요ㅠㅠ 그 특유의 시원한 향과 맛이 차갑게 먹었을때 제일 잘 어울리는 것 같아요",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039953.1.22",
          "form": "제 민초사랑의 시작점도 배스킨라빈스이기에...!",
          "original_form": "제 민초사랑의 시작점도 배스킨라빈스이기에...!",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039953.1.23",
          "form": "알죠알죠!!!!",
          "original_form": "알죠알죠!!!!",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039953.1.24",
          "form": "아니 뭐가 나쁜말이란거지ㅠ",
          "original_form": "아니 뭐가 나쁜말이란거지ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039953.1.25",
          "form": "제가 아까 복숭아 얘기드렸잖아요?",
          "original_form": "제가 아까 복숭아 얘기드렸잖아요?",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039953.1.26",
          "form": "파리바게트에서 파는 스틱 아이스크림 중에",
          "original_form": "파리바게트에서 파는 스틱 아이스크림 중에",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039953.1.27",
          "form": "리얼피치바",
          "original_form": "리얼피치바",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039953.1.28",
          "form": "꼬옥 먹어주면 되...",
          "original_form": "꼬옥 먹어주면 되...",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039953.1.29",
          "form": "오 추천 감사드립니다ㅠㅠㅠㅠ",
          "original_form": "오 추천 감사드립니다ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039953.1.30",
          "form": "복숭아는 사랑이죠....!!!",
          "original_form": "복숭아는 사랑이죠....!!!",
          "speaker_id": "2",
          "time": "20210903 23:33"
        }
      ]
    }
