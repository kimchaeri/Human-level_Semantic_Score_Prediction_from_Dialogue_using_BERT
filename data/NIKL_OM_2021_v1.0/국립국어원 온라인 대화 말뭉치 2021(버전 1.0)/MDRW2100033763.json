          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100033763.1.4",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033763.1.5",
          "form": "못봤어요",
          "original_form": "못봤어요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033763.1.6",
          "form": "못봤어요",
          "original_form": "못봤어요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033763.1.7",
          "form": "군만두가 무조건",
          "original_form": "군만두가 무조건",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033763.1.8",
          "form": "오거든요",
          "original_form": "오거든요",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033763.1.9",
          "form": "아씁",
          "original_form": "아씁",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033763.1.10",
          "form": "서비스로.....",
          "original_form": "서비스로.....",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033763.1.11",
          "form": "근데 폐업이라고요?",
          "original_form": "근데 폐업이라고요?",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033763.1.12",
          "form": "근데...하...",
          "original_form": "근데...하...",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033763.1.13",
          "form": "너무하다",
          "original_form": "너무하다",
