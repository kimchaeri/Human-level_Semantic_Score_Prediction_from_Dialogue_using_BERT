      "utterance": [
        {
          "id": "MDRW2100036879.1.1",
          "form": "ㅋㅋ레디톡은 비추에요 너무 배고파요ㅜ",
          "original_form": "ㅋㅋ레디톡은 비추에요 너무 배고파요ㅜ",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036879.1.2",
          "form": "앗 그래요?",
          "original_form": "앗 그래요?",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036879.1.3",
          "form": "밥안먹고 이거만먹어요?",
          "original_form": "밥안먹고 이거만먹어요?",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100036879.1.4",
          "form": "일단 프로그램이 여러가지가있어요",
          "original_form": "일단 프로그램이 여러가지가있어요",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100036879.1.5",
          "form": "근데 디톡스에서는 하루5개 이틀간 총10개 세시간 간격으로 저거만 먹어요",
          "original_form": "근데 디톡스에서는 하루5개 이틀간 총10개 세시간 간격으로 저거만 먹어요",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100036879.1.6",
          "form": "딴거안먹구요?",
          "original_form": "딴거안먹구요?",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100036879.1.7",
          "form": "우와 손떨릴듯요",
          "original_form": "우와 손떨릴듯요",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100036879.1.8",
          "form": "떨리는거 정도는 아닌데 짜증이 짜증이 ㅋㅋ",
          "original_form": "떨리는거 정도는 아닌데 짜증이 짜증이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036879.1.9",
          "form": "예민해지시죠?",
          "original_form": "예민해지시죠?",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036879.1.10",
          "form": "그리고 손끝이 차가워지고있어요 ㅋㅋ",
          "original_form": "그리고 손끝이 차가워지고있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036879.1.11",
          "form": "대화도 조심스러워지고 있어요 ㅋㅋㅋ",
          "original_form": "대화도 조심스러워지고 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100036879.1.12",
          "form": "예민해지실때 잘못말했다가 ㅋㅋ 차단각? ㅋㅋ",
          "original_form": "예민해지실때 잘못말했다가 ㅋㅋ 차단각? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100036879.1.13",
          "form": "그건 아니에요 ㅋㅋㅋ",
          "original_form": "그건 아니에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036879.1.14",
          "form": "이거 사실 제가 산게 아니라서",
          "original_form": "이거 사실 제가 산게 아니라서",
          "speaker_id": "2",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036879.1.15",
          "form": "딱히 암생각없어요",
          "original_form": "딱히 암생각없어요",
          "speaker_id": "2",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036879.1.16",
          "form": "저는 자극받고",
          "original_form": "저는 자극받고",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100036879.1.17",
          "form": "저녁메뉴",
          "original_form": "저녁메뉴",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100036879.1.18",
          "form": "닭가ㅅ살로 떼워요",
          "original_form": "닭가ㅅ살로 떼워요",
          "speaker_id": "1",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100036879.1.19",
          "form": "오 대단하세요 ㅋㅋ",
          "original_form": "오 대단하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:05"
        },
        {
          "id": "MDRW2100036879.1.20",
          "form": "저는 후기를 막 찾아보고있어요 레디톡 48시간",
          "original_form": "저는 후기를 막 찾아보고있어요 레디톡 48시간",
          "speaker_id": "2",
          "time": "20210831 19:05"
        },
        {
          "id": "MDRW2100036879.1.21",
          "form": "드시는 프로그램이 레디톡 48시간이예요",
          "original_form": "드시는 프로그램이 레디톡 48시간이예요",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100036879.1.22",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100036879.1.23",
          "form": "네 네이버치면 나오는 레디톡 48시간이에요",
          "original_form": "네 네이버치면 나오는 레디톡 48시간이에요",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100036879.1.24",
          "form": "네이버 메인에 그 썸네일로 막나오네요",
          "original_form": "네이버 메인에 그 썸네일로 막나오네요",
          "speaker_id": "2",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100036879.1.25",
          "form": "광고블로그들이 많네요",
          "original_form": "광고블로그들이 많네요",
          "speaker_id": "1",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100036879.1.26",
          "form": "찐후기가 궁금한데",
          "original_form": "찐후기가 궁금한데",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100036879.1.27",
          "form": "그죠 이제품은 무슨 마케팅만 엄청시리한듯",
          "original_form": "그죠 이제품은 무슨 마케팅만 엄청시리한듯",
          "speaker_id": "2",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100036879.1.28",
          "form": "맞아요 ㅜㅠ",
