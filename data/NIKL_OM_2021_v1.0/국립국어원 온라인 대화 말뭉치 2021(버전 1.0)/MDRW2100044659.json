      "utterance": [
        {
          "id": "MDRW2100044659.1.1",
          "form": "강아지 키워요?",
          "original_form": "강아지 키워요?",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044659.1.2",
          "form": "네^^ 지금도 제 무릎에 있어요^^",
          "original_form": "네^^ 지금도 제 무릎에 있어요^^",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044659.1.3",
          "form": "몇살이에용?",
          "original_form": "몇살이에용?",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044659.1.4",
          "form": "지금 9살이에요",
          "original_form": "지금 9살이에요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044659.1.5",
          "form": "무슨색인가요",
          "original_form": "무슨색인가요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044659.1.6",
          "form": "갈색이랑 흰색이요 ㅋㅋㅋㅋ 시츄에용",
          "original_form": "갈색이랑 흰색이요 ㅋㅋㅋㅋ 시츄에용",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044659.1.7",
          "form": "겁 많아요? ㅋㅋ",
          "original_form": "겁 많아요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044659.1.8",
          "form": "네 아주 겁쟁이에요 비만오면 덜덜 떨어요",
          "original_form": "네 아주 겁쟁이에요 비만오면 덜덜 떨어요",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044659.1.9",
          "form": "시츄가 겁쟁이래요 ㅋㅋ큐",
          "original_form": "시츄가 겁쟁이래요 ㅋㅋ큐",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044659.1.10",
          "form": "근데 겁쟁이여도 귀여워요ㅠㅠ 눈이 그렁그렁해서는 ㅜㅜㅠ",
          "original_form": "근데 겁쟁이여도 귀여워요ㅠㅠ 눈이 그렁그렁해서는 ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044659.1.11",
          "form": "오늘 비맞고 산책했어욤",
          "original_form": "오늘 비맞고 산책했어욤",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044659.1.12",
          "form": "아이구 비와도 산책해요",
          "original_form": "아이구 비와도 산책해요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044659.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044659.1.14",
          "form": "네 그냥 오늘 샤워시키려구요",
          "original_form": "네 그냥 오늘 샤워시키려구요",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044659.1.15",
          "form": "아항 매일 해야하나여 비와도?",
          "original_form": "아항 매일 해야하나여 비와도?",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044659.1.16",
          "form": "오늘은 비가 미스트같이 뿌리길래 ㅋㅋㅋㅋ이정도는 맞을만한가봐요",
          "original_form": "오늘은 비가 미스트같이 뿌리길래 ㅋㅋㅋㅋ이정도는 맞을만한가봐요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044659.1.17",
          "form": "아항 마니오면 또 겁내겟져?",
          "original_form": "아항 마니오면 또 겁내겟져?",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044659.1.18",
          "form": "네네 천둥치면 무서워해요",
          "original_form": "네네 천둥치면 무서워해요",
