      "utterance": [
        {
          "id": "MDRW2100040423.1.1",
          "form": "여행은 좋아하세요?",
          "original_form": "여행은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100040423.1.2",
          "form": "네네 좋아하죠",
          "original_form": "네네 좋아하죠",
          "speaker_id": "2",
          "time": "20210905 19:36"
        },
        {
          "id": "MDRW2100040423.1.3",
          "form": "다음주엔 여행가고파요",
          "original_form": "다음주엔 여행가고파요",
          "speaker_id": "2",
          "time": "20210905 19:36"
        },
        {
          "id": "MDRW2100040423.1.4",
          "form": "가면좋겠네요",
          "original_form": "가면좋겠네요",
          "speaker_id": "1",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100040423.1.5",
          "form": "부산이라고 하셨죠?",
          "original_form": "부산이라고 하셨죠?",
          "speaker_id": "1",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100040423.1.6",
          "form": "네네 맞아용",
          "original_form": "네네 맞아용",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100040423.1.7",
          "form": "경주나 통영 가고파요",
          "original_form": "경주나 통영 가고파요",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100040423.1.8",
          "form": "경주좋더라구요",
          "original_form": "경주좋더라구요",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100040423.1.9",
          "form": "얼마전에 갔는데",
          "original_form": "얼마전에 갔는데",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100040423.1.10",
          "form": "못보던곳들다녔어요",
          "original_form": "못보던곳들다녔어요",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100040423.1.11",
          "form": "부산이면 경주가깝지 않은가요?",
          "original_form": "부산이면 경주가깝지 않은가요?",
          "speaker_id": "1",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100040423.1.12",
          "form": "네네 버스로 한시간 반정도요",
          "original_form": "네네 버스로 한시간 반정도요",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100040423.1.13",
          "form": "어디 가셨나요 경주에",
          "original_form": "어디 가셨나요 경주에",
          "speaker_id": "2",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100040423.1.14",
          "form": "첨성대도가고 거리도구경하고..",
          "original_form": "첨성대도가고 거리도구경하고..",
          "speaker_id": "1",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040423.1.15",
          "form": "첨성대 공원 좋죠 사람많던가요?",
          "original_form": "첨성대 공원 좋죠 사람많던가요?",
          "speaker_id": "2",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100040423.1.16",
          "form": "네..엄청많아요",
          "original_form": "네..엄청많아요",
          "speaker_id": "1",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100040423.1.17",
          "form": "연날리는 사람도많던데",
          "original_form": "연날리는 사람도많던데",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100040423.1.18",
          "form": "여기저기 많아요",
          "original_form": "여기저기 많아요",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100040423.1.19",
          "form": "자전거타기도 좋죠?",
          "original_form": "자전거타기도 좋죠?",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100040423.1.20",
          "form": "네..곳곳이 다좋아요",
          "original_form": "네..곳곳이 다좋아요",
          "speaker_id": "1",
          "time": "20210905 20:39"
        }
      ]
