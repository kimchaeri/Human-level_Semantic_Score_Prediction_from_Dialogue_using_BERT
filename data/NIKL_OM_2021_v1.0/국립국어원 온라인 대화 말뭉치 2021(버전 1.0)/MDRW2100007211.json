      "utterance": [
        {
          "id": "MDRW2100007211.1.1",
          "form": "최근에 산건 뭐야?",
          "original_form": "최근에 산건 뭐야?",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007211.1.2",
          "form": "약국에서 식염수 샀어",
          "original_form": "약국에서 식염수 샀어",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007211.1.3",
          "form": "요즘 얼굴 트러블이 심각하거든",
          "original_form": "요즘 얼굴 트러블이 심각하거든",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007211.1.4",
          "form": "아ㅜㅜ",
          "original_form": "아ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007211.1.5",
          "form": "식염수는 어떻게 쓰는거야? 피부트러블에",
          "original_form": "식염수는 어떻게 쓰는거야? 피부트러블에",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007211.1.6",
          "form": "그냥 팩처럼 얼굴 진정 시켜주고 있어",
          "original_form": "그냥 팩처럼 얼굴 진정 시켜주고 있어",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007211.1.7",
          "form": "아하 그럼 효과가 있나봐",
          "original_form": "아하 그럼 효과가 있나봐",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007211.1.8",
          "form": "식염수 습포라고 한포진 같은데에 하던거래",
          "original_form": "식염수 습포라고 한포진 같은데에 하던거래",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007211.1.9",
          "form": "얼굴도 피부니깐 소독 진정되거든",
          "original_form": "얼굴도 피부니깐 소독 진정되거든",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007211.1.10",
          "form": "그렇구나ㅜ 효과가 짱짱하면 좋겠다ㅜ",
          "original_form": "그렇구나ㅜ 효과가 짱짱하면 좋겠다ㅜ",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007211.1.11",
          "form": "넌 최근에 새로 산게 있어?",
          "original_form": "넌 최근에 새로 산게 있어?",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007211.1.12",
          "form": "응 효과는 좋아 귀찮거든",
          "original_form": "응 효과는 좋아 귀찮거든",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007211.1.13",
          "form": "난 얼마전에 새핸드폰 질렀어",
          "original_form": "난 얼마전에 새핸드폰 질렀어",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007211.1.14",
          "form": "아직 받기 전이지만",
          "original_form": "아직 받기 전이지만",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007211.1.15",
          "form": "오 요즘 나오는 플립이야? 요즘 핫하다길래",
          "original_form": "오 요즘 나오는 플립이야? 요즘 핫하다길래",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007211.1.16",
          "form": "응응! 플립ㅋㅋㅋ",
          "original_form": "응응! 플립ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007211.1.17",
          "form": "무슨 색 골랐어?",
          "original_form": "무슨 색 골랐어?",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007211.1.18",
          "form": "크림으로!!",
          "original_form": "크림으로!!",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007211.1.19",
          "form": "요즘 색상 잘 뽑았다고 하던데",
          "original_form": "요즘 색상 잘 뽑았다고 하던데",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007211.1.20",
          "form": "처음에 그린했다가 맘이 흔들려 크림으로 바꿨엌ㅋ",
          "original_form": "처음에 그린했다가 맘이 흔들려 크림으로 바꿨엌ㅋ",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007211.1.21",
          "form": "오 이쁘겠다 폰꾸미기 기대되겠다",
          "original_form": "오 이쁘겠다 폰꾸미기 기대되겠다",
