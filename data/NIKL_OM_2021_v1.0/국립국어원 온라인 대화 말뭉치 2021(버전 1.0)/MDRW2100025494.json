      "utterance": [
        {
          "id": "MDRW2100025494.1.1",
          "form": "요즘 미용실 가고싶네요",
          "original_form": "요즘 미용실 가고싶네요",
          "speaker_id": "1",
          "time": "20210901 12:09"
        },
        {
          "id": "MDRW2100025494.1.2",
          "form": "저두요 ㅠㅠ",
          "original_form": "저두요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025494.1.3",
          "form": "염색 쫌 하고싶어요",
          "original_form": "염색 쫌 하고싶어요",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025494.1.4",
          "form": "뭐하고 싶으세요",
          "original_form": "뭐하고 싶으세요",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025494.1.5",
          "form": "저는 파마하고싶어요 ㅠㅠ근데 나이들어 보일까바요 ㅠ",
          "original_form": "저는 파마하고싶어요 ㅠㅠ근데 나이들어 보일까바요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:18"
        },
        {
          "id": "MDRW2100025494.1.6",
          "form": "파마하면 예뻐 보이니 괜찮을것같은데",
          "original_form": "파마하면 예뻐 보이니 괜찮을것같은데",
          "speaker_id": "1",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100025494.1.7",
          "form": "요즘 너무 먹어서 다이어트 해야해요",
          "original_form": "요즘 너무 먹어서 다이어트 해야해요",
          "speaker_id": "1",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100025494.1.8",
          "form": "저도 다이어트가하고싶어요",
          "original_form": "저도 다이어트가하고싶어요",
          "speaker_id": "2",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025494.1.9",
          "form": "친언니가 지흡권하더라구요",
          "original_form": "친언니가 지흡권하더라구요",
          "speaker_id": "2",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025494.1.10",
          "form": "지흡 멍든거 보면 무서운데 뱃살 빼고싶긴해요",
          "original_form": "지흡 멍든거 보면 무서운데 뱃살 빼고싶긴해요",
          "speaker_id": "1",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025494.1.11",
          "form": "효과 좋은가요?",
          "original_form": "효과 좋은가요?",
          "speaker_id": "1",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025494.1.12",
          "form": "친언니는 효과를 잘봤더라구하더라구요",
          "original_form": "친언니는 효과를 잘봤더라구하더라구요",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025494.1.13",
          "form": "했을때 식단을해야한데요",
          "original_form": "했을때 식단을해야한데요",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025494.1.14",
          "form": "역시 식단 필수인가봐요 다이어트는",
          "original_form": "역시 식단 필수인가봐요 다이어트는",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025494.1.15",
          "form": "맞아요 조곰만 관리해줘서 잘빠진다하는데 전 식욕이 강해서 ㅠㅠ",
          "original_form": "맞아요 조곰만 관리해줘서 잘빠진다하는데 전 식욕이 강해서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025494.1.16",
          "form": "저도 살빼긴 글럿나봐요",
          "original_form": "저도 살빼긴 글럿나봐요",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025494.1.17",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025494.1.18",
          "form": "운동이라도 해야하는데",
          "original_form": "운동이라도 해야하는데",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025494.1.19",
          "form": "움직이는것또한.. ㅋㅋㅋㅋㅋ",
          "original_form": "움직이는것또한.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025494.1.20",
          "form": "요즘 유투브 운동 영상 잘나오던데 하기가 귀찮네요",
          "original_form": "요즘 유투브 운동 영상 잘나오던데 하기가 귀찮네요",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025494.1.21",
          "form": "그것도 일주일만허고",
          "original_form": "그것도 일주일만허고",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025494.1.22",
          "form": "저는 접었어욬ㅋㅋㅋㅋ",
          "original_form": "저는 접었어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
