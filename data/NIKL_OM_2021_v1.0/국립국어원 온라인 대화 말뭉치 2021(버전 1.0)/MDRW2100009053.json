      "utterance": [
        {
          "id": "MDRW2100009053.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009053.1.2",
          "form": "군생활에 대해 아시는거있나요?",
          "original_form": "군생활에 대해 아시는거있나요?",
          "speaker_id": "1",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009053.1.3",
          "form": "아뇨 여자라서요",
          "original_form": "아뇨 여자라서요",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009053.1.4",
          "form": "그냥 군대가서 재수없으면 죽기도하고",
          "original_form": "그냥 군대가서 재수없으면 죽기도하고",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009053.1.5",
          "form": "축구많이한다 이정도요",
          "original_form": "축구많이한다 이정도요",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009053.1.6",
          "form": "얼마전 트랜스젠더 병사는 어찌생각하세요",
          "original_form": "얼마전 트랜스젠더 병사는 어찌생각하세요",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009053.1.7",
          "form": "군대가면 축구하겠죠?",
          "original_form": "군대가면 축구하겠죠?",
          "speaker_id": "1",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009053.1.8",
          "form": "그거도 군대얘기니까요",
          "original_form": "그거도 군대얘기니까요",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009053.1.9",
          "form": "네 축구하겠죠",
          "original_form": "네 축구하겠죠",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009053.1.10",
          "form": "트랜스젠더 병사가 성전환 후에도 여군으로 욜동하고 싶다구했는데 거절당했대요",
          "original_form": "트랜스젠더 병사가 성전환 후에도 여군으로 욜동하고 싶다구했는데 거절당했대요",
          "speaker_id": "1",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009053.1.11",
          "form": "그 병사 자. 살 한거 아세요?",
          "original_form": "그 병사 자. 살 한거 아세요?",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009053.1.12",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009053.1.13",
          "form": "아니요 저 몰랐어요",
          "original_form": "아니요 저 몰랐어요",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009053.1.14",
          "form": "왜요????어휴",
          "original_form": "왜요????어휴",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009053.1.15",
          "form": "군대에서 나가게되곰",
          "original_form": "군대에서 나가게되곰",
          "speaker_id": "2",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100009053.1.16",
          "form": "먹고살게 없었나봐요",
          "original_form": "먹고살게 없었나봐요",
          "speaker_id": "2",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100009053.1.17",
          "form": "어후 이게 무슨일인지ㅠㅠ",
          "original_form": "어후 이게 무슨일인지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:04"
        },
        {
          "id": "MDRW2100009053.1.18",
          "form": "안됐더라구요",
          "original_form": "안됐더라구요",
          "speaker_id": "2",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100009053.1.19",
          "form": "그 수술도 상관은 괜찮다한거같던데",
          "original_form": "그 수술도 상관은 괜찮다한거같던데",
          "speaker_id": "2",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100009053.1.20",
          "form": "더 윗선에서 나중에 안된다고했나봐요",
          "original_form": "더 윗선에서 나중에 안된다고했나봐요",
          "speaker_id": "2",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100009053.1.21",
          "form": "아ㅠㅠ",
          "original_form": "아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100009053.1.22",
          "form": "다들 뭐 그리 꽉 막혔는지",
          "original_form": "다들 뭐 그리 꽉 막혔는지",
          "speaker_id": "1",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100009053.1.23",
          "form": "진짜 안타깝네요ㅠ",
          "original_form": "진짜 안타깝네요ㅠ",
          "speaker_id": "1",
          "time": "20210822 21:08"
        }
      ]
