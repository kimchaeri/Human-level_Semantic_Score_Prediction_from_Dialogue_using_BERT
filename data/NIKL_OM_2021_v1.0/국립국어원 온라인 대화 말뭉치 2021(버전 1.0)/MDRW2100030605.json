        },
        {
          "id": "MDRW2100030605.1.2",
          "form": "어? 해군이세요?",
          "original_form": "어? 해군이세요?",
          "speaker_id": "2",
          "time": "20210904 10:09"
        },
        {
          "id": "MDRW2100030605.1.3",
          "form": "몇기십니까",
          "original_form": "몇기십니까",
          "speaker_id": "1",
          "time": "20210904 10:09"
        },
        {
          "id": "MDRW2100030605.1.4",
          "form": "아니신가보네",
          "original_form": "아니신가보네",
          "speaker_id": "2",
          "time": "20210904 10:11"
        },
        {
          "id": "MDRW2100030605.1.5",
          "form": "킹익 입니다 껄껄",
          "original_form": "킹익 입니다 껄껄",
          "speaker_id": "1",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030605.1.6",
          "form": "크 신의 아들",
          "original_form": "크 신의 아들",
          "speaker_id": "2",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100030605.1.7",
          "form": "군대 뭐 한번은 가볼만한데",
          "original_form": "군대 뭐 한번은 가볼만한데",
          "speaker_id": "2",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100030605.1.8",
          "form": "뺄 수 있음 좋죠",
          "original_form": "뺄 수 있음 좋죠",
          "speaker_id": "2",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100030605.1.9",
          "form": "어깨가 안좋아서 안보내주더라구요 ㅋㅋ",
          "original_form": "어깨가 안좋아서 안보내주더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100030605.1.10",
          "form": "어디다녀오셨나요",
          "original_form": "어디다녀오셨나요",
          "speaker_id": "1",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030605.1.11",
          "form": "저 해군 제대했습니다ㅋㅋㅋ",
          "original_form": "저 해군 제대했습니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100030605.1.12",
          "form": "고생하셨네요",
          "original_form": "고생하셨네요",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100030605.1.13",
          "form": "제친구도 해군다녀왔는데 천안함 터졌었다는..",
          "original_form": "제친구도 해군다녀왔는데 천안함 터졌었다는..",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100030605.1.14",
          "form": "저보다 조금 더 빨리 가셨네요ㅎㅎ",
          "original_form": "저보다 조금 더 빨리 가셨네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100030605.1.15",
          "form": "그 다음은 연평도...",
          "original_form": "그 다음은 연평도...",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100030605.1.16",
          "form": "진짜 그때 군생활하신분들 엄청 고생하셨겠네요 아휴",
          "original_form": "진짜 그때 군생활하신분들 엄청 고생하셨겠네요 아휴",
          "speaker_id": "1",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100030605.1.17",
          "form": "진짜 전시상황이었어요",
          "original_form": "진짜 전시상황이었어요",
          "speaker_id": "2",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100030605.1.18",
          "form": "진짜 얼마나 무서웠을까요 망할북한 ㅡㅡ",
          "original_form": "진짜 얼마나 무서웠을까요 망할북한 ㅡㅡ",
