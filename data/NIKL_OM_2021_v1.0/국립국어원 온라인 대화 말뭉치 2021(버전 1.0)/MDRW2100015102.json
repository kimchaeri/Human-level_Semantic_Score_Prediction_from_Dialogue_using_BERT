      "utterance": [
        {
          "id": "MDRW2100015102.1.1",
          "form": "최근 보신 드라마 있으세요",
          "original_form": "최근 보신 드라마 있으세요",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015102.1.2",
          "form": "드라마 보신거 있으신가요",
          "original_form": "드라마 보신거 있으신가요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015102.1.3",
          "form": "슬의생 잘 보고 있어요 name1님은요?",
          "original_form": "슬의생 잘 보고 있어요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015102.1.4",
          "form": "아 저는 최근에 펜트하우스봐써요",
          "original_form": "아 저는 최근에 펜트하우스봐써요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015102.1.5",
          "form": "슬의생 어때요? 재밌다고하던데",
          "original_form": "슬의생 어때요? 재밌다고하던데",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015102.1.6",
          "form": "재밌어요! 눈물빼는 에피소드도 많아서 혼자 있을때 봐야돼요",
          "original_form": "재밌어요! 눈물빼는 에피소드도 많아서 혼자 있을때 봐야돼요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015102.1.7",
          "form": "그렇게 슬픈가요",
          "original_form": "그렇게 슬픈가요",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015102.1.8",
          "form": "펜트하우스 원래 안보는데 티비 돌리다가 마지막 장면 보고 충격 받았어요",
          "original_form": "펜트하우스 원래 안보는데 티비 돌리다가 마지막 장면 보고 충격 받았어요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015102.1.9",
          "form": "아 저번주거요",
          "original_form": "아 저번주거요",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015102.1.10",
          "form": "아무리 청불이지만..너무 잔인하더라구요",
          "original_form": "아무리 청불이지만..너무 잔인하더라구요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015102.1.11",
          "form": "많이 자극적이긴 했어요",
          "original_form": "많이 자극적이긴 했어요",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015102.1.12",
          "form": "좀 과하더라구요",
          "original_form": "좀 과하더라구요",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015102.1.13",
          "form": "진짜 깜짝 놀랐어요ㅋㅋ 바로 고개 돌리긴했는데",
          "original_form": "진짜 깜짝 놀랐어요ㅋㅋ 바로 고개 돌리긴했는데",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015102.1.14",
          "form": "귀신영화 본 기분이더라구요",
          "original_form": "귀신영화 본 기분이더라구요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015102.1.15",
          "form": "그때만 그렇고 평소엔 괜찮나요?",
          "original_form": "그때만 그렇고 평소엔 괜찮나요?",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015102.1.16",
          "form": "약간 좀 고어스러운",
          "original_form": "약간 좀 고어스러운",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015102.1.17",
          "form": "아니요 사실 시즌1까지만 괜찮았고",
          "original_form": "아니요 사실 시즌1까지만 괜찮았고",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015102.1.18",
          "form": "2..도 억지 스러웠지만 뭐.. 하다가",
          "original_form": "2..도 억지 스러웠지만 뭐.. 하다가",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015102.1.19",
          "form": "3부터 너무 가더라구요",
          "original_form": "3부터 너무 가더라구요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015102.1.20",
          "form": "아하 그렇구나 슬의생 보세요 재밌어요!",
          "original_form": "아하 그렇구나 슬의생 보세요 재밌어요!",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015102.1.21",
          "form": "강추합니다",
          "original_form": "강추합니다",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015102.1.22",
          "form": "강추라니 꼭 봐야겠는걸요ㅎ",
          "original_form": "강추라니 꼭 봐야겠는걸요ㅎ",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015102.1.23",
          "form": "어떤에피소드가 기억에나아오",
          "original_form": "어떤에피소드가 기억에나아오",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015102.1.24",
          "form": "하나하나 다 재밌어서 딱히 추천해드리기가 어려운데 얼마전에 장기기증 관련한 에피소드 보고 많이 울었어요",
          "original_form": "하나하나 다 재밌어서 딱히 추천해드리기가 어려운데 얼마전에 장기기증 관련한 에피소드 보고 많이 울었어요",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015102.1.25",
          "form": "울고싶고 가슴이 답답한 날 보시면 눈물빼기 좋으실거에요",
          "original_form": "울고싶고 가슴이 답답한 날 보시면 눈물빼기 좋으실거에요",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015102.1.26",
          "form": "꼭 볼게요 추천 고맙습니다",
          "original_form": "꼭 볼게요 추천 고맙습니다",
          "speaker_id": "1",
          "time": "20210825 21:57"
        }
      ]
    }
