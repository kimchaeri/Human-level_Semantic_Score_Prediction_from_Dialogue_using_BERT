      "utterance": [
        {
          "id": "MDRW2100033609.1.1",
          "form": "방금까지 오다가 그쳤어요",
          "original_form": "방금까지 오다가 그쳤어요",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033609.1.2",
          "form": "거기는요?",
          "original_form": "거기는요?",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033609.1.3",
          "form": "여기도 비 멈춘지 몇시간은 지난 거 같아요",
          "original_form": "여기도 비 멈춘지 몇시간은 지난 거 같아요",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033609.1.4",
          "form": "약간 시원해요",
          "original_form": "약간 시원해요",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033609.1.5",
          "form": "약간 시원하고 습해요...",
          "original_form": "약간 시원하고 습해요...",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100033609.1.6",
          "form": "그래서 에어컨틀었어요",
          "original_form": "그래서 에어컨틀었어요",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100033609.1.7",
          "form": "습한건 맞아요",
          "original_form": "습한건 맞아요",
          "speaker_id": "1",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100033609.1.8",
          "form": "그래도 예전보다 덜해서 다행이에요",
          "original_form": "그래도 예전보다 덜해서 다행이에요",
          "speaker_id": "1",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100033609.1.9",
          "form": "맞아요 이제 여름도 끝나려나봐요",
          "original_form": "맞아요 이제 여름도 끝나려나봐요",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100033609.1.10",
          "form": "이제 조금씩 가을이 올 거 같기도 해요!",
          "original_form": "이제 조금씩 가을이 올 거 같기도 해요!",
          "speaker_id": "1",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100033609.1.11",
          "form": "너무 좋아요!!. 여름 힘들었어요",
          "original_form": "너무 좋아요!!. 여름 힘들었어요",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100033609.1.12",
          "form": "여름에 더워서 너무 지쳤죠 ㅠㅠㅠㅠㅋㅋ",
          "original_form": "여름에 더워서 너무 지쳤죠 ㅠㅠㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100033609.1.13",
          "form": "밖에서 조금만 돌아다녀도 땀이나서ㅠㅠㅠㅠ",
          "original_form": "밖에서 조금만 돌아다녀도 땀이나서ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100033609.1.14",
          "form": "거기다 마스크까지 하니 힘들더라구요",
          "original_form": "거기다 마스크까지 하니 힘들더라구요",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100033609.1.15",
          "form": "맞아요 마스크 때문에",
          "original_form": "맞아요 마스크 때문에",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100033609.1.16",
          "form": "숨이 턱턱 막혀요 ㅠㅠㅠ",
          "original_form": "숨이 턱턱 막혀요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100033609.1.17",
          "form": "그쵸ㅠㅠ 땀차서 트러블이나 나구요...",
          "original_form": "그쵸ㅠㅠ 땀차서 트러블이나 나구요...",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100033609.1.18",
          "form": "그래도 끝나가서 다행이예요",
          "original_form": "그래도 끝나가서 다행이예요",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100033609.1.19",
          "form": "이제 곧 가을이와요@!!!",
          "original_form": "이제 곧 가을이와요@!!!",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100033609.1.20",
          "form": "금방 겨울이 와서 롱패딩을 두르고 다니겠지만 가을을 즐겨봐요!",
          "original_form": "금방 겨울이 와서 롱패딩을 두르고 다니겠지만 가을을 즐겨봐요!",
          "speaker_id": "2",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100033609.1.21",
          "form": "ㅋㅋㅋㅋㅋ 짧은 가을 알차게 보내야죠",
          "original_form": "ㅋㅋㅋㅋㅋ 짧은 가을 알차게 보내야죠",
          "speaker_id": "1",
          "time": "20210824 17:23"
        }
