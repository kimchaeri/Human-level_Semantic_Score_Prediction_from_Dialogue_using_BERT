      "utterance": [
        {
          "id": "MDRW2100034376.1.1",
          "form": "식사하셨어요?!",
          "original_form": "식사하셨어요?!",
          "speaker_id": "1",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100034376.1.2",
          "form": "일어난지가 얼마안되가지고 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "일어난지가 얼마안되가지고 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100034376.1.3",
          "form": "아직 공복입니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아직 공복입니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100034376.1.4",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100034376.1.5",
          "form": "전 방금 또닭갈비 먹었어요",
          "original_form": "전 방금 또닭갈비 먹었어요",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100034376.1.6",
          "form": "오옷!! 아니 저 어제 샐러디 먹을거라고 했던가... ㅋㅋㅋ",
          "original_form": "오옷!! 아니 저 어제 샐러디 먹을거라고 했던가... ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034376.1.7",
          "form": "랩이고 뭐고",
          "original_form": "랩이고 뭐고",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034376.1.8",
          "form": "지금 밥 땡기네요",
          "original_form": "지금 밥 땡기네요",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034376.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100034376.1.10",
          "form": "한국인은 밥심이져!!!",
          "original_form": "한국인은 밥심이져!!!",
          "speaker_id": "1",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100034376.1.11",
          "form": "그니깐여 ㅋㅋㅋ 밥이 최고!!",
          "original_form": "그니깐여 ㅋㅋㅋ 밥이 최고!!",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034376.1.12",
          "form": "저는 약간 탄수화물 엄청 좋아해요",
          "original_form": "저는 약간 탄수화물 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034376.1.13",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034376.1.14",
          "form": "저는.. 탄수 당 지방 다좋아요",
          "original_form": "저는.. 탄수 당 지방 다좋아요",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034376.1.15",
          "form": "고기먹을때도 밥있어야해요",
          "original_form": "고기먹을때도 밥있어야해요",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034376.1.16",
          "form": "옼ㅋㅋㅋ",
          "original_form": "옼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034376.1.17",
          "form": "전 고기는 온리 고기만 먹어요",
          "original_form": "전 고기는 온리 고기만 먹어요",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034376.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 삼겹살이랑 쫄면 같이 먹으면 짱맛있는데!! 땡기네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 삼겹살이랑 쫄면 같이 먹으면 짱맛있는데!! 땡기네요",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100034376.1.19",
          "form": "쫄면은 사다먹어야하고 비빔면 같이먹는거도 유명하니까 비빔면도 같이먹어봤는데",
          "original_form": "쫄면은 사다먹어야하고 비빔면 같이먹는거도 유명하니까 비빔면도 같이먹어봤는데",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100034376.1.20",
          "form": "오 팔도도 좋져",
          "original_form": "오 팔도도 좋져",
          "speaker_id": "1",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100034376.1.21",
          "form": "그니깐여 같이 호로록 먹으면 좋더라구요",
          "original_form": "그니깐여 같이 호로록 먹으면 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100034376.1.22",
          "form": "침나요네요..",
          "original_form": "침나요네요..",
          "speaker_id": "1",
          "time": "20210828 13:44"
        }
      ]
