      "utterance": [
        {
          "id": "MDRW2100032080.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032080.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100032080.1.3",
          "form": "이제 거의 가을날씨가 다 됐네요",
          "original_form": "이제 거의 가을날씨가 다 됐네요",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032080.1.4",
          "form": "혹시 name2 계신 곳은 어떤가요?",
          "original_form": "혹시 &name2& 계신 곳은 어떤가요?",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032080.1.5",
          "form": "네 너무좋아요",
          "original_form": "네 너무좋아요",
          "speaker_id": "2",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032080.1.6",
          "form": "저는 대프리카라서",
          "original_form": "저는 대프리카라서",
          "speaker_id": "2",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032080.1.7",
          "form": "더 좋슴다ㅜㅜ",
          "original_form": "더 좋슴다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032080.1.8",
          "form": "헉 저도 대구 사는데...",
          "original_form": "헉 저도 대구 사는데...",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100032080.1.9",
          "form": "신기하네요 ㅋㅋㅋ",
          "original_form": "신기하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100032080.1.10",
          "form": "ㅋㅋ그러네요",
          "original_form": "ㅋㅋ그러네요",
          "speaker_id": "2",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100032080.1.11",
          "form": "무사히 여름 보냇슴다",
          "original_form": "무사히 여름 보냇슴다",
          "speaker_id": "2",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100032080.1.12",
          "form": "혹시 제일 좋아하는 계절은 언제이신가요??",
          "original_form": "혹시 제일 좋아하는 계절은 언제이신가요??",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032080.1.13",
          "form": "여름빼고 다좋아해요",
          "original_form": "여름빼고 다좋아해요",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032080.1.14",
          "form": "여름 너무 덥죠...",
          "original_form": "여름 너무 덥죠...",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032080.1.15",
          "form": "비오면 꿉꿉하고",
          "original_form": "비오면 꿉꿉하고",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032080.1.16",
          "form": "네 이제 여름끝이라서 너무좋슴다",
          "original_form": "네 이제 여름끝이라서 너무좋슴다",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100032080.1.17",
          "form": "확실히 요즘 많이 시원하긴 해요",
          "original_form": "확실히 요즘 많이 시원하긴 해요",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032080.1.18",
          "form": "추위는 많이 안 타시나봐요??",
          "original_form": "추위는 많이 안 타시나봐요??",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032080.1.19",
          "form": "네 땀쟁이라서",
          "original_form": "네 땀쟁이라서",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032080.1.20",
          "form": "추운거조아해요",
          "original_form": "추운거조아해요",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032080.1.21",
          "form": "저하고 너무 비슷하시다...",
          "original_form": "저하고 너무 비슷하시다...",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100032080.1.22",
          "form": "저 만나는 사람마다 땀 그만큼 흘리는 사람은 처음이래요",
          "original_form": "저 만나는 사람마다 땀 그만큼 흘리는 사람은 처음이래요",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100032080.1.23",
          "form": "ㅋㅋㅋ여름에 진짜 출퇴근만하고",
          "original_form": "ㅋㅋㅋ여름에 진짜 출퇴근만하고",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100032080.1.24",
          "form": "집콕하면서 몸사렷어요",
          "original_form": "집콕하면서 몸사렷어요",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100032080.1.25",
          "form": "좋아하는계절잇으신가요",
          "original_form": "좋아하는계절잇으신가요",
          "speaker_id": "2",
          "time": "20210905 23:28"
        }
      ]
    }
