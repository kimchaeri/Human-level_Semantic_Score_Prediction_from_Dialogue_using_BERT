        },
        {
          "id": "MDRW2100029133.1.6",
          "form": "name2 어머님은 어떤 스탈이세요?",
          "original_form": "&name2& 어머님은 어떤 스탈이세요?",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029133.1.7",
          "form": "name2 당신은 효자입니까?",
          "original_form": "&name2& 당신은 효자입니까?",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029133.1.8",
          "form": "저요?",
          "original_form": "저요?",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029133.1.9",
          "form": "효자가 어떤걸 말하는건가요?",
          "original_form": "효자가 어떤걸 말하는건가요?",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029133.1.10",
          "form": "네:) 엄마와의 관계?",
          "original_form": "네:) 엄마와의 관계?",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029133.1.11",
          "form": "어떤게 효자인건지",
          "original_form": "어떤게 효자인건지",
