      "utterance": [
        {
          "id": "MDRW2100009978.1.1",
          "form": "요즘 하는 게임 있어요?",
          "original_form": "요즘 하는 게임 있어요?",
          "speaker_id": "1",
          "time": "20210823 12:41"
        },
        {
          "id": "MDRW2100009978.1.2",
          "form": "스토쿠",
          "original_form": "스토쿠",
          "speaker_id": "2",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100009978.1.3",
          "form": "좋아하세요???ㅋㅋㅋ",
          "original_form": "좋아하세요???ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100009978.1.4",
          "form": "저요즘 스도쿠핮니다",
          "original_form": "저요즘 스도쿠핮니다",
          "speaker_id": "2",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100009978.1.5",
          "form": "네:) 저도 스도쿠 좋아해요",
          "original_form": "네:) 저도 스도쿠 좋아해요",
          "speaker_id": "1",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100009978.1.6",
          "form": "임신했을때도 \r\n왠지 태교에 좋을거 같아\r\n열심히 했었어염ㅋㅋ",
          "original_form": "임신했을때도 \r\n왠지 태교에 좋을거 같아\r\n열심히 했었어염ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100009978.1.7",
          "form": "호옹 ㅋㅋㅋ 그럼 빡고수신가욤 ㅋㅋ",
          "original_form": "호옹 ㅋㅋㅋ 그럼 빡고수신가욤 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100009978.1.8",
          "form": "다음에 만남\r\n스도쿠 함 댐비세요",
          "original_form": "다음에 만남\r\n스도쿠 함 댐비세요",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100009978.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100009978.1.10",
          "form": "아 연습해야겠네여",
          "original_form": "아 연습해야겠네여",
          "speaker_id": "2",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100009978.1.11",
          "form": "저 이렇게 말해놓고\r\n허세 부린거 되면 어쩌죵",
          "original_form": "저 이렇게 말해놓고\r\n허세 부린거 되면 어쩌죵",
          "speaker_id": "1",
          "time": "20210823 20:43"
        },
        {
          "id": "MDRW2100009978.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도 얼마안해봐서 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도 얼마안해봐서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100009978.1.13",
          "form": "예전에 애니팡은 좀 하셨나요?",
          "original_form": "예전에 애니팡은 좀 하셨나요?",
          "speaker_id": "1",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100009978.1.14",
          "form": "저..좀 했는데",
          "original_form": "저..좀 했는데",
          "speaker_id": "2",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100009978.1.15",
          "form": "나이드니까",
          "original_form": "나이드니까",
          "speaker_id": "2",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100009978.1.16",
          "form": "눈이아파요",
          "original_form": "눈이아파요",
          "speaker_id": "2",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100009978.1.17",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100009978.1.18",
          "form": "게임도 젊었을때 해야쥬ㅋㅋ",
          "original_form": "게임도 젊었을때 해야쥬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100009978.1.19",
          "form": "맞아유 ㅋㅋ",
          "original_form": "맞아유 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009978.1.20",
          "form": "보드게임같은거 잘해세유?",
          "original_form": "보드게임같은거 잘해세유?",
          "speaker_id": "2",
          "time": "20210824 08:04"
        },
        {
          "id": "MDRW2100009978.1.21",
          "form": "좋아하는데",
          "original_form": "좋아하는데",
          "speaker_id": "1",
          "time": "20210824 08:51"
        },
        {
          "id": "MDRW2100009978.1.22",
          "form": "해본지가 언제였더라요ㅋㅋ",
          "original_form": "해본지가 언제였더라요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 08:51"
        },
        {
          "id": "MDRW2100009978.1.23",
          "form": "언젠간 아들들과 함께할날이 오지않을ㅋ가유 ㅋㅋ",
          "original_form": "언젠간 아들들과 함께할날이 오지않을ㅋ가유 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:09"
        },
        {
          "id": "MDRW2100009978.1.24",
          "form": "그때쯤 되면\r\n폰으로 컴터로 겜들하고\r\n엄마랑은 안놀아주지 않을까요?ㅋㅋ",
          "original_form": "그때쯤 되면\r\n폰으로 컴터로 겜들하고\r\n엄마랑은 안놀아주지 않을까요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:16"
        },
        {
          "id": "MDRW2100009978.1.25",
          "form": "쿠키런 같은거 하고 있을거 같음ㅋㅋ",
          "original_form": "쿠키런 같은거 하고 있을거 같음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:16"
        },
        {
          "id": "MDRW2100009978.1.26",
          "form": "아..애기들..폰을 안놓는순간이 오겠구나 ㅠ",
          "original_form": "아..애기들..폰을 안놓는순간이 오겠구나 ㅠ",
          "speaker_id": "2",
          "time": "20210824 09:17"
        },
        {
          "id": "MDRW2100009978.1.27",
          "form": "그치ㅠ 꼬맹이들도 오겠지ㅋㅋ",
          "original_form": "그치ㅠ 꼬맹이들도 오겠지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:18"
        },
        {
          "id": "MDRW2100009978.1.28",
          "form": "그럼 집에서 각자 폰들고\r\n겜하느라 대화도 없어질듯ㅋㅋ",
          "original_form": "그럼 집에서 각자 폰들고\r\n겜하느라 대화도 없어질듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:18"
        },
        {
          "id": "MDRW2100009978.1.29",
          "form": "에횽... 지낮 그건 케어할라믄 전문가아니고서는...ㅜㅠ",
          "original_form": "에횽... 지낮 그건 케어할라믄 전문가아니고서는...ㅜㅠ",
          "speaker_id": "2",
          "time": "20210824 10:31"
        },
        {
          "id": "MDRW2100009978.1.30",
          "form": "힘들듯",
          "original_form": "힘들듯",
          "speaker_id": "2",
          "time": "20210824 10:31"
        },
        {
          "id": "MDRW2100009978.1.31",
          "form": "name2님 원래 모바일겜 하지 않으셨나염?",
          "original_form": "&name2&님 원래 모바일겜 하지 않으셨나염?",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100009978.1.32",
          "form": "이름이 생각이 안나넼ㅋ",
          "original_form": "이름이 생각이 안나넼ㅋ",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100009978.1.33",
          "form": "저도 기억이 ㅋㅋㅋ",
          "original_form": "저도 기억이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100009978.1.34",
          "form": "접은지 한참되었져",
          "original_form": "접은지 한참되었져",
          "speaker_id": "2",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100009978.1.35",
          "form": "귀찮아서",
          "original_form": "귀찮아서",
          "speaker_id": "2",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100009978.1.36",
          "form": "그것도 엄청 흥행해가지고\r\n주식도 많이 오른거 같던데",
          "original_form": "그것도 엄청 흥행해가지고\r\n주식도 많이 오른거 같던데",
          "speaker_id": "1",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100009978.1.37",
          "form": "주식??",
          "original_form": "주식??",
          "speaker_id": "2",
          "time": "20210824 11:59"
        },
        {
          "id": "MDRW2100009978.1.38",
          "form": "넹ㅋㅋ\r\n이제 나이가 들어가는건가\r\n게임도 잘 모르겠네요ㅋㅋㅈ",
          "original_form": "넹ㅋㅋ\r\n이제 나이가 들어가는건가\r\n게임도 잘 모르겠네요ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100009978.1.39",
          "form": "예전에는 신작게임이나\r\n인기있는 게임 정도는\r\n알고지냈던거 같은뎈ㅋ",
          "original_form": "예전에는 신작게임이나\r\n인기있는 게임 정도는\r\n알고지냈던거 같은뎈ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100009978.1.40",
          "form": "알아갈것도많고 할일도많고 ㅋㅋㅋ",
          "original_form": "알아갈것도많고 할일도많고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100009978.1.41",
          "form": "나이드니 한두가지가 어려운게아니네",
          "original_form": "나이드니 한두가지가 어려운게아니네",
