      "utterance": [
        {
          "id": "MDRW2100046345.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211019 10:58"
        },
        {
          "id": "MDRW2100046345.1.2",
          "form": "안녕하세요! 출근하셨나요?",
          "original_form": "안녕하세요! 출근하셨나요?",
          "speaker_id": "2",
          "time": "20211019 10:59"
        },
        {
          "id": "MDRW2100046345.1.3",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 10:59"
        },
        {
          "id": "MDRW2100046345.1.4",
          "form": "ㅋㅋㅋㅋ 어떤 일 하세요~?",
          "original_form": "ㅋㅋㅋㅋ 어떤 일 하세요~?",
          "speaker_id": "2",
          "time": "20211019 11:08"
        },
        {
          "id": "MDRW2100046345.1.5",
          "form": "아 저는 대학교 교직원으로 근무 중이에요!",
          "original_form": "아 저는 대학교 교직원으로 근무 중이에요!",
          "speaker_id": "1",
          "time": "20211019 11:10"
        },
        {
          "id": "MDRW2100046345.1.6",
          "form": "우와! 대학생의 젊은 기운을 받으며 일하시네요 ㅎㅎ",
          "original_form": "우와! 대학생의 젊은 기운을 받으며 일하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 11:12"
        },
        {
          "id": "MDRW2100046345.1.7",
          "form": "대학교 앞이면 맛집도 많겠어요!",
          "original_form": "대학교 앞이면 맛집도 많겠어요!",
          "speaker_id": "2",
          "time": "20211019 11:12"
        },
        {
          "id": "MDRW2100046345.1.8",
          "form": "네네ㅋㅋㅋㅋ젊은 기운을 받았으면 좋겠지만..",
          "original_form": "네네ㅋㅋㅋㅋ젊은 기운을 받았으면 좋겠지만..",
          "speaker_id": "1",
          "time": "20211019 11:13"
        },
        {
          "id": "MDRW2100046345.1.9",
          "form": "늘 꾸중만 듣고있어요ㅋㅋㅋ",
          "original_form": "늘 꾸중만 듣고있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 11:13"
        },
        {
          "id": "MDRW2100046345.1.10",
          "form": "학교가 작아서 주변에 맛집이 많이 없네요..",
          "original_form": "학교가 작아서 주변에 맛집이 많이 없네요..",
          "speaker_id": "1",
          "time": "20211019 11:13"
        },
        {
          "id": "MDRW2100046345.1.11",
          "form": "아 그렇군요 ㅠㅠ 오늘 점심 뭐 드실 거예요? ",
          "original_form": "아 그렇군요 ㅠㅠ 오늘 점심 뭐 드실 거예요? ",
          "speaker_id": "2",
          "time": "20211019 11:15"
        },
        {
          "id": "MDRW2100046345.1.12",
          "form": "전 오늘 평상시처럼 학식 먹을 것 같아요ㅋㅋ",
          "original_form": "전 오늘 평상시처럼 학식 먹을 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 11:17"
        },
        {
          "id": "MDRW2100046345.1.13",
          "form": "10년 째 학식 먹네요ㅠㅠ",
          "original_form": "10년 째 학식 먹네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20211019 11:17"
        },
        {
          "id": "MDRW2100046345.1.14",
          "form": "맛은 그냥 그렇지만 가성비로 승부보는..!",
          "original_form": "맛은 그냥 그렇지만 가성비로 승부보는..!",
          "speaker_id": "1",
          "time": "20211019 11:18"
        },
        {
          "id": "MDRW2100046345.1.15",
          "form": "name2님은 뭐 드실 생각이신가요",
          "original_form": "&name2&님은 뭐 드실 생각이신가요",
          "speaker_id": "1",
          "time": "20211019 11:18"
        },
        {
          "id": "MDRW2100046345.1.16",
          "form": "오 학식 오랜만에 먹고 싶네요 ㅎㅎ",
          "original_form": "오 학식 오랜만에 먹고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 11:20"
        },
        {
          "id": "MDRW2100046345.1.17",
          "form": "저는 오늘 떡볶이가 먹고 싶어요..!!",
          "original_form": "저는 오늘 떡볶이가 먹고 싶어요..!!",
          "speaker_id": "2",
          "time": "20211019 11:20"
        },
        {
          "id": "MDRW2100046345.1.18",
          "form": "오늘 학식 메뉴는 뭐예요?",
          "original_form": "오늘 학식 메뉴는 뭐예요?",
          "speaker_id": "2",
          "time": "20211019 11:20"
        },
        {
          "id": "MDRW2100046345.1.19",
          "form": "오 떡볶이 좋죠!!",
          "original_form": "오 떡볶이 좋죠!!",
          "speaker_id": "1",
          "time": "20211019 11:22"
        },
        {
          "id": "MDRW2100046345.1.20",
          "form": "오늘 오므라이스입니다ㅋㅋㅋ",
          "original_form": "오늘 오므라이스입니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 11:22"
        },
        {
          "id": "MDRW2100046345.1.21",
          "form": "가끔 학식 드시러 근처 대학교 가보세요 감회가 새록새록 날거에요ㅎㅎ",
          "original_form": "가끔 학식 드시러 근처 대학교 가보세요 감회가 새록새록 날거에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20211019 11:23"
        },
        {
          "id": "MDRW2100046345.1.22",
          "form": "전 학교 다닐 때도 학식을 잘 안 먹었던 거 같아요 ㅋㅋㅋ",
          "original_form": "전 학교 다닐 때도 학식을 잘 안 먹었던 거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 11:26"
        },
        {
          "id": "MDRW2100046345.1.23",
          "form": "오므라이스 부러운데요?!!",
          "original_form": "오므라이스 부러운데요?!!",
          "speaker_id": "2",
          "time": "20211019 11:26"
        },
        {
          "id": "MDRW2100046345.1.24",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20211019 11:26"
        },
        {
          "id": "MDRW2100046345.1.25",
          "form": "아하 저는 면 종류는 다 좋아해요ㅋㅋ",
          "original_form": "아하 저는 면 종류는 다 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 11:33"
        },
        {
          "id": "MDRW2100046345.1.26",
          "form": "특히 칼국수 잔치국수..!",
          "original_form": "특히 칼국수 잔치국수..!",
          "speaker_id": "1",
          "time": "20211019 11:34"
        },
        {
          "id": "MDRW2100046345.1.27",
          "form": "면류 좋아하시나요??",
          "original_form": "면류 좋아하시나요??",
          "speaker_id": "1",
          "time": "20211019 11:34"
        },
        {
          "id": "MDRW2100046345.1.28",
          "form": "오 저도 칼국수 좋아해요!",
          "original_form": "오 저도 칼국수 좋아해요!",
          "speaker_id": "2",
          "time": "20211019 11:43"
        },
        {
          "id": "MDRW2100046345.1.29",
          "form": "오늘 점심으로 칼국수도 생각해봐야겠네요 ㅎㅎ",
          "original_form": "오늘 점심으로 칼국수도 생각해봐야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 11:43"
        },
        {
          "id": "MDRW2100046345.1.30",
          "form": "오 좋아요!!",
          "original_form": "오 좋아요!!",
          "speaker_id": "1",
          "time": "20211019 11:44"
        },
        {
          "id": "MDRW2100046345.1.31",
          "form": "칼국수는 당연히 양념이 안 들어간 바지락 칼국수 추천드립니다ㅎㅎ",
          "original_form": "칼국수는 당연히 양념이 안 들어간 바지락 칼국수 추천드립니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20211019 11:44"
        },
        {
          "id": "MDRW2100046345.1.32",
