      "utterance": [
        {
          "id": "MDRW2100014397.1.1",
          "form": "연애요? 결혼이요?",
          "original_form": "연애요? 결혼이요?",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014397.1.2",
          "form": "나이가 드니까 이상형이 바뀌었어",
          "original_form": "나이가 드니까 이상형이 바뀌었어",
          "speaker_id": "2",
          "time": "20210827 10:19"
        },
        {
          "id": "MDRW2100014397.1.3",
          "form": "이제 장현승 보내준다",
          "original_form": "이제 장현승 보내준다",
          "speaker_id": "2",
          "time": "20210827 10:19"
        },
        {
          "id": "MDRW2100014397.1.4",
          "form": "하지만 심장은 약간 못보낸대",
          "original_form": "하지만 심장은 약간 못보낸대",
          "speaker_id": "2",
          "time": "20210827 10:19"
        },
        {
          "id": "MDRW2100014397.1.5",
          "form": "보내준단말 몇번쨉니까..",
          "original_form": "보내준단말 몇번쨉니까..",
          "speaker_id": "1",
          "time": "20210827 10:19"
        },
        {
          "id": "MDRW2100014397.1.6",
          "form": "이제 진짜 보내자..",
          "original_form": "이제 진짜 보내자..",
          "speaker_id": "1",
          "time": "20210827 10:20"
        },
        {
          "id": "MDRW2100014397.1.7",
          "form": "슬랜더 보내줘야하는데",
          "original_form": "슬랜더 보내줘야하는데",
          "speaker_id": "2",
          "time": "20210827 10:20"
        },
        {
          "id": "MDRW2100014397.1.8",
          "form": "그런애들보면 아직 심장 반응한다고",
          "original_form": "그런애들보면 아직 심장 반응한다고",
          "speaker_id": "2",
          "time": "20210827 10:20"
        },
        {
          "id": "MDRW2100014397.1.9",
          "form": "준호같은 애들 어디 없나",
          "original_form": "준호같은 애들 어디 없나",
          "speaker_id": "2",
          "time": "20210827 10:20"
        },
        {
          "id": "MDRW2100014397.1.10",
          "form": "ㅋㅋㅋㅋ..그럼애들또있나",
          "original_form": "ㅋㅋㅋㅋ..그럼애들또있나",
          "speaker_id": "1",
          "time": "20210827 10:20"
        },
        {
          "id": "MDRW2100014397.1.11",
          "form": "준호요..?",
          "original_form": "준호요..?",
          "speaker_id": "1",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100014397.1.12",
          "form": "없죠..",
          "original_form": "없죠..",
          "speaker_id": "1",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100014397.1.13",
          "form": "준호 얼굴 내스타일 아닌데 진짜 맘에들어",
          "original_form": "준호 얼굴 내스타일 아닌데 진짜 맘에들어",
          "speaker_id": "2",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100014397.1.14",
          "form": "준호랑 결혼하고싶다",
          "original_form": "준호랑 결혼하고싶다",
          "speaker_id": "2",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100014397.1.15",
          "form": "그의 얼굴과 몸 재력",
          "original_form": "그의 얼굴과 몸 재력",
          "speaker_id": "2",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100014397.1.16",
          "form": "성격조아",
          "original_form": "성격조아",
          "speaker_id": "1",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100014397.1.17",
          "form": "역시 볼수록 코드가 맞아야해..센스가있어야댜",
          "original_form": "역시 볼수록 코드가 맞아야해..센스가있어야댜",
          "speaker_id": "1",
          "time": "20210827 10:22"
        },
        {
          "id": "MDRW2100014397.1.18",
          "form": "어 알맹이는 가고 껍데기만 남았으면",
          "original_form": "어 알맹이는 가고 껍데기만 남았으면",
          "speaker_id": "2",
          "time": "20210827 10:22"
        },
        {
          "id": "MDRW2100014397.1.19",
          "form": "어차피 알맹이는 기대 안해",
          "original_form": "어차피 알맹이는 기대 안해",
          "speaker_id": "2",
          "time": "20210827 10:22"
        },
        {
          "id": "MDRW2100014397.1.20",
          "form": "개차반만아니면",
          "original_form": "개차반만아니면",
          "speaker_id": "2",
          "time": "20210827 10:22"
        },
        {
          "id": "MDRW2100014397.1.21",
          "form": "ㅋㅋㅋㄱㅇㄱ",
          "original_form": "ㅋㅋㅋㄱㅇㄱ",
          "speaker_id": "1",
          "time": "20210827 10:23"
        },
        {
          "id": "MDRW2100014397.1.22",
          "form": "그의 재력만 오면 도ㅑ",
          "original_form": "그의 재력만 오면 도ㅑ",
          "speaker_id": "2",
          "time": "20210827 10:23"
        },
        {
          "id": "MDRW2100014397.1.23",
          "form": "너무 기대치 낮자나 ㅋㅋ",
          "original_form": "너무 기대치 낮자나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:23"
        },
        {
