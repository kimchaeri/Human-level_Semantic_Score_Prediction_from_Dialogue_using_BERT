      "utterance": [
        {
          "id": "MDRW2100012748.1.1",
          "form": "군다 가기 시러...",
          "original_form": "군다 가기 시러...",
          "speaker_id": "1",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100012748.1.2",
          "form": "쓰읍..곧 가시나요",
          "original_form": "쓰읍..곧 가시나요",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012748.1.3",
          "form": "고2입니아...",
          "original_form": "고2입니아...",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012748.1.4",
          "form": "아직 약간 남으셨군요",
          "original_form": "아직 약간 남으셨군요",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012748.1.5",
          "form": "그 사이 재빨리 통일되기만을 바랍...니다..",
          "original_form": "그 사이 재빨리 통일되기만을 바랍...니다..",
          "speaker_id": "2",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012748.1.6",
          "form": "저는...저는..그 못가서..",
          "original_form": "저는...저는..그 못가서..",
          "speaker_id": "2",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100012748.1.7",
          "form": "공익이요?",
          "original_form": "공익이요?",
          "speaker_id": "1",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100012748.1.8",
          "form": "예 아뇨 전 그",
          "original_form": "예 아뇨 전 그",
          "speaker_id": "2",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100012748.1.9",
          "form": "그 여자라 안가지만요 그",
          "original_form": "그 여자라 안가지만요 그",
          "speaker_id": "2",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100012748.1.10",
          "form": "동생이 군대가있는데 혹시 그 썰이라도 풀까요",
          "original_form": "동생이 군대가있는데 혹시 그 썰이라도 풀까요",
          "speaker_id": "2",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100012748.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 00:41"
        },
        {
          "id": "MDRW2100012748.1.12",
          "form": "그 저희 동생이 군대에 갔는데",
          "original_form": "그 저희 동생이 군대에 갔는데",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012748.1.13",
          "form": "귀염둥이가 군대에 간다니까",
          "original_form": "귀염둥이가 군대에 간다니까",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012748.1.14",
          "form": "저희가족 다 울었죠",
          "original_form": "저희가족 다 울었죠",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012748.1.15",
          "form": "근데 이제 군대가면 폰을 준다하잖아요. 바로 주는 줄 알았는데 또 바로 안주대요?",
          "original_form": "근데 이제 군대가면 폰을 준다하잖아요. 바로 주는 줄 알았는데 또 바로 안주대요?",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012748.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012748.1.17",
          "form": "몇주동안 연락 끊겼다가 톡하는데 아주",
          "original_form": "몇주동안 연락 끊겼다가 톡하는데 아주",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012748.1.18",
          "form": "귀얌둥이라뇨 ㅋㅋㅋㅋ",
          "original_form": "귀얌둥이라뇨 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012748.1.19",
          "form": "첨으로 애뜻함을 느꼈어욬ㅋㅋㅋㅋㅋ",
          "original_form": "첨으로 애뜻함을 느꼈어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012748.1.20",
          "form": "군대 걱정 많으시겠어요ㅠㅠㅠ",
          "original_form": "군대 걱정 많으시겠어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100012748.1.21",
          "form": "아니 걱정이라기보다는 걍 싫으시겠어요",
          "original_form": "아니 걱정이라기보다는 걍 싫으시겠어요",
          "speaker_id": "2",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100012748.1.22",
          "form": "저라도..휴",
          "original_form": "저라도..휴",
          "speaker_id": "2",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100012748.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012748.1.24",
          "form": "몇살이세요?",
          "original_form": "몇살이세요?",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012748.1.25",
          "form": "군대 ㄹㅇ 노답",
          "original_form": "군대 ㄹㅇ 노답",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012748.1.26",
          "form": "예 저는 20대 이제 중반입니다",
          "original_form": "예 저는 20대 이제 중반입니다",
          "speaker_id": "2",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012748.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012748.1.28",
          "form": "그냥 가고 보내는 것 자체가 싫어욬ㅋㅋㅋㅋ",
          "original_form": "그냥 가고 보내는 것 자체가 싫어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100012748.1.29",
          "form": "엄...군대에 대해 들은 이야기같은거 있으신가요?",
          "original_form": "엄...군대에 대해 들은 이야기같은거 있으신가요?",
          "speaker_id": "2",
