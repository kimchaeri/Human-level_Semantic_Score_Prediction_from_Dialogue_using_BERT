      "utterance": [
        {
          "id": "MDRW2100005152.1.1",
          "form": "최근에 가본곳있으세요?",
          "original_form": "최근에 가본곳있으세요?",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005152.1.2",
          "form": "가보고싶은곳?",
          "original_form": "가보고싶은곳?",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005152.1.3",
          "form": "저 여름휴가로 성주 통영 다녀왔어요",
          "original_form": "저 여름휴가로 성주 통영 다녀왔어요",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005152.1.4",
          "form": "저는 강원도 고성, 양양이요",
          "original_form": "저는 강원도 고성, 양양이요",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005152.1.5",
          "form": "기억나는 여행지있으세요?",
          "original_form": "기억나는 여행지있으세요?",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005152.1.6",
          "form": "고성 양양쪽은 사람 많죠?",
          "original_form": "고성 양양쪽은 사람 많죠?",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005152.1.7",
          "form": "양양은 비치쪽에는 사람많은데 다른데는 망하기 직전ㅋㅋ\r\n고성은 디엠지에는 많은데 워낙 시골이라",
          "original_form": "양양은 비치쪽에는 사람많은데 다른데는 망하기 직전ㅋㅋ\r\n고성은 디엠지에는 많은데 워낙 시골이라",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005152.1.8",
          "form": "둘다 그렇게없었어요",
          "original_form": "둘다 그렇게없었어요",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005152.1.9",
          "form": "기억에 남는 여행지는 아가씨때 도쿄여행이 기억에 남아요 ㅎㅎ",
          "original_form": "기억에 남는 여행지는 아가씨때 도쿄여행이 기억에 남아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005152.1.10",
          "form": "도쿄갔을때 서울이랑 비슷해서 놀랬어요",
          "original_form": "도쿄갔을때 서울이랑 비슷해서 놀랬어요",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005152.1.11",
          "form": "한국인 여행객이 많아서 환경이 맞춰진건지",
          "original_form": "한국인 여행객이 많아서 환경이 맞춰진건지",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005152.1.12",
          "form": "저도 사람많을줄 알았는데 성주 자연휴양림 물놀이장에서 저희랑 다른 한팀만있어서 놀랬어요",
          "original_form": "저도 사람많을줄 알았는데 성주 자연휴양림 물놀이장에서 저희랑 다른 한팀만있어서 놀랬어요",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005152.1.13",
          "form": "맞아요 저도 서울에서 노는기분이였어요",
          "original_form": "맞아요 저도 서울에서 노는기분이였어요",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005152.1.14",
          "form": "물놀이 재밌었겠어요!!\r\n이번년도는 못갔어요ㅠㅠ\r\n마당에서 풀장 만들고 애기놀게하고",
          "original_form": "물놀이 재밌었겠어요!!\r\n이번년도는 못갔어요ㅠㅠ\r\n마당에서 풀장 만들고 애기놀게하고",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005152.1.15",
          "form": "한여름에 복직했는데 보건계열이라 코로나때문에 어디가기가 쉽지않더라고요ㅠ 감시중!",
          "original_form": "한여름에 복직했는데 보건계열이라 코로나때문에 어디가기가 쉽지않더라고요ㅠ 감시중!",
          "speaker_id": "1",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005152.1.16",
          "form": "아...진짜 답답하시겠어요",
          "original_form": "아...진짜 답답하시겠어요",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005152.1.17",
          "form": "도쿄간판이 생각나네요 한글막 써있고 ㅎㅎ",
          "original_form": "도쿄간판이 생각나네요 한글막 써있고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005152.1.18",
          "form": "19년도에 간 괌이 마지막이었어요",
          "original_form": "19년도에 간 괌이 마지막이었어요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005152.1.19",
          "form": "괌갔다오니 공항에 흰거입은사람들 천지고..",
          "original_form": "괌갔다오니 공항에 흰거입은사람들 천지고..",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005152.1.20",
          "form": "언제쯤 코로나가 없어질지ㅠ",
          "original_form": "언제쯤 코로나가 없어질지ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005152.1.21",
          "form": "아 20년도 초에 다녀왔어요",
          "original_form": "아 20년도 초에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005152.1.22",
          "form": "그러게요 ㅜㅠ",
          "original_form": "그러게요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005152.1.23",
          "form": "20년도 초면 대구만 코로나가 심할때니 ㅎㅎ",
          "original_form": "20년도 초면 대구만 코로나가 심할때니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005152.1.24",
          "form": "어디다녀오셨어요?",
          "original_form": "어디다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005152.1.25",
          "form": "괌이요!",
          "original_form": "괌이요!",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005152.1.26",
          "form": "미국 시골이라고 생각하시면되요",
          "original_form": "미국 시골이라고 생각하시면되요",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005152.1.27",
          "form": "공기좋고",
          "original_form": "공기좋고",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005152.1.28",
          "form": "태교여행 다녀왔어요",
          "original_form": "태교여행 다녀왔어요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005152.1.29",
          "form": "아 괌을 20년초에 다녀오신거군요",
          "original_form": "아 괌을 20년초에 다녀오신거군요",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005152.1.30",
          "form": "미국시골 와닿네요 ㅎㅎ",
          "original_form": "미국시골 와닿네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005152.1.31",
          "form": "너무 좋았어요 호수있으면 다들 수영하고",
          "original_form": "너무 좋았어요 호수있으면 다들 수영하고",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005152.1.32",
          "form": "외국 어디가보고싶으세요?",
          "original_form": "외국 어디가보고싶으세요?",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005152.1.33",
          "form": "호수에서 수영을요?",
          "original_form": "호수에서 수영을요?",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005152.1.34",
          "form": "맑은가보네요?",
          "original_form": "맑은가보네요?",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005152.1.35",
          "form": "네 진짜 물 색깔이 너무 영롱해요",
          "original_form": "네 진짜 물 색깔이 너무 영롱해요",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005152.1.36",
          "form": "저는 스위스요, ㅎ 가보고싶어요",
          "original_form": "저는 스위스요, ㅎ 가보고싶어요",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005152.1.37",
          "form": "호수마다 색이 다 다르고",
          "original_form": "호수마다 색이 다 다르고",
          "speaker_id": "1",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005152.1.38",
          "form": "초록빛?",
          "original_form": "초록빛?",
          "speaker_id": "1",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005152.1.39",
          "form": "외국인들은 살집상관없이 당당하게 비키니입고",
          "original_form": "외국인들은 살집상관없이 당당하게 비키니입고",
          "speaker_id": "1",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005152.1.40",
          "form": "그덕에 잘 놀다왔죠",
          "original_form": "그덕에 잘 놀다왔죠",
          "speaker_id": "1",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005152.1.41",
          "form": "와 진짜 상상이 안되요 ㅎㅎ",
          "original_form": "와 진짜 상상이 안되요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005152.1.42",
          "form": "꼭 한번 가봐야겠어요",
          "original_form": "꼭 한번 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005152.1.43",
          "form": "스위스요?",
          "original_form": "스위스요?",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005152.1.44",
          "form": "왜 가고싶어요?",
          "original_form": "왜 가고싶어요?",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005152.1.45",
          "form": "체르마트 인가? 풍경들이 너무 예쁜거같아서요 ㅎ",
          "original_form": "체르마트 인가? 풍경들이 너무 예쁜거같아서요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:42"
        }
      ]
