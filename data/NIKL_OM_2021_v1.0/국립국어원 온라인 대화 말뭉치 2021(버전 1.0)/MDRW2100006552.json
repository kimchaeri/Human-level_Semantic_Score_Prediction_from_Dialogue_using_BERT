      "utterance": [
        {
          "id": "MDRW2100006552.1.1",
          "form": "자녀계획은 어떻게 되시나요",
          "original_form": "자녀계획은 어떻게 되시나요",
          "speaker_id": "1",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100006552.1.2",
          "form": "1명만 생각 중이에요.",
          "original_form": "1명만 생각 중이에요.",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100006552.1.3",
          "form": "아 그러시군요 아직 출산 전인가요?",
          "original_form": "아 그러시군요 아직 출산 전인가요?",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100006552.1.4",
          "form": "네네 다음달이 예정일이에요!",
          "original_form": "네네 다음달이 예정일이에요!",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100006552.1.5",
          "form": "만삭이라 몸이 힘드시겠엉ㅅ",
          "original_form": "만삭이라 몸이 힘드시겠엉ㅅ",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100006552.1.6",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100006552.1.7",
          "form": "그래도 아직까지는 움직일만해서 괜찮은 것같아요",
          "original_form": "그래도 아직까지는 움직일만해서 괜찮은 것같아요",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100006552.1.8",
          "form": "그러시군요 자녀계획을 1명으로 정하신 이유가 있으실까요?",
          "original_form": "그러시군요 자녀계획을 1명으로 정하신 이유가 있으실까요?",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100006552.1.9",
          "form": "둘은 너무 힘들것 같아서요ㅠㅠ",
          "original_form": "둘은 너무 힘들것 같아서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006552.1.10",
          "form": "나중가서 생각이 바뀐다면 모르겠지만 지금은 한 명만이라도 잘 키우고 싶어요",
          "original_form": "나중가서 생각이 바뀐다면 모르겠지만 지금은 한 명만이라도 잘 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006552.1.11",
          "form": "그렇죠 맞습니다",
          "original_form": "그렇죠 맞습니다",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006552.1.12",
          "form": "처음엔 아무생각이 없었는데 아기는 새하얀 도화지같은 느낌인데 거기를 처음에 저랑 남편이 채워야한다는 부분이 너무 부담되서 하나라도 바르게 잘 키우는게 맞는것 같아요.",
          "original_form": "처음엔 아무생각이 없었는데 아기는 새하얀 도화지같은 느낌인데 거기를 처음에 저랑 남편이 채워야한다는 부분이 너무 부담되서 하나라도 바르게 잘 키우는게 맞는것 같아요.",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100006552.1.13",
          "form": "아 맞아요 많은 생각을 하고 계시네요~잘하실거예요",
          "original_form": "아 맞아요 많은 생각을 하고 계시네요~잘하실거예요",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100006552.1.14",
          "form": "ㅎㅎ 감사합니다ㅠㅠ 저도 제발 그랬으면 좋겠어요 ㅠㅠ",
          "original_form": "ㅎㅎ 감사합니다ㅠㅠ 저도 제발 그랬으면 좋겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100006552.1.15",
          "form": "태담도 하시나요?",
          "original_form": "태담도 하시나요?",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100006552.1.16",
          "form": "아기가 움직일때마다 하고 있어요!",
          "original_form": "아기가 움직일때마다 하고 있어요!",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100006552.1.17",
          "form": "태동이 안느껴지면 자고 았는건가 싶어서 아기는 안건들이려고 하네욬ㅋㅋ",
          "original_form": "태동이 안느껴지면 자고 았는건가 싶어서 아기는 안건들이려고 하네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100006552.1.18",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210908 02:30"
        }
      ]
