      "utterance": [
        {
          "id": "MDRW2100014487.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014487.1.2",
          "form": "TV보는 거 좋아하세요?",
          "original_form": "TV보는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014487.1.3",
          "form": "런닝맨 놀면뭐하니 유퀴즈 말고는 티비 거의안봐요 ㅎㅎ",
          "original_form": "런닝맨 놀면뭐하니 유퀴즈 말고는 티비 거의안봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014487.1.4",
          "form": "오 주로 예능을 보시는구나!",
          "original_form": "오 주로 예능을 보시는구나!",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014487.1.5",
          "form": "저는 슬의생도 보고",
          "original_form": "저는 슬의생도 보고",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014487.1.6",
          "form": "알고있지만도 봤어요ㅋㅋㅋ",
          "original_form": "알고있지만도 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014487.1.7",
          "form": "저는 영화덕후라서 거의 영화 미드봐요",
          "original_form": "저는 영화덕후라서 거의 영화 미드봐요",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014487.1.8",
          "form": "강추할만큼 재밌게본거 아무거나 추천해주세요ㅎㅎ",
          "original_form": "강추할만큼 재밌게본거 아무거나 추천해주세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014487.1.9",
          "form": "원픽!!",
          "original_form": "원픽!!",
          "speaker_id": "2",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014487.1.10",
          "form": "오 어떤 장르 좋아하세요?",
          "original_form": "오 어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014487.1.11",
          "form": "미드는 hbo꺼 제일 좋아해요",
          "original_form": "미드는 hbo꺼 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014487.1.12",
          "form": "체르노빌 왕좌의게임 등등등",
          "original_form": "체르노빌 왕좌의게임 등등등",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014487.1.13",
          "form": "hbo꺼 거의싹다봤어요",
          "original_form": "hbo꺼 거의싹다봤어요",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014487.1.14",
          "form": "오 대작을 보시는 구나",
          "original_form": "오 대작을 보시는 구나",
          "speaker_id": "1",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014487.1.15",
          "form": "전 미드는 가벼운 거 좋아해요",
          "original_form": "전 미드는 가벼운 거 좋아해요",
          "speaker_id": "1",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014487.1.16",
          "form": "웃고 넘기는 것들?",
          "original_form": "웃고 넘기는 것들?",
          "speaker_id": "1",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014487.1.17",
          "form": "모던패밀리랑 쿠거타운 같은 거요",
          "original_form": "모던패밀리랑 쿠거타운 같은 거요",
          "speaker_id": "1",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014487.1.18",
          "form": "빅뱅이론?",
          "original_form": "빅뱅이론?",
          "speaker_id": "2",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014487.1.19",
          "form": "오 맞아요!",
          "original_form": "오 맞아요!",
          "speaker_id": "1",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014487.1.20",
          "form": "그것도 재밌죠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그것도 재밌죠ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014487.1.21",
          "form": "저는 다크하고심오한취향인거같아요 ㅎㅎ\r\n기묘한이야기 ㅎㅎ",
          "original_form": "저는 다크하고심오한취향인거같아요 ㅎㅎ\r\n기묘한이야기 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014487.1.22",
          "form": "재밋다는건 다봐요 ㅎㅎ",
          "original_form": "재밋다는건 다봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014487.1.23",
          "form": "저 깜짝 놀라는 걸 잘 못봐서",
          "original_form": "저 깜짝 놀라는 걸 잘 못봐서",
          "speaker_id": "1",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014487.1.24",
          "form": "기묘한 이야기 추천은 엄청 받았는데",
          "original_form": "기묘한 이야기 추천은 엄청 받았는데",
          "speaker_id": "1",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014487.1.25",
          "form": "아직 못 봤어요",
          "original_form": "아직 못 봤어요",
          "speaker_id": "1",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014487.1.26",
          "form": "그럼 완전 비츄",
          "original_form": "그럼 완전 비츄",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014487.1.27",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014487.1.28",
          "form": "최근에 영화관에서 모가디슈 봣는데",
          "original_form": "최근에 영화관에서 모가디슈 봣는데",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014487.1.29",
          "form": "진짜재밋게봤어요",
          "original_form": "진짜재밋게봤어요",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014487.1.30",
          "form": "오?!",
          "original_form": "오?!",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014487.1.31",
          "form": "조인성 씨 나오는 거지요?",
          "original_form": "조인성 씨 나오는 거지요?",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014487.1.32",
          "form": "이시국이랑겹쳐서 더 인상깊어요ㅜㅜ",
          "original_form": "이시국이랑겹쳐서 더 인상깊어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014487.1.33",
          "form": "아프간 ㅠㅠ",
          "original_form": "아프간 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014487.1.34",
          "form": "혹시안보셧음 영화관아니더라도 나중에 꼭 보세요",
          "original_form": "혹시안보셧음 영화관아니더라도 나중에 꼭 보세요",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014487.1.35",
          "form": "어머 그렇구나",
          "original_form": "어머 그렇구나",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014487.1.36",
          "form": "네 꼭 볼게요!",
          "original_form": "네 꼭 볼게요!",
          "speaker_id": "1",
          "time": "20210825 19:44"
        }
      ]
    }
