      "utterance": [
        {
          "id": "MDRW2100023177.1.1",
          "form": "그치만 엄마 내가 먹고싶엇던건 평범한 공장제 잼이엇어......",
          "original_form": "그치만 엄마 내가 먹고싶엇던건 평범한 공장제 잼이엇어......",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023177.1.2",
          "form": "그걸 8시간동안 끓인다고",
          "original_form": "그걸 8시간동안 끓인다고",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023177.1.3",
          "form": "식구들 번갈아가면서",
          "original_form": "식구들 번갈아가면서",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023177.1.4",
          "form": "저었었음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저었었음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023177.1.5",
          "form": "얼탱쓰",
          "original_form": "얼탱쓰",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023177.1.6",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023177.1.7",
          "form": "그정도면 먹기도 전에 냄새로 질릴듯",
          "original_form": "그정도면 먹기도 전에 냄새로 질릴듯",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023177.1.8",
          "form": "좀?",
          "original_form": "좀?",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023177.1.9",
          "form": "그리고 살짝 타고",
          "original_form": "그리고 살짝 타고",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023177.1.10",
          "form": "그와중에 탔냐곸",
          "original_form": "그와중에 탔냐곸",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023177.1.11",
          "form": "너무 졸아서 건더기만 가득햇서",
          "original_form": "너무 졸아서 건더기만 가득햇서",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023177.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저런....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저런....",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023177.1.13",
          "form": "역시 음식은",
          "original_form": "역시 음식은",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023177.1.14",
          "form": "사먹어야돼",
          "original_form": "사먹어야돼",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023177.1.15",
          "form": "슬픈 딸기잼...",
          "original_form": "슬픈 딸기잼...",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023177.1.16",
          "form": "인간이 공장을 돌리기 시작한데에는",
          "original_form": "인간이 공장을 돌리기 시작한데에는",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023177.1.17",
          "form": "이유가 잇다",
          "original_form": "이유가 잇다",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023177.1.18",
          "form": "마자 다 이유가 있는 법이지...",
          "original_form": "마자 다 이유가 있는 법이지...",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023177.1.19",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023177.1.20",
          "form": "일케 과일 위에 햄류 얹는걸",
          "original_form": "일케 과일 위에 햄류 얹는걸",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023177.1.21",
          "form": "프로슈토라고 하는데",
          "original_form": "프로슈토라고 하는데",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023177.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ하몽사서 프로슈토해먹기",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ하몽사서 프로슈토해먹기",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023177.1.23",
          "form": "술안주로 좋고",
          "original_form": "술안주로 좋고",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023177.1.24",
          "form": "ㅇㅇ단짠이야",
          "original_form": "ㅇㅇ단짠이야",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023177.1.25",
          "form": "보통은 무르고 달달한 과일 위에 얹더라",
          "original_form": "보통은 무르고 달달한 과일 위에 얹더라",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023177.1.26",
          "form": "마싯겟다",
          "original_form": "마싯겟다",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023177.1.27",
          "form": "근데 내가 와인 취미가 없어서",
          "original_form": "근데 내가 와인 취미가 없어서",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023177.1.28",
          "form": "그냥 먹어도 맛있어",
          "original_form": "그냥 먹어도 맛있어",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023177.1.29",
          "form": "먹으면 음료수랑일듯",
          "original_form": "먹으면 음료수랑일듯",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023177.1.30",
          "form": "맛없을 수 없는 조합임 ㅋㅋㅋㅋ",
          "original_form": "맛없을 수 없는 조합임 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023177.1.31",
          "form": "그치만 그런 술안주들은",
          "original_form": "그치만 그런 술안주들은",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023177.1.32",
          "form": "그것만 먹으면 뭔가 모자라",
          "original_form": "그것만 먹으면 뭔가 모자라",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023177.1.33",
          "form": "글킨 해 ㅋㅋㅋㅋ",
          "original_form": "글킨 해 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023177.1.34",
          "form": "주로 양이",
          "original_form": "주로 양이",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023177.1.35",
          "form": "그럴때 난 보통",
          "original_form": "그럴때 난 보통",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023177.1.36",
          "form": "탄산수랑 먹음",
          "original_form": "탄산수랑 먹음",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023177.1.37",
          "form": "맥주랑 먹는 기분은 내는데 술마시긴 싫을 때",
          "original_form": "맥주랑 먹는 기분은 내는데 술마시긴 싫을 때",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023177.1.38",
          "form": "난 사실 탄산수도 싫어해서",
          "original_form": "난 사실 탄산수도 싫어해서",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023177.1.39",
          "form": "차라리 얼음물이야",
          "original_form": "차라리 얼음물이야",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023177.1.40",
          "form": "탄산수는 안에 뭔가 청을 탔을때만",
          "original_form": "탄산수는 안에 뭔가 청을 탔을때만",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023177.1.41",
          "form": "아닠",
          "original_form": "아닠",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023177.1.42",
          "form": "그치만 술안주들 기름져서",
          "original_form": "그치만 술안주들 기름져서",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023177.1.43",
          "form": "용납가능한 존재이다",
          "original_form": "용납가능한 존재이다",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023177.1.44",
          "form": "일반 물이랑 먹기엔 좀 밸런스가 안맞아",
          "original_form": "일반 물이랑 먹기엔 좀 밸런스가 안맞아",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023177.1.45",
          "form": "내장에 기름칠하는거지",
          "original_form": "내장에 기름칠하는거지",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023177.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ적나라하냌",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ적나라하냌",
          "speaker_id": "1",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023177.1.47",
          "form": "그래서 매실청을 타마시지",
          "original_form": "그래서 매실청을 타마시지",
          "speaker_id": "2",
          "time": "20210829 22:37"
        }
      ]
    }
