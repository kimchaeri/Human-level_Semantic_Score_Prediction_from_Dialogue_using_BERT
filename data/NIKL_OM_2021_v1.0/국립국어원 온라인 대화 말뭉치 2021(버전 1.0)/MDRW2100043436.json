      "utterance": [
        {
          "id": "MDRW2100043436.1.1",
          "form": "날씨가 추워졋네요 ㅠ",
          "original_form": "날씨가 추워졋네요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043436.1.2",
          "form": "네네ㅠ 저희는 그래도 어늘 더웟어요",
          "original_form": "네네ㅠ 저희는 그래도 어늘 더웟어요",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043436.1.3",
          "form": "아정말요?? 비안오세요??",
          "original_form": "아정말요?? 비안오세요??",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043436.1.4",
          "form": "비 와용 ㅋㅋㅋㅋ",
          "original_form": "비 와용 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043436.1.5",
          "form": "비왓는데도 더웟군요 ㅠㅠ",
          "original_form": "비왓는데도 더웟군요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043436.1.6",
          "form": "비오는날 좋아하세요?",
          "original_form": "비오는날 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043436.1.7",
          "form": "네네 실내에 있을때만요 ㅋㅋㅋ",
          "original_form": "네네 실내에 있을때만요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043436.1.8",
          "form": "오 맞아요 실내에있을땐 분위기 있죠 ㅋㅋ",
          "original_form": "오 맞아요 실내에있을땐 분위기 있죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043436.1.9",
          "form": "가디건 하나 챙겨야할거같아요",
          "original_form": "가디건 하나 챙겨야할거같아요",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043436.1.10",
          "form": "장화 사고싶어요",
          "original_form": "장화 사고싶어요",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043436.1.11",
          "form": "오 맞아요 요즘 많이 신고 다니드라구요!!",
          "original_form": "오 맞아요 요즘 많이 신고 다니드라구요!!",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043436.1.12",
          "form": "여름엔 양산 썻는데 ㅋㅋ",
          "original_form": "여름엔 양산 썻는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043436.1.13",
          "form": "우양산 많이 쓰이네요 요즘도",
          "original_form": "우양산 많이 쓰이네요 요즘도",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043436.1.14",
          "form": "네넵!! 훨씬 좋드라구요 ㅋㅋ",
          "original_form": "네넵!! 훨씬 좋드라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043436.1.15",
