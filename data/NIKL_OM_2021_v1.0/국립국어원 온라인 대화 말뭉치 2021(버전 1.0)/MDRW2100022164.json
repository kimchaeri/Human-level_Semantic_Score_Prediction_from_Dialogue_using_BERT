      "utterance": [
        {
          "id": "MDRW2100022164.1.1",
          "form": "지금 비오나욤?",
          "original_form": "지금 비오나욤?",
          "speaker_id": "1",
          "time": "20210829 10:11"
        },
        {
          "id": "MDRW2100022164.1.2",
          "form": "아까 안오던데",
          "original_form": "아까 안오던데",
          "speaker_id": "2",
          "time": "20210829 10:14"
        },
        {
          "id": "MDRW2100022164.1.3",
          "form": "거기는오나요",
          "original_form": "거기는오나요",
          "speaker_id": "2",
          "time": "20210829 10:14"
        },
        {
          "id": "MDRW2100022164.1.4",
          "form": "저는 창문없는곳이라",
          "original_form": "저는 창문없는곳이라",
          "speaker_id": "1",
          "time": "20210829 10:16"
        },
        {
          "id": "MDRW2100022164.1.5",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "1",
          "time": "20210829 10:16"
        },
        {
          "id": "MDRW2100022164.1.6",
          "form": "똑똑똑",
          "original_form": "똑똑똑",
          "speaker_id": "1",
          "time": "20210829 10:23"
        },
        {
          "id": "MDRW2100022164.1.7",
          "form": "날씨가흐려요",
          "original_form": "날씨가흐려요",
          "speaker_id": "2",
          "time": "20210829 10:23"
        },
        {
          "id": "MDRW2100022164.1.8",
          "form": "어디가세요?",
          "original_form": "어디가세요?",
          "speaker_id": "2",
          "time": "20210829 10:23"
        },
        {
          "id": "MDRW2100022164.1.9",
          "form": "쫌이따",
          "original_form": "쫌이따",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.10",
          "form": "조카돌보미하러가유",
          "original_form": "조카돌보미하러가유",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.11",
          "form": "모하세여",
          "original_form": "모하세여",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.12",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "2",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.13",
          "form": "전 자유",
          "original_form": "전 자유",
          "speaker_id": "2",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.14",
          "form": "에요",
          "original_form": "에요",
          "speaker_id": "2",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.15",
          "form": "생각만해도 허리아프네요",
          "original_form": "생각만해도 허리아프네요",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.16",
          "form": "아직좀더해주세유",
          "original_form": "아직좀더해주세유",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.17",
          "form": "열한시까지만",
          "original_form": "열한시까지만",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.18",
          "form": "붙잡을게욤",
          "original_form": "붙잡을게욤",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.19",
          "form": "일하세요?",
          "original_form": "일하세요?",
          "speaker_id": "2",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100022164.1.20",
          "form": "비올까요?",
          "original_form": "비올까요?",
          "speaker_id": "2",
          "time": "20210829 10:25"
        },
        {
          "id": "MDRW2100022164.1.21",
          "form": "네이버는 한시까지 비온다는 표시는잇네욤",
          "original_form": "네이버는 한시까지 비온다는 표시는잇네욤",
          "speaker_id": "1",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022164.1.22",
          "form": "여기 저만한가하고",
          "original_form": "여기 저만한가하고",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.23",
          "form": "다 바쁘네욤",
          "original_form": "다 바쁘네욤",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.24",
          "form": "요즘 비가 많이오는거같아요",
          "original_form": "요즘 비가 많이오는거같아요",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.25",
          "form": "컴플레인도걸리공",
          "original_form": "컴플레인도걸리공",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.26",
          "form": "우산챙겼어요?",
          "original_form": "우산챙겼어요?",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.27",
          "form": "조심하세요",
          "original_form": "조심하세요",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.28",
          "form": "안챙겻어욤",
          "original_form": "안챙겻어욤",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.29",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.30",
          "form": "챙겨다니세요",
          "original_form": "챙겨다니세요",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.31",
          "form": "날씨가 오락가락합니다",
          "original_form": "날씨가 오락가락합니다",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100022164.1.32",
          "form": "가을장마인가요",
          "original_form": "가을장마인가요",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100022164.1.33",
          "form": "가을장마 맞아욤",
          "original_form": "가을장마 맞아욤",
          "speaker_id": "1",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100022164.1.34",
          "form": "어제날씨",
          "original_form": "어제날씨",
          "speaker_id": "1",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100022164.1.35",
          "form": "진짜좋앗슴다",
          "original_form": "진짜좋앗슴다",
          "speaker_id": "1",
          "time": "20210829 10:37"
        }
      ]
