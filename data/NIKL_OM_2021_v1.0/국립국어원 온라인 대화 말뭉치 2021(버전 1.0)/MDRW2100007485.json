        {
          "id": "MDRW2100007485.1.1",
          "form": "수강 신청 성공하셨나요?",
          "original_form": "수강 신청 성공하셨나요?",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007485.1.2",
          "form": "전 올클리어까진 아니지만",
          "original_form": "전 올클리어까진 아니지만",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007485.1.3",
          "form": "ㅋㅋ나름 괜찮게 잡았어요",
          "original_form": "ㅋㅋ나름 괜찮게 잡았어요",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007485.1.4",
          "form": "19학점이욧",
          "original_form": "19학점이욧",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007485.1.5",
          "form": "전 이번에 살짝 무리해서 21학점 들으려고요",
          "original_form": "전 이번에 살짝 무리해서 21학점 들으려고요",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007485.1.6",
          "form": "대신 공강따윈 포기했습니다...",
          "original_form": "대신 공강따윈 포기했습니다...",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007485.1.7",
          "form": "교양은 꿀교양 잡으셨나요?",
          "original_form": "교양은 꿀교양 잡으셨나요?",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007485.1.8",
          "form": "전 인기좋은거 운좋게 잡아서 기분 좋아요 ㅎㅎ",
          "original_form": "전 인기좋은거 운좋게 잡아서 기분 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007485.1.9",
          "form": "저 역시 꿀교양을 잡았답니다ㅎㅎ",
          "original_form": "저 역시 꿀교양을 잡았답니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007485.1.10",
          "form": "어떤 거 잡으셨어요??",
          "original_form": "어떤 거 잡으셨어요??",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007485.1.11",
          "form": "저희 학교는 한국사가 인기가 많아요",
          "original_form": "저희 학교는 한국사가 인기가 많아요",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007485.1.12",
          "form": "저희는 봉사기획 교양이 있는데",
          "original_form": "저희는 봉사기획 교양이 있는데",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007485.1.13",
          "form": "녹강에 학점도 후하다셔서 ㅋㅋㅋㅋㅋ",
          "original_form": "녹강에 학점도 후하다셔서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007485.1.14",
          "form": "그냥 할거하면 a0는 보통 주신대요",
          "original_form": "그냥 할거하면 a0는 보통 주신대요",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007485.1.15",
          "form": "녹강에 학점느님이라 완벽하네요",
          "original_form": "녹강에 학점느님이라 완벽하네요",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007485.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007485.1.17",
          "form": "학교 주변에 즐길 거리는 꽤 있는 편이신가요?",
          "original_form": "학교 주변에 즐길 거리는 꽤 있는 편이신가요?",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007485.1.18",
          "form": "그러면 대면의 묘미가 있을텐데...",
          "original_form": "그러면 대면의 묘미가 있을텐데...",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007485.1.19",
          "form": "네 그래도 앞쪽에 상권이 있긴해요",
          "original_form": "네 그래도 앞쪽에 상권이 있긴해요",
