      "utterance": [
        {
          "id": "MDRW2100015674.1.1",
          "form": "커뮤니티 하시나요?",
          "original_form": "커뮤니티 하시나요?",
          "speaker_id": "1",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015674.1.2",
          "form": "아뇨ㅠㅠ 하시는거 있으세요?",
          "original_form": "아뇨ㅠㅠ 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100015674.1.3",
          "form": "아니요 저도 없어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아니요 저도 없어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100015674.1.4",
          "form": "온라인 사람을 오프라인에서 만나는분들 보면 신기해요",
          "original_form": "온라인 사람을 오프라인에서 만나는분들 보면 신기해요",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100015674.1.5",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100015674.1.6",
          "form": "주변에 정모 나가는 분 있으세요?",
          "original_form": "주변에 정모 나가는 분 있으세요?",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100015674.1.7",
          "form": "제친구는 게임에서 만난 사람들 만나던데..위험해보여요ㅋㅋ",
          "original_form": "제친구는 게임에서 만난 사람들 만나던데..위험해보여요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100015674.1.8",
          "form": "제친구는 절친이 되더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "제친구는 절친이 되더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100015674.1.9",
          "form": "신기햇어요",
          "original_form": "신기햇어요",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100015674.1.10",
          "form": "진짜요ㅋㅋㅋ저는 의심이 많아서ㅋㅋㅋ",
          "original_form": "진짜요ㅋㅋㅋ저는 의심이 많아서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100015674.1.11",
          "form": "저는 겁이 많아서 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 겁이 많아서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100015674.1.12",
          "form": "저구요...근데 다니는분들은 아주 잘 나가더라구요",
          "original_form": "저구요...근데 다니는분들은 아주 잘 나가더라구요",
          "speaker_id": "2",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100015674.1.13",
          "form": "맞아요 스스럼없이 정모 나가시는 분들 신기해요",
          "original_form": "맞아요 스스럼없이 정모 나가시는 분들 신기해요",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100015674.1.14",
          "form": "저도...저는 못나갈거같아요ㅋㅋ",
          "original_form": "저도...저는 못나갈거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
