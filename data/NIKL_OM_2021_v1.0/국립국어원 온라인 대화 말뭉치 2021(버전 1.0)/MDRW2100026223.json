      "utterance": [
        {
          "id": "MDRW2100026223.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026223.1.2",
          "form": "안녕하세요! 거기도 지금 비 안오죠?",
          "original_form": "안녕하세요! 거기도 지금 비 안오죠?",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026223.1.3",
          "form": "네네 지금은 습한데 시원해요",
          "original_form": "네네 지금은 습한데 시원해요",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026223.1.4",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026223.1.5",
          "form": "저는 겨울이요!",
          "original_form": "저는 겨울이요!",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026223.1.6",
          "form": "사실 가을~겨울쯤?? 너무 한겨울 말고요ㅜㅜ",
          "original_form": "사실 가을~겨울쯤?? 너무 한겨울 말고요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026223.1.7",
          "form": "붕어빵도 먹을 수 있고 ㅋㅋㅋ 타코야키... 크리스마스... 연말 분위기... 다 너무 좋아요!!",
          "original_form": "붕어빵도 먹을 수 있고 ㅋㅋㅋ 타코야키... 크리스마스... 연말 분위기... 다 너무 좋아요!!",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026223.1.8",
          "form": "오오오 저도요!!!!",
          "original_form": "오오오 저도요!!!!",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026223.1.9",
          "form": "진짜 연말만의 그 분위기는 너무 좋아요",
          "original_form": "진짜 연말만의 그 분위기는 너무 좋아요",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026223.1.10",
          "form": "사실 전 여름빼곤 다 좋아요ㅋㅋㅋ",
          "original_form": "사실 전 여름빼곤 다 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026223.1.11",
          "form": "그쵸ㅜㅜ 해리포터 봐야돼요",
          "original_form": "그쵸ㅜㅜ 해리포터 봐야돼요",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026223.1.12",
          "form": "에휴 저도 여름 싫어요ㅜㅜ",
          "original_form": "에휴 저도 여름 싫어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026223.1.13",
          "form": "벌레에 습도에 더위에...",
          "original_form": "벌레에 습도에 더위에...",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026223.1.14",
          "form": "진짜 여름이라서 좋은건 선풍기로 머리말릴 수 있다는거 하나뿐 입니다...",
          "original_form": "진짜 여름이라서 좋은건 선풍기로 머리말릴 수 있다는거 하나뿐 입니다...",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026223.1.15",
          "form": "진짜 모기가 너무너무너무 싫어요ㅜㅜ",
          "original_form": "진짜 모기가 너무너무너무 싫어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026223.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 모기 근데 지금 많은 철 아닌가요ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 모기 근데 지금 많은 철 아닌가요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026223.1.17",
          "form": "겁나기 시작했어요 제방 엄청 좁은데 모기 들어올까봐ㅠㅠ",
          "original_form": "겁나기 시작했어요 제방 엄청 좁은데 모기 들어올까봐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026223.1.18",
          "form": "이번 여름에 더워서 모기 없었다는데 선선해지니까 모기철이라더라구요ㅜㅜㅜ",
          "original_form": "이번 여름에 더워서 모기 없었다는데 선선해지니까 모기철이라더라구요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026223.1.19",
          "form": "저도 그래서 옷도 얇은 긴팔긴바지에 모기장 치고 자요..",
          "original_form": "저도 그래서 옷도 얇은 긴팔긴바지에 모기장 치고 자요..",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026223.1.20",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026223.1.21",
          "form": "저는 유칼립투스 오일 그냥 사정없이 뿌리려고요ㅠㅠ",
          "original_form": "저는 유칼립투스 오일 그냥 사정없이 뿌리려고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026223.1.22",
          "form": "알른 연말이 됐으면 좋겠어요!!",
          "original_form": "알른 연말이 됐으면 좋겠어요!!",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026223.1.23",
          "form": "오 그게 도움이 많이 되나봐요!! 기억해둬야겠어요",
          "original_form": "오 그게 도움이 많이 되나봐요!! 기억해둬야겠어요",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026223.1.24",
          "form": "저두요ㅜㅜ 그리고 겨울은 입을 옷이 많아서 좋아요",
          "original_form": "저두요ㅜㅜ 그리고 겨울은 입을 옷이 많아서 좋아요",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026223.1.25",
          "form": "꾸밀 수 있는게 많달까(?)",
          "original_form": "꾸밀 수 있는게 많달까(?)",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026223.1.26",
          "form": "유칼립투스 벌레쫓는 향이에요! ㅋㅋㅋㅋㅋ 키우셔도 좋을걸요??",
          "original_form": "유칼립투스 벌레쫓는 향이에요! ㅋㅋㅋㅋㅋ 키우셔도 좋을걸요??",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026223.1.27",
          "form": "맞아요 겨울은 뭔가 많이 입으니까...",
          "original_form": "맞아요 겨울은 뭔가 많이 입으니까...",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026223.1.28",
          "form": "외투도 다양하고..",
          "original_form": "외투도 다양하고..",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026223.1.29",
          "form": "역시 겨울이 좋네요!!",
          "original_form": "역시 겨울이 좋네요!!",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026223.1.30",
          "form": "유칼립투스 기억할게요!!",
          "original_form": "유칼립투스 기억할게요!!",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026223.1.31",
          "form": "맞아요 겨울이 최고!!",
          "original_form": "맞아요 겨울이 최고!!",
          "speaker_id": "1",
          "time": "20210901 23:00"
        }
      ]
    }
