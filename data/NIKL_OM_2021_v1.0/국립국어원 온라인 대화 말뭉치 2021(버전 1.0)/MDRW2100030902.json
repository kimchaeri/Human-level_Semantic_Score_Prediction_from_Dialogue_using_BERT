      "utterance": [
        {
          "id": "MDRW2100030902.1.1",
          "form": "그렇게 보면 또 싫고?",
          "original_form": "그렇게 보면 또 싫고?",
          "speaker_id": "2",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030902.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030902.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.4",
          "form": "아니 근데 그렇게 보면",
          "original_form": "아니 근데 그렇게 보면",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.5",
          "form": "또 보긴 보니까...",
          "original_form": "또 보긴 보니까...",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.6",
          "form": "아니 이렇게 보니 줏대가 없네(?)",
          "original_form": "아니 이렇게 보니 줏대가 없네(?)",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.8",
          "form": "좋죠 너무 확고하신 분 보다..",
          "original_form": "좋죠 너무 확고하신 분 보다..",
          "speaker_id": "2",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.10",
          "form": "어쩔대는 맞추고",
          "original_form": "어쩔대는 맞추고",
          "speaker_id": "2",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.11",
          "form": "이런 긍정적인..",
          "original_form": "이런 긍정적인..",
          "speaker_id": "1",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.12",
          "form": "저요?",
          "original_form": "저요?",
          "speaker_id": "2",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030902.1.13",
          "form": "긍정적이신 분이라고요?",
          "original_form": "긍정적이신 분이라고요?",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030902.1.14",
          "form": "물이 반이나 남았네식 해석이었",
          "original_form": "물이 반이나 남았네식 해석이었",
          "speaker_id": "1",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030902.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030902.1.16",
          "form": "저 원래 긍정적이죠 영화도 해피엔딩 좋아해요",
          "original_form": "저 원래 긍정적이죠 영화도 해피엔딩 좋아해요",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030902.1.17",
          "form": "새드 넘 술퍼서",
          "original_form": "새드 넘 술퍼서",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030902.1.18",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.19",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.20",
          "form": "다만",
          "original_form": "다만",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.21",
          "form": "해피엔딩은 좀 아쉬운게",
          "original_form": "해피엔딩은 좀 아쉬운게",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.22",
          "form": "아 너네들 행복하게 지내는 얘기도 보여줘...",
          "original_form": "아 너네들 행복하게 지내는 얘기도 보여줘...",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.24",
          "form": "나중에",
          "original_form": "나중에",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.25",
          "form": "이런게 좀 남아서",
          "original_form": "이런게 좀 남아서",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.26",
          "form": "잘지내는모습!!",
          "original_form": "잘지내는모습!!",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.27",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.29",
          "form": "그것도 궁금해요",
          "original_form": "그것도 궁금해요",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.30",
          "form": "센과치히로도",
          "original_form": "센과치히로도",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030902.1.31",
          "form": "결국 하쿠랑 만났을지 궁금하고",
          "original_form": "결국 하쿠랑 만났을지 궁금하고",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030902.1.32",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030902.1.33",
          "form": "센이랑 결혼했으면 ...",
          "original_form": "센이랑 결혼했으면 ...",
