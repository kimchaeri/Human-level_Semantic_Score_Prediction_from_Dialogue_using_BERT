      "utterance": [
        {
          "id": "MDRW2100013856.1.1",
          "form": "콜라보레이션 제품 구매해보신적 있나요",
          "original_form": "콜라보레이션 제품 구매해보신적 있나요",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013856.1.2",
          "form": "최근에 다양하게 나왔더라구요",
          "original_form": "최근에 다양하게 나왔더라구요",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013856.1.3",
          "form": "네 많이는아니고 곰표맥주..?",
          "original_form": "네 많이는아니고 곰표맥주..?",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013856.1.4",
          "form": "곰표팝콘 과자 이런거 먹어봤어요",
          "original_form": "곰표팝콘 과자 이런거 먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013856.1.5",
          "form": "저도 팝콘은 먹어봤어요",
          "original_form": "저도 팝콘은 먹어봤어요",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013856.1.6",
          "form": "맥주 어떻던가요?",
          "original_form": "맥주 어떻던가요?",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013856.1.7",
          "form": "구하려고 해도 쉽지가 않더라고요",
          "original_form": "구하려고 해도 쉽지가 않더라고요",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013856.1.8",
          "form": "사실 맥주 별로안좋아해서 맥주맛을모르거든여 ㅋㅋㅋ",
          "original_form": "사실 맥주 별로안좋아해서 맥주맛을모르거든여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013856.1.9",
          "form": "제입맛엔 똑같이 썼어요",
          "original_form": "제입맛엔 똑같이 썼어요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013856.1.10",
          "form": "아.. 음 다른 콜라보 제품은 써보셨나요",
          "original_form": "아.. 음 다른 콜라보 제품은 써보셨나요",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013856.1.11",
          "form": "저는 라인프렌즈 체크카드 발급받아봤어요",
          "original_form": "저는 라인프렌즈 체크카드 발급받아봤어요",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013856.1.12",
          "form": "아 캐릭터 그려져있는건가요?",
          "original_form": "아 캐릭터 그려져있는건가요?",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013856.1.13",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013856.1.14",
          "form": "비슷한 경험 있으신가요?",
          "original_form": "비슷한 경험 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013856.1.15",
          "form": "네 그 카카오프렌즈체크카드 발급받아본적있어요",
          "original_form": "네 그 카카오프렌즈체크카드 발급받아본적있어요",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013856.1.16",
          "form": "어떤캐릭터로 하섰었나요",
          "original_form": "어떤캐릭터로 하섰었나요",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013856.1.17",
          "form": "너무 귀엽다",
          "original_form": "너무 귀엽다",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
