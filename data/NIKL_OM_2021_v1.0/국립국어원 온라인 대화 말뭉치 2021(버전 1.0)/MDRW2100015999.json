      "utterance": [
        {
          "id": "MDRW2100015999.1.1",
          "form": "최근에 읽으신 책이있나요?",
          "original_form": "최근에 읽으신 책이있나요?",
          "speaker_id": "1",
          "time": "20210826 07:02"
        },
        {
          "id": "MDRW2100015999.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100015999.1.3",
          "form": "책을 정말 안읽는답니다",
          "original_form": "책을 정말 안읽는답니다",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100015999.1.4",
          "form": "저도 요즘 잘 안읽어요ㅠ",
          "original_form": "저도 요즘 잘 안읽어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100015999.1.5",
          "form": "저도요ㅠ",
          "original_form": "저도요ㅠ",
          "speaker_id": "2",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100015999.1.6",
          "form": "무슨장르좋아하세요?",
          "original_form": "무슨장르좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100015999.1.7",
          "form": "저는 에세이 좋아해요",
          "original_form": "저는 에세이 좋아해요",
          "speaker_id": "1",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100015999.1.8",
          "form": "자기계발서 같은거ㅋㅋㅋ",
          "original_form": "자기계발서 같은거ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100015999.1.9",
          "form": "추천책있나요?",
          "original_form": "추천책있나요?",
          "speaker_id": "2",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100015999.1.10",
          "form": "음 공지영의 딸을 위한 레시피인가 그거랑",
          "original_form": "음 공지영의 딸을 위한 레시피인가 그거랑",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100015999.1.11",
          "form": "여덟단어라는 책 추천해요 박웅현 작가님인가...",
          "original_form": "여덟단어라는 책 추천해요 박웅현 작가님인가...",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100015999.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100015999.1.13",
          "form": "역시 작가라그런지 제목이 감성있네요",
          "original_form": "역시 작가라그런지 제목이 감성있네요",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100015999.1.14",
          "form": "딸을위한레시피",
          "original_form": "딸을위한레시피",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100015999.1.15",
          "form": "그쵸 이거 재밌어요",
          "original_form": "그쵸 이거 재밌어요",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100015999.1.16",
          "form": "전 참",
          "original_form": "전 참",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100015999.1.17",
          "form": "책안본지오래됐네요",
          "original_form": "책안본지오래됐네요",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100015999.1.18",
          "form": "요즘은 밀리의서재로봐요",
          "original_form": "요즘은 밀리의서재로봐요",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100015999.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100015999.1.20",
          "form": "그거뭐죠 그 책읽는기계",
          "original_form": "그거뭐죠 그 책읽는기계",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100015999.1.21",
          "form": "그거도있으실거같네요",
          "original_form": "그거도있으실거같네요",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100015999.1.22",
          "form": "아 리더기는없고",
          "original_form": "아 리더기는없고",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100015999.1.23",
          "form": "폰이랑 패드로봅니다",
          "original_form": "폰이랑 패드로봅니다",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100015999.1.24",
          "form": "오 그거면되죠",
          "original_form": "오 그거면되죠",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100015999.1.25",
          "form": "스릴러는어떠세요?",
          "original_form": "스릴러는어떠세요?",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100015999.1.26",
          "form": "스릴러는 너무 무서워요",
          "original_form": "스릴러는 너무 무서워요",
          "speaker_id": "1",
          "time": "20210826 18:36"
        }
      ]
    }
