      "utterance": [
        {
          "id": "MDRW2100035449.1.1",
          "form": "피자도 좋아하세요?",
          "original_form": "피자도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035449.1.2",
          "form": "넹!!",
          "original_form": "넹!!",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100035449.1.3",
          "form": "야채빼고는 거의 다 먹어욪!",
          "original_form": "야채빼고는 거의 다 먹어욪!",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100035449.1.4",
          "form": "오오 ㅎㅎ 어디브랜드 좋아하세요?",
          "original_form": "오오 ㅎㅎ 어디브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100035449.1.5",
          "form": "피자에땅 좋아했는데 요즘엔 피자마루요!",
          "original_form": "피자에땅 좋아했는데 요즘엔 피자마루요!",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100035449.1.6",
          "form": "피자마루 맛있죠? 어떤게 최애세요 ㅎㅎ",
          "original_form": "피자마루 맛있죠? 어떤게 최애세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100035449.1.7",
          "form": "전 고구마피자 좋아해요",
          "original_form": "전 고구마피자 좋아해요",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100035449.1.8",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035449.1.9",
          "form": "꿀고구마 피자 나온후로 그것만 먹어오",
          "original_form": "꿀고구마 피자 나온후로 그것만 먹어오",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035449.1.10",
          "form": "오 꿀고구마도 나왔나요??! 요즘 안먹어봐서요 !",
          "original_form": "오 꿀고구마도 나왔나요??! 요즘 안먹어봐서요 !",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100035449.1.11",
          "form": "네! 완전 듬뿍 올라가있어 최고에요",
          "original_form": "네! 완전 듬뿍 올라가있어 최고에요",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100035449.1.12",
          "form": "헐 먹어볼께요! 피자마루는 싸서 좋아요 ㅎㅎ",
          "original_form": "헐 먹어볼께요! 피자마루는 싸서 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100035449.1.13",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100035449.1.14",
          "form": "원래 배달도 안됐는데 요즘은 하니 더 좋아요",
          "original_form": "원래 배달도 안됐는데 요즘은 하니 더 좋아요",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100035449.1.15",
          "form": "대학생때 되게 많이먹었는데 ㅎㅎ",
          "original_form": "대학생때 되게 많이먹었는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100035449.1.16",
          "form": "요즘은 배달도 하는군요???!",
          "original_form": "요즘은 배달도 하는군요???!",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100035449.1.17",
          "form": "배달비는 있죠 ??",
          "original_form": "배달비는 있죠 ??",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100035449.1.18",
          "form": "없어요!!!!",
          "original_form": "없어요!!!!",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035449.1.19",
          "form": "여긴 안받고 픽업하면 음료수 주고요!",
          "original_form": "여긴 안받고 픽업하면 음료수 주고요!",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035449.1.20",
          "form": "헐 좋네요 !!!",
          "original_form": "헐 좋네요 !!!",
          "speaker_id": "2",
          "time": "20210829 18:08"
        }
      ]
