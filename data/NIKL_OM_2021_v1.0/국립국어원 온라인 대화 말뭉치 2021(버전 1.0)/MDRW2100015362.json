      "utterance": [
        {
          "id": "MDRW2100015362.1.1",
          "form": "쇼핑 좋아하세요?ㅋㅋ",
          "original_form": "쇼핑 좋아하세요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015362.1.2",
          "form": "최근에 방 인테리어한다고",
          "original_form": "최근에 방 인테리어한다고",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015362.1.3",
          "form": "이불과 조명 서랍장을 샀어요",
          "original_form": "이불과 조명 서랍장을 샀어요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015362.1.4",
          "form": "우와 방 분위기가 완전 바뀌엇겟네요",
          "original_form": "우와 방 분위기가 완전 바뀌엇겟네요",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015362.1.5",
          "form": "맞아요 이불만 매트베개 통일 시켜도",
          "original_form": "맞아요 이불만 매트베개 통일 시켜도",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015362.1.6",
          "form": "인테리어가되더라구요",
          "original_form": "인테리어가되더라구요",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015362.1.7",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015362.1.8",
          "form": "맞아요 깔맞춤 중요하죠",
          "original_form": "맞아요 깔맞춤 중요하죠",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015362.1.9",
          "form": "수납장은 철제인데 아빠랑 동생이랑 같이 만들었어요",
          "original_form": "수납장은 철제인데 아빠랑 동생이랑 같이 만들었어요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015362.1.10",
          "form": "혼자는 노노",
          "original_form": "혼자는 노노",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015362.1.11",
          "form": "우와 철제수납장을 만들어요??",
          "original_form": "우와 철제수납장을 만들어요??",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015362.1.12",
          "form": "대단하시다",
          "original_form": "대단하시다",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015362.1.13",
          "form": "서랍장 수납칸이 보니까 철판을 접어서 만드는 거였어요",
          "original_form": "서랍장 수납칸이 보니까 철판을 접어서 만드는 거였어요",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015362.1.14",
          "form": "와ㄷㄷ철판을 접어요?",
          "original_form": "와ㄷㄷ철판을 접어요?",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015362.1.15",
          "form": "혼자는 엄두도 못내겟네요",
          "original_form": "혼자는 엄두도 못내겟네요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015362.1.16",
          "form": "그쵸 혼자 개봉했음 당근마켓행이었어요",
          "original_form": "그쵸 혼자 개봉했음 당근마켓행이었어요",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015362.1.17",
          "form": "사진도 보낼수있음 좋겠네요 ㅋㅋ",
          "original_form": "사진도 보낼수있음 좋겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015362.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러게요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015362.1.19",
          "form": "이불은 무슨색 사셧어요?",
          "original_form": "이불은 무슨색 사셧어요?",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015362.1.20",
          "form": "베이지색으로 체크무늬입니다",
          "original_form": "베이지색으로 체크무늬입니다",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015362.1.21",
          "form": "조명이 한몫하네요",
          "original_form": "조명이 한몫하네요",
          "speaker_id": "1",
          "time": "20210825 23:03"
