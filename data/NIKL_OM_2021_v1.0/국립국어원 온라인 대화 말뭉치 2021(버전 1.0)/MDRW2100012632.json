      "utterance": [
        {
          "id": "MDRW2100012632.1.1",
          "form": "가을 좋아하시나요?",
          "original_form": "가을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012632.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100012632.1.3",
          "form": "겨울은요? ㅋㅋㅋ",
          "original_form": "겨울은요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012632.1.4",
          "form": "겨울은 춥고 어두워서 별로 안좋아해요",
          "original_form": "겨울은 춥고 어두워서 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012632.1.5",
          "form": "어떤계절 젤 좋아하세요?",
          "original_form": "어떤계절 젤 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012632.1.6",
          "form": "저도 가을만 좋아요..",
          "original_form": "저도 가을만 좋아요..",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100012632.1.7",
          "form": "여름엔 땀이 싫고 겨울은 추워서 싫고",
          "original_form": "여름엔 땀이 싫고 겨울은 추워서 싫고",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100012632.1.8",
          "form": "봄은요?ㅋㅋ",
          "original_form": "봄은요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100012632.1.9",
          "form": "봄도 좋았는데 미세먼지 신경쓰여서.. ㅎㅎㅎ \n봄은 어떠세요?",
          "original_form": "봄도 좋았는데 미세먼지 신경쓰여서.. ㅎㅎㅎ \n봄은 어떠세요?",
          "speaker_id": "1",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100012632.1.10",
          "form": "봄도 여름겨울보단 좋죠! 근데 그래도 가을이 젤 좋은거 같아요",
          "original_form": "봄도 여름겨울보단 좋죠! 근데 그래도 가을이 젤 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100012632.1.11",
          "form": "뭔가 쌀쌀한  기분이 상쾌해요",
          "original_form": "뭔가 쌀쌀한  기분이 상쾌해요",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012632.1.12",
          "form": "봄이 좋은건 유일하게 세상에 핑크?가 많아지는거.. ㅋㅋㅋ",
          "original_form": "봄이 좋은건 유일하게 세상에 핑크?가 많아지는거.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012632.1.13",
          "form": "저도 쌀쌀하면서 따뜻해서 가을이 좋아유",
          "original_form": "저도 쌀쌀하면서 따뜻해서 가을이 좋아유",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012632.1.14",
          "form": "특히 옷입기가 젤 좋죠.. 머플러하기도좋고",
          "original_form": "특히 옷입기가 젤 좋죠.. 머플러하기도좋고",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012632.1.15",
          "form": "맞아요ㅋㅋ 일년중에 젤 멋부리기 좋은 계절이요",
          "original_form": "맞아요ㅋㅋ 일년중에 젤 멋부리기 좋은 계절이요",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012632.1.16",
          "form": "내일도 비가올까요?",
          "original_form": "내일도 비가올까요?",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100012632.1.17",
          "form": "오죠.. 계속 찔끔비..",
          "original_form": "오죠.. 계속 찔끔비..",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012632.1.18",
          "form": "너무 축축해서 싫어요",
          "original_form": "너무 축축해서 싫어요",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012632.1.19",
          "form": "저도요ㅜ 이런날씨에 버스타고 출근하면 진짜 힘들어요",
          "original_form": "저도요ㅜ 이런날씨에 버스타고 출근하면 진짜 힘들어요",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012632.1.20",
          "form": "아.. 비올때 버스는 진짜 넘 싫됴..",
          "original_form": "아.. 비올때 버스는 진짜 넘 싫됴..",
          "speaker_id": "1",
          "time": "20210824 23:40"
        }
      ]
