      "utterance": [
        {
          "id": "MDRW2100019134.1.1",
          "form": "지금 연애중이세요?",
          "original_form": "지금 연애중이세요?",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019134.1.2",
          "form": "전 솔로요 ㅜㅜ",
          "original_form": "전 솔로요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019134.1.3",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019134.1.4",
          "form": "저도 솔로에요 ㅋㅋㅋ",
          "original_form": "저도 솔로에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019134.1.5",
          "form": "같은 처지네요 ㅋㅋㅋㅋ",
          "original_form": "같은 처지네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019134.1.6",
          "form": "이상형 있으세요?",
          "original_form": "이상형 있으세요?",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019134.1.7",
          "form": "전 저랑 말이 잘통하고 다정한 사람을 좋아해요 ㅋㅋ",
          "original_form": "전 저랑 말이 잘통하고 다정한 사람을 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019134.1.8",
          "form": "이상형 있으신가요?",
          "original_form": "이상형 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019134.1.9",
          "form": "말 잘통하는거 저도 그래요 ㅋㅋㅋ",
          "original_form": "말 잘통하는거 저도 그래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019134.1.10",
          "form": "그게 최고인거같아요",
          "original_form": "그게 최고인거같아요",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019134.1.11",
          "form": "진짜 말 안통하면 하",
          "original_form": "진짜 말 안통하면 하",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019134.1.12",
          "form": "백퍼 싸우게 되는 ㅋㅋㅋ",
          "original_form": "백퍼 싸우게 되는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019134.1.13",
          "form": "너무 갑갑해여 ㅋㅋㅋ",
          "original_form": "너무 갑갑해여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019134.1.14",
          "form": "외모적인 이상형은 없으세요??",
          "original_form": "외모적인 이상형은 없으세요??",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019134.1.15",
          "form": "저는 강아지상 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 강아지상 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019134.1.16",
          "form": "오저랑 반대네요",
          "original_form": "오저랑 반대네요",
          "speaker_id": "1",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019134.1.17",
          "form": "저는 고양이상이요 ㅋㅋㅋ",
          "original_form": "저는 고양이상이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019134.1.18",
          "form": "오 고양이상 !",
          "original_form": "오 고양이상 !",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019134.1.19",
          "form": "넵 여자든 남자든 냥상이 이쁘더라고요 ㅋㅋㅋ",
          "original_form": "넵 여자든 남자든 냥상이 이쁘더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019134.1.20",
          "form": "약간 안재현같은 느낌일까요 ㅋㅋ",
          "original_form": "약간 안재현같은 느낌일까요 ㅋㅋ",
