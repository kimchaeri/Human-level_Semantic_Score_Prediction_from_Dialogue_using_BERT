      "utterance": [
        {
          "id": "MDRW2100040175.1.1",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100040175.1.2",
          "form": "아침에 수박쥬스 먹엇슴다",
          "original_form": "아침에 수박쥬스 먹엇슴다",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100040175.1.3",
          "form": "일욜인데 맛난거드시나용",
          "original_form": "일욜인데 맛난거드시나용",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100040175.1.4",
          "form": "그게 끝인가요....",
          "original_form": "그게 끝인가요....",
          "speaker_id": "2",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040175.1.5",
          "form": "전 떠먹는피자 만들어서 먹었습니다 복숭아랑",
          "original_form": "전 떠먹는피자 만들어서 먹었습니다 복숭아랑",
          "speaker_id": "2",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040175.1.6",
          "form": "지금 맛난거먹을려고 머리굴리고잇어요",
          "original_form": "지금 맛난거먹을려고 머리굴리고잇어요",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100040175.1.7",
          "form": "부지런하심다ㅜㅜㅜㅜ",
          "original_form": "부지런하심다ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100040175.1.8",
          "form": "일단 후식은 아이스크림대기중이에여ㅋㅋ",
          "original_form": "일단 후식은 아이스크림대기중이에여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100040175.1.9",
          "form": "ㅋㅋ 아침부터ㅋㅋ 과한 요리ㅋㅋ",
          "original_form": "ㅋㅋ 아침부터ㅋㅋ 과한 요리ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040175.1.10",
          "form": "완전 부지런!!ㅎㅎ",
          "original_form": "완전 부지런!!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040175.1.11",
          "form": "저도 아이스크림땡기네요",
          "original_form": "저도 아이스크림땡기네요",
          "speaker_id": "2",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040175.1.12",
          "form": "저는 게을러서 절대못해유",
          "original_form": "저는 게을러서 절대못해유",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040175.1.13",
          "form": "ㅈㅓ녁엔 치킨ㅋㅋ",
          "original_form": "ㅈㅓ녁엔 치킨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100040175.1.14",
          "form": "ㅋㅋㅋ굿굿굿",
          "original_form": "ㅋㅋㅋ굿굿굿",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100040175.1.15",
          "form": "치킨은",
          "original_form": "치킨은",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100040175.1.16",
          "form": "맛난거 결정하심 알려주세요ㅋㅋ 참고할게요ㅋㅋ",
          "original_form": "맛난거 결정하심 알려주세요ㅋㅋ 참고할게요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100040175.1.17",
          "form": "아침드시고 출근하셧나용",
          "original_form": "아침드시고 출근하셧나용",
          "speaker_id": "1",
          "time": "20210906 10:47"
        },
        {
          "id": "MDRW2100040175.1.18",
          "form": "네ㅋㅋ 그냥 평범하게 한식이요!",
          "original_form": "네ㅋㅋ 그냥 평범하게 한식이요!",
