      "utterance": [
        {
          "id": "MDRW2100042903.1.1",
          "form": "몰라 그냥 꼬투리 잡아서 그건 왜 그렇게 생각하냐",
          "original_form": "몰라 그냥 꼬투리 잡아서 그건 왜 그렇게 생각하냐",
          "speaker_id": "2",
          "time": "20210907 11:07"
        },
        {
          "id": "MDRW2100042903.1.2",
          "form": "이러고 있음",
          "original_form": "이러고 있음",
          "speaker_id": "2",
          "time": "20210907 11:08"
        },
        {
          "id": "MDRW2100042903.1.3",
          "form": "너네는 안해?",
          "original_form": "너네는 안해?",
          "speaker_id": "2",
          "time": "20210907 11:08"
        },
        {
          "id": "MDRW2100042903.1.4",
          "form": "자기소개한과목하는데",
          "original_form": "자기소개한과목하는데",
          "speaker_id": "1",
          "time": "20210907 11:08"
        },
        {
          "id": "MDRW2100042903.1.5",
          "form": "질문은안함",
          "original_form": "질문은안함",
          "speaker_id": "1",
          "time": "20210907 11:08"
        },
        {
          "id": "MDRW2100042903.1.6",
          "form": "실시간발표도아니고",
          "original_form": "실시간발표도아니고",
          "speaker_id": "1",
          "time": "20210907 11:08"
        },
        {
          "id": "MDRW2100042903.1.7",
          "form": "1학기때는아예안했음",
          "original_form": "1학기때는아예안했음",
          "speaker_id": "1",
          "time": "20210907 11:08"
        },
        {
          "id": "MDRW2100042903.1.8",
          "form": "오ㅓ",
          "original_form": "오ㅓ",
          "speaker_id": "2",
          "time": "20210907 11:08"
        },
        {
          "id": "MDRW2100042903.1.9",
          "form": "하는게신기함",
          "original_form": "하는게신기함",
          "speaker_id": "1",
          "time": "20210907 11:08"
        },
        {
          "id": "MDRW2100042903.1.10",
          "form": "와 대박 실시간 발표가 아님???",
          "original_form": "와 대박 실시간 발표가 아님???",
          "speaker_id": "2",
          "time": "20210907 11:08"
        },
        {
          "id": "MDRW2100042903.1.11",
          "form": "이야 대박",
          "original_form": "이야 대박",
          "speaker_id": "2",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100042903.1.12",
          "form": "그냥발표영상찍으면됨",
          "original_form": "그냥발표영상찍으면됨",
          "speaker_id": "1",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100042903.1.13",
          "form": "얼굴안나와도됨",
          "original_form": "얼굴안나와도됨",
          "speaker_id": "1",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100042903.1.14",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100042903.1.15",
          "form": "짱이다 진짜",
          "original_form": "짱이다 진짜",
          "speaker_id": "2",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100042903.1.16",
          "form": "자기소개에질문을한다는게",
          "original_form": "자기소개에질문을한다는게",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042903.1.17",
          "form": "충격임",
          "original_form": "충격임",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042903.1.18",
          "form": "나도 충격적임",
          "original_form": "나도 충격적임",
          "speaker_id": "2",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042903.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042903.1.20",
          "form": "너는",
          "original_form": "너는",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042903.1.21",
          "form": "1학기에는 겪지 못한 이 사건....",
          "original_form": "1학기에는 겪지 못한 이 사건....",
          "speaker_id": "2",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042903.1.22",
          "form": "언제함?",
          "original_form": "언제함?",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042903.1.23",
          "form": "발표했음?",
          "original_form": "발표했음?",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
