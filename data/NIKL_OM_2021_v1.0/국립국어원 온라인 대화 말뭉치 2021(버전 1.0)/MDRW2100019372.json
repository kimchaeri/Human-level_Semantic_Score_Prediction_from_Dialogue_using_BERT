      "utterance": [
        {
          "id": "MDRW2100019372.1.1",
          "form": "학원비도 겨우 내겠구만",
          "original_form": "학원비도 겨우 내겠구만",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019372.1.2",
          "form": "긍가",
          "original_form": "긍가",
          "speaker_id": "1",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019372.1.3",
          "form": "요즘 학원 안다니고 학교만 다닐 수 있는것도 아니고",
          "original_form": "요즘 학원 안다니고 학교만 다닐 수 있는것도 아니고",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019372.1.4",
          "form": "거기다 폰 비용이랑 용돈 이것저것 하면 진짜 100은 금방 넘길듯",
          "original_form": "거기다 폰 비용이랑 용돈 이것저것 하면 진짜 100은 금방 넘길듯",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019372.1.5",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019372.1.6",
          "form": "여자들 비혼 많이많이했으면 ㅎㅎ",
          "original_form": "여자들 비혼 많이많이했으면 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019372.1.7",
          "form": "솔직히ㅜ어릴때나 100만원 하면 커보였지",
          "original_form": "솔직히ㅜ어릴때나 100만원 하면 커보였지",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019372.1.8",
          "form": "어우 지금은ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어우 지금은ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019372.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019372.1.10",
          "form": "비혼 진짜 제발",
          "original_form": "비혼 진짜 제발",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019372.1.11",
          "form": "결혼해서 골치 썩히지 말고 비혼하세요 여러분",
          "original_form": "결혼해서 골치 썩히지 말고 비혼하세요 여러분",
          "speaker_id": "2",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019372.1.12",
          "form": "작년들어서 처음으로",
          "original_form": "작년들어서 처음으로",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019372.1.13",
          "form": "제발",
          "original_form": "제발",
          "speaker_id": "2",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019372.1.14",
          "form": "신생아수 20만명대됏다며 ㅎㅎ",
          "original_form": "신생아수 20만명대됏다며 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019372.1.15",
