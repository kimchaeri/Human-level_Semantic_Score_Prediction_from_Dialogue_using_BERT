      "utterance": [
        {
          "id": "MDRW2100007698.1.1",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007698.1.2",
          "form": "빵 좋아해요",
          "original_form": "빵 좋아해요",
          "speaker_id": "2",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007698.1.3",
          "form": "어떤 빵 좋아하세요?",
          "original_form": "어떤 빵 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007698.1.4",
          "form": "저는 밥보다 빵순이라서",
          "original_form": "저는 밥보다 빵순이라서",
          "speaker_id": "1",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007698.1.5",
          "form": "다좋아해요",
          "original_form": "다좋아해요",
          "speaker_id": "1",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007698.1.6",
          "form": "음 하나만 고르기어렵네요",
          "original_form": "음 하나만 고르기어렵네요",
          "speaker_id": "1",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007698.1.7",
          "form": "저도 하나만 고르기 어려워서 잔뜩 사요",
          "original_form": "저도 하나만 고르기 어려워서 잔뜩 사요",
          "speaker_id": "2",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007698.1.8",
          "form": "바게트좋아하세요?",
          "original_form": "바게트좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007698.1.9",
          "form": "뚜레쥬르 호두연유바게트 맛있어요",
          "original_form": "뚜레쥬르 호두연유바게트 맛있어요",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007698.1.10",
          "form": "저는 초코 치즈 들어간거 많이먹는것같아요",
          "original_form": "저는 초코 치즈 들어간거 많이먹는것같아요",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007698.1.11",
          "form": "저는 파리바게트가면 초코소라롤 꼭사요",
          "original_form": "저는 파리바게트가면 초코소라롤 꼭사요",
          "speaker_id": "1",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007698.1.12",
          "form": "연유빵이랑요 ㅋㅋ",
          "original_form": "연유빵이랑요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007698.1.13",
          "form": "초코소라빵맛있죠",
          "original_form": "초코소라빵맛있죠",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007698.1.14",
          "form": "달달한거좋아하시네요",
          "original_form": "달달한거좋아하시네요",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007698.1.15",
          "form": "그러고보니 그렇네요 ㅋㅋ",
          "original_form": "그러고보니 그렇네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007698.1.16",
          "form": "저는 단짠 번갈아서 먹어요ㅋㅋ",
          "original_form": "저는 단짠 번갈아서 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007698.1.17",
          "form": "여유만된다면 빵 직접 만들어보고싶어요 ㅋㅋ",
          "original_form": "여유만된다면 빵 직접 만들어보고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007698.1.18",
          "form": "요즘엔 밀키트가 많이 나와서 빵도 간편하게 만들수있을거같아요",
          "original_form": "요즘엔 밀키트가 많이 나와서 빵도 간편하게 만들수있을거같아요",
          "speaker_id": "2",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007698.1.19",
          "form": "에어 프라이어 있으면 가능할것같기도하고요",
          "original_form": "에어 프라이어 있으면 가능할것같기도하고요",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007698.1.20",
          "form": "근데 사실 밀가루줄여야하는데ㅜ",
          "original_form": "근데 사실 밀가루줄여야하는데ㅜ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007698.1.21",
          "form": "건강한 메뉴로 먹어야하는데말이죠",
          "original_form": "건강한 메뉴로 먹어야하는데말이죠",
          "speaker_id": "1",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007698.1.22",
          "form": "적당히 먹으면 괜찮지않을까요",
          "original_form": "적당히 먹으면 괜찮지않을까요",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007698.1.23",
          "form": "밀가루 끊으면 성격이 나빠져서ㅎㅎ",
          "original_form": "밀가루 끊으면 성격이 나빠져서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007698.1.24",
          "form": "와 진짜공감",
          "original_form": "와 진짜공감",
          "speaker_id": "1",
          "time": "20210822 10:08"
        }
      ]
