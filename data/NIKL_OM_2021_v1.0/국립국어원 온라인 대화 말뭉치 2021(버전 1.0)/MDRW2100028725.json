      "utterance": [
        {
          "id": "MDRW2100028725.1.1",
          "form": "저한테 전화왔을때가",
          "original_form": "저한테 전화왔을때가",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028725.1.2",
          "form": "친구 할머니가 돌아가셨을땐데",
          "original_form": "친구 할머니가 돌아가셨을땐데",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028725.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028725.1.4",
          "form": "친구 와이프가",
          "original_form": "친구 와이프가",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028725.1.5",
          "form": "자긴 안가겠다고 했대요",
          "original_form": "자긴 안가겠다고 했대요",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028725.1.6",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028725.1.7",
          "form": "친구가 이게 말이되냐고",
          "original_form": "친구가 이게 말이되냐고",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028725.1.8",
          "form": "근데 저 전남친네 형수도 시부모랑 연끊고 생일에도 안찾아가고 그러길래",
          "original_form": "근데 저 전남친네 형수도 시부모랑 연끊고 생일에도 안찾아가고 그러길래",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028725.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028725.1.10",
          "form": "형수도 형수겟지만 시부모 보통 아니겠다는",
          "original_form": "형수도 형수겟지만 시부모 보통 아니겠다는",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028725.1.11",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028725.1.12",
          "form": "그런 생각이 들었거든요...",
          "original_form": "그런 생각이 들었거든요...",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028725.1.13",
          "form": "하나를 보면 열을 안다고",
          "original_form": "하나를 보면 열을 안다고",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028725.1.14",
          "form": "그래서 더 결혼생각 안한것도잇어요",
          "original_form": "그래서 더 결혼생각 안한것도잇어요",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028725.1.15",
          "form": "ㅎㅎ..",
          "original_form": "ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028725.1.16",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028725.1.17",
          "form": "데이트중에 영통 거셔가지고",
          "original_form": "데이트중에 영통 거셔가지고",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028725.1.18",
          "form": "헐?",
          "original_form": "헐?",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028725.1.19",
          "form": "남친이 끊고 그랬는데",
          "original_form": "남친이 끊고 그랬는데",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028725.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028725.1.21",
          "form": "전 한번도 뵌적없어요",
          "original_form": "전 한번도 뵌적없어요",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028725.1.22",
          "form": "저 예전 소개팅녀가",
          "original_form": "저 예전 소개팅녀가",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028725.1.23",
          "form": "그렇게 만날때마다 엄마가 전화해서",
          "original_form": "그렇게 만날때마다 엄마가 전화해서",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028725.1.24",
          "form": "아이고..",
          "original_form": "아이고..",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028725.1.25",
          "form": "제가 도망쳤자나요",
          "original_form": "제가 도망쳤자나요",
          "speaker_id": "2",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028725.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028725.1.27",
          "form": "도망칠 신호를 미리 주면",
          "original_form": "도망칠 신호를 미리 주면",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028725.1.28",
          "form": "감사해야돼요",
          "original_form": "감사해야돼요",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028725.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028725.1.30",
          "form": "조상님께 감사",
          "original_form": "조상님께 감사",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028725.1.31",
          "form": "근데 저 아까 님이 여친이랑 싸운 얘기",
          "original_form": "근데 저 아까 님이 여친이랑 싸운 얘기",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028725.1.32",
          "form": "뭔가 알거같기도 해요",
          "original_form": "뭔가 알거같기도 해요",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028725.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028725.1.34",
          "form": "제 지금 남친 왜케 맨날",
          "original_form": "제 지금 남친 왜케 맨날",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028725.1.35",
          "form": "친구랑 밤마다 드라이브를 그렇게 가요",
          "original_form": "친구랑 밤마다 드라이브를 그렇게 가요",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028725.1.36",
          "form": "ㅋㅋㅋㅋㅋ 느낌 알겠죠",
          "original_form": "ㅋㅋㅋㅋㅋ 느낌 알겠죠",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028725.1.37",
          "form": "그느낌",
          "original_form": "그느낌",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028725.1.38",
          "form": "남자 둘이서 대체 왜 밤에 해수욕장 드라이브를 가서",
          "original_form": "남자 둘이서 대체 왜 밤에 해수욕장 드라이브를 가서",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028725.1.39",
          "form": "토스트를 먹고오는거지",
          "original_form": "토스트를 먹고오는거지",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028725.1.40",
          "form": "잉?",
          "original_form": "잉?",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028725.1.41",
          "form": "지금 해수욕장 사진 보내서 자랑해요",
          "original_form": "지금 해수욕장 사진 보내서 자랑해요",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028725.1.42",
          "form": "기분 묘하게 이상하죠?",
          "original_form": "기분 묘하게 이상하죠?",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028725.1.43",
          "form": "넼ㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028725.1.44",
          "form": "이게 쌓이면 1년뒤엔 싸울수도 있겠구나 싶네요",
          "original_form": "이게 쌓이면 1년뒤엔 싸울수도 있겠구나 싶네요",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028725.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028725.1.46",
          "form": "싸우지 마시구요",
          "original_form": "싸우지 마시구요",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028725.1.47",
          "form": "밤마다 맨날 동네친구 만나서 드라이브를",
          "original_form": "밤마다 맨날 동네친구 만나서 드라이브를",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028725.1.48",
          "form": "어느정도 봐주시다가",
          "original_form": "어느정도 봐주시다가",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028725.1.49",
          "form": "왜하는거야",
          "original_form": "왜하는거야",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028725.1.50",
          "form": "ㅋㅋㅋㅋㅋ넼ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ넼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028725.1.51",
          "form": "진짜 서운하면",
          "original_form": "진짜 서운하면",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028725.1.52",
          "form": "남친 기분 안상하게",
          "original_form": "남친 기분 안상하게",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028725.1.53",
          "form": "잘 말해봐요",
          "original_form": "잘 말해봐요",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028725.1.54",
          "form": "넼ㅋㅋㅋ 아잉 써먹었더니",
          "original_form": "넼ㅋㅋㅋ 아잉 써먹었더니",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028725.1.55",
          "form": "좋아하네요 뭔가 고전인데",
          "original_form": "좋아하네요 뭔가 고전인데",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028725.1.56",
          "form": "이게 먹히다니",
          "original_form": "이게 먹히다니",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028725.1.57",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028725.1.58",
          "form": "좋아해요?",
          "original_form": "좋아해요?",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028725.1.59",
          "form": "네 좋아하네요 와아아",
          "original_form": "네 좋아하네요 와아아",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028725.1.60",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028725.1.61",
          "form": "제가 남자니까",
          "original_form": "제가 남자니까",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028725.1.62",
          "form": "제가 좋아하는건",
          "original_form": "제가 좋아하는건",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
