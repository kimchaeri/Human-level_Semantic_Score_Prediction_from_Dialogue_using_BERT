      "utterance": [
        {
          "id": "MDRW2100029214.1.1",
          "form": "찍었는데 진짜 멋졌음",
          "original_form": "찍었는데 진짜 멋졌음",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.2",
          "form": "비 올 때 내원 정사 멋있죠",
          "original_form": "비 올 때 내원 정사 멋있죠",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.3",
          "form": "내친구가",
          "original_form": "내친구가",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.4",
          "form": "사이비",
          "original_form": "사이비",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.5",
          "form": "친구가",
          "original_form": "친구가",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.6",
          "form": "그 이만희 있는좋교뭐지",
          "original_form": "그 이만희 있는좋교뭐지",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.7",
          "form": "사이비 종교 믿었어요",
          "original_form": "사이비 종교 믿었어요",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.8",
          "form": "지금은 믿는지 몰겠는데",
          "original_form": "지금은 믿는지 몰겠는데",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.9",
          "form": "신천지",
          "original_form": "신천지",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.10",
          "form": "몇년동안 믿ㅇㅇㅁ",
          "original_form": "몇년동안 믿ㅇㅇㅁ",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.11",
          "form": "어 신천지",
          "original_form": "어 신천지",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.12",
          "form": "헐 인형 끊으세요",
          "original_form": "헐 인형 끊으세요",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.13",
          "form": "나랑 걔랑 다른애랑",
          "original_form": "나랑 걔랑 다른애랑",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029214.1.14",
          "form": "아직 믿고 있다면 연근 하세요",
          "original_form": "아직 믿고 있다면 연근 하세요",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.15",
          "form": "셋이 친한데",
          "original_form": "셋이 친한데",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.16",
          "form": "은근하세요 ㅋㅋㅋ",
          "original_form": "은근하세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.17",
          "form": "나는 안믿을것같아서",
          "original_form": "나는 안믿을것같아서",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.18",
          "form": "아예 시도안한거같고",
          "original_form": "아예 시도안한거같고",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.19",
          "form": "다른내친구에게",
          "original_form": "다른내친구에게",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.20",
          "form": "아예 안 믿을 거 같대",
          "original_form": "아예 안 믿을 거 같대",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.21",
          "form": "전도하려고",
          "original_form": "전도하려고",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.22",
          "form": "작업을 짰었음",
          "original_form": "작업을 짰었음",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.23",
          "form": "내친구는 멋도 모르고",
          "original_form": "내친구는 멋도 모르고",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.24",
          "form": "완전 세계 전 세계적이던데요",
          "original_form": "완전 세계 전 세계적이던데요",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029214.1.25",
          "form": "걸리뻔했다가",
          "original_form": "걸리뻔했다가",
          "speaker_id": "2",
          "time": "20210903 00:22"
        }
      ]
