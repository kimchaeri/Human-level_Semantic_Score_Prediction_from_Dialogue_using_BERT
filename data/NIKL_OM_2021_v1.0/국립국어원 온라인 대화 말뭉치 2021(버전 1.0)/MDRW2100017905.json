      "utterance": [
        {
          "id": "MDRW2100017905.1.1",
          "form": "절에서 공부잘되셨어요?",
          "original_form": "절에서 공부잘되셨어요?",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017905.1.2",
          "form": "네 그냥",
          "original_form": "네 그냥",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017905.1.3",
          "form": "공부는되는데 성적응",
          "original_form": "공부는되는데 성적응",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017905.1.4",
          "form": "안오름",
          "original_form": "안오름",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017905.1.5",
          "form": "그래도 멋지네요",
          "original_form": "그래도 멋지네요",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017905.1.6",
          "form": "잠시 야식먹고오께요~",
          "original_form": "잠시 야식먹고오께요~",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017905.1.7",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017905.1.8",
          "form": "기독교는 헌금때문에 부담되요",
          "original_form": "기독교는 헌금때문에 부담되요",
          "speaker_id": "2",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100017905.1.9",
          "form": "맞아요 십일조에 철철이 감사 헌금에",
          "original_form": "맞아요 십일조에 철철이 감사 헌금에",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100017905.1.10",
          "form": "부담될거같아요",
          "original_form": "부담될거같아요",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100017905.1.11",
          "form": "교회보면ㅇ진짜 땅값비싼곳에 건물이자나요",
          "original_form": "교회보면ㅇ진짜 땅값비싼곳에 건물이자나요",
          "speaker_id": "2",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100017905.1.12",
          "form": "그쵸 완전 노른자땅에 드높이!! 휘황찬란하게",
          "original_form": "그쵸 완전 노른자땅에 드높이!! 휘황찬란하게",
          "speaker_id": "1",
          "time": "20210830 10:38"
        },
        {
          "id": "MDRW2100017905.1.13",
          "form": "예수님이 보면 뭐라 할거에요 분명",
          "original_form": "예수님이 보면 뭐라 할거에요 분명",
          "speaker_id": "1",
          "time": "20210830 10:38"
        },
        {
          "id": "MDRW2100017905.1.14",
          "form": "저도 그부분이 이해가 안가요",
          "original_form": "저도 그부분이 이해가 안가요",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100017905.1.15",
          "form": "종교의 진정한 의미를 퇴색시키는 사람들이 참 많아요",
          "original_form": "종교의 진정한 의미를 퇴색시키는 사람들이 참 많아요",
          "speaker_id": "1",
          "time": "20210831 06:19"
        },
        {
          "id": "MDRW2100017905.1.16",
          "form": "아쉬운좀이죠 종교의",
          "original_form": "아쉬운좀이죠 종교의",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100017905.1.17",
          "form": "종교인들이 말로만 그치지말고 자신들의 신이 가르친대로 행동하면 좋겠어요 부다",
          "original_form": "종교인들이 말로만 그치지말고 자신들의 신이 가르친대로 행동하면 좋겠어요 부다",
          "speaker_id": "1",
          "time": "20210901 14:22"
        },
        {
          "id": "MDRW2100017905.1.18",
          "form": "네 믿음이란 마음의 의지 니깐요",
          "original_form": "네 믿음이란 마음의 의지 니깐요",
