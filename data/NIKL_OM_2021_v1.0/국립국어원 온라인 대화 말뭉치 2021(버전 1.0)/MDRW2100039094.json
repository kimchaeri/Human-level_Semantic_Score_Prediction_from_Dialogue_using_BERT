      "utterance": [
        {
          "id": "MDRW2100039094.1.1",
          "form": "맞아요 언니~",
          "original_form": "맞아요 언니~",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039094.1.2",
          "form": "나도 빈속에 커피 먹으면 속 쓰리던데예",
          "original_form": "나도 빈속에 커피 먹으면 속 쓰리던데예",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039094.1.3",
          "form": "엉 난 속쓰림은 없는데",
          "original_form": "엉 난 속쓰림은 없는데",
          "speaker_id": "1",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100039094.1.4",
          "form": "미리 관리 ㅎㅎ",
          "original_form": "미리 관리 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100039094.1.5",
          "form": "점심 먹었나?? 밥먹고나왔나??",
          "original_form": "점심 먹었나?? 밥먹고나왔나??",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100039094.1.6",
          "form": "밥 먹고 왔지요언니",
          "original_form": "밥 먹고 왔지요언니",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039094.1.7",
          "form": "배다 꺼져 배고파 죽겠네요ㅋㅋㅋ",
          "original_form": "배다 꺼져 배고파 죽겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039094.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ죽지마라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ죽지마라",
          "speaker_id": "1",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039094.1.9",
          "form": "나 제육볶음 먹었다",
          "original_form": "나 제육볶음 먹었다",
          "speaker_id": "1",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039094.1.10",
          "form": "너무 맛있군 ㅋㅋㅋㅋㄱ",
          "original_form": "너무 맛있군 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039094.1.11",
          "form": "쌈싸먹고 싶네요~",
          "original_form": "쌈싸먹고 싶네요~",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039094.1.12",
          "form": "식당에서 밥 먹어요??",
          "original_form": "식당에서 밥 먹어요??",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039094.1.13",
          "form": "엉 구내식당은 아니고",
          "original_form": "엉 구내식당은 아니고",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039094.1.14",
          "form": "여기저기 돌아다니면서 그날그날 먹고싶은걸로",
          "original_form": "여기저기 돌아다니면서 그날그날 먹고싶은걸로",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039094.1.15",
          "form": "골라 먹는 재미가 있구만요~",
          "original_form": "골라 먹는 재미가 있구만요~",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100039094.1.16",
          "form": "엉 수제비랑 제육 중에 고민하다가",
          "original_form": "엉 수제비랑 제육 중에 고민하다가",
          "speaker_id": "1",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100039094.1.17",
          "form": "최근 비와서 수제비\r\n칼국수 너무 많이 먹어서",
          "original_form": "최근 비와서 수제비\r\n칼국수 너무 많이 먹어서",
          "speaker_id": "1",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100039094.1.18",
          "form": "오늘은 제육",
          "original_form": "오늘은 제육",
          "speaker_id": "1",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100039094.1.19",
          "form": "맞아요 거의 수제비 였음ㅋㅋㅋ",
          "original_form": "맞아요 거의 수제비 였음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100039094.1.20",
          "form": "난파전 먹고 싶어요",
          "original_form": "난파전 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100039094.1.21",
          "form": "오 파전 좋다",
          "original_form": "오 파전 좋다",
          "speaker_id": "1",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039094.1.22",
          "form": "파전 해도 ㅋㅋ",
          "original_form": "파전 해도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039094.1.23",
          "form": "ㅋㅋㅋㅋㅋ사먹는게 빠를듯 하네여 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ사먹는게 빠를듯 하네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100039094.1.24",
          "form": "ㅋㅋㅋㅋㄱ 그렇제?\r\nㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ 그렇제?\r\nㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039094.1.25",
          "form": "그래 큰 기대안한다 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "그래 큰 기대안한다 ㅋㅋㅋㅋㅋㄱ",
