      "utterance": [
        {
          "id": "MDRW2100024862.1.1",
          "form": "밥 드셨나요?",
          "original_form": "밥 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:51"
        },
        {
          "id": "MDRW2100024862.1.2",
          "form": "네 저녁은 국물떡볶이해먹었어요 님은요",
          "original_form": "네 저녁은 국물떡볶이해먹었어요 님은요",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024862.1.3",
          "form": "헉.... 짱 맛있겠네여",
          "original_form": "헉.... 짱 맛있겠네여",
          "speaker_id": "1",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100024862.1.4",
          "form": "전 순대국밥을",
          "original_form": "전 순대국밥을",
          "speaker_id": "1",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100024862.1.5",
          "form": "먹었어요 ㅎ",
          "original_form": "먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100024862.1.6",
          "form": "오 잘하셨네요 ㅋㅋ 전 오늘도 국물떡볶이 해먹으려고요",
          "original_form": "오 잘하셨네요 ㅋㅋ 전 오늘도 국물떡볶이 해먹으려고요",
          "speaker_id": "2",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100024862.1.7",
          "form": "국물떡볶이..",
          "original_form": "국물떡볶이..",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100024862.1.8",
          "form": "너무 탁월한",
          "original_form": "너무 탁월한",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100024862.1.9",
          "form": "선택이네여 ㅎ",
          "original_form": "선택이네여 ㅎ",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100024862.1.10",
          "form": "ㅋㅋㅋ 1일1떡볶이하고있네요 ㅜㅜ 저녁 뭐드실던거요",
          "original_form": "ㅋㅋㅋ 1일1떡볶이하고있네요 ㅜㅜ 저녁 뭐드실던거요",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100024862.1.11",
          "form": "드실건가요 ㅎㅎ",
          "original_form": "드실건가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100024862.1.12",
          "form": "저 아직 못 골랐어요",
          "original_form": "저 아직 못 골랐어요",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100024862.1.13",
          "form": "배고프네요.......",
          "original_form": "배고프네요.......",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100024862.1.14",
          "form": "음......... 닭갈비 어떠셍ㅅ",
          "original_form": "음......... 닭갈비 어떠셍ㅅ",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100024862.1.15",
          "form": "어떠세요 ㅎㅎ",
          "original_form": "어떠세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100024862.1.16",
          "form": "닭갈비도 좋죠 ~~~~~~",
          "original_form": "닭갈비도 좋죠 ~~~~~~",
          "speaker_id": "1",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100024862.1.17",
          "form": "ㅎㅎ 동네맛집 뭐가있나요",
          "original_form": "ㅎㅎ 동네맛집 뭐가있나요",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100024862.1.18",
          "form": "음.. 제가 이사온지 얼마 안되서",
          "original_form": "음.. 제가 이사온지 얼마 안되서",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100024862.1.19",
          "form": "맛집에 대해 잘 몰라여ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맛집에 대해 잘 몰라여ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100024862.1.20",
          "form": "ㅋㅋㅋㅋ아 이제 차차 도장깨기 하셔야겠군요",
          "original_form": "ㅋㅋㅋㅋ아 이제 차차 도장깨기 하셔야겠군요",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100024862.1.21",
          "form": "네 조금 설레면서",
          "original_form": "네 조금 설레면서",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100024862.1.22",
          "form": "맛집이 그저그런 겨 같아",
          "original_form": "맛집이 그저그런 겨 같아",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
