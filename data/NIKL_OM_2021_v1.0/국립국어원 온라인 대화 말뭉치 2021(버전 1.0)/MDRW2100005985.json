      "utterance": [
        {
          "id": "MDRW2100005985.1.1",
          "form": "저녁 뭐드시나요",
          "original_form": "저녁 뭐드시나요",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100005985.1.2",
          "form": "신라면볶음면 먹었어요!!",
          "original_form": "신라면볶음면 먹었어요!!",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100005985.1.3",
          "form": "밥이 먹기싫어서 ㅋㅋ",
          "original_form": "밥이 먹기싫어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100005985.1.4",
          "form": "오 맛있을까요",
          "original_form": "오 맛있을까요",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100005985.1.5",
          "form": "네 맛있드라구요 ㅋㅋ",
          "original_form": "네 맛있드라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005985.1.6",
          "form": "전 국물라면 별로 안좋아해서 이쪽이 더 좋아요",
          "original_form": "전 국물라면 별로 안좋아해서 이쪽이 더 좋아요",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005985.1.7",
          "form": "짜파게티랑 비교하면요?",
          "original_form": "짜파게티랑 비교하면요?",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005985.1.8",
          "form": "둘다 다른맛이라 비교가 안될거 같아요 ㅋㅋ",
          "original_form": "둘다 다른맛이라 비교가 안될거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100005985.1.9",
          "form": "신라면볶음은 라볶이같은 맛이에요 라면스프넣은 달지않은 라볶이",
          "original_form": "신라면볶음은 라볶이같은 맛이에요 라면스프넣은 달지않은 라볶이",
          "speaker_id": "1",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100005985.1.10",
          "form": "음 전 짜파게티가 좀 더 ㅎㅎ",
          "original_form": "음 전 짜파게티가 좀 더 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100005985.1.11",
          "form": "네 짜파게티도 맛있죠!",
          "original_form": "네 짜파게티도 맛있죠!",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100005985.1.12",
          "form": "라면 뭐 좋아하시나요",
          "original_form": "라면 뭐 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
