      "utterance": [
        {
          "id": "MDRW2100003572.1.1",
          "form": "최근에 영화나 뮤지컬 본 적 있어요?",
          "original_form": "최근에 영화나 뮤지컬 본 적 있어요?",
          "speaker_id": "1",
          "time": "20210821 02:19"
        },
        {
          "id": "MDRW2100003572.1.2",
          "form": "최근에 저 뮤지컬 시카고봣어요",
          "original_form": "최근에 저 뮤지컬 시카고봣어요",
          "speaker_id": "1",
          "time": "20210821 02:20"
        },
        {
          "id": "MDRW2100003572.1.3",
          "form": "우와 재밌으셨겠어요",
          "original_form": "우와 재밌으셨겠어요",
          "speaker_id": "2",
          "time": "20210821 02:20"
        },
        {
          "id": "MDRW2100003572.1.4",
          "form": "전 지방 살아서 뮤지컬 보러가기가 힘들어요",
          "original_form": "전 지방 살아서 뮤지컬 보러가기가 힘들어요",
          "speaker_id": "2",
          "time": "20210821 02:20"
        },
        {
          "id": "MDRW2100003572.1.5",
          "form": "엄청 재미있었어요",
          "original_form": "엄청 재미있었어요",
          "speaker_id": "1",
          "time": "20210821 02:21"
        },
        {
          "id": "MDRW2100003572.1.6",
          "form": "저도 지방살때는 잘 못봤어요 ㅠㅠ",
          "original_form": "저도 지방살때는 잘 못봤어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:21"
        },
        {
          "id": "MDRW2100003572.1.7",
          "form": "어떤 배우로 보셨어요?",
          "original_form": "어떤 배우로 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 02:21"
        },
        {
          "id": "MDRW2100003572.1.8",
          "form": "최재림 아이비 최정원 이렇게",
          "original_form": "최재림 아이비 최정원 이렇게",
          "speaker_id": "1",
          "time": "20210821 02:21"
        },
        {
          "id": "MDRW2100003572.1.9",
          "form": "페어로 봤어요!",
          "original_form": "페어로 봤어요!",
          "speaker_id": "1",
          "time": "20210821 02:21"
        },
        {
          "id": "MDRW2100003572.1.10",
          "form": "부러워요~ 최재림 배우 연기 영상 봤는데 잘하더라구요",
          "original_form": "부러워요~ 최재림 배우 연기 영상 봤는데 잘하더라구요",
          "speaker_id": "2",
          "time": "20210821 02:22"
        },
        {
          "id": "MDRW2100003572.1.11",
          "form": "진짜 잘하시더라구요",
          "original_form": "진짜 잘하시더라구요",
          "speaker_id": "1",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003572.1.12",
          "form": "그래서 다시 한번 더 보고싶은 마음이에요",
          "original_form": "그래서 다시 한번 더 보고싶은 마음이에요",
          "speaker_id": "1",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003572.1.13",
          "form": "name2님은 최근에 본 영화가 있나요?",
          "original_form": "&name2&님은 최근에 본 영화가 있나요?",
          "speaker_id": "1",
          "time": "20210821 02:23"
        },
        {
          "id": "MDRW2100003572.1.14",
          "form": "저는 모가디슈 극장에서 봤어요",
          "original_form": "저는 모가디슈 극장에서 봤어요",
          "speaker_id": "2",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003572.1.15",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "2",
          "time": "20210821 02:24"
        },
        {
          "id": "MDRW2100003572.1.16",
          "form": "헐 봤어요!!",
          "original_form": "헐 봤어요!!",
          "speaker_id": "1",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003572.1.17",
          "form": "엄청 재밌어서 깜짝 놀랬어요",
          "original_form": "엄청 재밌어서 깜짝 놀랬어요",
          "speaker_id": "1",
          "time": "20210821 02:25"
        },
        {
          "id": "MDRW2100003572.1.18",
          "form": "그쵸 실화라고 해서 더 재밌었어요",
          "original_form": "그쵸 실화라고 해서 더 재밌었어요",
          "speaker_id": "2",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003572.1.19",
          "form": "오랜만에 극장 가니까 좋더라구요",
          "original_form": "오랜만에 극장 가니까 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003572.1.20",
          "form": "중간에 저도모르게",
          "original_form": "중간에 저도모르게",
          "speaker_id": "1",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003572.1.21",
          "form": "양손을 꼭 잡고 봤어요 ㅋㅋ심장 떨려서",
          "original_form": "양손을 꼭 잡고 봤어요 ㅋㅋ심장 떨려서",
          "speaker_id": "1",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003572.1.22",
          "form": "기대안했는데 진짜 재미있었어요",
          "original_form": "기대안했는데 진짜 재미있었어요",
          "speaker_id": "1",
          "time": "20210821 02:26"
        },
        {
          "id": "MDRW2100003572.1.23",
          "form": "저두요ㅎㅎㅎ 영화 티켓값이 아깝지 않았어요",
          "original_form": "저두요ㅎㅎㅎ 영화 티켓값이 아깝지 않았어요",
          "speaker_id": "2",
          "time": "20210821 02:28"
        },
        {
          "id": "MDRW2100003572.1.24",
          "form": "저두요 오랜만에 티켓값 다 주고봐도 괜찮은 영화였어요 ㅋㅋ",
          "original_form": "저두요 오랜만에 티켓값 다 주고봐도 괜찮은 영화였어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:29"
        },
        {
          "id": "MDRW2100003572.1.25",
          "form": "티켓 값이 점점 비싸지니까 극장에 잘 안 가게 되더라구요",
          "original_form": "티켓 값이 점점 비싸지니까 극장에 잘 안 가게 되더라구요",
          "speaker_id": "2",
          "time": "20210821 02:30"
        },
        {
          "id": "MDRW2100003572.1.26",
          "form": "진짜요 요즘 롯데시네마에서",
          "original_form": "진짜요 요즘 롯데시네마에서",
          "speaker_id": "1",
          "time": "20210821 02:30"
        },
        {
          "id": "MDRW2100003572.1.27",
          "form": "싸다구라는 이벤트하는데 그걸로 전 영화 자주봐요!",
          "original_form": "싸다구라는 이벤트하는데 그걸로 전 영화 자주봐요!",
          "speaker_id": "1",
          "time": "20210821 02:30"
        },
        {
          "id": "MDRW2100003572.1.28",
          "form": "그래도 영화는 확실히 극장에서 보는 게 제일 재밌는 것 같아요",
          "original_form": "그래도 영화는 확실히 극장에서 보는 게 제일 재밌는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 02:30"
        },
        {
          "id": "MDRW2100003572.1.29",
          "form": "진짜요 ㅋㅋ!!",
          "original_form": "진짜요 ㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210821 02:31"
        },
        {
          "id": "MDRW2100003572.1.30",
          "form": "오 정보 감사해요~",
          "original_form": "오 정보 감사해요~",
          "speaker_id": "2",
          "time": "20210821 02:31"
        }
      ]
