      "utterance": [
        {
          "id": "MDRW2100006708.1.1",
          "form": "가을이면 스벅 글레이즈드라떼를 마셔야하는데",
          "original_form": "가을이면 스벅 글레이즈드라떼를 마셔야하는데",
          "speaker_id": "2",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006708.1.2",
          "form": "글레이즈드가 뭐야",
          "original_form": "글레이즈드가 뭐야",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006708.1.3",
          "form": "나도 모르는데 맛나다고 추천해줘서 ㅋㅋ",
          "original_form": "나도 모르는데 맛나다고 추천해줘서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100006708.1.4",
          "form": "그런건 집에서 못만드나",
          "original_form": "그런건 집에서 못만드나",
          "speaker_id": "1",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100006708.1.5",
          "form": "일단한번 사먹어보쟈",
          "original_form": "일단한번 사먹어보쟈",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006708.1.6",
          "form": "오늘",
          "original_form": "오늘",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006708.1.7",
          "form": "아 직접 갈까요?",
          "original_form": "아 직접 갈까요?",
          "speaker_id": "1",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100006708.1.8",
          "form": "네 그럽시더 장보면서",
          "original_form": "네 그럽시더 장보면서",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100006708.1.9",
          "form": "무엇을 저녁에 먹을까요",
          "original_form": "무엇을 저녁에 먹을까요",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100006708.1.10",
          "form": "떡볶이",
          "original_form": "떡볶이",
          "speaker_id": "2",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100006708.1.11",
          "form": "맛나겟쥬",
          "original_form": "맛나겟쥬",
          "speaker_id": "2",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100006708.1.12",
          "form": "아기가 못먹자너ㅠ",
          "original_form": "아기가 못먹자너ㅠ",
          "speaker_id": "1",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100006708.1.13",
          "form": "그러네",
          "original_form": "그러네",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100006708.1.14",
          "form": "뭐먹을까",
          "original_form": "뭐먹을까",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100006708.1.15",
          "form": "일단 떡볶이?ㅋㅋ",
          "original_form": "일단 떡볶이?ㅋㅋ",
