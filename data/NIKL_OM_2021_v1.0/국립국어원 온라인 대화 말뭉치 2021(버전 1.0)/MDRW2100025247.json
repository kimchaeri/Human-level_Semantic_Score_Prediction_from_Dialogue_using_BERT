      "utterance": [
        {
          "id": "MDRW2100025247.1.1",
          "form": "영화 좋아하시나요",
          "original_form": "영화 좋아하시나요",
          "speaker_id": "2",
          "time": "20210901 02:40"
        },
        {
          "id": "MDRW2100025247.1.2",
          "form": "영화는 영화관가서 보는걸 좋아하는데 코로나이후론..... ㅠ",
          "original_form": "영화는 영화관가서 보는걸 좋아하는데 코로나이후론..... ㅠ",
          "speaker_id": "1",
          "time": "20210901 02:42"
        },
        {
          "id": "MDRW2100025247.1.3",
          "form": "그쵸 아무래도",
          "original_form": "그쵸 아무래도",
          "speaker_id": "2",
          "time": "20210901 02:46"
        },
        {
          "id": "MDRW2100025247.1.4",
          "form": "집에서 아무리 좋아졌대도",
          "original_form": "집에서 아무리 좋아졌대도",
          "speaker_id": "2",
          "time": "20210901 02:46"
        },
        {
          "id": "MDRW2100025247.1.5",
          "form": "영화관이랑은 느낌 다르잖아요",
          "original_form": "영화관이랑은 느낌 다르잖아요",
          "speaker_id": "2",
          "time": "20210901 02:46"
        },
        {
          "id": "MDRW2100025247.1.6",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025247.1.7",
          "form": "영화관 자주 가시나요?",
          "original_form": "영화관 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025247.1.8",
          "form": "저 원래는 한달에 한번?",
          "original_form": "저 원래는 한달에 한번?",
          "speaker_id": "2",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025247.1.9",
          "form": "근데 요새는 두세달?ㅠ",
          "original_form": "근데 요새는 두세달?ㅠ",
          "speaker_id": "2",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025247.1.10",
          "form": "그래도 좀 가시네요~",
          "original_form": "그래도 좀 가시네요~",
          "speaker_id": "1",
          "time": "20210901 02:49"
        },
        {
          "id": "MDRW2100025247.1.11",
          "form": "최근에 뭐 보셨어요?",
          "original_form": "최근에 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210901 02:49"
        },
        {
          "id": "MDRW2100025247.1.12",
          "form": "최근에 모가디슈요 ㅎㅎ",
          "original_form": "최근에 모가디슈요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025247.1.13",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025247.1.14",
          "form": "저는 기억이 안납니다 ㅋㅋㅋ",
          "original_form": "저는 기억이 안납니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025247.1.15",
          "form": "영화관 안간지 너무 오래되서요",
          "original_form": "영화관 안간지 너무 오래되서요",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025247.1.16",
          "form": "모가디슈 내용이 아프간 상황이랑 비슷하다고 하던데",
          "original_form": "모가디슈 내용이 아프간 상황이랑 비슷하다고 하던데",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025247.1.17",
          "form": "어떤가요?",
          "original_form": "어떤가요?",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025247.1.18",
          "form": "네 맞아요 반란 일어나서",
          "original_form": "네 맞아요 반란 일어나서",
          "speaker_id": "2",
          "time": "20210901 02:53"
        },
        {
          "id": "MDRW2100025247.1.19",
          "form": "뭐 믿고보는 조인성",
          "original_form": "뭐 믿고보는 조인성",
          "speaker_id": "2",
          "time": "20210901 02:53"
        },
        {
          "id": "MDRW2100025247.1.20",
          "form": "이정도로 정리가 ㅎㅎ",
          "original_form": "이정도로 정리가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:53"
        },
        {
          "id": "MDRW2100025247.1.21",
          "form": "정리가 된건가요 ㅎㅎ",
          "original_form": "정리가 된건가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 02:54"
        },
        {
          "id": "MDRW2100025247.1.22",
          "form": "조인성이 어떤 역할이에여?",
          "original_form": "조인성이 어떤 역할이에여?",
          "speaker_id": "1",
          "time": "20210901 02:54"
        },
        {
          "id": "MDRW2100025247.1.23",
          "form": "그 남한쪽 2인자?",
          "original_form": "그 남한쪽 2인자?",
          "speaker_id": "2",
          "time": "20210901 02:55"
        },
        {
          "id": "MDRW2100025247.1.24",
          "form": "탈출 루트 알아보는 영리한데",
          "original_form": "탈출 루트 알아보는 영리한데",
          "speaker_id": "2",
          "time": "20210901 02:55"
        },
        {
          "id": "MDRW2100025247.1.25",
          "form": "좀 건방진 느낌이오",
          "original_form": "좀 건방진 느낌이오",
          "speaker_id": "2",
          "time": "20210901 02:56"
        },
        {
          "id": "MDRW2100025247.1.26",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 02:57"
        },
        {
          "id": "MDRW2100025247.1.27",
          "form": "그럼 현지인을 탈출시키는거에요?",
          "original_form": "그럼 현지인을 탈출시키는거에요?",
          "speaker_id": "1",
          "time": "20210901 02:58"
        },
        {
          "id": "MDRW2100025247.1.28",
          "form": "아뇨 아뇨",
          "original_form": "아뇨 아뇨",
          "speaker_id": "2",
          "time": "20210901 02:58"
        },
        {
          "id": "MDRW2100025247.1.29",
          "form": "소말리아에 있는 한국인 대사관 직원들이요ㅋㅋ",
          "original_form": "소말리아에 있는 한국인 대사관 직원들이요ㅋㅋ",
