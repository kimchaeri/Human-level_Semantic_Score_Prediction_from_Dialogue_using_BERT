      "utterance": [
        {
          "id": "MDRW2100047251.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100047251.1.2",
          "form": "앗 즐겨하는 편은 아니지만 리듬게임만 합니다",
          "original_form": "앗 즐겨하는 편은 아니지만 리듬게임만 합니다",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100047251.1.3",
          "form": "name1님은 게임 좋아하시나요?",
          "original_form": "&name1&님은 게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100047251.1.4",
          "form": "저도 리듬게임 즐겨해요! 무슨 리듬게임 하세요?",
          "original_form": "저도 리듬게임 즐겨해요! 무슨 리듬게임 하세요?",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100047251.1.5",
          "form": "저 피아노 타일이요!!!",
          "original_form": "저 피아노 타일이요!!!",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100047251.1.6",
          "form": "최근까지는 아니지만 예전에 즐겨했어요",
          "original_form": "최근까지는 아니지만 예전에 즐겨했어요",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100047251.1.7",
          "form": "어떤거 하세여*",
          "original_form": "어떤거 하세여*",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100047251.1.8",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100047251.1.9",
          "form": "전 뱅드림이요! 아시나요??",
          "original_form": "전 뱅드림이요! 아시나요??",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100047251.1.10",
          "form": "아뇨아뇨 잼나나요??",
          "original_form": "아뇨아뇨 잼나나요??",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100047251.1.11",
          "form": "가챠겜이라서 스트레스가 크네요...",
          "original_form": "가챠겜이라서 스트레스가 크네요...",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100047251.1.12",
          "form": "쿠키런킹덤 핫한데 안하세요?",
          "original_form": "쿠키런킹덤 핫한데 안하세요?",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100047251.1.13",
          "form": "아앗 세상에 가챠라니",
          "original_form": "아앗 세상에 가챠라니",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100047251.1.14",
          "form": "ㅇㄴ 저 폰에서 쿠킹덤이 안되더라고요",
          "original_form": "ㅇㄴ 저 폰에서 쿠킹덤이 안되더라고요",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100047251.1.15",
          "form": "헙..ㅠㅠ 카카오 게임 하신적 있으세요? 모두의마블같은!",
          "original_form": "헙..ㅠㅠ 카카오 게임 하신적 있으세요? 모두의마블같은!",
          "speaker_id": "1",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100047251.1.16",
          "form": "헉 전혀요...헉..",
          "original_form": "헉 전혀요...헉..",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100047251.1.17",
          "form": "제가 완전...그쪽에는...",
          "original_form": "제가 완전...그쪽에는...",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100047251.1.18",
          "form": "무슨 게임을 제일 좋아하세요??",
          "original_form": "무슨 게임을 제일 좋아하세요??",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100047251.1.19",
          "form": "아 그러셨군요ㅠㅠ 혹시 오버워치 아세요?",
          "original_form": "아 그러셨군요ㅠㅠ 혹시 오버워치 아세요?",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100047251.1.20",
          "form": "넵 아 고건 압니다!!!!!!!",
          "original_form": "넵 아 고건 압니다!!!!!!!",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100047251.1.21",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100047251.1.22",
          "form": "우와 주변에 하는분 계세요?",
          "original_form": "우와 주변에 하는분 계세요?",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100047251.1.23",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100047251.1.24",
          "form": "거의 하던걸요??",
          "original_form": "거의 하던걸요??",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100047251.1.25",
          "form": "오버워치가ㅜ그 한국인 캐릭터 있는거 맞죠??",
          "original_form": "오버워치가ㅜ그 한국인 캐릭터 있는거 맞죠??",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100047251.1.26",
          "form": "네네 맞아요!",
          "original_form": "네네 맞아요!",
          "speaker_id": "1",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100047251.1.27",
          "form": "음청 많이하더라고요",
          "original_form": "음청 많이하더라고요",
          "speaker_id": "2",
          "time": "20210826 01:35"
        }
      ]
    }
