      "utterance": [
        {
          "id": "MDRW2100004103.1.1",
          "form": "좋아하는 음식 뭐예용?!?!",
          "original_form": "좋아하는 음식 뭐예용?!?!",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004103.1.2",
          "form": "ㅋㅋ전 고기요",
          "original_form": "ㅋㅋ전 고기요",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004103.1.3",
          "form": "아 풀좀 먹어야 해요",
          "original_form": "아 풀좀 먹어야 해요",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004103.1.4",
          "form": "전생에 사자였습니다 ㅎㅎ",
          "original_form": "전생에 사자였습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004103.1.5",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004103.1.6",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 저는 탄수화물 진짜 좋아해요...",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 저는 탄수화물 진짜 좋아해요...",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004103.1.7",
          "form": "맞아요 탄수화물 진리죠",
          "original_form": "맞아요 탄수화물 진리죠",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004103.1.8",
          "form": "면 빵 아 못끊어요",
          "original_form": "면 빵 아 못끊어요",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004103.1.9",
          "form": "진짜요.. 떡볶이도요..... 피자.......",
          "original_form": "진짜요.. 떡볶이도요..... 피자.......",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004103.1.10",
          "form": "맞아요 ㅋㅋ두끼서 왕창 먹었는게 요샌 꾹꾹눌러서 포장해와요",
          "original_form": "맞아요 ㅋㅋ두끼서 왕창 먹었는게 요샌 꾹꾹눌러서 포장해와요",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004103.1.11",
          "form": "한계를 시험하며 눌러서 포장 합니다 ㅋㅋ",
          "original_form": "한계를 시험하며 눌러서 포장 합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004103.1.12",
          "form": "넣는 만큼 가져가게 해줘서 담는 재미가 있어요",
          "original_form": "넣는 만큼 가져가게 해줘서 담는 재미가 있어요",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004103.1.13",
          "form": "두끼 담기 도전해 보셨어요?",
          "original_form": "두끼 담기 도전해 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004103.1.14",
          "form": "아뇨 저 안해봤어요... 정말 그렇게 꽉꽉 담아지나요??ㅋㅋㅋㅋㅋㅋㅋㅋ 해보고는 싶은데 아직 도전하지 못했어요",
          "original_form": "아뇨 저 안해봤어요... 정말 그렇게 꽉꽉 담아지나요??ㅋㅋㅋㅋㅋㅋㅋㅋ 해보고는 싶은데 아직 도전하지 못했어요",
          "speaker_id": "2",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004103.1.15",
          "form": "네 마구 들어가요 큰중국당면 옆으로 사이드에 누워서 넣으면 공간 절약돼요",
          "original_form": "네 마구 들어가요 큰중국당면 옆으로 사이드에 누워서 넣으면 공간 절약돼요",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004103.1.16",
          "form": "해보세요 승부욕 불타요",
          "original_form": "해보세요 승부욕 불타요",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004103.1.17",
          "form": "집에서 편하게 가져와서 먹음좋아요",
          "original_form": "집에서 편하게 가져와서 먹음좋아요",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004103.1.18",
          "form": "티비보며 먹어보세요",
          "original_form": "티비보며 먹어보세요",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004103.1.19",
          "form": "두번은 해먹을수 있어요",
          "original_form": "두번은 해먹을수 있어요",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004103.1.20",
          "form": "피자는 어디 피자요?",
          "original_form": "피자는 어디 피자요?",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004103.1.21",
          "form": "전 어제 피자헛 씬 피자 먹었는데 맛나더라구요",
          "original_form": "전 어제 피자헛 씬 피자 먹었는데 맛나더라구요",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004103.1.22",
          "form": "저 페퍼로니 피자 너무 좋아해요.... 피자헛 페퍼로니 치즈추가 기억해주세요  ...",
          "original_form": "저 페퍼로니 피자 너무 좋아해요.... 피자헛 페퍼로니 치즈추가 기억해주세요  ...",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004103.1.23",
          "form": "어머 저도요",
          "original_form": "어머 저도요",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004103.1.24",
          "form": "페페로니 만들어있는거 짱입니다",
          "original_form": "페페로니 만들어있는거 짱입니다",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004103.1.25",
          "form": "떡볶이는 전 맵게는 못먹고 순한맛요",
          "original_form": "떡볶이는 전 맵게는 못먹고 순한맛요",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004103.1.26",
          "form": "역시 먹을줄 아시네요!!ㅋㅋㅋㅋㅋㅋ",
          "original_form": "역시 먹을줄 아시네요!!ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004103.1.27",
          "form": "저는 매운맛을 좋아해요>.<",
          "original_form": "저는 매운맛을 좋아해요>.<",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004103.1.28",
          "form": "오 진정한 떡볶이 매니아네요",
          "original_form": "오 진정한 떡볶이 매니아네요",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004103.1.29",
          "form": "전 ㅜㅜ매운거 약해요",
          "original_form": "전 ㅜㅜ매운거 약해요",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004103.1.30",
          "form": "하지만 떡볶이 일주일 한번은 먹나봐요 ㅋㅋ",
          "original_form": "하지만 떡볶이 일주일 한번은 먹나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004103.1.31",
          "form": "자주 먹어요",
          "original_form": "자주 먹어요",
          "speaker_id": "1",
          "time": "20210821 15:24"
        }
      ]
