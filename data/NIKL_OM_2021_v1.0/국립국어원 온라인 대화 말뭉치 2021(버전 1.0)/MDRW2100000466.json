      "utterance": [
        {
          "id": "MDRW2100000466.1.1",
          "form": "요즘 쇼핑 뭐하시나요",
          "original_form": "요즘 쇼핑 뭐하시나요",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000466.1.2",
          "form": "집에 간식 많이 사는거 같아요",
          "original_form": "집에 간식 많이 사는거 같아요",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000466.1.3",
          "form": "악 맞아요ㅋㅋㅋㅋ",
          "original_form": "악 맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000466.1.4",
          "form": "저는 아줌마라서ㅋㅋㅋ",
          "original_form": "저는 아줌마라서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000466.1.5",
          "form": "코로나때문에 밖에 안 나가고 집콕이라 ㅎㅎ",
          "original_form": "코로나때문에 밖에 안 나가고 집콕이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000466.1.6",
          "form": "온라인쇼핑 많이해요",
          "original_form": "온라인쇼핑 많이해요",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000466.1.7",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000466.1.8",
          "form": "컴을 많이 하는 거 갘아요",
          "original_form": "컴을 많이 하는 거 갘아요",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000466.1.9",
          "form": "저 핸드폰 중독 된것 같아요",
          "original_form": "저 핸드폰 중독 된것 같아요",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000466.1.10",
          "form": "저는 오아시스에서 주로 쇼핑해요",
          "original_form": "저는 오아시스에서 주로 쇼핑해요",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000466.1.11",
          "form": "혹시 아시나요??",
          "original_form": "혹시 아시나요??",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000466.1.12",
          "form": "처음 들어보네요",
          "original_form": "처음 들어보네요",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000466.1.13",
          "form": "악 그렇군요",
          "original_form": "악 그렇군요",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000466.1.14",
          "form": "마켓컬리는 아시죵??",
          "original_form": "마켓컬리는 아시죵??",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000466.1.15",
          "form": "알지만 ㅎㅎ",
          "original_form": "알지만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000466.1.16",
          "form": "저는 옥셔 지마켓 애용해요",
          "original_form": "저는 옥셔 지마켓 애용해요",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000466.1.17",
          "form": "앗 저도요ㅋㅋㅋ",
          "original_form": "앗 저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000466.1.18",
