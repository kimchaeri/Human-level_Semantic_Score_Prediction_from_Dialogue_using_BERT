      "utterance": [
        {
          "id": "MDRW2100006845.1.1",
          "form": "저는 배달음식 좋아해요",
          "original_form": "저는 배달음식 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006845.1.2",
          "form": "오 치킨같은거요?",
          "original_form": "오 치킨같은거요?",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006845.1.3",
          "form": "네 피자도 좋아하고",
          "original_form": "네 피자도 좋아하고",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006845.1.4",
          "form": "좋아하는 치킨이 무엇인가요?",
          "original_form": "좋아하는 치킨이 무엇인가요?",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100006845.1.5",
          "form": "요즘엔 굽네가 맛있ㅇ더라구요",
          "original_form": "요즘엔 굽네가 맛있ㅇ더라구요",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006845.1.6",
          "form": "굽네치킨 맛있죠",
          "original_form": "굽네치킨 맛있죠",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006845.1.7",
          "form": "짭잘하고 간도 딱 잘돼있고...",
          "original_form": "짭잘하고 간도 딱 잘돼있고...",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006845.1.8",
          "form": "저도 어릴때부터 많이 먹어쓴데",
          "original_form": "저도 어릴때부터 많이 먹어쓴데",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006845.1.9",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006845.1.10",
          "form": "굽네는 칼로리도 낮더라구요",
          "original_form": "굽네는 칼로리도 낮더라구요",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006845.1.11",
          "form": "튀긴게 아니라서 그런가봐요",
          "original_form": "튀긴게 아니라서 그런가봐요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006845.1.12",
          "form": "네 다이어트할때 먹어도 좋다고",
          "original_form": "네 다이어트할때 먹어도 좋다고",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006845.1.13",
          "form": "참고해야겠네요 ㅎㅎ",
          "original_form": "참고해야겠네요 ㅎㅎ",
          "speaker_id": "2",
