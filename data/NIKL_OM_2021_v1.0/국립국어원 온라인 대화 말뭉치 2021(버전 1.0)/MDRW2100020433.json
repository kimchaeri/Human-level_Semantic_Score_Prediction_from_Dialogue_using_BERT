      "utterance": [
        {
          "id": "MDRW2100020433.1.1",
          "form": "넹ㅋㅋㅋ 시원한 맛",
          "original_form": "넹ㅋㅋㅋ 시원한 맛",
          "speaker_id": "2",
          "time": "20210828 05:31"
        },
        {
          "id": "MDRW2100020433.1.2",
          "form": "그런데 주말인데 되게 일찍 일어나셨네용",
          "original_form": "그런데 주말인데 되게 일찍 일어나셨네용",
          "speaker_id": "1",
          "time": "20210828 05:31"
        },
        {
          "id": "MDRW2100020433.1.3",
          "form": "저도 일찍 일어나서",
          "original_form": "저도 일찍 일어나서",
          "speaker_id": "1",
          "time": "20210828 05:31"
        },
        {
          "id": "MDRW2100020433.1.4",
          "form": "지금 파리바게트 오픈 기다리구있어요",
          "original_form": "지금 파리바게트 오픈 기다리구있어요",
          "speaker_id": "1",
          "time": "20210828 05:31"
        },
        {
          "id": "MDRW2100020433.1.5",
          "form": "넼ㅋㅋ 일찍 잤더니 눈이 떠졌어요",
          "original_form": "넼ㅋㅋ 일찍 잤더니 눈이 떠졌어요",
          "speaker_id": "2",
          "time": "20210828 05:31"
        },
        {
          "id": "MDRW2100020433.1.6",
          "form": "파바 오픈 ㅋㅋㅋㅋ",
          "original_form": "파바 오픈 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:31"
        },
        {
          "id": "MDRW2100020433.1.7",
          "form": "크 갓만든빵 사러가면",
          "original_form": "크 갓만든빵 사러가면",
          "speaker_id": "2",
          "time": "20210828 05:31"
        },
        {
          "id": "MDRW2100020433.1.8",
          "form": "아핫 그러셨구낭ㅋㅋㅋ",
          "original_form": "아핫 그러셨구낭ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:31"
        },
        {
          "id": "MDRW2100020433.1.9",
          "form": "좋죠 좋아",
          "original_form": "좋죠 좋아",
          "speaker_id": "2",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.10",
          "form": "맞아요ㅠㅠ 너무 맛있어요 갓구운빵",
          "original_form": "맞아요ㅠㅠ 너무 맛있어요 갓구운빵",
          "speaker_id": "1",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.11",
          "form": "무슨 빵 기다려요",
          "original_form": "무슨 빵 기다려요",
          "speaker_id": "2",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.12",
          "form": "저 크로와상이랑",
          "original_form": "저 크로와상이랑",
          "speaker_id": "1",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.13",
          "form": "토종효모빵이라고 있는데",
          "original_form": "토종효모빵이라고 있는데",
          "speaker_id": "1",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.14",
          "form": "이거 진짜 맛있어요",
          "original_form": "이거 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.15",
          "form": "오오 뭔가 이름부터",
          "original_form": "오오 뭔가 이름부터",
          "speaker_id": "2",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.16",
          "form": "맛있을 삘이메요",
          "original_form": "맛있을 삘이메요",
          "speaker_id": "2",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.17",
          "form": "빵순이라서",
          "original_form": "빵순이라서",
          "speaker_id": "2",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.18",
          "form": "슴슴한데 특유의 구운?듯한 향이나고",
          "original_form": "슴슴한데 특유의 구운?듯한 향이나고",
          "speaker_id": "1",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.20",
          "form": "쫄깃해요",
          "original_form": "쫄깃해요",
          "speaker_id": "1",
          "time": "20210828 05:32"
        },
        {
          "id": "MDRW2100020433.1.21",
          "form": "아 빵 좋아하시는구낭!",
          "original_form": "아 빵 좋아하시는구낭!",
          "speaker_id": "1",
          "time": "20210828 05:33"
        },
        {
          "id": "MDRW2100020433.1.22",
          "form": "오오 쫄깃한거 좋죠",
          "original_form": "오오 쫄깃한거 좋죠",
          "speaker_id": "2",
          "time": "20210828 05:33"
        },
        {
          "id": "MDRW2100020433.1.23",
          "form": "전 밀가루면 다 좋아요ㅋㅋ",
          "original_form": "전 밀가루면 다 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:33"
        }
      ]
