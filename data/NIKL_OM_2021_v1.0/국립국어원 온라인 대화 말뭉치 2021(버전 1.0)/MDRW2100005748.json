      "utterance": [
        {
          "id": "MDRW2100005748.1.1",
          "form": "텀블러 자주 들고 다니세요?",
          "original_form": "텀블러 자주 들고 다니세요?",
          "speaker_id": "2",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100005748.1.2",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100005748.1.3",
          "form": "요즘엔 코로나라고 안받아주는 곳이 많아져ㅅㅓ",
          "original_form": "요즘엔 코로나라고 안받아주는 곳이 많아져ㅅㅓ",
          "speaker_id": "2",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100005748.1.4",
          "form": "좀 슬퍼요",
          "original_form": "좀 슬퍼요",
          "speaker_id": "2",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100005748.1.5",
          "form": "저도 텀블러 할인은 해드리지만 텀블러 사용이 안된다고 ㅋㅋㅋ",
          "original_form": "저도 텀블러 할인은 해드리지만 텀블러 사용이 안된다고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100005748.1.6",
          "form": "ㅋㅋㅋ그니가요 스벅이 그러자나요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그니가요 스벅이 그러자나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:40"
        },
        {
          "id": "MDRW2100005748.1.7",
          "form": "보여주면 할인은해주고..근데 이제 일회용을 곁들인..",
          "original_form": "보여주면 할인은해주고..근데 이제 일회용을 곁들인..",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100005748.1.8",
          "form": "제주는 리유저블컵만 쓰는 매장있어요",
          "original_form": "제주는 리유저블컵만 쓰는 매장있어요",
          "speaker_id": "1",
          "time": "20210907 10:42"
        },
        {
          "id": "MDRW2100005748.1.9",
          "form": "공항에 반납기있고요",
          "original_form": "공항에 반납기있고요",
          "speaker_id": "1",
          "time": "20210907 10:42"
        },
        {
          "id": "MDRW2100005748.1.10",
          "form": "아 맞아 기사봤어요!",
          "original_form": "아 맞아 기사봤어요!",
          "speaker_id": "2",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100005748.1.11",
          "form": "좋은 취지라고 생각했는데 사용자들이 많나요?",
          "original_form": "좋은 취지라고 생각했는데 사용자들이 많나요?",
          "speaker_id": "2",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100005748.1.12",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100005748.1.13",
          "form": "어차피 다 그걸로 줘요",
          "original_form": "어차피 다 그걸로 줘요",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100005748.1.14",
          "form": "나중에 가면 저도 이용해봐야겠어요",
          "original_form": "나중에 가면 저도 이용해봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100005748.1.15",
          "form": "올해는 제주도 가나 했는데 ...흑",
          "original_form": "올해는 제주도 가나 했는데 ...흑",
          "speaker_id": "2",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100005748.1.16",
          "form": "리유저블컵이라 보증금 환불안받구 쓰고",
          "original_form": "리유저블컵이라 보증금 환불안받구 쓰고",
          "speaker_id": "1",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100005748.1.17",
          "form": "계속 쓸까 했는데 미묘하게 향이베서",
          "original_form": "계속 쓸까 했는데 미묘하게 향이베서",
          "speaker_id": "1",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100005748.1.18",
          "form": "계속 쓰긴 어려울 듯 해요",
          "original_form": "계속 쓰긴 어려울 듯 해요",
