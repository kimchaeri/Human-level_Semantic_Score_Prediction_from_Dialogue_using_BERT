      "utterance": [
        {
          "id": "MDRW2100008993.1.1",
          "form": "저녁은 드셨어용??",
          "original_form": "저녁은 드셨어용??",
          "speaker_id": "1",
          "time": "20210822 20:15"
        },
        {
          "id": "MDRW2100008993.1.2",
          "form": "오늘 일이 늦게 끝나서 가볍게 초코파이만 먹고 퇴근중이요ㅜ",
          "original_form": "오늘 일이 늦게 끝나서 가볍게 초코파이만 먹고 퇴근중이요ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:15"
        },
        {
          "id": "MDRW2100008993.1.3",
          "form": "이따 집 가서 먹어야죠",
          "original_form": "이따 집 가서 먹어야죠",
          "speaker_id": "2",
          "time": "20210822 20:16"
        },
        {
          "id": "MDRW2100008993.1.4",
          "form": "name1님은 뭘 드셨나요",
          "original_form": "&name1&님은 뭘 드셨나요",
          "speaker_id": "2",
          "time": "20210822 20:16"
        },
        {
          "id": "MDRW2100008993.1.5",
          "form": "오늘도 출근하셨구나 ㅜㅜ",
          "original_form": "오늘도 출근하셨구나 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 20:16"
        },
        {
          "id": "MDRW2100008993.1.6",
          "form": "저는 칼국수 먹고싶었는데 가게가 문을 닫아서 ㅠ",
          "original_form": "저는 칼국수 먹고싶었는데 가게가 문을 닫아서 ㅠ",
          "speaker_id": "1",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008993.1.7",
          "form": "오는길에 봉지라면사서 끓여먹었어요 ㅎㅎ",
          "original_form": "오는길에 봉지라면사서 끓여먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008993.1.8",
          "form": "가장 최근에 시켜먹은 배달음식은 뭔가용??",
          "original_form": "가장 최근에 시켜먹은 배달음식은 뭔가용??",
          "speaker_id": "1",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008993.1.9",
          "form": "앗ㅜㅜ 근처에 칼국수 배달하는 곳 없었나봐요",
          "original_form": "앗ㅜㅜ 근처에 칼국수 배달하는 곳 없었나봐요",
          "speaker_id": "2",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008993.1.10",
          "form": "도다리 세꼬시예요. 해산물을 좋아해서ㅎ",
          "original_form": "도다리 세꼬시예요. 해산물을 좋아해서ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008993.1.11",
          "form": "아아 동네에 칼국수맛집이라고 해서 벼르고 있었거든요 ㅋㅋㅋㅋ",
          "original_form": "아아 동네에 칼국수맛집이라고 해서 벼르고 있었거든요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008993.1.12",
          "form": "근데 거기가 문을 닫았어요ㅜㅜㅜ 오늘 쉰대요",
          "original_form": "근데 거기가 문을 닫았어요ㅜㅜㅜ 오늘 쉰대요",
          "speaker_id": "1",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008993.1.13",
          "form": "맛집은 일찍 문닫죠ㅜ",
          "original_form": "맛집은 일찍 문닫죠ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008993.1.14",
          "form": "아앗 가는 날이 장날ㅜ",
          "original_form": "아앗 가는 날이 장날ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008993.1.15",
          "form": "그러고보니 칼국수 가성비 맛집 한곳 아는데",
          "original_form": "그러고보니 칼국수 가성비 맛집 한곳 아는데",
          "speaker_id": "2",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100008993.1.16",
          "form": "서울에 있어요",
          "original_form": "서울에 있어요",
          "speaker_id": "2",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100008993.1.17",
          "form": "가격과 달리 국물이 진함",
          "original_form": "가격과 달리 국물이 진함",
          "speaker_id": "2",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100008993.1.18",
          "form": "어디 사세요?",
          "original_form": "어디 사세요?",
          "speaker_id": "2",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100008993.1.19",
          "form": "서울이요!",
          "original_form": "서울이요!",
          "speaker_id": "1",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100008993.1.20",
          "form": "와! 영천시장 도깨비손칼국수라고 시장구경하고 먹으면 굿굿",
          "original_form": "와! 영천시장 도깨비손칼국수라고 시장구경하고 먹으면 굿굿",
          "speaker_id": "2",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100008993.1.21",
          "form": "평소 맛집탐방하시나봐요 ㅎㅎ",
          "original_form": "평소 맛집탐방하시나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100008993.1.22",
          "form": "혹시 지나가게 되면 들르셔요",
          "original_form": "혹시 지나가게 되면 들르셔요",
          "speaker_id": "2",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100008993.1.23",
          "form": "아니 회사서 멀지 않아서 가끔 가요ㅎ",
          "original_form": "아니 회사서 멀지 않아서 가끔 가요ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100008993.1.24",
          "form": "가격이 2500원이라 엄청 싸요",
          "original_form": "가격이 2500원이라 엄청 싸요",
          "speaker_id": "2",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100008993.1.25",
          "form": "오오~~ 서대문! ㅎㅎㅎ 기억하고 있을게여ㅋㅋㅋ",
          "original_form": "오오~~ 서대문! ㅎㅎㅎ 기억하고 있을게여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:21"
        },
        {
          "id": "MDRW2100008993.1.26",
          "form": "면음식 별로 안좋아하는데 괜찮더라고요",
          "original_form": "면음식 별로 안좋아하는데 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210822 20:21"
        },
        {
          "id": "MDRW2100008993.1.27",
          "form": "ㅎㅎ넵넵 일부러 찾아갈 정도는 아니지만",
          "original_form": "ㅎㅎ넵넵 일부러 찾아갈 정도는 아니지만",
          "speaker_id": "2",
          "time": "20210822 20:21"
        },
        {
          "id": "MDRW2100008993.1.28",
          "form": "저도 칼국수를 막 사먹을정도로 좋아하진않는데 며칠전 밤에 비엄청오던날 너무 땡기더라구요~",
          "original_form": "저도 칼국수를 막 사먹을정도로 좋아하진않는데 며칠전 밤에 비엄청오던날 너무 땡기더라구요~",
          "speaker_id": "1",
          "time": "20210822 20:21"
        },
        {
          "id": "MDRW2100008993.1.29",
          "form": "겉절이에 딱 먹고싶었어요 ㅋㅋㅋ 쌀쌀해지면 뜨끈한 국물이 생각나는건 국룰 ㅋㅋㅋ",
          "original_form": "겉절이에 딱 먹고싶었어요 ㅋㅋㅋ 쌀쌀해지면 뜨끈한 국물이 생각나는건 국룰 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:22"
        },
        {
          "id": "MDRW2100008993.1.30",
          "form": "그쵸 비 오고 그러면 국물 생각나요ㅜ",
          "original_form": "그쵸 비 오고 그러면 국물 생각나요ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:22"
        },
        {
          "id": "MDRW2100008993.1.31",
          "form": "저도 장마철에 주로 갔어요",
          "original_form": "저도 장마철에 주로 갔어요",
          "speaker_id": "2",
          "time": "20210822 20:22"
        },
        {
          "id": "MDRW2100008993.1.32",
          "form": "이제 가을장마라 또 칼국수철이",
          "original_form": "이제 가을장마라 또 칼국수철이",
          "speaker_id": "2",
          "time": "20210822 20:22"
        },
        {
          "id": "MDRW2100008993.1.33",
          "form": "부디 내일은 그 칼국수 맛집이 문 열길 빌게요",
          "original_form": "부디 내일은 그 칼국수 맛집이 문 열길 빌게요",
          "speaker_id": "2",
          "time": "20210822 20:23"
        },
        {
          "id": "MDRW2100008993.1.34",
          "form": "내일은 삼계탕 먹기로 해서 ㅋㅋㅋ 칼국수는 언젠가...",
          "original_form": "내일은 삼계탕 먹기로 해서 ㅋㅋㅋ 칼국수는 언젠가...",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008993.1.35",
          "form": "아이고ㅎㅎ 삼계탕도 좋죠.",
          "original_form": "아이고ㅎㅎ 삼계탕도 좋죠.",
          "speaker_id": "2",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008993.1.36",
          "form": "저도 집 가면 레토르트 삼계탕 먹어야겠어요",
          "original_form": "저도 집 가면 레토르트 삼계탕 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100008993.1.37",
          "form": "요즘 날이 차가워서 뭔가 국물있고 뜨거운음식이 땡기는거 같아요ㅠㅋㅋ",
          "original_form": "요즘 날이 차가워서 뭔가 국물있고 뜨거운음식이 땡기는거 같아요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:29"
        }
      ]
    }
