      "utterance": [
        {
          "id": "MDRW2100007110.1.1",
          "form": "지금 야식먹을까봐요",
          "original_form": "지금 야식먹을까봐요",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007110.1.2",
          "form": "먹는얘기하니 배고파서",
          "original_form": "먹는얘기하니 배고파서",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007110.1.3",
          "form": "그쵸 ㅠ 저는 배는 안고픈데",
          "original_form": "그쵸 ㅠ 저는 배는 안고픈데",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007110.1.4",
          "form": "그냥 먹고 싶네요",
          "original_form": "그냥 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007110.1.5",
          "form": "불막창에 마늘 딱 얹져서 쌈을 똭",
          "original_form": "불막창에 마늘 딱 얹져서 쌈을 똭",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007110.1.6",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007110.1.7",
          "form": "맥주도있어야죠",
          "original_form": "맥주도있어야죠",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007110.1.8",
          "form": "암만요",
          "original_form": "암만요",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.9",
          "form": "ㅋㅋㅋㅋ불막창 일반막창 반반도 좋아요",
          "original_form": "ㅋㅋㅋㅋ불막창 일반막창 반반도 좋아요",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.10",
          "form": "약간 비슷한느낌으로",
          "original_form": "약간 비슷한느낌으로",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.11",
          "form": "장어구이...비싸서자주못가지만",
          "original_form": "장어구이...비싸서자주못가지만",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.12",
          "form": "맞아요 막창 최애 메뉴인데:",
          "original_form": "맞아요 막창 최애 메뉴인데:",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.13",
          "form": "코로나 터지곤 ㅠㅠ",
          "original_form": "코로나 터지곤 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.14",
          "form": "막창은진짜 너무맛있는데 배달치곤비싸서",
          "original_form": "막창은진짜 너무맛있는데 배달치곤비싸서",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.15",
          "form": "밖에서 못먹어봤네요 ㅠㅠ",
          "original_form": "밖에서 못먹어봤네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.16",
          "form": "요즘 직접사서구워먹어요ㅠ",
          "original_form": "요즘 직접사서구워먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.17",
          "form": "아.. 꾸릉내",
          "original_form": "아.. 꾸릉내",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.18",
          "form": "조심해야하니까...",
          "original_form": "조심해야하니까...",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.19",
          "form": "ㅜㅜ 잘굽기도쉽지않죠 집에선",
          "original_form": "ㅜㅜ 잘굽기도쉽지않죠 집에선",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007110.1.20",
          "form": "집에서 구우면 냄사가 ㅋㅋㅋㅋ",
          "original_form": "집에서 구우면 냄사가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007110.1.21",
          "form": "ㅂㄱㅋㅋㅋ환기팬 다 돌리구",
          "original_form": "ㅂㄱㅋㅋㅋ환기팬 다 돌리구",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007110.1.22",
          "form": "막창보통 배달 3만원넘으니까ㅠ",
          "original_form": "막창보통 배달 3만원넘으니까ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007110.1.23",
          "form": "에프에 돌리묜 냄,새가. ㅋㅋㅋ",
          "original_form": "에프에 돌리묜 냄,새가. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007110.1.24",
          "form": "그니까여ㅠㅠ 너무비싸여",
          "original_form": "그니까여ㅠㅠ 너무비싸여",
