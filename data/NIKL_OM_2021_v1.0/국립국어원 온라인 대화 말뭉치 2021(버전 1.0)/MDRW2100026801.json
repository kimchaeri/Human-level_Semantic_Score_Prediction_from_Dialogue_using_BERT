        {
          "id": "MDRW2100026801.1.1",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026801.1.2",
          "form": "내가 새로 오픈한 가게에서만 일해서 몰랐는데",
          "original_form": "내가 새로 오픈한 가게에서만 일해서 몰랐는데",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026801.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.4",
          "form": "가게들 진짜 더러운것 같음",
          "original_form": "가게들 진짜 더러운것 같음",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.5",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.6",
          "form": "으...",
          "original_form": "으...",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.7",
          "form": "나 진짜 하삼동에서는 파우더 같은거",
          "original_form": "나 진짜 하삼동에서는 파우더 같은거",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.8",
          "form": "으으...",
          "original_form": "으으...",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.9",
          "form": "텅 다 씻고 말려서 새로 넣고 그랬거든",
          "original_form": "텅 다 씻고 말려서 새로 넣고 그랬거든",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.10",
          "form": "통",
          "original_form": "통",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.11",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.12",
          "form": "컵 같은거 채울때도",
          "original_form": "컵 같은거 채울때도",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026801.1.13",
          "form": "새로운 컵이 밑으로 가게 하고 그랬는데",
          "original_form": "새로운 컵이 밑으로 가게 하고 그랬는데",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.14",
          "form": "여기는 걍 위에 계속 추가해서",
          "original_form": "여기는 걍 위에 계속 추가해서",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.15",
          "form": "밑에 깔린 파우더나 컵들은",
          "original_form": "밑에 깔린 파우더나 컵들은",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.16",
          "form": "으..",
          "original_form": "으..",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.17",
          "form": "언제꺼인지 가늠도 안간다",
          "original_form": "언제꺼인지 가늠도 안간다",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.18",
          "form": "믿을껀 스벅이나",
          "original_form": "믿을껀 스벅이나",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.19",
          "form": "할리스 뿐인것 같음",
          "original_form": "할리스 뿐인것 같음",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.20",
          "form": "그래서 그 스벅 알바생이",
          "original_form": "그래서 그 스벅 알바생이",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.21",
          "form": "저가 커피는 음...",
          "original_form": "저가 커피는 음...",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.22",
          "form": "도.망.쳣.구.나.ㅋㅋㅋㅋ",
          "original_form": "도.망.쳣.구.나.ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026801.1.23",
          "form": "할리스는 위생 괜찮대??",
          "original_form": "할리스는 위생 괜찮대??",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026801.1.24",
          "form": "그 우유 데우는 쇠컵도",
          "original_form": "그 우유 데우는 쇠컵도",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026801.1.25",
          "form": "할리스는 딱 봐도",
          "original_form": "할리스는 딱 봐도",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026801.1.26",
          "form": "깨끗해보이지 않나?",
          "original_form": "깨끗해보이지 않나?",
          "speaker_id": "2",
          "time": "20210902 00:16"
        }
      ]
