      "utterance": [
        {
          "id": "MDRW2100002400.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 11:08"
        },
        {
          "id": "MDRW2100002400.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002400.1.3",
          "form": "이상형이 어찌 되시나요!",
          "original_form": "이상형이 어찌 되시나요!",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002400.1.4",
          "form": "뭐 그런건 딱히 없어요",
          "original_form": "뭐 그런건 딱히 없어요",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002400.1.5",
          "form": "자주 마주치면 좋아지는 타입이라",
          "original_form": "자주 마주치면 좋아지는 타입이라",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002400.1.6",
          "form": "너무 푸짐하지만 않으면 되는",
          "original_form": "너무 푸짐하지만 않으면 되는",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002400.1.7",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002400.1.8",
          "form": "좋아하는 상대가 있으면 연애로 바로 이어지시나요?",
          "original_form": "좋아하는 상대가 있으면 연애로 바로 이어지시나요?",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002400.1.9",
          "form": "그건 상대도 마음이 있어야죠 ^^",
          "original_form": "그건 상대도 마음이 있어야죠 ^^",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100002400.1.10",
          "form": "주로 다가가면 멀어지고 다가오면 밀어내게 되더라고요",
          "original_form": "주로 다가가면 멀어지고 다가오면 밀어내게 되더라고요",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100002400.1.11",
          "form": "그쵸그쵸 ㅠㅠ 내가 좋아하는 상대가 나를 좋아하게 되는게 참 어려운일인것같아요",
          "original_form": "그쵸그쵸 ㅠㅠ 내가 좋아하는 상대가 나를 좋아하게 되는게 참 어려운일인것같아요",
          "speaker_id": "1",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100002400.1.12",
          "form": "사람 심리가 그런거 같아요",
          "original_form": "사람 심리가 그런거 같아요",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002400.1.13",
          "form": "갖지 못한것에 더 집착하고",
          "original_form": "갖지 못한것에 더 집착하고",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100002400.1.14",
          "form": "이젠 편안한 연애를 하고싶은데 막상 연애를 시작하려면 너무 귀찮은것같아요... ㅋㅋ",
          "original_form": "이젠 편안한 연애를 하고싶은데 막상 연애를 시작하려면 너무 귀찮은것같아요... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100002400.1.15",
          "form": "전 이제 연애를 하면 안되서",
          "original_form": "전 이제 연애를 하면 안되서",
          "speaker_id": "2",
          "time": "20210820 11:36"
        },
        {
          "id": "MDRW2100002400.1.16",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:36"
        },
        {
          "id": "MDRW2100002400.1.17",
          "form": "오?? 결혼하셨군요!!!",
          "original_form": "오?? 결혼하셨군요!!!",
          "speaker_id": "1",
          "time": "20210820 11:36"
        },
        {
          "id": "MDRW2100002400.1.18",
          "form": "연애는 상대의 집착 때문에 더 피곤하죠",
          "original_form": "연애는 상대의 집착 때문에 더 피곤하죠",
          "speaker_id": "2",
          "time": "20210820 11:36"
        },
        {
          "id": "MDRW2100002400.1.19",
          "form": "결혼은 혼자서도 고독을 별로 안타면 비추입니다",
          "original_form": "결혼은 혼자서도 고독을 별로 안타면 비추입니다",
          "speaker_id": "2",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100002400.1.20",
          "form": "아 ㅋㅋㅋㅋㅋㅋ 비추하시는군요",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ 비추하시는군요",
          "speaker_id": "1",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100002400.1.21",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210820 11:38"
        },
        {
          "id": "MDRW2100002400.1.22",
          "form": "혼자 살면 150이어도 충분한데",
          "original_form": "혼자 살면 150이어도 충분한데",
          "speaker_id": "2",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100002400.1.23",
          "form": "가정이 생기면 300도 부족한게",
          "original_form": "가정이 생기면 300도 부족한게",
          "speaker_id": "2",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100002400.1.24",
          "form": "삶이고 인간관계도 어려워지고요",
          "original_form": "삶이고 인간관계도 어려워지고요",
          "speaker_id": "2",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100002400.1.25",
          "form": "결혼하면 잘 싸우는 이유가",
          "original_form": "결혼하면 잘 싸우는 이유가",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002400.1.26",
          "form": "참 안좋은어픨",
          "original_form": "참 안좋은어픨",
          "speaker_id": "2",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100002400.1.27",
          "form": "연애는 헤어지면 그만",
          "original_form": "연애는 헤어지면 그만",
          "speaker_id": "2",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100002400.1.28",
          "form": "결혼은 평생 살아야 한다는 부담",
          "original_form": "결혼은 평생 살아야 한다는 부담",
          "speaker_id": "2",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100002400.1.29",
          "form": "그쵸 ㅋㅋㅋ 결혼은 신중해야돠는것같아요",
          "original_form": "그쵸 ㅋㅋㅋ 결혼은 신중해야돠는것같아요",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002400.1.30",
          "form": "그래서 단점이 더 커보여요",
          "original_form": "그래서 단점이 더 커보여요",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002400.1.31",
          "form": "사소한것도 연애에는 괜찮다가",
          "original_form": "사소한것도 연애에는 괜찮다가",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002400.1.32",
          "form": "결혼하면 커져요",
          "original_form": "결혼하면 커져요",
          "speaker_id": "2",
          "time": "20210820 11:42"
        }
      ]
    }
