      "utterance": [
        {
          "id": "MDRW2100037838.1.1",
          "form": "최근에 극장에서 영화보신 적 잇으세요?",
          "original_form": "최근에 극장에서 영화보신 적 잇으세요?",
          "speaker_id": "2",
          "time": "20210901 19:26"
        },
        {
          "id": "MDRW2100037838.1.2",
          "form": "아뇨! 코로나후론 안간것같아요,,,",
          "original_form": "아뇨! 코로나후론 안간것같아요,,,",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100037838.1.3",
          "form": "극장에서 본 마지막영화가 기억도 안나네요",
          "original_form": "극장에서 본 마지막영화가 기억도 안나네요",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100037838.1.4",
          "form": "와 언제적이에요",
          "original_form": "와 언제적이에요",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037838.1.5",
          "form": "진짜 오래전이시네요",
          "original_form": "진짜 오래전이시네요",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037838.1.6",
          "form": "뮤지컬이나 전시회 좋아하세요?!",
          "original_form": "뮤지컬이나 전시회 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037838.1.7",
          "form": "전시는 좀 자주갔어요!",
          "original_form": "전시는 좀 자주갔어요!",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100037838.1.8",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100037838.1.9",
          "form": "저도 코로나이후에 많이안갔지만..",
          "original_form": "저도 코로나이후에 많이안갔지만..",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100037838.1.10",
          "form": "어떤전시가 가장 기억에 남으세요?!",
          "original_form": "어떤전시가 가장 기억에 남으세요?!",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100037838.1.11",
          "form": "지금은 샤갈전이 떠오르네요",
          "original_form": "지금은 샤갈전이 떠오르네요",
          "speaker_id": "2",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100037838.1.12",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100037838.1.13",
          "form": "멋있어요",
          "original_form": "멋있어요",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100037838.1.14",
          "form": "잌ㅋㅋㅋㅋ",
          "original_form": "잌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100037838.1.15",
          "form": "초록색이 되게 이질적인 색인데",
          "original_form": "초록색이 되게 이질적인 색인데",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100037838.1.16",
          "form": "샤갈은 그걸 되게 따뜻한 느낌으로 써서 신기햇어요",
          "original_form": "샤갈은 그걸 되게 따뜻한 느낌으로 써서 신기햇어요",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100037838.1.17",
          "form": "사람 피부를 초록색으로쓰고 그랫던기억이..",
          "original_form": "사람 피부를 초록색으로쓰고 그랫던기억이..",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100037838.1.18",
          "form": "우와,., 저는전시회가도 막 감상을 잘 못해서",
          "original_form": "우와,., 저는전시회가도 막 감상을 잘 못해서",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100037838.1.19",
          "form": "10분컷이에요,,",
          "original_form": "10분컷이에요,,",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100037838.1.20",
          "form": "저도 사실 기억을 못해요...ㅋ큐ㅠㅠㅠ",
          "original_form": "저도 사실 기억을 못해요...ㅋ큐ㅠㅠㅠ",
