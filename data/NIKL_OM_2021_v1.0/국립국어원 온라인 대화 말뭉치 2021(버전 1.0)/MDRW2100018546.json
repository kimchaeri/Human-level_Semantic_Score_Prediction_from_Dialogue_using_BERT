      "utterance": [
        {
          "id": "MDRW2100018546.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018546.1.2",
          "form": "독서 좋아하시나요!",
          "original_form": "독서 좋아하시나요!",
          "speaker_id": "1",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018546.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018546.1.4",
          "form": "책 꾸준히 보려고 노력중이기는 한데",
          "original_form": "책 꾸준히 보려고 노력중이기는 한데",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018546.1.5",
          "form": "일하면서 책 보기가 쉽진 않네요",
          "original_form": "일하면서 책 보기가 쉽진 않네요",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018546.1.6",
          "form": "그쵸ㅠ 그리고 분야를 다양하게",
          "original_form": "그쵸ㅠ 그리고 분야를 다양하게",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100018546.1.7",
          "form": "문학상 수상작도 읽고 세계적인 이슈 책도 읽고 그래야하는데",
          "original_form": "문학상 수상작도 읽고 세계적인 이슈 책도 읽고 그래야하는데",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100018546.1.8",
          "form": "한쪽으로 치우치게 되고 그나마도 잘 안 읽히고ㅜ",
          "original_form": "한쪽으로 치우치게 되고 그나마도 잘 안 읽히고ㅜ",
          "speaker_id": "1",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100018546.1.9",
          "form": "네네 업무관련 책 아니면 잘 안보게 되고요",
          "original_form": "네네 업무관련 책 아니면 잘 안보게 되고요",
          "speaker_id": "2",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100018546.1.10",
          "form": "좋아하는 작가 있으세요?",
          "original_form": "좋아하는 작가 있으세요?",
          "speaker_id": "2",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100018546.1.11",
          "form": "아뇨 그런것도 딱히 없고 취향이 없어요",
          "original_form": "아뇨 그런것도 딱히 없고 취향이 없어요",
          "speaker_id": "1",
          "time": "20210827 08:11"
        },
        {
          "id": "MDRW2100018546.1.12",
          "form": "유명한 작품이라고 해서 읽어보묜 그냥그렇고 ....",
          "original_form": "유명한 작품이라고 해서 읽어보묜 그냥그렇고 ....",
          "speaker_id": "1",
          "time": "20210827 08:12"
        },
        {
          "id": "MDRW2100018546.1.13",
          "form": "그냥 독서가 안 맞나싶고ㅜ",
          "original_form": "그냥 독서가 안 맞나싶고ㅜ",
          "speaker_id": "1",
          "time": "20210827 08:12"
        },
        {
          "id": "MDRW2100018546.1.14",
          "form": "아 아직 본인에게 맞는 책을 못 찾으셨나봐요",
          "original_form": "아 아직 본인에게 맞는 책을 못 찾으셨나봐요",
          "speaker_id": "2",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100018546.1.15",
          "form": "네네ㅎㅎㅎ만화책은 술술 읽히는데ㅋㅋㅋ",
          "original_form": "네네ㅎㅎㅎ만화책은 술술 읽히는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:15"
        },
        {
          "id": "MDRW2100018546.1.16",
          "form": "그럼 보통 책 구매해서 보시나요?",
          "original_form": "그럼 보통 책 구매해서 보시나요?",
          "speaker_id": "1",
          "time": "20210827 08:15"
        },
        {
          "id": "MDRW2100018546.1.17",
          "form": "저희 동네 도서관이 잘돼있어서 도서관에서  빌리기도 해요",
          "original_form": "저희 동네 도서관이 잘돼있어서 도서관에서  빌리기도 해요",
          "speaker_id": "2",
          "time": "20210827 08:17"
        },
        {
          "id": "MDRW2100018546.1.18",
          "form": "저도요! 신작 같은거는 신청하면 서점에서 먼저 대출할수도 있는",
          "original_form": "저도요! 신작 같은거는 신청하면 서점에서 먼저 대출할수도 있는",
          "speaker_id": "1",
          "time": "20210827 08:17"
        },
        {
          "id": "MDRW2100018546.1.19",
          "form": "서비스도 있더라구요#",
          "original_form": "서비스도 있더라구요#",
          "speaker_id": "1",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018546.1.20",
          "form": "맞아요 신작도 잘 대출해줘서",
          "original_form": "맞아요 신작도 잘 대출해줘서",
          "speaker_id": "2",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018546.1.21",
          "form": "좋더라고요",
          "original_form": "좋더라고요",
          "speaker_id": "2",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018546.1.22",
          "form": "저빼고 다들 독서 많이하는지...",
          "original_form": "저빼고 다들 독서 많이하는지...",
          "speaker_id": "1",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018546.1.23",
          "form": "신작은 항상 대출예약이 꽉차서",
          "original_form": "신작은 항상 대출예약이 꽉차서",
          "speaker_id": "1",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018546.1.24",
          "form": "거의 티켓팅급이예여ㅠ",
          "original_form": "거의 티켓팅급이예여ㅠ",
          "speaker_id": "1",
          "time": "20210827 08:22"
        },
        {
          "id": "MDRW2100018546.1.25",
          "form": "그쵸 신작이나 베스트셀러는 대출예약이 꽉 차서ㅠ",
          "original_form": "그쵸 신작이나 베스트셀러는 대출예약이 꽉 차서ㅠ",
          "speaker_id": "2",
          "time": "20210827 08:23"
        },
        {
          "id": "MDRW2100018546.1.26",
          "form": "저는 오늘 정혜윤 작가 책 한권 챙겼어요",
          "original_form": "저는 오늘 정혜윤 작가 책 한권 챙겼어요",
          "speaker_id": "2",
          "time": "20210827 08:23"
        },
        {
          "id": "MDRW2100018546.1.27",
          "form": "주변에서 얘기를 많이 들은 작가라",
          "original_form": "주변에서 얘기를 많이 들은 작가라",
          "speaker_id": "2",
          "time": "20210827 08:23"
        },
        {
          "id": "MDRW2100018546.1.28",
          "form": "한번 읽어보려고요",
          "original_form": "한번 읽어보려고요",
          "speaker_id": "2",
          "time": "20210827 08:24"
        },
        {
          "id": "MDRW2100018546.1.29",
          "form": "오... 검색해봐야겠어요",
          "original_form": "오... 검색해봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018546.1.30",
          "form": "소설이예요?",
          "original_form": "소설이예요?",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018546.1.31",
          "form": "에세이류예요",
          "original_form": "에세이류예요",
          "speaker_id": "2",
          "time": "20210827 08:52"
        }
      ]
    }
