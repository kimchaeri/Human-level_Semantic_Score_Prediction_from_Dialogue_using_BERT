      "utterance": [
        {
          "id": "MDRW2100027096.1.1",
          "form": "저를 자를까봐 걱정됩니다",
          "original_form": "저를 자를까봐 걱정됩니다",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027096.1.2",
          "form": "아직 미혼이시죠?",
          "original_form": "아직 미혼이시죠?",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027096.1.3",
          "form": "미혼은 혼자 먹고싶은거 맘껏먹고",
          "original_form": "미혼은 혼자 먹고싶은거 맘껏먹고",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027096.1.4",
          "form": "소주도 맘껏 마시고",
          "original_form": "소주도 맘껏 마시고",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100027096.1.5",
          "form": "안주도 내맘대로",
          "original_form": "안주도 내맘대로",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100027096.1.6",
          "form": "그맛입니다!",
          "original_form": "그맛입니다!",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100027096.1.7",
          "form": "아니ㅋ  님은 더 잘 드시잖아욧!",
          "original_form": "아니ㅋ  님은 더 잘 드시잖아욧!",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027096.1.8",
          "form": "기만자 아닌가요ㅂㄷㅂㄷ",
          "original_form": "기만자 아닌가요ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027096.1.9",
          "form": "아닙니더 배우자 우선이라",
          "original_form": "아닙니더 배우자 우선이라",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100027096.1.10",
          "form": "전 해산물인데",
          "original_form": "전 해산물인데",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100027096.1.11",
          "form": "와이픈 파스타 뭐 이런거",
          "original_form": "와이픈 파스타 뭐 이런거",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027096.1.12",
          "form": "빵 같은거",
          "original_form": "빵 같은거",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027096.1.13",
          "form": "음식 안맞아유 ㅋㅋㅋ",
          "original_form": "음식 안맞아유 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027096.1.14",
          "form": "아하ㅎㅎ",
          "original_form": "아하ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027096.1.15",
          "form": "라고 하기엔 연어솥밥 드셨잖아욧!",
          "original_form": "라고 하기엔 연어솥밥 드셨잖아욧!",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027096.1.16",
          "form": "아 연어솥밥!",
          "original_form": "아 연어솥밥!",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027096.1.17",
          "form": "맞습니다 제가 잘못했네요 ㅎㅎ",
          "original_form": "맞습니다 제가 잘못했네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027096.1.18",
          "form": "백신 몇시에 맞으시나요?",
          "original_form": "백신 몇시에 맞으시나요?",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027096.1.19",
          "form": "지금 맞았어요!",
          "original_form": "지금 맞았어요!",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027096.1.20",
          "form": "이제 치과 갑니다ㅎ",
          "original_form": "이제 치과 갑니다ㅎ",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027096.1.21",
          "form": "헛 괜찮으신가요?",
          "original_form": "헛 괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027096.1.22",
          "form": "근데 너무 순식간이네요ㄷㄷ맞은 주사중 제일 짧음",
          "original_form": "근데 너무 순식간이네요ㄷㄷ맞은 주사중 제일 짧음",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027096.1.23",
          "form": "화이자?",
          "original_form": "화이자?",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027096.1.24",
          "form": "넹넝",
          "original_form": "넹넝",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027096.1.25",
          "form": "아이고 고생하셨습니다",
          "original_form": "아이고 고생하셨습니다",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027096.1.26",
          "form": "치과는 더 잘먹기 위해가시나요?",
          "original_form": "치과는 더 잘먹기 위해가시나요?",
          "speaker_id": "2",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027096.1.27",
          "form": "ㅎㅎ 저 이 뽑은 날도 삼겹살 먹고",
          "original_form": "ㅎㅎ 저 이 뽑은 날도 삼겹살 먹고",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027096.1.28",
          "form": "임플란트 한날 쌀국수 인스턴트 사다가 양파절임해 먹은 사람입니다",
          "original_form": "임플란트 한날 쌀국수 인스턴트 사다가 양파절임해 먹은 사람입니다",
          "speaker_id": "1",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027096.1.29",
          "form": "당근이죠!!",
          "original_form": "당근이죠!!",
          "speaker_id": "1",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027096.1.30",
          "form": "ㅎㄷㄷ 삼겹살 진심이시군요!",
          "original_form": "ㅎㄷㄷ 삼겹살 진심이시군요!",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027096.1.31",
          "form": "당근도 드시나요?",
          "original_form": "당근도 드시나요?",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027096.1.32",
          "form": "이를 안 뽑은 쪽으로 먹으면 됨ㅎ",
          "original_form": "이를 안 뽑은 쪽으로 먹으면 됨ㅎ",
          "speaker_id": "1",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027096.1.33",
          "form": "의외로ㅋ 채식을 안하는 건 아니에요",
          "original_form": "의외로ㅋ 채식을 안하는 건 아니에요",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027096.1.34",
          "form": "그저 사랑이 많이 작을 뿐이지",
          "original_form": "그저 사랑이 많이 작을 뿐이지",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027096.1.35",
          "form": "님은 당근 드시나요!",
          "original_form": "님은 당근 드시나요!",
          "speaker_id": "1",
          "time": "20210902 10:17"
        }
      ]
    }
