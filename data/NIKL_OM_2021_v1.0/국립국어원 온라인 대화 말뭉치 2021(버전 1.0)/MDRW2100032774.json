      "utterance": [
        {
          "id": "MDRW2100032774.1.1",
          "form": "그냥 온전히 쉬기로 결정 ..",
          "original_form": "그냥 온전히 쉬기로 결정 ..",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032774.1.2",
          "form": "아하...4단계이기도 하구..그렇죠ㅠㅠ",
          "original_form": "아하...4단계이기도 하구..그렇죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032774.1.3",
          "form": "아쉽네여!",
          "original_form": "아쉽네여!",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032774.1.4",
          "form": "네네ㅋㅋ 다들 놀러갈거같기도하구",
          "original_form": "네네ㅋㅋ 다들 놀러갈거같기도하구",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032774.1.5",
          "form": "오랜만에 휴가라",
          "original_form": "오랜만에 휴가라",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032774.1.6",
          "form": "체력충전좀 하려구용 ㅠㅠ",
          "original_form": "체력충전좀 하려구용 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032774.1.7",
          "form": "아하 휴식도 중요하져!",
          "original_form": "아하 휴식도 중요하져!",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032774.1.8",
          "form": "저도 그냥 집에 17일부터 쉬는데 그냥 집에 있으려구요...",
          "original_form": "저도 그냥 집에 17일부터 쉬는데 그냥 집에 있으려구요...",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032774.1.9",
          "form": "네네 ㅋㅋ 그냥 맛난거 시켜먹고 먹으러가고 하려구요",
          "original_form": "네네 ㅋㅋ 그냥 맛난거 시켜먹고 먹으러가고 하려구요",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032774.1.10",
          "form": "먹기위해 일합니당 ..",
          "original_form": "먹기위해 일합니당 ..",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032774.1.11",
          "form": "ㅋㅋㅋ저도 그런 편이예요",
          "original_form": "ㅋㅋㅋ저도 그런 편이예요",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032774.1.12",
          "form": "그래도 긴 연휴 아까운데 근교에 바람이라도 쐬고 그러세여",
          "original_form": "그래도 긴 연휴 아까운데 근교에 바람이라도 쐬고 그러세여",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032774.1.13",
          "form": "네네 그러려구용",
          "original_form": "네네 그러려구용",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032774.1.14",
          "form": "추석이다 뭐다해서 숙박요금도",
          "original_form": "추석이다 뭐다해서 숙박요금도",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032774.1.15",
          "form": "얼마나 쎈지 ..",
          "original_form": "얼마나 쎈지 ..",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032774.1.16",
          "form": "그쵸 숙박비로 그냥 더 맛있는거 먹는게 나아요 ㅋㅋㅋ",
          "original_form": "그쵸 숙박비로 그냥 더 맛있는거 먹는게 나아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100032774.1.17",
          "form": "그쵸ㅋㅋ 원래놀러가려고 방잡으려고 계산까지 다했는데",
          "original_form": "그쵸ㅋㅋ 원래놀러가려고 방잡으려고 계산까지 다했는데",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032774.1.18",
          "form": "숙박 식비 생각하니 2박에 거의 7-80 들어가더라구요 ㅠ",
          "original_form": "숙박 식비 생각하니 2박에 거의 7-80 들어가더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032774.1.19",
          "form": "아까워서 패쑤 .. 다음기회에 ㅋㅋ",
          "original_form": "아까워서 패쑤 .. 다음기회에 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032774.1.20",
          "form": "ㅋㅋㅋ저도 5월 연휴때 놀러가는데",
          "original_form": "ㅋㅋㅋ저도 5월 연휴때 놀러가는데",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032774.1.21",
          "form": "숙박비만 2박에 80썼어요...",
          "original_form": "숙박비만 2박에 80썼어요...",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032774.1.22",
          "form": "그뒤로는 그냥 근교 당일치기만...ㅠㅠ",
          "original_form": "그뒤로는 그냥 근교 당일치기만...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032774.1.23",
          "form": "맞아요 요즘은 시국도 글코",
          "original_form": "맞아요 요즘은 시국도 글코",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032774.1.24",
          "form": "워낙 풀빌라가 좋아서",
          "original_form": "워낙 풀빌라가 좋아서",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032774.1.25",
          "form": "예약하려하면 1박에 4.50 ㅠ",
          "original_form": "예약하려하면 1박에 4.50 ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032774.1.26",
          "form": "맞아요 심지어 예약 안된 곳 찾기도 어렵죠...",
          "original_form": "맞아요 심지어 예약 안된 곳 찾기도 어렵죠...",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100032774.1.27",
          "form": "인스타에서 유명한 곳은 1~2년 뒤도 예약돼있더라구요 ㅋㅋㅋ",
          "original_form": "인스타에서 유명한 곳은 1~2년 뒤도 예약돼있더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100032774.1.28",
          "form": "ㅋㅋㅋ돈벌긴 힘든데 쓰는건 왤케쉽나요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ돈벌긴 힘든데 쓰는건 왤케쉽나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:38"
        },
        {
          "id": "MDRW2100032774.1.29",
          "form": "아 서로 사는지역이 달라서요 ㅎㅎ 저한테로 오기로했어용",
          "original_form": "아 서로 사는지역이 달라서요 ㅎㅎ 저한테로 오기로했어용",
          "speaker_id": "1",
