      "utterance": [
        {
          "id": "MDRW2100004228.1.1",
          "form": "요즘 날씨가 시원해지고 있는거같아요ㅎ",
          "original_form": "요즘 날씨가 시원해지고 있는거같아요ㅎ",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004228.1.2",
          "form": "맞아요 딱 선선하고 좋아요",
          "original_form": "맞아요 딱 선선하고 좋아요",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004228.1.3",
          "form": "이번 여름은 너무 더워서...",
          "original_form": "이번 여름은 너무 더워서...",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004228.1.4",
          "form": "네 근데 여름이 금방 지나가는 기분도 들어요ㅎㅎ",
          "original_form": "네 근데 여름이 금방 지나가는 기분도 들어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004228.1.5",
          "form": "그런거같아요 올해는 그냥 눈깜빡할사이에 휙휙 지나가는 느낌이에요",
          "original_form": "그런거같아요 올해는 그냥 눈깜빡할사이에 휙휙 지나가는 느낌이에요",
          "speaker_id": "2",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004228.1.6",
          "form": "저희지역은 소나기가 그렇게 오더라구요",
          "original_form": "저희지역은 소나기가 그렇게 오더라구요",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004228.1.7",
          "form": "맞아요 동남아 날씨가 그렇다는데 걱정이에요",
          "original_form": "맞아요 동남아 날씨가 그렇다는데 걱정이에요",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004228.1.8",
          "form": "오 그렇군요 거긴 진짜 소나기 엄청 올거같아요",
          "original_form": "오 그렇군요 거긴 진짜 소나기 엄청 올거같아요",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004228.1.9",
          "form": "비 좋아하세요?",
          "original_form": "비 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004228.1.10",
          "form": "비보는건 좋아해요 비오는날 카페에서 멍때리기ㅎㅎ",
          "original_form": "비보는건 좋아해요 비오는날 카페에서 멍때리기ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004228.1.11",
          "form": "오 저도 보는건 좋아해요",
          "original_form": "오 저도 보는건 좋아해요",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004228.1.12",
          "form": "빗소리만 좋아하는것같기도해요",
          "original_form": "빗소리만 좋아하는것같기도해요",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004228.1.13",
          "form": "습한건 또 싫구ㅠㅜ",
          "original_form": "습한건 또 싫구ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004228.1.14",
          "form": "맞아요 에어컨틀고 뽀송한 상태ㅎㅎ",
          "original_form": "맞아요 에어컨틀고 뽀송한 상태ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004228.1.15",
          "form": "에어컨 최고..",
          "original_form": "에어컨 최고..",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004228.1.16",
          "form": "날이 시원해졌어도ㅎㅎ아직까진 필수같아요",
          "original_form": "날이 시원해졌어도ㅎㅎ아직까진 필수같아요",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004228.1.17",
          "form": "맞아요 에어컨 없었으면 죽고싶었을지도..",
          "original_form": "맞아요 에어컨 없었으면 죽고싶었을지도..",
          "speaker_id": "2",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004228.1.18",
          "form": "ㅎㅎㅎ진짜그래요 습도까지..",
          "original_form": "ㅎㅎㅎ진짜그래요 습도까지..",
