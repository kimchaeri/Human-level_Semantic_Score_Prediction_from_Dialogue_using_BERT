      "utterance": [
        {
          "id": "MDRW2100019199.1.1",
          "form": "스마일 캐시인가봐",
          "original_form": "스마일 캐시인가봐",
          "speaker_id": "1",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100019199.1.2",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019199.1.3",
          "form": "사천원 좋네",
          "original_form": "사천원 좋네",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019199.1.4",
          "form": "나름 크다",
          "original_form": "나름 크다",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019199.1.5",
          "form": "ㅇㅇ 한번들어가봐",
          "original_form": "ㅇㅇ 한번들어가봐",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019199.1.6",
          "form": "나 무안양파샀는데 오키로에 4900원이네",
          "original_form": "나 무안양파샀는데 오키로에 4900원이네",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019199.1.7",
          "form": "아쉽\r\n양파 안필요ㅎ",
          "original_form": "아쉽\r\n양파 안필요ㅎ",
          "speaker_id": "2",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019199.1.8",
          "form": "양파가 무안꺼 좋데",
          "original_form": "양파가 무안꺼 좋데",
          "speaker_id": "2",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019199.1.9",
          "form": "근데 크기가 큰거 같아 사이즈가 \"왕\"이네 ㅋㅋ",
          "original_form": "근데 크기가 큰거 같아 사이즈가 \"왕\"이네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019199.1.10",
          "form": "단단하고 달고",
          "original_form": "단단하고 달고",
          "speaker_id": "2",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019199.1.11",
          "form": "ㅇㅇ무안양파 단단하고 좋더라",
          "original_form": "ㅇㅇ무안양파 단단하고 좋더라",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019199.1.12",
          "form": "ㅋㅋ왕 얼마나 큰 왕이려나",
          "original_form": "ㅋㅋ왕 얼마나 큰 왕이려나",
          "speaker_id": "2",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019199.1.13",
          "form": "모루게써 ㅋㅋ  근데 크긴 클것 같당",
          "original_form": "모루게써 ㅋㅋ  근데 크긴 클것 같당",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019199.1.14",
          "form": "대 짜리는 5400원",
          "original_form": "대 짜리는 5400원",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019199.1.15",
          "form": "대가 있네 엄청 큰건가부다",
          "original_form": "대가 있네 엄청 큰건가부다",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100019199.1.16",
          "form": "헐 아까는 700원에 3키로 팔았대",
          "original_form": "헐 아까는 700원에 3키로 팔았대",
          "speaker_id": "1",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019199.1.17",
          "form": "엉? 가격 뭐여?",
          "original_form": "엉? 가격 뭐여?",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019199.1.18",
          "form": "반짝 이벤트한건가?",
          "original_form": "반짝 이벤트한건가?",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019199.1.19",
          "form": "선착순 이벤트였나보더라",
          "original_form": "선착순 이벤트였나보더라",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019199.1.20",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019199.1.21",
          "form": "나 양파 샀다가 대자로 다시 샀다가 엄마한테 전화하니까 그렇게 싼건 아니래서 둘다 취소함",
          "original_form": "나 양파 샀다가 대자로 다시 샀다가 엄마한테 전화하니까 그렇게 싼건 아니래서 둘다 취소함",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019199.1.22",
          "form": "엄마는 정답이셔ㅎ",
          "original_form": "엄마는 정답이셔ㅎ",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019199.1.23",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019199.1.24",
          "form": "엄마가 동네마트서 전에 20키로 짜리 9900원에사서",
          "original_form": "엄마가 동네마트서 전에 20키로 짜리 9900원에사서",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019199.1.25",
          "form": "나도 양파 저렴한가 물었다가 안사도 된다해서 기억하고 있는중",
          "original_form": "나도 양파 저렴한가 물었다가 안사도 된다해서 기억하고 있는중",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019199.1.26",
          "form": "20키로 엄청 잘 사셨다",
          "original_form": "20키로 엄청 잘 사셨다",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019199.1.27",
          "form": "ㅇㅇ 취소요청중인데 취소되겠지? 아님 어쩔수없구",
          "original_form": "ㅇㅇ 취소요청중인데 취소되겠지? 아님 어쩔수없구",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019199.1.28",
          "form": "물가가 참 오락가락이다",
          "original_form": "물가가 참 오락가락이다",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019199.1.29",
          "form": "ㅇㅇ 돼지고기 요즘 엄청 비싸",
          "original_form": "ㅇㅇ 돼지고기 요즘 엄청 비싸",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019199.1.30",
          "form": "바로해서 될거같아 보통 취소하긴하더라",
          "original_form": "바로해서 될거같아 보통 취소하긴하더라",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019199.1.31",
          "form": "삼겹살 너무 비싸서 못  먹을거 같더라",
          "original_form": "삼겹살 너무 비싸서 못  먹을거 같더라",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019199.1.32",
          "form": "요즘 계란도 비싸다가 외국산 들어오고 좀 내렸다더라",
          "original_form": "요즘 계란도 비싸다가 외국산 들어오고 좀 내렸다더라",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019199.1.33",
          "form": "삼겹이 왜 비싼지 원",
          "original_form": "삼겹이 왜 비싼지 원",
          "speaker_id": "2",
          "time": "20210827 16:38"
        }
      ]
