      "utterance": [
        {
          "id": "MDRW2100026261.1.1",
          "form": "요즘 거기도 비가 많이 오나요?",
          "original_form": "요즘 거기도 비가 많이 오나요?",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026261.1.2",
          "form": "내 지금도 오고 있어요~",
          "original_form": "내 지금도 오고 있어요~",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026261.1.3",
          "form": "해가 떴다 비가 왔다 좀 잡을 수 없네요",
          "original_form": "해가 떴다 비가 왔다 좀 잡을 수 없네요",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100026261.1.4",
          "form": "지금도요? 여긴 어제 밤에 엄청나게 오고 오늘은 안왔어요",
          "original_form": "지금도요? 여긴 어제 밤에 엄청나게 오고 오늘은 안왔어요",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100026261.1.5",
          "form": "이런 장마 처음이네요",
          "original_form": "이런 장마 처음이네요",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100026261.1.6",
          "form": "그러니까요~",
          "original_form": "그러니까요~",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026261.1.7",
          "form": "대신 조금 선선해지긴 한 거 같은데,,",
          "original_form": "대신 조금 선선해지긴 한 거 같은데,,",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026261.1.8",
          "form": "저도 얇은 긴팔 가끔 입어요ㅋㅋ",
          "original_form": "저도 얇은 긴팔 가끔 입어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026261.1.9",
          "form": "에어컨 바람 때문에 머리 아팠는데",
          "original_form": "에어컨 바람 때문에 머리 아팠는데",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026261.1.10",
          "form": "자연 바람이 좋긴 하네요",
          "original_form": "자연 바람이 좋긴 하네요",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026261.1.11",
          "form": "맞아요 비와서 좀 그런데 미세먼지는 또 없어서 좋고",
          "original_form": "맞아요 비와서 좀 그런데 미세먼지는 또 없어서 좋고",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026261.1.12",
          "form": "맞아요~ 역시 사람 마음은 다가 똑같네요ㅋㅋㅋ",
          "original_form": "맞아요~ 역시 사람 마음은 다가 똑같네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026261.1.13",
          "form": "요번 여름은 좀  짧았던 거 같죠??",
          "original_form": "요번 여름은 좀  짧았던 거 같죠??",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026261.1.14",
          "form": "짧고 강렬했죠ㅋㅋ 이렇게 더울 순 없었던거 같아요",
          "original_form": "짧고 강렬했죠ㅋㅋ 이렇게 더울 순 없었던거 같아요",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026261.1.15",
          "form": "맞아요 여름은 매해 더 더워지는 거 같아요",
          "original_form": "맞아요 여름은 매해 더 더워지는 거 같아요",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026261.1.16",
          "form": "내년은 더 겁나네요",
          "original_form": "내년은 더 겁나네요",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026261.1.17",
          "form": "그니까요 여름 전에 살을 엄청 빼서 더위를 좀 줄여보려고요",
          "original_form": "그니까요 여름 전에 살을 엄청 빼서 더위를 좀 줄여보려고요",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026261.1.18",
          "form": "맞아요 살찌니까 너무 더워요ㅠ",
          "original_form": "맞아요 살찌니까 너무 더워요ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026261.1.19",
          "form": "이보다 더울 순 없는 것 같아요",
          "original_form": "이보다 더울 순 없는 것 같아요",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026261.1.20",
          "form": "전 어제부터 다이어트 중입니다",
          "original_form": "전 어제부터 다이어트 중입니다",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026261.1.21",
          "form": "언제까지 갈 란지;;",
          "original_form": "언제까지 갈 란지;;",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026261.1.22",
          "form": "그쵸 저도 이번 여름 너무 더웠어요",
          "original_form": "그쵸 저도 이번 여름 너무 더웠어요",
          "speaker_id": "1",
