      "utterance": [
        {
          "id": "MDRW2100030105.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100030105.1.2",
          "form": "아뇨 결혼하셨나요?",
          "original_form": "아뇨 결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100030105.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100030105.1.4",
          "form": "아 맞다 아이가 있으시댔지",
          "original_form": "아 맞다 아이가 있으시댔지",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030105.1.5",
          "form": "혹시 몇살인가요?",
          "original_form": "혹시 몇살인가요?",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030105.1.6",
          "form": "30대여",
          "original_form": "30대여",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030105.1.7",
          "form": "앗....아이 나이 여쭌건데",
          "original_form": "앗....아이 나이 여쭌건데",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030105.1.8",
          "form": "아ㅋㅋ",
          "original_form": "아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100030105.1.9",
          "form": "초딩이요",
          "original_form": "초딩이요",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100030105.1.10",
          "form": "앗 귀엽겠네요 아드님인가요??",
          "original_form": "앗 귀엽겠네요 아드님인가요??",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100030105.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100030105.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ태권도 열심히 다니겠네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ태권도 열심히 다니겠네요",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100030105.1.13",
          "form": "안해여",
          "original_form": "안해여",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100030105.1.14",
          "form": "아 태권도는 요즘 유행이 아닌가요ㅠㅠ 저 때는 그랬어서...",
          "original_form": "아 태권도는 요즘 유행이 아닌가요ㅠㅠ 저 때는 그랬어서...",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100030105.1.15",
          "form": "하긴 그게 몇년 전인데.....",
          "original_form": "하긴 그게 몇년 전인데.....",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100030105.1.16",
          "form": "본인이 안할려고 해여",
          "original_form": "본인이 안할려고 해여",
