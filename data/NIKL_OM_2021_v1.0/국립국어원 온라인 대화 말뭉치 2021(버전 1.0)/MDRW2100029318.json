        },
        {
          "id": "MDRW2100029318.1.2",
          "form": "치킨 이름이 뭐에요",
          "original_form": "치킨 이름이 뭐에요",
          "speaker_id": "1",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029318.1.3",
          "form": "고 추 바사삭",
          "original_form": "고 추 바사삭",
          "speaker_id": "2",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029318.1.4",
          "form": "오 이름 특이하네 ??",
          "original_form": "오 이름 특이하네 ??",
          "speaker_id": "1",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029318.1.5",
          "form": "제가 지은게 아니라서ㅋ",
          "original_form": "제가 지은게 아니라서ㅋ",
          "speaker_id": "2",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029318.1.6",
          "form": "콜라도 줘요?",
          "original_form": "콜라도 줘요?",
          "speaker_id": "1",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100029318.1.7",
          "form": "네 맛나요",
          "original_form": "네 맛나요",
          "speaker_id": "2",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029318.1.8",
          "form": "무슨 브랜드 줘요?",
          "original_form": "무슨 브랜드 줘요?",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029318.1.9",
          "form": "펩시요ㅠㅠ",
          "original_form": "펩시요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029318.1.10",
          "form": "콜라 왜 안주나요",
          "original_form": "콜라 왜 안주나요",
          "speaker_id": "1",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029318.1.11",
          "form": "코카콜라는 왜 안주나요",
          "original_form": "코카콜라는 왜 안주나요",
