      "utterance": [
        {
          "id": "MDRW2100030274.1.1",
          "form": "음..난 연예인은 잘 몰라",
          "original_form": "음..난 연예인은 잘 몰라",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030274.1.2",
          "form": "그냥 나오면 봐 ㅎㅎ",
          "original_form": "그냥 나오면 봐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030274.1.3",
          "form": "나도 잘 모르긴 하지만",
          "original_form": "나도 잘 모르긴 하지만",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030274.1.4",
          "form": "그래도 좀 더 호감가는 연예인은 잇지않아?",
          "original_form": "그래도 좀 더 호감가는 연예인은 잇지않아?",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030274.1.5",
          "form": "너는 어떤 연예인이 호감가는뎅?",
          "original_form": "너는 어떤 연예인이 호감가는뎅?",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030274.1.6",
          "form": "난 트로트가수 임영웅이나 래퍼쪽이 끌리더라구",
          "original_form": "난 트로트가수 임영웅이나 래퍼쪽이 끌리더라구",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030274.1.7",
          "form": "스윙스나 래원 같은..",
          "original_form": "스윙스나 래원 같은..",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.8",
          "form": "트로트에 스며들엇구만 ㅋㅋ",
          "original_form": "트로트에 스며들엇구만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.9",
          "form": "그런가봐 ㅋㅋ 은근히 재밌더라구",
          "original_form": "그런가봐 ㅋㅋ 은근히 재밌더라구",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.10",
          "form": "유튜브에서도 자주 추천영상에 뜨기 시작했어",
          "original_form": "유튜브에서도 자주 추천영상에 뜨기 시작했어",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.11",
          "form": "트로트가..",
          "original_form": "트로트가..",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.12",
          "form": "티비 틀면 요즘 자주 나오지 ㅋㅋ",
          "original_form": "티비 틀면 요즘 자주 나오지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.13",
          "form": "ㅋㅋㅋㅋㅋ맨날 트로트 영상만 보니,,?",
          "original_form": "ㅋㅋㅋㅋㅋ맨날 트로트 영상만 보니,,?",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.14",
          "form": "맞아 트로트가 대세였으니까 ㅋㅋ",
          "original_form": "맞아 트로트가 대세였으니까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.15",
          "form": "아니 그건 아니지만",
          "original_form": "아니 그건 아니지만",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.16",
          "form": "잘 모르겠네 ㅋㅋ",
          "original_form": "잘 모르겠네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.17",
          "form": "재생목랙에 많이 저장돼있어서 그런가봐",
          "original_form": "재생목랙에 많이 저장돼있어서 그런가봐",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.18",
          "form": "트로트는 호불호가 꽤 갈리는것같어 ㅎㅎ",
          "original_form": "트로트는 호불호가 꽤 갈리는것같어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030274.1.19",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030274.1.20",
          "form": "언니는 어때?",
          "original_form": "언니는 어때?",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030274.1.21",
          "form": "그래도 예전보단 어린사람들도",
          "original_form": "그래도 예전보단 어린사람들도",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030274.1.22",
          "form": "많이 듣는것 같애",
          "original_form": "많이 듣는것 같애",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030274.1.23",
          "form": "ㅎㅎ맞아",
          "original_form": "ㅎㅎ맞아",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030274.1.24",
          "form": "원래 트로트는 할무니 할아버지들이 듣는거라는 이미지가있었는데",
          "original_form": "원래 트로트는 할무니 할아버지들이 듣는거라는 이미지가있었는데",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030274.1.25",
          "form": "거의 없어진거같아 ㅎㅎ",
          "original_form": "거의 없어진거같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030274.1.26",
          "form": "마저마저",
          "original_form": "마저마저",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030274.1.27",
          "form": "미스트롯이나 미스터트롯 영향이",
          "original_form": "미스트롯이나 미스터트롯 영향이",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030274.1.28",
          "form": "큰가봐",
