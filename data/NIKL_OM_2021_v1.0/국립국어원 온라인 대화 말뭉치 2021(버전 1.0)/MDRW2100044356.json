      "utterance": [
        {
          "id": "MDRW2100044356.1.1",
          "form": "이 시간에 배가 슬슬고프네요 ㅋㅋㅋㅋㅋ",
          "original_form": "이 시간에 배가 슬슬고프네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044356.1.2",
          "form": "ㅋㅋㅋㅋㅋ저녁은 뭐드셧어요",
          "original_form": "ㅋㅋㅋㅋㅋ저녁은 뭐드셧어요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044356.1.3",
          "form": "비빔밥 먹엇어요 ㅋㅋㅋ",
          "original_form": "비빔밥 먹엇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044356.1.4",
          "form": "오 재료 뭐넣으셧어요 ㅋㅋㅋㅋ",
          "original_form": "오 재료 뭐넣으셧어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044356.1.5",
          "form": "집에잇던 콩나물무침이랑 야채조금이랑 계란후라이요 ㅋㅋㅋㅋㅋ",
          "original_form": "집에잇던 콩나물무침이랑 야채조금이랑 계란후라이요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044356.1.6",
          "form": "핵간단ㅋㅋㅋㅋㅋ",
          "original_form": "핵간단ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044356.1.7",
          "form": "크 역시 기본이 젤 맛잇어여",
          "original_form": "크 역시 기본이 젤 맛잇어여",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044356.1.8",
          "form": "고추장 참기름도 넣으셧어여",
          "original_form": "고추장 참기름도 넣으셧어여",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044356.1.9",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044356.1.10",
          "form": "고추장 참기름 콸콸",
          "original_form": "고추장 참기름 콸콸",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044356.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ국룰이죠!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ국룰이죠!",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044356.1.12",
          "form": "간식은 안드셧어여?",
          "original_form": "간식은 안드셧어여?",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044356.1.13",
          "form": "아까 먹으려다가 안먹엇는데ㅠㅠ",
          "original_form": "아까 먹으려다가 안먹엇는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044356.1.14",
          "form": "먹엇어야햇어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹엇어야햇어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044356.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ뭐 드시려고햇어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ뭐 드시려고햇어요",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044356.1.16",
          "form": "감자칩이욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "감자칩이욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044356.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ어느감자칩이여?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어느감자칩이여?",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044356.1.18",
          "form": "포테토칩이요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "포테토칩이요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044356.1.19",
          "form": "ㅋㅋㅋㅋㅋ아 맛잇는데 안먹은지꽤됫네요",
          "original_form": "ㅋㅋㅋㅋㅋ아 맛잇는데 안먹은지꽤됫네요",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044356.1.20",
          "form": "저는 천하장사 소세지먹엇어욬ㅋㅋㅋ",
          "original_form": "저는 천하장사 소세지먹엇어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044356.1.21",
          "form": "저는 천하장사 소세지를 안먹은지 꽤됏네요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 천하장사 소세지를 안먹은지 꽤됏네요 ㅋㅋㅋㅋㅋ",
