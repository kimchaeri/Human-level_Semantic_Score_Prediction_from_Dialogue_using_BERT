      "utterance": [
        {
          "id": "MDRW2100028991.1.1",
          "form": "뮤지컬 보고싶당",
          "original_form": "뮤지컬 보고싶당",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028991.1.2",
          "form": "어떤 뮤지컬이 보고싶나유?",
          "original_form": "어떤 뮤지컬이 보고싶나유?",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100028991.1.3",
          "form": "당신과 보는 뮤지컬...",
          "original_form": "당신과 보는 뮤지컬...",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100028991.1.4",
          "form": "유투브로 뮤지컬 배운 사람 나야나...^.^...",
          "original_form": "유투브로 뮤지컬 배운 사람 나야나...^.^...",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100028991.1.5",
          "form": "나도 뮤지컬 직관하고 싶어ㅠㅠ 보러갑시다",
          "original_form": "나도 뮤지컬 직관하고 싶어ㅠㅠ 보러갑시다",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100028991.1.6",
          "form": "시카고 본적잇어?",
          "original_form": "시카고 본적잇어?",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100028991.1.7",
          "form": "올말이나 내년초에 대구 공연 한다던데",
          "original_form": "올말이나 내년초에 대구 공연 한다던데",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100028991.1.8",
          "form": "시카고 유투브로 영상만 봤어ㅋㅋ 록시 나오는거 맞나유?",
          "original_form": "시카고 유투브로 영상만 봤어ㅋㅋ 록시 나오는거 맞나유?",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100028991.1.9",
          "form": "맞아유~~~ 그거입니당",
          "original_form": "맞아유~~~ 그거입니당",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100028991.1.10",
          "form": "직관은 안해봣어나두",
          "original_form": "직관은 안해봣어나두",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100028991.1.11",
          "form": "오 좋아좋아!! 시카고 영화를 너무 재밌게 봐가지고ㅋㅋ",
          "original_form": "오 좋아좋아!! 시카고 영화를 너무 재밌게 봐가지고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100028991.1.12",
          "form": "조아앙 근데 가격이 좀 쎄긴 할거야 대극장 뮤지컬이라...",
          "original_form": "조아앙 근데 가격이 좀 쎄긴 할거야 대극장 뮤지컬이라...",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100028991.1.13",
          "form": "10만원대려나? 그래도 한번 사는 인생ㅋㅋ 뮤지컬 한번은 봐야지!!",
          "original_form": "10만원대려나? 그래도 한번 사는 인생ㅋㅋ 뮤지컬 한번은 봐야지!!",
          "speaker_id": "1",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100028991.1.14",
          "form": "마자마자 고정도일거야ㅋㅋㅋㅋ",
          "original_form": "마자마자 고정도일거야ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100028991.1.15",
          "form": "대구티켓팅한다하믄 알려주깡",
          "original_form": "대구티켓팅한다하믄 알려주깡",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100028991.1.16",
          "form": "얍얍 알려주십쇼! 뮤지컬은 티켓팅 경쟁률 어때?",
          "original_form": "얍얍 알려주십쇼! 뮤지컬은 티켓팅 경쟁률 어때?",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100028991.1.17",
          "form": "콘서트랑 연극은 광탈당했었는데...",
          "original_form": "콘서트랑 연극은 광탈당했었는데...",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100028991.1.18",
          "form": "뮤지컬은... 콘서트보단 쉬운데 연극이랑 비슷할라나",
          "original_form": "뮤지컬은... 콘서트보단 쉬운데 연극이랑 비슷할라나",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100028991.1.19",
          "form": "배우 누구 나오는지 따라 다른거같애",
          "original_form": "배우 누구 나오는지 따라 다른거같애",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100028991.1.20",
          "form": "아이돌잇으면 힘들어짐ㅜㅜ",
          "original_form": "아이돌잇으면 힘들어짐ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100028991.1.21",
          "form": "아 알지알지ㅠㅠ다른 배우 공연은 자리있는데 내 배우 공연만 회색인...흑흑",
          "original_form": "아 알지알지ㅠㅠ다른 배우 공연은 자리있는데 내 배우 공연만 회색인...흑흑",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100028991.1.22",
          "form": "아이돌 있으면 자리잡기 엄청 힘들겠다;;",
          "original_form": "아이돌 있으면 자리잡기 엄청 힘들겠다;;",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100028991.1.23",
          "form": "그렇지... 그래서 대구공연 오는 인원 봐야 알수잇을듯",
          "original_form": "그렇지... 그래서 대구공연 오는 인원 봐야 알수잇을듯",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100028991.1.24",
          "form": "이번에 록시역에 티파니 잇엇잖아ㅋㅋㅋ",
          "original_form": "이번에 록시역에 티파니 잇엇잖아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100028991.1.25",
          "form": "아 유투브로 연습영상 봤었어ㅋㅋ티켓팅 빡셌겠구나",
          "original_form": "아 유투브로 연습영상 봤었어ㅋㅋ티켓팅 빡셌겠구나",
          "speaker_id": "1",
          "time": "20210903 00:48"
        },
        {
          "id": "MDRW2100028991.1.26",
          "form": "그랫을거라 예상중!",
          "original_form": "그랫을거라 예상중!",
          "speaker_id": "2",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100028991.1.27",
          "form": "대구도 그렇게 오면 더 힘들거같기도",
          "original_form": "대구도 그렇게 오면 더 힘들거같기도",
          "speaker_id": "2",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100028991.1.28",
          "form": "왜냐면 지방 공연은 길게안하더라구",
          "original_form": "왜냐면 지방 공연은 길게안하더라구",
          "speaker_id": "2",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100028991.1.29",
          "form": "흑흑ㅠㅠ심지어 난 주말이 아니면 못보는데 주말로 잡을려면 더 힘들겠군",
          "original_form": "흑흑ㅠㅠ심지어 난 주말이 아니면 못보는데 주말로 잡을려면 더 힘들겠군",
          "speaker_id": "1",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100028991.1.30",
          "form": "ㅠㅠㅠ그래도 힘내면! 할수있다!",
          "original_form": "ㅠㅠㅠ그래도 힘내면! 할수있다!",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100028991.1.31",
          "form": "앞열 안 노리면 덜 빡세긴 할거얌...",
          "original_form": "앞열 안 노리면 덜 빡세긴 할거얌...",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100028991.1.32",
