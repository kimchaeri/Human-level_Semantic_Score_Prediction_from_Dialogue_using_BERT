        },
        {
          "id": "MDRW2100019053.1.2",
          "form": "약간 버거킹뺘고 거의 다 비슷하게 생각함",
          "original_form": "약간 버거킹뺘고 거의 다 비슷하게 생각함",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019053.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019053.1.4",
          "form": "맘터는",
          "original_form": "맘터는",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019053.1.5",
          "form": "맘터도 좋지",
          "original_form": "맘터도 좋지",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019053.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019053.1.7",
          "form": "나아직버거킹",
          "original_form": "나아직버거킹",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019053.1.8",
          "form": "한번도안먹어봄",
          "original_form": "한번도안먹어봄",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019053.1.9",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019053.1.10",
          "form": "버거킹.. 그닥 맛있는지는 모르겠음",
          "original_form": "버거킹.. 그닥 맛있는지는 모르겠음",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019053.1.11",
          "form": "ㅋㅋㅋㅋ뭐야",
          "original_form": "ㅋㅋㅋㅋ뭐야",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019053.1.12",
          "form": "버거킹빼고비슷하다며",
          "original_form": "버거킹빼고비슷하다며",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019053.1.13",
          "form": "ㅇㅇ 버거킹빼고 다 비슷하게 좋아한다곸ㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 버거킹빼고 다 비슷하게 좋아한다곸ㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019053.1.14",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019053.1.15",
          "form": "그소리였음?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그소리였음?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019053.1.16",
          "form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019053.1.17",
          "form": "아 당근이지",
          "original_form": "아 당근이지",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019053.1.18",
          "form": "싫어란다는 걸로 봄???",
          "original_form": "싫어란다는 걸로 봄???",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019053.1.19",
          "form": "ㅇㅇㅇㅇㅇ당연히",
          "original_form": "ㅇㅇㅇㅇㅇ당연히",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019053.1.20",
          "form": "반대인줄알았음",
          "original_form": "반대인줄알았음",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019053.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019053.1.22",
          "form": "버거킹그정도로맛없음?",
          "original_form": "버거킹그정도로맛없음?",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019053.1.23",
          "form": "약간 무ㅗ랄꺼",
          "original_form": "약간 무ㅗ랄꺼",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019053.1.24",
          "form": "늘 브랜드가 가지고 있는 이미지에 비해 맛이 있진 않았음",
          "original_form": "늘 브랜드가 가지고 있는 이미지에 비해 맛이 있진 않았음",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019053.1.25",
          "form": "그냥 평범하고 평범한데 평범하고 이상하게 평범하게 맛없는 그런 느낌",
          "original_form": "그냥 평범하고 평범한데 평범하고 이상하게 평범하게 맛없는 그런 느낌",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019053.1.26",
          "form": "아 다른사람들이 다 맛있다고해서",
          "original_form": "아 다른사람들이 다 맛있다고해서",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019053.1.27",
          "form": "맛있는줄",
          "original_form": "맛있는줄",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019053.1.28",
          "form": "그리고버거킹약간비싸",
          "original_form": "그리고버거킹약간비싸",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019053.1.29",
          "form": "햄버거치고",
          "original_form": "햄버거치고",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019053.1.30",
          "form": "ㅇㅈ 그래서",
          "original_form": "ㅇㅈ 그래서",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019053.1.31",
          "form": "더 그렇게 좋지 않다고",
          "original_form": "더 그렇게 좋지 않다고",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019053.1.32",
          "form": "느끼게 되나봐",
          "original_form": "느끼게 되나봐",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019053.1.33",
          "form": "아 진짜 맞음",
          "original_form": "아 진짜 맞음",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019053.1.34",
          "form": "요즘 당 줄이려고 크리스탈 라이트 마시는 중인데 맛있긴 한데 달아서 괜찮나 싶음",
          "original_form": "요즘 당 줄이려고 크리스탈 라이트 마시는 중인데 맛있긴 한데 달아서 괜찮나 싶음",
          "speaker_id": "2",
          "time": "20210828 12:56"
        }
      ]
