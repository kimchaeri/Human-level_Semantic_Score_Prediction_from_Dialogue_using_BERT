      "utterance": [
        {
          "id": "MDRW2100042733.1.1",
          "form": "아직까지 더 부룩하구만ㅋㅋㅋㅋ",
          "original_form": "아직까지 더 부룩하구만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100042733.1.2",
          "form": "오늘은 준비를 너무 일찍 해서",
          "original_form": "오늘은 준비를 너무 일찍 해서",
          "speaker_id": "2",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100042733.1.3",
          "form": "시간이 너무 남아 괴롭군ㅋㅋㅋㅋ",
          "original_form": "시간이 너무 남아 괴롭군ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100042733.1.4",
          "form": "벌써 세 번을 싸웠네;;",
          "original_form": "벌써 세 번을 싸웠네;;",
          "speaker_id": "2",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100042733.1.5",
          "form": "나는 지금 준비하고 있다ㅋ",
          "original_form": "나는 지금 준비하고 있다ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100042733.1.6",
          "form": "세 번이나 싸우다니 웃기네",
          "original_form": "세 번이나 싸우다니 웃기네",
          "speaker_id": "1",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100042733.1.7",
          "form": "ㅋㄲㄱㅋㅋ",
          "original_form": "ㅋㄲㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100042733.1.8",
          "form": "아침은 여유 없이 빨리 준비하고 나가는 걸로ㅋㅋㅋ",
          "original_form": "아침은 여유 없이 빨리 준비하고 나가는 걸로ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100042733.1.9",
          "form": "그래 내가 여유 없이 빨리 준비하는 편이잖아",
          "original_form": "그래 내가 여유 없이 빨리 준비하는 편이잖아",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042733.1.10",
          "form": "마음이 급하다",
          "original_form": "마음이 급하다",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042733.1.11",
          "form": "지금도 name3이는",
          "original_form": "지금도 &name3&이는",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042733.1.12",
          "form": "첵스 초코를 먹고 있고",
          "original_form": "첵스 초코를 먹고 있고",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042733.1.13",
          "form": "나는 옷을 입히고 있다",
          "original_form": "나는 옷을 입히고 있다",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042733.1.14",
          "form": "준비 끝",
          "original_form": "준비 끝",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042733.1.15",
          "form": "그래 준비 끝과 동시에 나가야지ㅋㅋㅋㅋ",
          "original_form": "그래 준비 끝과 동시에 나가야지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042733.1.16",
          "form": "그래야 다른 마음이 안 듬ㅋㅋㅋ",
          "original_form": "그래야 다른 마음이 안 듬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042733.1.17",
          "form": "이만 닦고 나가면 된다",
          "original_form": "이만 닦고 나가면 된다",
          "speaker_id": "1",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042733.1.18",
          "form": "ㅋㅋㅋㄱ슾디ㅡ",
          "original_form": "ㅋㅋㅋㄱ슾디ㅡ",
          "speaker_id": "1",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042733.1.19",
          "form": "스피드ㅋ",
          "original_form": "스피드ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042733.1.20",
          "form": "나도 10분 땡하면나가야지ㅋㅋㅋㅋㅋ",
          "original_form": "나도 10분 땡하면나가야지ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042733.1.21",
          "form": "데려다 줄거가??",
          "original_form": "데려다 줄거가??",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042733.1.22",
          "form": "우리는 name4 시간 때문에 10분에 나가야 된다ㅠ",
          "original_form": "우리는 &name4& 시간 때문에 10분에 나가야 된다ㅠ",
          "speaker_id": "2",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042733.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042733.1.24",
          "form": "name3이가 요즘 또 무서운거봤다구",
          "original_form": "&name3&이가 요즘 또 무서운거봤다구",
          "speaker_id": "1",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042733.1.25",
          "form": "데려다달라캐가",
          "original_form": "데려다달라캐가",
          "speaker_id": "1",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042733.1.26",
          "form": "데려다줘야지",
          "original_form": "데려다줘야지",
          "speaker_id": "1",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042733.1.27",
          "form": "아하 그렇구먼",
          "original_form": "아하 그렇구먼",
          "speaker_id": "1",
          "time": "20210907 08:07"
        },
        {
          "id": "MDRW2100042733.1.28",
          "form": "안 그러면 같이 가면 좋은데ㅋㅋㅋㅋ",
          "original_form": "안 그러면 같이 가면 좋은데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:36"
        },
        {
          "id": "MDRW2100042733.1.29",
          "form": "둘 다 클리어ㅋㅋㅋㅋ",
          "original_form": "둘 다 클리어ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:36"
        },
        {
          "id": "MDRW2100042733.1.30",
          "form": "응응 좋다좋다ㅋㅋ",
          "original_form": "응응 좋다좋다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042733.1.31",
          "form": "나는 하나는  또 첵스초코를 먹고있다ㅋ",
          "original_form": "나는 하나는  또 첵스초코를 먹고있다ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042733.1.32",
          "form": "요즘 아침마다 저거 먹네 애들이ㄱㄲ",
          "original_form": "요즘 아침마다 저거 먹네 애들이ㄱㄲ",
          "speaker_id": "1",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042733.1.33",
          "form": "밥싫대ㅋㅋ",
          "original_form": "밥싫대ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042733.1.34",
          "form": "첵스 초코 이마이 사랑한다ㅋㅋㅋㅋ",
          "original_form": "첵스 초코 이마이 사랑한다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042733.1.35",
          "form": "우리는",
          "original_form": "우리는",
          "speaker_id": "2",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042733.1.36",
          "form": "학교 가면 밥맛없다고 먹기 싫다 해서",
          "original_form": "학교 가면 밥맛없다고 먹기 싫다 해서",
          "speaker_id": "2",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042733.1.37",
          "form": "아침에는 꼭 또 밥을 달란다;;",
          "original_form": "아침에는 꼭 또 밥을 달란다;;",
          "speaker_id": "2",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042733.1.38",
          "form": "아 그렇구나ㅋㅋ",
          "original_form": "아 그렇구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:38"
        },
        {
          "id": "MDRW2100042733.1.39",
          "form": "콘프라이트를 줘도",
          "original_form": "콘프라이트를 줘도",
          "speaker_id": "2",
          "time": "20210907 08:38"
        },
        {
          "id": "MDRW2100042733.1.40",
          "form": "그러이 대용량사왔다ㅋㅋ",
          "original_form": "그러이 대용량사왔다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:38"
        },
        {
          "id": "MDRW2100042733.1.41",
