      "utterance": [
        {
          "id": "MDRW2100034028.1.1",
          "form": "name2님 점심 뭐 드실예정이세용?",
          "original_form": "&name2&님 점심 뭐 드실예정이세용?",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100034028.1.2",
          "form": "저는 회사안에서 먹어서 백반밖에 못먹어요ㅠ",
          "original_form": "저는 회사안에서 먹어서 백반밖에 못먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100034028.1.3",
          "form": "가끔 나가서 먹으면 좋은데 그러기도 애매해요ㅠ",
          "original_form": "가끔 나가서 먹으면 좋은데 그러기도 애매해요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100034028.1.4",
          "form": "name1님은 뭐 드실건가요?",
          "original_form": "&name1&님은 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100034028.1.5",
          "form": "저는 죽 먹을 예정이에욧 헉...",
          "original_form": "저는 죽 먹을 예정이에욧 헉...",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100034028.1.6",
          "form": "반찬...반찬이 중요하죠.백반은..",
          "original_form": "반찬...반찬이 중요하죠.백반은..",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100034028.1.7",
          "form": "마자요ㅠ 반찬이 중요하죠ㅠ",
          "original_form": "마자요ㅠ 반찬이 중요하죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100034028.1.8",
          "form": "아프셔서 드시는건가요?ㅠ",
          "original_form": "아프셔서 드시는건가요?ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100034028.1.9",
          "form": "네ㅠㅠ 식도염때문에요ㅠㅠㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네ㅠㅠ 식도염때문에요ㅠㅠㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100034028.1.10",
          "form": "백신맞았다고 아무거나 막 먹었더니 속이 탈났네요!ㅜ",
          "original_form": "백신맞았다고 아무거나 막 먹었더니 속이 탈났네요!ㅜ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100034028.1.11",
          "form": "에구ㅠ 그럼 저녁도 죽인가요?ㅠ",
          "original_form": "에구ㅠ 그럼 저녁도 죽인가요?ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100034028.1.12",
          "form": "식도염 많이 아프실텐데ㅠ",
          "original_form": "식도염 많이 아프실텐데ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100034028.1.13",
          "form": "아뇨...저녁엔...",
          "original_form": "아뇨...저녁엔...",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100034028.1.14",
          "form": "파스타...먹을거예요. 속을 버립니다",
          "original_form": "파스타...먹을거예요. 속을 버립니다",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100034028.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100034028.1.16",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100034028.1.17",
          "form": "아ㅋㅋㅋㅋ속 대신 맛을 선택하셨군요ㅋㅋ\r\n파스타 맛있겠네요! 무슨파스타 드시려구요?",
          "original_form": "아ㅋㅋㅋㅋ속 대신 맛을 선택하셨군요ㅋㅋ\r\n파스타 맛있겠네요! 무슨파스타 드시려구요?",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100034028.1.18",
          "form": "저 빠네요~~~~! 빠네 너무 좋아요!!!!!!!!!!!!",
          "original_form": "저 빠네요~~~~! 빠네 너무 좋아요!!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100034028.1.19",
          "form": "핏짜도 먹을겁니다 히히",
          "original_form": "핏짜도 먹을겁니다 히히",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100034028.1.20",
          "form": "오! 파네 맛있죠ㅠ 오호 피자까지!\r\n너무 맛있겠네요ㅠ",
          "original_form": "오! 파네 맛있죠ㅠ 오호 피자까지!\r\n너무 맛있겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100034028.1.21",
          "form": "빠네면 가서 드시는건가요?",
          "original_form": "빠네면 가서 드시는건가요?",
          "speaker_id": "2",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100034028.1.22",
          "form": "아뇨!! 포장이요!",
          "original_form": "아뇨!! 포장이요!",
          "speaker_id": "1",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100034028.1.23",
          "form": "코로나이후 식당가서 음식을 못 먹겠더라고요ㅠㅠ",
          "original_form": "코로나이후 식당가서 음식을 못 먹겠더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100034028.1.24",
          "form": "마자요 식당가면 불안하더라구요ㅠ\r\n집이 최고죠!",
          "original_form": "마자요 식당가면 불안하더라구요ㅠ\r\n집이 최고죠!",
          "speaker_id": "2",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100034028.1.25",
          "form": "맞아요ㅠㅠㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100034028.1.26",
          "form": "배달하면 용기가 너무 많이 드니까...",
          "original_form": "배달하면 용기가 너무 많이 드니까...",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100034028.1.27",
          "form": "포장으로 하려고요!ㅋㅋㅋㅋ 플라스틱 아웃...!",
          "original_form": "포장으로 하려고요!ㅋㅋㅋㅋ 플라스틱 아웃...!",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100034028.1.28",
          "form": "오! 마자요! 배달용기도 진짜 문제에요\r\n저도 웬만하면 포장하려고 하는데 쉽지 않더라구요ㅠ",
          "original_form": "오! 마자요! 배달용기도 진짜 문제에요\r\n저도 웬만하면 포장하려고 하는데 쉽지 않더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100034028.1.29",
          "form": "하 맞아요ㅠㅠ 가서 기다리는 것도 좀 버겁고요 그쵸!?!?!?!",
          "original_form": "하 맞아요ㅠㅠ 가서 기다리는 것도 좀 버겁고요 그쵸!?!?!?!",
          "speaker_id": "1",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100034028.1.30",
          "form": "마자요ㅠ 용기 사이즈 맞는걸로 챙겨가야되고 기다려야되고ㅠ\r\n그치만 환경 생각도 해야되니까 노력하고있어요",
          "original_form": "마자요ㅠ 용기 사이즈 맞는걸로 챙겨가야되고 기다려야되고ㅠ\r\n그치만 환경 생각도 해야되니까 노력하고있어요",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100034028.1.31",
          "form": "하 맞아요ㅜㅜㅜㅜ",
          "original_form": "하 맞아요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100034028.1.32",
          "form": "사람들이 조금씩 용기내서 했으면 좋겠어요",
          "original_form": "사람들이 조금씩 용기내서 했으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100034028.1.33",
          "form": "좀 불편하긴 하지만..ㅜ",
          "original_form": "좀 불편하긴 하지만..ㅜ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100034028.1.34",
          "form": "마자요ㅠ 이제 진짜 모두가 실천해야할때 인거같아요ㅠ",
          "original_form": "마자요ㅠ 이제 진짜 모두가 실천해야할때 인거같아요ㅠ",
          "speaker_id": "2",
