      "utterance": [
        {
          "id": "MDRW2100007105.1.1",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007105.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007105.1.3",
          "form": "여기는 게임이네요!",
          "original_form": "여기는 게임이네요!",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007105.1.4",
          "form": "게임 좋아하시나요?!",
          "original_form": "게임 좋아하시나요?!",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007105.1.5",
          "form": "네 좋아해요! 저는 포켓몬 좋아해요!",
          "original_form": "네 좋아해요! 저는 포켓몬 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007105.1.6",
          "form": "헉 포켓몬 어떤거요?! 잘은 모르지만... 포켓몬 관련 게임 짱많지않나요?!",
          "original_form": "헉 포켓몬 어떤거요?! 잘은 모르지만... 포켓몬 관련 게임 짱많지않나요?!",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007105.1.7",
          "form": "맞아요! 저는 어릴때 하던 포켓몬 게임 좋아해서 아직도 가지고 있어요ㅋㅋ 펄기아랑 기라티나요!",
          "original_form": "맞아요! 저는 어릴때 하던 포켓몬 게임 좋아해서 아직도 가지고 있어요ㅋㅋ 펄기아랑 기라티나요!",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007105.1.8",
          "form": "헉 대박... 한번 게임 시작하시면 완전 오래 플레이하시나봐요...!!",
          "original_form": "헉 대박... 한번 게임 시작하시면 완전 오래 플레이하시나봐요...!!",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007105.1.9",
          "form": "그런 편이긴 해요ㅋㅋ",
          "original_form": "그런 편이긴 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007105.1.10",
          "form": "포켓몬 게임이면... 그 픽셀형 맞나요?",
          "original_form": "포켓몬 게임이면... 그 픽셀형 맞나요?",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007105.1.11",
          "form": "야생의 치코리타가 나타났다!",
          "original_form": "야생의 치코리타가 나타났다!",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007105.1.12",
          "form": "이... 이런거?!",
          "original_form": "이... 이런거?!",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007105.1.13",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007105.1.14",
          "form": "저는 픽셀게임도 좋아해서요",
          "original_form": "저는 픽셀게임도 좋아해서요",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007105.1.15",
          "form": "헉 역시 이게 맞았군요!",
          "original_form": "헉 역시 이게 맞았군요!",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007105.1.16",
          "form": "그거 맞아요ㅎㅎ",
          "original_form": "그거 맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007105.1.17",
          "form": "오 쯔꾸르게임 이것저것 하시는건가요!?",
          "original_form": "오 쯔꾸르게임 이것저것 하시는건가요!?",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007105.1.18",
          "form": "쯔꾸르 게임 알긴 아는데 저는 닌텐도로 하는 콘솔겜을 더 좋아해서 잘 안하네요",
          "original_form": "쯔꾸르 게임 알긴 아는데 저는 닌텐도로 하는 콘솔겜을 더 좋아해서 잘 안하네요",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007105.1.19",
          "form": "아 콘솔겜 위주로 하시는구나...! 뭐뭐 해보셨던거에요?!",
          "original_form": "아 콘솔겜 위주로 하시는구나...! 뭐뭐 해보셨던거에요?!",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007105.1.20",
          "form": "닌텐도... 가져보고싶었는데... 아직까지도 끝까지 못 가진 사치품이에요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "닌텐도... 가져보고싶었는데... 아직까지도 끝까지 못 가진 사치품이에요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007105.1.21",
          "form": "저도 스위치는 없어요! 예전에 가지고있던걸로 계속 하는거에요ㅎㅎ",
          "original_form": "저도 스위치는 없어요! 예전에 가지고있던걸로 계속 하는거에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007105.1.22",
          "form": "튀동숲도 해욬ㅋ 남들 모동숲할때",
          "original_form": "튀동숲도 해욬ㅋ 남들 모동숲할때",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007105.1.23",
          "form": "엌ㅋㅋㅋㅋㅋㅋ 약간... 과거 게임들 많이 하시네요",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋ 약간... 과거 게임들 많이 하시네요",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007105.1.24",
          "form": "진짜로 한번 잡으신거 끝까지 하시나봐요...!",
          "original_form": "진짜로 한번 잡으신거 끝까지 하시나봐요...!",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007105.1.25",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:57"
        }
      ]
