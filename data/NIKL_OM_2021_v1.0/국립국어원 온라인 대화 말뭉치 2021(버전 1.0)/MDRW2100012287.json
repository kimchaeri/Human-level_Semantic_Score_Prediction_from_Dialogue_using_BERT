        },
        {
          "id": "MDRW2100012287.1.6",
          "form": "뽕",
          "original_form": "뽕",
          "speaker_id": "1",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100012287.1.7",
          "form": "썼다 아녀요?? ㅋㅋㅋㅋ",
          "original_form": "썼다 아녀요?? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100012287.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012287.1.9",
          "form": "쓰고 바로 방으로 들어감 ㅋㅋㅋㄱ",
          "original_form": "쓰고 바로 방으로 들어감 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012287.1.10",
          "form": "자나??",
          "original_form": "자나??",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012287.1.11",
          "form": "빨래 건조기 넣는다고 안자고 기다렸어요ㅠ",
          "original_form": "빨래 건조기 넣는다고 안자고 기다렸어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012287.1.12",
          "form": "안캤음 벌써 꿈나라 ㅋㅋㅋ",
          "original_form": "안캤음 벌써 꿈나라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012287.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ 오늘은 퇴근이 늦구먼",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ 오늘은 퇴근이 늦구먼",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012287.1.14",
          "form": "건조기 돌아가고 있나??",
          "original_form": "건조기 돌아가고 있나??",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012287.1.15",
          "form": "넣고 그냥 자는거가??",
          "original_form": "넣고 그냥 자는거가??",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012287.1.16",
          "form": "건조기에서 말려서 방치 ㅋㅋㅋㅋㅋ",
          "original_form": "건조기에서 말려서 방치 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012287.1.17",
          "form": "낼 또 다꺼내서 다려야쥬ㅠㅠ",
          "original_form": "낼 또 다꺼내서 다려야쥬ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012287.1.18",
          "form": "아 ㅋㅋㅋㅋㅋㅋㄱ 윽시 번거롭고 힘들게사네",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㄱ 윽시 번거롭고 힘들게사네",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012287.1.19",
          "form": "빨래 넣어서 방치한다 캤는데..",
          "original_form": "빨래 넣어서 방치한다 캤는데..",
          "speaker_id": "1",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012287.1.20",
          "form": "다림질이라니....",
          "original_form": "다림질이라니....",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012287.1.21",
          "form": "연이옷하고 내꺼 거의 린넨이라가ㅠ",
          "original_form": "연이옷하고 내꺼 거의 린넨이라가ㅠ",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012287.1.22",
          "form": "다 주름이 주름이ㅠ",
          "original_form": "다 주름이 주름이ㅠ",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012287.1.23",
          "form": "웁스",
          "original_form": "웁스",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012287.1.24",
          "form": "린넨을 건조기에 넣나?? ㅋㅋㅋㅋㅋㄱ",
          "original_form": "린넨을 건조기에 넣나?? ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012287.1.25",
          "form": "그렇네여 사람가리네 ㅋㅋㅋㅋㅋ",
          "original_form": "그렇네여 사람가리네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012287.1.26",
          "form": "그냥 널어놓으면 되자나",
          "original_form": "그냥 널어놓으면 되자나",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012287.1.27",
          "form": "지는 싹다 건조기 ㅋㅋㅋㅋ",
          "original_form": "지는 싹다 건조기 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012287.1.28",
          "form": "웁스 ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "웁스 ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012287.1.29",
          "form": "좀 널어라 린넨은",
          "original_form": "좀 널어라 린넨은",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100012287.1.30",
          "form": "엄청 줄지않나??",
          "original_form": "엄청 줄지않나??",
          "speaker_id": "2",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012287.1.31",
          "form": "이거 언니 가스건조기는 거의 안줄어여 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이거 언니 가스건조기는 거의 안줄어여 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012287.1.32",
          "form": "아 진짜??",
          "original_form": "아 진짜??",
          "speaker_id": "2",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012287.1.33",
          "form": "내가 그래서 못버림 ㅋㅋㅋㅋㅋ",
          "original_form": "내가 그래서 못버림 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012287.1.34",
          "form": "가스는 또 다르구먼",
          "original_form": "가스는 또 다르구먼",
          "speaker_id": "2",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012287.1.35",
