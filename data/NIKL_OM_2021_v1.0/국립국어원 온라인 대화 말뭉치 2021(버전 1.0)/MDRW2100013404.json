      "utterance": [
        {
          "id": "MDRW2100013404.1.1",
          "form": "종교 믿으세요?",
          "original_form": "종교 믿으세요?",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013404.1.2",
          "form": "저는 종교를 믿고싶지만 믿지 않습니다",
          "original_form": "저는 종교를 믿고싶지만 믿지 않습니다",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013404.1.3",
          "form": "모순이 되죠",
          "original_form": "모순이 되죠",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013404.1.4",
          "form": "저는 안믿어요",
          "original_form": "저는 안믿어요",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013404.1.5",
          "form": "태어날때부터 종교가없어서 더 그런가봐요",
          "original_form": "태어날때부터 종교가없어서 더 그런가봐요",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013404.1.6",
          "form": "저는 마음이 약한지라 뭐라도 믿고 싶네요",
          "original_form": "저는 마음이 약한지라 뭐라도 믿고 싶네요",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013404.1.7",
          "form": "언니라도 믿고싶은데 말이죠",
          "original_form": "언니라도 믿고싶은데 말이죠",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013404.1.8",
          "form": "그리고 20살부터 종교에대한 불신이 생겼죠",
          "original_form": "그리고 20살부터 종교에대한 불신이 생겼죠",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013404.1.9",
          "form": "종교 불신이라도 마음의 안식이 와요",
          "original_form": "종교 불신이라도 마음의 안식이 와요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013404.1.10",
          "form": "뭐라도 믿으면 말이죠",
          "original_form": "뭐라도 믿으면 말이죠",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013404.1.11",
          "form": "평온한 심리상태를 갖고 싶네요",
          "original_form": "평온한 심리상태를 갖고 싶네요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013404.1.12",
          "form": "저는 예전엔 3대종교를 믿고싶었으나",
          "original_form": "저는 예전엔 3대종교를 믿고싶었으나",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013404.1.13",
          "form": "최근에 불미스러운일이 있어서",
          "original_form": "최근에 불미스러운일이 있어서",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013404.1.14",
          "form": "아무 종교든 안믿기로했어요",
          "original_form": "아무 종교든 안믿기로했어요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013404.1.15",
          "form": "신을 믿진 않지만 가상의 존재에 기대면 마음이 편해지죠",
          "original_form": "신을 믿진 않지만 가상의 존재에 기대면 마음이 편해지죠",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013404.1.16",
          "form": "그냥 동생을 믿으려고요",
          "original_form": "그냥 동생을 믿으려고요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013404.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013404.1.18",
          "form": "그럼 무엇을 믿으실건가요",
          "original_form": "그럼 무엇을 믿으실건가요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013404.1.19",
          "form": "아무리 언니에게 미움을 받고 욕을 먹어도 예수님은 나를 사랑하신다 생각하면 마음이 편안해질듯요",
          "original_form": "아무리 언니에게 미움을 받고 욕을 먹어도 예수님은 나를 사랑하신다 생각하면 마음이 편안해질듯요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013404.1.20",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013404.1.21",
          "form": "그 언니분 참 나쁘네요",
          "original_form": "그 언니분 참 나쁘네요",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013404.1.22",
          "form": "물론 예수를 믿진 않지만 마음은 편하잖아요?",
          "original_form": "물론 예수를 믿진 않지만 마음은 편하잖아요?",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013404.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013404.1.24",
          "form": "꼭 기독교 믿으시길 바랍니다",
          "original_form": "꼭 기독교 믿으시길 바랍니다",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013404.1.25",
          "form": "공감!공감!공감!",
          "original_form": "공감!공감!공감!",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013404.1.26",
          "form": "저는 평생 안믿으려고요",
          "original_form": "저는 평생 안믿으려고요",
          "speaker_id": "2",
