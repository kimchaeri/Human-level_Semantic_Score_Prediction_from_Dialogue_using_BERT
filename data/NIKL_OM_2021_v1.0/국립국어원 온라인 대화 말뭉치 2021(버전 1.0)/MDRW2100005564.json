      "utterance": [
        {
          "id": "MDRW2100005564.1.1",
          "form": "아이돌이신",
          "original_form": "아이돌이신",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100005564.1.2",
          "form": "name2는",
          "original_form": "&name2&는",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100005564.1.3",
          "form": "공연이나 전시회등등 좋아하나요",
          "original_form": "공연이나 전시회등등 좋아하나요",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100005564.1.4",
          "form": "막 제가 적극적으로 보러다니는 편은 아닌 것 같네여",
          "original_form": "막 제가 적극적으로 보러다니는 편은 아닌 것 같네여",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100005564.1.5",
          "form": "누가 가자하면 따라갑니다",
          "original_form": "누가 가자하면 따라갑니다",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100005564.1.6",
          "form": "역시 공인이라서..",
          "original_form": "역시 공인이라서..",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100005564.1.7",
          "form": "초대 받아야 가네요",
          "original_form": "초대 받아야 가네요",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100005564.1.8",
          "form": "셀럽!!",
          "original_form": "셀럽!!",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100005564.1.9",
          "form": "네? 당황스럽내요",
          "original_form": "네? 당황스럽내요",
          "speaker_id": "2",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100005564.1.10",
          "form": "일단 공연이나 전시회를 보려면",
          "original_form": "일단 공연이나 전시회를 보려면",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100005564.1.11",
          "form": "집밖을 나가야하지 않습니까",
          "original_form": "집밖을 나가야하지 않습니까",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100005564.1.12",
          "form": "그 과정이 저에겐..",
          "original_form": "그 과정이 저에겐..",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100005564.1.13",
          "form": "공감됩니다..",
          "original_form": "공감됩니다..",
          "speaker_id": "1",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100005564.1.14",
          "form": "근데 막상 보면 재밌긴하지",
          "original_form": "근데 막상 보면 재밌긴하지",
          "speaker_id": "1",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100005564.1.15",
          "form": "근데 또 폰이나 티비로 보는 것과",
          "original_form": "근데 또 폰이나 티비로 보는 것과",
          "speaker_id": "2",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100005564.1.16",
          "form": "실제로 보는건다르죠",
          "original_form": "실제로 보는건다르죠",
          "speaker_id": "2",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100005564.1.17",
          "form": "맞ㅇ아요",
          "original_form": "맞ㅇ아요",
          "speaker_id": "2",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100005564.1.18",
          "form": "코로나 종식하고",
          "original_form": "코로나 종식하고",
          "speaker_id": "1",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100005564.1.19",
          "form": "돈이 있다면",
          "original_form": "돈이 있다면",
          "speaker_id": "1",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100005564.1.20",
          "form": "공연이던 전시회던",
          "original_form": "공연이던 전시회던",
          "speaker_id": "1",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100005564.1.21",
          "form": "가고 싶다",
          "original_form": "가고 싶다",
          "speaker_id": "1",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100005564.1.22",
          "form": "공연... 재밌을 거 같네요",
          "original_form": "공연... 재밌을 거 같네요",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100005564.1.23",
          "form": "근데 사람이 많을 것 같아서",
          "original_form": "근데 사람이 많을 것 같아서",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100005564.1.24",
          "form": "그건 또 별로고",
          "original_form": "그건 또 별로고",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100005564.1.25",
          "form": "사람은 많아도",
          "original_form": "사람은 많아도",
          "speaker_id": "1",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100005564.1.26",
          "form": "참여유도형 공연이 아니라면",
          "original_form": "참여유도형 공연이 아니라면",
          "speaker_id": "1",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100005564.1.27",
          "form": "재밌을 듯",
          "original_form": "재밌을 듯",
          "speaker_id": "1",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100005564.1.28",
          "form": "노래 듣는 건 좋을 듯",
          "original_form": "노래 듣는 건 좋을 듯",
          "speaker_id": "2",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100005564.1.29",
          "form": "아 근데 그건 신기함",
          "original_form": "아 근데 그건 신기함",
          "speaker_id": "2",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100005564.1.30",
          "form": "객석에서 한 명 뽑아서 무대에 올라오라고 하는거..",
          "original_form": "객석에서 한 명 뽑아서 무대에 올라오라고 하는거..",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100005564.1.31",
          "form": "그게 참여유도형인가",
          "original_form": "그게 참여유도형인가",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100005564.1.32",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100005564.1.33",
          "form": "거절하고 싶지만",
          "original_form": "거절하고 싶지만",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100005564.1.34",
          "form": "거절하면",
          "original_form": "거절하면",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100005564.1.35",
          "form": "분위기 싸해지는..",
          "original_form": "분위기 싸해지는..",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100005564.1.36",
          "form": "근데 좋아하는 가수가..",
          "original_form": "근데 좋아하는 가수가..",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100005564.1.37",
          "form": "올라오라고 하면..",
          "original_form": "올라오라고 하면..",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100005564.1.38",
          "form": "난 간다",
          "original_form": "난 간다",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100005564.1.39",
          "form": "그건 덥석가지",
          "original_form": "그건 덥석가지",
          "speaker_id": "1",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100005564.1.40",
          "form": "내가 알던 가수들은",
          "original_form": "내가 알던 가수들은",
          "speaker_id": "2",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100005564.1.41",
          "form": "객석에서 한명 뽑아서 무대 위에 의자에 앉히고",
          "original_form": "객석에서 한명 뽑아서 무대 위에 의자에 앉히고",
          "speaker_id": "2",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100005564.1.42",
          "form": "멤버들 다 그 분 옆에 서서",
          "original_form": "멤버들 다 그 분 옆에 서서",
          "speaker_id": "2",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100005564.1.43",
          "form": "그건 진짜...와..극락이다",
          "original_form": "그건 진짜...와..극락이다",
          "speaker_id": "1",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100005564.1.44",
          "form": "그 분 이름 넣어서 노래불러주더라..",
          "original_form": "그 분 이름 넣어서 노래불러주더라..",
          "speaker_id": "2",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100005564.1.45",
          "form": "죽어도 좋을거 같애",
          "original_form": "죽어도 좋을거 같애",
          "speaker_id": "1",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100005564.1.46",
          "form": "그니까..",
          "original_form": "그니까..",
          "speaker_id": "2",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100005564.1.47",
          "form": "진짜 몸에 굿즈로 도배하고 가서..",
          "original_form": "진짜 몸에 굿즈로 도배하고 가서..",
          "speaker_id": "2",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100005564.1.48",
          "form": "객석에서 춤춘다",
          "original_form": "객석에서 춤춘다",
          "speaker_id": "2",
          "time": "20210823 00:22"
        }
      ]
    }
  ]
