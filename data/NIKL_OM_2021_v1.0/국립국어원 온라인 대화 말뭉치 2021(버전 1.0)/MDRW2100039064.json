      "utterance": [
        {
          "id": "MDRW2100039064.1.1",
          "form": "모아이군요 뜻이있나요",
          "original_form": "모아이군요 뜻이있나요",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100039064.1.2",
          "form": "모멘트 오브 얼웨이즈니스에요 함께하는 모든 순간ㅎㅎ",
          "original_form": "모멘트 오브 얼웨이즈니스에요 함께하는 모든 순간ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100039064.1.3",
          "form": "아로하는 뜻이 뭔가요?",
          "original_form": "아로하는 뜻이 뭔가요?",
          "speaker_id": "2",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100039064.1.4",
          "form": "저는 잘 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "저는 잘 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:49"
        },
        {
          "id": "MDRW2100039064.1.5",
          "form": "아스트로의 하나뿐인 팬이래요!!!",
          "original_form": "아스트로의 하나뿐인 팬이래요!!!",
          "speaker_id": "1",
          "time": "20210903 08:50"
        },
        {
          "id": "MDRW2100039064.1.6",
          "form": "오호ㅎㅎ 의미 좋네요",
          "original_form": "오호ㅎㅎ 의미 좋네요",
          "speaker_id": "2",
          "time": "20210903 08:50"
        },
        {
          "id": "MDRW2100039064.1.7",
          "form": "요즘 애프터미드나잇 노래 유튜브로 자주봐요",
          "original_form": "요즘 애프터미드나잇 노래 유튜브로 자주봐요",
          "speaker_id": "1",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100039064.1.8",
          "form": "그거 저번에 나온 신곡 맞죠?",
          "original_form": "그거 저번에 나온 신곡 맞죠?",
          "speaker_id": "2",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100039064.1.9",
          "form": "맞아요 이번신곡이에여",
          "original_form": "맞아요 이번신곡이에여",
          "speaker_id": "1",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100039064.1.10",
          "form": "1등도했어요 히히",
          "original_form": "1등도했어요 히히",
          "speaker_id": "1",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100039064.1.11",
          "form": "공중파 1등 처음으로 했다고 알고 있는데 맞나요?",
          "original_form": "공중파 1등 처음으로 했다고 알고 있는데 맞나요?",
          "speaker_id": "2",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100039064.1.12",
          "form": "맞아요맞아요 데뷔 2000일 만에ㅠㅠ",
          "original_form": "맞아요맞아요 데뷔 2000일 만에ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100039064.1.13",
          "form": "와 잘아시네요",
          "original_form": "와 잘아시네요",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100039064.1.14",
          "form": "아스트로 노래 자주 들었어요",
          "original_form": "아스트로 노래 자주 들었어요",
          "speaker_id": "2",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100039064.1.15",
          "form": "all night 엄청 좋아했구요ㅎㅎ",
          "original_form": "all night 엄청 좋아했구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100039064.1.16",
          "form": "우와 올나잇도 너무좋죠",
          "original_form": "우와 올나잇도 너무좋죠",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039064.1.17",
          "form": "투바투 노래도 추천해쥬세요",
          "original_form": "투바투 노래도 추천해쥬세요",
