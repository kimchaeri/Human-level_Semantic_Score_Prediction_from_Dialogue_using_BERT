      "utterance": [
        {
          "id": "MDRW2100040341.1.1",
          "form": "여행은 자주 다니세요",
          "original_form": "여행은 자주 다니세요",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100040341.1.2",
          "form": "자주 갔었죠ㅜㅜ 한때는",
          "original_form": "자주 갔었죠ㅜㅜ 한때는",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040341.1.3",
          "form": "최근에 다녀온 곳이 있나요?",
          "original_form": "최근에 다녀온 곳이 있나요?",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040341.1.4",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040341.1.5",
          "form": "코로나 때문에는 못 갔었는데",
          "original_form": "코로나 때문에는 못 갔었는데",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040341.1.6",
          "form": "끝나면 전라도로 가고 싶어요",
          "original_form": "끝나면 전라도로 가고 싶어요",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040341.1.7",
          "form": "역시나 맛있는 음식을 먹으려구요",
          "original_form": "역시나 맛있는 음식을 먹으려구요",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040341.1.8",
          "form": "ㅋㅋㅋ진리죠",
          "original_form": "ㅋㅋㅋ진리죠",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040341.1.9",
          "form": "저 전라도는 거의 못가봤어요",
          "original_form": "저 전라도는 거의 못가봤어요",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040341.1.10",
          "form": "무시무시한 밑반찬 등 소문은 들었는데!!",
          "original_form": "무시무시한 밑반찬 등 소문은 들었는데!!",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040341.1.11",
          "form": "어우 대박이에요",
          "original_form": "어우 대박이에요",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100040341.1.12",
          "form": "아무곳이나 들어가도 맛집이에요ㅋㅋ",
          "original_form": "아무곳이나 들어가도 맛집이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100040341.1.13",
          "form": "아니ㅋㅋ 그정도입니꽈!!",
          "original_form": "아니ㅋㅋ 그정도입니꽈!!",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100040341.1.14",
          "form": "저 여수나 부산만 줄창 갔는데",
          "original_form": "저 여수나 부산만 줄창 갔는데",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100040341.1.15",
          "form": "네! 화순에 한번 갔는데",
          "original_form": "네! 화순에 한번 갔는데",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100040341.1.16",
          "form": "진짜..",
          "original_form": "진짜..",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040341.1.17",
          "form": "단무지도 맛있었어요ㅋㄱㅋ",
          "original_form": "단무지도 맛있었어요ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040341.1.18",
          "form": "아니ㅋ  단무지가요ㅋㅋ",
          "original_form": "아니ㅋ  단무지가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040341.1.19",
          "form": "다 양념을 따로 하시더라구요",
          "original_form": "다 양념을 따로 하시더라구요",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040341.1.20",
          "form": "전라도 정말 무시무시한 지역이네요ㄷㄷㄷ",
          "original_form": "전라도 정말 무시무시한 지역이네요ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040341.1.21",
          "form": "ㅋㅋㅋㅋㅋ기존 단무지에 새콤하게",
          "original_form": "ㅋㅋㅋㅋㅋ기존 단무지에 새콤하게",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100040341.1.22",
          "form": "전 제주한달살이보다는 전라도 한달살이 하고싶어요ㅋㅋ",
          "original_form": "전 제주한달살이보다는 전라도 한달살이 하고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100040341.1.23",
          "form": "전라도ㅜ",
          "original_form": "전라도ㅜ",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100040341.1.24",
          "form": "실비집도 가보셨나요!",
          "original_form": "실비집도 가보셨나요!",
          "speaker_id": "2",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100040341.1.25",
          "form": "짜그리탕이라고 있드라구요",
          "original_form": "짜그리탕이라고 있드라구요",
          "speaker_id": "1",
          "time": "20210904 16:53"
        },
        {
          "id": "MDRW2100040341.1.26",
          "form": "아 알아요",
          "original_form": "아 알아요",
          "speaker_id": "2",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040341.1.27",
          "form": "그 이모한테 고백할 뻔 했어요",
          "original_form": "그 이모한테 고백할 뻔 했어요",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040341.1.28",
          "form": "결혼하자고ㅋㅋㅋㅋㅋ",
          "original_form": "결혼하자고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:54"
        },
        {
          "id": "MDRW2100040341.1.29",
          "form": "그 김치찌개보다 내용물 실한거",
          "original_form": "그 김치찌개보다 내용물 실한거",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040341.1.30",
          "form": "아니ㄷㄷ",
          "original_form": "아니ㄷㄷ",
          "speaker_id": "2",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040341.1.31",
          "form": "맞아요!ㅋㅋㅋㅋ",
          "original_form": "맞아요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:55"
        },
        {
          "id": "MDRW2100040341.1.32",
          "form": "제가 먹은 짜글이는 그냥 김치찌개같던데ㅜ",
          "original_form": "제가 먹은 짜글이는 그냥 김치찌개같던데ㅜ",
          "speaker_id": "2",
          "time": "20210904 16:55"
        }
      ]
