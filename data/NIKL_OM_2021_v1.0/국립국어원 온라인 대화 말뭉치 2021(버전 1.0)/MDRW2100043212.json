      "utterance": [
        {
          "id": "MDRW2100043212.1.1",
          "form": "파라솔살때 빵 도대체 얼마나 사야되노ㅋㅋ",
          "original_form": "파라솔살때 빵 도대체 얼마나 사야되노ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043212.1.2",
          "form": "냉동도 팔아",
          "original_form": "냉동도 팔아",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043212.1.3",
          "form": "스파게티 같은거",
          "original_form": "스파게티 같은거",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043212.1.4",
          "form": "그런거 사 ㅋㅋ",
          "original_form": "그런거 사 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043212.1.5",
          "form": "스파게티도 판다고? 파바에?",
          "original_form": "스파게티도 판다고? 파바에?",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043212.1.6",
          "form": "응 있어 음식 냉동 ㅎ",
          "original_form": "응 있어 음식 냉동 ㅎ",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043212.1.7",
          "form": "새로나옴ㅋ",
          "original_form": "새로나옴ㅋ",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043212.1.8",
          "form": "오ㅋㅋ첨알았네",
          "original_form": "오ㅋㅋ첨알았네",
          "speaker_id": "2",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043212.1.9",
          "form": "어머님은 잘사시겠나ㅋㅋ",
          "original_form": "어머님은 잘사시겠나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043212.1.10",
          "form": "니가 미리 골라서 보여줌 안대나???",
          "original_form": "니가 미리 골라서 보여줌 안대나???",
          "speaker_id": "1",
          "time": "20210907 16:17"
        },
        {
          "id": "MDRW2100043212.1.11",
          "form": "난 왜 소고기 업찌",
          "original_form": "난 왜 소고기 업찌",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043212.1.12",
          "form": "야 address 아직도 전화 안옴",
          "original_form": "야 &address& 아직도 전화 안옴",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043212.1.13",
          "form": "짜증.낼 아니가?",
          "original_form": "짜증.낼 아니가?",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100043212.1.14",
          "form": "내랑은 얘기안하는데 신랑이 알아서하겠지",
          "original_form": "내랑은 얘기안하는데 신랑이 알아서하겠지",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043212.1.15",
          "form": "아 ㅋㅋㅋㅋ 니가 그럼 신랑한테 캐 ㅎㅎ",
          "original_form": "아 ㅋㅋㅋㅋ 니가 그럼 신랑한테 캐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043212.1.16",
          "form": "이거이거 사라고 그 냉동 음식들 맛있다고 본것 같음",
          "original_form": "이거이거 사라고 그 냉동 음식들 맛있다고 본것 같음",
          "speaker_id": "1",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043212.1.17",
          "form": "빵은 어머님이 드시겠지모ㅋ",
          "original_form": "빵은 어머님이 드시겠지모ㅋ",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043212.1.18",
          "form": "우린파라솔만있음되 ㅋㅋ",
          "original_form": "우린파라솔만있음되 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:33"
        },
        {
          "id": "MDRW2100043212.1.19",
          "form": "아 그러면 뭐 알아서 하시겠지!",
          "original_form": "아 그러면 뭐 알아서 하시겠지!",
          "speaker_id": "1",
          "time": "20210907 16:36"
        },
        {
          "id": "MDRW2100043212.1.20",
          "form": "그려 ㅋㅋ\r\naddress점에는 내일부터 결제가능하다더만..\r\n여긴 오늘 부터 와도 된다캤다대",
          "original_form": "그려 ㅋㅋ\r\n&address&점에는 내일부터 결제가능하다더만..\r\n여긴 오늘 부터 와도 된다캤다대",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043212.1.21",
          "form": "address점 전화왔더낰",
          "original_form": "&address&점 전화왔더낰",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043212.1.22",
          "form": "ㅠㅠ 여튼클날뻔",
          "original_form": "ㅠㅠ 여튼클날뻔",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043212.1.23",
          "form": "내가 접때 전화하고 전화온거 말고는 다른건 없었어",
          "original_form": "내가 접때 전화하고 전화온거 말고는 다른건 없었어",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043212.1.24",
          "form": "아 어제 전화준다 안캤나??",
          "original_form": "아 어제 전화준다 안캤나??",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043212.1.25",
          "form": "전화 왔었어 ㅋㅋ\r\n목요일에 간다고 캤찌 ㅋㅋ",
          "original_form": "전화 왔었어 ㅋㅋ\r\n목요일에 간다고 캤찌 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043212.1.26",
          "form": "뭐사지 빵 ㅋㅋㅋㅋ",
          "original_form": "뭐사지 빵 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100043212.1.27",
          "form": "어머님 대박 ㅋㅋㅋㅋ",
          "original_form": "어머님 대박 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100043212.1.28",
          "form": "밤에는 빵없다고 오전에 가서 빵만 가져왔대 ㅋㅋ\r\n저녁에 한꺼번에 계산하기로 하고 ㅋㅋ",
          "original_form": "밤에는 빵없다고 오전에 가서 빵만 가져왔대 ㅋㅋ\r\n저녁에 한꺼번에 계산하기로 하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100043212.1.29",
          "form": "빵도 가져가라고 하셨단다 ㅋㅋㅋ",
          "original_form": "빵도 가져가라고 하셨단다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
