      "utterance": [
        {
          "id": "MDRW2100012427.1.1",
          "form": "비가 많이 왔네요",
          "original_form": "비가 많이 왔네요",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012427.1.2",
          "form": "네 어제 비가 많이 왔어요",
          "original_form": "네 어제 비가 많이 왔어요",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012427.1.3",
          "form": "가을장마라네요ㅜㅜ",
          "original_form": "가을장마라네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100012427.1.4",
          "form": "그러게요.  오늘도 날씨가 좋지 않네요ㅠ",
          "original_form": "그러게요.  오늘도 날씨가 좋지 않네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 08:30"
        },
        {
          "id": "MDRW2100012427.1.5",
          "form": "매일 비가와요ㅜㅜ",
          "original_form": "매일 비가와요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 10:51"
        },
        {
          "id": "MDRW2100012427.1.6",
          "form": "계속 비가 오니 우울해 지네요",
          "original_form": "계속 비가 오니 우울해 지네요",
          "speaker_id": "2",
          "time": "20210825 11:15"
        },
        {
          "id": "MDRW2100012427.1.7",
          "form": "비오는거 싫어하시는군여",
          "original_form": "비오는거 싫어하시는군여",
          "speaker_id": "1",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100012427.1.8",
          "form": "저도...",
          "original_form": "저도...",
          "speaker_id": "1",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100012427.1.9",
          "form": "집에 있을때는 비 오는게 좋아요",
          "original_form": "집에 있을때는 비 오는게 좋아요",
          "speaker_id": "2",
          "time": "20210825 12:34"
        },
        {
          "id": "MDRW2100012427.1.10",
          "form": "낼은 남부지방은 맑대요",
          "original_form": "낼은 남부지방은 맑대요",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100012427.1.11",
          "form": "아까 비 엄청 왔어요ㅠ",
          "original_form": "아까 비 엄청 왔어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100012427.1.12",
          "form": "설마 부산?",
          "original_form": "설마 부산?",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100012427.1.13",
          "form": "부산에 비 많이 왔다던데",
          "original_form": "부산에 비 많이 왔다던데",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100012427.1.14",
          "form": "네 부산 살아요",
          "original_form": "네 부산 살아요",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100012427.1.15",
          "form": "바로 맞췄네요",
          "original_form": "바로 맞췄네요",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100012427.1.16",
          "form": "내일은 맑을거에요",
          "original_form": "내일은 맑을거에요",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100012427.1.17",
          "form": "광합성좀 하셔요",
          "original_form": "광합성좀 하셔요",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100012427.1.18",
          "form": "그러게요. 낼은 비 안온다고 예보에  있긴 하네요ㅋ",
          "original_form": "그러게요. 낼은 비 안온다고 예보에  있긴 하네요ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        }
      ]
