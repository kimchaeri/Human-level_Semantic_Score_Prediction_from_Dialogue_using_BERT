      "utterance": [
        {
          "id": "MDRW2100034947.1.1",
          "form": "코로나 이후에  얼굴 피부 괜찮으세요?",
          "original_form": "코로나 이후에  얼굴 피부 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100034947.1.2",
          "form": "마스크 때문에ㅜ 트러블이 잦아요",
          "original_form": "마스크 때문에ㅜ 트러블이 잦아요",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100034947.1.3",
          "form": "name1님은 어떠세요",
          "original_form": "&name1&님은 어떠세요",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100034947.1.4",
          "form": "저도요  피부 어케 관리하세요",
          "original_form": "저도요  피부 어케 관리하세요",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100034947.1.5",
          "form": "그냥;; 피부진정 마스크팩 간혹 해요",
          "original_form": "그냥;; 피부진정 마스크팩 간혹 해요",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100034947.1.6",
          "form": "님은 비법이 있나요",
          "original_form": "님은 비법이 있나요",
          "speaker_id": "2",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034947.1.7",
          "form": "저도 마스크팩 하는데 그때 뿐인거 같아요",
          "original_form": "저도 마스크팩 하는데 그때 뿐인거 같아요",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034947.1.8",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034947.1.9",
          "form": "이놈의 코로나가 피부 다 잡네요",
          "original_form": "이놈의 코로나가 피부 다 잡네요",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034947.1.10",
          "form": "화장안해도 되니 그건 좋으나 차라리 화장을 하고싶네요",
          "original_form": "화장안해도 되니 그건 좋으나 차라리 화장을 하고싶네요",
          "speaker_id": "1",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100034947.1.11",
          "form": "코로나의 유일한 순기능이죠ㅜ",
          "original_form": "코로나의 유일한 순기능이죠ㅜ",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100034947.1.12",
          "form": "건강관리는 따로 하시나요",
          "original_form": "건강관리는 따로 하시나요",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100034947.1.13",
          "form": "예전엔 그래도 주3화 운동했는데 지금은 더 엉망인거 같아요",
          "original_form": "예전엔 그래도 주3화 운동했는데 지금은 더 엉망인거 같아요",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100034947.1.14",
          "form": "아 헬스를 다니셨나요",
          "original_form": "아 헬스를 다니셨나요",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100034947.1.15",
          "form": "아뇨아뇨 걷기요",
          "original_form": "아뇨아뇨 걷기요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034947.1.16",
          "form": "체질에 맞아서요 ㅎㅎ",
          "original_form": "체질에 맞아서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034947.1.17",
          "form": "아ㅎㅎ 걷기가 제일 좋은 운동이라죠ㅎ",
          "original_form": "아ㅎㅎ 걷기가 제일 좋은 운동이라죠ㅎ",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034947.1.18",
          "form": "하루 얼마 걸으세요?",
          "original_form": "하루 얼마 걸으세요?",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034947.1.19",
          "form": "1만보 걸었어요",
          "original_form": "1만보 걸었어요",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034947.1.20",
          "form": "하루 1만보라니ㄷㄷㄷ 대단하세요",
          "original_form": "하루 1만보라니ㄷㄷㄷ 대단하세요",
