        },
        {
          "id": "MDRW2100030600.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210904 10:02"
        },
        {
          "id": "MDRW2100030600.1.3",
          "form": "책 좋아하시나요?",
          "original_form": "책 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100030600.1.4",
          "form": "네 저는 좋아하는데 name2님은 좋아하시나요?",
          "original_form": "네 저는 좋아하는데 &name2&님은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100030600.1.5",
          "form": "좋아하신다면 어떤 책을 좋아하세요?",
          "original_form": "좋아하신다면 어떤 책을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030600.1.6",
          "form": "저는 책을 별로 안좋아하는데 요즘 취미 책을 찾아보고있어요!",
          "original_form": "저는 책을 별로 안좋아하는데 요즘 취미 책을 찾아보고있어요!",
          "speaker_id": "2",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030600.1.7",
          "form": "사진이나 요리 등등이요!",
          "original_form": "사진이나 요리 등등이요!",
          "speaker_id": "2",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030600.1.8",
          "form": "아하! 사진찍는거 좋아하시나봐요!",
          "original_form": "아하! 사진찍는거 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030600.1.9",
          "form": "잘 찍고 싶어요ㅜㅜ 블로그 하고싶거든요!!",
          "original_form": "잘 찍고 싶어요ㅜㅜ 블로그 하고싶거든요!!",
          "speaker_id": "2",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.10",
          "form": "사진찍는 책을 봐도 잘 찍는 법을 모르겠더라구요 ㅠ",
          "original_form": "사진찍는 책을 봐도 잘 찍는 법을 모르겠더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.11",
          "form": "맞아요! 일단은 감인거 같아요 ㅋㅋㅋ",
          "original_form": "맞아요! 일단은 감인거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.12",
          "form": "앗 저도 블로그 하고 있어요",
          "original_form": "앗 저도 블로그 하고 있어요",
          "speaker_id": "1",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.13",
          "form": "그리고 장비 ㅋㅋㅋ 카메라가 좋아야해요",
          "original_form": "그리고 장비 ㅋㅋㅋ 카메라가 좋아야해요",
          "speaker_id": "2",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.14",
          "form": "우엉??",
          "original_form": "우엉??",
          "speaker_id": "2",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.15",
          "form": "진짜요??",
          "original_form": "진짜요??",
          "speaker_id": "2",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.16",
          "form": "책 리뷰 관련해서 하고 있어서",
          "original_form": "책 리뷰 관련해서 하고 있어서",
          "speaker_id": "1",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.17",
          "form": "사진을 찍는 기술이 특별히 필요하지 않아서",
          "original_form": "사진을 찍는 기술이 특별히 필요하지 않아서",
          "speaker_id": "1",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.18",
          "form": "다행이다 하고 있지만요 ㅋㅋㅋ",
          "original_form": "다행이다 하고 있지만요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030600.1.19",
          "form": "오 책을 정말 좋아하시는균요...!!",
          "original_form": "오 책을 정말 좋아하시는균요...!!",
          "speaker_id": "2",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030600.1.20",
          "form": "카메라 장비 진짜 ㅠ",
          "original_form": "카메라 장비 진짜 ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030600.1.21",
          "form": "어떤 책을 읽고계세요??",
          "original_form": "어떤 책을 읽고계세요??",
          "speaker_id": "2",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030600.1.22",
          "form": "취미로 책읽는걸 즐겨 보고 있어요",
          "original_form": "취미로 책읽는걸 즐겨 보고 있어요",
          "speaker_id": "1",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030600.1.23",
          "form": "장르에 구애받지 않고 워낙 광활하게 봐서",
          "original_form": "장르에 구애받지 않고 워낙 광활하게 봐서",
          "speaker_id": "1",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030600.1.24",
          "form": "최근에는 어린이용 책에 손을 대서",
          "original_form": "최근에는 어린이용 책에 손을 대서",
          "speaker_id": "1",
          "time": "20210904 10:08"
        },
        {
          "id": "MDRW2100030600.1.25",
          "form": "드래곤길들이기 원서를 읽어보고 있어요",
          "original_form": "드래곤길들이기 원서를 읽어보고 있어요",
          "speaker_id": "1",
          "time": "20210904 10:08"
        },
        {
          "id": "MDRW2100030600.1.26",
          "form": "아 그게 외국거였군요??!!",
          "original_form": "아 그게 외국거였군요??!!",
          "speaker_id": "2",
          "time": "20210904 10:08"
        },
        {
          "id": "MDRW2100030600.1.27",
          "form": "저는 해리포터 원서 시도했다가 안읽은지 꽤 됐어요 ㅋㅋㅋㅋ",
          "original_form": "저는 해리포터 원서 시도했다가 안읽은지 꽤 됐어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:08"
        },
        {
          "id": "MDRW2100030600.1.28",
          "form": "아 ㅋㅋㅋ 해리포터 원서는 저도 읽다가 그냥 한글로 읽기로 마음 바꿨어요",
          "original_form": "아 ㅋㅋㅋ 해리포터 원서는 저도 읽다가 그냥 한글로 읽기로 마음 바꿨어요",
          "speaker_id": "1",
          "time": "20210904 10:09"
        },
        {
          "id": "MDRW2100030600.1.29",
          "form": "한글 책이 최고죠",
          "original_form": "한글 책이 최고죠",
          "speaker_id": "1",
          "time": "20210904 10:09"
        }
      ]
