        },
        {
          "id": "MDRW2100017352.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017352.1.4",
          "form": "가본 곳 중에 어디 좋으셨어요",
