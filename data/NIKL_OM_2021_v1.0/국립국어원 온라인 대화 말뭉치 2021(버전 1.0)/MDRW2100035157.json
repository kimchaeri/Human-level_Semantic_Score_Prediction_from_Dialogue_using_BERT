      "utterance": [
        {
          "id": "MDRW2100035157.1.1",
          "form": "안녕하세용 아침 드셨어용?",
          "original_form": "안녕하세용 아침 드셨어용?",
          "speaker_id": "1",
          "time": "20210829 08:40"
        },
        {
          "id": "MDRW2100035157.1.2",
          "form": "어니오 아직 안 먹었어요!",
          "original_form": "어니오 아직 안 먹었어요!",
          "speaker_id": "2",
          "time": "20210829 08:40"
        },
        {
          "id": "MDRW2100035157.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 08:40"
        },
        {
          "id": "MDRW2100035157.1.4",
          "form": "전 방금 카레 먹고왔어용!ㅎㅎㅎ",
          "original_form": "전 방금 카레 먹고왔어용!ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 08:40"
        },
        {
          "id": "MDRW2100035157.1.5",
          "form": "오 미리 요리하신 건가요!!",
          "original_form": "오 미리 요리하신 건가요!!",
          "speaker_id": "2",
          "time": "20210829 08:41"
        },
        {
          "id": "MDRW2100035157.1.6",
          "form": "넵넵ㅋㅋㅋ 카레에 고기들어간거 좋아하시나용?",
          "original_form": "넵넵ㅋㅋㅋ 카레에 고기들어간거 좋아하시나용?",
          "speaker_id": "1",
          "time": "20210829 08:41"
        },
        {
          "id": "MDRW2100035157.1.7",
          "form": "고기없는 카레 안 좋아해요 ㅋㅋㅋ",
          "original_form": "고기없는 카레 안 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 08:41"
        },
        {
          "id": "MDRW2100035157.1.8",
          "form": "ㅋㅋㅋ 저두요 고기 들어가야 깊은맛이나요",
          "original_form": "ㅋㅋㅋ 저두요 고기 들어가야 깊은맛이나요",
          "speaker_id": "1",
          "time": "20210829 08:41"
        },
        {
          "id": "MDRW2100035157.1.9",
          "form": "오늘 뭐 드실예정이세용?",
          "original_form": "오늘 뭐 드실예정이세용?",
          "speaker_id": "1",
          "time": "20210829 08:42"
        },
        {
          "id": "MDRW2100035157.1.10",
          "form": "고민하고 있어요",
          "original_form": "고민하고 있어요",
          "speaker_id": "2",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100035157.1.11",
          "form": "뭘 먹어야 주말을 즐길 수 있을 지ㅠ",
          "original_form": "뭘 먹어야 주말을 즐길 수 있을 지ㅠ",
          "speaker_id": "2",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100035157.1.12",
          "form": "주말엔 요리해서 먹어야지 싶다가도",
          "original_form": "주말엔 요리해서 먹어야지 싶다가도",
          "speaker_id": "1",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100035157.1.13",
          "form": "귀찮아서 시켜먹을까",
          "original_form": "귀찮아서 시켜먹을까",
          "speaker_id": "1",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100035157.1.14",
          "form": "늘 고민해요ㅋㅋㅋ",
          "original_form": "늘 고민해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100035157.1.15",
          "form": "저는 어젠 요리해서 오늘은 안 하려구요",
          "original_form": "저는 어젠 요리해서 오늘은 안 하려구요",
          "speaker_id": "2",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100035157.1.16",
          "form": "엇 어젠 뭐 드셨어용?",
          "original_form": "엇 어젠 뭐 드셨어용?",
          "speaker_id": "1",
          "time": "20210829 08:46"
        },
        {
          "id": "MDRW2100035157.1.17",
          "form": "어젠 된장찌개도 하고 비빔국수도 했어요 ㅋㅋㅋㅋ",
          "original_form": "어젠 된장찌개도 하고 비빔국수도 했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:09"
        },
        {
          "id": "MDRW2100035157.1.18",
          "form": "요리왕인척 ㅋㅋㅋ",
          "original_form": "요리왕인척 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:09"
        },
        {
          "id": "MDRW2100035157.1.19",
          "form": "ㅋㅋㅋㅋ 충분히 요리왕이신데용!",
          "original_form": "ㅋㅋㅋㅋ 충분히 요리왕이신데용!",
          "speaker_id": "1",
          "time": "20210829 09:09"
        },
        {
          "id": "MDRW2100035157.1.20",
          "form": "저도 박막례 할머님이 광고하시는 비빔국수 사먹었는데",
          "original_form": "저도 박막례 할머님이 광고하시는 비빔국수 사먹었는데",
          "speaker_id": "1",
          "time": "20210829 09:09"
        },
        {
          "id": "MDRW2100035157.1.21",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 09:09"
        },
        {
          "id": "MDRW2100035157.1.22",
          "form": "오 그거 완전 치열했다고 하던데!!",
          "original_form": "오 그거 완전 치열했다고 하던데!!",
          "speaker_id": "2",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100035157.1.23",
