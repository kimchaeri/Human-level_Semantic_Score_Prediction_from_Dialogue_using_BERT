      "utterance": [
        {
          "id": "MDRW2100033060.1.1",
          "form": "오늘 왜 이렇게 피곤한가 했더만 커피를 안마셨네",
          "original_form": "오늘 왜 이렇게 피곤한가 했더만 커피를 안마셨네",
          "speaker_id": "2",
          "time": "20210906 14:56"
        },
        {
          "id": "MDRW2100033060.1.2",
          "form": "이런.. 얼른 한 잔 마셔",
          "original_form": "이런.. 얼른 한 잔 마셔",
          "speaker_id": "1",
          "time": "20210906 15:01"
        },
        {
          "id": "MDRW2100033060.1.3",
          "form": "신랑은 커피 마셨어?",
          "original_form": "신랑은 커피 마셨어?",
          "speaker_id": "2",
          "time": "20210906 15:02"
        },
        {
          "id": "MDRW2100033060.1.4",
          "form": "ㅇㅇ 난 이미 마셨지 ㅎㅎ",
          "original_form": "ㅇㅇ 난 이미 마셨지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:02"
        },
        {
          "id": "MDRW2100033060.1.5",
          "form": "회사 커피도 맥심이야?",
          "original_form": "회사 커피도 맥심이야?",
          "speaker_id": "2",
          "time": "20210906 15:02"
        },
        {
          "id": "MDRW2100033060.1.6",
          "form": "맥심 종류별로 있어~ 화이트골드 모카골드",
          "original_form": "맥심 종류별로 있어~ 화이트골드 모카골드",
          "speaker_id": "1",
          "time": "20210906 15:02"
        },
        {
          "id": "MDRW2100033060.1.7",
          "form": "오오 골라서 먹으면 되겠네 ㅎㅎ",
          "original_form": "오오 골라서 먹으면 되겠네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100033060.1.8",
          "form": "근데 난 모카골드가 좋아",
          "original_form": "근데 난 모카골드가 좋아",
          "speaker_id": "1",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100033060.1.9",
          "form": "나도~ 화이트 골드는 프림맛이 너무 진해",
          "original_form": "나도~ 화이트 골드는 프림맛이 너무 진해",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100033060.1.10",
          "form": "나 우유 싫어해서 그런지 프림맛 나는 화이트 골드가 안맞더라고",
          "original_form": "나 우유 싫어해서 그런지 프림맛 나는 화이트 골드가 안맞더라고",
          "speaker_id": "1",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100033060.1.11",
          "form": "나랑 비슷하네 ㅎㅎ",
          "original_form": "나랑 비슷하네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100033060.1.12",
          "form": "색시도 물 끓여서 어여 모카골드 마셔",
          "original_form": "색시도 물 끓여서 어여 모카골드 마셔",
          "speaker_id": "1",
          "time": "20210906 15:04"
        },
        {
          "id": "MDRW2100033060.1.13",
          "form": "이미 끓이고 있지 ㅋㅋ",
          "original_form": "이미 끓이고 있지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:04"
        },
        {
          "id": "MDRW2100033060.1.14",
          "form": "ㅎㅎ 역시나",
          "original_form": "ㅎㅎ 역시나",
          "speaker_id": "1",
          "time": "20210906 15:04"
        },
        {
          "id": "MDRW2100033060.1.15",
          "form": "오후엔 커피를 마셔줘야 정신이 맑아짐",
          "original_form": "오후엔 커피를 마셔줘야 정신이 맑아짐",
          "speaker_id": "2",
          "time": "20210906 15:04"
        },
        {
          "id": "MDRW2100033060.1.16",
          "form": "맞아 오후엔 커피 필수임",
          "original_form": "맞아 오후엔 커피 필수임",
