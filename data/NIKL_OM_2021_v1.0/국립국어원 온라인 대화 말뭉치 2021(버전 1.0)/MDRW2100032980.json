          "speaker_id": "2",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100032980.1.7",
          "form": "이것도 그나마 최근에",
          "original_form": "이것도 그나마 최근에",
          "speaker_id": "1",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100032980.1.8",
          "form": "지금은 얼마지",
          "original_form": "지금은 얼마지",
