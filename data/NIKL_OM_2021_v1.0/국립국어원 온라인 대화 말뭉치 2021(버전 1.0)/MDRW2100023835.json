      "utterance": [
        {
          "id": "MDRW2100023835.1.1",
          "form": "그게 뭐애요? 광고?",
          "original_form": "그게 뭐애요? 광고?",
          "speaker_id": "2",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100023835.1.2",
          "form": "sbs에서 애기들 데리고 했던 프로그램있거든요",
          "original_form": "sbs에서 애기들 데리고 했던 프로그램있거든요",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023835.1.3",
          "form": "그때 아마 처음나왔을때가 6살? 7살? 이었어요 ㅎㅎ",
          "original_form": "그때 아마 처음나왔을때가 6살? 7살? 이었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023835.1.4",
          "form": "아아... 예능이요...",
          "original_form": "아아... 예능이요...",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023835.1.5",
          "form": "아.. 그래요? 옛날부터 나온건 몰랏어요..",
          "original_form": "아.. 그래요? 옛날부터 나온건 몰랏어요..",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023835.1.6",
          "form": "진짜 개구쟁이처럼 귀여웠는데 잘컸어요~~",
          "original_form": "진짜 개구쟁이처럼 귀여웠는데 잘컸어요~~",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023835.1.7",
          "form": "시그널에도 나와서 감개무량했는데 아이돌이라니 ㅋㅋ",
          "original_form": "시그널에도 나와서 감개무량했는데 아이돌이라니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023835.1.8",
          "form": "엥? 시그널에서도요?",
          "original_form": "엥? 시그널에서도요?",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023835.1.9",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023835.1.10",
          "form": "시그널에 어떤역? 얼나전에 봣는뎅..",
          "original_form": "시그널에 어떤역? 얼나전에 봣는뎅..",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023835.1.11",
          "form": "아 스테이시로 처음아셨나봐요~",
          "original_form": "아 스테이시로 처음아셨나봐요~",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023835.1.12",
          "form": "대도사건 있죠!",
          "original_form": "대도사건 있죠!",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023835.1.13",
          "form": "네.. 전 스테이씨로 천봐서..",
          "original_form": "네.. 전 스테이씨로 천봐서..",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023835.1.14",
          "form": "그 딸 ㅜㅜ 성수대교 모티브로 한 ㅜㅠ",
          "original_form": "그 딸 ㅜㅜ 성수대교 모티브로 한 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023835.1.15",
          "form": "아.. 그딸?",
          "original_form": "아.. 그딸?",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100023835.1.16",
          "form": "이거말고도 아역활동 많이해서 나온 작품 많아요 ㅎㅎ",
          "original_form": "이거말고도 아역활동 많이해서 나온 작품 많아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100023835.1.17",
          "form": "아.. 정말여? 드라마 보면서 귀엽네하고 넘어갓는데.. 그게 스테이씨엿다뉘...",
          "original_form": "아.. 정말여? 드라마 보면서 귀엽네하고 넘어갓는데.. 그게 스테이씨엿다뉘...",
          "speaker_id": "2",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100023835.1.18",
          "form": "그래서 되게 의아했어요 아이돌한다고 할때!",
          "original_form": "그래서 되게 의아했어요 아이돌한다고 할때!",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100023835.1.19",
          "form": "그래서 수테이씨가 신인치고 인지도 있었던 거군요..",
          "original_form": "그래서 수테이씨가 신인치고 인지도 있었던 거군요..",
          "speaker_id": "2",
          "time": "20210830 18:43"
        },
        {
          "id": "MDRW2100023835.1.20",
          "form": "근데 저는 아이돌 시은보단 연기자가 더 좋아서 좀 아쉬워요 ㅎㅎ",
          "original_form": "근데 저는 아이돌 시은보단 연기자가 더 좋아서 좀 아쉬워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:43"
        },
        {
          "id": "MDRW2100023835.1.21",
          "form": "길게 보기에도 연기자쪽이 안정적이라",
          "original_form": "길게 보기에도 연기자쪽이 안정적이라",
