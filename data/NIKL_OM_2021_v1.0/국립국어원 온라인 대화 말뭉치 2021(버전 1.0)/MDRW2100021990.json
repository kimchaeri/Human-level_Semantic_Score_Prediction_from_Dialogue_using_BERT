      "utterance": [
        {
          "id": "MDRW2100021990.1.1",
          "form": "그니까 컨디션 차이가 큰듯",
          "original_form": "그니까 컨디션 차이가 큰듯",
          "speaker_id": "2",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021990.1.2",
          "form": "왜 그러는지를 모르겠어",
          "original_form": "왜 그러는지를 모르겠어",
          "speaker_id": "1",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021990.1.3",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021990.1.4",
          "form": "그래도 라면은 꾸준히 잘 먹잖어",
          "original_form": "그래도 라면은 꾸준히 잘 먹잖어",
          "speaker_id": "1",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021990.1.5",
          "form": "별로 안매워서 먹는건데?",
          "original_form": "별로 안매워서 먹는건데?",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021990.1.6",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021990.1.7",
          "form": "응 매우면 못 먹지 않나?",
          "original_form": "응 매우면 못 먹지 않나?",
          "speaker_id": "2",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021990.1.8",
          "form": "그래도 그거 꽤 맵지않아?",
          "original_form": "그래도 그거 꽤 맵지않아?",
          "speaker_id": "1",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021990.1.9",
          "form": "괜찮던데?",
          "original_form": "괜찮던데?",
          "speaker_id": "2",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021990.1.10",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021990.1.11",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021990.1.12",
          "form": "다른 덜 매운 음식도 못먹을 때 있길래",
          "original_form": "다른 덜 매운 음식도 못먹을 때 있길래",
          "speaker_id": "1",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021990.1.13",
