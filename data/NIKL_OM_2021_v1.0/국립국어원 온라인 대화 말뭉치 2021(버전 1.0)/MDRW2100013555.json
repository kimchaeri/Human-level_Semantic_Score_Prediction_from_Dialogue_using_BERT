      "utterance": [
        {
          "id": "MDRW2100013555.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013555.1.2",
          "form": "점심 뭐드셨나용!??!",
          "original_form": "점심 뭐드셨나용!??!",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013555.1.3",
          "form": "저는 아점으로 김치찌개요!",
          "original_form": "저는 아점으로 김치찌개요!",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013555.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013555.1.5",
          "form": "저는 아직이요ㅎㅎ",
          "original_form": "저는 아직이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013555.1.6",
          "form": "샐러드랩 먹으려고 해요 ㅠㅠ",
          "original_form": "샐러드랩 먹으려고 해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013555.1.7",
          "form": "오늘 skt 도미노 50퍼 할인한대요!!",
          "original_form": "오늘 skt 도미노 50퍼 할인한대요!!",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013555.1.8",
          "form": "근데 저녁에 어디 나가서...못먹을것같아용 ㅜ",
          "original_form": "근데 저녁에 어디 나가서...못먹을것같아용 ㅜ",
          "speaker_id": "1",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013555.1.9",
          "form": "저녁에 약속있으셔서 샐러드 드시는건가요?",
          "original_form": "저녁에 약속있으셔서 샐러드 드시는건가요?",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013555.1.10",
          "form": "건강식이네요!",
          "original_form": "건강식이네요!",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013555.1.11",
          "form": "악.. kt는 슬퍼합니다.. 50퍼면 대박인데요",
          "original_form": "악.. kt는 슬퍼합니다.. 50퍼면 대박인데요",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013555.1.12",
          "form": "아 피자는 저녁에 먹고싶은데 약속때매 못먹는거구",
          "original_form": "아 피자는 저녁에 먹고싶은데 약속때매 못먹는거구",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013555.1.13",
          "form": "샐러드는 하.. 다이어트해야돼용",
          "original_form": "샐러드는 하.. 다이어트해야돼용",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013555.1.14",
          "form": "맛있는거 많이먹구싶어용",
          "original_form": "맛있는거 많이먹구싶어용",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013555.1.15",
          "form": "야채 좋아하세요?",
          "original_form": "야채 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013555.1.16",
          "form": "저는 청경채 좋아하고 나물은 싫어해요",
          "original_form": "저는 청경채 좋아하고 나물은 싫어해요",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013555.1.17",
          "form": "하 다이어트..그쵸그쵸",
          "original_form": "하 다이어트..그쵸그쵸",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013555.1.18",
          "form": "저는 나물은 그나마 괜찮은데 가지같은거 싫어하고 두릅도..",
          "original_form": "저는 나물은 그나마 괜찮은데 가지같은거 싫어하고 두릅도..",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013555.1.19",
          "form": "쓴 나물은 싫어요ㅋㅋ!!",
          "original_form": "쓴 나물은 싫어요ㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013555.1.20",
          "form": "그 파리바게트도 요기요에서 픽업 5천원 할인하는거 아시나요?!",
          "original_form": "그 파리바게트도 요기요에서 픽업 5천원 할인하는거 아시나요?!",
          "speaker_id": "2",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013555.1.21",
          "form": "ㅋㅋㅋㅋㅋ 한정식 힘들어요 ",
          "original_form": "ㅋㅋㅋㅋㅋ 한정식 힘들어요 ",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013555.1.22",
          "form": "샐러드 그래서 가끔 행사하면 가성비 챙겨서 사먹어용ㅋㅋ",
          "original_form": "샐러드 그래서 가끔 행사하면 가성비 챙겨서 사먹어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013555.1.23",
          "form": "아 맞아용 저녁마다 사서 먹고있어요 ㅋㅋㅋㅋ",
          "original_form": "아 맞아용 저녁마다 사서 먹고있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013555.1.24",
          "form": "가성비 ㅇㅈ이욥",
          "original_form": "가성비 ㅇㅈ이욥",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013555.1.25",
          "form": "12개 됐을까용..?!",
          "original_form": "12개 됐을까용..?!",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013555.1.26",
          "form": "오 name1님도 아시는구나",
          "original_form": "오 &name1&님도 아시는구나",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013555.1.27",
          "form": "아뇨 아직요ㅠ",
          "original_form": "아뇨 아직요ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013555.1.28",
          "form": "저녁엔 뭐드시러 나가세요?",
          "original_form": "저녁엔 뭐드시러 나가세요?",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013555.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ 네 그거하러가입했어요 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네 그거하러가입했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013555.1.30",
          "form": "저녁에는 공연보러가는데",
          "original_form": "저녁에는 공연보러가는데",
          "speaker_id": "1",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013555.1.31",
          "form": "뭘 먹을것같진않고",
          "original_form": "뭘 먹을것같진않고",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013555.1.32",
          "form": "피자같이 거하게...? 는 못먹을 것같다는 말이였어욥 ㅎㅎ!",
          "original_form": "피자같이 거하게...? 는 못먹을 것같다는 말이였어욥 ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013555.1.33",
          "form": "내일 새로 사귄 친구랑 밥 먹기로했는데 뭐먹을지 조금 고민이 되네용",
          "original_form": "내일 새로 사귄 친구랑 밥 먹기로했는데 뭐먹을지 조금 고민이 되네용",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013555.1.34",
          "form": "아 공연보러 가시는구나 재밌겠네요!",
          "original_form": "아 공연보러 가시는구나 재밌겠네요!",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013555.1.35",
          "form": "헉 새로 사귄 친구면 무난한 음식 고르셔야겠어요!",
          "original_form": "헉 새로 사귄 친구면 무난한 음식 고르셔야겠어요!",
          "speaker_id": "2",
