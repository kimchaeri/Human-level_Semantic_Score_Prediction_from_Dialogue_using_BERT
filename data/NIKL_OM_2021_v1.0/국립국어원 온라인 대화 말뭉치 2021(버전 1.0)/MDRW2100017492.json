      "utterance": [
        {
          "id": "MDRW2100017492.1.1",
          "form": "요즘 어떤 음악 들으세요?",
          "original_form": "요즘 어떤 음악 들으세요?",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017492.1.2",
          "form": "저 요즘 빅마마 노래 다시듣고잇어요",
          "original_form": "저 요즘 빅마마 노래 다시듣고잇어요",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017492.1.3",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017492.1.4",
          "form": "빅마마 노래 좋죠",
          "original_form": "빅마마 노래 좋죠",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017492.1.5",
          "form": "유퀴즈 나온거 보고 꽂혀서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "유퀴즈 나온거 보고 꽂혀서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017492.1.6",
          "form": "오랜만에 들으니까 좋더라구요",
          "original_form": "오랜만에 들으니까 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017492.1.7",
          "form": "저도 요새 떠서 들었었어요",
          "original_form": "저도 요새 떠서 들었었어요",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017492.1.8",
          "form": "name2님은 어떤음악들으세요?",
          "original_form": "&name2&님은 어떤음악들으세요?",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017492.1.9",
          "form": "전 요새 팝송 들어요!",
          "original_form": "전 요새 팝송 들어요!",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017492.1.10",
          "form": "누구 좋아하세요",
          "original_form": "누구 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017492.1.11",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017492.1.12",
          "form": "요즘엔 저스틴비버 노래 많이 듣는것 같아요",
          "original_form": "요즘엔 저스틴비버 노래 많이 듣는것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017492.1.13",
          "form": "아 최근에 신곡 나왓지 않나요?",
          "original_form": "아 최근에 신곡 나왓지 않나요?",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017492.1.14",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017492.1.15",
          "form": "그거 많이 듣고있어요",
          "original_form": "그거 많이 듣고있어요",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017492.1.16",
          "form": "저도 들어봣어요 좋더라구요",
          "original_form": "저도 들어봣어요 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017492.1.17",
          "form": "마자요 가사도 좋고 괜찮더라구요",
          "original_form": "마자요 가사도 좋고 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017492.1.18",
          "form": "원래 팝송 좋아하셧어요?",
          "original_form": "원래 팝송 좋아하셧어요?",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017492.1.19",
          "form": "가끔 들었어요",
          "original_form": "가끔 들었어요",
          "speaker_id": "2",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017492.1.20",
          "form": "저도 가끔듣는편이엇어요",
          "original_form": "저도 가끔듣는편이엇어요",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017492.1.21",
          "form": "시험기간에 특히 많이 듣는것 같아요",
          "original_form": "시험기간에 특히 많이 듣는것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017492.1.22",
          "form": "저도 그랫어요 ㅋㅋㅋㅋ한글가사는 귀에 꽂혀버려서 ㅋㅋㅋㅋ",
          "original_form": "저도 그랫어요 ㅋㅋㅋㅋ한글가사는 귀에 꽂혀버려서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:01"
        },
        {
          "id": "MDRW2100017492.1.23",
          "form": "마자요 한글가사는 신나서 공부 던지고 놀아버려요..ㅎㅎ",
          "original_form": "마자요 한글가사는 신나서 공부 던지고 놀아버려요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100017492.1.24",
          "form": "ㅋㅋㅋㅋ맞아요 그리고 시험칠때 후렴구 계속 떠오름 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ맞아요 그리고 시험칠때 후렴구 계속 떠오름 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100017492.1.25",
          "form": "마자요... 진짜 계속 머리에서 맴돌아서 집중을 할수가 없어요",
          "original_form": "마자요... 진짜 계속 머리에서 맴돌아서 집중을 할수가 없어요",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100017492.1.26",
          "form": "계속 떠올라서 시험칠때 완전 망한적도 잇어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "계속 떠올라서 시험칠때 완전 망한적도 잇어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100017492.1.27",
          "form": "와 ㅋㅋㅋ 저두요",
          "original_form": "와 ㅋㅋㅋ 저두요",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100017492.1.28",
          "form": "그래서 그냥 포기했었어요",
          "original_form": "그래서 그냥 포기했었어요",
          "speaker_id": "2",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100017492.1.29",
          "form": "진짜 예전노래인데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 예전노래인데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100017492.1.30",
          "form": "십센치의 아메리카노가 그렇게 귀에서 맴도는거예요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "십센치의 아메리카노가 그렇게 귀에서 맴도는거예요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100017492.1.31",
          "form": "그거 진짜 아메아메아메 이부분...",
          "original_form": "그거 진짜 아메아메아메 이부분...",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100017492.1.32",
          "form": "미쳐요 시험 전에 들으면 큰일나요...",
          "original_form": "미쳐요 시험 전에 들으면 큰일나요...",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100017492.1.33",
          "form": "그뒤로 그노래 안들엇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그뒤로 그노래 안들엇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100017492.1.34",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100017492.1.35",
          "form": "저 저번 시험에 들었다가",
          "original_form": "저 저번 시험에 들었다가",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100017492.1.36",
          "form": "계속 맴돌아서 아.. 망했다 했어요",
          "original_form": "계속 맴돌아서 아.. 망했다 했어요",
