      "utterance": [
        {
          "id": "MDRW2100020918.1.1",
          "form": "오늘 점심 어떤거 드셨나요?",
          "original_form": "오늘 점심 어떤거 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020918.1.2",
          "form": "파스타 해먹으려구요",
          "original_form": "파스타 해먹으려구요",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020918.1.3",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020918.1.4",
          "form": "저는 돼지국밥 먹었어요 하하",
          "original_form": "저는 돼지국밥 먹었어요 하하",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020918.1.5",
          "form": "우와 무슨 파스타요?",
          "original_form": "우와 무슨 파스타요?",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020918.1.6",
          "form": "머쉬룸 크림 파스타요ㅎㅎ",
          "original_form": "머쉬룸 크림 파스타요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020918.1.7",
          "form": "요즘 시판소스가 아주 잘나와서요",
          "original_form": "요즘 시판소스가 아주 잘나와서요",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020918.1.8",
          "form": "우와 맛있겠네요",
          "original_form": "우와 맛있겠네요",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020918.1.9",
          "form": "시판소스 어떤거 드세요? 추천해주세요",
          "original_form": "시판소스 어떤거 드세요? 추천해주세요",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020918.1.10",
          "form": "크림소스는 사다가 실패한 적이 많거든요ㅜㅜ",
          "original_form": "크림소스는 사다가 실패한 적이 많거든요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020918.1.11",
          "form": "청정원에서 나온게 맛있더라구요",
          "original_form": "청정원에서 나온게 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020918.1.12",
          "form": "알리오올리오는 몇통째 계속 사먹고있어요",
          "original_form": "알리오올리오는 몇통째 계속 사먹고있어요",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020918.1.13",
          "form": "우와 청정원 참고하겠습니다!",
          "original_form": "우와 청정원 참고하겠습니다!",
          "speaker_id": "1",
          "time": "20210829 10:42"
        },
        {
          "id": "MDRW2100020918.1.14",
          "form": "알리오 올리오도 시판소스가 있군요?? 신기하네요",
          "original_form": "알리오 올리오도 시판소스가 있군요?? 신기하네요",
          "speaker_id": "1",
          "time": "20210829 10:42"
        },
        {
          "id": "MDRW2100020918.1.15",
          "form": "직접하는것보다 소스가 맛있어서 그걸로만 먹어요ㅋㅋ",
          "original_form": "직접하는것보다 소스가 맛있어서 그걸로만 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100020918.1.16",
          "form": "와 생각도 못했는걸요 저도 소스 사서 먹어봐야겠네요",
          "original_form": "와 생각도 못했는걸요 저도 소스 사서 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100020918.1.17",
          "form": "직접하면 잘 못해가지고 ㅋㅋㅋ 아무튼 사 먹어 봐야겠어요",
          "original_form": "직접하면 잘 못해가지고 ㅋㅋㅋ 아무튼 사 먹어 봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100020918.1.18",
          "form": "ㅋㅋ강력추천합니당",
          "original_form": "ㅋㅋ강력추천합니당",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100020918.1.19",
          "form": "아침드시는 편이세요?",
          "original_form": "아침드시는 편이세요?",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100020918.1.20",
          "form": "저요..? 먹는 버릇 해야하는데 잘 안먹게 되네요",
          "original_form": "저요..? 먹는 버릇 해야하는데 잘 안먹게 되네요",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100020918.1.21",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100020918.1.22",
          "form": "저는 항상먹어요",
          "original_form": "저는 항상먹어요",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100020918.1.23",
          "form": "안먹음 뭔가 허전해서",
          "original_form": "안먹음 뭔가 허전해서",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100020918.1.24",
          "form": "우와 그렇군요",
          "original_form": "우와 그렇군요",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100020918.1.25",
          "form": "저도 그런 습관을 들여야겠네요",
          "original_form": "저도 그런 습관을 들여야겠네요",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100020918.1.26",
          "form": "아침 주로 뭐 드시나요?",
          "original_form": "아침 주로 뭐 드시나요?",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100020918.1.27",
          "form": "밥이나 빵 먹어요!",
          "original_form": "밥이나 빵 먹어요!",
          "speaker_id": "2",
          "time": "20210829 13:47"
        }
      ]
    }
