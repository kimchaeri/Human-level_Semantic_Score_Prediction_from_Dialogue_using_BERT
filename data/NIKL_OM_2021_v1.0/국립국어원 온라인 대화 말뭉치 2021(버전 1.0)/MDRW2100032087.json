      "utterance": [
        {
          "id": "MDRW2100032087.1.1",
          "form": "저도 어느 정도 줄 서서 먹는 거는 이해 하지만 그 정도까지는 먹고 싶진 않아요",
          "original_form": "저도 어느 정도 줄 서서 먹는 거는 이해 하지만 그 정도까지는 먹고 싶진 않아요",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032087.1.2",
          "form": "위치 추적이 되어서 제주도에서만 예약할 수 있대요",
          "original_form": "위치 추적이 되어서 제주도에서만 예약할 수 있대요",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032087.1.3",
          "form": "그러게 말입니다",
          "original_form": "그러게 말입니다",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032087.1.4",
          "form": "오 그런 어플도 있군요",
          "original_form": "오 그런 어플도 있군요",
          "speaker_id": "2",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032087.1.5",
          "form": "그래서 예약한 다음에 파는 사람들 생겼나 봐요",
          "original_form": "그래서 예약한 다음에 파는 사람들 생겼나 봐요",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032087.1.6",
          "form": "장사는 끝이 없습니다",
          "original_form": "장사는 끝이 없습니다",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032087.1.7",
          "form": "세상에 나만 성이나",
          "original_form": "세상에 나만 성이나",
          "speaker_id": "2",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032087.1.8",
          "form": "저도 그 물결에 합류해 볼까요",
          "original_form": "저도 그 물결에 합류해 볼까요",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032087.1.9",
          "form": "치열하게 해서 맛있는 걸 먹어야 되겠군요",
          "original_form": "치열하게 해서 맛있는 걸 먹어야 되겠군요",
          "speaker_id": "2",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032087.1.10",
          "form": "저는 제주도 이니까요",
          "original_form": "저는 제주도 이니까요",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032087.1.11",
          "form": "타는 쪽인가요",
          "original_form": "타는 쪽인가요",
          "speaker_id": "2",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032087.1.12",
          "form": "치즈 돈까스가 그렇게 맛있다네요",
          "original_form": "치즈 돈까스가 그렇게 맛있다네요",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032087.1.13",
          "form": "타는 쪽인가요",
          "original_form": "타는 쪽인가요",
          "speaker_id": "2",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100032087.1.14",
