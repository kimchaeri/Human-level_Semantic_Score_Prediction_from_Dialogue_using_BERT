      "utterance": [
        {
          "id": "MDRW2100000954.1.1",
          "form": "플립 콜라보레이션 반은 예쁘고 반은 못생겼어",
          "original_form": "플립 콜라보레이션 반은 예쁘고 반은 못생겼어",
          "speaker_id": "1",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000954.1.2",
          "form": "랜덤으로 오는 건데 이상한 게 오면 어쩌지",
          "original_form": "랜덤으로 오는 건데 이상한 게 오면 어쩌지",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.3",
          "form": "아 근데 그거 마케팅 잘한거같아",
          "original_form": "아 근데 그거 마케팅 잘한거같아",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.4",
          "form": "나는 그냥 폰 본거보다",
          "original_form": "나는 그냥 폰 본거보다",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.5",
          "form": "맞아 평생 들을 래미안 다 들어봤다",
          "original_form": "맞아 평생 들을 래미안 다 들어봤다",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.6",
          "form": "안그래도 사람들",
          "original_form": "안그래도 사람들",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.7",
          "form": "래미안오면 어쩌지 하고",
          "original_form": "래미안오면 어쩌지 하고",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.8",
          "form": "걱정하던데",
          "original_form": "걱정하던데",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.9",
          "form": "나는 삼성라이온즈는 좋은데",
          "original_form": "나는 삼성라이온즈는 좋은데",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.10",
          "form": "수원블루윙즈 콜라보는 싫어...",
          "original_form": "수원블루윙즈 콜라보는 싫어...",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.11",
          "form": "그 콜라보는 나중에 팔아도 잘 팔릴듯",
          "original_form": "그 콜라보는 나중에 팔아도 잘 팔릴듯",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.12",
          "form": "수원블루윙즈는 사실 처음 들어보고...",
          "original_form": "수원블루윙즈는 사실 처음 들어보고...",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.13",
          "form": "와근데 역시 콜라보 잘했네",
          "original_form": "와근데 역시 콜라보 잘했네",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.14",
          "form": "홍보효과 대박이다",
          "original_form": "홍보효과 대박이다",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000954.1.15",
          "form": "근데 그거도 야구랑 콜라보한거임?",
          "original_form": "근데 그거도 야구랑 콜라보한거임?",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000954.1.16",
          "form": "야구 브랜드랑만 콜라보!",
          "original_form": "야구 브랜드랑만 콜라보!",
          "speaker_id": "1",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000954.1.17",
          "form": "수원 블루윙즈가 뭐야",
          "original_form": "수원 블루윙즈가 뭐야",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000954.1.18",
          "form": "그거는 축구클럽이름이래",
          "original_form": "그거는 축구클럽이름이래",
          "speaker_id": "1",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000954.1.19",
          "form": "진짜 첨들어보는 팀이랑도 했네",
          "original_form": "진짜 첨들어보는 팀이랑도 했네",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000954.1.20",
          "form": "케이리그 축구 구단 중에 하나",
          "original_form": "케이리그 축구 구단 중에 하나",
          "speaker_id": "1",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000954.1.21",
          "form": "아하아하",
          "original_form": "아하아하",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000954.1.22",
          "form": "맞제 근데 그러니까 홍보효과 가득.",
          "original_form": "맞제 근데 그러니까 홍보효과 가득.",
          "speaker_id": "1",
          "time": "20210819 20:53"
        }
      ]
