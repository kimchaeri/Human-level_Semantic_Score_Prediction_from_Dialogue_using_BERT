      "utterance": [
        {
          "id": "MDRW2100027979.1.1",
          "form": "시나리오도 한번에 나왔단 소린데",
          "original_form": "시나리오도 한번에 나왔단 소린데",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100027979.1.2",
          "form": "쓰다가 지친건가",
          "original_form": "쓰다가 지친건가",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100027979.1.3",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100027979.1.4",
          "form": "아니 근데",
          "original_form": "아니 근데",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100027979.1.5",
          "form": "ㅋㅋㅋㄱㄱㅋㄱ기본적인전개도 얼개가안맞으면",
          "original_form": "ㅋㅋㅋㄱㄱㅋㄱ기본적인전개도 얼개가안맞으면",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100027979.1.6",
          "form": "어케 작가를핫시",
          "original_form": "어케 작가를핫시",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100027979.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제가 진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제가 진짜",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.8",
          "form": "설정구멍을 못견디는 병이 있어서",
          "original_form": "설정구멍을 못견디는 병이 있어서",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.9",
          "form": "하지",
          "original_form": "하지",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.10",
          "form": "아나도",
          "original_form": "아나도",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.11",
          "form": "그런거만 보면 계속 물고 늘어지면서 고통받는데",
          "original_form": "그런거만 보면 계속 물고 늘어지면서 고통받는데",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.12",
          "form": "좀심한건못견뎌",
          "original_form": "좀심한건못견뎌",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.13",
          "form": "작품이 재밌기라도 하면",
          "original_form": "작품이 재밌기라도 하면",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.14",
          "form": "근데 신과함께는 보다가 ㄹㅇ",
          "original_form": "근데 신과함께는 보다가 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.15",
          "form": "짜증만나고",
          "original_form": "짜증만나고",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.16",
          "form": "극적 허용인가봄^^..... 이러고 넘기지만",
          "original_form": "극적 허용인가봄^^..... 이러고 넘기지만",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.17",
          "form": "구멍이한두군데도아니고",
          "original_form": "구멍이한두군데도아니고",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.18",
          "form": "재미도 없는데 설정구멍이 있다?",
          "original_form": "재미도 없는데 설정구멍이 있다?",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.19",
          "form": "용서할수 없어",
          "original_form": "용서할수 없어",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100027979.1.20",
          "form": "ㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㄱㄱ귿네문제는",
          "original_form": "ㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㄱㄱ귿네문제는",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100027979.1.21",
          "form": "신과 함께는 구멍이 너무 너무 커서",
          "original_form": "신과 함께는 구멍이 너무 너무 커서",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100027979.1.22",
          "form": "어떻게 할 수가 없어",
          "original_form": "어떻게 할 수가 없어",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100027979.1.23",
          "form": "재미도 노잼이고 구멍은 너무 크고",
          "original_form": "재미도 노잼이고 구멍은 너무 크고",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100027979.1.24",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100027979.1.25",
          "form": "애초에 단추를 잘못끼운듯",
          "original_form": "애초에 단추를 잘못끼운듯",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100027979.1.26",
          "form": "내가 주호민이었으면 사기 당했다 그랬을 거야",
          "original_form": "내가 주호민이었으면 사기 당했다 그랬을 거야",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100027979.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도 성적은 좋아서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도 성적은 좋아서",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100027979.1.28",
          "form": "많이 벌었으니",
          "original_form": "많이 벌었으니",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100027979.1.29",
          "form": "망정이지",
          "original_form": "망정이지",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100027979.1.30",
          "form": "흥행도 망했으면",
          "original_form": "흥행도 망했으면",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100027979.1.31",
          "form": "불만 많았을듯",
          "original_form": "불만 많았을듯",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100027979.1.32",
          "form": "오히려 흥행을 성공한 게 더 독이 됐던 것 같은데",
          "original_form": "오히려 흥행을 성공한 게 더 독이 됐던 것 같은데",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100027979.1.33",
          "form": "아 하긴...",
          "original_form": "아 하긴...",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100027979.1.34",
          "form": "1편 잘되니까 이제 됐다 하면서 2편은 아주 개발 세발로 만들어서",
          "original_form": "1편 잘되니까 이제 됐다 하면서 2편은 아주 개발 세발로 만들어서",
          "speaker_id": "1",
