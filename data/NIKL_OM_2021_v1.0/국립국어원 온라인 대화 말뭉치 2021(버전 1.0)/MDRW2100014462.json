      "utterance": [
        {
          "id": "MDRW2100014462.1.1",
          "form": "지금은 안 키우고 있어, 너는?",
          "original_form": "지금은 안 키우고 있어, 너는?",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014462.1.2",
          "form": "나는 키워본 적은 없고",
          "original_form": "나는 키워본 적은 없고",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014462.1.3",
          "form": "주변에서 키우는 건 봤어",
          "original_form": "주변에서 키우는 건 봤어",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014462.1.4",
          "form": "어떤 종을 주로 봤어?",
          "original_form": "어떤 종을 주로 봤어?",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014462.1.5",
          "form": "말티즈랑 푸들!",
          "original_form": "말티즈랑 푸들!",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100014462.1.6",
          "form": "둘 다 소형견이네...귀여웠겠다ㅠ",
          "original_form": "둘 다 소형견이네...귀여웠겠다ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100014462.1.7",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100014462.1.8",
          "form": "애들 다 활발해서 이뻤어",
          "original_form": "애들 다 활발해서 이뻤어",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100014462.1.9",
          "form": "너는 어떤 종류 키웠었어?",
          "original_form": "너는 어떤 종류 키웠었어?",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100014462.1.10",
          "form": "나는 예전에 고슴도치 키워봤어",
          "original_form": "나는 예전에 고슴도치 키워봤어",
          "speaker_id": "2",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100014462.1.11",
          "form": "오 그럼 애들 밀웜 먹지 않아,",
          "original_form": "오 그럼 애들 밀웜 먹지 않아,",
          "speaker_id": "1",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100014462.1.12",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 17:45"
        },
        {
          "id": "MDRW2100014462.1.13",
          "form": "나는 못 주고 부모님이 줬어...마지막까지 안 익숙해지더라ㅠ",
          "original_form": "나는 못 주고 부모님이 줬어...마지막까지 안 익숙해지더라ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100014462.1.14",
          "form": "친구 중에 고슴도치 키우는 애가 있었는데",
          "original_form": "친구 중에 고슴도치 키우는 애가 있었는데",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100014462.1.15",
          "form": "밀웜 먹는다는 걸 끄때 알았어",
          "original_form": "밀웜 먹는다는 걸 끄때 알았어",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100014462.1.16",
          "form": "나도 벌레는 못잡겠어",
          "original_form": "나도 벌레는 못잡겠어",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100014462.1.17",
          "form": "어우...비 오면 나오는 곱등이랑도 천적인데 밀웜은 꿈틀...ㅠㅠ",
          "original_form": "어우...비 오면 나오는 곱등이랑도 천적인데 밀웜은 꿈틀...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100014462.1.18",
          "form": "아..곱등ㅈ이 너무 싫어",
          "original_form": "아..곱등ㅈ이 너무 싫어",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100014462.1.19",
          "form": "고슴도치 용품들 아직도 있어?",
          "original_form": "고슴도치 용품들 아직도 있어?",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100014462.1.20",
          "form": "아니ㅋㅋㅋ 오래전이라 카페랑 주변에 다 나눠줬었어ㅋㅋ",
          "original_form": "아니ㅋㅋㅋ 오래전이라 카페랑 주변에 다 나눠줬었어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:30"
        }
      ]
