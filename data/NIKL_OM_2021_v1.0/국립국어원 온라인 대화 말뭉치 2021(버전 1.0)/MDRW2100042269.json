      "utterance": [
        {
          "id": "MDRW2100042269.1.1",
          "form": "지금오고있나용? ㅇㅁㅇ",
          "original_form": "지금오고있나용? ㅇㅁㅇ",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042269.1.2",
          "form": "네 저희 동네는 비가 점점 많이오고 있어요",
          "original_form": "네 저희 동네는 비가 점점 많이오고 있어요",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042269.1.3",
          "form": "내일까지 올거레요",
          "original_form": "내일까지 올거레요",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042269.1.4",
          "form": "내일도 밖에 돌아다녀야하는데 비가싫습니다 흑흑",
          "original_form": "내일도 밖에 돌아다녀야하는데 비가싫습니다 흑흑",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042269.1.5",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042269.1.6",
          "form": "비가 지켜워지려고 하네요",
          "original_form": "비가 지켜워지려고 하네요",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042269.1.7",
          "form": "장화안좋아하는데\n비오는날은또생각나구",
          "original_form": "장화안좋아하는데\n비오는날은또생각나구",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042269.1.8",
          "form": "ㅋㅋㅋㅋ..",
          "original_form": "ㅋㅋㅋㅋ..",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042269.1.9",
          "form": "비오는 거 실내에서 보는 건 좋은데",
          "original_form": "비오는 거 실내에서 보는 건 좋은데",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042269.1.10",
          "form": "밖에 다닐 때 습하고 꿉꿉한게 별로에요",
          "original_form": "밖에 다닐 때 습하고 꿉꿉한게 별로에요",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042269.1.11",
          "form": "맞아요 실내에서보면 좋죠ㅜㅜ...",
          "original_form": "맞아요 실내에서보면 좋죠ㅜㅜ...",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042269.1.12",
          "form": "다니는건 음..",
          "original_form": "다니는건 음..",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042269.1.13",
          "form": "내읾까지만 오는 거면 그나마 나은데 며칠 더 올까봐 걱정이에요",
          "original_form": "내읾까지만 오는 거면 그나마 나은데 며칠 더 올까봐 걱정이에요",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042269.1.14",
          "form": "일기예보가엉망이라서..jnj  감도안오네요흑",
          "original_form": "일기예보가엉망이라서..jnj  감도안오네요흑",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042269.1.15",
          "form": "그것도 맞아요",
          "original_form": "그것도 맞아요",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042269.1.16",
          "form": "예보가 틀릴 수도 있어서 계획을 잘 못하겠어요",
          "original_form": "예보가 틀릴 수도 있어서 계획을 잘 못하겠어요",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042269.1.17",
          "form": "네 계속 우양산들고다니구 가디건도챙기구 짐이많아요ㅋㅋㅋ",
          "original_form": "네 계속 우양산들고다니구 가디건도챙기구 짐이많아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042269.1.18",
          "form": "짐이 많아지죠",
          "original_form": "짐이 많아지죠",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042269.1.19",
          "form": "옷도 점점 두꺼워져서 무거워지구요",
          "original_form": "옷도 점점 두꺼워져서 무거워지구요",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042269.1.20",
          "form": "여름은 옷차림이 가벼운 건 참 좋았는데 말이죠",
          "original_form": "여름은 옷차림이 가벼운 건 참 좋았는데 말이죠",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042269.1.21",
          "form": "그래도땀때문에 최악인여름..ㅜㅜㅎㅎ",
