      "utterance": [
        {
          "id": "MDRW2100038047.1.1",
          "form": "결혼은 하셨나요",
          "original_form": "결혼은 하셨나요",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038047.1.2",
          "form": "아니요 미혼이에요",
          "original_form": "아니요 미혼이에요",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100038047.1.3",
          "form": "엄마랑 지지고볶고 살고있어요 ㅋ",
          "original_form": "엄마랑 지지고볶고 살고있어요 ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100038047.1.4",
          "form": "연애 중이신가요 ?",
          "original_form": "연애 중이신가요 ?",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038047.1.5",
          "form": "음 아니요ㅜㅜ",
          "original_form": "음 아니요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038047.1.6",
          "form": "암것도 없어요ㅜㅜㅜ",
          "original_form": "암것도 없어요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038047.1.7",
          "form": "외롭지는 않으세요",
          "original_form": "외롭지는 않으세요",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038047.1.8",
          "form": "혼자서도 잘놀긴해요ㅜ",
          "original_form": "혼자서도 잘놀긴해요ㅜ",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038047.1.9",
          "form": "집안일을 좀 도와주시나요",
          "original_form": "집안일을 좀 도와주시나요",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038047.1.10",
          "form": "제가 하는건 성에 안차하셔서ㅜㅜ",
          "original_form": "제가 하는건 성에 안차하셔서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038047.1.11",
          "form": "전 청소가 집안일중 제일 싫어요",
          "original_form": "전 청소가 집안일중 제일 싫어요",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038047.1.12",
          "form": "그중에서 화장실청소가 제일 답없죠 ㅋㅋ",
          "original_form": "그중에서 화장실청소가 제일 답없죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038047.1.13",
          "form": "화장실도 좀 난해하죠",
          "original_form": "화장실도 좀 난해하죠",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038047.1.14",
          "form": "요리는 좋아하시나요 ?",
          "original_form": "요리는 좋아하시나요 ?",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038047.1.15",
          "form": "의욕은 넘쳐요 ㅋㅋ",
          "original_form": "의욕은 넘쳐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100038047.1.16",
          "form": "하지만 손에 칼잡아본적이별로없네요",
          "original_form": "하지만 손에 칼잡아본적이별로없네요",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100038047.1.17",
          "form": "전 주부라 칼이 익숙하죠\r\n잘하진 못하지만 .....",
          "original_form": "전 주부라 칼이 익숙하죠\r\n잘하진 못하지만 .....",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100038047.1.18",
          "form": "전 아직 엄마 살림 건드리는 느낌이라 ㅜ",
          "original_form": "전 아직 엄마 살림 건드리는 느낌이라 ㅜ",
