      "utterance": [
        {
          "id": "MDRW2100039074.1.1",
          "form": "드라마는 어떤거 보세요",
          "original_form": "드라마는 어떤거 보세요",
          "speaker_id": "1",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100039074.1.2",
          "form": "드라마는 어떤거보세요",
          "original_form": "드라마는 어떤거보세요",
          "speaker_id": "1",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100039074.1.3",
          "form": "드라마 잘 안봐요 가장 최근에 본 tv 드라마가 아마",
          "original_form": "드라마 잘 안봐요 가장 최근에 본 tv 드라마가 아마",
          "speaker_id": "2",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100039074.1.4",
          "form": "18어게인이였나...",
          "original_form": "18어게인이였나...",
          "speaker_id": "2",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100039074.1.5",
          "form": "아 그건 저도 잠깐잠깐봤어요\r\n그럼 어떤프로 보세요",
          "original_form": "아 그건 저도 잠깐잠깐봤어요\r\n그럼 어떤프로 보세요",
          "speaker_id": "1",
          "time": "20210903 09:23"
        },
        {
          "id": "MDRW2100039074.1.6",
          "form": "유퀴즈랑 식스센스, 놀면뭐하니 봐요",
          "original_form": "유퀴즈랑 식스센스, 놀면뭐하니 봐요",
          "speaker_id": "2",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100039074.1.7",
          "form": "유느님 유멘",
          "original_form": "유느님 유멘",
          "speaker_id": "2",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100039074.1.8",
          "form": "아 저도 잘봐요 셋다 재미있죠",
          "original_form": "아 저도 잘봐요 셋다 재미있죠",
          "speaker_id": "1",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100039074.1.9",
          "form": "맞아요ㅎㅎ 혹시 식스센스에서 나온 음식점 가본데 있으세요?",
          "original_form": "맞아요ㅎㅎ 혹시 식스센스에서 나온 음식점 가본데 있으세요?",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100039074.1.10",
          "form": "아니요 가보진 않앗어요",
          "original_form": "아니요 가보진 않앗어요",
          "speaker_id": "1",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100039074.1.11",
          "form": "저번에 나왔던 시장 떡갈비집 진짜 맛있어요",
          "original_form": "저번에 나왔던 시장 떡갈비집 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100039074.1.12",
          "form": "벌써 2번 다녀왔어요ㅎㅎ",
          "original_form": "벌써 2번 다녀왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100039074.1.13",
          "form": "와 일부러 찾아가신거세요?",
          "original_form": "와 일부러 찾아가신거세요?",
          "speaker_id": "1",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100039074.1.14",
          "form": "네ㅋㅋ 고기는 못참거든요",
          "original_form": "네ㅋㅋ 고기는 못참거든요",
          "speaker_id": "2",
          "time": "20210903 09:28"
        },
        {
          "id": "MDRW2100039074.1.15",
          "form": "전 맛있는거 나와도 찾아갈 엄두는 안나더라고요",
          "original_form": "전 맛있는거 나와도 찾아갈 엄두는 안나더라고요",
          "speaker_id": "1",
          "time": "20210903 09:33"
        },
        {
          "id": "MDRW2100039074.1.16",
          "form": "저는 티비에서 보고 자주 찾아다녔어요",
          "original_form": "저는 티비에서 보고 자주 찾아다녔어요",
          "speaker_id": "2",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100039074.1.17",
          "form": "와 맛집탐방 좋아하시나봐요",
          "original_form": "와 맛집탐방 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039074.1.18",
          "form": "네ㅎㅎ 인생의 낙이죠",
          "original_form": "네ㅎㅎ 인생의 낙이죠",
          "speaker_id": "2",
