      "utterance": [
        {
          "id": "MDRW2100031951.1.1",
          "form": "요즘 책 좀 읽어?",
          "original_form": "요즘 책 좀 읽어?",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100031951.1.2",
          "form": "ㅋ하나도 안읽어",
          "original_form": "ㅋ하나도 안읽어",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100031951.1.3",
          "form": "책 좀 읽어ㅋㅋ",
          "original_form": "책 좀 읽어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100031951.1.4",
          "form": "너는 뭐읽는데",
          "original_form": "너는 뭐읽는데",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100031951.1.5",
          "form": "나는 신화읽잖아",
          "original_form": "나는 신화읽잖아",
          "speaker_id": "2",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100031951.1.6",
          "form": "신화? 뭔 신화",
          "original_form": "신화? 뭔 신화",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100031951.1.7",
          "form": "그리스로마신화",
          "original_form": "그리스로마신화",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100031951.1.8",
          "form": "동양신화는 안읽어?",
          "original_form": "동양신화는 안읽어?",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100031951.1.9",
          "form": "나중에 읽어보려고",
          "original_form": "나중에 읽어보려고",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100031951.1.10",
          "form": "저번에 중국신화 읽지 않았었나?",
          "original_form": "저번에 중국신화 읽지 않았었나?",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100031951.1.11",
          "form": "어 잠깐 읽었었지",
          "original_form": "어 잠깐 읽었었지",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100031951.1.12",
          "form": "그 책은 별로였어?",
          "original_form": "그 책은 별로였어?",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100031951.1.13",
          "form": "어렵더라구..",
          "original_form": "어렵더라구..",
