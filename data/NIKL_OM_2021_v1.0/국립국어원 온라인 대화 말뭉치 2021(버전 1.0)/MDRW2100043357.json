      "utterance": [
        {
          "id": "MDRW2100043357.1.1",
          "form": "헐 고 추 바사삭 시키셨어요?",
          "original_form": "헐 고 추 바사삭 시키셨어요?",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043357.1.2",
          "form": "저그거 진짜좋아해요",
          "original_form": "저그거 진짜좋아해요",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043357.1.3",
          "form": "아니요ㅜㅜ희망사항이에요",
          "original_form": "아니요ㅜㅜ희망사항이에요",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043357.1.4",
          "form": "열심히이거해서",
          "original_form": "열심히이거해서",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043357.1.5",
          "form": "바사삭먹어봐야져",
          "original_form": "바사삭먹어봐야져",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043357.1.6",
          "form": "ㅡㅜㅠ",
          "original_form": "ㅡㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043357.1.7",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ아 ㅋㅋㅋㅋㅋㅋ맞아요.... 베스킨도 먹구... 치킨두먹구...",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ아 ㅋㅋㅋㅋㅋㅋ맞아요.... 베스킨도 먹구... 치킨두먹구...",
          "speaker_id": "2",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043357.1.8",
          "form": "그니깐요ㅜㅜ",
          "original_form": "그니깐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043357.1.9",
          "form": "코로나라서 그렇지",
          "original_form": "코로나라서 그렇지",
          "speaker_id": "1",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043357.1.10",
          "form": "맞아요 진짜 ㅋㅋㅋ네이버 포인트 들어오는 거로도 요기요에서 시켜먹을거에요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 진짜 ㅋㅋㅋ네이버 포인트 들어오는 거로도 요기요에서 시켜먹을거에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043357.1.11",
          "form": "요기요 단골인가봐요?",
          "original_form": "요기요 단골인가봐요?",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043357.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043357.1.13",
          "form": "요기요가 편해서요 ㅋㅋㅋㅋ 근데 배달 쓰레기 많이나와서 이제 자제하고있어요",
          "original_form": "요기요가 편해서요 ㅋㅋㅋㅋ 근데 배달 쓰레기 많이나와서 이제 자제하고있어요",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043357.1.14",
          "form": "요즘 라이더들 인기많자나여",
          "original_form": "요즘 라이더들 인기많자나여",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043357.1.15",
          "form": "배달라이더 자전거타고 가는거 봤는데",
          "original_form": "배달라이더 자전거타고 가는거 봤는데",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043357.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043357.1.17",
          "form": "ㅋㅋㅋㅋ대박 ㅋㅋㅋ 안힘드셨나요?",
          "original_form": "ㅋㅋㅋㅋ대박 ㅋㅋㅋ 안힘드셨나요?",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043357.1.18",
          "form": "저 안힘들어요 자전거타고가는 배달민족라이더 가는것만봐서",
          "original_form": "저 안힘들어요 자전거타고가는 배달민족라이더 가는것만봐서",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043357.1.19",
          "form": "어떻게하면 일할수있는지 궁금해여",
          "original_form": "어떻게하면 일할수있는지 궁금해여",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043357.1.20",
          "form": "아아 그거 배달의민족 라이더 앱이있어요",
          "original_form": "아아 그거 배달의민족 라이더 앱이있어요",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043357.1.21",
          "form": "그거 깔고 라이더등록하고 안전교육받으면 할수있대요!",
          "original_form": "그거 깔고 라이더등록하고 안전교육받으면 할수있대요!",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043357.1.22",
          "form": "아진짜요?",
          "original_form": "아진짜요?",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043357.1.23",
          "form": "누구나할수있는거맞죠?",
          "original_form": "누구나할수있는거맞죠?",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043357.1.24",
          "form": "네 ㅋㅋㅋ검색해보세요 제친구도 몇번해봣어요",
          "original_form": "네 ㅋㅋㅋ검색해보세요 제친구도 몇번해봣어요",
          "speaker_id": "2",
          "time": "20210907 18:59"
        }
      ]
    }
