      "utterance": [
        {
          "id": "MDRW2100030516.1.1",
          "form": "저도 필라테스 관심있었는데",
          "original_form": "저도 필라테스 관심있었는데",
          "speaker_id": "2",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100030516.1.2",
          "form": "앗 왜 나쁜말이래지!",
          "original_form": "앗 왜 나쁜말이래지!",
          "speaker_id": "1",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100030516.1.3",
          "form": "오늘 강사님이 제 배를",
          "original_form": "오늘 강사님이 제 배를",
          "speaker_id": "1",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100030516.1.4",
          "form": "집주변 필라테스 강좌는 다 한회차에 7만원 그러더라구요",
          "original_form": "집주변 필라테스 강좌는 다 한회차에 7만원 그러더라구요",
          "speaker_id": "2",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100030516.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 02:45"
        },
        {
          "id": "MDRW2100030516.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.7",
          "form": "아니 나쁜말이라니~~~~",
          "original_form": "아니 나쁜말이라니~~~~",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.9",
          "form": "제 배를 터치 해보시더니",
          "original_form": "제 배를 터치 해보시더니",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.10",
          "form": "정말 근육이 없고 말랑말랑하다고",
          "original_form": "정말 근육이 없고 말랑말랑하다고",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.11",
          "form": "감탄하셨어요",
          "original_form": "감탄하셨어요",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.12",
          "form": "앗ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이건 감탄받을게 아닌뎈ㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이건 감탄받을게 아닌뎈ㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.14",
          "form": "맞아요 필라테스",
          "original_form": "맞아요 필라테스",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.15",
          "form": "진짜 비싸죠!!!!",
          "original_form": "진짜 비싸죠!!!!",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.16",
          "form": "배는 다..말랑말랑한게아니었나요",
          "original_form": "배는 다..말랑말랑한게아니었나요",
          "speaker_id": "2",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.17",
          "form": "눈물ㅜㅜ",
          "original_form": "눈물ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.18",
          "form": "저도 그래서 그냥 가성비로..... 8대 1을 들어요...ㅠ",
          "original_form": "저도 그래서 그냥 가성비로..... 8대 1을 들어요...ㅠ",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.19",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "1",
          "time": "20210904 02:46"
        },
        {
          "id": "MDRW2100030516.1.20",
          "form": "우와 8대1로 진행하면",
          "original_form": "우와 8대1로 진행하면",
          "speaker_id": "2",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.21",
          "form": "돈만 많으면 진짜 일대일 할거같아요....",
          "original_form": "돈만 많으면 진짜 일대일 할거같아요....",
          "speaker_id": "1",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.22",
          "form": "얼마정도하나요?",
          "original_form": "얼마정도하나요?",
          "speaker_id": "2",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.23",
          "form": "근데 저는 친구가 소개해줘서",
          "original_form": "근데 저는 친구가 소개해줘서",
          "speaker_id": "1",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.24",
          "form": "25회에 35만원인거고",
          "original_form": "25회에 35만원인거고",
          "speaker_id": "1",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.25",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.26",
          "form": "원래대로라면 24회에",
          "original_form": "원래대로라면 24회에",
          "speaker_id": "1",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.27",
          "form": "42만원...?",
          "original_form": "42만원...?",
          "speaker_id": "1",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.28",
          "form": "진짜좋네요",
          "original_form": "진짜좋네요",
          "speaker_id": "2",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.29",
          "form": "43만원?! 이랬어요",
          "original_form": "43만원?! 이랬어요",
          "speaker_id": "1",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.30",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "1",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.31",
          "form": "강사님도 친절하시고...",
          "original_form": "강사님도 친절하시고...",
          "speaker_id": "1",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.32",
          "form": "그래도 한 회차에 2만원정도니까",
          "original_form": "그래도 한 회차에 2만원정도니까",
          "speaker_id": "2",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.33",
          "form": "네네 8ㅅ8 진짜 땀 한바가지...",
          "original_form": "네네 8ㅅ8 진짜 땀 한바가지...",
          "speaker_id": "1",
          "time": "20210904 02:47"
        },
        {
          "id": "MDRW2100030516.1.34",
          "form": "저도 8대1이런걸로 다시한번 알아봐야겠네요!!",
          "original_form": "저도 8대1이런걸로 다시한번 알아봐야겠네요!!",
