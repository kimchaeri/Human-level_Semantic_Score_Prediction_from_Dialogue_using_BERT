      "utterance": [
        {
          "id": "MDRW2100001404.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 10:30"
        },
        {
          "id": "MDRW2100001404.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211015 10:30"
        },
        {
          "id": "MDRW2100001404.1.3",
          "form": "무엇을 어떻게 해야하나요..?",
          "original_form": "무엇을 어떻게 해야하나요..?",
          "speaker_id": "1",
          "time": "20211015 10:32"
        },
        {
          "id": "MDRW2100001404.1.4",
          "form": "오늘 아침은 드셨나요?",
          "original_form": "오늘 아침은 드셨나요?",
          "speaker_id": "2",
          "time": "20211015 10:33"
        },
        {
          "id": "MDRW2100001404.1.5",
          "form": "네 빵하고 커피랑 먹었어요!",
          "original_form": "네 빵하고 커피랑 먹었어요!",
          "speaker_id": "1",
          "time": "20211015 10:33"
        },
        {
          "id": "MDRW2100001404.1.6",
          "form": "오 무슨 빵 드셨어요 저도 빵 좋아하는데",
          "original_form": "오 무슨 빵 드셨어요 저도 빵 좋아하는데",
          "speaker_id": "2",
          "time": "20211015 10:33"
        },
        {
          "id": "MDRW2100001404.1.7",
          "form": "크로와상이요~",
          "original_form": "크로와상이요~",
          "speaker_id": "1",
          "time": "20211015 10:34"
        },
        {
          "id": "MDRW2100001404.1.8",
          "form": "평소에도 아침에 빵 자주 드시나요?",
          "original_form": "평소에도 아침에 빵 자주 드시나요?",
          "speaker_id": "2",
          "time": "20211015 10:36"
        },
        {
          "id": "MDRW2100001404.1.9",
          "form": "아뇨 평소에는 아침을 잘 먹지 않아요",
          "original_form": "아뇨 평소에는 아침을 잘 먹지 않아요",
          "speaker_id": "1",
          "time": "20211015 10:37"
        },
        {
          "id": "MDRW2100001404.1.10",
          "form": "저는 아침을 항상 챙겨먹으려구 노력해요",
          "original_form": "저는 아침을 항상 챙겨먹으려구 노력해요",
          "speaker_id": "2",
          "time": "20211015 10:47"
        },
        {
          "id": "MDRW2100001404.1.11",
          "form": "건강에 좋다하더라구요 ㅋㅋ",
          "original_form": "건강에 좋다하더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 10:47"
        },
        {
          "id": "MDRW2100001404.1.12",
          "form": "그쵸 근데 저는 잠을 더 선호하는 편이라서요 아침을 거르고 조금 더 자는 편이에요",
          "original_form": "그쵸 근데 저는 잠을 더 선호하는 편이라서요 아침을 거르고 조금 더 자는 편이에요",
          "speaker_id": "1",
          "time": "20211015 10:55"
        },
        {
          "id": "MDRW2100001404.1.13",
          "form": "저도 아침잠이 많은데",
          "original_form": "저도 아침잠이 많은데",
          "speaker_id": "2",
          "time": "20211015 10:58"
        },
        {
          "id": "MDRW2100001404.1.14",
          "form": "요즘 미라클모닝이다 뭐다 해서 따라하다보니",
          "original_form": "요즘 미라클모닝이다 뭐다 해서 따라하다보니",
          "speaker_id": "2",
          "time": "20211015 10:58"
        },
        {
          "id": "MDRW2100001404.1.15",
          "form": "자연스럽게 아침밥을 먹게되더라구요 ",
          "original_form": "자연스럽게 아침밥을 먹게되더라구요 ",
          "speaker_id": "2",
          "time": "20211015 10:58"
        },
        {
          "id": "MDRW2100001404.1.16",
          "form": "저는 회사 출근시간이 7시라....챙겨먹기 벅차요",
          "original_form": "저는 회사 출근시간이 7시라....챙겨먹기 벅차요",
          "speaker_id": "1",
          "time": "20211015 10:58"
        },
        {
          "id": "MDRW2100001404.1.17",
          "form": "헉 엄청 빠른편이시네요",
          "original_form": "헉 엄청 빠른편이시네요",
          "speaker_id": "2",
          "time": "20211015 11:01"
        },
        {
          "id": "MDRW2100001404.1.18",
          "form": "아기 분유값이랑 기저귀값 벌려면 열심히 일해야죠",
          "original_form": "아기 분유값이랑 기저귀값 벌려면 열심히 일해야죠",
          "speaker_id": "1",
          "time": "20211015 11:02"
        },
        {
          "id": "MDRW2100001404.1.19",
          "form": "맞아요.. 집에 아기 있으면.. ㅠ",
          "original_form": "맞아요.. 집에 아기 있으면.. ㅠ",
          "speaker_id": "2",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001404.1.20",
          "form": "근데 힘든만큼 이뻐요",
          "original_form": "근데 힘든만큼 이뻐요",
          "speaker_id": "1",
          "time": "20211015 11:04"
        },
        {
          "id": "MDRW2100001404.1.21",
          "form": "정말 세상 모든 아기들은 예쁘고 사랑스러운 것 같아요",
          "original_form": "정말 세상 모든 아기들은 예쁘고 사랑스러운 것 같아요",
          "speaker_id": "2",
          "time": "20211015 11:05"
        }
      ]
