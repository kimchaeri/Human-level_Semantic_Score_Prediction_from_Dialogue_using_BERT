      "utterance": [
        {
          "id": "MDRW2100015351.1.1",
          "form": "운동 좀 하시나요",
          "original_form": "운동 좀 하시나요",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015351.1.2",
          "form": "아니요 안해요",
          "original_form": "아니요 안해요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015351.1.3",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015351.1.4",
          "form": "귀찮아서요",
          "original_form": "귀찮아서요",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015351.1.5",
          "form": "근데 막상하면 재밌게 잘해요",
          "original_form": "근데 막상하면 재밌게 잘해요",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015351.1.6",
          "form": "배드민턴이나",
          "original_form": "배드민턴이나",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015351.1.7",
          "form": "탁구나",
          "original_form": "탁구나",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015351.1.8",
          "form": "이런 것들이요",
          "original_form": "이런 것들이요",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015351.1.9",
          "form": "잘요?",
          "original_form": "잘요?",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015351.1.10",
          "form": "그렇게 생각하시는군요",
          "original_form": "그렇게 생각하시는군요",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015351.1.11",
          "form": "네 잘해요",
          "original_form": "네 잘해요",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015351.1.12",
          "form": "재밌네요",
          "original_form": "재밌네요",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015351.1.13",
          "form": "최근에 제가 완승했거든요",
          "original_form": "최근에 제가 완승했거든요",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015351.1.14",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015351.1.15",
          "form": "초등학생이랑 하셨어요?",
          "original_form": "초등학생이랑 하셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015351.1.16",
          "form": "음 어찌보면 그렇게도 볼 수 있겠네요",
          "original_form": "음 어찌보면 그렇게도 볼 수 있겠네요",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015351.1.17",
          "form": "달리기 잘하세요?",
          "original_form": "달리기 잘하세요?",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015351.1.18",
          "form": "그럴게 볼수도 있다니 흥미롭네요",
          "original_form": "그럴게 볼수도 있다니 흥미롭네요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015351.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015351.1.20",
          "form": "50m 몇 초 뛰세요?",
          "original_form": "50m 몇 초 뛰세요?",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015351.1.21",
          "form": "6초요",
          "original_form": "6초요",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015351.1.22",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015351.1.23",
          "form": "그럼 학창시절에 계주 뛰어보셨겠네요",
          "original_form": "그럼 학창시절에 계주 뛰어보셨겠네요",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015351.1.24",
          "form": "어떠셨나요?",
          "original_form": "어떠셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015351.1.25",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015351.1.26",
          "form": "제가 다 발랐슴니다",
          "original_form": "제가 다 발랐슴니다",
          "speaker_id": "1",
          "time": "20210825 22:52"
        }
