      "utterance": [
        {
          "id": "MDRW2100007047.1.1",
          "form": "밥 먹었어?",
          "original_form": "밥 먹었어?",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007047.1.2",
          "form": "아니 안먹었어",
          "original_form": "아니 안먹었어",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007047.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007047.1.4",
          "form": "헉 아직 안먹었구나 ㅠㅠ 나는 맥날 불고기세트 냠냠 했어",
          "original_form": "헉 아직 안먹었구나 ㅠㅠ 나는 맥날 불고기세트 냠냠 했어",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007047.1.5",
          "form": "왜 아직 안먹었어 ㅠㅠ",
          "original_form": "왜 아직 안먹었어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007047.1.6",
          "form": "하루종일 잠만자서 ㅋㅋㅋ저녁에 일어났는데",
          "original_form": "하루종일 잠만자서 ㅋㅋㅋ저녁에 일어났는데",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007047.1.7",
          "form": "별로 생각이없었어",
          "original_form": "별로 생각이없었어",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007047.1.8",
          "form": "밤에 뭐 먹기도 그러니까 아침까지 굶어야지..",
          "original_form": "밤에 뭐 먹기도 그러니까 아침까지 굶어야지..",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007047.1.9",
          "form": "일어난지 얼마 안됐구나ㅋㅋㅋ",
          "original_form": "일어난지 얼마 안됐구나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007047.1.10",
          "form": "헉 굶을거야",
          "original_form": "헉 굶을거야",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007047.1.11",
          "form": "안돼에ㅔ 간단하게라도 챙겨먹어 ㅠㅠㅠㅠ 머 먹을 거 없어 ㅠㅠ??",
          "original_form": "안돼에ㅔ 간단하게라도 챙겨먹어 ㅠㅠㅠㅠ 머 먹을 거 없어 ㅠㅠ??",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007047.1.12",
          "form": "라면밖에없다..",
          "original_form": "라면밖에없다..",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007047.1.13",
          "form": "라면어제먹었는데",
          "original_form": "라면어제먹었는데",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007047.1.14",
          "form": "집에서 뭐 해먹기 어렵지않니",
          "original_form": "집에서 뭐 해먹기 어렵지않니",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007047.1.15",
          "form": "아아ㅏ 그건 맞지맞지",
          "original_form": "아아ㅏ 그건 맞지맞지",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007047.1.16",
          "form": "나 집에서 직접 만들어 먹은 적 잘 없다",
          "original_form": "나 집에서 직접 만들어 먹은 적 잘 없다",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007047.1.17",
          "form": "그래도 간단하게 샤브샤브 같은 건 만들어먹는데 담에 채소 잔뜩 섭취 하고 싶을때 해봐",
          "original_form": "그래도 간단하게 샤브샤브 같은 건 만들어먹는데 담에 채소 잔뜩 섭취 하고 싶을때 해봐",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007047.1.18",
          "form": "샤브샤브 어케해",
          "original_form": "샤브샤브 어케해",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007047.1.19",
          "form": "샤브샤브 생각보다 쉬웡 대신 재료가 많긴 하다",
          "original_form": "샤브샤브 생각보다 쉬웡 대신 재료가 많긴 하다",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007047.1.20",
          "form": "육수도 멸치육수 아니면 그냥 샤브용육수 팔고 원하는 채소랑 고기 넣구 펄펄 끊이면 완성",
          "original_form": "육수도 멸치육수 아니면 그냥 샤브용육수 팔고 원하는 채소랑 고기 넣구 펄펄 끊이면 완성",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007047.1.21",
          "form": "재료가 많아 응..ㅎ",
          "original_form": "재료가 많아 응..ㅎ",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007047.1.22",
          "form": "나는 재료 3개이상넘어가면안된다 ㅋㅋㅋㅋㅋ",
          "original_form": "나는 재료 3개이상넘어가면안된다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007047.1.23",
          "form": "걍먹고싶은거다넣고 끓이면되네",
          "original_form": "걍먹고싶은거다넣고 끓이면되네",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007047.1.24",
          "form": "앜ㅋㅋ 3개 넘 작지 않나 너무하잖엌ㅋㅋ",
          "original_form": "앜ㅋㅋ 3개 넘 작지 않나 너무하잖엌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007047.1.25",
          "form": "걍 먹고싶은거 때려 넣는거 맞지 ㅎㅋㅋ",
          "original_form": "걍 먹고싶은거 때려 넣는거 맞지 ㅎㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007047.1.26",
          "form": "샤브샤브 장점이란 말야",
          "original_form": "샤브샤브 장점이란 말야",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007047.1.27",
          "form": "그럼 머 좋아하는 거 있어???",
          "original_form": "그럼 머 좋아하는 거 있어???",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007047.1.28",
          "form": "가리는건없는데",
          "original_form": "가리는건없는데",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007047.1.29",
          "form": "집에서해먹는건 마파두부정도?? 소스랑 두부만 사서 볶는거",
          "original_form": "집에서해먹는건 마파두부정도?? 소스랑 두부만 사서 볶는거",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007047.1.30",
          "form": "나 요리유투브 보는건 좋아하는데 하진않네",
          "original_form": "나 요리유투브 보는건 좋아하는데 하진않네",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007047.1.31",
          "form": "마파두부 재료가 두개야ㅋㅋㅋㅋ진짜 3개 안넘네ㅋ큐ㅠ",
          "original_form": "마파두부 재료가 두개야ㅋㅋㅋㅋ진짜 3개 안넘네ㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007047.1.32",
          "form": "나두 요리튜브 좋아하는데 따라 해서 잘 된 꼴을 못보긴 했어",
          "original_form": "나두 요리튜브 좋아하는데 따라 해서 잘 된 꼴을 못보긴 했어",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007047.1.33",
          "form": "헐근데간단한것도 많잖아",
          "original_form": "헐근데간단한것도 많잖아",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007047.1.34",
          "form": "민트초코 좋아하니",
          "original_form": "민트초코 좋아하니",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
