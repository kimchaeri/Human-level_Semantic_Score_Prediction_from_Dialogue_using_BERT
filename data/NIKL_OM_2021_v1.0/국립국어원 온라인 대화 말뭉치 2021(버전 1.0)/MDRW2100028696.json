      "utterance": [
        {
          "id": "MDRW2100028696.1.1",
          "form": "옷이 너무 사고 싶어요..",
          "original_form": "옷이 너무 사고 싶어요..",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028696.1.2",
          "form": "옷은 아무리 사도 모자란 느낌이에요",
          "original_form": "옷은 아무리 사도 모자란 느낌이에요",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028696.1.3",
          "form": "맞아요! 뭔가 저번년도에 산 거 같은데 하나도 없고 그러네요...",
          "original_form": "맞아요! 뭔가 저번년도에 산 거 같은데 하나도 없고 그러네요...",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028696.1.4",
          "form": "전 이번에 정리를 해서 텅텅 빈 거 같아요",
          "original_form": "전 이번에 정리를 해서 텅텅 빈 거 같아요",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028696.1.5",
          "form": "진짜 사야되는데...",
          "original_form": "진짜 사야되는데...",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028696.1.6",
          "form": "슬슬 장만하셔야겠네요",
          "original_form": "슬슬 장만하셔야겠네요",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028696.1.7",
          "form": "할인하는데 찾아서 둘러보려구요ㅋㅋ",
          "original_form": "할인하는데 찾아서 둘러보려구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028696.1.8",
          "form": "요즘 옷 너무 비싸지 않나요ㅠㅠ",
          "original_form": "요즘 옷 너무 비싸지 않나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028696.1.9",
          "form": "네. 할인도 하나마나고 너무 저렴한건 별로 안좋고요",
          "original_form": "네. 할인도 하나마나고 너무 저렴한건 별로 안좋고요",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028696.1.10",
          "form": "그쵸 이게 너무 싼데 후기가 좋으면 이제 의심을 하는...",
          "original_form": "그쵸 이게 너무 싼데 후기가 좋으면 이제 의심을 하는...",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028696.1.11",
          "form": "잘 찾는것도 재능인거 같아요ㅋㅋ",
          "original_form": "잘 찾는것도 재능인거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028696.1.12",
          "form": "가서 보고 사는게 역시 좋긴하죠",
          "original_form": "가서 보고 사는게 역시 좋긴하죠",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028696.1.13",
          "form": "맞아요ㅋㅋ 입어볼 수 있는 가게로 가서 보는 게 좋은 것 같아요",
          "original_form": "맞아요ㅋㅋ 입어볼 수 있는 가게로 가서 보는 게 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028696.1.14",
          "form": "사고 싶은 옷 있어요?",
          "original_form": "사고 싶은 옷 있어요?",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028696.1.15",
          "form": "가디건 사보고 싶어요! 사실 한 번도 안 사봤거든요",
          "original_form": "가디건 사보고 싶어요! 사실 한 번도 안 사봤거든요",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028696.1.16",
          "form": "오버핏 유행이잖아요",
          "original_form": "오버핏 유행이잖아요",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100028696.1.17",
          "form": "입은거 보면 귀엽더라구요",
          "original_form": "입은거 보면 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100028696.1.18",
          "form": "맞아요,,!!! 브이넥 가디건도 사보고 싶어요",
          "original_form": "맞아요,,!!! 브이넥 가디건도 사보고 싶어요",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100028696.1.19",
          "form": "가디건 잘 사놓으면 계절 상관없이 입기 좋아요",
          "original_form": "가디건 잘 사놓으면 계절 상관없이 입기 좋아요",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100028696.1.20",
          "form": "여름에도 에어컨 바람때문에 잘 입었네요ㅋㅋ",
          "original_form": "여름에도 에어컨 바람때문에 잘 입었네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100028696.1.21",
          "form": "헉 뭔가 딱 걸치기 좋은 정도의 두께일까요! 잘 사고 싶어요 흑흑 맨날 옷 고르기 실패하는 ㅠㅠ",
          "original_form": "헉 뭔가 딱 걸치기 좋은 정도의 두께일까요! 잘 사고 싶어요 흑흑 맨날 옷 고르기 실패하는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100028696.1.22",
          "form": "몇번은 꼭 실패하게 되져...ㅎㅎ",
          "original_form": "몇번은 꼭 실패하게 되져...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100028696.1.23",
          "form": "맞아요ㅠㅠ 값이 싸지도 않아서 넘 슬퍼욕..",
          "original_form": "맞아요ㅠㅠ 값이 싸지도 않아서 넘 슬퍼욕..",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100028696.1.24",
          "form": "뭔가 오랜만에 옷 얘기해서 재밌네요",
          "original_form": "뭔가 오랜만에 옷 얘기해서 재밌네요",
