      "utterance": [
        {
          "id": "MDRW2100013683.1.1",
          "form": "공연좋아하시나요",
          "original_form": "공연좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013683.1.2",
          "form": "공연은 거의 본적이 없어요",
          "original_form": "공연은 거의 본적이 없어요",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013683.1.3",
          "form": "영화도요?",
          "original_form": "영화도요?",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013683.1.4",
          "form": "저는 bts콘서트 가보는것이 소원이었는데",
          "original_form": "저는 bts콘서트 가보는것이 소원이었는데",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013683.1.5",
          "form": "영원히 이루지못할것같네요",
          "original_form": "영원히 이루지못할것같네요",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013683.1.6",
          "form": "영화는 가끔보는 편이요\r\n방탄은 경쟁이 치열해서 힘들거같아요",
          "original_form": "영화는 가끔보는 편이요\r\n방탄은 경쟁이 치열해서 힘들거같아요",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013683.1.7",
          "form": "코로나라 극장가도 텅텅 비어있어서 재미가없네요",
          "original_form": "코로나라 극장가도 텅텅 비어있어서 재미가없네요",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100013683.1.8",
          "form": "네 진짜 거의 텅텅비엇더라고요\r\n그래도 이번에 조인성나온 모가디슈 관객 많이들엇대요",
          "original_form": "네 진짜 거의 텅텅비엇더라고요\r\n그래도 이번에 조인성나온 모가디슈 관객 많이들엇대요",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100013683.1.9",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100013683.1.10",
          "form": "아 많이들어봤어요  조금 잔인하지않나요?",
          "original_form": "아 많이들어봤어요  조금 잔인하지않나요?",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100013683.1.11",
          "form": "무서운 영화를 안좋아해서",
          "original_form": "무서운 영화를 안좋아해서",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100013683.1.12",
          "form": "저도 보진않았는데 아무래도 전쟁이니까 잔인하겟져",
          "original_form": "저도 보진않았는데 아무래도 전쟁이니까 잔인하겟져",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100013683.1.13",
          "form": "저도 무서운거 못봐여",
          "original_form": "저도 무서운거 못봐여",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100013683.1.14",
          "form": "네 저는 전쟁영화 잔인한영화 너무 무섭거든요.",
          "original_form": "네 저는 전쟁영화 잔인한영화 너무 무섭거든요.",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100013683.1.15",
          "form": "사람 목숨이 파리목숨처럼 하찮게 죽어나가서 너무 불편해요",
          "original_form": "사람 목숨이 파리목숨처럼 하찮게 죽어나가서 너무 불편해요",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100013683.1.16",
          "form": "그쳐 저도 그런건 싫더라고요",
          "original_form": "그쳐 저도 그런건 싫더라고요",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100013683.1.17",
          "form": "요즘은 워낙 잔인한 영화가 많아서 좀 그래요",
          "original_form": "요즘은 워낙 잔인한 영화가 많아서 좀 그래요",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100013683.1.18",
          "form": "네맞아요.",
          "original_form": "네맞아요.",
          "speaker_id": "1",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100013683.1.19",
          "form": "조폭영화 이런것들도 너무 잔인하고",
          "original_form": "조폭영화 이런것들도 너무 잔인하고",
          "speaker_id": "1",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100013683.1.20",
          "form": "네 그런영화 아니어도 욕이 일상으로 나오니까\r\n그런거 보기싫어서 극장도 점점 덜가게 되더라고요",
          "original_form": "네 그런영화 아니어도 욕이 일상으로 나오니까\r\n그런거 보기싫어서 극장도 점점 덜가게 되더라고요",
          "speaker_id": "2",
          "time": "20210826 08:20"
        },
        {
          "id": "MDRW2100013683.1.21",
          "form": "네 정말 힐링이 되고 내용좋은 그런 영화는 없을까요",
          "original_form": "네 정말 힐링이 되고 내용좋은 그런 영화는 없을까요",
          "speaker_id": "1",
          "time": "20210826 08:22"
        },
        {
          "id": "MDRW2100013683.1.22",
          "form": "어린이영화나 가능할까요 ㅎ",
          "original_form": "어린이영화나 가능할까요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 08:22"
        },
        {
          "id": "MDRW2100013683.1.23",
          "form": "그러게요 진짜 요즘 좋은영화 찾기 어려워요",
          "original_form": "그러게요 진짜 요즘 좋은영화 찾기 어려워요",
          "speaker_id": "2",
          "time": "20210826 08:23"
        }
      ]
    }
