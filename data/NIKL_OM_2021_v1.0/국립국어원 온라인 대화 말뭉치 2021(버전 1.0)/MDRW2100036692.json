      "utterance": [
        {
          "id": "MDRW2100036692.1.1",
          "form": "흥칫뿡야 라고 노래 아세요?",
          "original_form": "흥칫뿡야 라고 노래 아세요?",
          "speaker_id": "1",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100036692.1.2",
          "form": "아이돌 노래인데",
          "original_form": "아이돌 노래인데",
          "speaker_id": "1",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100036692.1.3",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "2",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100036692.1.4",
          "form": "오 ㅋㅋㅋㅋㅋ 저도 멜론으로 스밍",
          "original_form": "오 ㅋㅋㅋㅋㅋ 저도 멜론으로 스밍",
          "speaker_id": "2",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100036692.1.5",
          "form": "누구노래에요? 첨들어봐옄ㅋㅋㅋ",
          "original_form": "누구노래에요? 첨들어봐옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100036692.1.6",
          "form": "이름은 기억이 잘..",
          "original_form": "이름은 기억이 잘..",
          "speaker_id": "1",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100036692.1.7",
          "form": "노래가 통통튀는데 문세윤이 콜라보로 무대 같이한적있어요",
          "original_form": "노래가 통통튀는데 문세윤이 콜라보로 무대 같이한적있어요",
          "speaker_id": "1",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100036692.1.8",
          "form": "한번 보세요ㅋㅋㅋㅋ부끄뚱 하기 전단계?",
          "original_form": "한번 보세요ㅋㅋㅋㅋ부끄뚱 하기 전단계?",
          "speaker_id": "1",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100036692.1.9",
          "form": "악ㅋㅋㅋ봐볼게욬ㅋㅋㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋ봐볼게욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100036692.1.10",
          "form": "춤 진짜 잘춰요ㅋㅋㅋㅋ",
          "original_form": "춤 진짜 잘춰요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100036692.1.11",
          "form": "노래 중독성이니까 주의하세요",
          "original_form": "노래 중독성이니까 주의하세요",
          "speaker_id": "1",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100036692.1.12",
          "form": "이따 점심시간에 봐야겠어여 ㅋㅋㅋㅋ 히히",
          "original_form": "이따 점심시간에 봐야겠어여 ㅋㅋㅋㅋ 히히",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100036692.1.13",
          "form": "인사이드아웃 영화 아세요?",
          "original_form": "인사이드아웃 영화 아세요?",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100036692.1.14",
          "form": "오오 알져 ㅋㅋㅋ",
          "original_form": "오오 알져 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036692.1.15",
          "form": "버럭이 슬픔이 ㅋㅋㅋㅋㅋ",
          "original_form": "버럭이 슬픔이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036692.1.16",
          "form": "거기 빙봉 나오잖아요!",
          "original_form": "거기 빙봉 나오잖아요!",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100036692.1.17",
          "form": "문세윤이 흥칫뿡야 출때 완전 빙봉같아요ㅋㅋㅋㅋ",
          "original_form": "문세윤이 흥칫뿡야 출때 완전 빙봉같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100036692.1.18",
          "form": "악ㅋㅋㅋㅋㅋ 닮은느낌 뭔지 알거같아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋㅋㅋ 닮은느낌 뭔지 알거같아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100036692.1.19",
          "form": "라비 목표가 뮤비 100만이던데",
          "original_form": "라비 목표가 뮤비 100만이던데",
          "speaker_id": "1",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100036692.1.20",
          "form": "생각난김에 보고와야겠어요ㅋㅋ",
          "original_form": "생각난김에 보고와야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:54"
        },
        {
          "id": "MDRW2100036692.1.21",
          "form": "저는 요즘 1박2일 안봐서 몰랐는데 둘다 거기 멤바인가봐요?? 댓글보니까 ㅋㅋㅋ",
          "original_form": "저는 요즘 1박2일 안봐서 몰랐는데 둘다 거기 멤바인가봐요?? 댓글보니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:59"
        },
        {
          "id": "MDRW2100036692.1.22",
          "form": "네네 요즘 1박2일 완전재밋어요",
          "original_form": "네네 요즘 1박2일 완전재밋어요",
          "speaker_id": "1",
          "time": "20210901 06:19"
        },
        {
          "id": "MDRW2100036692.1.23",
          "form": "기싸움하는것도 없고",
          "original_form": "기싸움하는것도 없고",
          "speaker_id": "1",
          "time": "20210901 06:19"
        },
        {
          "id": "MDRW2100036692.1.24",
          "form": "1박2일 레전드편 몇개 있어요",
          "original_form": "1박2일 레전드편 몇개 있어요",
          "speaker_id": "1",
          "time": "20210901 06:20"
        },
        {
          "id": "MDRW2100036692.1.25",
          "form": "시간날때 함봐보세요 ㅡㅋㅋㅋㅋㅋㅋ",
          "original_form": "시간날때 함봐보세요 ㅡㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 06:20"
        },
        {
          "id": "MDRW2100036692.1.26",
          "form": "방토피아편도 추천합니다",
          "original_form": "방토피아편도 추천합니다",
