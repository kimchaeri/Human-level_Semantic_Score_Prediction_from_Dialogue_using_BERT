      "utterance": [
        {
          "id": "MDRW2100005866.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100005866.1.2",
          "form": "안좋아하는데 요즘은 가고싶네요",
          "original_form": "안좋아하는데 요즘은 가고싶네요",
          "speaker_id": "2",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100005866.1.3",
          "form": "아 여행 안좋아하셔요?",
          "original_form": "아 여행 안좋아하셔요?",
          "speaker_id": "1",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100005866.1.4",
          "form": "요즘 어디로 떠나구싶으신가요",
          "original_form": "요즘 어디로 떠나구싶으신가요",
          "speaker_id": "1",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100005866.1.5",
          "form": "제주도나 강릉 가고싶어여",
          "original_form": "제주도나 강릉 가고싶어여",
          "speaker_id": "2",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100005866.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100005866.1.7",
          "form": "바다쪽을 가구싶으시구나",
          "original_form": "바다쪽을 가구싶으시구나",
          "speaker_id": "1",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100005866.1.8",
          "form": "원래는 왜 안좋아하셨어요?",
          "original_form": "원래는 왜 안좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100005866.1.9",
          "form": "네 바다 좋아해요",
          "original_form": "네 바다 좋아해요",
          "speaker_id": "2",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100005866.1.10",
          "form": "원래 밖을 잘 안좋아해요 ㅋㅋ",
          "original_form": "원래 밖을 잘 안좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100005866.1.11",
          "form": "ㅋㅋㅋㅋ집콕러 시구나",
          "original_form": "ㅋㅋㅋㅋ집콕러 시구나",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100005866.1.12",
          "form": "전되게 활동적이라 생각함에도 불구하고 이렇게 비오는날만 되면",
          "original_form": "전되게 활동적이라 생각함에도 불구하고 이렇게 비오는날만 되면",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100005866.1.13",
          "form": "집에 있고싶은거같아요",
          "original_form": "집에 있고싶은거같아요",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100005866.1.14",
          "form": "아 그쵸 비올 때 돌아다니는 거 배로 힘들어요",
          "original_form": "아 그쵸 비올 때 돌아다니는 거 배로 힘들어요",
          "speaker_id": "2",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100005866.1.15",
          "form": "진짜그런거같아요",
          "original_form": "진짜그런거같아요",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005866.1.16",
          "form": "가보셨던 여행지중에",
          "original_form": "가보셨던 여행지중에",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005866.1.17",
          "form": "기억에 남으시는곳 있으신가요?",
          "original_form": "기억에 남으시는곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005866.1.18",
          "form": "제주도 바다가 진짜 예뻤어요",
          "original_form": "제주도 바다가 진짜 예뻤어요",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005866.1.19",
          "form": "아 제주도 제가진짜 좋아하는 여행지죠ㅜㅜ",
          "original_form": "아 제주도 제가진짜 좋아하는 여행지죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005866.1.20",
          "form": "우리나라에 있는 외국같은 느낌이랄까",
          "original_form": "우리나라에 있는 외국같은 느낌이랄까",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100005866.1.21",
          "form": "해외여행 좀 가셨나요?",
          "original_form": "해외여행 좀 가셨나요?",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100005866.1.22",
          "form": "여행을 많이좋아해서 좀 다녀온거같아요",
          "original_form": "여행을 많이좋아해서 좀 다녀온거같아요",
