      "utterance": [
        {
          "id": "MDRW2100046891.1.1",
          "form": "어떻게보시나여?",
          "original_form": "어떻게보시나여?",
          "speaker_id": "1",
          "time": "20210930 17:50"
        },
        {
          "id": "MDRW2100046891.1.2",
          "form": "재밌을것 같은데 얼마일까요?",
          "original_form": "재밌을것 같은데 얼마일까요?",
          "speaker_id": "2",
          "time": "20210930 17:51"
        },
        {
          "id": "MDRW2100046891.1.3",
          "form": "사람 선정 기준도 구금",
          "original_form": "사람 선정 기준도 구금",
          "speaker_id": "2",
          "time": "20210930 17:51"
        },
        {
          "id": "MDRW2100046891.1.4",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "2",
          "time": "20210930 17:51"
        },
        {
          "id": "MDRW2100046891.1.5",
          "form": "일론머스크가하는거라서",
          "original_form": "일론머스크가하는거라서",
          "speaker_id": "1",
          "time": "20210930 17:58"
        },
        {
          "id": "MDRW2100046891.1.6",
          "form": "어짜피 연구결과도 함께하는거라",
          "original_form": "어짜피 연구결과도 함께하는거라",
          "speaker_id": "1",
          "time": "20210930 17:59"
        },
        {
          "id": "MDRW2100046891.1.7",
          "form": "그럼 무료인가요?",
          "original_form": "그럼 무료인가요?",
          "speaker_id": "2",
          "time": "20210930 18:01"
        },
        {
          "id": "MDRW2100046891.1.8",
          "form": "찾아보니 선정하는거같아요",
          "original_form": "찾아보니 선정하는거같아요",
          "speaker_id": "1",
          "time": "20210930 18:02"
        },
        {
          "id": "MDRW2100046891.1.9",
          "form": "오.. 엄청난 행운이겠네요",
          "original_form": "오.. 엄청난 행운이겠네요",
          "speaker_id": "2",
          "time": "20210930 18:11"
        },
        {
          "id": "MDRW2100046891.1.10",
          "form": "근데 일론머스크 정말 다양한거 많이하네요",
          "original_form": "근데 일론머스크 정말 다양한거 많이하네요",
          "speaker_id": "2",
          "time": "20210930 18:11"
        },
        {
          "id": "MDRW2100046891.1.11",
          "form": "비트코인도 일론머스크때문에 좌지우지해여 ㅎ",
          "original_form": "비트코인도 일론머스크때문에 좌지우지해여 ㅎ",
          "speaker_id": "1",
          "time": "20210930 18:12"
        },
        {
          "id": "MDRW2100046891.1.12",
          "form": "그러게요. 우주관광은 어떤 기분일까요 ㅎㅎ 가보고싶네요",
          "original_form": "그러게요. 우주관광은 어떤 기분일까요 ㅎㅎ 가보고싶네요",
          "speaker_id": "2",
          "time": "20210930 18:19"
        },
        {
          "id": "MDRW2100046891.1.13",
          "form": "조만간 누구나 갈수있는 그런세상이 올수도있어요",
          "original_form": "조만간 누구나 갈수있는 그런세상이 올수도있어요",
          "speaker_id": "1",
          "time": "20210930 18:20"
        },
        {
          "id": "MDRW2100046891.1.14",
          "form": "ㅋㅋ그때까지 저희가 살아있을까요?",
          "original_form": "ㅋㅋ그때까지 저희가 살아있을까요?",
          "speaker_id": "2",
          "time": "20210930 18:28"
        },
        {
          "id": "MDRW2100046891.1.15",
          "form": "꼭 가보고 싶은데",
          "original_form": "꼭 가보고 싶은데",
          "speaker_id": "2",
          "time": "20210930 18:28"
        },
        {
          "id": "MDRW2100046891.1.16",
          "form": "저한테도 기회가 오길 ㅋㅋ",
          "original_form": "저한테도 기회가 오길 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210930 18:28"
        },
        {
          "id": "MDRW2100046891.1.17",
          "form": "전기자동차나오고 5g세상나온지 얼마안됐는데 충분히가능해요",
          "original_form": "전기자동차나오고 5g세상나온지 얼마안됐는데 충분히가능해요",
          "speaker_id": "1",
          "time": "20210930 18:30"
        },
        {
          "id": "MDRW2100046891.1.18",
          "form": "아직 민간인 선정기준은 안나온거죠? 언제가는지도?",
          "original_form": "아직 민간인 선정기준은 안나온거죠? 언제가는지도?",
          "speaker_id": "2",
          "time": "20210930 18:48"
        },
        {
          "id": "MDRW2100046891.1.19",
          "form": "그런거같아요 조만간 선정기준나오겠네요",
