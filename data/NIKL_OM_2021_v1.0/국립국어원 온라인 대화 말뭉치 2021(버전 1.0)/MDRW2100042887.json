      "utterance": [
        {
          "id": "MDRW2100042887.1.1",
          "form": "그럼 내년 언제까지 계시는건가요?",
          "original_form": "그럼 내년 언제까지 계시는건가요?",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042887.1.2",
          "form": "네 내년 3월까지요",
          "original_form": "네 내년 3월까지요",
          "speaker_id": "1",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042887.1.3",
          "form": "얼마나 다닐수 있을지 모르겠어요",
          "original_form": "얼마나 다닐수 있을지 모르겠어요",
          "speaker_id": "1",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042887.1.4",
          "form": "벌써 반년정도 계셨군요",
          "original_form": "벌써 반년정도 계셨군요",
          "speaker_id": "2",
          "time": "20210907 10:59"
        },
        {
          "id": "MDRW2100042887.1.5",
          "form": "아뇨 내려온지 3주요",
          "original_form": "아뇨 내려온지 3주요",
          "speaker_id": "1",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042887.1.6",
          "form": "이제 여행은 시작이예요",
          "original_form": "이제 여행은 시작이예요",
          "speaker_id": "1",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042887.1.7",
          "form": "6개월간",
          "original_form": "6개월간",
          "speaker_id": "2",
          "time": "20210907 11:03"
        },
        {
          "id": "MDRW2100042887.1.8",
          "form": "후회없이 신나게",
          "original_form": "후회없이 신나게",
          "speaker_id": "2",
          "time": "20210907 11:04"
        },
        {
          "id": "MDRW2100042887.1.9",
          "form": "제 몫까지 놀아주세요(?)",
          "original_form": "제 몫까지 놀아주세요(?)",
          "speaker_id": "2",
          "time": "20210907 11:04"
        },
        {
          "id": "MDRW2100042887.1.10",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:04"
        },
        {
          "id": "MDRW2100042887.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100042887.1.12",
          "form": "제주오면 가고싶은 곳 있으세요",
          "original_form": "제주오면 가고싶은 곳 있으세요",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042887.1.13",
          "form": "네 저는 희한하게 종달리가 왜그렇게 좋은지 모르겠어요",
          "original_form": "네 저는 희한하게 종달리가 왜그렇게 좋은지 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 11:12"
        },
        {
          "id": "MDRW2100042887.1.14",
          "form": "되게 작은 시골마을이거든요? 현지인들도 시골마을이라고 하는",
          "original_form": "되게 작은 시골마을이거든요? 현지인들도 시골마을이라고 하는",
          "speaker_id": "2",
          "time": "20210907 11:12"
        },
        {
          "id": "MDRW2100042887.1.15",
          "form": "근데 거기서 몇년전에 이틀정도 있었는데 그때 기억이 너무 좋았나봐요",
          "original_form": "근데 거기서 몇년전에 이틀정도 있었는데 그때 기억이 너무 좋았나봐요",
          "speaker_id": "2",
          "time": "20210907 11:12"
        },
        {
          "id": "MDRW2100042887.1.16",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042887.1.17",
          "form": "저는 위미리요 항구마을",
          "original_form": "저는 위미리요 항구마을",
          "speaker_id": "1",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042887.1.18",
          "form": "종달리? 한번 가봐야겠네요",
          "original_form": "종달리? 한번 가봐야겠네요",
          "speaker_id": "1",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042887.1.19",
          "form": "위미리! 처음들어보는데 항구마을이라니 궁금해요",
          "original_form": "위미리! 처음들어보는데 항구마을이라니 궁금해요",
          "speaker_id": "2",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042887.1.20",
          "form": "종달리 엄청 조용하고 평화로워요 ㅎㅎ",
          "original_form": "종달리 엄청 조용하고 평화로워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042887.1.21",
          "form": "근데 거기서 우도가는 선착장도 있어요",
          "original_form": "근데 거기서 우도가는 선착장도 있어요",
          "speaker_id": "2",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042887.1.22",
          "form": "아라",
          "original_form": "아라",
          "speaker_id": "1",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042887.1.23",
          "form": "우도 좋쵸",
          "original_form": "우도 좋쵸",
          "speaker_id": "1",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042887.1.24",
          "form": "크 해변",
          "original_form": "크 해변",
          "speaker_id": "1",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042887.1.25",
          "form": "제주도 항상 짧게만 있어서 많이 못본거같아요",
          "original_form": "제주도 항상 짧게만 있어서 많이 못본거같아요",
          "speaker_id": "2",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100042887.1.26",
          "form": "나중엔 좀 오래 머물면서 여유있게 보규싶네요 ㅠㅠ",
          "original_form": "나중엔 좀 오래 머물면서 여유있게 보규싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100042887.1.27",
          "form": "오래보면 지겨울지도? ㅎㅎ",
          "original_form": "오래보면 지겨울지도? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042887.1.28",
          "form": "ㅋㅋㅋ너무 이벤트가 없나요",
          "original_form": "ㅋㅋㅋ너무 이벤트가 없나요",
          "speaker_id": "2",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042887.1.29",
          "form": "저는 십년전인가 친구랑 둘이 겨울에 ㅋㅋㅋㅋ 뚜벅이로 여행갔다가",
          "original_form": "저는 십년전인가 친구랑 둘이 겨울에 ㅋㅋㅋㅋ 뚜벅이로 여행갔다가",
          "speaker_id": "2",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042887.1.30",
          "form": "진짜 힘들어 디1지는 줄 알았거든요",
          "original_form": "진짜 힘들어 디1지는 줄 알았거든요",
          "speaker_id": "2",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042887.1.31",
          "form": "그땐 둘다 면허도 ㄴ없엇어가지고 ㅋㅋㅋㅋ",
          "original_form": "그땐 둘다 면허도 ㄴ없엇어가지고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:38"
        },
        {
          "id": "MDRW2100042887.1.32",
          "form": "그래서 그 때 이후로 저는 차가 필수임을 뼈저리게 느꼈어요..",
          "original_form": "그래서 그 때 이후로 저는 차가 필수임을 뼈저리게 느꼈어요..",
          "speaker_id": "2",
          "time": "20210907 11:38"
        },
        {
          "id": "MDRW2100042887.1.33",
          "form": "아마 무리한 스케줄 때문이지 않았을까 생각해봅니다",
          "original_form": "아마 무리한 스케줄 때문이지 않았을까 생각해봅니다",
          "speaker_id": "1",
          "time": "20210907 11:44"
        }
      ]
