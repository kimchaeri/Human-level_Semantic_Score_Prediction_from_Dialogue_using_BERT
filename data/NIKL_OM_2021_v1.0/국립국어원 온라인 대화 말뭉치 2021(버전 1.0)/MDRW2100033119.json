      "utterance": [
        {
          "id": "MDRW2100033119.1.1",
          "form": "동아리 뭐하세요?",
          "original_form": "동아리 뭐하세요?",
          "speaker_id": "2",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100033119.1.2",
          "form": "저는 강변걷기 하고있어용",
          "original_form": "저는 강변걷기 하고있어용",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100033119.1.3",
          "form": "ㄱ주변에 강변이 있나봐요",
          "original_form": "ㄱ주변에 강변이 있나봐요",
          "speaker_id": "2",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100033119.1.4",
          "form": "근데 솔직히 걷기 귀찮아요..",
          "original_form": "근데 솔직히 걷기 귀찮아요..",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100033119.1.5",
          "form": "저도 예전에 걷기반이었는데",
          "original_form": "저도 예전에 걷기반이었는데",
          "speaker_id": "2",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100033119.1.6",
          "form": "맨날 똑같은 데만 걸어서..",
          "original_form": "맨날 똑같은 데만 걸어서..",
          "speaker_id": "2",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100033119.1.7",
          "form": "그마음 이해합니다..",
          "original_form": "그마음 이해합니다..",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100033119.1.8",
          "form": "다음엔 다른 동아리로",
          "original_form": "다음엔 다른 동아리로",
          "speaker_id": "2",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100033119.1.9",
          "form": "가세요",
          "original_form": "가세요",
          "speaker_id": "2",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100033119.1.10",
          "form": "그래야겠네요",
          "original_form": "그래야겠네요",
          "speaker_id": "1",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100033119.1.11",
          "form": "급식은 맛있나요",
          "original_form": "급식은 맛있나요",
          "speaker_id": "2",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100033119.1.12",
          "form": "초등학교때보다 맛있어요",
          "original_form": "초등학교때보다 맛있어요",
          "speaker_id": "1",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100033119.1.13",
          "form": "초등학교 때는 별로였어요?",
          "original_form": "초등학교 때는 별로였어요?",
          "speaker_id": "2",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100033119.1.14",
          "form": "너무 맛이 없었어요",
          "original_form": "너무 맛이 없었어요",
          "speaker_id": "1",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100033119.1.15",
          "form": "근데 사실 기억이 안나요 ㅋㅋㅋ",
          "original_form": "근데 사실 기억이 안나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100033119.1.16",
          "form": "아ㅠㅠ 아무래도 그렇죠 기억이..",
          "original_form": "아ㅠㅠ 아무래도 그렇죠 기억이..",
          "speaker_id": "2",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100033119.1.17",
          "form": "정확한 맛이 기억이 안나네오",
          "original_form": "정확한 맛이 기억이 안나네오",
          "speaker_id": "1",
          "time": "20210906 17:12"
        }
