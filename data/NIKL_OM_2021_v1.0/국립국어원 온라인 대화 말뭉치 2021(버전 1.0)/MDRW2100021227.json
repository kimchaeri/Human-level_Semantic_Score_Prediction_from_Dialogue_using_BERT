      "utterance": [
        {
          "id": "MDRW2100021227.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021227.1.2",
          "form": "다이어트나 운동하시나요?",
          "original_form": "다이어트나 운동하시나요?",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021227.1.3",
          "form": "운동하고있어요! 식이는 안하구요ㅋㅋ",
          "original_form": "운동하고있어요! 식이는 안하구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021227.1.4",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021227.1.5",
          "form": "네 저도 운동만해요ㅎㅎ",
          "original_form": "네 저도 운동만해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021227.1.6",
          "form": "식이는 맨날 계획만 해서 ㅠ",
          "original_form": "식이는 맨날 계획만 해서 ㅠ",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021227.1.7",
          "form": "무슨운동하세요",
          "original_form": "무슨운동하세요",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021227.1.8",
          "form": "식이는 했다가도 다시 돌아와서 안하는게 낫겠더라구오ㅠ",
          "original_form": "식이는 했다가도 다시 돌아와서 안하는게 낫겠더라구오ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021227.1.9",
          "form": "필라테스하고 있어요!",
          "original_form": "필라테스하고 있어요!",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021227.1.10",
          "form": "좀 강제성이 있어야 하게 되더라고요",
          "original_form": "좀 강제성이 있어야 하게 되더라고요",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021227.1.11",
          "form": "오 필라테스 재밌나요?",
          "original_form": "오 필라테스 재밌나요?",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021227.1.12",
          "form": "저도 해보고싶은데 뭔가 옷도 사야할거같고",
          "original_form": "저도 해보고싶은데 뭔가 옷도 사야할거같고",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021227.1.13",
          "form": "장벽이 있더라구요ㅠ",
          "original_form": "장벽이 있더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021227.1.14",
          "form": "재미가 막 있지는 않은데 효과가 좋아요",
          "original_form": "재미가 막 있지는 않은데 효과가 좋아요",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021227.1.15",
          "form": "마자요 그래서 피티받을때는 좀  빠졌는데ㅠ",
          "original_form": "마자요 그래서 피티받을때는 좀  빠졌는데ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021227.1.16",
          "form": "근육 많이 느나요?",
          "original_form": "근육 많이 느나요?",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021227.1.17",
          "form": "살이 빠지는 건 잘 모르겠지만",
          "original_form": "살이 빠지는 건 잘 모르겠지만",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021227.1.18",
          "form": "근육은 좀 느는거같고",
          "original_form": "근육은 좀 느는거같고",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021227.1.19",
          "form": "제가 허리가 좀 안좋은데",
          "original_form": "제가 허리가 좀 안좋은데",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021227.1.20",
          "form": "확실히 그런건 좋더라고요",
          "original_form": "확실히 그런건 좋더라고요",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021227.1.21",
          "form": "오 역시 필라테스도 허리에 좋군요",
          "original_form": "오 역시 필라테스도 허리에 좋군요",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021227.1.22",
          "form": "저도 허리아파서 피티했었는데",
          "original_form": "저도 허리아파서 피티했었는데",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021227.1.23",
          "form": "확실히 근육이 생기니까 허리도 덜아프더라구요",
          "original_form": "확실히 근육이 생기니까 허리도 덜아프더라구요",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021227.1.24",
          "form": "맞아요 근육이 좀 있어야하더라고요",
          "original_form": "맞아요 근육이 좀 있어야하더라고요",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021227.1.25",
          "form": "피티만큼 근육이 딱!생기는건 아닌데",
          "original_form": "피티만큼 근육이 딱!생기는건 아닌데",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021227.1.26",
          "form": "전체적으로 밸런스가 좀 맞아지는 느낌이에요",
          "original_form": "전체적으로 밸런스가 좀 맞아지는 느낌이에요",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021227.1.27",
          "form": "필테는 속근육 생긴다그러더라구요",
          "original_form": "필테는 속근육 생긴다그러더라구요",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021227.1.28",
          "form": "교정에도 좋겠네요",
          "original_form": "교정에도 좋겠네요",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021227.1.29",
          "form": "네 처음에은 살빼려고 하긴했는데 ㅎㅎ",
          "original_form": "네 처음에은 살빼려고 하긴했는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021227.1.30",
          "form": "지금은 자세교정때문에 계속하고 있어요",
          "original_form": "지금은 자세교정때문에 계속하고 있어요",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021227.1.31",
          "form": "저도 자세가 안좋아서 교정받아야하는데ㅠ",
          "original_form": "저도 자세가 안좋아서 교정받아야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:08"
        }
      ]
    }
