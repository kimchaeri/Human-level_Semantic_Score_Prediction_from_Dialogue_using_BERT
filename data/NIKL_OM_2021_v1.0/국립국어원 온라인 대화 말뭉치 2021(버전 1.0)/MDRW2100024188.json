      "utterance": [
        {
          "id": "MDRW2100024188.1.1",
          "form": "요즘 재택근무하시나요",
          "original_form": "요즘 재택근무하시나요",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024188.1.2",
          "form": "아니요 저는 출근해요",
          "original_form": "아니요 저는 출근해요",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024188.1.3",
          "form": "저는 구직 중인데 친구들이 재택을 해요",
          "original_form": "저는 구직 중인데 친구들이 재택을 해요",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024188.1.4",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024188.1.5",
          "form": "저도 재택하고싶어요",
          "original_form": "저도 재택하고싶어요",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024188.1.6",
          "form": "근데 오히려 시간이 없대요",
          "original_form": "근데 오히려 시간이 없대요",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024188.1.7",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024188.1.8",
          "form": "뭔가 더 감시받는 느낌이래요",
          "original_form": "뭔가 더 감시받는 느낌이래요",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024188.1.9",
          "form": "아 그럴수도있겠네요",
          "original_form": "아 그럴수도있겠네요",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024188.1.10",
          "form": "그래도 출근 시간이 없는 건 좋대요",
          "original_form": "그래도 출근 시간이 없는 건 좋대요",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024188.1.11",
          "form": "그게 재택의 큰 장점인 것 같아요",
          "original_form": "그게 재택의 큰 장점인 것 같아요",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024188.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024188.1.13",
          "form": "그래도 전 재택하고 싶네요",
          "original_form": "그래도 전 재택하고 싶네요",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024188.1.14",
          "form": "재택 근무하면 편할 것 같긴해요",
          "original_form": "재택 근무하면 편할 것 같긴해요",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024188.1.15",
          "form": "전 오히려 업무 효율도 늘 것 같아요",
          "original_form": "전 오히려 업무 효율도 늘 것 같아요",
          "speaker_id": "1",
          "time": "20210830 23:04"
