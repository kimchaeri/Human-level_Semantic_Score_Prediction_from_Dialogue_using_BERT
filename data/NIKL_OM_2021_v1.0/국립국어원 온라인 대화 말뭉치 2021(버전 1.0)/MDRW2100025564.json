      "utterance": [
        {
          "id": "MDRW2100025564.1.1",
          "form": "네 함께 키웠어요",
          "original_form": "네 함께 키웠어요",
          "speaker_id": "2",
          "time": "20210901 13:30"
        },
        {
          "id": "MDRW2100025564.1.2",
          "form": "갈수록 정들어요 강아지도 냥이도",
          "original_form": "갈수록 정들어요 강아지도 냥이도",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025564.1.3",
          "form": "저는 가족들이 싫어해서요ㅜ",
          "original_form": "저는 가족들이 싫어해서요ㅜ",
          "speaker_id": "1",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025564.1.4",
          "form": "키우려면 독립해야하는데",
          "original_form": "키우려면 독립해야하는데",
          "speaker_id": "1",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025564.1.5",
          "form": "그러면 못키우죠",
          "original_form": "그러면 못키우죠",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025564.1.6",
          "form": "근데 데려오면 안이뻐할수없어요ㅎ",
          "original_form": "근데 데려오면 안이뻐할수없어요ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025564.1.7",
          "form": "안그래도 엄마가 한때 우울증오셨을때",
          "original_form": "안그래도 엄마가 한때 우울증오셨을때",
          "speaker_id": "1",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100025564.1.8",
          "form": "그냥 확 데려와버릴까",
          "original_form": "그냥 확 데려와버릴까",
          "speaker_id": "1",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025564.1.9",
          "form": "푸들이 좋을것같아",
          "original_form": "푸들이 좋을것같아",
          "speaker_id": "1",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025564.1.10",
          "form": "푸들 이뻐요",
          "original_form": "푸들 이뻐요",
          "speaker_id": "2",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025564.1.11",
          "form": "이런생각까지했거든요 ㅋㅋ",
          "original_form": "이런생각까지했거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025564.1.12",
          "form": "성격좋구요.",
          "original_form": "성격좋구요.",
          "speaker_id": "2",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025564.1.13",
          "form": "요키는 조금 새초롬했어요",
          "original_form": "요키는 조금 새초롬했어요",
          "speaker_id": "2",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025564.1.14",
          "form": "안그래도 푸들들이 애교가많아서 어르신들이좋아하신단 말도들었고",
          "original_form": "안그래도 푸들들이 애교가많아서 어르신들이좋아하신단 말도들었고",
          "speaker_id": "1",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025564.1.15",
          "form": "하 ㅋㅋ근데 미스터트롯이 시작했네요? ㅋㅋㅋ",
          "original_form": "하 ㅋㅋ근데 미스터트롯이 시작했네요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025564.1.16",
          "form": "네대체로 건강해요",
          "original_form": "네대체로 건강해요",
          "speaker_id": "2",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025564.1.17",
          "form": "우울증엔 동물과 트로트 최고 ㅎㅎ",
          "original_form": "우울증엔 동물과 트로트 최고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100025564.1.18",
          "form": "저희 어머님과 비슷하시군요ㅎ",
          "original_form": "저희 어머님과 비슷하시군요ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:37"
        },
        {
          "id": "MDRW2100025564.1.19",
          "form": "언젠가 꼭 키우세요",
          "original_form": "언젠가 꼭 키우세요",
          "speaker_id": "2",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025564.1.20",
          "form": "시고르자브종도 똑똑하고 이뻐요",
          "original_form": "시고르자브종도 똑똑하고 이뻐요",
          "speaker_id": "2",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025564.1.21",
          "form": "영웅이가 건행하면서 엄마를 치유했어요ㄷㄷㄷ",
          "original_form": "영웅이가 건행하면서 엄마를 치유했어요ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100025564.1.22",
          "form": "무시무시한 임영웅님 효과",
          "original_form": "무시무시한 임영웅님 효과",
          "speaker_id": "1",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100025564.1.23",
          "form": "반려동물 주제여야한대요ㅜ트로트주제있나요. 어무닌 찬원이가 으뜸이에요",
          "original_form": "반려동물 주제여야한대요ㅜ트로트주제있나요. 어무닌 찬원이가 으뜸이에요",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025564.1.24",
          "form": "요즘 동네에 유기견은 안보여요.",
          "original_form": "요즘 동네에 유기견은 안보여요.",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025564.1.25",
          "form": "흠 저도 강아지는 못본것같아요",
          "original_form": "흠 저도 강아지는 못본것같아요",
          "speaker_id": "1",
          "time": "20210901 13:47"
        },
        {
          "id": "MDRW2100025564.1.26",
          "form": "고양이는 있어도",
          "original_form": "고양이는 있어도",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025564.1.27",
          "form": "아 그래서 저는 아버지께서 귀국하시는걸 노리고있어요 ㅋㅋ",
          "original_form": "아 그래서 저는 아버지께서 귀국하시는걸 노리고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025564.1.28",
          "form": "아프면 병원비가 비싸니 자금적으로도 여유되면 입양해야겠더라구요.",
          "original_form": "아프면 병원비가 비싸니 자금적으로도 여유되면 입양해야겠더라구요.",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100025564.1.29",
          "form": "전 아버지  은퇴하시면 한번더 권해보려고요 적적하실테니",
          "original_form": "전 아버지  은퇴하시면 한번더 권해보려고요 적적하실테니",
          "speaker_id": "1",
          "time": "20210901 13:58"
        }
      ]
    }
