      "utterance": [
        {
          "id": "MDRW2100016608.1.1",
          "form": "커피마시면 화장실 너무 자주 가",
          "original_form": "커피마시면 화장실 너무 자주 가",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016608.1.2",
          "form": "나 요즘 오트라떼 꽂혔댔자나",
          "original_form": "나 요즘 오트라떼 꽂혔댔자나",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016608.1.3",
          "form": "엉 계속해",
          "original_form": "엉 계속해",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016608.1.4",
          "form": "근데 스벅에 안 들어옮",
          "original_form": "근데 스벅에 안 들어옮",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016608.1.5",
          "form": "오트라떼가 스벅거 아니야?",
          "original_form": "오트라떼가 스벅거 아니야?",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016608.1.6",
          "form": "오트밀크가 수급이 어려워서 안된대더라",
          "original_form": "오트밀크가 수급이 어려워서 안된대더라",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016608.1.7",
          "form": "집에 귀리우유는 먹어봄?",
          "original_form": "집에 귀리우유는 먹어봄?",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016608.1.8",
          "form": "ㅇㅇ진작 먹어봤지",
          "original_form": "ㅇㅇ진작 먹어봤지",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016608.1.9",
          "form": "맛이 어때",
          "original_form": "맛이 어때",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016608.1.10",
          "form": "그건 엄청 되직하고 진해",
          "original_form": "그건 엄청 되직하고 진해",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016608.1.11",
          "form": "고소해?",
          "original_form": "고소해?",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016608.1.12",
          "form": "고소함ㅇㅇ 그치만 커피타기에 적합한것 같진않아",
          "original_form": "고소함ㅇㅇ 그치만 커피타기에 적합한것 같진않아",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016608.1.13",
          "form": "달지는 않아?",
          "original_form": "달지는 않아?",
          "speaker_id": "2",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016608.1.14",
          "form": "좀 단맛이 있지",
          "original_form": "좀 단맛이 있지",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016608.1.15",
          "form": "귀리우유가 칼로리가 우유보다 낮음",
          "original_form": "귀리우유가 칼로리가 우유보다 낮음",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016608.1.16",
          "form": "오트가 귀리아니야? 근데 커피랑은 별로 안어울려?",
          "original_form": "오트가 귀리아니야? 근데 커피랑은 별로 안어울려?",
          "speaker_id": "2",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016608.1.17",
          "form": "서울우유거가 안어울리는 것 같음",
          "original_form": "서울우유거가 안어울리는 것 같음",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016608.1.18",
          "form": "달달하다며 칼로리가 낮을수있나?",
          "original_form": "달달하다며 칼로리가 낮을수있나?",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016608.1.19",
          "form": "달달한맛이 있는데 당이 높진 않더라고",
          "original_form": "달달한맛이 있는데 당이 높진 않더라고",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016608.1.20",
          "form": "오 좋은데? 나도 먹어봐야지~",
          "original_form": "오 좋은데? 나도 먹어봐야지~",
          "speaker_id": "2",
          "time": "20210826 14:05"
        }
      ]
    }
