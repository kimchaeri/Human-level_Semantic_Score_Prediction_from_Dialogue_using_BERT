      "utterance": [
        {
          "id": "MDRW2100036800.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036800.1.2",
          "form": "카페 자주가세요?",
          "original_form": "카페 자주가세요?",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036800.1.3",
          "form": "자주안가요.",
          "original_form": "자주안가요.",
          "speaker_id": "2",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036800.1.4",
          "form": "싫어하시나?",
          "original_form": "싫어하시나?",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036800.1.5",
          "form": "요즘 코로나라 못가요 ㅠㅜ",
          "original_form": "요즘 코로나라 못가요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036800.1.6",
          "form": "코로나때 가보신적 없으세요?",
          "original_form": "코로나때 가보신적 없으세요?",
          "speaker_id": "1",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036800.1.7",
          "form": "제일 최근엔 메가커피 갔엇어요",
          "original_form": "제일 최근엔 메가커피 갔엇어요",
          "speaker_id": "2",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036800.1.8",
          "form": "인상깊은 음료 잇나요?",
          "original_form": "인상깊은 음료 잇나요?",
          "speaker_id": "1",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036800.1.9",
          "form": "메가리카노라고 아메리카노 대용량인데\r\n연하지만 맛잇더라고교",
          "original_form": "메가리카노라고 아메리카노 대용량인데\r\n연하지만 맛잇더라고교",
          "speaker_id": "2",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036800.1.10",
          "form": "바나나 조리퐁 아세요?",
          "original_form": "바나나 조리퐁 아세요?",
          "speaker_id": "1",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036800.1.11",
          "form": "메가리카노랑 바나나 퐁크러쉬 맛잇엇어요~",
          "original_form": "메가리카노랑 바나나 퐁크러쉬 맛잇엇어요~",
          "speaker_id": "2",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036800.1.12",
          "form": "집에서 만들수있는 맛이던ㄷ0",
          "original_form": "집에서 만들수있는 맛이던ㄷ0",
          "speaker_id": "1",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036800.1.13",
          "form": "그런데 바닥이 너무 눅진해서 별로였어요~\r\n윗부분만 먹고 버리는게 답",
          "original_form": "그런데 바닥이 너무 눅진해서 별로였어요~\r\n윗부분만 먹고 버리는게 답",
          "speaker_id": "2",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036800.1.14",
          "form": "저희집에 비나나가 있거든요",
          "original_form": "저희집에 비나나가 있거든요",
          "speaker_id": "1",
          "time": "20210831 12:27"
        }
      ]
