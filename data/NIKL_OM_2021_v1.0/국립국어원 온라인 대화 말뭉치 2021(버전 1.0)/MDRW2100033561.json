      "utterance": [
        {
          "id": "MDRW2100033561.1.1",
          "form": "저는 먹은거 설겆이좀 하러가유",
          "original_form": "저는 먹은거 설겆이좀 하러가유",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100033561.1.2",
          "form": "또뵈요",
          "original_form": "또뵈요",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100033561.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100033561.1.4",
          "form": "스벅왔어요 ㅎ",
          "original_form": "스벅왔어요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100033561.1.5",
          "form": "쿠키블루베리 머핀드셔보셨어요?",
          "original_form": "쿠키블루베리 머핀드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100033561.1.6",
          "form": "머핀은 안먹어봤어요",
          "original_form": "머핀은 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100033561.1.7",
          "form": "머핀드시게요?",
          "original_form": "머핀드시게요?",
          "speaker_id": "1",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100033561.1.8",
          "form": "머핀이랑 커피시켰어요",
          "original_form": "머핀이랑 커피시켰어요",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100033561.1.9",
          "form": "블루베리잼에 상위는 쿠키라 맛있네요",
          "original_form": "블루베리잼에 상위는 쿠키라 맛있네요",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100033561.1.10",
          "form": "전 초코크레이프ㅋ",
          "original_form": "전 초코크레이프ㅋ",
          "speaker_id": "1",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100033561.1.11",
          "form": "바삭하게 달달하게 맛나네요",
          "original_form": "바삭하게 달달하게 맛나네요",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100033561.1.12",
          "form": "아메랑 먹긴 괜찮겠어요",
          "original_form": "아메랑 먹긴 괜찮겠어요",
          "speaker_id": "1",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100033561.1.13",
          "form": "따뜻하게 데워달랬더니 더 괜찮은듯요",
          "original_form": "따뜻하게 데워달랬더니 더 괜찮은듯요",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100033561.1.14",
          "form": "점심드시고 디저트드시러가셨나요ㅎ",
          "original_form": "점심드시고 디저트드시러가셨나요ㅎ",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100033561.1.15",
          "form": "크레이프는 머예요?",
          "original_form": "크레이프는 머예요?",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100033561.1.16",
          "form": "케이크?",
          "original_form": "케이크?",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100033561.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100033561.1.18",
          "form": "한겹씩 얇게 쌓은 케익이요",
          "original_form": "한겹씩 얇게 쌓은 케익이요",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100033561.1.19",
          "form": "딸뤠미 미술학원델다주고 학원옆 스벅에서 끝날때까지 기다릴려구요",
          "original_form": "딸뤠미 미술학원델다주고 학원옆 스벅에서 끝날때까지 기다릴려구요",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100033561.1.20",
          "form": "커피빈 밀크크레이프 좋아하는데  맛나요",
          "original_form": "커피빈 밀크크레이프 좋아하는데  맛나요",
          "speaker_id": "1",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100033561.1.21",
          "form": "커피빈 매장 아직 있나요?",
          "original_form": "커피빈 매장 아직 있나요?",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100033561.1.22",
          "form": "저희집근처엔 다 없어졌어요",
          "original_form": "저희집근처엔 다 없어졌어요",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100033561.1.23",
          "form": "서울에나 좀있고 많이 없더라구요",
          "original_form": "서울에나 좀있고 많이 없더라구요",
          "speaker_id": "1",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100033561.1.24",
          "form": "죄다  스벅천지",
          "original_form": "죄다  스벅천지",
          "speaker_id": "1",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100033561.1.25",
          "form": "네 스벅은 계속 오픈오픈",
          "original_form": "네 스벅은 계속 오픈오픈",
          "speaker_id": "2",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100033561.1.26",
          "form": "여기 근방에 4개ㅋ",
          "original_form": "여기 근방에 4개ㅋ",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100033561.1.27",
          "form": "걸어가는거리에ㅋ",
          "original_form": "걸어가는거리에ㅋ",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100033561.1.28",
          "form": "집근처에 스벅있는데\r\n집근처 이마트안에 스벅이 또생겼더라구요",
          "original_form": "집근처에 스벅있는데\r\n집근처 이마트안에 스벅이 또생겼더라구요",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100033561.1.29",
          "form": "신세계꺼라 이마트에도 많이 들어가는듯요",
          "original_form": "신세계꺼라 이마트에도 많이 들어가는듯요",
          "speaker_id": "1",
          "time": "20210824 16:50"
        }
      ]
    }
