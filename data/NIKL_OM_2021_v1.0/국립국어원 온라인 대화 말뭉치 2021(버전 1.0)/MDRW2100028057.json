      "utterance": [
        {
          "id": "MDRW2100028057.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028057.1.2",
          "form": "흥미는 있는데 시국이 이래서 못가봣어요",
          "original_form": "흥미는 있는데 시국이 이래서 못가봣어요",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028057.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028057.1.4",
          "form": "어디가고싶으세요?",
          "original_form": "어디가고싶으세요?",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028057.1.5",
          "form": "유럽ㅇ\n이요",
          "original_form": "유럽ㅇ\n이요",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028057.1.6",
          "form": "저도 유럽 독일가보고싶은데 인종차별 넘 무서워요 ㅋㅋㅋ",
          "original_form": "저도 유럽 독일가보고싶은데 인종차별 넘 무서워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028057.1.7",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028057.1.8",
          "form": "특정지역만 그런것 아닌가오?",
          "original_form": "특정지역만 그런것 아닌가오?",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028057.1.9",
          "form": "그건 모르겠어요 ㅋㅋㅋ근데 말이 아예 안통하니 무서워요 ㅋㅋ",
          "original_form": "그건 모르겠어요 ㅋㅋㅋ근데 말이 아예 안통하니 무서워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028057.1.10",
          "form": "하긴요",
          "original_form": "하긴요",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028057.1.11",
          "form": "아무래도 전 영어도 잘못해서 ㅎㅎ",
          "original_form": "아무래도 전 영어도 잘못해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028057.1.12",
          "form": "패키지 있자뇽ㅇ",
          "original_form": "패키지 있자뇽ㅇ",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028057.1.13",
          "form": "오 그것도그렇네요 그거 함 가봐야겠어여 ㅋㅋㅋ",
          "original_form": "오 그것도그렇네요 그거 함 가봐야겠어여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028057.1.14",
          "form": "준비만 하죠",
          "original_form": "준비만 하죠",
