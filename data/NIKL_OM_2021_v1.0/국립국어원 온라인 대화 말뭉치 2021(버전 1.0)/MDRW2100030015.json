      "utterance": [
        {
          "id": "MDRW2100030015.1.1",
          "form": "앗 저는 양식이용ㅋㅋㅋ",
          "original_form": "앗 저는 양식이용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100030015.1.2",
          "form": "반대네요ㅋㅋ",
          "original_form": "반대네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100030015.1.3",
          "form": "전 매운거 좋아하거든요",
          "original_form": "전 매운거 좋아하거든요",
          "speaker_id": "2",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100030015.1.4",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100030015.1.5",
          "form": "아니요 ㅠㅜ저맵찔이에용 ㅜㅜ",
          "original_form": "아니요 ㅠㅜ저맵찔이에용 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100030015.1.6",
          "form": "아 그럴것 같았어요ㅋ",
          "original_form": "아 그럴것 같았어요ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030015.1.7",
          "form": "양식좋아하신다해서~",
          "original_form": "양식좋아하신다해서~",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030015.1.8",
          "form": "전 파스타도 좀 매운맛 있음 맛있더라구요ㅎ",
          "original_form": "전 파스타도 좀 매운맛 있음 맛있더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100030015.1.9",
          "form": "아하 ㅋㅋㅋㅋㅋㅋ 눈치가빠르시네요~~",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋ 눈치가빠르시네요~~",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100030015.1.10",
          "form": "파스타에 페퍼론치노들어가면 맛나죠 ㅎㅎ",
          "original_form": "파스타에 페퍼론치노들어가면 맛나죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100030015.1.11",
          "form": "매운맛내는게 페퍼론치노 엿군요!",
          "original_form": "매운맛내는게 페퍼론치노 엿군요!",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100030015.1.12",
          "form": "전 잘 몰라서ㅎ",
          "original_form": "전 잘 몰라서ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100030015.1.13",
          "form": "요즘 밀키트로도 이것저것 잘 나오더라구요",
          "original_form": "요즘 밀키트로도 이것저것 잘 나오더라구요",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100030015.1.14",
          "form": "감바스랑 스테이크도 밀키트로 시켜서 해먹어봣는데 괜찮던데요?ㅎ",
          "original_form": "감바스랑 스테이크도 밀키트로 시켜서 해먹어봣는데 괜찮던데요?ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100030015.1.15",
          "form": "넹 ㅋㅋㅋㅋㅋ",
          "original_form": "넹 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100030015.1.16",
          "form": "아정말여?? 저는 밀키트는 떡볶이밖에안먹어봐서!",
          "original_form": "아정말여?? 저는 밀키트는 떡볶이밖에안먹어봐서!",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100030015.1.17",
          "form": "요즘 밀키트 별의별 메뉴로 다 나오더라구요~",
          "original_form": "요즘 밀키트 별의별 메뉴로 다 나오더라구요~",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100030015.1.18",
          "form": "왠만한 메뉴는 다 있는거 같아요ㅋ",
          "original_form": "왠만한 메뉴는 다 있는거 같아요ㅋ",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100030015.1.19",
          "form": "맞아요 너무잘나오져~",
          "original_form": "맞아요 너무잘나오져~",
          "speaker_id": "1",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100030015.1.20",
