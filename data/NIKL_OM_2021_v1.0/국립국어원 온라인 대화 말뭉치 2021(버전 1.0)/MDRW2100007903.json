      "utterance": [
        {
          "id": "MDRW2100007903.1.1",
          "form": "싸이월드아시나요?",
          "original_form": "싸이월드아시나요?",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007903.1.2",
          "form": "알죠 ㅎ",
          "original_form": "알죠 ㅎ",
          "speaker_id": "2",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007903.1.3",
          "form": "추억이죠",
          "original_form": "추억이죠",
          "speaker_id": "2",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007903.1.4",
          "form": "싸이월드 많이 사용하셨나요?",
          "original_form": "싸이월드 많이 사용하셨나요?",
          "speaker_id": "1",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007903.1.5",
          "form": "아뇨 전안했어요 ㅎ 하셨나요?",
          "original_form": "아뇨 전안했어요 ㅎ 하셨나요?",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100007903.1.6",
          "form": "저도 많이 안했어요 싸이월드부활되면 좋을까요?",
          "original_form": "저도 많이 안했어요 싸이월드부활되면 좋을까요?",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100007903.1.7",
          "form": "재밌긴할거같아요",
          "original_form": "재밌긴할거같아요",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100007903.1.8",
          "form": "구경만해도 재밌어서요",
          "original_form": "구경만해도 재밌어서요",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100007903.1.9",
          "form": "싸이월드 부활하면 이용하실건가요?",
          "original_form": "싸이월드 부활하면 이용하실건가요?",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100007903.1.10",
          "form": "바빠서 못할거같네요",
          "original_form": "바빠서 못할거같네요",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100007903.1.11",
          "form": "하시면 구경갈게요",
          "original_form": "하시면 구경갈게요",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100007903.1.12",
          "form": "싸이월드 도토리가 뭔지 아세요?",
          "original_form": "싸이월드 도토리가 뭔지 아세요?",
          "speaker_id": "1",
          "time": "20210823 09:32"
        },
        {
          "id": "MDRW2100007903.1.13",
          "form": "알죠 ㅎㅎ",
          "original_form": "알죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100007903.1.14",
          "form": "추억돋네요",
          "original_form": "추억돋네요",
          "speaker_id": "2",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100007903.1.15",
          "form": "비지엠 뭐좋아하시나요?",
          "original_form": "비지엠 뭐좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100007903.1.16",
          "form": "예전에 좋을텐데인가 했던걸로 기억나네요ㅎㅎ 일촌 많으셨나요?",
          "original_form": "예전에 좋을텐데인가 했던걸로 기억나네요ㅎㅎ 일촌 많으셨나요?",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100007903.1.17",
          "form": "오 좋을텐데 기억나네요",
          "original_form": "오 좋을텐데 기억나네요",
          "speaker_id": "2",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100007903.1.18",
          "form": "저는 아예안했었어요 ㅜ 인기에 편승안하는 반대타입이라 ㅋㅋ",
          "original_form": "저는 아예안했었어요 ㅜ 인기에 편승안하는 반대타입이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100007903.1.19",
          "form": "프리스타일와이가 생각나네요",
          "original_form": "프리스타일와이가 생각나네요",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100007903.1.20",
          "form": "라이프이즈쿨이랑",
          "original_form": "라이프이즈쿨이랑",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100007903.1.21",
          "form": "맞아요 그노래도 기억나네요 싸이월드 부활하면 잘될까요?",
          "original_form": "맞아요 그노래도 기억나네요 싸이월드 부활하면 잘될까요?",
          "speaker_id": "1",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100007903.1.22",
          "form": "요즘애들이 할거같아서 유행할거같네요",
