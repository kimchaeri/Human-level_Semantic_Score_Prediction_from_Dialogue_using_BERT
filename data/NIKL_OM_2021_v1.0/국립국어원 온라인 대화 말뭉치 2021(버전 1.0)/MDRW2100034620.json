      "utterance": [
        {
          "id": "MDRW2100034620.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100034620.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100034620.1.3",
          "form": "음식 뭐 좋아하세요?",
          "original_form": "음식 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100034620.1.4",
          "form": "음 해물이나 매운음식들 아니라면 대체로 잘 먹는거 같아요",
          "original_form": "음 해물이나 매운음식들 아니라면 대체로 잘 먹는거 같아요",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034620.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034620.1.6",
          "form": "음 저는 호불호 많이 갈리는 거 빼곤 다 좋아해요",
          "original_form": "음 저는 호불호 많이 갈리는 거 빼곤 다 좋아해요",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034620.1.7",
          "form": "빵순이라 빵도 좋구여",
          "original_form": "빵순이라 빵도 좋구여",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034620.1.8",
          "form": "빵 좋아하시는군요!",
          "original_form": "빵 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034620.1.9",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034620.1.10",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034620.1.11",
          "form": "저는 향신료가 강하거나 곱창 막창 닭발 요런것도 잘 못 먹어요ㅜㅜ",
          "original_form": "저는 향신료가 강하거나 곱창 막창 닭발 요런것도 잘 못 먹어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034620.1.12",
          "form": "빵 좋아는 하는데 많이 먹으면 소화가 잘 안 되서, 조금씩 먹는 편이에요",
          "original_form": "빵 좋아는 하는데 많이 먹으면 소화가 잘 안 되서, 조금씩 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034620.1.13",
          "form": "헉 저도 그런거 다 못 먹어요!",
          "original_form": "헉 저도 그런거 다 못 먹어요!",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034620.1.14",
          "form": "요새는 소화잘되는거로 만드는 빵집도 많더라구요!",
          "original_form": "요새는 소화잘되는거로 만드는 빵집도 많더라구요!",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034620.1.15",
          "form": "오 반가운 느낌이에요!",
          "original_form": "오 반가운 느낌이에요!",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034620.1.16",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034620.1.17",
          "form": "주변에서 곱창 막창 이런거 못 먹는다고 하면",
          "original_form": "주변에서 곱창 막창 이런거 못 먹는다고 하면",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034620.1.18",
          "form": "저희동네도 유명한집 하나있는데 소화 잘 된댕ㅅ",
          "original_form": "저희동네도 유명한집 하나있는데 소화 잘 된댕ㅅ",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034620.1.19",
          "form": "대요",
          "original_form": "대요",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034620.1.20",
          "form": "왜 그 맛있는걸 못 먹냐고 하더라구요",
          "original_form": "왜 그 맛있는걸 못 먹냐고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034620.1.21",
          "form": "맞아요 저도",
          "original_form": "맞아요 저도",
          "speaker_id": "1",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034620.1.22",
          "form": "주위에 저 빼고 다 먹어요 ㅋㅋㅋ",
          "original_form": "주위에 저 빼고 다 먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:42"
        }
      ]
