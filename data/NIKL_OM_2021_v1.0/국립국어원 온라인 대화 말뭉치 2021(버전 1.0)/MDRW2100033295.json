      "utterance": [
        {
          "id": "MDRW2100033295.1.1",
          "form": "뭔가 나가사키짬뽕은 매운맛 전혀 없고 야채 왕창 들어가서 우리가 아는 맛이랑 약간 다르더라!",
          "original_form": "뭔가 나가사키짬뽕은 매운맛 전혀 없고 야채 왕창 들어가서 우리가 아는 맛이랑 약간 다르더라!",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100033295.1.2",
          "form": "언니는 운동 해야 해",
          "original_form": "언니는 운동 해야 해",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033295.1.3",
          "form": "일주일째 하고 있어",
          "original_form": "일주일째 하고 있어",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033295.1.4",
          "form": "근략 운동!",
          "original_form": "근략 운동!",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033295.1.5",
          "form": "짱이다!",
          "original_form": "짱이다!",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033295.1.6",
          "form": "나는 놀고 있는데",
          "original_form": "나는 놀고 있는데",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033295.1.7",
          "form": "요즘 물을 많이 마셔야겠다고 생각하고 있어",
          "original_form": "요즘 물을 많이 마셔야겠다고 생각하고 있어",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033295.1.8",
          "form": "웅 너도 물 마시라고 알림 보내주는 어플 깔아봐",
          "original_form": "웅 너도 물 마시라고 알림 보내주는 어플 깔아봐",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100033295.1.9",
          "form": "아니먄 루틴을 정해서",
          "original_form": "아니먄 루틴을 정해서",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100033295.1.10",
          "form": "화장실 가고 싶어질 때마다 물 마시기",
          "original_form": "화장실 가고 싶어질 때마다 물 마시기",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033295.1.11",
          "form": "그리고 물도 물이지만",
          "original_form": "그리고 물도 물이지만",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033295.1.12",
          "form": "내가 화장실을 하루에 한번만 가 힝",
          "original_form": "내가 화장실을 하루에 한번만 가 힝",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033295.1.13",
          "form": "근육을 늘려야 체수분도 늘어",
          "original_form": "근육을 늘려야 체수분도 늘어",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033295.1.14",
          "form": "그렇다고 누가 얘기해주더라궁",
          "original_form": "그렇다고 누가 얘기해주더라궁",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033295.1.15",
          "form": ">.<",
          "original_form": ">.<",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033295.1.16",
          "form": "언닌 근략 운동 꾸준히 햇더니",
          "original_form": "언닌 근략 운동 꾸준히 햇더니",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033295.1.17",
          "form": "건강해지는 기분이야",
          "original_form": "건강해지는 기분이야",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033295.1.18",
          "form": "멋있다",
          "original_form": "멋있다",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100033295.1.19",
          "form": "나 이번에 일본 단기파견 공문 안뜨면",
          "original_form": "나 이번에 일본 단기파견 공문 안뜨면",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100033295.1.20",
          "form": "정말정말 링피트 살까봐",
          "original_form": "정말정말 링피트 살까봐",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100033295.1.21",
          "form": "지금은 혹시 몰라 저축중이라 안사는중!",
          "original_form": "지금은 혹시 몰라 저축중이라 안사는중!",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100033295.1.22",
          "form": "링피트가 잇음 운동을 하겟지?",
          "original_form": "링피트가 잇음 운동을 하겟지?",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100033295.1.23",
          "form": "당근이지!!",
          "original_form": "당근이지!!",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100033295.1.24",
          "form": "그래도 비싼돈 주고 샀는데 ㅠ.ㅠ",
          "original_form": "그래도 비싼돈 주고 샀는데 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100033295.1.25",
          "form": "건강한 삶을 지켜야지!!",
          "original_form": "건강한 삶을 지켜야지!!",
          "speaker_id": "2",
