          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100019088.1.7",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100019088.1.8",
          "form": "알아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "알아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100019088.1.9",
          "form": "무슨색을...?",
          "original_form": "무슨색을...?",
