        {
          "id": "MDRW2100020378.1.63",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020378.1.64",
          "form": "치료 무서워서",
          "original_form": "치료 무서워서",
