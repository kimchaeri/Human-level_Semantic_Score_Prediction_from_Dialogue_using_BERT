        },
        {
          "id": "MDRW2100020378.1.5",
          "form": "아까.",
          "original_form": "아까.",
          "speaker_id": "2",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100020378.1.6",
          "form": "토스트를",
          "original_form": "토스트를",
          "speaker_id": "2",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100020378.1.7",
          "form": "겜도 하기전에",
          "original_form": "겜도 하기전에",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100020378.1.8",
          "form": "하루죙일 먹을순 업ㄱ비",
          "original_form": "하루죙일 먹을순 업ㄱ비",
          "speaker_id": "2",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100020378.1.9",
          "form": "채킹해주새요",
          "original_form": "채킹해주새요",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020378.1.10",
          "form": "양치중에도 대화를 채워야지.",
          "original_form": "양치중에도 대화를 채워야지.",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100020378.1.11",
          "form": "칫솔",
          "original_form": "칫솔",
          "speaker_id": "1",
          "time": "20210828 02:34"
        },
        {
          "id": "MDRW2100020378.1.12",
          "form": "벽에 붙여서 거는거역으면 좋겟음",
          "original_form": "벽에 붙여서 거는거역으면 좋겟음",
          "speaker_id": "1",
          "time": "20210828 02:34"
        },
        {
          "id": "MDRW2100020378.1.13",
          "form": "오 나 지금 그거쓰는 중",
          "original_form": "오 나 지금 그거쓰는 중",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.14",
          "form": "치약은 그냥 공유해요",
          "original_form": "치약은 그냥 공유해요",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.15",
          "form": "오대박",
          "original_form": "오대박",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.16",
          "form": "채약을",
          "original_form": "채약을",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.17",
          "form": "따로스는걸",
          "original_form": "따로스는걸",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.18",
          "form": "생각해보진않앗내",
          "original_form": "생각해보진않앗내",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.19",
          "form": "혹시모름",
          "original_form": "혹시모름",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.20",
          "form": "그.. 룸메중에 진짜",
          "original_form": "그.. 룸메중에 진짜",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.21",
          "form": "비즈니스 룸메같은 경우는",
          "original_form": "비즈니스 룸메같은 경우는",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.22",
          "form": "생필품 닥딱 나눠스덪데",
          "original_form": "생필품 닥딱 나눠스덪데",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020378.1.23",
          "form": "휴지같은건 번갈아가면서 사고..",
          "original_form": "휴지같은건 번갈아가면서 사고..",
          "speaker_id": "2",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020378.1.24",
          "form": "그건 글켓다",
          "original_form": "그건 글켓다",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020378.1.25",
          "form": "난른데",
          "original_form": "난른데",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020378.1.26",
          "form": "비즈니스룸메 말고",
          "original_form": "비즈니스룸메 말고",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020378.1.27",
          "form": "걍진자 가족룸메처럼살고싳어",
          "original_form": "걍진자 가족룸메처럼살고싳어",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020378.1.28",
          "form": "ㅇㅋ",
          "original_form": "ㅇㅋ",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020378.1.29",
          "form": "그리고 님아",
          "original_form": "그리고 님아",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020378.1.30",
          "form": "이건 내 희망사항이고 필수아닌데",
          "original_form": "이건 내 희망사항이고 필수아닌데",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020378.1.31",
          "form": "최초 병원 방문시 같이 가주기",
          "original_form": "최초 병원 방문시 같이 가주기",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020378.1.32",
          "form": "해주기 가능?",
          "original_form": "해주기 가능?",
          "speaker_id": "2",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020378.1.33",
          "form": "오좋다",
          "original_form": "오좋다",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020378.1.34",
