      "utterance": [
        {
          "id": "MDRW2100000695.1.1",
          "form": "키우시는 동물 있으세요?",
          "original_form": "키우시는 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000695.1.2",
          "form": "고양이 키우고 있어요",
          "original_form": "고양이 키우고 있어요",
          "speaker_id": "1",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000695.1.3",
          "form": "고양이 좋아하세요?",
          "original_form": "고양이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000695.1.4",
          "form": "앗!!! 고양이 좋아해요!",
          "original_form": "앗!!! 고양이 좋아해요!",
          "speaker_id": "2",
          "time": "20210819 17:46"
        },
        {
          "id": "MDRW2100000695.1.5",
          "form": "비록 키우지는 않지만ㅜ",
          "original_form": "비록 키우지는 않지만ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:46"
        },
        {
          "id": "MDRW2100000695.1.6",
          "form": "고양이는 키우면 행복은 한데",
          "original_form": "고양이는 키우면 행복은 한데",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000695.1.7",
          "form": "키우는걸 권장하고싶진 않아요",
          "original_form": "키우는걸 권장하고싶진 않아요",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000695.1.8",
          "form": "왜요??",
          "original_form": "왜요??",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000695.1.9",
          "form": "진짜 자주아파요 은근히",
          "original_form": "진짜 자주아파요 은근히",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000695.1.10",
          "form": "피부병도 생기고",
          "original_form": "피부병도 생기고",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000695.1.11",
          "form": "ㅜㅠ 예민한가보네요 애기들이ㅜ",
          "original_form": "ㅜㅠ 예민한가보네요 애기들이ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000695.1.12",
          "form": "털도 엄청 날리구...",
          "original_form": "털도 엄청 날리구...",
          "speaker_id": "1",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000695.1.13",
          "form": "저는 여건이 안되다보니 키우진 못하고 고양이카페에 가끔 갔어요",
          "original_form": "저는 여건이 안되다보니 키우진 못하고 고양이카페에 가끔 갔어요",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000695.1.14",
          "form": "근데 귀여워요",
          "original_form": "근데 귀여워요",
          "speaker_id": "1",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000695.1.15",
          "form": "요즘에는 못가고있지만ㅜ",
          "original_form": "요즘에는 못가고있지만ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000695.1.16",
          "form": "귀엽죠ㅜ",
          "original_form": "귀엽죠ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000695.1.17",
          "form": "냥이가 개냥이인가요ㅋㅋ",
          "original_form": "냥이가 개냥이인가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000695.1.18",
          "form": "전형적인 고양이입니다",
          "original_form": "전형적인 고양이입니다",
          "speaker_id": "1",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000695.1.19",
          "form": "맨날 삐져서 고개 팩 돌리고",
          "original_form": "맨날 삐져서 고개 팩 돌리고",
          "speaker_id": "1",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000695.1.20",
          "form": "앗..ㅋㅋㅋㅋㅋ",
          "original_form": "앗..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000695.1.21",
          "form": "근데 간식주면 발라당 발라당",
          "original_form": "근데 간식주면 발라당 발라당",
          "speaker_id": "1",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000695.1.22",
          "form": "조금뒤엔 다시 삐져서 팩 돌리고",
          "original_form": "조금뒤엔 다시 삐져서 팩 돌리고",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000695.1.23",
          "form": "혹시 여건이 되면 고양이 키우실 생각은 있으신가요?",
          "original_form": "혹시 여건이 되면 고양이 키우실 생각은 있으신가요?",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000695.1.24",
          "form": "아마도요?",
          "original_form": "아마도요?",
          "speaker_id": "2",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000695.1.25",
          "form": "간식주면 발라당이라니ㅋㅋ자본주의 고양이...!!!!",
          "original_form": "간식주면 발라당이라니ㅋㅋ자본주의 고양이...!!!!",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000695.1.26",
          "form": "키우실거면 꼭 두마리 키우세요",
          "original_form": "키우실거면 꼭 두마리 키우세요",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000695.1.27",
          "form": "외로울까봐요?",
          "original_form": "외로울까봐요?",
          "speaker_id": "2",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000695.1.28",
          "form": "진짜 외로워해요",
          "original_form": "진짜 외로워해요",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100000695.1.29",
          "form": "혼자두면 사람이 놀아주는것도 한계가 있고",
          "original_form": "혼자두면 사람이 놀아주는것도 한계가 있고",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100000695.1.30",
          "form": "둘 데려다놓으면 지들끼리 잘 놀고",
          "original_form": "둘 데려다놓으면 지들끼리 잘 놀고",
          "speaker_id": "1",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000695.1.31",
          "form": "ㅜㅠ 이럴까봐 제가 키울 수가 없어요ㅜ",
          "original_form": "ㅜㅠ 이럴까봐 제가 키울 수가 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000695.1.32",
          "form": "건강해지구",
          "original_form": "건강해지구",
          "speaker_id": "1",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000695.1.33",
          "form": "추가금도 생각보다 얼마 안들어요",
          "original_form": "추가금도 생각보다 얼마 안들어요",
          "speaker_id": "1",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000695.1.34",
          "form": "중성화 비용이 초반에 좀 들긴한데",
          "original_form": "중성화 비용이 초반에 좀 들긴한데",
          "speaker_id": "1",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000695.1.35",
          "form": "강아지나 고양이나 혼자두기 너무 마음아파서ㅜ",
          "original_form": "강아지나 고양이나 혼자두기 너무 마음아파서ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000695.1.36",
          "form": "두마리 데리고오면 해결됩니다...!",
          "original_form": "두마리 데리고오면 해결됩니다...!",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000695.1.37",
          "form": "참고할게요!",
          "original_form": "참고할게요!",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000695.1.38",
          "form": "꿀팁알려주셨네요ㅋㅋ",
          "original_form": "꿀팁알려주셨네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000695.1.39",
          "form": "ㅋㅋㅋ 근데 사실 안키우는게 최곤것 같긴 해요",
          "original_form": "ㅋㅋㅋ 근데 사실 안키우는게 최곤것 같긴 해요",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000695.1.40",
          "form": "생명의 무게라는게 생각보다",
          "original_form": "생명의 무게라는게 생각보다",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000695.1.41",
          "form": "훨씬 무거워요",
          "original_form": "훨씬 무거워요",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000695.1.42",
          "form": "그쵸ㅜ",
          "original_form": "그쵸ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000695.1.43",
          "form": "시간을 돌려서 키울래 말래 물어보면ㄱ",
          "original_form": "시간을 돌려서 키울래 말래 물어보면ㄱ",
          "speaker_id": "1",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100000695.1.44",
          "form": "역시 전 고양이카페로...ㅋㅋㅋ",
          "original_form": "역시 전 고양이카페로...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100000695.1.45",
          "form": "그래도 키울것같긴 하지만ㅋㅋㅋ",
          "original_form": "그래도 키울것같긴 하지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100000695.1.46",
          "form": "사실 키우기보단 모시고 사는 편이네요",
          "original_form": "사실 키우기보단 모시고 사는 편이네요",
          "speaker_id": "1",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100000695.1.47",
          "form": "ㅋㅋㅋ집사의 인생",
          "original_form": "ㅋㅋㅋ집사의 인생",
