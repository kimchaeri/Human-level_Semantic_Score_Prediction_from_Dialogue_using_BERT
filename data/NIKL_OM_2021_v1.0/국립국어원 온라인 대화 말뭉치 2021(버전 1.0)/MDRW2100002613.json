      "utterance": [
        {
          "id": "MDRW2100002613.1.1",
          "form": "파리 미술관은 어디가 좋으셨어요?",
          "original_form": "파리 미술관은 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002613.1.2",
          "form": "기념품은 막상 살게 없어요",
          "original_form": "기념품은 막상 살게 없어요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002613.1.3",
          "form": "루브르,오르세,로댕 등등 있는곳 거의다갔어요ㅎ",
          "original_form": "루브르,오르세,로댕 등등 있는곳 거의다갔어요ㅎ",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002613.1.4",
          "form": "으아 아예 프랑스만 가셨나요?",
          "original_form": "으아 아예 프랑스만 가셨나요?",
          "speaker_id": "1",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002613.1.5",
          "form": "로댕은 못가봤네요",
          "original_form": "로댕은 못가봤네요",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002613.1.6",
          "form": "로댕도 멋있었어요ㅎㅎ",
          "original_form": "로댕도 멋있었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002613.1.7",
          "form": "숙소가 민박이었어서 이동에 시간을 잡아먹다보니ㅠ",
          "original_form": "숙소가 민박이었어서 이동에 시간을 잡아먹다보니ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002613.1.8",
          "form": "저도 민박에서 묵었었어요~",
          "original_form": "저도 민박에서 묵었었어요~",
          "speaker_id": "2",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002613.1.9",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002613.1.10",
          "form": "에글리즈 뻥땅의?ㅋㅋ",
          "original_form": "에글리즈 뻥땅의?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002613.1.11",
          "form": "아니요ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002613.1.12",
          "form": "다시갈날 곧 오겟죠ㅎ",
          "original_form": "다시갈날 곧 오겟죠ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002613.1.13",
          "form": "전 민박집의 중국인 아주머니 밥이 너무 맛있어서",
          "original_form": "전 민박집의 중국인 아주머니 밥이 너무 맛있어서",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002613.1.14",
          "form": "파리에서 중국음식에 맛들렸어요ㅋㅋ",
          "original_form": "파리에서 중국음식에 맛들렸어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002613.1.15",
          "form": "담번에 거기민박으로 가야겠네요ㅋ",
          "original_form": "담번에 거기민박으로 가야겠네요ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002613.1.16",
          "form": "프랑스에서 정찬을 제대로 못먹어본 거 같아요",
          "original_form": "프랑스에서 정찬을 제대로 못먹어본 거 같아요",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002613.1.17",
          "form": "생선스테이크도 맛있었어요.",
          "original_form": "생선스테이크도 맛있었어요.",
          "speaker_id": "2",
          "time": "20210820 16:06"
        },
        {
          "id": "MDRW2100002613.1.18",
          "form": "달팽이요리가 너무 좋았네요ㅋ",
          "original_form": "달팽이요리가 너무 좋았네요ㅋ",
          "speaker_id": "2",
          "time": "20210820 16:06"
        },
        {
          "id": "MDRW2100002613.1.19",
          "form": "맞아요ㅠㅠ 저도 빵으로 설거지ㅋㅋㅋ",
          "original_form": "맞아요ㅠㅠ 저도 빵으로 설거지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002613.1.20",
          "form": "달팽이라고 전혀 생각되지 않은 여행음식이었어요.",
          "original_form": "달팽이라고 전혀 생각되지 않은 여행음식이었어요.",
          "speaker_id": "1",
          "time": "20210820 16:12"
        }
      ]
