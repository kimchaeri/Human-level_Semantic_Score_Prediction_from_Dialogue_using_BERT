      "utterance": [
        {
          "id": "MDRW2100017801.1.1",
          "form": "내일은 뭘 먹을지 또 고민이네여",
          "original_form": "내일은 뭘 먹을지 또 고민이네여",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017801.1.2",
          "form": "저녁 뭐드셨어용",
          "original_form": "저녁 뭐드셨어용",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100017801.1.3",
          "form": "그러게요 저 오늘은 김치볶음밥 먹었어요!",
          "original_form": "그러게요 저 오늘은 김치볶음밥 먹었어요!",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100017801.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100017801.1.5",
          "form": "저는 양념게장에 밥먹었어요",
          "original_form": "저는 양념게장에 밥먹었어요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100017801.1.6",
          "form": "제 친구들 게장 엄청 좋아하더라구요ㅋㅋㅋ",
          "original_form": "제 친구들 게장 엄청 좋아하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100017801.1.7",
          "form": "완전 밥도둑이잖아요ㅋㅋ",
          "original_form": "완전 밥도둑이잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100017801.1.8",
          "form": "간장보단 양념이 더 맛있는거같아요",
          "original_form": "간장보단 양념이 더 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100017801.1.9",
          "form": "저도 간장보다 양념이 더 좋아요ㅎㅎㅎ",
          "original_form": "저도 간장보다 양념이 더 좋아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100017801.1.10",
          "form": "저번에 여행 가서 게장 먹는데",
          "original_form": "저번에 여행 가서 게장 먹는데",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100017801.1.11",
          "form": "친구들 그렇게 잘 먹는 건 처음 봤어요ㅋㅋㅋㅋ",
          "original_form": "친구들 그렇게 잘 먹는 건 처음 봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100017801.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ여행가서 먹었으니 더 맛있었나봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ여행가서 먹었으니 더 맛있었나봐요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100017801.1.13",
          "form": "그랬나봐요 너무 인상적이어서 아직도 기억나요.. ㅋㅋㅋ",
          "original_form": "그랬나봐요 너무 인상적이어서 아직도 기억나요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100017801.1.14",
          "form": "오늘은 식사 맛있는 거 하셨어요?",
          "original_form": "오늘은 식사 맛있는 거 하셨어요?",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100017801.1.15",
          "form": "저녁 야채곱창 먹었어요!!!",
          "original_form": "저녁 야채곱창 먹었어요!!!",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100017801.1.16",
          "form": "헉 야채곱창ㅠㅜㅠ 저두 야식으로 먹고 싶어요",
          "original_form": "헉 야채곱창ㅠㅜㅠ 저두 야식으로 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100017801.1.17",
          "form": "짱맛 맥주도 같이 한잔 했더니 기분 좋네용",
          "original_form": "짱맛 맥주도 같이 한잔 했더니 기분 좋네용",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100017801.1.18",
          "form": "ㅠㅜㅜㅠ지금 제일 부럽네요",
          "original_form": "ㅠㅜㅜㅠ지금 제일 부럽네요",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100017801.1.19",
          "form": "먹고 볶음밥도 먹어야 되는데",
          "original_form": "먹고 볶음밥도 먹어야 되는데",
