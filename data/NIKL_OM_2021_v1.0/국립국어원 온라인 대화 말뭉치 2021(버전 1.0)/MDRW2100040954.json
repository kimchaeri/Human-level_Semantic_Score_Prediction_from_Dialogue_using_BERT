      "utterance": [
        {
          "id": "MDRW2100040954.1.1",
          "form": "저 좋아하는 배우",
          "original_form": "저 좋아하는 배우",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040954.1.2",
          "form": "이주영 좋아해요!!!",
          "original_form": "이주영 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040954.1.3",
          "form": "또 누구더라 .. 김민희도 좋아하구...",
          "original_form": "또 누구더라 .. 김민희도 좋아하구...",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040954.1.4",
          "form": "김태리도 좋아해요!",
          "original_form": "김태리도 좋아해요!",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040954.1.5",
          "form": "헐 저도 김태리 김민희 넘 좋아해서",
          "original_form": "헐 저도 김태리 김민희 넘 좋아해서",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040954.1.6",
          "form": "아가씨 진짜 여러 번 봤어요",
          "original_form": "아가씨 진짜 여러 번 봤어요",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100040954.1.7",
          "form": "헐 저도요 ㅋㅋㅋㅋㅋ",
          "original_form": "헐 저도요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100040954.1.8",
          "form": "좀 빻은 설정 몇 개만 빼면 정말",
          "original_form": "좀 빻은 설정 몇 개만 빼면 정말",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100040954.1.9",
          "form": "완벽한 영화 아닌가요ㅡ",
          "original_form": "완벽한 영화 아닌가요ㅡ",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100040954.1.10",
          "form": "저는 박찬욱을 더 좋아해요!!! 그 누구더라",
          "original_form": "저는 박찬욱을 더 좋아해요!!! 그 누구더라",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100040954.1.11",
          "form": "박찬욱이랑 항상 같이 언급되는 감독보다요!",
          "original_form": "박찬욱이랑 항상 같이 언급되는 감독보다요!",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100040954.1.12",
          "form": "그 봉준호!? 감독이요!?!?!??!",
          "original_form": "그 봉준호!? 감독이요!?!?!??!",
          "speaker_id": "2",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100040954.1.13",
          "form": "그렇담 저도요!!!!",
          "original_form": "그렇담 저도요!!!!",
          "speaker_id": "2",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100040954.1.14",
          "form": "아 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100040954.1.15",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ 잘 떠올려주셔서 감사해요!!!!",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ 잘 떠올려주셔서 감사해요!!!!",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100040954.1.16",
          "form": "그.... 감성이 뭐랄까",
          "original_form": "그.... 감성이 뭐랄까",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100040954.1.17",
          "form": "재미!! 만 따지면 봉준호꺼가 더 재밌긴 한데",
          "original_form": "재미!! 만 따지면 봉준호꺼가 더 재밌긴 한데",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100040954.1.18",
          "form": "의미!! 는 박찬욱 감독 작품이 더 많은 느낌이랄까요?",
          "original_form": "의미!! 는 박찬욱 감독 작품이 더 많은 느낌이랄까요?",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100040954.1.19",
          "form": "맞아요 글고 전 개인적으로 송강호 배우 연기 스타일이",
          "original_form": "맞아요 글고 전 개인적으로 송강호 배우 연기 스타일이",
          "speaker_id": "2",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100040954.1.20",
          "form": "다 넘 비슷학하게 느껴져서",
          "original_form": "다 넘 비슷학하게 느껴져서",
          "speaker_id": "2",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100040954.1.21",
          "form": "봉준호 감독 ㅇ영화의 매력을 죽인다고 생각해요 ^.ㅜ",
          "original_form": "봉준호 감독 ㅇ영화의 매력을 죽인다고 생각해요 ^.ㅜ",
          "speaker_id": "2",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100040954.1.22",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "1",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100040954.1.23",
          "form": "맞아요!!!!!",
          "original_form": "맞아요!!!!!",
          "speaker_id": "1",
          "time": "20210905 14:17"
        },
        {
          "id": "MDRW2100040954.1.24",
          "form": "자꾸 송강호를 못잃는 게;;;",
          "original_form": "자꾸 송강호를 못잃는 게;;;",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100040954.1.25",
          "form": "아니 좀 잘생긴 배우 데려오던가...(궁시렁",
          "original_form": "아니 좀 잘생긴 배우 데려오던가...(궁시렁",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100040954.1.26",
          "form": "아 그니까요!!!!! 송강호 진짜.... 할많하않입니다...",
          "original_form": "아 그니까요!!!!! 송강호 진짜.... 할많하않입니다...",
          "speaker_id": "2",
          "time": "20210905 14:22"
        },
        {
          "id": "MDRW2100040954.1.27",
          "form": "그래서 박찬욱 감독 영화를 더ㅜ보는 편이기도 해요",
          "original_form": "그래서 박찬욱 감독 영화를 더ㅜ보는 편이기도 해요",
          "speaker_id": "2",
          "time": "20210905 14:22"
        },
        {
          "id": "MDRW2100040954.1.28",
          "form": "오오 저도예요!!! 반갑네요 히히",
          "original_form": "오오 저도예요!!! 반갑네요 히히",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100040954.1.29",
          "form": "혹시 박찬욱 감독 영화 중에",
          "original_form": "혹시 박찬욱 감독 영화 중에",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100040954.1.30",
          "form": "스토커 보셨나요?",
          "original_form": "스토커 보셨나요?",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100040954.1.31",
          "form": "네!!!! 좀 예전에 봐서 ㅠ.^ 솔직히 가물가물하지만",
          "original_form": "네!!!! 좀 예전에 봐서 ㅠ.^ 솔직히 가물가물하지만",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100040954.1.32",
          "form": "재밌게 본 작품이었어요! 여주가 넘 제 스타일이었고!",
          "original_form": "재밌게 본 작품이었어요! 여주가 넘 제 스타일이었고!",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100040954.1.33",
          "form": "오 반갑네요!!",
          "original_form": "오 반갑네요!!",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100040954.1.34",
          "form": "저는 예전에 한 번 봤을 땐 별 감흥이 없었는데",
          "original_form": "저는 예전에 한 번 봤을 땐 별 감흥이 없었는데",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100040954.1.35",
          "form": "친한 친구가 그 영화를 좋아한대서 저도 다시 봤거든요",
          "original_form": "친한 친구가 그 영화를 좋아한대서 저도 다시 봤거든요",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100040954.1.36",
          "form": "그러니까 처음 봤을 때 놓쳤었던 게 보이더라구요",
          "original_form": "그러니까 처음 봤을 때 놓쳤었던 게 보이더라구요",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100040954.1.37",
          "form": "그래서 제가 좋아하는 영화 리스트에 올랐어요~!",
          "original_form": "그래서 제가 좋아하는 영화 리스트에 올랐어요~!",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100040954.1.38",
          "form": "헛 맞아요 은근 제가 해석해야하는 부분들이?",
          "original_form": "헛 맞아요 은근 제가 해석해야하는 부분들이?",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100040954.1.39",
          "form": "많더라고요!",
          "original_form": "많더라고요!",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100040954.1.40",
          "form": "그게 그 감독 영화의 묘미같아요",
          "original_form": "그게 그 감독 영화의 묘미같아요",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100040954.1.41",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100040954.1.42",
