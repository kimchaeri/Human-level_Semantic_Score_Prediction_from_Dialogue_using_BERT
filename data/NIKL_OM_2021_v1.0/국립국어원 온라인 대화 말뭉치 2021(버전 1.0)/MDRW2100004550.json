      "utterance": [
        {
          "id": "MDRW2100004550.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004550.1.2",
          "form": "안키워요 알러지있네요",
          "original_form": "안키워요 알러지있네요",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004550.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004550.1.4",
          "form": "저도 아직",
          "original_form": "저도 아직",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004550.1.5",
          "form": "털알러지있으신가요",
          "original_form": "털알러지있으신가요",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004550.1.6",
          "form": "안키워봐서 모르지만 고양이털은 싫아요",
          "original_form": "안키워봐서 모르지만 고양이털은 싫아요",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004550.1.7",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004550.1.8",
          "form": "고양이 귀엽기는 하다고 생각해요",
          "original_form": "고양이 귀엽기는 하다고 생각해요",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004550.1.9",
          "form": "울음소리가 싫어요",
          "original_form": "울음소리가 싫어요",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004550.1.10",
          "form": "헐 왜요",
          "original_form": "헐 왜요",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004550.1.11",
          "form": "그게제일 귀여운데",
          "original_form": "그게제일 귀여운데",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004550.1.12",
          "form": "밤애 들고양이들 아기울음처럼 울어서요",
          "original_form": "밤애 들고양이들 아기울음처럼 울어서요",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004550.1.13",
          "form": "아 그건 맞아요 저도 들으니까 놀라겠더라구요",
          "original_form": "아 그건 맞아요 저도 들으니까 놀라겠더라구요",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004550.1.14",
          "form": "밤에는 장난아니네요",
          "original_form": "밤에는 장난아니네요",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004550.1.15",
          "form": "그래도 남이 키우는 고양이는 귀여워요",
          "original_form": "그래도 남이 키우는 고양이는 귀여워요",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004550.1.16",
          "form": "그런것같아요",
          "original_form": "그런것같아요",
          "speaker_id": "1",
          "time": "20210821 17:17"
        }
      ]
