        {
          "id": "MDRW2100019166.1.1",
          "form": "나도 2.2에 무슨 캐가 나올진 모르겠지만",
          "original_form": "나도 2.2에 무슨 캐가 나올진 모르겠지만",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019166.1.2",
          "form": "일단 2.1에서 원석 주는거 봐보고",
          "original_form": "일단 2.1에서 원석 주는거 봐보고",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019166.1.3",
          "form": "제발 소 한번만",
          "original_form": "제발 소 한번만",
          "speaker_id": "2",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019166.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.5",
          "form": "앞으로 다신 복각 안해도 되니까",
          "original_form": "앞으로 다신 복각 안해도 되니까",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.6",
          "form": "한번만.......",
          "original_form": "한번만.......",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.7",
          "form": "캐릭터를 향한 강렬한 열망",
          "original_form": "캐릭터를 향한 강렬한 열망",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.8",
          "form": "그러다가 확뚫 당하면 어쩌려고 ...",
          "original_form": "그러다가 확뚫 당하면 어쩌려고 ...",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.9",
          "form": "무난한 캐디와 쓸만한 성능 모두 채운애가",
          "original_form": "무난한 캐디와 쓸만한 성능 모두 채운애가",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.10",
          "form": "흔치 않단말야....",
          "original_form": "흔치 않단말야....",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.11",
          "form": "아냐 나 라이덴 안할거야",
          "original_form": "아냐 나 라이덴 안할거야",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.12",
          "form": "그렇긴 하지",
          "original_form": "그렇긴 하지",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.13",
          "form": "2천장 존버할거고",
          "original_form": "2천장 존버할거고",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.14",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.15",
          "form": "소 안오면",
          "original_form": "소 안오면",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.16",
          "form": "아하 2천장",
          "original_form": "아하 2천장",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019166.1.17",
          "form": "버리고 걍 무료 5성이나",
          "original_form": "버리고 걍 무료 5성이나",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.18",
          "form": "쓸거야",
          "original_form": "쓸거야",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.19",
          "form": "이런겜은 캐한테 애정이 안생기면",
          "original_form": "이런겜은 캐한테 애정이 안생기면",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.21",
          "form": "접게된다구",
          "original_form": "접게된다구",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.22",
          "form": "그렇긴 함 ㅇㅇ...",
          "original_form": "그렇긴 함 ㅇㅇ...",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.23",
          "form": "걍 존버할거야",
          "original_form": "걍 존버할거야",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.24",
          "form": "얘네도 그거 알아서",
          "original_form": "얘네도 그거 알아서",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.25",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.26",
          "form": "굳이 특정 캐릭터 없어도",
          "original_form": "굳이 특정 캐릭터 없어도",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019166.1.27",
          "form": "플레이 가능하게 시스템 만든거같은데",
          "original_form": "플레이 가능하게 시스템 만든거같은데",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.28",
          "form": "ㅇㅇ........",
          "original_form": "ㅇㅇ........",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.29",
          "form": "몰라 여기 필터링 기준 대체 모르겠음",
          "original_form": "몰라 여기 필터링 기준 대체 모르겠음",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.30",
          "form": "아 근데 요이미야 나왔으니까",
          "original_form": "아 근데 요이미야 나왔으니까",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.31",
          "form": "이제 엠버는 불활 1티어에서",
          "original_form": "이제 엠버는 불활 1티어에서",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.32",
          "form": "내려왔겠지?",
          "original_form": "내려왔겠지?",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.33",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.34",
          "form": "솔직히 엠버가 1티어라니 이해할수없었어",
          "original_form": "솔직히 엠버가 1티어라니 이해할수없었어",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.35",
          "form": "근데 요이미야 성능 별로래",
          "original_form": "근데 요이미야 성능 별로래",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.36",
          "form": "ㅇㅇ별로였어",
          "original_form": "ㅇㅇ별로였어",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019166.1.37",
          "form": "궁이 구리레",
          "original_form": "궁이 구리레",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019166.1.38",
          "form": "구린데",
          "original_form": "구린데",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019166.1.39",
          "form": "스킬도 그닥이라",
          "original_form": "스킬도 그닥이라",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019166.1.40",
          "form": "ㅏ",
          "original_form": "ㅏ",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019166.1.41",
          "form": "악",
          "original_form": "악",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019166.1.42",
          "form": "난 성능이 좋은 4성 캐릭터 일거라고 생각을 했거든",
          "original_form": "난 성능이 좋은 4성 캐릭터 일거라고 생각을 했거든",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019166.1.43",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019166.1.44",
          "form": "그냥 흔한 기본캐릭터 수준이던데",
          "original_form": "그냥 흔한 기본캐릭터 수준이던데",
          "speaker_id": "1",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019166.1.45",
          "form": "단지 그 속성이 없어서 탑티어가 되어버린듯...",
          "original_form": "단지 그 속성이 없어서 탑티어가 되어버린듯...",
          "speaker_id": "1",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019166.1.46",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㄲㅋㅋㅋㅋ이전 탑티어가",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㄲㅋㅋㅋㅋ이전 탑티어가",
          "speaker_id": "2",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019166.1.47",
          "form": "엠버인것만 봐도.......",
          "original_form": "엠버인것만 봐도.......",
          "speaker_id": "2",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019166.1.48",
          "form": "솔직히 엠버는 스킬도 구리고 궁도 구렸어",
          "original_form": "솔직히 엠버는 스킬도 구리고 궁도 구렸어",
          "speaker_id": "2",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019166.1.49",
          "form": "ㅇㅇ 오토 없는 활캐라니;;",
          "original_form": "ㅇㅇ 오토 없는 활캐라니;;",
          "speaker_id": "1",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019166.1.50",
          "form": "4성인 이유도 걍 3성 등급이 존재하지 않아서 인듯;;",
          "original_form": "4성인 이유도 걍 3성 등급이 존재하지 않아서 인듯;;",
          "speaker_id": "1",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019166.1.51",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ궁도 ㄹㅇ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ궁도 ㄹㅇ",
