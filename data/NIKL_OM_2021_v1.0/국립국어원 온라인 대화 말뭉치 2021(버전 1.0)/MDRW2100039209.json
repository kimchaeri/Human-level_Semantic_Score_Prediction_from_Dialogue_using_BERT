      "utterance": [
        {
          "id": "MDRW2100039209.1.1",
          "form": "뭐 사고싶은거 있으세요?.",
          "original_form": "뭐 사고싶은거 있으세요?.",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039209.1.2",
          "form": "사고싶은건 가을옷?",
          "original_form": "사고싶은건 가을옷?",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039209.1.3",
          "form": "근데 오늘 충동적으로 미용실와서",
          "original_form": "근데 오늘 충동적으로 미용실와서",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039209.1.4",
          "form": "생각지않은",
          "original_form": "생각지않은",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039209.1.5",
          "form": "머리?",
          "original_form": "머리?",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039209.1.6",
          "form": "지출이생겻어욤ㅋ",
          "original_form": "지출이생겻어욤ㅋ",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039209.1.7",
          "form": "ㅋㅋ 맘에 드시나요?",
          "original_form": "ㅋㅋ 맘에 드시나요?",
          "speaker_id": "2",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039209.1.8",
          "form": "하고잇슴다",
          "original_form": "하고잇슴다",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039209.1.9",
          "form": "저도 파마이용권 사둘까..해요ㅋㅋ",
          "original_form": "저도 파마이용권 사둘까..해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039209.1.10",
          "form": "좋아하는브랜드잇으신가용",
          "original_form": "좋아하는브랜드잇으신가용",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039209.1.11",
          "form": "따로 브랜드를 따지지않아요",
          "original_form": "따로 브랜드를 따지지않아요",
          "speaker_id": "2",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100039209.1.12",
          "form": "히피펌하고싶어서ㅋㅋ",
          "original_form": "히피펌하고싶어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100039209.1.13",
          "form": "ㅋㅋ와우",
          "original_form": "ㅋㅋ와우",
          "speaker_id": "1",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100039209.1.14",
          "form": "저는 굵게 말고 잇어요ㅋㅋ",
          "original_form": "저는 굵게 말고 잇어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100039209.1.15",
          "form": "근데 짧아서 안된다고ㅜㅜ 길러서 가야해요!",
          "original_form": "근데 짧아서 안된다고ㅜㅜ 길러서 가야해요!",
          "speaker_id": "2",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100039209.1.16",
          "form": "굵으면 우아해요?",
          "original_form": "굵으면 우아해요?",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.17",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.18",
          "form": "저는 그냥 자연스러운게좋아서",
          "original_form": "저는 그냥 자연스러운게좋아서",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.19",
          "form": "굵게말면",
          "original_form": "굵게말면",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.20",
          "form": "빨리풀러요ㅜㅜ",
          "original_form": "빨리풀러요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.21",
          "form": "그런데 너무 금방 ㅜ",
          "original_form": "그런데 너무 금방 ㅜ",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.22",
          "form": "마자요ㅜㅜ",
          "original_form": "마자요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.23",
          "form": "맞아요 한지 잘 모르게ㅜ",
          "original_form": "맞아요 한지 잘 모르게ㅜ",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.24",
          "form": "오늘은 충동적으로",
          "original_form": "오늘은 충동적으로",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.25",
          "form": "와밧슴다",
          "original_form": "와밧슴다",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.26",
          "form": "전 빠글빠글하게 해요ㅋㅋ",
          "original_form": "전 빠글빠글하게 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039209.1.27",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:42"
        },
        {
          "id": "MDRW2100039209.1.28",
          "form": "기분전환이죠!!",
          "original_form": "기분전환이죠!!",
