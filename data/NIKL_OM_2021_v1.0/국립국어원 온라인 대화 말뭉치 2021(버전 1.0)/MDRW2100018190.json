      "utterance": [
        {
          "id": "MDRW2100018190.1.1",
          "form": "요즘에 뭐 산거 있으세요??",
          "original_form": "요즘에 뭐 산거 있으세요??",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018190.1.2",
          "form": "저 제모기 샀어요",
          "original_form": "저 제모기 샀어요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018190.1.3",
          "form": "근데 피부과 가는게 나을거 같아요",
          "original_form": "근데 피부과 가는게 나을거 같아요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018190.1.4",
          "form": "ㅋㅋㅋㅋ 여름 끝났는데 뭘 미세요",
          "original_form": "ㅋㅋㅋㅋ 여름 끝났는데 뭘 미세요",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018190.1.5",
          "form": "이게 장기전이에요",
          "original_form": "이게 장기전이에요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018190.1.6",
          "form": "저는 옷으로 숨기고 다닐려고 그랬는데",
          "original_form": "저는 옷으로 숨기고 다닐려고 그랬는데",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018190.1.7",
          "form": "제모기 쓰면 얇아지긴 해요??",
          "original_form": "제모기 쓰면 얇아지긴 해요??",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018190.1.8",
          "form": "저는 잘안써서 아직 모르겠어요",
          "original_form": "저는 잘안써서 아직 모르겠어요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018190.1.9",
          "form": "추천하진 않아여",
          "original_form": "추천하진 않아여",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018190.1.10",
          "form": "실큰제모기입니다",
          "original_form": "실큰제모기입니다",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018190.1.11",
          "form": "실큰 제모기 지금 적어둬야겠어요",
          "original_form": "실큰 제모기 지금 적어둬야겠어요",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018190.1.12",
          "form": "ㅋㅋㅋㅋ 효과있으셔야될텐데요",
          "original_form": "ㅋㅋㅋㅋ 효과있으셔야될텐데요",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018190.1.13",
          "form": "생각보다 아파요",
          "original_form": "생각보다 아파요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018190.1.14",
          "form": "그래서 안쓰고 약하게 해서",
          "original_form": "그래서 안쓰고 약하게 해서",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018190.1.15",
          "form": "더 효과가 없나봐요",
          "original_form": "더 효과가 없나봐요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018190.1.16",
          "form": "요즘에 구매하신게 있나요?",
          "original_form": "요즘에 구매하신게 있나요?",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018190.1.17",
          "form": "아 그렇구나 저는 그냥 밀고 있어서",
          "original_form": "아 그렇구나 저는 그냥 밀고 있어서",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018190.1.18",
          "form": "요즘에는 뭘 샀는지 생각 좀 해봐야겠네요",
          "original_form": "요즘에는 뭘 샀는지 생각 좀 해봐야겠네요",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018190.1.19",
          "form": "먹을거만 사먹은거 같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹을거만 사먹은거 같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018190.1.20",
          "form": "오늘 치즈를 사셨죠",
          "original_form": "오늘 치즈를 사셨죠",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018190.1.21",
          "form": "ㅋㅋㅋㅋㅋ 과자도 샀어요",
          "original_form": "ㅋㅋㅋㅋㅋ 과자도 샀어요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018190.1.22",
          "form": "과자는 뭐 좋아하세요?",
          "original_form": "과자는 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018190.1.23",
          "form": "저는 꼬북칩쵸큐츄러스",
          "original_form": "저는 꼬북칩쵸큐츄러스",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018190.1.24",
          "form": "저는 감자칩 좋아해요 요즘은 수미칩만 먹고 있어요",
          "original_form": "저는 감자칩 좋아해요 요즘은 수미칩만 먹고 있어요",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018190.1.25",
          "form": "꼬북칩 맛있죠",
          "original_form": "꼬북칩 맛있죠",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018190.1.26",
          "form": "스윙칩볶음고추장맛",
          "original_form": "스윙칩볶음고추장맛",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018190.1.27",
          "form": "예감치즈그라탕맛",
          "original_form": "예감치즈그라탕맛",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018190.1.28",
          "form": "제가 좋아하는 감자들이에여",
          "original_form": "제가 좋아하는 감자들이에여",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018190.1.29",
          "form": "ㅋㅋㅋㅋ 좋아하는 맛이 딱 있으시네요",
          "original_form": "ㅋㅋㅋㅋ 좋아하는 맛이 딱 있으시네요",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018190.1.30",
          "form": "저는 포카칩 기본 어니언 수미칩 기본 좋아해요",
          "original_form": "저는 포카칩 기본 어니언 수미칩 기본 좋아해요",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018190.1.31",
          "form": "콰삭칩이라고 나온거 먹어봤거든요",
          "original_form": "콰삭칩이라고 나온거 먹어봤거든요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018190.1.32",
          "form": "그건 좀 별로였어요",
          "original_form": "그건 좀 별로였어요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018190.1.33",
          "form": "저 다먹어봤어요",
          "original_form": "저 다먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018190.1.34",
          "form": "과자 좋아하시는구나!!",
          "original_form": "과자 좋아하시는구나!!",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018190.1.35",
          "form": "뭔들....",
          "original_form": "뭔들....",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018190.1.36",
          "form": "먹는거 다 좋아해서 돼지죠ㅋ",
