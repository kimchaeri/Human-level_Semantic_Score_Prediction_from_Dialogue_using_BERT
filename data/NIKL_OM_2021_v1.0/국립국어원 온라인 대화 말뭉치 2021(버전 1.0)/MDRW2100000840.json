      "utterance": [
        {
          "id": "MDRW2100000840.1.1",
          "form": "혹시 결혼하셨어요? 연애중이라던가?",
          "original_form": "혹시 결혼하셨어요? 연애중이라던가?",
          "speaker_id": "2",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000840.1.2",
          "form": "대박 결혼하셨어요?",
          "original_form": "대박 결혼하셨어요?",
          "speaker_id": "1",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000840.1.3",
          "form": "전 결혼도 연애도 아무것도 안했어요!",
          "original_form": "전 결혼도 연애도 아무것도 안했어요!",
          "speaker_id": "1",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000840.1.4",
          "form": "네네 ㅎㅎ 전 29인데 작년겨울에 올렸어요 ~ 연애 안해보신거예요 ??",
          "original_form": "네네 ㅎㅎ 전 29인데 작년겨울에 올렸어요 ~ 연애 안해보신거예요 ??",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000840.1.5",
          "form": "네,, 완전 어릴 때 했으니까 그건 연애라고 하기도 그렇고",
          "original_form": "네,, 완전 어릴 때 했으니까 그건 연애라고 하기도 그렇고",
          "speaker_id": "1",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000840.1.6",
          "form": "몇년전도 그냥 썸만 타다가 끝났어요 ㅎ",
          "original_form": "몇년전도 그냥 썸만 타다가 끝났어요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000840.1.7",
          "form": "몇살이신지 물어봐도될까용 ?",
          "original_form": "몇살이신지 물어봐도될까용 ?",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000840.1.8",
          "form": "저 스물여섯이에요 !",
          "original_form": "저 스물여섯이에요 !",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000840.1.9",
          "form": "근데 연애에 대해 관심도 딱히 안생기더라구요",
          "original_form": "근데 연애에 대해 관심도 딱히 안생기더라구요",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000840.1.10",
          "form": "뭐 연애가 필수는 아니니깐요 ㅎㅎ",
          "original_form": "뭐 연애가 필수는 아니니깐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000840.1.11",
          "form": "좋은 상대가 생기면 자연스레",
          "original_form": "좋은 상대가 생기면 자연스레",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000840.1.12",
          "form": "만나고 하는거죠 뭐 ^^",
          "original_form": "만나고 하는거죠 뭐 ^^",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000840.1.13",
          "form": "맞아요 ㅎ 그냥 천천히",
          "original_form": "맞아요 ㅎ 그냥 천천히",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000840.1.14",
          "form": "운명이 나오기를..",
          "original_form": "운명이 나오기를..",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000840.1.15",
          "form": "맘에드는 운명이생기면",
          "original_form": "맘에드는 운명이생기면",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000840.1.16",
          "form": "어떤 데이트 해보고싶으세요?",
          "original_form": "어떤 데이트 해보고싶으세요?",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000840.1.17",
          "form": "데이트.. 움... 그냥 드라이브 아니면 뭐 산책하면서 걷는...",
          "original_form": "데이트.. 움... 그냥 드라이브 아니면 뭐 산책하면서 걷는...",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000840.1.18",
          "form": "운명과 다르게 소소한 걸 하고싶어하네요",
          "original_form": "운명과 다르게 소소한 걸 하고싶어하네요",
