      "utterance": [
        {
          "id": "MDRW2100013639.1.1",
          "form": "쓰읍..군대 가세요?",
          "original_form": "쓰읍..군대 가세요?",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013639.1.2",
          "form": "당신 배추를...!!!!",
          "original_form": "당신 배추를...!!!!",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013639.1.3",
          "form": "추석 때 울집 와봐요",
          "original_form": "추석 때 울집 와봐요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013639.1.4",
          "form": "카레가루..ㄹㅇ 꿀팁이다",
          "original_form": "카레가루..ㄹㅇ 꿀팁이다",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013639.1.5",
          "form": "군대가세요?",
          "original_form": "군대가세요?",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013639.1.6",
          "form": "ㅈㅅ",
          "original_form": "ㅈㅅ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013639.1.7",
          "form": "아 ㄱㅊ",
          "original_form": "아 ㄱㅊ",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013639.1.8",
          "form": "아직 안갔어요",
          "original_form": "아직 안갔어요",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013639.1.9",
          "form": "일이 많아서",
          "original_form": "일이 많아서",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013639.1.10",
          "form": "정리좀 되면 가려고요",
          "original_form": "정리좀 되면 가려고요",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013639.1.11",
          "form": "저에게 질문을 하고 사라지시다니",
          "original_form": "저에게 질문을 하고 사라지시다니",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013639.1.12",
          "form": "정말 정말 미안한가보군요",
          "original_form": "정말 정말 미안한가보군요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013639.1.13",
          "form": "이해할게요 돌아와봐요",
          "original_form": "이해할게요 돌아와봐요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013639.1.14",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013639.1.15",
          "form": "아나 군대 정리되면 갈 수 있는 거였냐고요",
          "original_form": "아나 군대 정리되면 갈 수 있는 거였냐고요",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013639.1.16",
          "form": "아 저 밧데리가 나가가지고 돌아왔어요",
          "original_form": "아 저 밧데리가 나가가지고 돌아왔어요",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013639.1.17",
          "form": "못갈건뭐야",
          "original_form": "못갈건뭐야",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013639.1.18",
          "form": "name2씨 어디가셨죠? 돌아와",
          "original_form": "&name2&씨 어디가셨죠? 돌아와",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013639.1.19",
          "form": "앗 오셧다",
          "original_form": "앗 오셧다",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013639.1.20",
          "form": "앗 바떼리",
          "original_form": "앗 바떼리",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013639.1.21",
          "form": "군대 가면 어떤 보직으로 가고싶나요",
          "original_form": "군대 가면 어떤 보직으로 가고싶나요",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013639.1.22",
          "form": "생각해본적은 없는데",
          "original_form": "생각해본적은 없는데",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013639.1.23",
          "form": "전 조교. 남들 굴리고싶네요(?)",
          "original_form": "전 조교. 남들 굴리고싶네요(?)",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013639.1.24",
          "form": "권련의 맛",
          "original_form": "권련의 맛",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013639.1.25",
          "form": "어.. 사실 크게 고민을",
          "original_form": "어.. 사실 크게 고민을",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013639.1.26",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013639.1.27",
          "form": "권력",
          "original_form": "권력",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013639.1.28",
          "form": "악마의 조교 잘할듯",
          "original_form": "악마의 조교 잘할듯",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013639.1.29",
          "form": "광기어려서",
          "original_form": "광기어려서",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013639.1.30",
          "form": "똑바로 안합니까아아아악",
          "original_form": "똑바로 안합니까아아아악",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013639.1.31",
          "form": "ㅈ..제가요?",
          "original_form": "ㅈ..제가요?",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013639.1.32",
          "form": "잘할거같은데",
          "original_form": "잘할거같은데",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013639.1.33",
          "form": "쓰읍...맞는 말이라 반박 불가",
          "original_form": "쓰읍...맞는 말이라 반박 불가",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013639.1.34",
          "form": "본인을 너무 과소평..",
          "original_form": "본인을 너무 과소평..",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013639.1.35",
