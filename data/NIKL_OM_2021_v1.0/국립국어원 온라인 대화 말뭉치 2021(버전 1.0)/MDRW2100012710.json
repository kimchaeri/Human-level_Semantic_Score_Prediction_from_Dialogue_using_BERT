      "utterance": [
        {
          "id": "MDRW2100012710.1.1",
          "form": "좋네요ㅋㅋㅋ epl개막한건",
          "original_form": "좋네요ㅋㅋㅋ epl개막한건",
          "speaker_id": "2",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012710.1.2",
          "form": "챙겨보시나요?",
          "original_form": "챙겨보시나요?",
          "speaker_id": "2",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012710.1.3",
          "form": "시간대가 안 맞아서 하이라이트만 줍줍하고 있어요 ㅋㅋ",
          "original_form": "시간대가 안 맞아서 하이라이트만 줍줍하고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012710.1.4",
          "form": "와우 하이라이트를 보시다니",
          "original_form": "와우 하이라이트를 보시다니",
          "speaker_id": "2",
          "time": "20210825 00:23"
        },
        {
          "id": "MDRW2100012710.1.5",
          "form": "그나마 최애팀은 어디인가요?",
          "original_form": "그나마 최애팀은 어디인가요?",
          "speaker_id": "2",
          "time": "20210825 00:23"
        },
        {
          "id": "MDRW2100012710.1.6",
          "form": "최애팀이라기보다 손흥민 때문에 보는거라 거의 토트넘 위주로만 봐요 ㅎㅎ",
          "original_form": "최애팀이라기보다 손흥민 때문에 보는거라 거의 토트넘 위주로만 봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:24"
        },
        {
          "id": "MDRW2100012710.1.7",
          "form": "오ㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012710.1.8",
          "form": "맨시티전 보셨나요?",
          "original_form": "맨시티전 보셨나요?",
          "speaker_id": "2",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012710.1.9",
          "form": "진짜 맨시티 킬러인가봐요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 맨시티 킬러인가봐요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012710.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100012710.1.11",
          "form": "그골 진짜",
          "original_form": "그골 진짜",
          "speaker_id": "2",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100012710.1.12",
          "form": "간지나지않았나요?",
          "original_form": "간지나지않았나요?",
          "speaker_id": "2",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100012710.1.13",
          "form": "쏘니존 진짜 감아차는거 너무 좋아요ㅠ 코너랑 프리킥도 전담으로 차는 것 같던데 크으",
          "original_form": "쏘니존 진짜 감아차는거 너무 좋아요ㅠ 코너랑 프리킥도 전담으로 차는 것 같던데 크으",
          "speaker_id": "1",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012710.1.14",
          "form": "그덕에 햄스트링이 무리가온거같더라고요ㅠ",
          "original_form": "그덕에 햄스트링이 무리가온거같더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012710.1.15",
          "form": "코너킥 전담인건 너무멋있어요ㅋㅋㅋ",
          "original_form": "코너킥 전담인건 너무멋있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012710.1.16",
          "form": "으악 부상이 그래서 그렇구나ㅠㅠ",
          "original_form": "으악 부상이 그래서 그렇구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012710.1.17",
          "form": "올해 토트넘 스쿼드 어때요?",
          "original_form": "올해 토트넘 스쿼드 어때요?",
          "speaker_id": "1",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012710.1.18",
          "form": "음... 사실",
          "original_form": "음... 사실",
          "speaker_id": "2",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012710.1.19",
          "form": "손흥민 톱으로 경기를 잘풀어나갈수있을지 의문이 들긴해요",
          "original_form": "손흥민 톱으로 경기를 잘풀어나갈수있을지 의문이 들긴해요",
          "speaker_id": "2",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012710.1.20",
          "form": "케인의 이적이 불확실해지면",
          "original_form": "케인의 이적이 불확실해지면",
          "speaker_id": "2",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012710.1.21",
          "form": "다시 쏜케로 갈거같긴한데",
          "original_form": "다시 쏜케로 갈거같긴한데",
          "speaker_id": "2",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012710.1.22",
          "form": "손흥민 톱 체제를 유지하려면 다른 선수를 더",
          "original_form": "손흥민 톱 체제를 유지하려면 다른 선수를 더",
          "speaker_id": "2",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100012710.1.23",
          "form": "영입해와야겠더라고요ㅠ",
          "original_form": "영입해와야겠더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100012710.1.24",
          "form": "여전히 선수풀이 백지장이네요ㅜㅠㅠ",
          "original_form": "여전히 선수풀이 백지장이네요ㅜㅠㅠ",
          "speaker_id": "1",
