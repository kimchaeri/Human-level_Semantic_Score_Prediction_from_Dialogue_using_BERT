      "utterance": [
        {
          "id": "MDRW2100008116.1.1",
          "form": "영양제 드시는거있나요?",
          "original_form": "영양제 드시는거있나요?",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008116.1.2",
          "form": "아하 저는요즘 친구가 선물해준 비타민 씨 먹어요",
          "original_form": "아하 저는요즘 친구가 선물해준 비타민 씨 먹어요",
          "speaker_id": "1",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008116.1.3",
          "form": "카톡 기프티콘으로 와서 주문 딱 해가지구",
          "original_form": "카톡 기프티콘으로 와서 주문 딱 해가지구",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008116.1.4",
          "form": "오ㅎ 좋은친구분이시네요",
          "original_form": "오ㅎ 좋은친구분이시네요",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008116.1.5",
          "form": "유산균이랑 오메가 이런거 많이먹더라구요",
          "original_form": "유산균이랑 오메가 이런거 많이먹더라구요",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008116.1.6",
          "form": "생일이라고 받았죠 ㅎㅎ",
          "original_form": "생일이라고 받았죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008116.1.7",
          "form": "아 맞아요 요즘엔 그런게 유행이던데",
          "original_form": "아 맞아요 요즘엔 그런게 유행이던데",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008116.1.8",
          "form": "건강생각해서 꾸준히 먹어야되는데",
          "original_form": "건강생각해서 꾸준히 먹어야되는데",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008116.1.9",
          "form": "귀차나서 잘 안챙겨먹게 되네요",
          "original_form": "귀차나서 잘 안챙겨먹게 되네요",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008116.1.10",
          "form": "저는 겉면에 인어공주가 그려져있길래 예쁘네? 하고 먹는중",
          "original_form": "저는 겉면에 인어공주가 그려져있길래 예쁘네? 하고 먹는중",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008116.1.11",
          "form": "유산균은 챙겨먹으면",
          "original_form": "유산균은 챙겨먹으면",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008116.1.12",
          "form": "정말 좋대요 ㅎㅎㅎ",
          "original_form": "정말 좋대요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008116.1.13",
          "form": "아니면 비타민 씨 하고 디",
          "original_form": "아니면 비타민 씨 하고 디",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008116.1.14",
          "form": "장에 좋다고하는데 락토핏 한통 먹어봤어요",
          "original_form": "장에 좋다고하는데 락토핏 한통 먹어봤어요",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008116.1.15",
          "form": "아아 뭔줄알아요 그거 유명하죠 ㅎㅎ",
          "original_form": "아아 뭔줄알아요 그거 유명하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008116.1.16",
          "form": "비타민가 피로회복에 좋은건가요?",
          "original_form": "비타민가 피로회복에 좋은건가요?",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008116.1.17",
          "form": "면역력에 좋대요 © 많이 먹으면",
          "original_form": "면역력에 좋대요 © 많이 먹으면",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008116.1.18",
          "form": "기본적으로 감기에좋고",
          "original_form": "기본적으로 감기에좋고",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008116.1.19",
          "form": "D는 한국사람 계속 결핍증이라니깐요",
          "original_form": "D는 한국사람 계속 결핍증이라니깐요",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008116.1.20",
          "form": "아무래도 비타민이랑 유산균이 최고아닐까 싶엉ㅅ",
          "original_form": "아무래도 비타민이랑 유산균이 최고아닐까 싶엉ㅅ",
          "speaker_id": "1",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008116.1.21",
          "form": "비타민d는 먹어줘야되겠더라구요",
          "original_form": "비타민d는 먹어줘야되겠더라구요",
          "speaker_id": "2",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008116.1.22",
          "form": "그쵸 사실 햇빛 잘 쐬면 공짜인데 아깝긴해두 ㅠ",
          "original_form": "그쵸 사실 햇빛 잘 쐬면 공짜인데 아깝긴해두 ㅠ",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008116.1.23",
          "form": "주사로 맞으려면 비싸더라구요 오만원인가",
          "original_form": "주사로 맞으려면 비싸더라구요 오만원인가",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008116.1.24",
          "form": "주사가 효과가 좋긴하겠어요ㅎ",
          "original_form": "주사가 효과가 좋긴하겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008116.1.25",
          "form": "ㅎㅎㅎ 아침에 눈이 일찍떠져요",
          "original_form": "ㅎㅎㅎ 아침에 눈이 일찍떠져요",
          "speaker_id": "1",
