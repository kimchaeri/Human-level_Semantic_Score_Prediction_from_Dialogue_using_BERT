      "utterance": [
        {
          "id": "MDRW2100022969.1.1",
          "form": "국내여행 좋아하세요?",
          "original_form": "국내여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100022969.1.2",
          "form": "네 여행 좋아하는데 경험은 별로 없어요",
          "original_form": "네 여행 좋아하는데 경험은 별로 없어요",
          "speaker_id": "2",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022969.1.3",
          "form": "여행 많이 다니셨나요?",
          "original_form": "여행 많이 다니셨나요?",
          "speaker_id": "2",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100022969.1.4",
          "form": "저는 별로 안좋아해서 안가봤어요ㅠ",
          "original_form": "저는 별로 안좋아해서 안가봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022969.1.5",
          "form": "요즘 캠핑 가보고싶어지더라구요",
          "original_form": "요즘 캠핑 가보고싶어지더라구요",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022969.1.6",
          "form": "아 요즘 캠핑이 유행이죠",
          "original_form": "아 요즘 캠핑이 유행이죠",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022969.1.7",
          "form": "종류도 다양하고요",
          "original_form": "종류도 다양하고요",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022969.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100022969.1.9",
          "form": "궁금하더라구요~",
          "original_form": "궁금하더라구요~",
          "speaker_id": "1",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100022969.1.10",
          "form": "한달정도만 차끌고 가보고싶어요",
          "original_form": "한달정도만 차끌고 가보고싶어요",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100022969.1.11",
          "form": "좋은 경험이 되겠네요",
          "original_form": "좋은 경험이 되겠네요",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100022969.1.12",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100022969.1.13",
          "form": "전에 핑클 캠핑할때 그 코스로 가봐도 좋겠더라구요",
          "original_form": "전에 핑클 캠핑할때 그 코스로 가봐도 좋겠더라구요",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100022969.1.14",
          "form": "아 핑클 캠핑 간 거 기사로 본 것 같아요ㅎ 캠핑 코스가 좋은가봐요?",
          "original_form": "아 핑클 캠핑 간 거 기사로 본 것 같아요ㅎ 캠핑 코스가 좋은가봐요?",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100022969.1.15",
          "form": "넹! 이곳저곳 다 가보니 좋아보이더라구요~",
          "original_form": "넹! 이곳저곳 다 가보니 좋아보이더라구요~",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100022969.1.16",
          "form": "여행지 많으면 좋죠 그만큼 다양한 경험도 하니까요",
          "original_form": "여행지 많으면 좋죠 그만큼 다양한 경험도 하니까요",
          "speaker_id": "2",
          "time": "20210830 07:19"
        },
        {
          "id": "MDRW2100022969.1.17",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210830 07:47"
        },
        {
          "id": "MDRW2100022969.1.18",
          "form": "혼자는 약간 외로울 것 같아 친구랑 가고싶네요!",
          "original_form": "혼자는 약간 외로울 것 같아 친구랑 가고싶네요!",
          "speaker_id": "1",
          "time": "20210830 07:47"
        },
        {
          "id": "MDRW2100022969.1.19",
          "form": "친구랑 함께면 좋죠 전 혼자 여행도 한번 해보고 싶어요",
          "original_form": "친구랑 함께면 좋죠 전 혼자 여행도 한번 해보고 싶어요",
          "speaker_id": "2",
          "time": "20210830 16:22"
        },
        {
          "id": "MDRW2100022969.1.20",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100022969.1.21",
          "form": "유럽에 혼자 나가서 돌아다녀보고 싶어요",
          "original_form": "유럽에 혼자 나가서 돌아다녀보고 싶어요",
          "speaker_id": "1",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100022969.1.22",
          "form": "아 유럽은 누구나 꿈꾸는 여행지죠 물론 저두요",
          "original_form": "아 유럽은 누구나 꿈꾸는 여행지죠 물론 저두요",
          "speaker_id": "2",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100022969.1.23",
          "form": "맞아요 누가 지원 좀 해줬음 좋겠어요ㅋㅋㅋ",
          "original_form": "맞아요 누가 지원 좀 해줬음 좋겠어요ㅋㅋㅋ",
          "speaker_id": "1",
