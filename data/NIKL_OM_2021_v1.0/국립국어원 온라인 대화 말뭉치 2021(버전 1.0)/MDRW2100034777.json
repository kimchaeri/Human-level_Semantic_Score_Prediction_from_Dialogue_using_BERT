      "utterance": [
        {
          "id": "MDRW2100034777.1.1",
          "form": "오늘 저녁은 뭐드셨어용?",
          "original_form": "오늘 저녁은 뭐드셨어용?",
          "speaker_id": "2",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100034777.1.2",
          "form": "오늘 저녁은 고기 먹었어요",
          "original_form": "오늘 저녁은 고기 먹었어요",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100034777.1.3",
          "form": "저녁 뭐드셨나요?",
          "original_form": "저녁 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100034777.1.4",
          "form": "와 고기ㅜㅜ",
          "original_form": "와 고기ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100034777.1.5",
          "form": "저는 어제 오리고기먹고 체하는바람에ㅠㅠ",
          "original_form": "저는 어제 오리고기먹고 체하는바람에ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034777.1.6",
          "form": "오늘 내내 죽먹었어요..",
          "original_form": "오늘 내내 죽먹었어요..",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034777.1.7",
          "form": "머리까지 아프고 속은 뒤집어지고ㅜㅜ 어후..",
          "original_form": "머리까지 아프고 속은 뒤집어지고ㅜㅜ 어후..",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034777.1.8",
          "form": "저녁도 죽 드셨나요?",
          "original_form": "저녁도 죽 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034777.1.9",
          "form": "네 완전 죽데이..",
          "original_form": "네 완전 죽데이..",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034777.1.10",
          "form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100034777.1.11",
          "form": "속 안좋을 때는 아예 굶는 것도 방법인거같아요",
          "original_form": "속 안좋을 때는 아예 굶는 것도 방법인거같아요",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100034777.1.12",
          "form": "저는 전에 심하게 체한 적 있어서 하루 그냥 안먹었어여",
          "original_form": "저는 전에 심하게 체한 적 있어서 하루 그냥 안먹었어여",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100034777.1.13",
          "form": "그러고 약먹었더니 좀 낫더라구요..",
          "original_form": "그러고 약먹었더니 좀 낫더라구요..",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100034777.1.14",
          "form": "저는 나아가는 과정이었는지 죽을 먹으니까 좀 더 낫더라구요!",
          "original_form": "저는 나아가는 과정이었는지 죽을 먹으니까 좀 더 낫더라구요!",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100034777.1.15",
          "form": "그래서 라떼도 도전 ...ㅋㅋ",
          "original_form": "그래서 라떼도 도전 ...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100034777.1.16",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100034777.1.17",
          "form": "앗 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034777.1.18",
          "form": "라떼도 괜찮던가요",
          "original_form": "라떼도 괜찮던가요",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034777.1.19",
          "form": "지금도 간간히 머리는 아픈데 아까보단 괜찮은거 같아요ㅋㅋㅋㅋ",
          "original_form": "지금도 간간히 머리는 아픈데 아까보단 괜찮은거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034777.1.20",
          "form": "지금은 무화과랑 복숭아도 한입 먹었어요",
          "original_form": "지금은 무화과랑 복숭아도 한입 먹었어요",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034777.1.21",
          "form": "와 무화과 맛있죠",
          "original_form": "와 무화과 맛있죠",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034777.1.22",
          "form": "달달하니 과즙도 맛있어요",
          "original_form": "달달하니 과즙도 맛있어요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034777.1.23",
          "form": "여름엔 복숭아랑 무화과 먹는 재미인거같아요",
          "original_form": "여름엔 복숭아랑 무화과 먹는 재미인거같아요",
          "speaker_id": "1",
          "time": "20210828 22:11"
        }
      ]
