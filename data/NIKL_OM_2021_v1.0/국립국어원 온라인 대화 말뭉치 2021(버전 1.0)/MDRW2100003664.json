      "utterance": [
        {
          "id": "MDRW2100003664.1.1",
          "form": "비엄청오니까 건조기 사고싶더라구여",
          "original_form": "비엄청오니까 건조기 사고싶더라구여",
          "speaker_id": "1",
          "time": "20210821 10:13"
        },
        {
          "id": "MDRW2100003664.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 10:15"
        },
        {
          "id": "MDRW2100003664.1.3",
          "form": "건조기 요새 필수죠",
          "original_form": "건조기 요새 필수죠",
          "speaker_id": "2",
          "time": "20210821 10:15"
        },
        {
          "id": "MDRW2100003664.1.4",
          "form": "잔 오래된것니 있는데",
          "original_form": "잔 오래된것니 있는데",
          "speaker_id": "2",
          "time": "20210821 10:15"
        },
        {
          "id": "MDRW2100003664.1.5",
          "form": "옷이 아동복 되버려요 ㅜㅜ줄어들아요",
          "original_form": "옷이 아동복 되버려요 ㅜㅜ줄어들아요",
          "speaker_id": "2",
          "time": "20210821 10:16"
        },
        {
          "id": "MDRW2100003664.1.6",
          "form": "들어요",
          "original_form": "들어요",
          "speaker_id": "2",
          "time": "20210821 10:16"
        },
        {
          "id": "MDRW2100003664.1.7",
          "form": "새로나온 제품 사고싶어요",
          "original_form": "새로나온 제품 사고싶어요",
          "speaker_id": "2",
          "time": "20210821 10:17"
        },
        {
          "id": "MDRW2100003664.1.8",
          "form": "요새같이 습한기후로 바뀔때 필수같아요",
          "original_form": "요새같이 습한기후로 바뀔때 필수같아요",
          "speaker_id": "2",
          "time": "20210821 10:18"
        },
        {
          "id": "MDRW2100003664.1.9",
          "form": "건조기가 줄어드는게 좀그렇긴하더라구여",
          "original_form": "건조기가 줄어드는게 좀그렇긴하더라구여",
          "speaker_id": "1",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003664.1.10",
          "form": "근데 진짜 뽀송하고 바짝말려지고",
          "original_form": "근데 진짜 뽀송하고 바짝말려지고",
          "speaker_id": "1",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003664.1.11",
          "form": "네 조심해도 한순간에 실수하고",
          "original_form": "네 조심해도 한순간에 실수하고",
          "speaker_id": "2",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003664.1.12",
          "form": "그 건조되고 나는 냄새 아시져",
          "original_form": "그 건조되고 나는 냄새 아시져",
          "speaker_id": "1",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003664.1.13",
          "form": "자주쓰면 굘국 줄어요",
          "original_form": "자주쓰면 굘국 줄어요",
          "speaker_id": "2",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003664.1.14",
          "form": "결국",
          "original_form": "결국",
          "speaker_id": "2",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003664.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003664.1.16",
          "form": "저 그거 싫어요",
          "original_form": "저 그거 싫어요",
          "speaker_id": "2",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003664.1.17",
          "form": "요샌 그거 없애는 시트도 있다던데",
          "original_form": "요샌 그거 없애는 시트도 있다던데",
          "speaker_id": "2",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.18",
          "form": "그 종이형 유연세제넣고",
          "original_form": "그 종이형 유연세제넣고",
          "speaker_id": "1",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.19",
          "form": "하면 넘좋드라규여",
          "original_form": "하면 넘좋드라규여",
          "speaker_id": "1",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.20",
          "form": "네 효과가 좋군요",
          "original_form": "네 효과가 좋군요",
          "speaker_id": "2",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.21",
          "form": "저도 사봐야겠어요",
          "original_form": "저도 사봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.22",
          "form": "예전에 사용한적있는데 지금은없어오ㅜ",
          "original_form": "예전에 사용한적있는데 지금은없어오ㅜ",
          "speaker_id": "1",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.23",
          "form": "전 구형이라 더심해요",
          "original_form": "전 구형이라 더심해요",
          "speaker_id": "2",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.24",
          "form": "사고싶어요ㅜㅜㅜㅜ",
          "original_form": "사고싶어요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.25",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.26",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003664.1.27",
          "form": "목돈이라 건조기 사기 힘들어요",
          "original_form": "목돈이라 건조기 사기 힘들어요",
          "speaker_id": "2",
          "time": "20210821 10:21"
        },
        {
          "id": "MDRW2100003664.1.28",
          "form": "세탁기도 바꿀때됐는데",
          "original_form": "세탁기도 바꿀때됐는데",
          "speaker_id": "1",
          "time": "20210821 10:21"
        },
        {
          "id": "MDRW2100003664.1.29",
          "form": "돈쓸일많네요ㅜㅜ",
          "original_form": "돈쓸일많네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 10:21"
        }
      ]
    }
