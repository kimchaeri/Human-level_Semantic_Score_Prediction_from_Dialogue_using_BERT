      "utterance": [
        {
          "id": "MDRW2100046789.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 11:10"
        },
        {
          "id": "MDRW2100046789.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211105 11:36"
        },
        {
          "id": "MDRW2100046789.1.3",
          "form": "\r\n영화 좋아하시나요?",
          "original_form": "\r\n영화 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211105 11:36"
        },
        {
          "id": "MDRW2100046789.1.4",
          "form": "네 영화도 좋아하고 드라마 보는것도 좋아해요",
          "original_form": "네 영화도 좋아하고 드라마 보는것도 좋아해요",
          "speaker_id": "1",
          "time": "20211105 11:36"
        },
        {
          "id": "MDRW2100046789.1.5",
          "form": "둘다 좋아하시는구나 ㅎㅎ",
          "original_form": "둘다 좋아하시는구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 11:36"
        },
        {
          "id": "MDRW2100046789.1.6",
          "form": "최근에 재밌게 보신 영화나 드라마 있으신가요?",
          "original_form": "최근에 재밌게 보신 영화나 드라마 있으신가요?",
          "speaker_id": "2",
          "time": "20211105 11:36"
        },
        {
          "id": "MDRW2100046789.1.7",
          "form": "최근에 재밌게 본 건 아무래도 오징어게임이 아닐까 싶네요 ㅎㅎ",
          "original_form": "최근에 재밌게 본 건 아무래도 오징어게임이 아닐까 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211105 11:37"
        },
        {
          "id": "MDRW2100046789.1.8",
          "form": "아 역시 ㅋㅋㅋ 요즘 제일 핫하잖아요",
          "original_form": "아 역시 ㅋㅋㅋ 요즘 제일 핫하잖아요",
          "speaker_id": "2",
          "time": "20211105 11:37"
        },
        {
          "id": "MDRW2100046789.1.9",
          "form": "넵 영화는 이터널스 예매를 해뒀는데",
          "original_form": "넵 영화는 이터널스 예매를 해뒀는데",
          "speaker_id": "1",
          "time": "20211105 11:38"
        },
        {
          "id": "MDRW2100046789.1.10",
          "form": "평이 안좋아서 취소해야할까 고민이에요",
          "original_form": "평이 안좋아서 취소해야할까 고민이에요",
          "speaker_id": "1",
          "time": "20211105 11:38"
        },
        {
          "id": "MDRW2100046789.1.11",
          "form": "오 이거 마블 영화인가요?",
          "original_form": "오 이거 마블 영화인가요?",
          "speaker_id": "2",
          "time": "20211105 11:38"
        },
        {
          "id": "MDRW2100046789.1.12",
          "form": "네~ 마블영화라 보긴해야하는데 평이 좋지가 않네요",
          "original_form": "네~ 마블영화라 보긴해야하는데 평이 좋지가 않네요",
          "speaker_id": "1",
          "time": "20211105 11:39"
        },
        {
          "id": "MDRW2100046789.1.13",
          "form": "아이맥스로 예약까지 해뒀는데..",
          "original_form": "아이맥스로 예약까지 해뒀는데..",
          "speaker_id": "1",
          "time": "20211105 11:39"
        },
        {
          "id": "MDRW2100046789.1.14",
          "form": "아 ㅠ 마블 영화는 그래도 평타는 치는데 이번엔 좀 많이 별로인가봐요 ..",
          "original_form": "아 ㅠ 마블 영화는 그래도 평타는 치는데 이번엔 좀 많이 별로인가봐요 ..",
          "speaker_id": "2",
          "time": "20211105 11:39"
        },
        {
          "id": "MDRW2100046789.1.15",
          "form": "아 여기에 마동석님 출연하시는구나",
          "original_form": "아 여기에 마동석님 출연하시는구나",
          "speaker_id": "2",
          "time": "20211105 11:40"
        },
        {
          "id": "MDRW2100046789.1.16",
          "form": "네 맞아요 ㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 11:40"
        },
        {
          "id": "MDRW2100046789.1.17",
          "form": "오랜만에 마블영화라 많이 기대했는데",
          "original_form": "오랜만에 마블영화라 많이 기대했는데",
          "speaker_id": "1",
          "time": "20211105 11:41"
        },
        {
          "id": "MDRW2100046789.1.18",
          "form": "좀 그러네요 ㅋㅋ",
          "original_form": "좀 그러네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 11:41"
        },
        {
          "id": "MDRW2100046789.1.19",
          "form": "그러네요 ㅠㅠ 저도 마블 영화는 꼭 보는 편인데",
          "original_form": "그러네요 ㅠㅠ 저도 마블 영화는 꼭 보는 편인데",
          "speaker_id": "2",
          "time": "20211105 11:41"
        },
        {
          "id": "MDRW2100046789.1.20",
          "form": "코로나 이후로는 영화관에 잘 안가다보니까 관심이 많이 떨어졌어요",
          "original_form": "코로나 이후로는 영화관에 잘 안가다보니까 관심이 많이 떨어졌어요",
          "speaker_id": "2",
          "time": "20211105 11:41"
        },
        {
          "id": "MDRW2100046789.1.21",
          "form": "다음 마블영화인 스파이더맨은 대박일꺼 같으니 또 기다려야죠 ㅎ",
          "original_form": "다음 마블영화인 스파이더맨은 대박일꺼 같으니 또 기다려야죠 ㅎ",
          "speaker_id": "1",
          "time": "20211105 11:42"
        }
      ]
    }
