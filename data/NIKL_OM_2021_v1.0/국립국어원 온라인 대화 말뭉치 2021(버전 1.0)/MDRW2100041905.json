      "utterance": [
        {
          "id": "MDRW2100041905.1.1",
          "form": "그런 상황이 되니까",
          "original_form": "그런 상황이 되니까",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041905.1.2",
          "form": "자기애가 당한다고 생각하겠지",
          "original_form": "자기애가 당한다고 생각하겠지",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041905.1.3",
          "form": "그냥 그렇게 노는지 알텐데",
          "original_form": "그냥 그렇게 노는지 알텐데",
          "speaker_id": "1",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041905.1.4",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041905.1.5",
          "form": "그니까요 별로 안 친하면 이해를 하는데",
          "original_form": "그니까요 별로 안 친하면 이해를 하는데",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041905.1.6",
          "form": "캠핑까지 같이 가고 그렇게 자주 놀았는데",
          "original_form": "캠핑까지 같이 가고 그렇게 자주 놀았는데",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041905.1.7",
          "form": "우리도 지운거는 name3이\r\n무조건 잘못했지만",
          "original_form": "우리도 지운거는 &name3&이\r\n무조건 잘못했지만",
          "speaker_id": "2",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041905.1.8",
          "form": "그 두 번 때문에",
          "original_form": "그 두 번 때문에",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041905.1.9",
          "form": "이렇게 등 돌리는 거는",
          "original_form": "이렇게 등 돌리는 거는",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041905.1.10",
          "form": "자전거 타고 안타고는 전화오는거는",
          "original_form": "자전거 타고 안타고는 전화오는거는",
          "speaker_id": "2",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041905.1.11",
          "form": "좀 짜증나서..",
          "original_form": "좀 짜증나서..",
          "speaker_id": "2",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041905.1.12",
          "form": "한 이틀있다가 내가 언니한테 먼저연락하니까",
          "original_form": "한 이틀있다가 내가 언니한테 먼저연락하니까",
          "speaker_id": "2",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041905.1.13",
          "form": "본인도 안타깝다면서",
          "original_form": "본인도 안타깝다면서",
          "speaker_id": "2",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041905.1.14",
          "form": "name4가 울면서 잠들었다고",
          "original_form": "&name4&가 울면서 잠들었다고",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041905.1.15",
          "form": "차라리 그냥 속 시원하게 터놓고 풀면 은 될 텐데",
          "original_form": "차라리 그냥 속 시원하게 터놓고 풀면 은 될 텐데",
          "speaker_id": "1",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041905.1.16",
          "form": "그냥 같이 안 어울리는 걸로만 먹은 듯",
          "original_form": "그냥 같이 안 어울리는 걸로만 먹은 듯",
          "speaker_id": "1",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041905.1.17",
          "form": "엉 그러더니 다음주부터 name4 다른학원간다고",
          "original_form": "엉 그러더니 다음주부터 &name4& 다른학원간다고",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041905.1.18",
          "form": "겨울방학때 집에 혼자 있어야되서 생각하고 있었는데",
          "original_form": "겨울방학때 집에 혼자 있어야되서 생각하고 있었는데",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041905.1.19",
          "form": "거의 일 년을 같이 그렇게 놀았는데",
          "original_form": "거의 일 년을 같이 그렇게 놀았는데",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041905.1.20",
          "form": "끝이 너무 허무하고 만요",
          "original_form": "끝이 너무 허무하고 만요",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041905.1.21",
          "form": "좀 당겨서 일찍 보낸다면서",
          "original_form": "좀 당겨서 일찍 보낸다면서",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041905.1.22",
          "form": "차라리 그냥 솔직하게 얘기하지ㅋㅋㅋ",
          "original_form": "차라리 그냥 솔직하게 얘기하지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041905.1.23",
          "form": "그래 다 변명이지 뭐",
          "original_form": "그래 다 변명이지 뭐",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041905.1.24",
          "form": "여름방학도 잘 넘어갔는데 뭐",
          "original_form": "여름방학도 잘 넘어갔는데 뭐",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041905.1.25",
          "form": "그러니까요 차라리 그냥 터놓고 얘기하면",
          "original_form": "그러니까요 차라리 그냥 터놓고 얘기하면",
          "speaker_id": "1",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041905.1.26",
          "form": "겨울에는 태권도 마치고 놀면 추우니까 바로 집에 오라하려니",
          "original_form": "겨울에는 태권도 마치고 놀면 추우니까 바로 집에 오라하려니",
          "speaker_id": "2",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041905.1.27",
          "form": "언니 일마치고 올때까지 기다려야되서",
          "original_form": "언니 일마치고 올때까지 기다려야되서",
          "speaker_id": "2",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041905.1.28",
          "form": "씁쓸하네예~",
          "original_form": "씁쓸하네예~",
