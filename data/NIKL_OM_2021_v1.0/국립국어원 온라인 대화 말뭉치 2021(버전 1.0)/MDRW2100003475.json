      "utterance": [
        {
          "id": "MDRW2100003475.1.1",
          "form": "저는 짜장면 좋아해요!",
          "original_form": "저는 짜장면 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003475.1.2",
          "form": "name1님은요? 볶음밥도 잘 먹습니당ㅎㅎ",
          "original_form": "&name1&님은요? 볶음밥도 잘 먹습니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003475.1.3",
          "form": "짜장면은 기본좋아하시나요?",
          "original_form": "짜장면은 기본좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003475.1.4",
          "form": "저는 간짜장 좋더라구요",
          "original_form": "저는 간짜장 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 11:33"
        },
        {
          "id": "MDRW2100003475.1.5",
          "form": "저도 간짜장 좋아해요",
          "original_form": "저도 간짜장 좋아해요",
          "speaker_id": "2",
          "time": "20210821 11:34"
        },
        {
          "id": "MDRW2100003475.1.6",
          "form": "해물 좋아해서 해물짜장 이런것도 좋아요ㅎㅎ",
          "original_form": "해물 좋아해서 해물짜장 이런것도 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:34"
        },
        {
          "id": "MDRW2100003475.1.7",
          "form": "해물짜장 너무 맛있죠!",
          "original_form": "해물짜장 너무 맛있죠!",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003475.1.8",
          "form": "짜장먹을 때 따로 곁들여먹는거 있나요",
          "original_form": "짜장먹을 때 따로 곁들여먹는거 있나요",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003475.1.9",
          "form": "저는 클래식한 단무지요ㅋㅋㅋ",
          "original_form": "저는 클래식한 단무지요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003475.1.10",
          "form": "짜장은 꼭 단무지랑 함께 먹어요",
          "original_form": "짜장은 꼭 단무지랑 함께 먹어요",
          "speaker_id": "2",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003475.1.11",
          "form": "그리고 탕수육도 좋아해요",
          "original_form": "그리고 탕수육도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003475.1.12",
          "form": "역시 짜장에는 단무지가 가장 잘어울리죠",
          "original_form": "역시 짜장에는 단무지가 가장 잘어울리죠",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003475.1.13",
          "form": "탕수육은 찹쌀좋아하시나요?",
          "original_form": "탕수육은 찹쌀좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003475.1.14",
          "form": "기본좋아하시나요?",
          "original_form": "기본좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003475.1.15",
          "form": "찹쌀도 좋고 오리지널도 좋아요!",
          "original_form": "찹쌀도 좋고 오리지널도 좋아요!",
          "speaker_id": "2",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003475.1.16",
          "form": "그렇다면 부먹 찍먹!",
          "original_form": "그렇다면 부먹 찍먹!",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003475.1.17",
          "form": "둘 다 먹고싶은 날이 좀 다르더라구요? 바삭한게 땡기는 날이 있구",
          "original_form": "둘 다 먹고싶은 날이 좀 다르더라구요? 바삭한게 땡기는 날이 있구",
          "speaker_id": "2",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003475.1.18",
          "form": "당연히 찍먹이죠",
          "original_form": "당연히 찍먹이죠",
          "speaker_id": "2",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003475.1.19",
          "form": "뭔가를 아시네요",
          "original_form": "뭔가를 아시네요",
          "speaker_id": "1",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003475.1.20",
          "form": "전 바삭한걸 좋아해서 찍먹파인거 같아요",
          "original_form": "전 바삭한걸 좋아해서 찍먹파인거 같아요",
          "speaker_id": "2",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003475.1.21",
          "form": "눅눅한거 싫어요ㅠㅠ",
          "original_form": "눅눅한거 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003475.1.22",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003475.1.23",
          "form": "튀김은 바삭한게 최고인거같아요",
          "original_form": "튀김은 바삭한게 최고인거같아요",
          "speaker_id": "1",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003475.1.24",
          "form": "그쵸 전 오징어튀김도 좋더라구요",
          "original_form": "그쵸 전 오징어튀김도 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 11:40"
        }
      ]
    }
