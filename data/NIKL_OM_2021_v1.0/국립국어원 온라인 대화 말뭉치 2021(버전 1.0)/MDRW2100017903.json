        {
          "id": "MDRW2100017903.1.1",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "2",
          "time": "20210901 14:30"
        },
        {
          "id": "MDRW2100017903.1.2",
          "form": "오늘은 비가 조금오네요",
          "original_form": "오늘은 비가 조금오네요",
          "speaker_id": "1",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100017903.1.3",
          "form": "네 밤부터 계속 내리네요",
          "original_form": "네 밤부터 계속 내리네요",
          "speaker_id": "2",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100017903.1.4",
          "form": "저희는 아침에안오고 지금븐터오네요",
          "original_form": "저희는 아침에안오고 지금븐터오네요",
          "speaker_id": "1",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100017903.1.5",
          "form": "지역마다 차이가 제법 있나봐요",
          "original_form": "지역마다 차이가 제법 있나봐요",
          "speaker_id": "2",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100017903.1.6",
          "form": "이번이 마지막 장마죠",
          "original_form": "이번이 마지막 장마죠",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100017903.1.7",
          "form": "이렇게 여름이 끝나가네요",
          "original_form": "이렇게 여름이 끝나가네요",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100017903.1.8",
          "form": "다행인지 여름 마스크 힘들었어요",
          "original_form": "다행인지 여름 마스크 힘들었어요",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100017903.1.9",
          "form": "맞아요 더위에 땀차고 많이 힘들었어요ㅠㅠㅠ",
          "original_form": "맞아요 더위에 땀차고 많이 힘들었어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100017903.1.10",
          "form": "그래도 여름 가면 수월해지니요",
          "original_form": "그래도 여름 가면 수월해지니요",
          "speaker_id": "1",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100017903.1.11",
          "form": "바람이 선선해지고 있어 참 다행이에요",
          "original_form": "바람이 선선해지고 있어 참 다행이에요",
          "speaker_id": "2",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100017903.1.12",
          "form": "올 겨울에도 독감 유행없어야할텐데",
          "original_form": "올 겨울에도 독감 유행없어야할텐데",
          "speaker_id": "2",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100017903.1.13",
          "form": "마스크쓰니 감기는 적은거같아요",
          "original_form": "마스크쓰니 감기는 적은거같아요",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100017903.1.14",
          "form": "네 확실히 줄었어요",
          "original_form": "네 확실히 줄었어요",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100017903.1.15",
          "form": "사람들이 평소에도 위생에",
          "original_form": "사람들이 평소에도 위생에",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100017903.1.16",
          "form": "신경쓰면 좋겠어요",
          "original_form": "신경쓰면 좋겠어요",
