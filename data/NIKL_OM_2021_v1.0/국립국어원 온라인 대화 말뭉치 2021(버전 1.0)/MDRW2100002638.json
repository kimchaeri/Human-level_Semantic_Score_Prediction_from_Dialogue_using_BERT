      "utterance": [
        {
          "id": "MDRW2100002638.1.1",
          "form": "홈플에서 계란쿠폰줘서 장봤어요 ㅎ",
          "original_form": "홈플에서 계란쿠폰줘서 장봤어요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002638.1.2",
          "form": "오 싼가요?",
          "original_form": "오 싼가요?",
          "speaker_id": "2",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002638.1.3",
          "form": "4천원짜리 990원에 살수있더라구요",
          "original_form": "4천원짜리 990원에 살수있더라구요",
          "speaker_id": "1",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002638.1.4",
          "form": "한판은 아니죠?",
          "original_form": "한판은 아니죠?",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002638.1.5",
          "form": "네 10개인거같아요 ㅎ",
          "original_form": "네 10개인거같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002638.1.6",
          "form": "진짜 예전에는 한판이 5천원미만이였던거같은데",
          "original_form": "진짜 예전에는 한판이 5천원미만이였던거같은데",
          "speaker_id": "1",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002638.1.7",
          "form": "요즘 계란 넘 비싸요ㅠㅠ",
          "original_form": "요즘 계란 넘 비싸요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002638.1.8",
          "form": "한판에 8-9000원이라니 금계란이더라구요",
          "original_form": "한판에 8-9000원이라니 금계란이더라구요",
          "speaker_id": "2",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002638.1.9",
          "form": "맞아요 그래서 이렇게 싸게줄때 사야겠다싶어 담고 이것저것 담아서 샀어요",
          "original_form": "맞아요 그래서 이렇게 싸게줄때 사야겠다싶어 담고 이것저것 담아서 샀어요",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002638.1.10",
          "form": "온라인 홈플인가요? 익스프레스?",
          "original_form": "온라인 홈플인가요? 익스프레스?",
          "speaker_id": "2",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002638.1.11",
          "form": "첫구매라고 7000원쿠폰도 주고 무배쿠폰도 줘서 나름 알뜰쇼핑한거같긴해요 ㅎ",
          "original_form": "첫구매라고 7000원쿠폰도 주고 무배쿠폰도 줘서 나름 알뜰쇼핑한거같긴해요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002638.1.12",
          "form": "익스프레스요",
          "original_form": "익스프레스요",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002638.1.13",
          "form": "완전 득템 하셨는데요? 저도 첫구매되는거같은데 알아봐야겠어요",
          "original_form": "완전 득템 하셨는데요? 저도 첫구매되는거같은데 알아봐야겠어요",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002638.1.14",
          "form": "첫구매되시면 한번 둘러보세요",
          "original_form": "첫구매되시면 한번 둘러보세요",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002638.1.15",
          "form": "신한 11퍼인가? 그런쿠폰도 줘요",
          "original_form": "신한 11퍼인가? 그런쿠폰도 줘요",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002638.1.16",
          "form": "신한카드로 결제죠?",
          "original_form": "신한카드로 결제죠?",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002638.1.17",
          "form": "네 신한카드로 결제해야되요",
          "original_form": "네 신한카드로 결제해야되요",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002638.1.18",
          "form": "저는 다른쿠폰 적용되서 저거는 못썼어요",
          "original_form": "저는 다른쿠폰 적용되서 저거는 못썼어요",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002638.1.19",
          "form": "중복적용안되나요?",
          "original_form": "중복적용안되나요?",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002638.1.20",
          "form": "과일쿠폰인가? 머다른거랑 중복이 안되더라구요",
          "original_form": "과일쿠폰인가? 머다른거랑 중복이 안되더라구요",
          "speaker_id": "1",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002638.1.21",
          "form": "근데 배송비나 7000원 다른거는 중복되기도해요",
          "original_form": "근데 배송비나 7000원 다른거는 중복되기도해요",
          "speaker_id": "1",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002638.1.22",
          "form": "그럼 비교해보고 사야겠네요  정보감사요",
          "original_form": "그럼 비교해보고 사야겠네요  정보감사요",
          "speaker_id": "2",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002638.1.23",
          "form": "근데 즉시배송이라 취소가 안되더라구요",
          "original_form": "근데 즉시배송이라 취소가 안되더라구요",
          "speaker_id": "1",
          "time": "20210820 14:18"
        }
      ]
    }
