      "utterance": [
        {
          "id": "MDRW2100019010.1.1",
          "form": "싼 건 또 후기가 너무 어마무시해서 못 사겠음",
          "original_form": "싼 건 또 후기가 너무 어마무시해서 못 사겠음",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019010.1.2",
          "form": "의자랑거의노트북가격이랑",
          "original_form": "의자랑거의노트북가격이랑",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019010.1.3",
          "form": "비슷해",
          "original_form": "비슷해",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019010.1.4",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019010.1.5",
          "form": "에바야",
          "original_form": "에바야",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019010.1.6",
          "form": "싼건휴기안좋음?",
          "original_form": "싼건휴기안좋음?",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019010.1.7",
          "form": "허리아프다고함?",
          "original_form": "허리아프다고함?",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019010.1.8",
          "form": "아니 쓰다가 부러져서 허리가 나가거나",
          "original_form": "아니 쓰다가 부러져서 허리가 나가거나",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019010.1.9",
          "form": "팔이 부러지거나 하는 후기를 너무 많이 봄",
          "original_form": "팔이 부러지거나 하는 후기를 너무 많이 봄",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019010.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019010.1.11",
          "form": "그건진짜",
          "original_form": "그건진짜",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019010.1.12",
          "form": "심한데",
          "original_form": "심한데",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019010.1.13",
          "form": "사람이앉을수는있게만들어야지",
          "original_form": "사람이앉을수는있게만들어야지",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019010.1.14",
          "form": "부서지면그게의자임?",
          "original_form": "부서지면그게의자임?",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019010.1.15",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019010.1.16",
          "form": "그거체형도고려해야하잖아",
          "original_form": "그거체형도고려해야하잖아",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019010.1.17",
          "form": "비싸도체형에안맞으면불편하다며",
          "original_form": "비싸도체형에안맞으면불편하다며",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019010.1.18",
          "form": "어 맞아 그래야 한다고 하더라",
          "original_form": "어 맞아 그래야 한다고 하더라",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019010.1.19",
          "form": "그래서 수제 제작 맡기는 사람도 있대",
          "original_form": "그래서 수제 제작 맡기는 사람도 있대",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019010.1.20",
          "form": "ㅋㅋㅋ딱그말쓰려고했는데",
          "original_form": "ㅋㅋㅋ딱그말쓰려고했는데",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019010.1.21",
          "form": "차라리그게날거같음",
          "original_form": "차라리그게날거같음",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019010.1.22",
          "form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌㅌㅌㅌ",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019010.1.23",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019010.1.24",
          "form": "국가지원금체크카드로받을거임?",
          "original_form": "국가지원금체크카드로받을거임?",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019010.1.25",
          "form": "아니면그이음카드?",
          "original_form": "아니면그이음카드?",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019010.1.26",
          "form": "체크 카드",
          "original_form": "체크 카드",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.27",
          "form": "일단 이음이 없음",
          "original_form": "일단 이음이 없음",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.28",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.29",
          "form": "너아직",
          "original_form": "너아직",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.30",
          "form": "그체크카드쓰지",
          "original_form": "그체크카드쓰지",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.31",
          "form": "고등학교",
          "original_form": "고등학교",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.32",
          "form": "아 ㄴㄴㄴㄴㄴㄴ",
          "original_form": "아 ㄴㄴㄴㄴㄴㄴ",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.33",
          "form": "카드는 바꿈",
          "original_form": "카드는 바꿈",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.34",
          "form": "오 드디어 배꿈?",
          "original_form": "오 드디어 배꿈?",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.35",
          "form": "그걸 어똫게 써",
          "original_form": "그걸 어똫게 써",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019010.1.36",
          "form": "ㅋㅋㅋㅋ그건그럼",
          "original_form": "ㅋㅋㅋㅋ그건그럼",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019010.1.37",
          "form": "사진붙어있어서",
          "original_form": "사진붙어있어서",
