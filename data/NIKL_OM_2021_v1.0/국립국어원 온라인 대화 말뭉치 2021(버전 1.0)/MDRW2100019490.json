      "utterance": [
        {
          "id": "MDRW2100019490.1.1",
          "form": "쇼핑 위시리스트 있으세요?",
          "original_form": "쇼핑 위시리스트 있으세요?",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019490.1.2",
          "form": "늘 사고싶은건 많아요ㅎㅎ",
          "original_form": "늘 사고싶은건 많아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019490.1.3",
          "form": "ㅋㅋ 전 한참 줄줄이 써놨다가 지금은 비어있어요",
          "original_form": "ㅋㅋ 전 한참 줄줄이 써놨다가 지금은 비어있어요",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019490.1.4",
          "form": "위시 공유로 저의 쇼핑욕 좀 자극해주세요ㅋㅋ",
          "original_form": "위시 공유로 저의 쇼핑욕 좀 자극해주세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019490.1.5",
          "form": "와디즈아세요?",
          "original_form": "와디즈아세요?",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019490.1.6",
          "form": "들어본 적 있어요!",
          "original_form": "들어본 적 있어요!",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019490.1.7",
          "form": "펀딩 플랫폼인가요?",
          "original_form": "펀딩 플랫폼인가요?",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019490.1.8",
          "form": "네네 거기만 봐도 사고싶은거 넘 마나여ㅜ",
          "original_form": "네네 거기만 봐도 사고싶은거 넘 마나여ㅜ",
          "speaker_id": "2",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100019490.1.9",
          "form": "오오 들어가볼게요",
          "original_form": "오오 들어가볼게요",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019490.1.10",
          "form": "와 헐 여기 안경 너무 이쁜 거 있어요",
          "original_form": "와 헐 여기 안경 너무 이쁜 거 있어요",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019490.1.11",
          "form": "거기 한번드가면 헤어나오질 못하겠더라고요 ㅋㅋ",
          "original_form": "거기 한번드가면 헤어나오질 못하겠더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019490.1.12",
          "form": "ㅋㅋㅋㅋㅋ 개미지옥??",
          "original_form": "ㅋㅋㅋㅋㅋ 개미지옥??",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019490.1.13",
          "form": "저 그러고보니 선글라스 사고싶었는데 마스크랑 같이 쓰면 간첩같을까봐 참았거든요ㅋㅋㅋ 이제 슬슬 살 거 찾아볼까봐요ㅋㅋ",
          "original_form": "저 그러고보니 선글라스 사고싶었는데 마스크랑 같이 쓰면 간첩같을까봐 참았거든요ㅋㅋㅋ 이제 슬슬 살 거 찾아볼까봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019490.1.14",
          "form": "거기랑 아이디어스 ㅋㅋ",
          "original_form": "거기랑 아이디어스 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019490.1.15",
          "form": "아이디어스 저도 알아요!ㅋㅋ",
          "original_form": "아이디어스 저도 알아요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019490.1.16",
          "form": "악세사리 이쁜 거 많죠 손재주 좋은 사람들 참 많은 것 같아요",
          "original_form": "악세사리 이쁜 거 많죠 손재주 좋은 사람들 참 많은 것 같아요",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019490.1.17",
          "form": "워킹화 갖고싶어요 발안아픈거로 ㅎㅎ",
          "original_form": "워킹화 갖고싶어요 발안아픈거로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019490.1.18",
          "form": "오 바닥 두꺼운 큰 신발? 슬림하고 가벼운 신발? 어느쪽 좋아해요?",
          "original_form": "오 바닥 두꺼운 큰 신발? 슬림하고 가벼운 신발? 어느쪽 좋아해요?",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019490.1.19",
          "form": "오래걸어도 괜찮은걸로?",
          "original_form": "오래걸어도 괜찮은걸로?",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019490.1.20",
          "form": "그럼 무게가 좀 있어도 두께감 있는 게 낫죵? 그런 신발은 전 스케쳐스 좋더라구요",
          "original_form": "그럼 무게가 좀 있어도 두께감 있는 게 낫죵? 그런 신발은 전 스케쳐스 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 20:10"
        }
      ]
