      "utterance": [
        {
          "id": "MDRW2100024088.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024088.1.2",
          "form": "반려동물과 함께하시나요?",
          "original_form": "반려동물과 함께하시나요?",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100024088.1.3",
          "form": "아니요 키우고 싶어요~ 님은요?",
          "original_form": "아니요 키우고 싶어요~ 님은요?",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024088.1.4",
          "form": "전 토끼랑살아요",
          "original_form": "전 토끼랑살아요",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024088.1.5",
          "form": "우와 토끼! 토끼랑 교감 잘 되나요?",
          "original_form": "우와 토끼! 토끼랑 교감 잘 되나요?",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024088.1.6",
          "form": "네. 자기이름 부르면 알고 와요",
          "original_form": "네. 자기이름 부르면 알고 와요",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024088.1.7",
          "form": "기다려 손 뽀뽀 이런건 하구요",
          "original_form": "기다려 손 뽀뽀 이런건 하구요",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024088.1.8",
          "form": "웬일이야 듣기만 해도 너무 귀여워요",
          "original_form": "웬일이야 듣기만 해도 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024088.1.9",
          "form": "넹넹 귀여워요",
          "original_form": "넹넹 귀여워요",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100024088.1.10",
          "form": "근데 요즘환절기라 털갈이가 아주 ️뿜뿜 해요",
          "original_form": "근데 요즘환절기라 털갈이가 아주 ️뿜뿜 해요",
          "speaker_id": "2",
          "time": "20210830 22:29"
        },
        {
          "id": "MDRW2100024088.1.11",
          "form": "공기중에 털이 마구 날리겠어요",
          "original_form": "공기중에 털이 마구 날리겠어요",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024088.1.12",
          "form": "그것마저 귀여울듯..",
          "original_form": "그것마저 귀여울듯..",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024088.1.13",
          "form": "세수하면 얼구에서 한뭉티기나와요",
          "original_form": "세수하면 얼구에서 한뭉티기나와요",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024088.1.14",
          "form": "손이 많이 가도 그만큼 정들고 예쁠 것 같아요",
          "original_form": "손이 많이 가도 그만큼 정들고 예쁠 것 같아요",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024088.1.15",
          "form": "아간데 이뿌죠",
          "original_form": "아간데 이뿌죠",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024088.1.16",
          "form": "거의 할아부지지만",
          "original_form": "거의 할아부지지만",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024088.1.17",
          "form": "나이 먹어도 평생 애기같다던데요 ㅎㅎ 반려동물",
          "original_form": "나이 먹어도 평생 애기같다던데요 ㅎㅎ 반려동물",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024088.1.18",
          "form": "내눈에만 애기지 다른사람이 보면이렇게 크냐고",
          "original_form": "내눈에만 애기지 다른사람이 보면이렇게 크냐고",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024088.1.19",
          "form": "사진하고 다르다고들 해요",
          "original_form": "사진하고 다르다고들 해요",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024088.1.20",
          "form": "외모는 애기애기 하거든요",
          "original_form": "외모는 애기애기 하거든요",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024088.1.21",
          "form": "ㅎㅎㅎ 크든 작든 정들면 다 예뻐보이고 애기같고.. 정말 그 교감이 엄청난 것 같더라고요",
          "original_form": "ㅎㅎㅎ 크든 작든 정들면 다 예뻐보이고 애기같고.. 정말 그 교감이 엄청난 것 같더라고요",
