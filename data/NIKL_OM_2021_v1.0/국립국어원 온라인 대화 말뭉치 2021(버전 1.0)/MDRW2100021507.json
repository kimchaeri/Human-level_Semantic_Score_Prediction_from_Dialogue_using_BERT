      "utterance": [
        {
          "id": "MDRW2100021507.1.1",
          "form": "헐헐ㅋㅋㅋㅋ",
          "original_form": "헐헐ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021507.1.2",
          "form": "그거 아세요?",
          "original_form": "그거 아세요?",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.3",
          "form": "저 막 할일 없을 때 그 분 라이브 방송도 켜놓고 그랬어요",
          "original_form": "저 막 할일 없을 때 그 분 라이브 방송도 켜놓고 그랬어요",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.4",
          "form": "오리 포함해서 새들은",
          "original_form": "오리 포함해서 새들은",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.5",
          "form": "괄약근이 ㄹ없는데",
          "original_form": "괄약근이 ㄹ없는데",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.6",
          "form": "그 이유가 날기위해서는",
          "original_form": "그 이유가 날기위해서는",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.7",
          "form": "배변활동으로 몸을 가볍게해야해서",
          "original_form": "배변활동으로 몸을 가볍게해야해서",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.8",
          "form": "어 마자여 새덕후 채널에서 봤어요 ㅋㅋㅋㅋ",
          "original_form": "어 마자여 새덕후 채널에서 봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.9",
          "form": "바로 배출하는 시스템?이 된거래요",
          "original_form": "바로 배출하는 시스템?이 된거래요",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.10",
          "form": "너무 신기해요",
          "original_form": "너무 신기해요",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.11",
          "form": "그래서 날아가면서 싼다고",
          "original_form": "그래서 날아가면서 싼다고",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.12",
          "form": "ㅋㅋㅋㅋㅋ그니까요",
          "original_form": "ㅋㅋㅋㅋㅋ그니까요",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021507.1.14",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021507.1.15",
          "form": "저 그거 보고 새 키우고 싶어졌는데...",
          "original_form": "저 그거 보고 새 키우고 싶어졌는데...",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021507.1.16",
          "form": "오리도 배변훈련이 불가하구용ㅠㅠ",
          "original_form": "오리도 배변훈련이 불가하구용ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021507.1.17",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021507.1.18",
          "form": "새가 은근히 똑똑하더라구요",
          "original_form": "새가 은근히 똑똑하더라구요",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021507.1.19",
          "form": "앜ㅋㅋㅋㅋㅋ 알아주시다니 기뻐요",
          "original_form": "앜ㅋㅋㅋㅋㅋ 알아주시다니 기뻐요",
          "speaker_id": "1",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100021507.1.20",
          "form": "동물 키우고싶은데",
          "original_form": "동물 키우고싶은데",
          "speaker_id": "1",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100021507.1.21",
          "form": "병원비를 감당 못할것같아서ㅠㅠ 흑흑",
          "original_form": "병원비를 감당 못할것같아서ㅠㅠ 흑흑",
          "speaker_id": "1",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100021507.1.22",
          "form": "맞아요ㅠㅠ 동물들은 보험도 안되구",
          "original_form": "맞아요ㅠㅠ 동물들은 보험도 안되구",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021507.1.23",
          "form": "저도 기르고 싶은데 무섭더라구요",
          "original_form": "저도 기르고 싶은데 무섭더라구요",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021507.1.24",
          "form": "한 생명을 죽응 때 까지 책임져야란다는게",
          "original_form": "한 생명을 죽응 때 까지 책임져야란다는게",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021507.1.25",
          "form": "트위터에서 오리 키우는 어떤분",
          "original_form": "트위터에서 오리 키우는 어떤분",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100021507.1.26",
          "form": "오리가 다쳐서 동물병원에 데려갔는데",
          "original_form": "오리가 다쳐서 동물병원에 데려갔는데",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100021507.1.27",
          "form": "치료하고 처치하는데만",
          "original_form": "치료하고 처치하는데만",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100021507.1.28",
          "form": "18만원이 나왔더라구요ㅠㅠㅠ",
          "original_form": "18만원이 나왔더라구요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100021507.1.29",
          "form": "부처님...???",
          "original_form": "부처님...???",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021507.1.30",
          "form": "헐대박",
          "original_form": "헐대박",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021507.1.31",
          "form": "안그래도 제가 아시냐고 여쭤볼랬는데",
          "original_form": "안그래도 제가 아시냐고 여쭤볼랬는데",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021507.1.32",
          "form": "어떻게 아세요?",
          "original_form": "어떻게 아세요?",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021507.1.33",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021507.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021507.1.35",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021507.1.36",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021507.1.37",
          "form": "구독해요",
          "original_form": "구독해요",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021507.1.38",
          "form": "와아",
          "original_form": "와아",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021507.1.39",
          "form": "진짜 새 좋아하시는구내",
          "original_form": "진짜 새 좋아하시는구내",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021507.1.40",
          "form": "오리 너무 귀엽구 재미있어서요",
          "original_form": "오리 너무 귀엽구 재미있어서요",
          "speaker_id": "2",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021507.1.41",
          "form": "반가워욬ㅋㅋㅋ",
          "original_form": "반가워욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021507.1.42",
          "form": "그리고 좀 올려주시는 얘기 보면",
          "original_form": "그리고 좀 올려주시는 얘기 보면",
          "speaker_id": "2",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021507.1.43",
          "form": "인스타도 오리 구독용 계정 하나 팠는데ㅋㅋㅋㅋ",
          "original_form": "인스타도 오리 구독용 계정 하나 팠는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021507.1.44",
          "form": "반려동물 현실에 대해서.. 알게된달까",
          "original_form": "반려동물 현실에 대해서.. 알게된달까",
          "speaker_id": "2",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021507.1.45",
          "form": "요즘은 오리 많이 키우더라고요",
          "original_form": "요즘은 오리 많이 키우더라고요",
          "speaker_id": "2",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021507.1.46",
          "form": "맞아요 오리키우는 분들이 늘어나서",
          "original_form": "맞아요 오리키우는 분들이 늘어나서",
          "speaker_id": "1",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100021507.1.47",
          "form": "넘 기뻐요",
          "original_form": "넘 기뻐요",
          "speaker_id": "1",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100021507.1.48",
          "form": "오리 콜덕 오리 한마리 분양하는데",
          "original_form": "오리 콜덕 오리 한마리 분양하는데",
          "speaker_id": "1",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100021507.1.49",
          "form": "삼백만원 든다고해서",
          "original_form": "삼백만원 든다고해서",
          "speaker_id": "1",
