      "utterance": [
        {
          "id": "MDRW2100020138.1.1",
          "form": "아뇨 식단은 따로 안해요",
          "original_form": "아뇨 식단은 따로 안해요",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020138.1.2",
          "form": "생존형 운동이에요 ㅋㅋㅋ",
          "original_form": "생존형 운동이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020138.1.3",
          "form": "아아 ㅋㅋㅋ 저도요 ㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋ 저도요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020138.1.4",
          "form": "근데 또 운동 끝나고 저녁 드시기엔",
          "original_form": "근데 또 운동 끝나고 저녁 드시기엔",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020138.1.5",
          "form": "애매하지않으세요?",
          "original_form": "애매하지않으세요?",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020138.1.6",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020138.1.7",
          "form": "그래서 보통은 퇴근하고 간단하게 먹고 가요",
          "original_form": "그래서 보통은 퇴근하고 간단하게 먹고 가요",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020138.1.8",
          "form": "귀찮으면 그냥 야식으로 먹기도 하구요",
          "original_form": "귀찮으면 그냥 야식으로 먹기도 하구요",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020138.1.9",
          "form": "맞아요 ㅋㅋ 저도 야식 먹게되더라구요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ 저도 야식 먹게되더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020138.1.10",
          "form": "보통 뭐 자주 드세요?",
          "original_form": "보통 뭐 자주 드세요?",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020138.1.11",
          "form": "근처에 써브웨이 있어서",
          "original_form": "근처에 써브웨이 있어서",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020138.1.12",
          "form": "샌드위치 많이 먹어요",
          "original_form": "샌드위치 많이 먹어요",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020138.1.13",
          "form": "아 써브웨이 좋아하는데 주변에 없어요 ㅋㅋ ㅜㅜ",
          "original_form": "아 써브웨이 좋아하는데 주변에 없어요 ㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020138.1.14",
          "form": "사실 운동하고나면 허해서",
          "original_form": "사실 운동하고나면 허해서",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020138.1.15",
          "form": "뭘 먹어도 맛있더라구요 ㅋㅋㅋ",
          "original_form": "뭘 먹어도 맛있더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020138.1.16",
          "form": "맞아요 ㅋㅋ 근데 닭가슴살은 맛없어요 .",
          "original_form": "맞아요 ㅋㅋ 근데 닭가슴살은 맛없어요 .",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020138.1.17",
          "form": "냉동된걸 너무 많이 먹어서 질렸어요ㅋㅋ",
          "original_form": "냉동된걸 너무 많이 먹어서 질렸어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020138.1.18",
          "form": "아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020138.1.19",
          "form": "요즘은 한팩씩 포장된거 잘나오더라구요",
          "original_form": "요즘은 한팩씩 포장된거 잘나오더라구요",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020138.1.20",
          "form": "아 맞아요! 차라리 한팩씩 있는게 맛있어요 ㅎㅎ",
          "original_form": "아 맞아요! 차라리 한팩씩 있는게 맛있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020138.1.21",
          "form": "맛도 다양하고 전자레인지 돌리면 되니까 편해요",
          "original_form": "맛도 다양하고 전자레인지 돌리면 되니까 편해요",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020138.1.22",
          "form": "운동하고 오면 손 하나 까딱하기 싫어서 ㅋㅋㅋ",
          "original_form": "운동하고 오면 손 하나 까딱하기 싫어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020138.1.23",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020138.1.24",
          "form": "저는 가끔 단백질쉐이크도 먹어요 ㅋㅋ",
          "original_form": "저는 가끔 단백질쉐이크도 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020138.1.25",
          "form": "맛있나요??",
          "original_form": "맛있나요??",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020138.1.26",
          "form": "그 단백질 특유의 향 혹시 아시나요??!",
          "original_form": "그 단백질 특유의 향 혹시 아시나요??!",
          "speaker_id": "1",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020138.1.27",
          "form": "맛은 괜찮은데 향이 좀 나요 ㅜㅜ",
          "original_form": "맛은 괜찮은데 향이 좀 나요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 00:29"
        }
      ]
