      "utterance": [
        {
          "id": "MDRW2100028019.1.1",
          "form": "저녁밥얘기를 해볼까",
          "original_form": "저녁밥얘기를 해볼까",
          "speaker_id": "1",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100028019.1.2",
          "form": "후 올게왔군",
          "original_form": "후 올게왔군",
          "speaker_id": "2",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028019.1.3",
          "form": "삼겹??",
          "original_form": "삼겹??",
          "speaker_id": "2",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028019.1.4",
          "form": "삼겹!!! 저기압일땐 고기앞으로",
          "original_form": "삼겹!!! 저기압일땐 고기앞으로",
          "speaker_id": "1",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028019.1.5",
          "form": "나는 고기를 좀 씹어야겟어",
          "original_form": "나는 고기를 좀 씹어야겟어",
          "speaker_id": "1",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028019.1.6",
          "form": "좋다!",
          "original_form": "좋다!",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028019.1.7",
          "form": "삼겹 링크를 좀 보내보겠어",
          "original_form": "삼겹 링크를 좀 보내보겠어",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028019.1.8",
          "form": "확인 부탁드려요",
          "original_form": "확인 부탁드려요",
          "speaker_id": "2",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028019.1.9",
          "form": "두근두근",
          "original_form": "두근두근",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028019.1.10",
          "form": "늘 먹던 그곳?",
          "original_form": "늘 먹던 그곳?",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028019.1.11",
          "form": "새로운  곳 찾아보려규영",
          "original_form": "새로운  곳 찾아보려규영",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028019.1.12",
          "form": "거기 넘 뭔가 많이줘..",
          "original_form": "거기 넘 뭔가 많이줘..",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028019.1.13",
          "form": "아항 그래요",
          "original_form": "아항 그래요",
          "speaker_id": "1",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028019.1.14",
          "form": "고기만 하는 곳이 많아졌네용",
          "original_form": "고기만 하는 곳이 많아졌네용",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028019.1.15",
          "form": "난 뭐든 고기기만 하면 좋아..",
          "original_form": "난 뭐든 고기기만 하면 좋아..",
          "speaker_id": "1",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028019.1.16",
          "form": "고깃집을 못가서 그런가?",
          "original_form": "고깃집을 못가서 그런가?",
          "speaker_id": "1",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028019.1.17",
          "form": "흠..",
          "original_form": "흠..",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028019.1.18",
          "form": "1. 직화삼겹+계란말이\r\n2. 양념삼겹\r\n3. 간장삼겹",
          "original_form": "1. 직화삼겹+계란말이\r\n2. 양념삼겹\r\n3. 간장삼겹",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028019.1.19",
          "form": "오오 1번!! 직화랑 계란!",
          "original_form": "오오 1번!! 직화랑 계란!",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028019.1.20",
          "form": "진상맨 안오고 무사퇴근! 맘편히 저녁먹겠다ㅠㅠ",
          "original_form": "진상맨 안오고 무사퇴근! 맘편히 저녁먹겠다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100028019.1.21",
          "form": "다행이다!!!",
          "original_form": "다행이다!!!",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028019.1.22",
          "form": "저녁은...저 도착 40분 전 쯤 시킬께용",
          "original_form": "저녁은...저 도착 40분 전 쯤 시킬께용",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028019.1.23",
          "form": "웅웅 구래요! 집가서 치우고있을게!",
          "original_form": "웅웅 구래요! 집가서 치우고있을게!",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028019.1.24",
          "form": "밥먹을자리 만들어야지",
          "original_form": "밥먹을자리 만들어야지",
          "speaker_id": "1",
          "time": "20210902 18:25"
        }
      ]
