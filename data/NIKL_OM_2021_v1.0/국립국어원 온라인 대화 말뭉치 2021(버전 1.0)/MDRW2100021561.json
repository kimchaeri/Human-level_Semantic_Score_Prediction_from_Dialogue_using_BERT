      "utterance": [
        {
          "id": "MDRW2100021561.1.1",
          "form": "타타라스나 퀘스트 했어?",
          "original_form": "타타라스나 퀘스트 했어?",
          "speaker_id": "1",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100021561.1.2",
          "form": "오늘치는 아직 안했어",
          "original_form": "오늘치는 아직 안했어",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021561.1.3",
          "form": "근데 별게 없는데",
          "original_form": "근데 별게 없는데",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021561.1.4",
          "form": "이게 4일인가 5일로 게임 컨텐츠를 쪼개느라 그런가봐",
          "original_form": "이게 4일인가 5일로 게임 컨텐츠를 쪼개느라 그런가봐",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021561.1.5",
          "form": "ㅇㅇ그런것같아",
          "original_form": "ㅇㅇ그런것같아",
          "speaker_id": "2",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021561.1.6",
          "form": "왜 굳이 글케 한거지",
          "original_form": "왜 굳이 글케 한거지",
          "speaker_id": "2",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021561.1.7",
          "form": "그럼 시간돌리는기능은",
          "original_form": "그럼 시간돌리는기능은",
          "speaker_id": "2",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021561.1.8",
          "form": "왜넣은거야",
          "original_form": "왜넣은거야",
          "speaker_id": "2",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021561.1.9",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021561.1.10",
          "form": "시간 기능은 ㄹㅇ 사진용...",
          "original_form": "시간 기능은 ㄹㅇ 사진용...",
          "speaker_id": "1",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100021561.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021561.1.12",
          "form": "플레이스테이션 접속보상 탐난다",
          "original_form": "플레이스테이션 접속보상 탐난다",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021561.1.13",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021561.1.14",
          "form": "접속안하면 아일레아엿나 여튼 걔 못얻는건가?",
          "original_form": "접속안하면 아일레아엿나 여튼 걔 못얻는건가?",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021561.1.15",
          "form": "2.1에서는 플스 유저한테만",
          "original_form": "2.1에서는 플스 유저한테만",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021561.1.16",
          "form": "2.2에서는 전원 지급이라",
          "original_form": "2.2에서는 전원 지급이라",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021561.1.17",
          "form": "한달 반 기다리면 받을 수 있긴 함...",
          "original_form": "한달 반 기다리면 받을 수 있긴 함...",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021561.1.18",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021561.1.19",
          "form": "머 언젠가 주긴 하겠네",
          "original_form": "머 언젠가 주긴 하겠네",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021561.1.20",
          "form": "글킨 하지...",
          "original_form": "글킨 하지...",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.21",
          "form": "아 마자",
          "original_form": "아 마자",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.22",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.23",
          "form": "나 지금 성유물 스토리들 보는데",
          "original_form": "나 지금 성유물 스토리들 보는데",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.24",
          "form": "나만 5성 없어....",
          "original_form": "나만 5성 없어....",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.25",
          "form": "피에 물든 기사도",
          "original_form": "피에 물든 기사도",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.26",
          "form": "혹시 성유물도",
          "original_form": "혹시 성유물도",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.27",
          "form": "이거 심연사도 이야기인듯",
          "original_form": "이거 심연사도 이야기인듯",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.28",
          "form": "등급에따라",
          "original_form": "등급에따라",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.29",
          "form": "내용 달라져?",
          "original_form": "내용 달라져?",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.30",
          "form": "헐.....",
          "original_form": "헐.....",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021561.1.31",
          "form": "그건 모르겠어...",
          "original_form": "그건 모르겠어...",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021561.1.32",
          "form": "걔 스토리가",
          "original_form": "걔 스토리가",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021561.1.33",
          "form": "있었다니",
          "original_form": "있었다니",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021561.1.34",
          "form": "일단 내가 보는 스토리는 5성 기준",
          "original_form": "일단 내가 보는 스토리는 5성 기준",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021561.1.35",
          "form": "성유물 파츠 중에 가면 디자인이 일단 똑같고",
          "original_form": "성유물 파츠 중에 가면 디자인이 일단 똑같고",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021561.1.36",
          "form": "나도 이따가 함봐바야겠다",
          "original_form": "나도 이따가 함봐바야겠다",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021561.1.37",
          "form": "스토리가",
          "original_form": "스토리가",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021561.1.38",
          "form": "근데 난 아직 많이 없어서...",
          "original_form": "근데 난 아직 많이 없어서...",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021561.1.39",
          "form": "마물을 처치하던 기사가 사람들이 자길 무서워한단걸 깨닫고",
          "original_form": "마물을 처치하던 기사가 사람들이 자길 무서워한단걸 깨닫고",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021561.1.40",
          "form": "마물 소굴에서 죽기로 결심하고 심연에 들어갔다가",
          "original_form": "마물 소굴에서 죽기로 결심하고 심연에 들어갔다가",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021561.1.41",
          "form": "ㅇㅎ......",
          "original_form": "ㅇㅎ......",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021561.1.42",
          "form": "이게 사실은 신이 멸망시킨 켄리아 주민들이란걸 깨닫고",
          "original_form": "이게 사실은 신이 멸망시킨 켄리아 주민들이란걸 깨닫고",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021561.1.43",
          "form": "근데 그럼",
          "original_form": "근데 그럼",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021561.1.44",
          "form": "그쪽 편에 서기로 햇다는거",
          "original_form": "그쪽 편에 서기로 햇다는거",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021561.1.45",
          "form": "켄리아사람이",
          "original_form": "켄리아사람이",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021561.1.46",
          "form": "아니었던거네",
          "original_form": "아니었던거네",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021561.1.47",
          "form": "걘 아닌가봐",
          "original_form": "걘 아닌가봐",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021561.1.48",
          "form": "어쩐지 사람같이 생겼더라",
          "original_form": "어쩐지 사람같이 생겼더라",
          "speaker_id": "1",
          "time": "20210828 22:57"
        }
      ]
    }
  ]
