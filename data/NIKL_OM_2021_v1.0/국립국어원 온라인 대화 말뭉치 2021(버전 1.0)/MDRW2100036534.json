      "utterance": [
        {
          "id": "MDRW2100036534.1.1",
          "form": "맞아 펫숍을 그만 이용해야할텐데",
          "original_form": "맞아 펫숍을 그만 이용해야할텐데",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036534.1.2",
          "form": "펫샵 동물은 입양해선 안돼",
          "original_form": "펫샵 동물은 입양해선 안돼",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.3",
          "form": "나는 전혀 알수가없어",
          "original_form": "나는 전혀 알수가없어",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.4",
          "form": "개농장에서 데려온 애둘이야",
          "original_form": "개농장에서 데려온 애둘이야",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.5",
          "form": "유전병도 있을 수도 있는데",
          "original_form": "유전병도 있을 수도 있는데",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.6",
          "form": "책임 못질꺼면 나처럼 처음부터 키우지 말아야지",
          "original_form": "책임 못질꺼면 나처럼 처음부터 키우지 말아야지",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.7",
          "form": "비싼값을 주고 왜 굳이 걔네를 데려오는지 모르겠어",
          "original_form": "비싼값을 주고 왜 굳이 걔네를 데려오는지 모르겠어",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.8",
          "form": "유기견들도 사랑받아야해",
          "original_form": "유기견들도 사랑받아야해",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.9",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.10",
          "form": "사람들이 참 책임감이 없어",
          "original_form": "사람들이 참 책임감이 없어",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.11",
          "form": "그리고 유기견 편견도 너무 심해",
          "original_form": "그리고 유기견 편견도 너무 심해",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.12",
          "form": "버려지는 동물들이 참 많다더라",
          "original_form": "버려지는 동물들이 참 많다더라",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.13",
          "form": "맞아 유기견들도 순한야들 많아",
          "original_form": "맞아 유기견들도 순한야들 많아",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.14",
          "form": "미디어에서 좀 좋게 알려줬으면 좋겠어",
          "original_form": "미디어에서 좀 좋게 알려줬으면 좋겠어",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.15",
          "form": "맞아 이상한 편견 그만 주입시키고",
          "original_form": "맞아 이상한 편견 그만 주입시키고",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100036534.1.16",
          "form": "오히려 펫숍 아이들이 더 키우기 힘들껄",
          "original_form": "오히려 펫숍 아이들이 더 키우기 힘들껄",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036534.1.17",
          "form": "최근에도 논란이 있던데 너무아쉽다",
          "original_form": "최근에도 논란이 있던데 너무아쉽다",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036534.1.18",
          "form": "그러니까말이야",
          "original_form": "그러니까말이야",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036534.1.19",
          "form": "연예인들이나 유명인들이 유기견에 대한 인식을 개선시켜줬으면 해",
          "original_form": "연예인들이나 유명인들이 유기견에 대한 인식을 개선시켜줬으면 해",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036534.1.20",
          "form": "나 오늘 커뮤에서 어떤 걸 본 줄 알아?",
          "original_form": "나 오늘 커뮤에서 어떤 걸 본 줄 알아?",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100036534.1.21",
          "form": "강아지 정수리를 봤어",
          "original_form": "강아지 정수리를 봤어",
          "speaker_id": "1",
          "time": "20210831 23:10"
        }
      ]
    }
