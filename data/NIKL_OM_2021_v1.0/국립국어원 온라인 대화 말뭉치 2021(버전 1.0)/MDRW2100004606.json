      "utterance": [
        {
          "id": "MDRW2100004606.1.1",
          "form": "인터넷 쇼핑 많이 하나요?",
          "original_form": "인터넷 쇼핑 많이 하나요?",
          "speaker_id": "1",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004606.1.2",
          "form": "인터넷으로 이것저것 사는 편이죠",
          "original_form": "인터넷으로 이것저것 사는 편이죠",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004606.1.3",
          "form": "중고거래 특히 많이해요",
          "original_form": "중고거래 특히 많이해요",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004606.1.4",
          "form": "당근마켓인가요?",
          "original_form": "당근마켓인가요?",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004606.1.5",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004606.1.6",
          "form": "접근성이 좋아서",
          "original_form": "접근성이 좋아서",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004606.1.7",
          "form": "파는건 해본적 없고 맨날 뭘 사네요",
          "original_form": "파는건 해본적 없고 맨날 뭘 사네요",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004606.1.8",
          "form": "득템 하신 거 있나요?",
          "original_form": "득템 하신 거 있나요?",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004606.1.9",
          "form": "이번에 영어교재 샀는대",
          "original_form": "이번에 영어교재 샀는대",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004606.1.10",
          "form": "2천원에 득템해서 기분이 좋내요",
          "original_form": "2천원에 득템해서 기분이 좋내요",
          "speaker_id": "2",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004606.1.11",
          "form": "인형 무나하시는 것도 쓸어오고",
          "original_form": "인형 무나하시는 것도 쓸어오고",
          "speaker_id": "2",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004606.1.12",
          "form": "와 2천원",
          "original_form": "와 2천원",
          "speaker_id": "1",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004606.1.13",
          "form": "당근마켓은 파는것만 생각했는데",
          "original_form": "당근마켓은 파는것만 생각했는데",
          "speaker_id": "1",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004606.1.14",
          "form": "사는것도 괜찮네요",
          "original_form": "사는것도 괜찮네요",
          "speaker_id": "1",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004606.1.15",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004606.1.16",
          "form": "한번씩은 꼭 검색해보고사요",
          "original_form": "한번씩은 꼭 검색해보고사요",
          "speaker_id": "2",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004606.1.17",
          "form": "물건 새로 사는 것보다",
          "original_form": "물건 새로 사는 것보다",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004606.1.18",
          "form": "중고품 사는게 환경에도 좋고 돈도 아껴서 좋네요",
          "original_form": "중고품 사는게 환경에도 좋고 돈도 아껴서 좋네요",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004606.1.19",
          "form": "음식같은 건 주로 어떻게 사시나요?",
          "original_form": "음식같은 건 주로 어떻게 사시나요?",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004606.1.20",
          "form": "음식은 티몬에서 닭가슴살 많이 주문해먹네료",
          "original_form": "음식은 티몬에서 닭가슴살 많이 주문해먹네료",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004606.1.21",
          "form": "야채는 그냥 일반 마트가고요",
          "original_form": "야채는 그냥 일반 마트가고요",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004606.1.22",
          "form": "저는 재래시장 자주 가고 생필품은 이마트 자주 이용해요",
          "original_form": "저는 재래시장 자주 가고 생필품은 이마트 자주 이용해요",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004606.1.23",
          "form": "배달은 자주 시키나요?",
          "original_form": "배달은 자주 시키나요?",
          "speaker_id": "1",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004606.1.24",
          "form": "배달 자주시키는 편인거같네요",
          "original_form": "배달 자주시키는 편인거같네요",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004606.1.25",
          "form": "할인쿠폰 먹여서 사면 이쪽이 더 싼것같아서",
          "original_form": "할인쿠폰 먹여서 사면 이쪽이 더 싼것같아서",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004606.1.26",
          "form": "네이버 자주 이용해요",
          "original_form": "네이버 자주 이용해요",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004606.1.27",
          "form": "네이버는 포인트 모아서 결제할 수 있어 좋죠",
          "original_form": "네이버는 포인트 모아서 결제할 수 있어 좋죠",
          "speaker_id": "1",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004606.1.28",
          "form": "멤버십도 쇼핑 자주하면 좋을 것 같고요",
          "original_form": "멤버십도 쇼핑 자주하면 좋을 것 같고요",
          "speaker_id": "1",
          "time": "20210821 18:21"
        }
      ]
    }
