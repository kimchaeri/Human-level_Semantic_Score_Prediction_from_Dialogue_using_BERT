      "utterance": [
        {
          "id": "MDRW2100029607.1.1",
          "form": "양도키우고",
          "original_form": "양도키우고",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029607.1.2",
          "form": "돼지도 키우고",
          "original_form": "돼지도 키우고",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029607.1.3",
          "form": "소도 키우고",
          "original_form": "소도 키우고",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029607.1.4",
          "form": "집도 짓고요",
          "original_form": "집도 짓고요",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029607.1.5",
          "form": "닭도 키울수 있어요",
          "original_form": "닭도 키울수 있어요",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029607.1.6",
          "form": "최근엔 염소도 생겼어요",
          "original_form": "최근엔 염소도 생겼어요",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.7",
          "form": "엄청 현실적이네요",
          "original_form": "엄청 현실적이네요",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.8",
          "form": "마을 찾으면",
          "original_form": "마을 찾으면",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.9",
          "form": "집지어야 자는거 아닌가요",
          "original_form": "집지어야 자는거 아닌가요",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.10",
          "form": "거기서 고양이 길들여서",
          "original_form": "거기서 고양이 길들여서",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.11",
          "form": "데려올수 있어요",
          "original_form": "데려올수 있어요",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.12",
          "form": "집없음 땅바닥서 자나요",
          "original_form": "집없음 땅바닥서 자나요",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.13",
          "form": "침대만 만들어서 다녀도 된답니다",
          "original_form": "침대만 만들어서 다녀도 된답니다",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.14",
          "form": "냥이 이름도 지어주나요",
          "original_form": "냥이 이름도 지어주나요",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.15",
          "form": "심즈같네요 섬세하네요",
          "original_form": "심즈같네요 섬세하네요",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.16",
          "form": "이름은 못지어주지만",
          "original_form": "이름은 못지어주지만",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.17",
          "form": "목줄 색은 바꿀수있어요",
          "original_form": "목줄 색은 바꿀수있어요",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029607.1.18",
          "form": "지금도 그 게임하시나요",
          "original_form": "지금도 그 게임하시나요",
          "speaker_id": "1",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029607.1.19",
          "form": "양털 염색할수 있어요",
          "original_form": "양털 염색할수 있어요",
          "speaker_id": "2",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029607.1.20",
          "form": "한번씩 해요",
          "original_form": "한번씩 해요",
          "speaker_id": "2",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029607.1.21",
          "form": "시간잘가겠군요",
          "original_form": "시간잘가겠군요",
          "speaker_id": "1",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029607.1.22",
          "form": "모험하러 가는거죠",
          "original_form": "모험하러 가는거죠",
          "speaker_id": "2",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029607.1.23",
          "form": "폰으로 하시나요",
          "original_form": "폰으로 하시나요",
          "speaker_id": "1",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029607.1.24",
          "form": "낚시두해요",
          "original_form": "낚시두해요",
          "speaker_id": "2",
          "time": "20210903 10:55"
        }
      ]
