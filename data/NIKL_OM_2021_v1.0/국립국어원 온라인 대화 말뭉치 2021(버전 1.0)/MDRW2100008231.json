      "utterance": [
        {
          "id": "MDRW2100008231.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100008231.1.2",
          "form": "저는 안 키워요",
          "original_form": "저는 안 키워요",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008231.1.3",
          "form": "이나 님은 키우시나요?",
          "original_form": "이나 님은 키우시나요?",
          "speaker_id": "2",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008231.1.4",
          "form": "부모님이 반대하셔서ㅎㅎㅎㅎ",
          "original_form": "부모님이 반대하셔서ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008231.1.5",
          "form": "알레르기 있으세요?",
          "original_form": "알레르기 있으세요?",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008231.1.6",
          "form": "아뇨 전 없어요",
          "original_form": "아뇨 전 없어요",
          "speaker_id": "1",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008231.1.7",
          "form": "있으세외?",
          "original_form": "있으세외?",
          "speaker_id": "1",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008231.1.8",
          "form": "아뇨 저도 없어요",
          "original_form": "아뇨 저도 없어요",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008231.1.9",
          "form": "나중에라도 키우고 싶으신가요?",
          "original_form": "나중에라도 키우고 싶으신가요?",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008231.1.10",
          "form": "기회가 된다면 그리고 능력이있다면요!",
          "original_form": "기회가 된다면 그리고 능력이있다면요!",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008231.1.11",
          "form": "어떤 동물 키우고 싶으세요?",
          "original_form": "어떤 동물 키우고 싶으세요?",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008231.1.12",
          "form": "고양이요!",
          "original_form": "고양이요!",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008231.1.13",
          "form": "고양이 너무 귀엽죠",
          "original_form": "고양이 너무 귀엽죠",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008231.1.14",
          "form": "저도 고양이 영상 자주 찾아봐요",
          "original_form": "저도 고양이 영상 자주 찾아봐요",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008231.1.15",
          "form": "완전 힐링",
          "original_form": "완전 힐링",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008231.1.16",
          "form": "그 도도한 매력이 너무 좋아요ㅜ",
          "original_form": "그 도도한 매력이 너무 좋아요ㅜ",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008231.1.17",
          "form": "맞아요 가끔 부리는 애교도 귀엽고",
          "original_form": "맞아요 가끔 부리는 애교도 귀엽고",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008231.1.18",
          "form": "가능만하다면 강아지도 같이 키우고싶어요ㅎ",
          "original_form": "가능만하다면 강아지도 같이 키우고싶어요ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:22"
        }
      ]
