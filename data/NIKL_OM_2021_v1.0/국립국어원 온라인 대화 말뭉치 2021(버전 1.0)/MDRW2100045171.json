      "utterance": [
        {
          "id": "MDRW2100045171.1.1",
          "form": "이제.가을인갓 같죠?",
          "original_form": "이제.가을인갓 같죠?",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045171.1.2",
          "form": "네선선하네요!",
          "original_form": "네선선하네요!",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045171.1.3",
          "form": "환절기에 힘드신건 없으신가요?",
          "original_form": "환절기에 힘드신건 없으신가요?",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045171.1.4",
          "form": "비염러라 ㅠㅠㅠㅠ",
          "original_form": "비염러라 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045171.1.5",
          "form": "저도여 ㅠ",
          "original_form": "저도여 ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045171.1.6",
          "form": "두코로 숨쉬고 싶어여ㅠㅠ",
          "original_form": "두코로 숨쉬고 싶어여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045171.1.7",
          "form": "저 가을오면 약을 달고 살아요 코세척이랑",
          "original_form": "저 가을오면 약을 달고 살아요 코세척이랑",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045171.1.8",
          "form": "아ㅠㅠ 코세척 첫째도 ㅠㅠ ㅠ필수에여ㅠ",
          "original_form": "아ㅠㅠ 코세척 첫째도 ㅠㅠ ㅠ필수에여ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045171.1.9",
          "form": "것도 유전이라ㅠㅠ",
          "original_form": "것도 유전이라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045171.1.10",
          "form": "그런것 같아요 우리 첫쨔는 아직 코세척을 못해서요 ㅠ",
          "original_form": "그런것 같아요 우리 첫쨔는 아직 코세척을 못해서요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045171.1.11",
          "form": "7.8세쯤 한것 같은데 ..",
          "original_form": "7.8세쯤 한것 같은데 ..",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045171.1.12",
          "form": "잘 꼬득여서 하고 나면 지금은 시원해서 좋아해여",
          "original_form": "잘 꼬득여서 하고 나면 지금은 시원해서 좋아해여",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045171.1.13",
          "form": "우리앤 7세인데.기겁을하던데요. 그래서 그냥 훌쩍이며 다녀요 ㅠ ㅜ",
          "original_form": "우리앤 7세인데.기겁을하던데요. 그래서 그냥 훌쩍이며 다녀요 ㅠ ㅜ",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045171.1.14",
          "form": "근데 그것도 하고나서 잠깐 시원한거라ㅠㅠㅠ",
          "original_form": "근데 그것도 하고나서 잠깐 시원한거라ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045171.1.15",
          "form": "한번 꼬득여봐야겠어요. 암튼 가을 너무 힘들어요",
          "original_form": "한번 꼬득여봐야겠어요. 암튼 가을 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210908 01:13"
        }
      ]
