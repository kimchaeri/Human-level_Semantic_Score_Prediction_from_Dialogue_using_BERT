        },
        {
          "id": "MDRW2100023990.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023990.1.4",
          "form": "전 기독교에요!",
          "original_form": "전 기독교에요!",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023990.1.5",
          "form": "교회 활동에 잘 참여하시나요?",
          "original_form": "교회 활동에 잘 참여하시나요?",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023990.1.6",
          "form": "아뇨ㅠ 예배만 유튜브로 보고있어요",
          "original_form": "아뇨ㅠ 예배만 유튜브로 보고있어요",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100023990.1.7",
          "form": "종교에 관심 없으신가요?",
          "original_form": "종교에 관심 없으신가요?",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100023990.1.8",
          "form": "네 관심이 안 가더라구요",
          "original_form": "네 관심이 안 가더라구요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100023990.1.9",
          "form": "그래도 공휴일은 좋아해요",
          "original_form": "그래도 공휴일은 좋아해요",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100023990.1.10",
          "form": "아 저도 공휴일 좋아함닷!ㅋㅋ",
          "original_form": "아 저도 공휴일 좋아함닷!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100023990.1.11",
          "form": "요즘 논란이 많아 속상해요ㅠ",
          "original_form": "요즘 논란이 많아 속상해요ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100023990.1.12",
          "form": "무슨 논란이요?",
          "original_form": "무슨 논란이요?",
          "speaker_id": "1",
          "time": "20210831 07:35"
        },
        {
          "id": "MDRW2100023990.1.13",
          "form": "기독교 관련해서 말이많은거오?",
          "original_form": "기독교 관련해서 말이많은거오?",
          "speaker_id": "2",
          "time": "20210831 07:57"
        },
        {
          "id": "MDRW2100023990.1.14",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210831 07:57"
        },
        {
          "id": "MDRW2100023990.1.15",
          "form": "아  일부 사이비나 이단 때문에 그렇죠",
          "original_form": "아  일부 사이비나 이단 때문에 그렇죠",
          "speaker_id": "1",
          "time": "20210831 07:59"
        },
        {
          "id": "MDRW2100023990.1.16",
          "form": "네ㅠ 그러니까요",
          "original_form": "네ㅠ 그러니까요",
          "speaker_id": "2",
          "time": "20210831 08:25"
        },
        {
          "id": "MDRW2100023990.1.17",
          "form": "집회한다고 뉴스나옴 휴.. 이래요",
          "original_form": "집회한다고 뉴스나옴 휴.. 이래요",
          "speaker_id": "2",
          "time": "20210831 08:25"
        },
        {
          "id": "MDRW2100023990.1.18",
          "form": "또 집회를... 진짜 그런 사람들 때문에 속상하시겠어요",
          "original_form": "또 집회를... 진짜 그런 사람들 때문에 속상하시겠어요",
          "speaker_id": "1",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100023990.1.19",
          "form": "네ㅠ 요즘 기독교라 그러면 좀 안좋게 보더라구요",
          "original_form": "네ㅠ 요즘 기독교라 그러면 좀 안좋게 보더라구요",
          "speaker_id": "2",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100023990.1.20",
          "form": "일부 잘못된 행동 하는 사람들 때문에 전체가 욕 먹으면 안 되는데요 ㅠ 힘내세요",
