      "utterance": [
        {
          "id": "MDRW2100026420.1.1",
          "form": "부산대 앞쪽에 돈까스 맛집 엄청많더라고요",
          "original_form": "부산대 앞쪽에 돈까스 맛집 엄청많더라고요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026420.1.2",
          "form": "그리구 부산역에서 30분정도 버스타고 가시면",
          "original_form": "그리구 부산역에서 30분정도 버스타고 가시면",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026420.1.3",
          "form": "석",
          "original_form": "석",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026420.1.4",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026420.1.5",
          "form": "부대앞에 맛있는게 많아요",
          "original_form": "부대앞에 맛있는게 많아요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026420.1.6",
          "form": "학교앞이라 자주갔었어요. 라멘집이나 돈까스집이요",
          "original_form": "학교앞이라 자주갔었어요. 라멘집이나 돈까스집이요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026420.1.7",
          "form": "부대앞에 김밥도 맛있지 않아요?",
          "original_form": "부대앞에 김밥도 맛있지 않아요?",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026420.1.8",
          "form": "샌드위치집은 잘 안가게 되는데",
          "original_form": "샌드위치집은 잘 안가게 되는데",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026420.1.9",
          "form": "정문쪽에 김밥집 너무 맛있어요",
          "original_form": "정문쪽에 김밥집 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026420.1.10",
          "form": "그죠 한끼 때우기 좋아요",
          "original_form": "그죠 한끼 때우기 좋아요",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026420.1.11",
          "form": "부대쪽 자주가셨으면 코하루라는 라멘집도 가보셨나요?",
          "original_form": "부대쪽 자주가셨으면 코하루라는 라멘집도 가보셨나요?",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026420.1.12",
          "form": "거기도 추천드려요",
          "original_form": "거기도 추천드려요",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026420.1.13",
          "form": "아 거기 나중에 가보려고요!",
          "original_form": "아 거기 나중에 가보려고요!",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026420.1.14",
          "form": "거기 유명하더라고요",
          "original_form": "거기 유명하더라고요",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026420.1.15",
          "form": "오뚜기집이던가 거기 아세요?",
          "original_form": "오뚜기집이던가 거기 아세요?",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026420.1.16",
          "form": "오뚜기 분식 말하시나요?",
          "original_form": "오뚜기 분식 말하시나요?",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026420.1.17",
          "form": "저분식을 안좋아해서...거긴 안가봤어요",
          "original_form": "저분식을 안좋아해서...거긴 안가봤어요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026420.1.18",
          "form": "아 거기 맞아요 거기도 나중에 가봐야지하고 있어요",
          "original_form": "아 거기 맞아요 거기도 나중에 가봐야지하고 있어요",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026420.1.19",
          "form": "그 돈카츠집 유명한데도 있는데",
          "original_form": "그 돈카츠집 유명한데도 있는데",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026420.1.20",
          "form": "죄다 상호명이 기억이 안나네요ㅠ",
          "original_form": "죄다 상호명이 기억이 안나네요ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026420.1.21",
          "form": "근데 부대쪽에 친구분이 있나요?",
          "original_form": "근데 부대쪽에 친구분이 있나요?",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026420.1.22",
          "form": "보통 놀러와서 부대쪽안가시던데",
          "original_form": "보통 놀러와서 부대쪽안가시던데",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026420.1.23",
          "form": "네 친구가 부대쪽에 살아서",
          "original_form": "네 친구가 부대쪽에 살아서",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026420.1.24",
          "form": "부대쪽이 다른 동네보다 학교앞이다보니",
          "original_form": "부대쪽이 다른 동네보다 학교앞이다보니",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026420.1.25",
          "form": "물가가 싸서 종종 갔어요!",
          "original_form": "물가가 싸서 종종 갔어요!",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026420.1.26",
          "form": "그러시구나! 어쩐지 부대쪽을 많이 아시더라",
          "original_form": "그러시구나! 어쩐지 부대쪽을 많이 아시더라",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026420.1.27",
          "form": "확실히 어느 지역이든 학교앞은 가성비 좋거나 맛있는곳이 많은거 같아요!",
          "original_form": "확실히 어느 지역이든 학교앞은 가성비 좋거나 맛있는곳이 많은거 같아요!",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026420.1.28",
          "form": "맞아요 대학로가 가성비는 최고인거 같아요",
          "original_form": "맞아요 대학로가 가성비는 최고인거 같아요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026420.1.29",
          "form": "서울도 대학로가 싸더라구요",
          "original_form": "서울도 대학로가 싸더라구요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026420.1.30",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026420.1.31",
          "form": "서울도 물가는 비싼편이지만 대학로 가면 그래도 가성비 있게 먹을 수 있는거 같아요",
          "original_form": "서울도 물가는 비싼편이지만 대학로 가면 그래도 가성비 있게 먹을 수 있는거 같아요",
          "speaker_id": "2",
          "time": "20210901 22:44"
        }
      ]
    }
