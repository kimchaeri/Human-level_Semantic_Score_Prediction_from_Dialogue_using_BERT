      "utterance": [
        {
          "id": "MDRW2100026756.1.1",
          "form": "그쵸 왜 제 주변 사람들만 집에 있는지ㅠㅠ",
          "original_form": "그쵸 왜 제 주변 사람들만 집에 있는지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026756.1.2",
          "form": "ㅠㅠ제주변도...",
          "original_form": "ㅠㅠ제주변도...",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026756.1.3",
          "form": "비도 가을장마라",
          "original_form": "비도 가을장마라",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026756.1.4",
          "form": "너무 많이오고..",
          "original_form": "너무 많이오고..",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026756.1.5",
          "form": "갈수없어요 ㅠㅠ",
          "original_form": "갈수없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026756.1.6",
          "form": "맞아 전 펌 했는데 비 때문에 ㅋㅋㅋㅋㅋㅋ 맨날 쳐져서 슬퍼요~!~!",
          "original_form": "맞아 전 펌 했는데 비 때문에 ㅋㅋㅋㅋㅋㅋ 맨날 쳐져서 슬퍼요~!~!",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026756.1.7",
          "form": "으어... 펌했는데 ㅠㅠㅠㅜ비가 ㅠㅠㅠㅠㅠ",
          "original_form": "으어... 펌했는데 ㅠㅠㅠㅜ비가 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026756.1.8",
          "form": "그래도 오늘은 좀 떴네요 ㅎㅎ 습한데 좀 덜한 것 같아요",
          "original_form": "그래도 오늘은 좀 떴네요 ㅎㅎ 습한데 좀 덜한 것 같아요",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026756.1.9",
          "form": "그래요? 오늘도 비장난아니었는데 ㅠㅠ",
          "original_form": "그래요? 오늘도 비장난아니었는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026756.1.10",
          "form": "비맞고 집왔어요 ㅠ",
          "original_form": "비맞고 집왔어요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026756.1.11",
          "form": "오늘은 그래도 비가 안오네요 ㅎㅎ",
          "original_form": "오늘은 그래도 비가 안오네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100026756.1.12",
          "form": "오늘도 꽤 선선하네요!! 이제 비 좀 그만 왔음 좋겠어요...",
          "original_form": "오늘도 꽤 선선하네요!! 이제 비 좀 그만 왔음 좋겠어요...",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100026756.1.13",
          "form": "저도 비그만오길바랍니다 ㅠ",
          "original_form": "저도 비그만오길바랍니다 ㅠ",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100026756.1.14",
          "form": "하지만 비와요 ㅠㅠㅠㅠ",
          "original_form": "하지만 비와요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100026756.1.15",
          "form": "오늘은 뜨겁네요 날씨가... 어유",
          "original_form": "오늘은 뜨겁네요 날씨가... 어유",
          "speaker_id": "2",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100026756.1.16",
          "form": "너무 좋아서 탈이에요 그래도 바람은 안 뜨거워요",
          "original_form": "너무 좋아서 탈이에요 그래도 바람은 안 뜨거워요",
          "speaker_id": "2",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100026756.1.17",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100026756.1.18",
          "form": "여긴 흐리고 바람이 차서 춥네요",
          "original_form": "여긴 흐리고 바람이 차서 춥네요",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100026756.1.19",
          "form": "오늘 하늘이 정말 예뻐요 한 번 보세요",
          "original_form": "오늘 하늘이 정말 예뻐요 한 번 보세요",
          "speaker_id": "2",
          "time": "20210905 16:14"
        },
        {
          "id": "MDRW2100026756.1.20",
          "form": "주말이라 그런가 더 기분 좋은 하루네요 ㅎㅎ",
          "original_form": "주말이라 그런가 더 기분 좋은 하루네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:14"
        },
        {
          "id": "MDRW2100026756.1.21",
          "form": "하늘..못봤네요 ㅠㅠ",
          "original_form": "하늘..못봤네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100026756.1.22",
          "form": "이미 너무 검검해서 ㅠ",
          "original_form": "이미 너무 검검해서 ㅠ",
