      "utterance": [
        {
          "id": "MDRW2100033750.1.1",
          "form": "있었어요?",
          "original_form": "있었어요?",
          "speaker_id": "2",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100033750.1.2",
          "form": "카페베네 예전에 가입했었는데",
          "original_form": "카페베네 예전에 가입했었는데",
          "speaker_id": "1",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100033750.1.3",
          "form": "앱으로 들어온대요?",
          "original_form": "앱으로 들어온대요?",
          "speaker_id": "1",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100033750.1.4",
          "form": "네ㅠ 카페배네 앱도 예전에 룰렛 같은거 하셨으면 있으실거에요 없어도 익일 지급이니 가입하심 될껀데 ㅠㅠ 애네들 이런식으로 신규고객 잘 땡겨가네요ㅠ 수박 머시기 맛있어보이진 않는데 ㅋ",
          "original_form": "네ㅠ 카페배네 앱도 예전에 룰렛 같은거 하셨으면 있으실거에요 없어도 익일 지급이니 가입하심 될껀데 ㅠㅠ 애네들 이런식으로 신규고객 잘 땡겨가네요ㅠ 수박 머시기 맛있어보이진 않는데 ㅋ",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100033750.1.5",
          "form": "그거했어요ㅋ",
          "original_form": "그거했어요ㅋ",
          "speaker_id": "1",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100033750.1.6",
          "form": "룰렛",
          "original_form": "룰렛",
          "speaker_id": "1",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100033750.1.7",
          "form": "재미 좋았었는데 ㅋㅋㅋ",
          "original_form": "재미 좋았었는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100033750.1.8",
          "form": "아뒤있을듯요 휴면계정이거나",
          "original_form": "아뒤있을듯요 휴면계정이거나",
          "speaker_id": "1",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100033750.1.9",
          "form": "전당첨안되더라구요ㅎ",
          "original_form": "전당첨안되더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100033750.1.10",
          "form": "되어도 여러개 안써줘서 막 해주는곳 찾아가고 했던것 같아요",
          "original_form": "되어도 여러개 안써줘서 막 해주는곳 찾아가고 했던것 같아요",
          "speaker_id": "2",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100033750.1.11",
          "form": "망고젤리만 찾아먹은듯요ㅎ",
          "original_form": "망고젤리만 찾아먹은듯요ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100033750.1.12",
          "form": "매장도 별로 없는 카페베네 ㅋㅋ 다행이 동네에 딱 1개 있어서 ㅋ",
          "original_form": "매장도 별로 없는 카페베네 ㅋㅋ 다행이 동네에 딱 1개 있어서 ㅋ",
          "speaker_id": "2",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100033750.1.13",
          "form": "저희동네도 하나있는데 앱은 낼깔아봐야겠네요",
          "original_form": "저희동네도 하나있는데 앱은 낼깔아봐야겠네요",
          "speaker_id": "1",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100033750.1.14",
          "form": "수박철다지났는데 왜이제서야 수박쥬스를ㅎㅎ",
          "original_form": "수박철다지났는데 왜이제서야 수박쥬스를ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100033750.1.15",
          "form": "재고처리인가...봉가.//ㅠㅠ",
          "original_form": "재고처리인가...봉가.//ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100033750.1.16",
          "form": "판매는 안되겠죠?",
          "original_form": "판매는 안되겠죠?",
          "speaker_id": "1",
          "time": "20210825 00:50"
        },
        {
          "id": "MDRW2100033750.1.17",
          "form": "앱으로 줘서",
          "original_form": "앱으로 줘서",
          "speaker_id": "1",
          "time": "20210825 00:50"
        },
        {
          "id": "MDRW2100033750.1.18",
          "form": "요 앞전에 파는거 같기도 다른 메뉴 한번 했었거든요 검색 한번 해보세요 카페베네",
          "original_form": "요 앞전에 파는거 같기도 다른 메뉴 한번 했었거든요 검색 한번 해보세요 카페베네",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033750.1.19",
          "form": "라이스 뭐시기 ㅋㅋ 저는 받고 귀찮아서 안갔어요ㅠㅠ 그때는 백원주고 샀는데",
          "original_form": "라이스 뭐시기 ㅋㅋ 저는 받고 귀찮아서 안갔어요ㅠㅠ 그때는 백원주고 샀는데",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033750.1.20",
          "form": "우선 앱부터보고 그때가서 고민해야겠어요",
          "original_form": "우선 앱부터보고 그때가서 고민해야겠어요",
          "speaker_id": "1",
          "time": "20210825 01:03"
        }
      ]
    }
