      "utterance": [
        {
          "id": "MDRW2100001911.1.1",
          "form": "아직 학생은 아니시져?",
          "original_form": "아직 학생은 아니시져?",
          "speaker_id": "2",
          "time": "20210819 23:04"
        },
        {
          "id": "MDRW2100001911.1.2",
          "form": "네 학생아니에요. 학교다니시나요",
          "original_form": "네 학생아니에요. 학교다니시나요",
          "speaker_id": "1",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001911.1.3",
          "form": "학교 졸업한지오래죠 ㅠ",
          "original_form": "학교 졸업한지오래죠 ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001911.1.4",
          "form": "대학교 나오셨어영?",
          "original_form": "대학교 나오셨어영?",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001911.1.5",
          "form": "수강신청 생각나네여 ㅋㅋ",
          "original_form": "수강신청 생각나네여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001911.1.6",
          "form": "수강신청 지옥이죠ㅎㅎ 뭐가 가장 신청하기 힘들었나요?",
          "original_form": "수강신청 지옥이죠ㅎㅎ 뭐가 가장 신청하기 힘들었나요?",
          "speaker_id": "1",
          "time": "20210819 23:08"
        },
        {
          "id": "MDRW2100001911.1.7",
          "form": "공강만드려고 몰아서 하는게 젤힘들죠 ㅠ",
          "original_form": "공강만드려고 몰아서 하는게 젤힘들죠 ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001911.1.8",
          "form": "아ㅠㅠ 혹시 기숙사 살았아요? 저는 기숙사 안살아서 학교 가는게 너무 힘들었어요",
          "original_form": "아ㅠㅠ 혹시 기숙사 살았아요? 저는 기숙사 안살아서 학교 가는게 너무 힘들었어요",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001911.1.9",
          "form": "아 저는 집이 멀진않았어요 !",
          "original_form": "아 저는 집이 멀진않았어요 !",
          "speaker_id": "2",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001911.1.10",
          "form": "다니는데 힘든건 없었네용 ㅎㅎ",
          "original_form": "다니는데 힘든건 없었네용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001911.1.11",
          "form": "집 가까운게 최고인거같아요 진짜",
          "original_form": "집 가까운게 최고인거같아요 진짜",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001911.1.12",
          "form": "학교 다닐 때 힘든 점은 없었나요?",
          "original_form": "학교 다닐 때 힘든 점은 없었나요?",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001911.1.13",
          "form": "저는 팀플.. 팀플 안맞으면..진짜",
          "original_form": "저는 팀플.. 팀플 안맞으면..진짜",
          "speaker_id": "1",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001911.1.14",
          "form": "ㅋㅋㅋㅋㅋ맞아요 팀플진짜",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 팀플진짜",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001911.1.15",
          "form": "짜증나죠 ..",
          "original_form": "짜증나죠 ..",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001911.1.16",
          "form": "과제많이 내는 교수님도 짜증..",
          "original_form": "과제많이 내는 교수님도 짜증..",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001911.1.17",
          "form": "진짜 과제도.. 인정합니다..",
          "original_form": "진짜 과제도.. 인정합니다..",
          "speaker_id": "1",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001911.1.18",
          "form": "저는 사람이지 컴퓨터가 아닌데.. 답도없이 많이 내주면..진짜ㅎ",
          "original_form": "저는 사람이지 컴퓨터가 아닌데.. 답도없이 많이 내주면..진짜ㅎ",
          "speaker_id": "1",
          "time": "20210819 23:18"
        },
        {
          "id": "MDRW2100001911.1.19",
          "form": "맞아요 ... 정말 과제는 딱히 도움안됏던거같아요",
          "original_form": "맞아요 ... 정말 과제는 딱히 도움안됏던거같아요",
          "speaker_id": "2",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100001911.1.20",
          "form": "힘들기만 힘들고 ㅠ",
          "original_form": "힘들기만 힘들고 ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100001911.1.21",
          "form": "그럼 재밌었던건 뭐있었어요?\r\n전 아무래도.. 새로운곳에 가서 새로운 사람을 만난다는거?",
          "original_form": "그럼 재밌었던건 뭐있었어요?\r\n전 아무래도.. 새로운곳에 가서 새로운 사람을 만난다는거?",
          "speaker_id": "1",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100001911.1.22",
          "form": "그래도 학생으루 돌아가고싶네영",
          "original_form": "그래도 학생으루 돌아가고싶네영",
          "speaker_id": "2",
          "time": "20210819 23:19"
        }
      ]
