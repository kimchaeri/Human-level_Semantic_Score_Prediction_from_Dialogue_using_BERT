      "utterance": [
        {
          "id": "MDRW2100013652.1.1",
          "form": "지금 붓 들었러요",
          "original_form": "지금 붓 들었러요",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013652.1.2",
          "form": "또 나와요?",
          "original_form": "또 나와요?",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013652.1.3",
          "form": "와 몰랐어요",
          "original_form": "와 몰랐어요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013652.1.4",
          "form": "꽃이요? 계속 나오죠ㅋㅋㅋㅋㅋ",
          "original_form": "꽃이요? 계속 나오죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013652.1.5",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013652.1.6",
          "form": "저하다 ㅋㅋ 꽃이 똑 떨어졌네요",
          "original_form": "저하다 ㅋㅋ 꽃이 똑 떨어졌네요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013652.1.7",
          "form": "역시 똥손이예요",
          "original_form": "역시 똥손이예요",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013652.1.8",
          "form": "꽃이 다죽어요 ㅋㅌ",
          "original_form": "꽃이 다죽어요 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013652.1.9",
          "form": "그정도로 떨어질거면 원래 죽은 꽃일꺼에요~~",
          "original_form": "그정도로 떨어질거면 원래 죽은 꽃일꺼에요~~",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013652.1.10",
          "form": "그래요?? 전 쫄았네요",
          "original_form": "그래요?? 전 쫄았네요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013652.1.11",
          "form": "정말 박사시네요",
          "original_form": "정말 박사시네요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013652.1.12",
          "form": "8년 내공 인정입니다 ㅎㅎ",
          "original_form": "8년 내공 인정입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013652.1.13",
          "form": "요걸 키워서 토마토 사진 찍을수 있을지 ㅋㅋ",
          "original_form": "요걸 키워서 토마토 사진 찍을수 있을지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013652.1.14",
          "form": "가을에도 열매나요",
          "original_form": "가을에도 열매나요",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013652.1.15",
          "form": "지금은 가망이 없는데",
          "original_form": "지금은 가망이 없는데",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013652.1.16",
          "form": "네 가을까지도 가능해요?",
          "original_form": "네 가을까지도 가능해요?",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013652.1.17",
          "form": "!",
          "original_form": "!",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013652.1.18",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013652.1.19",
          "form": "오 그래요",
          "original_form": "오 그래요",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013652.1.20",
          "form": "여름만 나나 했어요",
          "original_form": "여름만 나나 했어요",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013652.1.21",
          "form": "파는 잘자라요?",
          "original_form": "파는 잘자라요?",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013652.1.22",
          "form": "파는 음 두곳에서 키우는데",
          "original_form": "파는 음 두곳에서 키우는데",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013652.1.23",
          "form": "큰 화분은 잘 자라는데",
          "original_form": "큰 화분은 잘 자라는데",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013652.1.24",
          "form": "작은 화분은 힘이 없어서",
          "original_form": "작은 화분은 힘이 없어서",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013652.1.25",
          "form": "네 맞아요 저도 작은건 죽었어요",
          "original_form": "네 맞아요 저도 작은건 죽었어요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013652.1.26",
          "form": "역시 양분의 양이 틀려서 그런가요",
          "original_form": "역시 양분의 양이 틀려서 그런가요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013652.1.27",
          "form": "그리고 뿌리가 곰 팡이 펴서 ㅡㅡ버렸러요",
          "original_form": "그리고 뿌리가 곰 팡이 펴서 ㅡㅡ버렸러요",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013652.1.28",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013652.1.29",
          "form": "뿌리가 깊게 못내려서 그렇지 싶어요",
          "original_form": "뿌리가 깊게 못내려서 그렇지 싶어요",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013652.1.30",
          "form": "아 그건 싹 버리셔야ㅎㅎ",
          "original_form": "아 그건 싹 버리셔야ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013652.1.31",
          "form": "아 그런거군요",
          "original_form": "아 그런거군요",
          "speaker_id": "2",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013652.1.32",
          "form": "작아서 얕긴했어요 ㅜㅜ",
          "original_form": "작아서 얕긴했어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013652.1.33",
          "form": "좋은거 알아가네요 ㅎㅎ",
          "original_form": "좋은거 알아가네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013652.1.34",
          "form": "토마토 수확량은 어떤가요 ㅎㅎ",
          "original_form": "토마토 수확량은 어떤가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013652.1.35",
          "form": "왠지 엄청 따실것 같아요 풍성하게",
          "original_form": "왠지 엄청 따실것 같아요 풍성하게",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013652.1.36",
          "form": "한개에서 한 20개는 열리죠?ㅋㅋ",
          "original_form": "한개에서 한 20개는 열리죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013652.1.37",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013652.1.38",
          "form": "대박이예요",
          "original_form": "대박이예요",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013652.1.39",
          "form": "수확이 엄청나네요",
          "original_form": "수확이 엄청나네요",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013652.1.40",
          "form": "진짜 쀼듯하시겠어요",
          "original_form": "진짜 쀼듯하시겠어요",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013652.1.41",
          "form": "ㅋㅋ올해도 열개정도 이미 ㅎㅎ",
          "original_form": "ㅋㅋ올해도 열개정도 이미 ㅎㅎ",
