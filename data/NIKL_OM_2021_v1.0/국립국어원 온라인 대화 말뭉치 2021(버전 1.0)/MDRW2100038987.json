      "utterance": [
        {
          "id": "MDRW2100038987.1.1",
          "form": "뭐는 양면으로 생각해야겠네",
          "original_form": "뭐는 양면으로 생각해야겠네",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100038987.1.2",
          "form": "억울한 피해자도 있엇지만 경찰에서도 사망하거나 힘들었나봐",
          "original_form": "억울한 피해자도 있엇지만 경찰에서도 사망하거나 힘들었나봐",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100038987.1.3",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100038987.1.4",
          "form": "미국 범죄자인데 죄를 자백했음에도 무죄를 받았데",
          "original_form": "미국 범죄자인데 죄를 자백했음에도 무죄를 받았데",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100038987.1.5",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100038987.1.6",
          "form": "진짜 범죄자는 맞데",
          "original_form": "진짜 범죄자는 맞데",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100038987.1.7",
          "form": "피해자가족은 억울하겠다",
          "original_form": "피해자가족은 억울하겠다",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100038987.1.8",
          "form": "피의자의 권리에 대해 설명하지 않아서 무죄로",
          "original_form": "피의자의 권리에 대해 설명하지 않아서 무죄로",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100038987.1.9",
          "form": "이건 경찰의 잘못으로 그런거네",
          "original_form": "이건 경찰의 잘못으로 그런거네",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100038987.1.10",
          "form": "에궁 미란다 원칙에서 미란다는 그 범죄자의 이름이래",
          "original_form": "에궁 미란다 원칙에서 미란다는 그 범죄자의 이름이래",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100038987.1.11",
          "form": "사람이름이었구나",
          "original_form": "사람이름이었구나",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100038987.1.12",
          "form": "그러게 몰랐어",
          "original_form": "그러게 몰랐어",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100038987.1.13",
          "form": "자백만으로는 범죄성립이 안되네",
          "original_form": "자백만으로는 범죄성립이 안되네",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100038987.1.14",
          "form": "그러게 자백에 대한 증거가 있어야하는거네",
          "original_form": "그러게 자백에 대한 증거가 있어야하는거네",
