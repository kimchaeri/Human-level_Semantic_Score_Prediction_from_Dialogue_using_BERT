      "utterance": [
        {
          "id": "MDRW2100036447.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036447.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036447.1.3",
          "form": "오늘 뭐 드셨어요?",
          "original_form": "오늘 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036447.1.4",
          "form": "저는 김치찌개 먹었어요 name2님은 뭐 드셨나요??",
          "original_form": "저는 김치찌개 먹었어요 &name2&님은 뭐 드셨나요??",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036447.1.5",
          "form": "맛잇겠다ㅜ",
          "original_form": "맛잇겠다ㅜ",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036447.1.6",
          "form": "저는 지금 먹으려고요 진라면 끓여서!",
          "original_form": "저는 지금 먹으려고요 진라면 끓여서!",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036447.1.7",
          "form": "오늘 바쁘셨나봐요",
          "original_form": "오늘 바쁘셨나봐요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036447.1.8",
          "form": "그런건 아닌데 낮잠을",
          "original_form": "그런건 아닌데 낮잠을",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036447.1.9",
          "form": "아주 실컷잤어요ㅎㅎㅎ",
          "original_form": "아주 실컷잤어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036447.1.10",
          "form": "배고파서 깬..",
          "original_form": "배고파서 깬..",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036447.1.11",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036447.1.12",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036447.1.13",
          "form": "오이 들어간거 빼고는",
          "original_form": "오이 들어간거 빼고는",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036447.1.14",
          "form": "대체적으로 잘 먿는 편이에요ㅎㅎ",
          "original_form": "대체적으로 잘 먿는 편이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036447.1.15",
          "form": "김치찌개에 고기들어갔나요!!! 참치?!",
          "original_form": "김치찌개에 고기들어갔나요!!! 참치?!",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036447.1.16",
          "form": "저는 돼지고기 있는거요",
          "original_form": "저는 돼지고기 있는거요",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036447.1.17",
          "form": "진라면에 계란 넣으시나여요??",
          "original_form": "진라면에 계란 넣으시나여요??",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036447.1.18",
          "form": "네네 오늘은 슬라이스치즈도",
          "original_form": "네네 오늘은 슬라이스치즈도",
          "speaker_id": "2",
          "time": "20210831 00:22"
        },
        {
          "id": "MDRW2100036447.1.19",
          "form": "넣어봤어요ㅋㅋㅋㅋ",
          "original_form": "넣어봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:22"
        },
        {
          "id": "MDRW2100036447.1.20",
          "form": "살짝 그 느끼한게 땡겨서",
          "original_form": "살짝 그 느끼한게 땡겨서",
          "speaker_id": "2",
          "time": "20210831 00:23"
        },
        {
          "id": "MDRW2100036447.1.21",
          "form": "맛있으셨나요?",
          "original_form": "맛있으셨나요?",
          "speaker_id": "1",
          "time": "20210831 00:23"
        },
        {
          "id": "MDRW2100036447.1.22",
          "form": "밤에 먹는 라면은",
          "original_form": "밤에 먹는 라면은",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100036447.1.23",
          "form": "당연히 맛있죠ㅎㅎ",
          "original_form": "당연히 맛있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100036447.1.24",
          "form": "정말 맛있으셨을 것 같네요ㅎㅎㅎ",
          "original_form": "정말 맛있으셨을 것 같네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:26"
        }
      ]
