      "utterance": [
        {
          "id": "MDRW2100032672.1.1",
          "form": "슈돌 계속 해",
          "original_form": "슈돌 계속 해",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032672.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032672.1.3",
          "form": "슈돌 보는중",
          "original_form": "슈돌 보는중",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032672.1.4",
          "form": "대탈출 난이도 너무높다",
          "original_form": "대탈출 난이도 너무높다",
          "speaker_id": "1",
          "time": "20210906 00:07"
        },
        {
          "id": "MDRW2100032672.1.5",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "2",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100032672.1.6",
          "form": "나중에 꼭 볼게",
          "original_form": "나중에 꼭 볼게",
          "speaker_id": "2",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100032672.1.7",
          "form": "응 진짜 웨이래 요즘계속 높아",
          "original_form": "응 진짜 웨이래 요즘계속 높아",
          "speaker_id": "1",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100032672.1.8",
          "form": "ㅋㅋㅋㅋ다들 시즌4까지 오면서",
          "original_form": "ㅋㅋㅋㅋ다들 시즌4까지 오면서",
          "speaker_id": "2",
          "time": "20210906 00:17"
        },
        {
          "id": "MDRW2100032672.1.9",
          "form": "수준이 높아졌나봐",
          "original_form": "수준이 높아졌나봐",
          "speaker_id": "2",
          "time": "20210906 00:17"
        },
        {
          "id": "MDRW2100032672.1.10",
          "form": "응응근데 제작진은",
          "original_form": "응응근데 제작진은",
          "speaker_id": "1",
          "time": "20210906 00:34"
        },
        {
          "id": "MDRW2100032672.1.11",
          "form": "더높아짐",
          "original_form": "더높아짐",
          "speaker_id": "1",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100032672.1.12",
          "form": "레벨이 진짜",
          "original_form": "레벨이 진짜",
          "speaker_id": "1",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100032672.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ기대된다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ기대된다",
          "speaker_id": "2",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100032672.1.14",
          "form": "찾아봐야지ㅎㅎ",
          "original_form": "찾아봐야지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100032672.1.15",
          "form": "근데 이번이 첫화라",
          "original_form": "근데 이번이 첫화라",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032672.1.16",
          "form": "담주까지 이어지는거야",
          "original_form": "담주까지 이어지는거야",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032672.1.17",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "2",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032672.1.18",
          "form": "오히려 좋아ㅎ",
          "original_form": "오히려 좋아ㅎ",
          "speaker_id": "2",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032672.1.19",
          "form": "왱?",
          "original_form": "왱?",
          "speaker_id": "1",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032672.1.20",
          "form": "담주는본방보게??",
          "original_form": "담주는본방보게??",
          "speaker_id": "1",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032672.1.21",
          "form": "그럴수도 있고!",
          "original_form": "그럴수도 있고!",
          "speaker_id": "2",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032672.1.22",
          "form": "길어야 재밌어ㅎ",
          "original_form": "길어야 재밌어ㅎ",
          "speaker_id": "2",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032672.1.23",
          "form": "원래 2회분씩 하자나",
          "original_form": "원래 2회분씩 하자나",
          "speaker_id": "1",
          "time": "20210906 00:40"
        },
        {
          "id": "MDRW2100032672.1.24",
          "form": "???",
          "original_form": "???",
          "speaker_id": "1",
          "time": "20210906 00:40"
        },
        {
          "id": "MDRW2100032672.1.25",
          "form": "나 다 안보고 그 안에서 티비 틀었을 때 나오는 것만 봐서 잘 몰라",
          "original_form": "나 다 안보고 그 안에서 티비 틀었을 때 나오는 것만 봐서 잘 몰라",
          "speaker_id": "2",
          "time": "20210906 00:42"
        },
        {
          "id": "MDRW2100032672.1.26",
          "form": "근데 그때마다 재밌어서 다 같이 봤었지",
          "original_form": "근데 그때마다 재밌어서 다 같이 봤었지",
          "speaker_id": "2",
          "time": "20210906 00:42"
        },
        {
          "id": "MDRW2100032672.1.27",
          "form": "앜ㅋㅋㅋ글쿠나 뭔가 준비하고 본적은 없나보네",
          "original_form": "앜ㅋㅋㅋ글쿠나 뭔가 준비하고 본적은 없나보네",
          "speaker_id": "1",
          "time": "20210906 00:46"
        },
        {
          "id": "MDRW2100032672.1.28",
          "form": "나는 이거 딱 각잡고? 봐야 잼있더라구",
          "original_form": "나는 이거 딱 각잡고? 봐야 잼있더라구",
          "speaker_id": "1",
          "time": "20210906 00:46"
        },
        {
          "id": "MDRW2100032672.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032672.1.30",
          "form": "그렇게도 봐야지",
          "original_form": "그렇게도 봐야지",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032672.1.31",
          "form": "담에 한번 봐봐",
          "original_form": "담에 한번 봐봐",
          "speaker_id": "1",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100032672.1.32",
          "form": "강추",
          "original_form": "강추",
          "speaker_id": "1",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100032672.1.33",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "2",
