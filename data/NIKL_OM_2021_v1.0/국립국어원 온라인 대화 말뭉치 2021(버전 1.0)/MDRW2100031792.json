      "utterance": [
        {
          "id": "MDRW2100031792.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210905 00:29"
        },
        {
          "id": "MDRW2100031792.1.2",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210905 00:29"
        },
        {
          "id": "MDRW2100031792.1.3",
          "form": "떡볶이 김밥 오므라이스 냉면 등등 좋아해요",
          "original_form": "떡볶이 김밥 오므라이스 냉면 등등 좋아해요",
          "speaker_id": "2",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031792.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031792.1.5",
          "form": "떡볶이 김밥은 진짜 싫어하는사람 없는것 같아요.ㅋㅋ",
          "original_form": "떡볶이 김밥은 진짜 싫어하는사람 없는것 같아요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:58"
        },
        {
          "id": "MDRW2100031792.1.6",
          "form": "밀떡? 쌀떡?",
          "original_form": "밀떡? 쌀떡?",
          "speaker_id": "1",
          "time": "20210905 09:59"
        },
        {
          "id": "MDRW2100031792.1.7",
          "form": "안 가리지만 밀떡을 많이 먹어요ㅎㅎ",
          "original_form": "안 가리지만 밀떡을 많이 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:10"
        },
        {
          "id": "MDRW2100031792.1.8",
          "form": "아 저도 밀떡이요ㅋㅋ",
          "original_form": "아 저도 밀떡이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:14"
        },
        {
          "id": "MDRW2100031792.1.9",
          "form": "로제? 빨간? 간장?  중에는요?ㅋ",
          "original_form": "로제? 빨간? 간장?  중에는요?ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:14"
        },
        {
          "id": "MDRW2100031792.1.10",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031792.1.11",
          "form": "로제는 엽떡에서 먹었는데 별로였어요",
          "original_form": "로제는 엽떡에서 먹었는데 별로였어요",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031792.1.12",
          "form": "아침으로 김치찌개 먹으러 왔습니당",
          "original_form": "아침으로 김치찌개 먹으러 왔습니당",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031792.1.13",
          "form": "아 엽떡 로제 별로였군요 ㅋㅋ",
          "original_form": "아 엽떡 로제 별로였군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031792.1.14",
          "form": "로제도 가게 마다 다르긴 하더라구요 ㅋ",
          "original_form": "로제도 가게 마다 다르긴 하더라구요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031792.1.15",
          "form": "오오 김치찌개 맛있져 ㅋㅋ",
          "original_form": "오오 김치찌개 맛있져 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031792.1.16",
          "form": "저는 후레이크 먹었어요 ㅋㅋ",
          "original_form": "저는 후레이크 먹었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031792.1.17",
          "form": "오, 시리얼 드셨구나",
          "original_form": "오, 시리얼 드셨구나",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031792.1.18",
          "form": "네 간단하게 아침으로 먹었어요 ㅋ",
          "original_form": "네 간단하게 아침으로 먹었어요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031792.1.19",
          "form": "점심때 밥으로 먹으려구요 ㅋ",
          "original_form": "점심때 밥으로 먹으려구요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031792.1.20",
          "form": "혼자 드세요? 식구들이랑?",
          "original_form": "혼자 드세요? 식구들이랑?",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031792.1.21",
          "form": "가족들이랑 여행중이라 같이 먹어요ㅎㅎ",
          "original_form": "가족들이랑 여행중이라 같이 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031792.1.22",
          "form": "아 여행중이시군요 ㅋ",
          "original_form": "아 여행중이시군요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031792.1.23",
          "form": "여행이면은 맛있는거 많이 드시겠네요 ㅋㅋ",
          "original_form": "여행이면은 맛있는거 많이 드시겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031792.1.24",
          "form": "네네ㅎㅎ 매번 다른 메뉴를 먹는 재미가",
          "original_form": "네네ㅎㅎ 매번 다른 메뉴를 먹는 재미가",
          "speaker_id": "2",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031792.1.25",
          "form": "맞아요.ㅋㅋ좋네요ㅋㅋ",
          "original_form": "맞아요.ㅋㅋ좋네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:34"
        }
      ]
