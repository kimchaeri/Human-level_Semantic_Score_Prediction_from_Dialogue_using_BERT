      "utterance": [
        {
          "id": "MDRW2100039486.1.1",
          "form": "밥 반공기에 김 싸먹어요 ㅎ.ㄹ",
          "original_form": "밥 반공기에 김 싸먹어요 ㅎ.ㄹ",
          "speaker_id": "1",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039486.1.2",
          "form": "헐 요거트랑 복숭아 좋은거 같아요",
          "original_form": "헐 요거트랑 복숭아 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039486.1.3",
          "form": "그 약간 sns에서 유행하는",
          "original_form": "그 약간 sns에서 유행하는",
          "speaker_id": "2",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039486.1.4",
          "form": "복숭아 안에 씨 파내고",
          "original_form": "복숭아 안에 씨 파내고",
          "speaker_id": "2",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039486.1.5",
          "form": "안에 요거트 채워넣는고 아세요??",
          "original_form": "안에 요거트 채워넣는고 아세요??",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039486.1.6",
          "form": "저 그거 해보려다가... 애먼 복숭아만 조각내버려서",
          "original_form": "저 그거 해보려다가... 애먼 복숭아만 조각내버려서",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039486.1.7",
          "form": "그냥.. 조각내서 요거트에 넣어 먹어요...",
          "original_form": "그냥.. 조각내서 요거트에 넣어 먹어요...",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039486.1.8",
          "form": "name2님 해보셨어요?!",
          "original_form": "&name2&님 해보셨어요?!",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039486.1.9",
          "form": "앗 이런..",
          "original_form": "앗 이런..",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039486.1.10",
          "form": "전 아직 도전조차 해보지 못했습니다..",
          "original_form": "전 아직 도전조차 해보지 못했습니다..",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039486.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ오늘 한번 해볼가했어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오늘 한번 해볼가했어요",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039486.1.12",
          "form": "방금 요거트랑 복숭아 먹으라는 말 보구..!!",
          "original_form": "방금 요거트랑 복숭아 먹으라는 말 보구..!!",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039486.1.13",
          "form": "헛 손 조심하셔요 저 칼로 빼다가",
          "original_form": "헛 손 조심하셔요 저 칼로 빼다가",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039486.1.14",
          "form": "아찔한 광경을 볼 뻔했다구요....!",
          "original_form": "아찔한 광경을 볼 뻔했다구요....!",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039486.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗 알겠습니다 진정한 꿀팁이네요..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗 알겠습니다 진정한 꿀팁이네요..",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039486.1.16",
          "form": "벌써 무서워지는 이유는.. 뭘까요.. 흐흑..",
          "original_form": "벌써 무서워지는 이유는.. 뭘까요.. 흐흑..",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039486.1.17",
          "form": "그냥 잘라서 요거트랑..ㅁ",
          "original_form": "그냥 잘라서 요거트랑..ㅁ",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039486.1.18",
          "form": "먹는게 나을까요..?",
          "original_form": "먹는게 나을까요..?",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039486.1.19",
          "form": "아녜요 name2님은 성공할지도 몰라여!!!!!!!",
          "original_form": "아녜요 &name2&님은 성공할지도 몰라여!!!!!!!",
          "speaker_id": "1",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100039486.1.20",
          "form": "한 번 도전은 해보셔요",
          "original_form": "한 번 도전은 해보셔요",
          "speaker_id": "1",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100039486.1.21",
          "form": "무서우면 수저로도 팔 수 있을 걸요?",
          "original_form": "무서우면 수저로도 팔 수 있을 걸요?",
          "speaker_id": "1",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100039486.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ복숭아의 반을 잘라서 파내는거죠..?",
