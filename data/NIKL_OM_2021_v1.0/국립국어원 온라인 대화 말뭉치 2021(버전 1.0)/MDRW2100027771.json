      "utterance": [
        {
          "id": "MDRW2100027771.1.1",
          "form": "난 녹차라떼가 좋아",
          "original_form": "난 녹차라떼가 좋아",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027771.1.2",
          "form": "근데 요즘은 복숭아 아이스티가 좋더라",
          "original_form": "근데 요즘은 복숭아 아이스티가 좋더라",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027771.1.3",
          "form": "복숭아 아이스티에 오미자 섞어서 마시면 정말 맛있더라",
          "original_form": "복숭아 아이스티에 오미자 섞어서 마시면 정말 맛있더라",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027771.1.4",
          "form": "그거 저번에 준다면서 안줬어",
          "original_form": "그거 저번에 준다면서 안줬어",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027771.1.5",
          "form": "나 너네집에 놀러간 이유가 그건데 안줬따고",
          "original_form": "나 너네집에 놀러간 이유가 그건데 안줬따고",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027771.1.6",
          "form": "내가준다고했는데",
          "original_form": "내가준다고했는데",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027771.1.7",
          "form": "니가 안마신다며",
          "original_form": "니가 안마신다며",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027771.1.8",
          "form": "내가 두번이나 물어봄",
          "original_form": "내가 두번이나 물어봄",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027771.1.9",
          "form": "그냥 아이스티라면서요ㅠㅠ",
          "original_form": "그냥 아이스티라면서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027771.1.10",
          "form": "아놔 우리집에 아이스티 마시러 올거면 당연히 그거지",
          "original_form": "아놔 우리집에 아이스티 마시러 올거면 당연히 그거지",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027771.1.11",
          "form": "슬프네 근데 그때 너무 졸려서 내가 무슨 말을 하고 살았는지 기억이 안나",
          "original_form": "슬프네 근데 그때 너무 졸려서 내가 무슨 말을 하고 살았는지 기억이 안나",
          "speaker_id": "1",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027771.1.12",
          "form": "아이스티 거절한 기억도 없어",
          "original_form": "아이스티 거절한 기억도 없어",
          "speaker_id": "1",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027771.1.13",
          "form": "이녀석...",
          "original_form": "이녀석...",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027771.1.14",
          "form": "난근데 진짜 물어봤단다",
          "original_form": "난근데 진짜 물어봤단다",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027771.1.15",
          "form": "암튼 너는 요즘 좋아하는 음료가 뭐니",
          "original_form": "암튼 너는 요즘 좋아하는 음료가 뭐니",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027771.1.16",
          "form": "내 아이스티....오미자",
          "original_form": "내 아이스티....오미자",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027771.1.17",
          "form": "나도 말차라떼 좋아 이제 카페인 먹을 수 있어",
          "original_form": "나도 말차라떼 좋아 이제 카페인 먹을 수 있어",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027771.1.18",
          "form": "요즘 카페는 녹차라떼라 안하고 말차라떼라 하더라",
          "original_form": "요즘 카페는 녹차라떼라 안하고 말차라떼라 하더라",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027771.1.19",
          "form": "녹차랑 말차랑 달라서 그래",
          "original_form": "녹차랑 말차랑 달라서 그래",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027771.1.20",
          "form": "그래? 뭐가 달라?",
          "original_form": "그래? 뭐가 달라?",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027771.1.21",
          "form": "엄밀히 따지자면 말차라떼가 맞는 말이긴 해",
          "original_form": "엄밀히 따지자면 말차라떼가 맞는 말이긴 해",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027771.1.22",
          "form": "녹차는 말그대로 티야",
          "original_form": "녹차는 말그대로 티야",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027771.1.23",
          "form": "왜왜?",
          "original_form": "왜왜?",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027771.1.24",
          "form": "녹차잎으로 차 우려 먹는게 녹차고",
          "original_form": "녹차잎으로 차 우려 먹는게 녹차고",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027771.1.25",
          "form": "말차는 녹차가루 같은거에 물 조금 넣고",
          "original_form": "말차는 녹차가루 같은거에 물 조금 넣고",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027771.1.26",
          "form": "그 일본식 발인가 그거로 열심히 휚휘끼",
          "original_form": "그 일본식 발인가 그거로 열심히 휚휘끼",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027771.1.27",
          "form": "저어서 만든게",
          "original_form": "저어서 만든게",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027771.1.28",
          "form": "말차여",
          "original_form": "말차여",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027771.1.29",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027771.1.30",
          "form": "이해했어",
          "original_form": "이해했어",
