      "utterance": [
        {
          "id": "MDRW2100002525.1.1",
          "form": "너 요즘 학교생활 어뗘??",
          "original_form": "너 요즘 학교생활 어뗘??",
          "speaker_id": "1",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100002525.1.2",
          "form": "수시 잘 챙기는중이야?",
          "original_form": "수시 잘 챙기는중이야?",
          "speaker_id": "1",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100002525.1.3",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100002525.1.4",
          "form": "나는 2점대여",
          "original_form": "나는 2점대여",
          "speaker_id": "2",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100002525.1.5",
          "form": "아마도 여대갈듯",
          "original_form": "아마도 여대갈듯",
          "speaker_id": "2",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100002525.1.6",
          "form": "와우..",
          "original_form": "와우..",
          "speaker_id": "1",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100002525.1.7",
          "form": "꿈의곳이구만",
          "original_form": "꿈의곳이구만",
          "speaker_id": "1",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100002525.1.8",
          "form": "학교에서 애들하곤 어때?",
          "original_form": "학교에서 애들하곤 어때?",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.9",
          "form": "머 고3인데",
          "original_form": "머 고3인데",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.10",
          "form": "똑같지 평소랑.ㅡㅡ",
          "original_form": "똑같지 평소랑.ㅡㅡ",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.11",
          "form": "그래도 나름 재밌어",
          "original_form": "그래도 나름 재밌어",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.12",
          "form": "언니야는",
          "original_form": "언니야는",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.13",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.14",
          "form": "대학 비대면이라서",
          "original_form": "대학 비대면이라서",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.15",
          "form": "선배도없고",
          "original_form": "선배도없고",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.16",
          "form": "동기도없다ㅜ",
          "original_form": "동기도없다ㅜ",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.17",
          "form": "우씨",
          "original_form": "우씨",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.18",
          "form": "ㅜㅠㅜㅠ",
          "original_form": "ㅜㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.19",
          "form": "내년에도 비대면일까...",
          "original_form": "내년에도 비대면일까...",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.20",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.21",
          "form": "내년은 먼가",
          "original_form": "내년은 먼가",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.22",
          "form": "백신맞아서",
          "original_form": "백신맞아서",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.23",
          "form": "비대면 아닐듯??",
          "original_form": "비대면 아닐듯??",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.24",
          "form": "오싸",
          "original_form": "오싸",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.25",
          "form": "그땐 오티하겠지??",
          "original_form": "그땐 오티하겠지??",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.26",
          "form": "큨큨",
          "original_form": "큨큨",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.27",
          "form": "칫",
          "original_form": "칫",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.28",
          "form": "나도 오티해줘",
          "original_form": "나도 오티해줘",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100002525.1.29",
          "form": "신제품 중고 21학번이라구",
          "original_form": "신제품 중고 21학번이라구",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.30",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.32",
          "form": "진짜웃기다",
          "original_form": "진짜웃기다",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.33",
          "form": "코로나..",
          "original_form": "코로나..",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.34",
          "form": "코로나때메 학교가 너무 변했어",
          "original_form": "코로나때메 학교가 너무 변했어",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.35",
          "form": "아그니깐...",
          "original_form": "아그니깐...",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.36",
          "form": "너무 많이변함",
          "original_form": "너무 많이변함",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.37",
          "form": "너네는 밥도 급식 다 따로먹구..",
          "original_form": "너네는 밥도 급식 다 따로먹구..",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.38",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.39",
          "form": "그니까네",
          "original_form": "그니까네",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.40",
          "form": "너무슬프다",
          "original_form": "너무슬프다",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.41",
          "form": "글고",
          "original_form": "글고",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.42",
          "form": "매점도없자나",
          "original_form": "매점도없자나",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.43",
          "form": "우리땐 다있었는디..",
          "original_form": "우리땐 다있었는디..",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100002525.1.44",
          "form": "아매점",
          "original_form": "아매점",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100002525.1.45",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100002525.1.46",
          "form": "나는...",
          "original_form": "나는...",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100002525.1.47",
          "form": "아침만 먹고 너무배고파",
          "original_form": "아침만 먹고 너무배고파",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100002525.1.48",
          "form": "나 쉬는시간마다",
          "original_form": "나 쉬는시간마다",
          "speaker_id": "1",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100002525.1.49",
          "form": "매점 가서 매점빵이랑",
          "original_form": "매점 가서 매점빵이랑",
          "speaker_id": "1",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100002525.1.50",
          "form": "음료수사서 10분만에 다머금",
          "original_form": "음료수사서 10분만에 다머금",
          "speaker_id": "1",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100002525.1.51",
          "form": "왘ㅋㅋㅋㅋㅋ",
          "original_form": "왘ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100002525.1.52",
          "form": "어케그렇게해",
          "original_form": "어케그렇게해",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100002525.1.53",
          "form": "머야 학교생활인줄 알았는데",
          "original_form": "머야 학교생활인줄 알았는데",
          "speaker_id": "1",
          "time": "20210820 23:36"
        }
      ]
