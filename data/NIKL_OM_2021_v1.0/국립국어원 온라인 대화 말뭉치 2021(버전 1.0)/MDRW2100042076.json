      "utterance": [
        {
          "id": "MDRW2100042076.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100042076.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100042076.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100042076.1.4",
          "form": "아뇨 전 좀 늦게 먹으려구요!",
          "original_form": "아뇨 전 좀 늦게 먹으려구요!",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042076.1.5",
          "form": "뭐 드실 건가요?",
          "original_form": "뭐 드실 건가요?",
          "speaker_id": "1",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042076.1.6",
          "form": "카레 먹을것 같아요~",
          "original_form": "카레 먹을것 같아요~",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042076.1.7",
          "form": "카레 좋아하시나요?",
          "original_form": "카레 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042076.1.8",
          "form": "네 좋아해요 매콤해서 맛있어요",
          "original_form": "네 좋아해요 매콤해서 맛있어요",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042076.1.9",
          "form": "매콤한 카레 좋아하시는구나",
          "original_form": "매콤한 카레 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100042076.1.10",
          "form": "매운거 잘드시나봥6",
          "original_form": "매운거 잘드시나봥6",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100042076.1.11",
          "form": "잘드시나봐요",
          "original_form": "잘드시나봐요",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100042076.1.12",
          "form": "좋아하는 편이에요 그래서 떡볶이도 좋아해요",
          "original_form": "좋아하는 편이에요 그래서 떡볶이도 좋아해요",
          "speaker_id": "1",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100042076.1.13",
          "form": "떡볶이 맛있죠",
          "original_form": "떡볶이 맛있죠",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042076.1.14",
          "form": "전 요즘 로제떡볶이가 좋더라구요",
          "original_form": "전 요즘 로제떡볶이가 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042076.1.15",
          "form": "저도요! 어떤 브랜드 거 드셔보셨어요?",
          "original_form": "저도요! 어떤 브랜드 거 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042076.1.16",
          "form": "제가 매운걸 잘 못먹어서 개인적으로 걸작떡볶이가 맛있었어요~",
          "original_form": "제가 매운걸 잘 못먹어서 개인적으로 걸작떡볶이가 맛있었어요~",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042076.1.17",
          "form": "걸작은 안 먹어봤네요 걸작떡볶이는 안 매운 편이라고 들었던 것 같아요",
          "original_form": "걸작은 안 먹어봤네요 걸작떡볶이는 안 매운 편이라고 들었던 것 같아요",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042076.1.18",
          "form": "맞아요 엽떡보단 당연히 안매운데 전 불닭도 못먹어서 걸작도 맵더라구요..ㅠㅠ",
          "original_form": "맞아요 엽떡보단 당연히 안매운데 전 불닭도 못먹어서 걸작도 맵더라구요..ㅠㅠ",
