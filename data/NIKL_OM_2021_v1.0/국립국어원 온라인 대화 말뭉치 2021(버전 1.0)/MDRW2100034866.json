      "utterance": [
        {
          "id": "MDRW2100034866.1.1",
          "form": "아ㅋㅋㅋ 진짜 엄청 밀더라구요",
          "original_form": "아ㅋㅋㅋ 진짜 엄청 밀더라구요",
          "speaker_id": "2",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100034866.1.2",
          "form": "저 머리에 카메라 올려본건 그때 처음 경험한거였어요",
          "original_form": "저 머리에 카메라 올려본건 그때 처음 경험한거였어요",
          "speaker_id": "2",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100034866.1.3",
          "form": "아 ㅋㄱㅋㅋㅋ 맞아욬ㅋㅋㅋ",
          "original_form": "아 ㅋㄱㅋㅋㅋ 맞아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034866.1.4",
          "form": "전 제 앞사람이 홈마였는데 입이 진짜 험해서",
          "original_form": "전 제 앞사람이 홈마였는데 입이 진짜 험해서",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034866.1.5",
          "form": "신고하고싶었어요ㅋㅋㅋ",
          "original_form": "신고하고싶었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034866.1.6",
          "form": "엄청났나보네요",
          "original_form": "엄청났나보네요",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100034866.1.7",
          "form": "상상초월하는분들 많았어요",
          "original_form": "상상초월하는분들 많았어요",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100034866.1.8",
          "form": "맞죠 .. 그 이후로 다신 안가요",
          "original_form": "맞죠 .. 그 이후로 다신 안가요",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034866.1.9",
          "form": "가도 좌석만 갔어요 ㅋㅋㅋ",
          "original_form": "가도 좌석만 갔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034866.1.10",
          "form": "현명하십니다ㅋㅋㅋ",
          "original_form": "현명하십니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034866.1.11",
          "form": "저도 그 이후로 서있는거 싫더라구요",
          "original_form": "저도 그 이후로 서있는거 싫더라구요",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034866.1.12",
          "form": "화장실도 못가고 ㅋㅋ 욕만 듣고 ㅠㅠ",
          "original_form": "화장실도 못가고 ㅋㅋ 욕만 듣고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034866.1.13",
          "form": "외국인도 많았었어요",
          "original_form": "외국인도 많았었어요",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034866.1.14",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100034866.1.15",
          "form": "숨도 못쉬겠더라구요",
          "original_form": "숨도 못쉬겠더라구요",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100034866.1.16",
          "form": "하도 밀어서",
          "original_form": "하도 밀어서",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100034866.1.17",
          "form": "맞아요 전 혹시나해서 그때 마스크 끼고 갔어요 ㅋㅋ",
          "original_form": "맞아요 전 혹시나해서 그때 마스크 끼고 갔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034866.1.18",
          "form": "정수리 냄새 난다고해섴ㅋㅋ",
          "original_form": "정수리 냄새 난다고해섴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034866.1.19",
          "form": "ㅋㅋㅋㅋ정보만이 살길이네요ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ정보만이 살길이네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034866.1.20",
          "form": "왠지 친구가 굽 좀 있는거 신는다더라구요 이유가 있는거였네요",
          "original_form": "왠지 친구가 굽 좀 있는거 신는다더라구요 이유가 있는거였네요",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034866.1.21",
          "form": "와 근데 굽 있는거 신으면",
          "original_form": "와 근데 굽 있는거 신으면",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100034866.1.22",
          "form": "발목 나가겠던데요 ㅠㅠ",
          "original_form": "발목 나가겠던데요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100034866.1.23",
          "form": "맞아요.. 밀치면 뚝 할거같았어요ㅠ",
          "original_form": "맞아요.. 밀치면 뚝 할거같았어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100034866.1.24",
          "form": "맞아요 ,, 아마 앞으론 당분간 좌석이지 않을까요 ㅎㅎ",
          "original_form": "맞아요 ,, 아마 앞으론 당분간 좌석이지 않을까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100034866.1.25",
          "form": "시상식을 거의 온라인으로 하지만 ㅜㅜ",
          "original_form": "시상식을 거의 온라인으로 하지만 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100034866.1.26",
          "form": "하긴 그렇겠네요ㅠ",
          "original_form": "하긴 그렇겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 22:02"
        }
      ]
