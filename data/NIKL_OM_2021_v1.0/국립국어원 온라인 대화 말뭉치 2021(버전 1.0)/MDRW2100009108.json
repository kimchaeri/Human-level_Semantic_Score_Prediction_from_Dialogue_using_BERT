      "utterance": [
        {
          "id": "MDRW2100009108.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009108.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009108.1.3",
          "form": "자빱티비 보시나요?",
          "original_form": "자빱티비 보시나요?",
          "speaker_id": "1",
          "time": "20210822 21:23"
        },
        {
          "id": "MDRW2100009108.1.4",
          "form": "네 자빱티비 봐요!",
          "original_form": "네 자빱티비 봐요!",
          "speaker_id": "2",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009108.1.5",
          "form": "콘님 레어닉 차지하셨네요ㅋㅌㅌ",
          "original_form": "콘님 레어닉 차지하셨네요ㅋㅌㅌ",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009108.1.6",
          "form": "저 여기서 콘님들 두 분 만났어요",
          "original_form": "저 여기서 콘님들 두 분 만났어요",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009108.1.7",
          "form": "네ㅋㅋㅋㅋㅋㅋ바로~ 이 닉네임 했어요",
          "original_form": "네ㅋㅋㅋㅋㅋㅋ바로~ 이 닉네임 했어요",
          "speaker_id": "2",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009108.1.8",
          "form": "콘님들이랑 우정리나 양잠시로 얘기하니까 재미있더라구요",
          "original_form": "콘님들이랑 우정리나 양잠시로 얘기하니까 재미있더라구요",
          "speaker_id": "2",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009108.1.9",
          "form": "ㅋㅋㅋㅋ 콘님들 많이 만나보셨겠네요",
          "original_form": "ㅋㅋㅋㅋ 콘님들 많이 만나보셨겠네요",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009108.1.10",
          "form": "제일 좋아하시는 콘텐츠가 뭐예요?",
          "original_form": "제일 좋아하시는 콘텐츠가 뭐예요?",
          "speaker_id": "1",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009108.1.11",
          "form": "아악 너무너무 어렵지만 우정리1이요!",
          "original_form": "아악 너무너무 어렵지만 우정리1이요!",
          "speaker_id": "2",
          "time": "20210823 10:14"
        },
        {
          "id": "MDRW2100009108.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 10:14"
        },
        {
          "id": "MDRW2100009108.1.13",
          "form": "저는 서리사막이요",
          "original_form": "저는 서리사막이요",
          "speaker_id": "1",
          "time": "20210823 10:16"
        },
        {
          "id": "MDRW2100009108.1.14",
          "form": "진짜 영화같은 스토리와 퀄리티라 좋아요",
          "original_form": "진짜 영화같은 스토리와 퀄리티라 좋아요",
          "speaker_id": "1",
          "time": "20210823 10:16"
        },
        {
          "id": "MDRW2100009108.1.15",
          "form": "맞아요! 제 주변에도 서리사막의 도둑들로 유입된 친구들 많아요",
          "original_form": "맞아요! 제 주변에도 서리사막의 도둑들로 유입된 친구들 많아요",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009108.1.16",
          "form": "도롱아.....ㅜㅜ..",
          "original_form": "도롱아.....ㅜㅜ..",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009108.1.17",
          "form": "도롱이... 그럴리가없는데:]",
          "original_form": "도롱이... 그럴리가없는데:]",
          "speaker_id": "1",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009108.1.18",
          "form": "요즘 우정리2보고 있으세요?",
          "original_form": "요즘 우정리2보고 있으세요?",
          "speaker_id": "1",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009108.1.19",
          "form": "그럼요~ 우정리노트2도 재미있게 보고 있어요",
          "original_form": "그럼요~ 우정리노트2도 재미있게 보고 있어요",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100009108.1.20",
          "form": "저는 생방도 챙겨보는 편이여서 지금은 세븐데이즈..이거 다시 보고 있어요ㅋㅋㅋ",
          "original_form": "저는 생방도 챙겨보는 편이여서 지금은 세븐데이즈..이거 다시 보고 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100009108.1.21",
          "form": "저는 그거 아직 안 봐서 나중에 보려고요!",
          "original_form": "저는 그거 아직 안 봐서 나중에 보려고요!",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100009108.1.22",
          "form": "우정리2는 어떻게 흘러갈지 모르겠어요 그 토깽이그림자가 또 무슨 짓을 벌일지....",
          "original_form": "우정리2는 어떻게 흘러갈지 모르겠어요 그 토깽이그림자가 또 무슨 짓을 벌일지....",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100009108.1.23",
          "form": "세븐데이즈 너무 쟈미있었어요ㅋㅋㅋㅋㅋ나중에 꼭 보세요",
          "original_form": "세븐데이즈 너무 쟈미있었어요ㅋㅋㅋㅋㅋ나중에 꼭 보세요",
          "speaker_id": "2",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100009108.1.24",
          "form": "아 그쵸! 저 친구들이랑 분명 그림자가 다른 주민들 속에 들어간것갘다고",
          "original_form": "아 그쵸! 저 친구들이랑 분명 그림자가 다른 주민들 속에 들어간것갘다고",
          "speaker_id": "2",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100009108.1.25",
          "form": "엄청 초반부터 말했는데 다 돌멩이 취급 당했었거든요ㅠㅠ..",
          "original_form": "엄청 초반부터 말했는데 다 돌멩이 취급 당했었거든요ㅠㅠ..",
          "speaker_id": "2",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100009108.1.26",
          "form": "그래도 이번에 라디오나오면서 약간 다이아의 가능성이..",
          "original_form": "그래도 이번에 라디오나오면서 약간 다이아의 가능성이..",
          "speaker_id": "2",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100009108.1.27",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "1",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100009108.1.28",
          "form": "근데 전 곡길댁과 가맥집 사장님의 그것을 보고나서는... 그 생각밖에 안 떠올라요ㅋㅋㅋ",
          "original_form": "근데 전 곡길댁과 가맥집 사장님의 그것을 보고나서는... 그 생각밖에 안 떠올라요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100009108.1.29",
          "form": "충격적인 장면이었어요",
          "original_form": "충격적인 장면이었어요",
          "speaker_id": "1",
          "time": "20210823 17:52"
        },
        {
          "id": "MDRW2100009108.1.30",
          "form": "아 ㅁㅈ어요..둘이 계속 다니는거보니꺼",
          "original_form": "아 ㅁㅈ어요..둘이 계속 다니는거보니꺼",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100009108.1.31",
          "form": "뭔가 있는거 같긴했는대",
          "original_form": "뭔가 있는거 같긴했는대",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100009108.1.32",
          "form": "love일줄이야...",
          "original_form": "love일줄이야...",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100009108.1.33",
          "form": "심지어 커플티까지 입다니....",
          "original_form": "심지어 커플티까지 입다니....",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009108.1.34",
          "form": "이번 콘텐츠는 우정리1때 캐릭터 서사랑 비교할 수 있어서 더 재밌어요",
          "original_form": "이번 콘텐츠는 우정리1때 캐릭터 서사랑 비교할 수 있어서 더 재밌어요",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009108.1.35",
          "form": "복수댁이랑 대지댁 달라진 건 넘 슬프지만ㅠ",
          "original_form": "복수댁이랑 대지댁 달라진 건 넘 슬프지만ㅠ",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009108.1.36",
          "form": "아 맞아요ㅋㄱㅋㄱㅋㄱㅋㄹㅋ",
          "original_form": "아 맞아요ㅋㄱㅋㄱㅋㄱㅋㄹㅋ",
          "speaker_id": "2",
          "time": "20210824 10:19"
        },
        {
          "id": "MDRW2100009108.1.37",
          "form": "매번 복수야~...너 할머니 은혜를 모르고~..",
          "original_form": "매번 복수야~...너 할머니 은혜를 모르고~..",
          "speaker_id": "2",
          "time": "20210824 10:19"
        },
        {
          "id": "MDRW2100009108.1.38",
          "form": "이럼서 봐요ㅠㅠ..",
          "original_form": "이럼서 봐요ㅠㅠ..",
          "speaker_id": "2",
          "time": "20210824 10:20"
        },
        {
          "id": "MDRW2100009108.1.39",
          "form": "전 왕눈댁의 속마음이 넘 궁금해요",
          "original_form": "전 왕눈댁의 속마음이 넘 궁금해요",
          "speaker_id": "2",
          "time": "20210824 10:20"
        },
        {
          "id": "MDRW2100009108.1.40",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:34"
        },
        {
          "id": "MDRW2100009108.1.41",
          "form": "그러게요 왕눈댁 대체 뭘 묻은거야...??",
          "original_form": "그러게요 왕눈댁 대체 뭘 묻은거야...??",
          "speaker_id": "1",
          "time": "20210824 10:34"
        },
        {
          "id": "MDRW2100009108.1.42",
          "form": "제발 고추장 장독대였으면 좋겠어요",
          "original_form": "제발 고추장 장독대였으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 10:34"
        },
        {
          "id": "MDRW2100009108.1.43",
          "form": "아ㅋㄱㅋㄹㅋㄱㅋㄱㅋㄹㅋㅋ그러게요ㅋㅅㅋㅅㅋ고추장이나 김치ㅠㅠ...",
          "original_form": "아ㅋㄱㅋㄹㅋㄱㅋㄱㅋㄹㅋㅋ그러게요ㅋㅅㅋㅅㅋ고추장이나 김치ㅠㅠ...",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100009108.1.44",
          "form": "빱님 방금 올라온 영상 보셨나요?",
          "original_form": "빱님 방금 올라온 영상 보셨나요?",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100009108.1.45",
          "form": "마지막 멘트 너무 웃겨요ㅋㅋㅋ",
          "original_form": "마지막 멘트 너무 웃겨요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100009108.1.46",
          "form": "아 ㅂ봤어요 ㅣㅋㄱㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋㅋㄲㅋ",
          "original_form": "아 ㅂ봤어요 ㅣㅋㄱㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋㅋㄲㅋ",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100009108.1.47",
          "form": "스타듀밸리는 저렇게 빱이의 잔잔한 토크가",
          "original_form": "스타듀밸리는 저렇게 빱이의 잔잔한 토크가",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100009108.1.48",
          "form": "넘 매력적이에요",
          "original_form": "넘 매력적이에요",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100009108.1.49",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 잔잔하고 불건전한 토크",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 잔잔하고 불건전한 토크",
