      "utterance": [
        {
          "id": "MDRW2100016759.1.1",
          "form": "우리 그럼 내일 전시회 갔다가",
          "original_form": "우리 그럼 내일 전시회 갔다가",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016759.1.2",
          "form": "맛있는거 먹자",
          "original_form": "맛있는거 먹자",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016759.1.3",
          "form": "는 나야나",
          "original_form": "는 나야나",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016759.1.4",
          "form": "답장 해 줘",
          "original_form": "답장 해 줘",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016759.1.5",
          "form": "나라구",
          "original_form": "나라구",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016759.1.6",
          "form": "날 봐줘",
          "original_form": "날 봐줘",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016759.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016759.1.8",
          "form": "조와 머머글가",
          "original_form": "조와 머머글가",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016759.1.9",
          "form": "그 근방에 맛난거",
          "original_form": "그 근방에 맛난거",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016759.1.10",
          "form": "잇나",
          "original_form": "잇나",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016759.1.11",
          "form": "삼성역",
          "original_form": "삼성역",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016759.1.12",
          "form": "근처 검색해봐야지",
          "original_form": "근처 검색해봐야지",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016759.1.13",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016759.1.14",
          "form": "내일 너 점심 안먹고 오는거?",
          "original_form": "내일 너 점심 안먹고 오는거?",
          "speaker_id": "1",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016759.1.15",
          "form": "아 향수뿌렸는데 너무 씨게 나네",
          "original_form": "아 향수뿌렸는데 너무 씨게 나네",
          "speaker_id": "1",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016759.1.16",
          "form": "물어봣는데",
          "original_form": "물어봣는데",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016759.1.17",
          "form": "답장이 엄네",
          "original_form": "답장이 엄네",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016759.1.18",
          "form": "힝구",
          "original_form": "힝구",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016759.1.19",
          "form": "바쁘신가",
          "original_form": "바쁘신가",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016759.1.20",
          "form": "거기 예쁜 거 파나",
          "original_form": "거기 예쁜 거 파나",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016759.1.21",
          "form": "흐음",
          "original_form": "흐음",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016759.1.22",
          "form": "예쁜거..?",
          "original_form": "예쁜거..?",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100016759.1.23",
          "form": "엽서 사고싶다",
          "original_form": "엽서 사고싶다",
          "speaker_id": "1",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100016759.1.24",
          "form": "지브리 엽서 있나",
          "original_form": "지브리 엽서 있나",
          "speaker_id": "1",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100016759.1.25",
          "form": "호 잇다고 본거같아",
          "original_form": "호 잇다고 본거같아",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016759.1.26",
          "form": "호오오오오오",
          "original_form": "호오오오오오",
          "speaker_id": "1",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016759.1.27",
          "form": "3장부터 구매가능하다고 들은듯",
          "original_form": "3장부터 구매가능하다고 들은듯",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016759.1.28",
          "form": "비싼가",
          "original_form": "비싼가",
          "speaker_id": "1",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016759.1.29",
          "form": "홍대가서 구경도 했던 거 ㄱㅏㅌ긴한데",
          "original_form": "홍대가서 구경도 했던 거 ㄱㅏㅌ긴한데",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016759.1.30",
          "form": "원령공주 맞나",
          "original_form": "원령공주 맞나",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016759.1.31",
          "form": "그거 보고싶음",
          "original_form": "그거 보고싶음",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016759.1.32",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016759.1.33",
          "form": "나도 이름만 들어보고",
          "original_form": "나도 이름만 들어보고",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016759.1.34",
          "form": "보려고 맨날 넷플에 추가해놓고 안봄",
          "original_form": "보려고 맨날 넷플에 추가해놓고 안봄",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016759.1.35",
          "form": "안봣으",
          "original_form": "안봣으",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016759.1.36",
          "form": "중딩때 학교에서 선생님이 틀어줬는데",
          "original_form": "중딩때 학교에서 선생님이 틀어줬는데",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016759.1.37",
          "form": "엄청 잔인했어",
          "original_form": "엄청 잔인했어",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016759.1.38",
          "form": "팔 뜯기고",
          "original_form": "팔 뜯기고",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016759.1.39",
          "form": "작품중에서 특히 되게 철학적인 느낌",
          "original_form": "작품중에서 특히 되게 철학적인 느낌",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016759.1.40",
          "form": "마자 글타고들음",
          "original_form": "마자 글타고들음",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016759.1.41",
          "form": "엽서 보면 삐삐쳐줘",
          "original_form": "엽서 보면 삐삐쳐줘",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016759.1.42",
          "form": "조왕~",
          "original_form": "조왕~",
          "speaker_id": "2",
          "time": "20210826 15:26"
        }
      ]
    }
  ]
