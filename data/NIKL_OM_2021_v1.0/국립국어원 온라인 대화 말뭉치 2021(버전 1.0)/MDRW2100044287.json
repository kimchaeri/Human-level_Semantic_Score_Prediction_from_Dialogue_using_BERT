      "utterance": [
        {
          "id": "MDRW2100044287.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044287.1.2",
          "form": "안녕하세요 거기 비오나요?",
          "original_form": "안녕하세요 거기 비오나요?",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044287.1.3",
          "form": "아뇨 쌀랑하기만해요",
          "original_form": "아뇨 쌀랑하기만해요",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044287.1.4",
          "form": "그곳은 비가왔나요?",
          "original_form": "그곳은 비가왔나요?",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044287.1.5",
          "form": "여기도 비 한방울씩 가끔오고",
          "original_form": "여기도 비 한방울씩 가끔오고",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044287.1.6",
          "form": "거의안와요",
          "original_form": "거의안와요",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044287.1.7",
          "form": "좋네요 진짜가을날씨같아요",
          "original_form": "좋네요 진짜가을날씨같아요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044287.1.8",
          "form": "가을이 확실히 좋은계절이네요",
          "original_form": "가을이 확실히 좋은계절이네요",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044287.1.9",
          "form": "자꾸 짧아져서슬퍼요",
          "original_form": "자꾸 짧아져서슬퍼요",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044287.1.10",
          "form": "겨울이 조금만천천히 왔음 싶어요",
          "original_form": "겨울이 조금만천천히 왔음 싶어요",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044287.1.11",
          "form": "네 겨울여름은 너무 힘들어요",
          "original_form": "네 겨울여름은 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044287.1.12",
          "form": "그니까요 너무너무 추워졌잖아요",
          "original_form": "그니까요 너무너무 추워졌잖아요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044287.1.13",
          "form": "name2님은 패딩파세요 코트파세요?",
          "original_form": "&name2&님은 패딩파세요 코트파세요?",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044287.1.14",
          "form": "저는 코트파였는데 나이드니 패딩이 너무좋네요",
          "original_form": "저는 코트파였는데 나이드니 패딩이 너무좋네요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044287.1.15",
          "form": "저도요 코트는 있는데 패딩에 손이가요",
          "original_form": "저도요 코트는 있는데 패딩에 손이가요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044287.1.16",
          "form": "추우니까 따뜻한거 최고더라구요",
          "original_form": "추우니까 따뜻한거 최고더라구요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044287.1.17",
          "form": "그쳐 조만간 겨울옷을 꺼내야겠어요",
          "original_form": "그쳐 조만간 겨울옷을 꺼내야겠어요",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044287.1.18",
          "form": "엄두는 안나지만요ㅋㅋ",
          "original_form": "엄두는 안나지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044287.1.19",
          "form": "아아 여름옷 가을옷 제대로 입어보지도 못했는데 겨울옷이라니ㅋㅋ",
          "original_form": "아아 여름옷 가을옷 제대로 입어보지도 못했는데 겨울옷이라니ㅋㅋ",
