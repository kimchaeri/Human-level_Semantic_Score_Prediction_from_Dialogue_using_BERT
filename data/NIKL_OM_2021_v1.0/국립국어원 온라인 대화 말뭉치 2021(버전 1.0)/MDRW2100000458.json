      "utterance": [
        {
          "id": "MDRW2100000458.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000458.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000458.1.3",
          "form": "저 지금 면접보고 오는 길이에요",
          "original_form": "저 지금 면접보고 오는 길이에요",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000458.1.4",
          "form": "아 정말요? 어디 면접 보고 오시는길이세요?",
          "original_form": "아 정말요? 어디 면접 보고 오시는길이세요?",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000458.1.5",
          "form": "찰떡같이 이야기할 수 있겠어요",
          "original_form": "찰떡같이 이야기할 수 있겠어요",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000458.1.6",
          "form": "오오! 합격이 보이나요?",
          "original_form": "오오! 합격이 보이나요?",
          "speaker_id": "1",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000458.1.7",
          "form": "잠깐 일할 곳이라 면접준비를 열심히 안 했는데",
          "original_form": "잠깐 일할 곳이라 면접준비를 열심히 안 했는데",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000458.1.8",
          "form": "그래서 한 대 맞은 느낌이에요",
          "original_form": "그래서 한 대 맞은 느낌이에요",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000458.1.9",
          "form": "땀만 뻘뻘 흘리다가 왔어요 하하",
          "original_form": "땀만 뻘뻘 흘리다가 왔어요 하하",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000458.1.10",
          "form": "앜ㅋㅋㅋㅋ 그럴 수 있죠ㅠㅠㅠ 잠깐 일 할 곳이니 맘 편하게 생각하면 좋을 것 같아요",
          "original_form": "앜ㅋㅋㅋㅋ 그럴 수 있죠ㅠㅠㅠ 잠깐 일 할 곳이니 맘 편하게 생각하면 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000458.1.11",
          "form": "열심히 대답하시는 분들 보니까 괜히 부끄러워지더라고요",
          "original_form": "열심히 대답하시는 분들 보니까 괜히 부끄러워지더라고요",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000458.1.12",
          "form": "다대다 면접이었거든요",
          "original_form": "다대다 면접이었거든요",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000458.1.13",
          "form": "헉 그래요? 그로면 압박이 장난아니였겠네요...",
          "original_form": "헉 그래요? 그로면 압박이 장난아니였겠네요...",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000458.1.14",
          "form": "조바심도 나고 그럴텐데...",
          "original_form": "조바심도 나고 그럴텐데...",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000458.1.15",
          "form": "반성하고 다음엔 더 열심히 준비하는 걸로 하하",
          "original_form": "반성하고 다음엔 더 열심히 준비하는 걸로 하하",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000458.1.16",
          "form": "지금 하시는 일 좋아하세요?",
          "original_form": "지금 하시는 일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000458.1.17",
          "form": "저는 고등학생이라서",
          "original_form": "저는 고등학생이라서",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000458.1.18",
          "form": "어머나 그러시구나",
          "original_form": "어머나 그러시구나",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000458.1.19",
          "form": "전공 생각하는 거 있으세요?",
          "original_form": "전공 생각하는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000458.1.20",
          "form": "경제학과 생각중이에요!",
          "original_form": "경제학과 생각중이에요!",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000458.1.21",
          "form": "아니면 화계 쪽우로 생각 하고있어요!",
          "original_form": "아니면 화계 쪽우로 생각 하고있어요!",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000458.1.22",
          "form": "혹시 시험볼 생각이 있어요?",
          "original_form": "혹시 시험볼 생각이 있어요?",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000458.1.23",
          "form": "회계사?",
          "original_form": "회계사?",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000458.1.24",
          "form": "네네 회계사 시험 볼 생각도 있죠!",
          "original_form": "네네 회계사 시험 볼 생각도 있죠!",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000458.1.25",
          "form": "아하 그렇구나",
          "original_form": "아하 그렇구나",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000458.1.26",
          "form": "저는 직장생활을 어느 정도해서 그런지",
          "original_form": "저는 직장생활을 어느 정도해서 그런지",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000458.1.27",
          "form": "기술이 최고라는 생각이ㅋㅋㅋㅋㅋㅋ",
          "original_form": "기술이 최고라는 생각이ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000458.1.28",
          "form": "앟ㅋㅋㅋㅋㅋㅋ 그쵸... 기술직도 돈 꽤 벌지 않아요?",
          "original_form": "앟ㅋㅋㅋㅋㅋㅋ 그쵸... 기술직도 돈 꽤 벌지 않아요?",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000458.1.29",
          "form": "뭐 직종에 따라 다르겠지만! 오래오래 하기에는 기술을 가지고 있는 게 좋을 것 같더라고요",
          "original_form": "뭐 직종에 따라 다르겠지만! 오래오래 하기에는 기술을 가지고 있는 게 좋을 것 같더라고요",
          "speaker_id": "2",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000458.1.30",
          "form": "저도 문과라 약간 후회? 하고 있어요",
          "original_form": "저도 문과라 약간 후회? 하고 있어요",
          "speaker_id": "2",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000458.1.31",
          "form": "아ㅠㅠㅠㅋㅋㅋ 문과시구나 저도 괜히 문과 왔나 이런 생각이 들기도 하고.. 상경계열 아니면 정말 뭘 할게 없다는 현실에 수긍하며 살고 있습니다..",
          "original_form": "아ㅠㅠㅠㅋㅋㅋ 문과시구나 저도 괜히 문과 왔나 이런 생각이 들기도 하고.. 상경계열 아니면 정말 뭘 할게 없다는 현실에 수긍하며 살고 있습니다..",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000458.1.32",
          "form": "저는 교육쪽인데",
          "original_form": "저는 교육쪽인데",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000458.1.33",
          "form": "정말 보람과 사명감을 가지고 일하지만",
          "original_form": "정말 보람과 사명감을 가지고 일하지만",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000458.1.34",
          "form": "야망을 가질 수는 없는 직종이라서",
          "original_form": "야망을 가질 수는 없는 직종이라서",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000458.1.35",
          "form": "이직생각하고 있어요",
          "original_form": "이직생각하고 있어요",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000458.1.36",
          "form": "아 정말요? 교육 계열이시구나",
          "original_form": "아 정말요? 교육 계열이시구나",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000458.1.37",
          "form": "아직 선택할 수 있는 폭이 너무 많은 나이니까",
          "original_form": "아직 선택할 수 있는 폭이 너무 많은 나이니까",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000458.1.38",
          "form": "아무쪼록 좋아하는 일을 찾으면 좋겠어요!",
          "original_form": "아무쪼록 좋아하는 일을 찾으면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000458.1.39",
          "form": "대학가면 또 시야도 넓어지고",
          "original_form": "대학가면 또 시야도 넓어지고",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000458.1.40",
          "form": "맞아요 여러가지 일을 찾아봐야죠!!",
          "original_form": "맞아요 여러가지 일을 찾아봐야죠!!",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000458.1.41",
          "form": "생각하지 못한 쪽으로 갈 수도 있으니까요",
          "original_form": "생각하지 못한 쪽으로 갈 수도 있으니까요",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000458.1.42",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210819 16:33"
        }
      ]
    }
