        },
        {
          "id": "MDRW2100039540.1.3",
          "form": "게임콘서트 같은데 가고싶어",
          "original_form": "게임콘서트 같은데 가고싶어",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039540.1.4",
          "form": "와 그건뭐야",
          "original_form": "와 그건뭐야",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.5",
          "form": "해외는 규모가 크니까 그런게 부러움 ㅠ",
          "original_form": "해외는 규모가 크니까 그런게 부러움 ㅠ",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.6",
          "form": "게임 ost 연주하는 콘서트",
          "original_form": "게임 ost 연주하는 콘서트",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.7",
          "form": "게임사에서 하는건줄",
          "original_form": "게임사에서 하는건줄",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.8",
          "form": "그 왜 신작공개같은거",
          "original_form": "그 왜 신작공개같은거",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.9",
          "form": "aaa급 게임들은 오케스트라 연주회도 하고 하니까",
          "original_form": "aaa급 게임들은 오케스트라 연주회도 하고 하니까",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.10",
          "form": "아 그건 게임쇼~",
          "original_form": "아 그건 게임쇼~",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.11",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.12",
          "form": "그니까 게임사 상관없이",
          "original_form": "그니까 게임사 상관없이",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.13",
          "form": "사적으로 여는거?",
          "original_form": "사적으로 여는거?",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039540.1.14",
          "form": "ost하니까",
          "original_form": "ost하니까",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100039540.1.15",
          "form": "게임 한중일 얘기하면서",
          "original_form": "게임 한중일 얘기하면서",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100039540.1.16",
          "form": "일본 캐릭성",
          "original_form": "일본 캐릭성",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100039540.1.17",
          "form": "중국 그래픽",
          "original_form": "중국 그래픽",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100039540.1.18",
          "form": "한국 오슷",
          "original_form": "한국 오슷",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100039540.1.19",
          "form": "잘만든다고ㅋㅋㅋ하지않았나",
          "original_form": "잘만든다고ㅋㅋㅋ하지않았나",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039540.1.20",
          "form": "ㄴㄴ 보통 그 게임사에서 해",
          "original_form": "ㄴㄴ 보통 그 게임사에서 해",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039540.1.21",
          "form": "aaa급쪽은",
          "original_form": "aaa급쪽은",
          "speaker_id": "1",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039540.1.22",
          "form": "특히 일본이나 이족은 아예 게임사 자체 오케스트라가 있어",
          "original_form": "특히 일본이나 이족은 아예 게임사 자체 오케스트라가 있어",
