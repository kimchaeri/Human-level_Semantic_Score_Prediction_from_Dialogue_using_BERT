      "utterance": [
        {
          "id": "MDRW2100005870.1.1",
          "form": "안녕하세요 ~~ 독서 자주 하세요 ?",
          "original_form": "안녕하세요 ~~ 독서 자주 하세요 ?",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100005870.1.2",
          "form": "독서 자주 하려고 노력하는데 읽다 만 게 대부분이에요",
          "original_form": "독서 자주 하려고 노력하는데 읽다 만 게 대부분이에요",
          "speaker_id": "2",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100005870.1.3",
          "form": "오 저도요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 저도요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100005870.1.4",
          "form": "어떤 어떤 책 읽으셨어요?",
          "original_form": "어떤 어떤 책 읽으셨어요?",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100005870.1.5",
          "form": "최근에 이북으로 프랑켄슈타인을 읽었는데",
          "original_form": "최근에 이북으로 프랑켄슈타인을 읽었는데",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005870.1.6",
          "form": "뭔가 재미는 있는데 읽다보면 졸려서",
          "original_form": "뭔가 재미는 있는데 읽다보면 졸려서",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005870.1.7",
          "form": "읽어주는 거로라도 내용을 들어보자하고 틀었는데 어느새 자고 있더라구요 ㅋㅋㄱㄱ",
          "original_form": "읽어주는 거로라도 내용을 들어보자하고 틀었는데 어느새 자고 있더라구요 ㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005870.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그거 완전 asmr아닌가요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그거 완전 asmr아닌가요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100005870.1.9",
          "form": "전 이북은 눈 아퍼서 못 읽겠더라고요",
          "original_form": "전 이북은 눈 아퍼서 못 읽겠더라고요",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100005870.1.10",
          "form": "프랑켄슈타인 어떤 내용인가요 ?",
          "original_form": "프랑켄슈타인 어떤 내용인가요 ?",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005870.1.11",
          "form": "맞아요 ㅋㅋㄱㄱㄱ 완전 잠이 솔솔",
          "original_form": "맞아요 ㅋㅋㄱㄱㄱ 완전 잠이 솔솔",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005870.1.12",
          "form": "제가 아는 프랑켄슈타인이 맞을까요 ????",
          "original_form": "제가 아는 프랑켄슈타인이 맞을까요 ????",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005870.1.13",
          "form": "네네 맞아요!!",
          "original_form": "네네 맞아요!!",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005870.1.14",
          "form": "근데 제가 이걸 대강의 내용만 알고 실제 소설은 읽어본 적이 없어서",
          "original_form": "근데 제가 이걸 대강의 내용만 알고 실제 소설은 읽어본 적이 없어서",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005870.1.15",
          "form": "읽어봤는데 생각보다 심오한 내용이더라구요",
          "original_form": "읽어봤는데 생각보다 심오한 내용이더라구요",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005870.1.16",
          "form": "오 ! 그렇군요",
          "original_form": "오 ! 그렇군요",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100005870.1.17",
          "form": "저도 읽어봐야겠네요 !!",
          "original_form": "저도 읽어봐야겠네요 !!",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100005870.1.18",
          "form": "또 다른 책 기억남으시는 거 있으신가요 ??",
          "original_form": "또 다른 책 기억남으시는 거 있으신가요 ??",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100005870.1.19",
          "form": "저번에 김금희 작가의 복자에게를 읽었는데",
          "original_form": "저번에 김금희 작가의 복자에게를 읽었는데",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100005870.1.20",
          "form": "그땐 제가 집중력이 있었는지 그 책이 워낙 재밌던건지 모르겠지만",
          "original_form": "그땐 제가 집중력이 있었는지 그 책이 워낙 재밌던건지 모르겠지만",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100005870.1.21",
          "form": "하루만에 다읽었어요",
          "original_form": "하루만에 다읽었어요",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100005870.1.22",
          "form": "오 !!!",
          "original_form": "오 !!!",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100005870.1.23",
          "form": "저도 읽어보고 싶네요",
          "original_form": "저도 읽어보고 싶네요",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100005870.1.24",
          "form": "웬지 조금 슬플 것 같아요",
          "original_form": "웬지 조금 슬플 것 같아요",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100005870.1.25",
          "form": "맞나요 ?",
          "original_form": "맞나요 ?",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100005870.1.26",
          "form": "맞아요 슬픈 내용이 담겨있긴 해요",
          "original_form": "맞아요 슬픈 내용이 담겨있긴 해요",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100005870.1.27",
          "form": "근데 소설 속 배경이 제주도라서 또 묘사가 예쁜 문장도 많아서 좋아요",
          "original_form": "근데 소설 속 배경이 제주도라서 또 묘사가 예쁜 문장도 많아서 좋아요",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100005870.1.28",
          "form": "오 너무 읽어보고 싶어요 진짜",
          "original_form": "오 너무 읽어보고 싶어요 진짜",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100005870.1.29",
          "form": "내일 바로 도서관 다녀와야겠어요 추천해주신 책 두개 다 읽어볼게요 !",
          "original_form": "내일 바로 도서관 다녀와야겠어요 추천해주신 책 두개 다 읽어볼게요 !",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100005870.1.30",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100005870.1.31",
          "form": "아니에요 ㅎㅎ 사실 책 많이 읽는 편은 아닌데 마침 생각나서 말씀드렸어요",
          "original_form": "아니에요 ㅎㅎ 사실 책 많이 읽는 편은 아닌데 마침 생각나서 말씀드렸어요",
          "speaker_id": "2",
          "time": "20210907 14:58"
        }
      ]
