      "utterance": [
        {
          "id": "MDRW2100011376.1.1",
          "form": "유투브에 애완동물먹방 많아요",
          "original_form": "유투브에 애완동물먹방 많아요",
          "speaker_id": "1",
          "time": "20210824 11:48"
        },
        {
          "id": "MDRW2100011376.1.2",
          "form": "수미님 유툽 구독머하세요",
          "original_form": "수미님 유툽 구독머하세요",
          "speaker_id": "1",
          "time": "20210824 12:22"
        },
        {
          "id": "MDRW2100011376.1.3",
          "form": "그건 밝히기가 좀 곤란하네요 ㅋㅋ죄송해요",
          "original_form": "그건 밝히기가 좀 곤란하네요 ㅋㅋ죄송해요",
          "speaker_id": "2",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011376.1.4",
          "form": "글쿤요 저는 에쎔타운 구독했는데 ㅋㅋ",
          "original_form": "글쿤요 저는 에쎔타운 구독했는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:24"
        },
        {
          "id": "MDRW2100011376.1.5",
          "form": "그게 반려동물 먹방이에요?",
          "original_form": "그게 반려동물 먹방이에요?",
          "speaker_id": "2",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100011376.1.6",
          "form": "첨들어봐요",
          "original_form": "첨들어봐요",
          "speaker_id": "2",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100011376.1.7",
          "form": "SM타운이요 저는 태민팬이라서요",
          "original_form": "SM타운이요 저는 태민팬이라서요",
          "speaker_id": "1",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100011376.1.8",
          "form": "댕댕이와 야옹이도 구독했어요",
          "original_form": "댕댕이와 야옹이도 구독했어요",
          "speaker_id": "1",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100011376.1.9",
          "form": "그러시군요 자주 들어가보시나요?",
          "original_form": "그러시군요 자주 들어가보시나요?",
          "speaker_id": "2",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100011376.1.10",
          "form": "먹는소리 틀어놓고 작업해요 ㅋㅋ챱챱먹어요",
          "original_form": "먹는소리 틀어놓고 작업해요 ㅋㅋ챱챱먹어요",
          "speaker_id": "1",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011376.1.11",
          "form": "아 라디오처럼 쓰시는건가봐요 무슨작업하세요?",
          "original_form": "아 라디오처럼 쓰시는건가봐요 무슨작업하세요?",
          "speaker_id": "2",
          "time": "20210824 12:30"
        },
        {
          "id": "MDRW2100011376.1.12",
          "form": "편집이용",
          "original_form": "편집이용",
          "speaker_id": "1",
          "time": "20210824 12:30"
        },
        {
          "id": "MDRW2100011376.1.13",
          "form": "수미님은요",
          "original_form": "수미님은요",
          "speaker_id": "1",
          "time": "20210824 12:31"
        },
        {
          "id": "MDRW2100011376.1.14",
          "form": "저는 영상 그냥 봐용",
          "original_form": "저는 영상 그냥 봐용",
          "speaker_id": "2",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100011376.1.15",
          "form": "글고 제가 영상편집할줄 아니까",
          "original_form": "글고 제가 영상편집할줄 아니까",
          "speaker_id": "2",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100011376.1.16",
          "form": "영상보면 어찌 편집했는지 눈에들어와서",
          "original_form": "영상보면 어찌 편집했는지 눈에들어와서",
          "speaker_id": "2",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100011376.1.17",
          "form": "보면 더 피곤해지더라구요",
          "original_form": "보면 더 피곤해지더라구요",
          "speaker_id": "2",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100011376.1.18",
          "form": "오오 직업병이군요ㅜㅜㅜㅜ",
          "original_form": "오오 직업병이군요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100011376.1.19",
          "form": "코로나시대에 잘나가는 직업이네요",
          "original_form": "코로나시대에 잘나가는 직업이네요",
          "speaker_id": "1",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100011376.1.20",
          "form": "네열심히만하면요",
          "original_form": "네열심히만하면요",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011376.1.21",
          "form": "해보진 않았는데 편집잘하면 편집해주는걸로",
          "original_form": "해보진 않았는데 편집잘하면 편집해주는걸로",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011376.1.22",
          "form": "돈벌수있을거같긴 해요",
          "original_form": "돈벌수있을거같긴 해요",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011376.1.23",
          "form": "여기저기 수요가 많더라구요",
          "original_form": "여기저기 수요가 많더라구요",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011376.1.24",
          "form": "프리랜서 사이트에서 구하나요",
          "original_form": "프리랜서 사이트에서 구하나요",
          "speaker_id": "2",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011376.1.25",
          "form": "저도 그런일좀해서 돈벌고싶네요",
          "original_form": "저도 그런일좀해서 돈벌고싶네요",
          "speaker_id": "2",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011376.1.26",
          "form": "저는 지인들 통해서 결혼영상 촬영편집해줬어요",
          "original_form": "저는 지인들 통해서 결혼영상 촬영편집해줬어요",
          "speaker_id": "1",
          "time": "20210824 12:38"
        }
      ]
    }
