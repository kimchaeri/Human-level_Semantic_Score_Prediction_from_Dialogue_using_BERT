      "utterance": [
        {
          "id": "MDRW2100043059.1.1",
          "form": "마셔보셨어요?",
          "original_form": "마셔보셨어요?",
          "speaker_id": "2",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100043059.1.2",
          "form": "메로나는 참외맛이라던데 사실 ㅋㅋㅋ",
          "original_form": "메로나는 참외맛이라던데 사실 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100043059.1.3",
          "form": "한국인 입맛에 메론보다 참외가 맞아서 그렇대요",
          "original_form": "한국인 입맛에 메론보다 참외가 맞아서 그렇대요",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043059.1.4",
          "form": "앞뒤 말이 안맞으시는데 ㅋㅋㅋㅋ",
          "original_form": "앞뒤 말이 안맞으시는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043059.1.5",
          "form": "망고맛은 망고맛 맞을거같긴 해요 ㅋㅋㅋ",
          "original_form": "망고맛은 망고맛 맞을거같긴 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043059.1.6",
          "form": "갈아만든 배 탱크보이 맛 나지 않아요?",
          "original_form": "갈아만든 배 탱크보이 맛 나지 않아요?",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043059.1.7",
          "form": "아침햇살 맛도 까먹었고...",
          "original_form": "아침햇살 맛도 까먹었고...",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043059.1.8",
          "form": "약간 달달한 쌀뜨물 맛이었던 건 기억나요 ㅋㅋ",
          "original_form": "약간 달달한 쌀뜨물 맛이었던 건 기억나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043059.1.9",
          "form": "그래갖구 시골갈때 먹었던거같아요",
          "original_form": "그래갖구 시골갈때 먹었던거같아요",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043059.1.10",
          "form": "식혜 맛있는데 밥알 거르고 먹는게 좋아요 ㅋㅋㅋ",
          "original_form": "식혜 맛있는데 밥알 거르고 먹는게 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043059.1.11",
          "form": "어차피 밥만가지구 만드는거 아니잖아요! 당이 주윈ㅇ료인거라고요",
          "original_form": "어차피 밥만가지구 만드는거 아니잖아요! 당이 주윈ㅇ료인거라고요",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100043059.1.12",
          "form": "엄마 식혜 맛있는데 ㅠㅠ 먹고싶네요",
          "original_form": "엄마 식혜 맛있는데 ㅠㅠ 먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 13:51"
        },
        {
          "id": "MDRW2100043059.1.13",
          "form": "식혜 그냥 사먹을까 싶기도 하고!! 비락식혜",
          "original_form": "식혜 그냥 사먹을까 싶기도 하고!! 비락식혜",
          "speaker_id": "2",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100043059.1.14",
          "form": "근데 어느세월에 거기까지 가요 ㅋㅋㅋ 멀다구",
          "original_form": "근데 어느세월에 거기까지 가요 ㅋㅋㅋ 멀다구",
          "speaker_id": "2",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043059.1.15",
          "form": "됐어요 ㅋㅋㅋ 집에 스티로폼 박스도 없으면서",
          "original_form": "됐어요 ㅋㅋㅋ 집에 스티로폼 박스도 없으면서",
          "speaker_id": "2",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043059.1.16",
          "form": "단열재 안넣으면 여름엔 녹을걸요",
          "original_form": "단열재 안넣으면 여름엔 녹을걸요",
          "speaker_id": "2",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043059.1.17",
          "form": "그래도 아직 더운거같아요",
          "original_form": "그래도 아직 더운거같아요",
          "speaker_id": "2",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043059.1.18",
          "form": "까무룩까무룩이요? ㅋㅋㅋㅋ 가끔 틀어요 저 더위 많이 타니까... 어쩔수 없어버리는",
          "original_form": "까무룩까무룩이요? ㅋㅋㅋㅋ 가끔 틀어요 저 더위 많이 타니까... 어쩔수 없어버리는",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043059.1.19",
          "form": "그래두 여름에는 전기세 그렇게 높지 않으니까 다행이에요",
          "original_form": "그래두 여름에는 전기세 그렇게 높지 않으니까 다행이에요",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100043059.1.20",
          "form": "누진세 완화시켜줘서 잘된 ㅋㅋㅋ",
          "original_form": "누진세 완화시켜줘서 잘된 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100043059.1.21",
          "form": "아무튼 여름이니까 수박주스 많이 먹게 되는데",
          "original_form": "아무튼 여름이니까 수박주스 많이 먹게 되는데",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100043059.1.22",
          "form": "할리스 수박주스가 젤 맛있는거같아요",
          "original_form": "할리스 수박주스가 젤 맛있는거같아요",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100043059.1.23",
          "form": "다른곳에서도 먹어봤는데 할리스가 제일인듯 ㅋㅋㅋ",
          "original_form": "다른곳에서도 먹어봤는데 할리스가 제일인듯 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100043059.1.24",
          "form": "그건 잘 생각안나요 어디 메가커피인가 다른데인가 먹어봤는데 거긴 별루였음",
          "original_form": "그건 잘 생각안나요 어디 메가커피인가 다른데인가 먹어봤는데 거긴 별루였음",
          "speaker_id": "2",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100043059.1.25",
          "form": "그건 그래요 씹는맛이 없으니까 별로인거같은",
          "original_form": "그건 그래요 씹는맛이 없으니까 별로인거같은",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043059.1.26",
          "form": "스무디가 좋은데 스무디 파는곳은 별로 안많더라구요",
          "original_form": "스무디가 좋은데 스무디 파는곳은 별로 안많더라구요",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043059.1.27",
          "form": "그건 그런거같아요 ㅠ 아쉬워... 진짜 과일값 폭등해가지구 너무함",
          "original_form": "그건 그런거같아요 ㅠ 아쉬워... 진짜 과일값 폭등해가지구 너무함",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043059.1.28",
          "form": "ㅋㅋㅋㅋ 뭐지... 하여튼 이번 여름엔 복숭아 음료도 많이 안나오고 그래서 억울해요",
          "original_form": "ㅋㅋㅋㅋ 뭐지... 하여튼 이번 여름엔 복숭아 음료도 많이 안나오고 그래서 억울해요",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043059.1.29",
          "form": "물복 안먹고 딱복만 먹는 사람 너무 억울한",
          "original_form": "물복 안먹고 딱복만 먹는 사람 너무 억울한",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043059.1.30",
          "form": "이거 대화가 내창에 왜 안뜨냐 나갔다 왔는데",
          "original_form": "이거 대화가 내창에 왜 안뜨냐 나갔다 왔는데",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043059.1.31",
          "form": "잉 저는 잘뜨는데 뭐지",
          "original_form": "잉 저는 잘뜨는데 뭐지",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043059.1.32",
          "form": "복숭아 음료 통조림맛 나지 않아? 너 그거 별로 안좋아하잖아",
          "original_form": "복숭아 음료 통조림맛 나지 않아? 너 그거 별로 안좋아하잖아",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043059.1.33",
          "form": "근데 통조림 복숭아랑 그건 좀 다른맛이 있어서",
          "original_form": "근데 통조림 복숭아랑 그건 좀 다른맛이 있어서",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043059.1.34",
          "form": "이디야 복숭아 플랫치노 맛있어욤",
          "original_form": "이디야 복숭아 플랫치노 맛있어욤",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043059.1.35",
          "form": "너무 통조림 맛이라 나는 별로",
          "original_form": "너무 통조림 맛이라 나는 별로",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043059.1.36",
          "form": "엄마는 비싼입맛이시군요..",
          "original_form": "엄마는 비싼입맛이시군요..",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043059.1.37",
          "form": "너무 단거는 안좋아해",
          "original_form": "너무 단거는 안좋아해",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043059.1.38",
          "form": "인공적인 맛도 별로야",
          "original_form": "인공적인 맛도 별로야",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043059.1.39",
          "form": "저두 인공적인 맛은 ㅂ려로예요 ㅋㅋㅋ",
          "original_form": "저두 인공적인 맛은 ㅂ려로예요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100043059.1.40",
          "form": "너 그래서 딸기맛 사탕도 안먹잖아",
          "original_form": "너 그래서 딸기맛 사탕도 안먹잖아",
          "speaker_id": "1",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100043059.1.41",
          "form": "딸기맛 사탕이 진짜 인공맛의 끝이죠",
          "original_form": "딸기맛 사탕이 진짜 인공맛의 끝이죠",
          "speaker_id": "2",
          "time": "20210907 14:11"
        },
        {
          "id": "MDRW2100043059.1.42",
          "form": "엄마두 그거 좋아하셨잖아요 ㅋㅋㅋ",
          "original_form": "엄마두 그거 좋아하셨잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043059.1.43",
          "form": "저대신 먹는거 딸기맛사탕!!",
          "original_form": "저대신 먹는거 딸기맛사탕!!",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043059.1.44",
          "form": "집에 아직도 츄파츕스 남아있다",
          "original_form": "집에 아직도 츄파츕스 남아있다",
          "speaker_id": "1",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043059.1.45",
          "form": "그거 이제 버려야 하는거 아닌가 싶은데 ㅠ",
          "original_form": "그거 이제 버려야 하는거 아닌가 싶은데 ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100043059.1.46",
          "form": "사탕도 유통기한 있냐?",
          "original_form": "사탕도 유통기한 있냐?",
          "speaker_id": "1",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100043059.1.47",
          "form": "찾아봤는데 2년정도래요 츄파츕스는",
          "original_form": "찾아봤는데 2년정도래요 츄파츕스는",
          "speaker_id": "2",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100043059.1.48",
          "form": "못먹겠네 버려야겠다",
          "original_form": "못먹겠네 버려야겠다",
          "speaker_id": "1",
