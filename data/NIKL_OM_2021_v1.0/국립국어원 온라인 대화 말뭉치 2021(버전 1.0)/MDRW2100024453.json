      "utterance": [
        {
          "id": "MDRW2100024453.1.1",
          "form": "ㅎㅎ 쏠쏠하더라구요!",
          "original_form": "ㅎㅎ 쏠쏠하더라구요!",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100024453.1.2",
          "form": "뭘 파셨나요??",
          "original_form": "뭘 파셨나요??",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100024453.1.3",
          "form": "오오 거래고수이신가봐용!",
          "original_form": "오오 거래고수이신가봐용!",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024453.1.4",
          "form": "앗 그런건 아니지만.... 엄청 싸게 올려요",
          "original_form": "앗 그런건 아니지만.... 엄청 싸게 올려요",
          "speaker_id": "1",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024453.1.5",
          "form": "저 홈트 필테용 산거랑 고양이 용품 올렸는데 답이 안오네오ㅡㅜㅠ",
          "original_form": "저 홈트 필테용 산거랑 고양이 용품 올렸는데 답이 안오네오ㅡㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024453.1.6",
          "form": "그러면 거래가 빨리빨라 되더라구요ㅋㅋ",
          "original_form": "그러면 거래가 빨리빨라 되더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024453.1.7",
          "form": "앗ㅜㅠㅠ",
          "original_form": "앗ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024453.1.8",
          "form": "앗! 싸게 올리시는구낭!",
          "original_form": "앗! 싸게 올리시는구낭!",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024453.1.9",
          "form": "약간 여러 물품 같이 올려도 잘 팔려요!",
          "original_form": "약간 여러 물품 같이 올려도 잘 팔려요!",
          "speaker_id": "1",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024453.1.10",
          "form": "역시 가격을 내려야...하는걸까용..",
          "original_form": "역시 가격을 내려야...하는걸까용..",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100024453.1.11",
          "form": "묶어서 팔기ㅎㅋㅋㅋ",
          "original_form": "묶어서 팔기ㅎㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024453.1.12",
          "form": "오오?!",
          "original_form": "오오?!",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024453.1.13",
          "form": "아ㅋㅋㅋㅋㅋ 그런 방법도 있군여",
          "original_form": "아ㅋㅋㅋㅋㅋ 그런 방법도 있군여",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024453.1.14",
          "form": "고양이 용품같은것도 여러개 세트로 팔아보세요ㅎㅎ!!",
          "original_form": "고양이 용품같은것도 여러개 세트로 팔아보세요ㅎㅎ!!",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024453.1.15",
          "form": "넵ㅎㅎ",
          "original_form": "넵ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024453.1.16",
          "form": "저번에 어떤분이 사진도 중요하대서 다시찍었더니 하트 찍히긴하더라구요!",
          "original_form": "저번에 어떤분이 사진도 중요하대서 다시찍었더니 하트 찍히긴하더라구요!",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024453.1.17",
          "form": "그래봐야겠어요!!",
          "original_form": "그래봐야겠어요!!",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024453.1.18",
          "form": "맞아요ㅎㅎ 사진 중요하죠",
          "original_form": "맞아요ㅎㅎ 사진 중요하죠",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024453.1.19",
          "form": "사보신적은 있으세요?",
          "original_form": "사보신적은 있으세요?",
          "speaker_id": "1",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024453.1.20",
          "form": "넹!! 고양이 용품이이욧",
          "original_form": "넹!! 고양이 용품이이욧",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024453.1.21",
          "form": "산건 그거밖에 없네요ㅋㅋ",
          "original_form": "산건 그거밖에 없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
