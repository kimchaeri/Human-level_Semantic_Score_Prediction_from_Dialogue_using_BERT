      "utterance": [
        {
          "id": "MDRW2100014151.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014151.1.2",
          "form": "차돌짬뽕 저도 좋아해요 ㅎㅎ 좀 기름지긴하지만 고소해서 맛있어요.",
          "original_form": "차돌짬뽕 저도 좋아해요 ㅎㅎ 좀 기름지긴하지만 고소해서 맛있어요.",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014151.1.3",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014151.1.4",
          "form": "매운짬뽕도 좋더라고요저는",
          "original_form": "매운짬뽕도 좋더라고요저는",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014151.1.5",
          "form": "어우 저도 좋아는 하는데 점점 배탈이 자주 나서 이젠 못 먹겠어요 ㅜ",
          "original_form": "어우 저도 좋아는 하는데 점점 배탈이 자주 나서 이젠 못 먹겠어요 ㅜ",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014151.1.6",
          "form": "위장이 튼튼하신가봐요 ! ㅋㅋ",
          "original_form": "위장이 튼튼하신가봐요 ! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014151.1.7",
          "form": "ㅠ 아무래도그렇긴해요",
          "original_form": "ㅠ 아무래도그렇긴해요",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014151.1.8",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014151.1.9",
          "form": "네, 조금 일찍 먹었어요.",
          "original_form": "네, 조금 일찍 먹었어요.",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014151.1.10",
          "form": "늦게 먹으면 잘 때 불편해서 일찍 먹고 자는 편이예요.",
          "original_form": "늦게 먹으면 잘 때 불편해서 일찍 먹고 자는 편이예요.",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014151.1.11",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014151.1.12",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014151.1.13",
          "form": "저녁은 몇시에드시나요?",
          "original_form": "저녁은 몇시에드시나요?",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014151.1.14",
          "form": "요즘은 가급적 6시 반 전에 먹는 편이예요. 재택이 가능하니까 그 점은 편리한 것 같습니다.",
          "original_form": "요즘은 가급적 6시 반 전에 먹는 편이예요. 재택이 가능하니까 그 점은 편리한 것 같습니다.",
          "speaker_id": "2",
          "time": "20210826 09:00"
        },
        {
          "id": "MDRW2100014151.1.15",
          "form": "어제 저녁 뭐 드셨나요?",
          "original_form": "어제 저녁 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 09:01"
        },
        {
          "id": "MDRW2100014151.1.16",
          "form": "저 어제 쫄돈이요!",
          "original_form": "저 어제 쫄돈이요!",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100014151.1.17",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100014151.1.18",
          "form": "쫄면 돈가쓰인가요?",
          "original_form": "쫄면 돈가쓰인가요?",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100014151.1.19",
          "form": "맞아요ㅋㅋㅋ어제 저녁메뉴였죠",
          "original_form": "맞아요ㅋㅋㅋ어제 저녁메뉴였죠",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100014151.1.20",
          "form": "저는 아직입니다. 이제 곧 먹으려고요. 잔치국수 먹을 거예요",
          "original_form": "저는 아직입니다. 이제 곧 먹으려고요. 잔치국수 먹을 거예요",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100014151.1.21",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100014151.1.22",
          "form": "직접 하나요???",
          "original_form": "직접 하나요???",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100014151.1.23",
          "form": "샘표 키트로 합니다",
          "original_form": "샘표 키트로 합니다",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100014151.1.24",
          "form": "온천달걀만 따로 만들면 돼요",
          "original_form": "온천달걀만 따로 만들면 돼요",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100014151.1.25",
          "form": "면 끓여서 놓고 분말스프 풀고 달걀 면 넣고 달걀 넣고",
          "original_form": "면 끓여서 놓고 분말스프 풀고 달걀 면 넣고 달걀 넣고",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100014151.1.26",
          "form": "인스턴트 같지 않고 맛있어요",
          "original_form": "인스턴트 같지 않고 맛있어요",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100014151.1.27",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100014151.1.28",
          "form": "요즘은 잔치국수도",
          "original_form": "요즘은 잔치국수도",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100014151.1.29",
          "form": "밀키트가 나오는군요",
          "original_form": "밀키트가 나오는군요",
          "speaker_id": "1",
          "time": "20210826 14:05"
        }
      ]
    }
  ]
