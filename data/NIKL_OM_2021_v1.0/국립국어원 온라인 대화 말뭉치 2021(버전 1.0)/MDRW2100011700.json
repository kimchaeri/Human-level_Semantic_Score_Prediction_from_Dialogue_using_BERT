      "utterance": [
        {
          "id": "MDRW2100011700.1.1",
          "form": "넌 남자친구 있니???",
          "original_form": "넌 남자친구 있니???",
          "speaker_id": "1",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011700.1.2",
          "form": "내 남친은 영국으로 유학감 나 버리고",
          "original_form": "내 남친은 영국으로 유학감 나 버리고",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011700.1.3",
          "form": "그놈보다는 성공한다",
          "original_form": "그놈보다는 성공한다",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011700.1.4",
          "form": "ㅅ...ㅂ....",
          "original_form": "ㅅ...ㅂ....",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011700.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ난...음",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ난...음",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011700.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011700.1.7",
          "form": "시오넬이라고",
          "original_form": "시오넬이라고",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011700.1.8",
          "form": "동화나라에 놀러온 반반한놈있는데",
          "original_form": "동화나라에 놀러온 반반한놈있는데",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011700.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011700.1.10",
          "form": "어떤 놈인가 얼굴 함 보자",
          "original_form": "어떤 놈인가 얼굴 함 보자",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011700.1.11",
          "form": "그래 검색해봐",
          "original_form": "그래 검색해봐",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011700.1.12",
          "form": "아 좀 귀찮아서 다음에 검색할게",
          "original_form": "아 좀 귀찮아서 다음에 검색할게",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011700.1.13",
          "form": "결혼은 할꺼니?",
          "original_form": "결혼은 할꺼니?",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011700.1.14",
          "form": "당연히아니지",
          "original_form": "당연히아니지",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011700.1.15",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011700.1.16",
          "form": "평생 비혼하려고^^",
          "original_form": "평생 비혼하려고^^",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011700.1.17",
          "form": "나도나도",
          "original_form": "나도나도",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011700.1.18",
          "form": "비혼 최고",
          "original_form": "비혼 최고",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011700.1.19",
          "form": "결혼 최악",
          "original_form": "결혼 최악",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011700.1.20",
          "form": "맞다맞다 혼자 잘살아야지",
          "original_form": "맞다맞다 혼자 잘살아야지",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011700.1.21",
          "form": "동물이랑 같이",
          "original_form": "동물이랑 같이",
          "speaker_id": "1",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011700.1.22",
          "form": "결혼하지말고",
          "original_form": "결혼하지말고",
          "speaker_id": "1",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011700.1.23",
          "form": "테슬라 끌고 다니면서 살자구",
