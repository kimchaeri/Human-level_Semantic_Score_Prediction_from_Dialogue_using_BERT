      "utterance": [
        {
          "id": "MDRW2100045226.1.1",
          "form": "보통 몇시에 주무세요?",
          "original_form": "보통 몇시에 주무세요?",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045226.1.2",
          "form": "두 세시 넘어서 자요 님은요?",
          "original_form": "두 세시 넘어서 자요 님은요?",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045226.1.3",
          "form": "저두요~~~",
          "original_form": "저두요~~~",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045226.1.4",
          "form": "상장일에만좀일찍일어나구요ㅋㅋㅋ",
          "original_form": "상장일에만좀일찍일어나구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045226.1.5",
          "form": "네 맞아요 저도 당일에 일찍 일어나요",
          "original_form": "네 맞아요 저도 당일에 일찍 일어나요",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045226.1.6",
          "form": "카뱅때 다행히 늦게일어났어요ㅋㅋㅋ",
          "original_form": "카뱅때 다행히 늦게일어났어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045226.1.7",
          "form": "님은 이상형 있나요?",
          "original_form": "님은 이상형 있나요?",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045226.1.8",
          "form": "저는 웃기고 마음잘맞는사람인데",
          "original_form": "저는 웃기고 마음잘맞는사람인데",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045226.1.9",
          "form": "안웃긴사람과 마음맞춰가며살고있네요ㅋㅋ",
          "original_form": "안웃긴사람과 마음맞춰가며살고있네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045226.1.10",
          "form": "아하 결혼 하셨군요! 어찌저찌 사시네요\r\n저는 키크고 공감능력 있는 사람요",
          "original_form": "아하 결혼 하셨군요! 어찌저찌 사시네요\r\n저는 키크고 공감능력 있는 사람요",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045226.1.11",
          "form": "남녀는태생이달라서 서로이해하고 배려하는게 결혼제1덕목같아요~~~",
          "original_form": "남녀는태생이달라서 서로이해하고 배려하는게 결혼제1덕목같아요~~~",
          "speaker_id": "1",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045226.1.12",
          "form": "어쩜 그리다를까요ㅋㅋ",
          "original_form": "어쩜 그리다를까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045226.1.13",
          "form": "비슷한줄알고결혼해보니^^^^",
          "original_form": "비슷한줄알고결혼해보니^^^^",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045226.1.14",
          "form": "저희 엄마도 그땐 몰랐고 지금은 너무 알아서 탈이라세요 근데 전 결혼 안할거라....",
          "original_form": "저희 엄마도 그땐 몰랐고 지금은 너무 알아서 탈이라세요 근데 전 결혼 안할거라....",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045226.1.15",
          "form": "아 저심심이로 만난 싱글들",
          "original_form": "아 저심심이로 만난 싱글들",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045226.1.16",
          "form": "대부분 그리생각하시네요",
          "original_form": "대부분 그리생각하시네요",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045226.1.17",
          "form": "결혼필수아니라고",
          "original_form": "결혼필수아니라고",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045226.1.18",
          "form": "요즘 다들 그렇게 생각하더라구요 아닐수도 있지만 ㅋㅋㅋ",
          "original_form": "요즘 다들 그렇게 생각하더라구요 아닐수도 있지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045226.1.19",
          "form": "좋은사람만나면할수도있겠조^^",
          "original_form": "좋은사람만나면할수도있겠조^^",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045226.1.20",
          "form": "연애는하셔요 그래도",
          "original_form": "연애는하셔요 그래도",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045226.1.21",
          "form": "맘에안들면 바로차버리기ㅋㅋㅋ",
          "original_form": "맘에안들면 바로차버리기ㅋㅋㅋ",
