        {
          "id": "MDRW2100024427.1.1",
          "form": "언니 출근했어유?ㅋ",
          "original_form": "언니 출근했어유?ㅋ",
          "speaker_id": "1",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100024427.1.2",
          "form": "출근했지ㅋㅋ",
          "original_form": "출근했지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024427.1.3",
          "form": "고생해유 ㅋㅋㅋㅋㅋㅋㅋㅋ아침은 먹고 갔어요?",
          "original_form": "고생해유 ㅋㅋㅋㅋㅋㅋㅋㅋ아침은 먹고 갔어요?",
          "speaker_id": "1",
          "time": "20210831 10:21"
        },
        {
          "id": "MDRW2100024427.1.4",
          "form": "아침먹고땡ㅋㅋ",
          "original_form": "아침먹고땡ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:37"
        },
        {
          "id": "MDRW2100024427.1.5",
          "form": "그러나배고픔",
          "original_form": "그러나배고픔",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024427.1.6",
          "form": "오늘은 한솥",
          "original_form": "오늘은 한솥",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024427.1.7",
          "form": "한솥 잘 먹네요 언니 ㅋㅋㅋ",
          "original_form": "한솥 잘 먹네요 언니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024427.1.8",
          "form": "그래ㅋ 한솥ㅋㅋ",
          "original_form": "그래ㅋ 한솥ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100024427.1.9",
          "form": "느끼하다 오늘은",
          "original_form": "느끼하다 오늘은",
          "speaker_id": "2",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100024427.1.10",
          "form": "ㅋㅋㅋ느끼하쥬 ㅋㅋㅋ 저는 뭘 먹을가 고민중......매일 그 반찬이 그반찬이라서 .....ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ느끼하쥬 ㅋㅋㅋ 저는 뭘 먹을가 고민중......매일 그 반찬이 그반찬이라서 .....ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100024427.1.11",
          "form": "집에왔는듸ㅣ 치우기 왜이리싫노ㅋ",
          "original_form": "집에왔는듸ㅣ 치우기 왜이리싫노ㅋ",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100024427.1.12",
          "form": "날씨 좋네",
          "original_form": "날씨 좋네",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100024427.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ언니 이제왓어요 ?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ언니 이제왓어요 ?",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024427.1.14",
          "form": "ㅋㅋㅋ집은 늘 치우기 싫음 ㅋㅋ 늘 어지렵혀서 ㅋㅋ",
          "original_form": "ㅋㅋㅋ집은 늘 치우기 싫음 ㅋㅋ 늘 어지렵혀서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024427.1.15",
          "form": "어ㅋㅋ 빨래돌리고있다",
          "original_form": "어ㅋㅋ 빨래돌리고있다",
          "speaker_id": "2",
          "time": "20210831 13:15"
        },
        {
          "id": "MDRW2100024427.1.16",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210831 13:15"
        },
        {
          "id": "MDRW2100024427.1.17",
          "form": "ㅋㅋㅋ 나도 오늘은 베개빨고 했어요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 나도 오늘은 베개빨고 했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:15"
        },
        {
          "id": "MDRW2100024427.1.18",
          "form": "내일은 이불빨래 ㅋㅋ",
          "original_form": "내일은 이불빨래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:15"
        },
        {
