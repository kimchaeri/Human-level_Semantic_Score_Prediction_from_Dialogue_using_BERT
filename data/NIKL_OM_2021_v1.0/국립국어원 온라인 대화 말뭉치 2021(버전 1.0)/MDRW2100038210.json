      "utterance": [
        {
          "id": "MDRW2100038210.1.1",
          "form": "탁재훈 생일인가봐",
          "original_form": "탁재훈 생일인가봐",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100038210.1.2",
          "form": "그런가보네 미역국 끓여줄려나",
          "original_form": "그런가보네 미역국 끓여줄려나",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038210.1.3",
          "form": "케익도 있다",
          "original_form": "케익도 있다",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038210.1.4",
          "form": "생일이라고 케익도 장만햇나보다",
          "original_form": "생일이라고 케익도 장만햇나보다",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038210.1.5",
          "form": "치킨케익?",
          "original_form": "치킨케익?",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038210.1.6",
          "form": "뭐지 저런것도 있어 신기하네",
          "original_form": "뭐지 저런것도 있어 신기하네",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038210.1.7",
          "form": "그러게 나도 첨봄",
          "original_form": "그러게 나도 첨봄",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100038210.1.8",
          "form": "ㅋㅋ이제 티비에 나왓으니 치킨 케익 엄청 나올듯",
          "original_form": "ㅋㅋ이제 티비에 나왓으니 치킨 케익 엄청 나올듯",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100038210.1.9",
          "form": "치킨집에서 다 팔겠다",
          "original_form": "치킨집에서 다 팔겠다",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100038210.1.10",
          "form": "맞어 여기저기에서 미우새 치킨케익보고",
          "original_form": "맞어 여기저기에서 미우새 치킨케익보고",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100038210.1.11",
          "form": "50살인가봐",
          "original_form": "50살인가봐",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038210.1.12",
          "form": "택재훈도 어찌보면 동안이다",
          "original_form": "택재훈도 어찌보면 동안이다",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038210.1.13",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100038210.1.14",
