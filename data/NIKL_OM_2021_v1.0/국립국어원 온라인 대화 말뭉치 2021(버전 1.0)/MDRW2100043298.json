      "utterance": [
        {
          "id": "MDRW2100043298.1.1",
          "form": "근데 그 배우가 원래 어떤스탈인진 모르겟는데",
          "original_form": "근데 그 배우가 원래 어떤스탈인진 모르겟는데",
          "speaker_id": "2",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100043298.1.2",
          "form": "거기선 목소ㄹ ㅣ 도 엄청 허스키하게",
          "original_form": "거기선 목소ㄹ ㅣ 도 엄청 허스키하게",
          "speaker_id": "2",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043298.1.3",
          "form": "나른 se/xy 하거든요",
          "original_form": "나른 se/xy 하거든요",
          "speaker_id": "2",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043298.1.4",
          "form": "하 진짜 그놈의 나쁜말",
          "original_form": "하 진짜 그놈의 나쁜말",
          "speaker_id": "2",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043298.1.5",
          "form": "오 ㅋㅋㅋ 뭔 느낌인지 알겠어요",
          "original_form": "오 ㅋㅋㅋ 뭔 느낌인지 알겠어요",
          "speaker_id": "1",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043298.1.6",
          "form": "퇴페 세쿠시 ㅋㅋ",
          "original_form": "퇴페 세쿠시 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043298.1.7",
          "form": "퇴폐",
          "original_form": "퇴폐",
          "speaker_id": "1",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043298.1.8",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043298.1.9",
          "form": "딱 그거",
          "original_form": "딱 그거",
          "speaker_id": "2",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043298.1.10",
          "form": "잼나요 ㅌㅋㅋㅋ",
          "original_form": "잼나요 ㅌㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043298.1.11",
          "form": "담에 넷플 끊으면 봐야겠네요",
          "original_form": "담에 넷플 끊으면 봐야겠네요",
          "speaker_id": "1",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043298.1.12",
          "form": "전 최근에 젤 재밌게본미드가",
          "original_form": "전 최근에 젤 재밌게본미드가",
          "speaker_id": "1",
          "time": "20210907 17:09"
        },
        {
          "id": "MDRW2100043298.1.13",
          "form": "와이우먼킬 이었어요",
          "original_form": "와이우먼킬 이었어요",
          "speaker_id": "1",
          "time": "20210907 17:09"
        },
        {
          "id": "MDRW2100043298.1.14",
          "form": "와!!!",
          "original_form": "와!!!",
          "speaker_id": "2",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100043298.1.15",
          "form": "저도요!!!!",
          "original_form": "저도요!!!!",
          "speaker_id": "2",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100043298.1.16",
          "form": "그거 진짜 줜잼",
          "original_form": "그거 진짜 줜잼",
          "speaker_id": "2",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100043298.1.17",
          "form": "시즌2는 아직 못봤어요",
          "original_form": "시즌2는 아직 못봤어요",
          "speaker_id": "2",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100043298.1.18",
          "form": "저두요  시즌투는 아직 ㅋㅋ",
          "original_form": "저두요  시즌투는 아직 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043298.1.19",
          "form": "기묘한 이야기 보셨나요?",
          "original_form": "기묘한 이야기 보셨나요?",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043298.1.20",
          "form": "네네 ㅋㅋㅋ 그건 다 봣어요",
          "original_form": "네네 ㅋㅋㅋ 그건 다 봣어요",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100043298.1.21",
          "form": "뭔가 넷플을 본다면",
          "original_form": "뭔가 넷플을 본다면",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100043298.1.22",
          "form": "기묘한은 꼭 봐줘야하는 너낌 ㅋㅋㅋ",
          "original_form": "기묘한은 꼭 봐줘야하는 너낌 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100043298.1.23",
          "form": "맞아요 저 그거때메 넷플 가입했었는데!",
          "original_form": "맞아요 저 그거때메 넷플 가입했었는데!",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043298.1.24",
          "form": "진짜 재밌더라구요 미드 잘 안보는데 넘 재밌게봤어요",
          "original_form": "진짜 재밌더라구요 미드 잘 안보는데 넘 재밌게봤어요",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043298.1.25",
          "form": "ㅋㅋㅋㅋ맞죠맞죠 저는 첨에 옥자 보려고 가입했어요",
          "original_form": "ㅋㅋㅋㅋ맞죠맞죠 저는 첨에 옥자 보려고 가입했어요",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043298.1.26",
          "form": "그거 제 눈물바튼 ㅠㅠ",
          "original_form": "그거 제 눈물바튼 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043298.1.27",
          "form": "그것도 재밌나보네요 ㅎㅎ 담에봐야겠어요",
          "original_form": "그것도 재밌나보네요 ㅎㅎ 담에봐야겠어요",
          "speaker_id": "1",
          "time": "20210907 17:46"
        },
        {
          "id": "MDRW2100043298.1.28",
          "form": "네 저 한 3번인가 봣어요",
          "original_form": "네 저 한 3번인가 봣어요",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043298.1.29",
          "form": "우와 인생영화인가 보네요 ㅎㅎ",
          "original_form": "우와 인생영화인가 보네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100043298.1.30",
          "form": "전 얼마전에 끝까지간다 봤는데 괜찮더라구요",
          "original_form": "전 얼마전에 끝까지간다 봤는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100043298.1.31",
          "form": "아 그 이선균 나오는거 맞죠?",
          "original_form": "아 그 이선균 나오는거 맞죠?",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043298.1.32",
          "form": "저도 예전에 본거같아요 ㅋㅋㅋㅋ",
          "original_form": "저도 예전에 본거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043298.1.33",
          "form": "나름 흥미롭게 봤던 기억이 ㅋㅋㅋ",
          "original_form": "나름 흥미롭게 봤던 기억이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043298.1.34",
          "form": "아 요즘엔 더 체어 드라마도 보고임ㅅ어요 넷플",
          "original_form": "아 요즘엔 더 체어 드라마도 보고임ㅅ어요 넷플",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043298.1.35",
          "form": "그 산드라 오가 주연인!!",
          "original_form": "그 산드라 오가 주연인!!",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043298.1.36",
          "form": "아 들어봤어요! 한국 돌잡이하는거 나오더라구요 ㅋㅋ",
          "original_form": "아 들어봤어요! 한국 돌잡이하는거 나오더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043298.1.37",
          "form": "오 저 이제 1편 다봣는데 돌잡이는 아직 ㅋㅋㅋㅋ",
          "original_form": "오 저 이제 1편 다봣는데 돌잡이는 아직 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043298.1.38",
          "form": "아 산드라오 하니까 그 유명한 드라마뭐죠 왓챠에?",
          "original_form": "아 산드라오 하니까 그 유명한 드라마뭐죠 왓챠에?",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043298.1.39",
          "form": "저는 안봤는데 유머 이런글에 올라오더라구요",
          "original_form": "저는 안봤는데 유머 이런글에 올라오더라구요",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043298.1.40",
          "form": "아 킬링이브!! 그것도 진짜 잼나게 봣어요",
          "original_form": "아 킬링이브!! 그것도 진짜 잼나게 봣어요",
          "speaker_id": "2",
