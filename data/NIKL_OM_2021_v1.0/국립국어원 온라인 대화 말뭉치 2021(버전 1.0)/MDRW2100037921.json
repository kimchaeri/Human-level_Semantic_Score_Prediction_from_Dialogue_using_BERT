      "utterance": [
        {
          "id": "MDRW2100037921.1.1",
          "form": "목소리로 오다니 ㄷㄷ",
          "original_form": "목소리로 오다니 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100037921.1.2",
          "form": "지나고나서 아셨나봐용 ㅎㅎ",
          "original_form": "지나고나서 아셨나봐용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100037921.1.3",
          "form": "전혀 상관없이 ㅋㅋ",
          "original_form": "전혀 상관없이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037921.1.4",
          "form": "친구가 알레르기 검사한다는거에요",
          "original_form": "친구가 알레르기 검사한다는거에요",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037921.1.5",
          "form": "헉 ㅋㅋㅋ 그거 저도 해보고싶었는데",
          "original_form": "헉 ㅋㅋㅋ 그거 저도 해보고싶었는데",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.6",
          "form": "근데 그게 무슨 제휴된 병원이래서",
          "original_form": "근데 그게 무슨 제휴된 병원이래서",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.7",
          "form": "비싸지요?",
          "original_form": "비싸지요?",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.9",
          "form": "공짜였나? 진짜쌌나?",
          "original_form": "공짜였나? 진짜쌌나?",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.10",
          "form": "그랬어요",
          "original_form": "그랬어요",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.11",
          "form": "헉 부러워요",
          "original_form": "헉 부러워요",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.12",
          "form": "제가 다니던 대학 제휴병원이었던거같아요",
          "original_form": "제가 다니던 대학 제휴병원이었던거같아요",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.13",
          "form": "대신 ㅋㅋ등록금 냈으니",
          "original_form": "대신 ㅋㅋ등록금 냈으니",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.14",
          "form": "아하 그런이유가 ㅋㅋㅋㅋ",
          "original_form": "아하 그런이유가 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037921.1.15",
          "form": "뽕뽑자며 그런건 서비스 받아야한다며",
          "original_form": "뽕뽑자며 그런건 서비스 받아야한다며",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037921.1.16",
          "form": "갔다가 알았네여 ㅋㅋ어이없게도",
          "original_form": "갔다가 알았네여 ㅋㅋ어이없게도",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037921.1.17",
          "form": "아니였음 계속 몰랐을수도 있었겠어용",
          "original_form": "아니였음 계속 몰랐을수도 있었겠어용",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037921.1.18",
          "form": "그러고나서 엄마한테 엄마그거알아? 나동물털이랑 먼지랑 알레르기있대",
          "original_form": "그러고나서 엄마한테 엄마그거알아? 나동물털이랑 먼지랑 알레르기있대",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100037921.1.19",
          "form": "심한건아니니까 약먹을필요는없대",
          "original_form": "심한건아니니까 약먹을필요는없대",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037921.1.20",
          "form": "그러니까 엄마가 그거 엄마도있다고 ㅋㅋ",
          "original_form": "그러니까 엄마가 그거 엄마도있다고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037921.1.21",
          "form": "유전이 쪼꼼 있었나봐요 ㅎㅎ",
          "original_form": "유전이 쪼꼼 있었나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100037921.1.22",
          "form": "기도 붓고 이런건 아니라 다행이에요",
          "original_form": "기도 붓고 이런건 아니라 다행이에요",
          "speaker_id": "2",
          "time": "20210901 21:18"
        }
      ]
