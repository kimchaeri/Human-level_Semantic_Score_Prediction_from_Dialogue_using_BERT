      "utterance": [
        {
          "id": "MDRW2100035352.1.1",
          "form": "채널을 다 돌려봤는데 재밌는게 없다",
          "original_form": "채널을 다 돌려봤는데 재밌는게 없다",
          "speaker_id": "2",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100035352.1.2",
          "form": "ㅋㅋㅋ  한나 틀어ㅋ 재미없다만",
          "original_form": "ㅋㅋㅋ  한나 틀어ㅋ 재미없다만",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100035352.1.3",
          "form": "스크린ㅋ",
          "original_form": "스크린ㅋ",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100035352.1.4",
          "form": "한나 볼봐에야 여러번 빌리엘리어트 보겠다",
          "original_form": "한나 볼봐에야 여러번 빌리엘리어트 보겠다",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100035352.1.5",
          "form": "나도 틀어나서 보고 있음",
          "original_form": "나도 틀어나서 보고 있음",
          "speaker_id": "1",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100035352.1.6",
          "form": "ㅋㅋ지금 나오는 애가 한나인갑지",
          "original_form": "ㅋㅋ지금 나오는 애가 한나인갑지",
          "speaker_id": "2",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100035352.1.7",
          "form": "아까 배 타고 있는 여자",
          "original_form": "아까 배 타고 있는 여자",
          "speaker_id": "1",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100035352.1.8",
          "form": "내용이 음침하다ㅋ",
          "original_form": "내용이 음침하다ㅋ",
          "speaker_id": "1",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035352.1.9",
          "form": "아~ 한나 배역의 배우 안다",
          "original_form": "아~ 한나 배역의 배우 안다",
          "speaker_id": "2",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035352.1.10",
          "form": "많이 본듯한 애다",
          "original_form": "많이 본듯한 애다",
          "speaker_id": "1",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035352.1.11",
          "form": "엉 근데 어디서 본게 기억 안난다",
          "original_form": "엉 근데 어디서 본게 기억 안난다",
          "speaker_id": "2",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035352.1.12",
          "form": "해리포터 나왔나?",
          "original_form": "해리포터 나왔나?",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035352.1.13",
          "form": "작은아씨들에 나왓네 거의 고전 영화",
          "original_form": "작은아씨들에 나왓네 거의 고전 영화",
          "speaker_id": "2",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035352.1.14",
          "form": "아 작은아씨들..  안봤지만 나왔구나",
          "original_form": "아 작은아씨들..  안봤지만 나왔구나",
          "speaker_id": "1",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035352.1.15",
          "form": "아 나 어디서 본지 검색해보니 기억낫다",
          "original_form": "아 나 어디서 본지 검색해보니 기억낫다",
          "speaker_id": "2",
