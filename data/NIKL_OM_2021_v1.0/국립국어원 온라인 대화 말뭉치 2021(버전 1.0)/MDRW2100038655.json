      "utterance": [
        {
          "id": "MDRW2100038655.1.1",
          "form": "여행가고 싶은곳이 있나요",
          "original_form": "여행가고 싶은곳이 있나요",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100038655.1.2",
          "form": "제주도 가고 싶어요ㅠ",
          "original_form": "제주도 가고 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100038655.1.3",
          "form": "저도 제주도 가고 싶네요",
          "original_form": "저도 제주도 가고 싶네요",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100038655.1.4",
          "form": "제주도 어디가고 싶으세요",
          "original_form": "제주도 어디가고 싶으세요",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100038655.1.5",
          "form": "저는 쇠소깍이요",
          "original_form": "저는 쇠소깍이요",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100038655.1.6",
          "form": "그곳이 어딘가요?\r\n잘 모르는 곳이예요",
          "original_form": "그곳이 어딘가요?\r\n잘 모르는 곳이예요",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100038655.1.7",
          "form": "제주도에 있는 천?이에요",
          "original_form": "제주도에 있는 천?이에요",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038655.1.8",
          "form": "뗏목타기 체험 있어요ㅎㅎ",
          "original_form": "뗏목타기 체험 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038655.1.9",
          "form": "아 가본곳이 아니라서 체험 좋을것 같네요",
          "original_form": "아 가본곳이 아니라서 체험 좋을것 같네요",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100038655.1.10",
          "form": "그럼 어디 가보셨어요?",
          "original_form": "그럼 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100038655.1.11",
          "form": "과물 노천탕. 천년의숲 비자림 가봤어요\r\n산책하기 좋아요",
          "original_form": "과물 노천탕. 천년의숲 비자림 가봤어요\r\n산책하기 좋아요",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100038655.1.12",
          "form": "와 저는 하나도 안가봤어요",
          "original_form": "와 저는 하나도 안가봤어요",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100038655.1.13",
          "form": "저랑 정반대로 가셨나봐요",
          "original_form": "저랑 정반대로 가셨나봐요",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100038655.1.14",
          "form": "저도 뗏목타기 체험은 안가봤어요",
          "original_form": "저도 뗏목타기 체험은 안가봤어요",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100038655.1.15",
          "form": "가보고 싶네요",
          "original_form": "가보고 싶네요",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100038655.1.16",
          "form": "저야말로 숲 가보고 싶어요",
          "original_form": "저야말로 숲 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038655.1.17",
          "form": "가보시면 힐링하기  좋을거에요",
          "original_form": "가보시면 힐링하기  좋을거에요",
          "speaker_id": "1",
