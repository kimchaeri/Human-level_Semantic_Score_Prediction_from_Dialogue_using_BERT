      "utterance": [
        {
          "id": "MDRW2100007829.1.1",
          "form": "혹시 폰게임같은건 하세요?",
          "original_form": "혹시 폰게임같은건 하세요?",
          "speaker_id": "2",
          "time": "20210822 11:12"
        },
        {
          "id": "MDRW2100007829.1.2",
          "form": "오 저 rusty lake 시리즈 좀 열심히 했어요!",
          "original_form": "오 저 rusty lake 시리즈 좀 열심히 했어요!",
          "speaker_id": "1",
          "time": "20210822 11:15"
        },
        {
          "id": "MDRW2100007829.1.3",
          "form": "방탈출 게임 같은거 좋아해요 ㅎㅎ",
          "original_form": "방탈출 게임 같은거 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:15"
        },
        {
          "id": "MDRW2100007829.1.4",
          "form": "하는 거 있으세요?",
          "original_form": "하는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 11:16"
        },
        {
          "id": "MDRW2100007829.1.5",
          "form": "저는 좀 단순한 고전게임 타입인가봐요",
          "original_form": "저는 좀 단순한 고전게임 타입인가봐요",
          "speaker_id": "2",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007829.1.6",
          "form": "최근엔 게임하면돈버는게임에 빠졌어요",
          "original_form": "최근엔 게임하면돈버는게임에 빠졌어요",
          "speaker_id": "2",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007829.1.7",
          "form": "치킨각이랑 캐시퍼하고있어요",
          "original_form": "치킨각이랑 캐시퍼하고있어요",
          "speaker_id": "2",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007829.1.8",
          "form": "돈버는 게임이면 거의 부업인데욬ㅋㅋㅋㅋ",
          "original_form": "돈버는 게임이면 거의 부업인데욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007829.1.9",
          "form": "광고 보는 게임인가요?",
          "original_form": "광고 보는 게임인가요?",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007829.1.10",
          "form": "돈 많이버는건아니에요 ㅋㅋ",
          "original_form": "돈 많이버는건아니에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007829.1.11",
          "form": "현생 포기하고 24시간해봐야 한달 치킨 두마리사먹으려나 ㅋㅋ",
          "original_form": "현생 포기하고 24시간해봐야 한달 치킨 두마리사먹으려나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007829.1.12",
          "form": "게임 하는 김에 돈도 벌고 그런 느낌이군욬ㅋㅋ",
          "original_form": "게임 하는 김에 돈도 벌고 그런 느낌이군욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007829.1.13",
          "form": "치킨각은 그 농장형? 게임이에요 ㅋㅋ",
          "original_form": "치킨각은 그 농장형? 게임이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007829.1.14",
          "form": "오 한번 검색해봐야겠어요 농장 육성하는 게임인가요?",
          "original_form": "오 한번 검색해봐야겠어요 농장 육성하는 게임인가요?",
          "speaker_id": "1",
          "time": "20210822 11:51"
        },
        {
          "id": "MDRW2100007829.1.15",
          "form": "정확히는 치킨을 키우는 게임이요 ㅋㅋ",
          "original_form": "정확히는 치킨을 키우는 게임이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:52"
        },
        {
          "id": "MDRW2100007829.1.16",
          "form": "치킨ㅋㅋㅋㅋㅋㅋ 치킨을 육성해서 돈버는 게임이라 치킨각인거군요",
          "original_form": "치킨ㅋㅋㅋㅋㅋㅋ 치킨을 육성해서 돈버는 게임이라 치킨각인거군요",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007829.1.17",
          "form": "저는 치킨 기프티콘을 주나 했어요",
          "original_form": "저는 치킨 기프티콘을 주나 했어요",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007829.1.18",
          "form": "광고를 봐서 리워드가 적립되나요?",
          "original_form": "광고를 봐서 리워드가 적립되나요?",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007829.1.19",
          "form": "게임하다보면 배너광고 간간히 뜨고요",
          "original_form": "게임하다보면 배너광고 간간히 뜨고요",
          "speaker_id": "2",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100007829.1.20",
          "form": "그냥 치킨이 알낳은거 깔때마다 포인트나 골드가 적립되여",
          "original_form": "그냥 치킨이 알낳은거 깔때마다 포인트나 골드가 적립되여",
          "speaker_id": "2",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100007829.1.21",
          "form": "ㅋㅋㅋㅋㅋ아 웃기네요 저도 깔아야겠어요",
          "original_form": "ㅋㅋㅋㅋㅋ아 웃기네요 저도 깔아야겠어요",
          "speaker_id": "1",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100007829.1.22",
          "form": "예전에 농장게임중에\r\n진짜 농산물 리워드로 배송해주는 그런 게임 있었는데 그거 생각나네요",
          "original_form": "예전에 농장게임중에\r\n진짜 농산물 리워드로 배송해주는 그런 게임 있었는데 그거 생각나네요",
          "speaker_id": "1",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100007829.1.23",
          "form": "그거 이름이 무슨 팜이었는데 ㅋㅋ 농장팜인가 ㅋㅋㅋ",
          "original_form": "그거 이름이 무슨 팜이었는데 ㅋㅋ 농장팜인가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100007829.1.24",
          "form": "러스키 레이크는 어떤 겜인가여",
          "original_form": "러스키 레이크는 어떤 겜인가여",
