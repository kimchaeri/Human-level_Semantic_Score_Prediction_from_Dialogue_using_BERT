        },
        {
          "id": "MDRW2100044073.1.3",
          "form": "벌레 물렸나",
          "original_form": "벌레 물렸나",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044073.1.4",
          "form": "잘 보니까 그런건 없는데 ㅠㅠ",
          "original_form": "잘 보니까 그런건 없는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044073.1.5",
          "form": "눈병 난거 아니야",
          "original_form": "눈병 난거 아니야",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044073.1.6",
          "form": "헐..그럴수도 ㅠㅠ",
          "original_form": "헐..그럴수도 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044073.1.7",
          "form": "내일 병원 데리고 가봐라",
          "original_form": "내일 병원 데리고 가봐라",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044073.1.8",
          "form": "그래야겠다..",
          "original_form": "그래야겠다..",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044073.1.9",
          "form": "걱정말고",
          "original_form": "걱정말고",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044073.1.10",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044073.1.11",
          "form": "눈이 벌겋나?",
          "original_form": "눈이 벌겋나?",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044073.1.12",
          "form": "아니 그냥",
          "original_form": "아니 그냥",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044073.1.13",
          "form": "긁기만 한다",
          "original_form": "긁기만 한다",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044073.1.14",
          "form": "벌레물림 같다",
          "original_form": "벌레물림 같다",
