      "utterance": [
        {
          "id": "MDRW2100002214.1.1",
          "form": "전 요새 바달로 살아요",
          "original_form": "전 요새 바달로 살아요",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002214.1.2",
          "form": "배달로 살아요",
          "original_form": "배달로 살아요",
          "speaker_id": "2",
          "time": "20210820 00:43"
        },
        {
          "id": "MDRW2100002214.1.3",
          "form": "배달 뭐 많이 시키세요?",
          "original_form": "배달 뭐 많이 시키세요?",
          "speaker_id": "1",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002214.1.4",
          "form": "햄버거 족발 샌드위치",
          "original_form": "햄버거 족발 샌드위치",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002214.1.5",
          "form": "불량메뉴죠",
          "original_form": "불량메뉴죠",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002214.1.6",
          "form": "ㅋㅋㅋㅋㅋ저도 오늘 햄버거 시켜먹었어요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 오늘 햄버거 시켜먹었어요",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002214.1.7",
          "form": "버거킹 행사를 많이하더라구여",
          "original_form": "버거킹 행사를 많이하더라구여",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002214.1.8",
          "form": "아 시키는것도 행사를 해요?",
          "original_form": "아 시키는것도 행사를 해요?",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002214.1.9",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002214.1.10",
          "form": "매번 비싸게 시켰네요",
          "original_form": "매번 비싸게 시켰네요",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002214.1.11",
          "form": "서브웨이도 잘시켜요",
          "original_form": "서브웨이도 잘시켜요",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002214.1.12",
          "form": "어플에서 하는 행사들이여 요기요같은",
          "original_form": "어플에서 하는 행사들이여 요기요같은",
          "speaker_id": "1",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002214.1.13",
          "form": "오늘오천원할인이더라구요",
          "original_form": "오늘오천원할인이더라구요",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002214.1.14",
          "form": "서브웨이 저희동네는ㅜ없어서 못먹고있어요",
          "original_form": "서브웨이 저희동네는ㅜ없어서 못먹고있어요",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002214.1.15",
          "form": "오천원 다박이네요",
          "original_form": "오천원 다박이네요",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002214.1.16",
          "form": "저희도 멀어요ㅜㅜ배달료 비싸요",
          "original_form": "저희도 멀어요ㅜㅜ배달료 비싸요",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002214.1.17",
          "form": "전 배달료비싸면 아까워서 못먹겠더라구여ㅜ샌드위치하나가격이랑 맞먹는",
          "original_form": "전 배달료비싸면 아까워서 못먹겠더라구여ㅜ샌드위치하나가격이랑 맞먹는",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002214.1.18",
          "form": "배달비도 점점 오르던데",
          "original_form": "배달비도 점점 오르던데",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002214.1.19",
          "form": "맞아요 요새 근데 가격을 내려서 시켜요",
          "original_form": "맞아요 요새 근데 가격을 내려서 시켜요",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002214.1.20",
          "form": "그전엔 꿈도 못꿨죠",
          "original_form": "그전엔 꿈도 못꿨죠",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002214.1.21",
          "form": "전 할인하는 종류들을 많이시켜먹는것같아요 ㅋㅋㅋ",
          "original_form": "전 할인하는 종류들을 많이시켜먹는것같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002214.1.22",
          "form": "저도 그렇긴해요",
          "original_form": "저도 그렇긴해요",
          "speaker_id": "2",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002214.1.23",
          "form": "근데 햄버거 할인 못받고 헛 살았어요 ㅜㅜ",
          "original_form": "근데 햄버거 할인 못받고 헛 살았어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002214.1.24",
          "form": "햄버거는 직접가거나 어플할인받는게 젤싼거같긴해여",
          "original_form": "햄버거는 직접가거나 어플할인받는게 젤싼거같긴해여",
          "speaker_id": "1",
          "time": "20210820 01:01"
        }
      ]
