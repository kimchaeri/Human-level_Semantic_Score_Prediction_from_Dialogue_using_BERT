      "utterance": [
        {
          "id": "MDRW2100020566.1.1",
          "form": "샌드위치 어떤거 드시나요",
          "original_form": "샌드위치 어떤거 드시나요",
          "speaker_id": "2",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020566.1.2",
          "form": "약간 롤처럼 되어있는거 아시나요",
          "original_form": "약간 롤처럼 되어있는거 아시나요",
          "speaker_id": "1",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020566.1.3",
          "form": "비싸긴 진짜 비싸더라구요 어제 보니까 기본이 6000원부터라 들었다놔써요",
          "original_form": "비싸긴 진짜 비싸더라구요 어제 보니까 기본이 6000원부터라 들었다놔써요",
          "speaker_id": "2",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020566.1.4",
          "form": "어 아는데 그",
          "original_form": "어 아는데 그",
          "speaker_id": "2",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020566.1.5",
          "form": "지금은 그게 먹고 싶네요",
          "original_form": "지금은 그게 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020566.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100020566.1.7",
          "form": "저는 그리고 또",
          "original_form": "저는 그리고 또",
          "speaker_id": "1",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100020566.1.8",
          "form": "아는데 아니까 아는걸로만해요 뭔지알아요 그거 맛있어요",
          "original_form": "아는데 아니까 아는걸로만해요 뭔지알아요 그거 맛있어요",
          "speaker_id": "2",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100020566.1.9",
          "form": "또 뭐 좋아하세요",
          "original_form": "또 뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100020566.1.10",
          "form": "빠바샌드위치 4조각 짜리 있잖아요",
          "original_form": "빠바샌드위치 4조각 짜리 있잖아요",
          "speaker_id": "1",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100020566.1.11",
          "form": "런치샌드위치?",
          "original_form": "런치샌드위치?",
          "speaker_id": "2",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100020566.1.12",
          "form": "안에 슬라이스햄들은거",
          "original_form": "안에 슬라이스햄들은거",
          "speaker_id": "2",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100020566.1.13",
          "form": "아마 그건거 같아요 늘 이름을 안보고 사서",
          "original_form": "아마 그건거 같아요 늘 이름을 안보고 사서",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.15",
          "form": "아닌가.. 페스츄리.. 빵에",
          "original_form": "아닌가.. 페스츄리.. 빵에",
          "speaker_id": "2",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.16",
          "form": "그리고 파바가 은근 샐러드맛집",
          "original_form": "그리고 파바가 은근 샐러드맛집",
          "speaker_id": "2",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.17",
          "form": "근데 뭔들 다 맛있어요",
          "original_form": "근데 뭔들 다 맛있어요",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.19",
          "form": "다이어트할 때 많이 먹었죠",
          "original_form": "다이어트할 때 많이 먹었죠",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.20",
          "form": "오리엔탈소스가 너무맛있어요",
          "original_form": "오리엔탈소스가 너무맛있어요",
          "speaker_id": "2",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.21",
          "form": "닭가슴살 은근 많이 들었어요",
          "original_form": "닭가슴살 은근 많이 들었어요",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.22",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.23",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020566.1.24",
          "form": "값은 비싼데 맛있어서 그거 어제 집어왔어요",
          "original_form": "값은 비싼데 맛있어서 그거 어제 집어왔어요",
