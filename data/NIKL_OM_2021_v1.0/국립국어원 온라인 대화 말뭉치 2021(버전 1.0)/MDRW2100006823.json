      "utterance": [
        {
          "id": "MDRW2100006823.1.1",
          "form": "추석 계획 있으신가요",
          "original_form": "추석 계획 있으신가요",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100006823.1.2",
          "form": "시골에 갈생각인데 ....",
          "original_form": "시골에 갈생각인데 ....",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100006823.1.3",
          "form": "어떤 계획 이신가요",
          "original_form": "어떤 계획 이신가요",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100006823.1.4",
          "form": "저도 근거리에 있는 양가부모님과 식사할것 같네요",
          "original_form": "저도 근거리에 있는 양가부모님과 식사할것 같네요",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100006823.1.5",
          "form": "그런데 추석 방역이 걱정되긴 해요",
          "original_form": "그런데 추석 방역이 걱정되긴 해요",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100006823.1.6",
          "form": "그렇죠... 아무래도 이동이 있긴 하겠죠...",
          "original_form": "그렇죠... 아무래도 이동이 있긴 하겠죠...",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100006823.1.7",
          "form": "추석엔 접종 완료자포함 6인~8인 이라 좀 낫죠",
          "original_form": "추석엔 접종 완료자포함 6인~8인 이라 좀 낫죠",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100006823.1.8",
          "form": "네 가족분들은 완료하셨나요",
          "original_form": "네 가족분들은 완료하셨나요",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100006823.1.9",
          "form": "부모님과 형님네는 완료 했죠",
          "original_form": "부모님과 형님네는 완료 했죠",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100006823.1.10",
          "form": "아 그럼 모이실순있겠네요",
          "original_form": "아 그럼 모이실순있겠네요",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100006823.1.11",
          "form": "네 그렇긴하죠",
          "original_form": "네 그렇긴하죠",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006823.1.12",
          "form": "멀리 이동하셔야하나요",
          "original_form": "멀리 이동하셔야하나요",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006823.1.13",
          "form": "그래도 혹 돌파감염도 걱정 되요",
          "original_form": "그래도 혹 돌파감염도 걱정 되요",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006823.1.14",
          "form": "그렇죠..변이 바이러스에",
          "original_form": "그렇죠..변이 바이러스에",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006823.1.15",
          "form": "연세가 많으신 부모님 이라서요",
          "original_form": "연세가 많으신 부모님 이라서요",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100006823.1.16",
          "form": "아니요 멀진 않아요 \n차로 한시간 걸려요",
          "original_form": "아니요 멀진 않아요 \n차로 한시간 걸려요",
