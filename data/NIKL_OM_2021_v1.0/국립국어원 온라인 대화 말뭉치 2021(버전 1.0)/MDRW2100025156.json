      "utterance": [
        {
          "id": "MDRW2100025156.1.1",
          "form": "비온다",
          "original_form": "비온다",
          "speaker_id": "1",
          "time": "20210831 23:54"
        },
        {
          "id": "MDRW2100025156.1.2",
          "form": "응 여기도 비와",
          "original_form": "응 여기도 비와",
          "speaker_id": "2",
          "time": "20210831 23:54"
        },
        {
          "id": "MDRW2100025156.1.3",
          "form": "넌 비오는 날 좋아해?",
          "original_form": "넌 비오는 날 좋아해?",
          "speaker_id": "2",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025156.1.4",
          "form": "많이와? 여긴 구멍뚫린거처럼와",
          "original_form": "많이와? 여긴 구멍뚫린거처럼와",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025156.1.5",
          "form": "집에서 보는건 좋아해",
          "original_form": "집에서 보는건 좋아해",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025156.1.6",
          "form": "여기도 장난 아니야",
          "original_form": "여기도 장난 아니야",
          "speaker_id": "2",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025156.1.7",
          "form": "여름 장마도 아닌데 엄청 내린다",
          "original_form": "여름 장마도 아닌데 엄청 내린다",
          "speaker_id": "2",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025156.1.8",
          "form": "그러게 가을장마인가",
          "original_form": "그러게 가을장마인가",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025156.1.9",
          "form": "이제 후드티 날씨인가",
          "original_form": "이제 후드티 날씨인가",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025156.1.10",
          "form": "오 맞아 아침에 많이 쌀쌀하더라",
          "original_form": "오 맞아 아침에 많이 쌀쌀하더라",
          "speaker_id": "2",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025156.1.11",
          "form": "이제 장마 지나면 기온 훅 떨어질 거 같아",
          "original_form": "이제 장마 지나면 기온 훅 떨어질 거 같아",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025156.1.12",
          "form": "맞아 이제 슬슬 옷장정리 해야될듯",
          "original_form": "맞아 이제 슬슬 옷장정리 해야될듯",
          "speaker_id": "1",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025156.1.13",
          "form": "이번주 주말부터 기온 떨어지는거같더라",
          "original_form": "이번주 주말부터 기온 떨어지는거같더라",
          "speaker_id": "1",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025156.1.14",
          "form": "와 진짜 말만 들어도 귀찮다",
          "original_form": "와 진짜 말만 들어도 귀찮다",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025156.1.15",
          "form": "이번여름은 마스크 때문에 더 힘들었던 거 같은데",
          "original_form": "이번여름은 마스크 때문에 더 힘들었던 거 같은데",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025156.1.16",
          "form": "그나마 다행인가",
          "original_form": "그나마 다행인가",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025156.1.17",
          "form": "이제 겨울엔",
          "original_form": "이제 겨울엔",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.18",
          "form": "마스크사이로 김나와",
          "original_form": "마스크사이로 김나와",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.19",
          "form": "진짜싫다",
          "original_form": "진짜싫다",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.20",
          "form": "김서리는 거 싫지",
          "original_form": "김서리는 거 싫지",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.21",
          "form": "너무 축축해져",
          "original_form": "너무 축축해져",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.22",
          "form": "이제 그런 날씨가 되어가고있구나",
          "original_form": "이제 그런 날씨가 되어가고있구나",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.23",
          "form": "미세먼지도 오겠네",
          "original_form": "미세먼지도 오겠네",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.24",
          "form": "아직 반팔 입고 다니지만ㅋㅋㅋㅋ",
          "original_form": "아직 반팔 입고 다니지만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.25",
          "form": "아악 싫다",
          "original_form": "아악 싫다",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.26",
          "form": "나도 아직 반바지입어ㅋㅋㅋ",
          "original_form": "나도 아직 반바지입어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025156.1.27",
          "form": "암튼 내일도 비온다니까 우산 잘 챙겨!",
          "original_form": "암튼 내일도 비온다니까 우산 잘 챙겨!",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100025156.1.28",
          "form": "그래 너도 내일도 우산챙겨",
          "original_form": "그래 너도 내일도 우산챙겨",
          "speaker_id": "1",
          "time": "20210831 23:59"
        }
      ]
    }
  ]
