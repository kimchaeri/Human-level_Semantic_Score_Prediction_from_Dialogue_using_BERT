      "utterance": [
        {
          "id": "MDRW2100030190.1.1",
          "form": "저는 심해요 ㅠㅠ",
          "original_form": "저는 심해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030190.1.2",
          "form": "어 저는 좀 심했다가 참을만 했다가 해요",
          "original_form": "어 저는 좀 심했다가 참을만 했다가 해요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030190.1.3",
          "form": "학생때 진짜 심했고",
          "original_form": "학생때 진짜 심했고",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030190.1.4",
          "form": "성인 되곤 좀 나아졌다가",
          "original_form": "성인 되곤 좀 나아졌다가",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030190.1.5",
          "form": "가끔씩 기절? 정도로 아플 때가 있긴 해요",
          "original_form": "가끔씩 기절? 정도로 아플 때가 있긴 해요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030190.1.6",
          "form": "손발 벌벌 떨고 쓰러지고ㅋㅋㅋ...",
          "original_form": "손발 벌벌 떨고 쓰러지고ㅋㅋㅋ...",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030190.1.7",
          "form": "와..,",
          "original_form": "와..,",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030190.1.8",
          "form": "한번씩 크게 오시나봐요,,",
          "original_form": "한번씩 크게 오시나봐요,,",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030190.1.9",
          "form": "저는 허리로 오더라구요",
          "original_form": "저는 허리로 오더라구요",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030190.1.10",
          "form": "진짜 누워있지도 못하고 서있지도 못하고,,ㅠ",
          "original_form": "진짜 누워있지도 못하고 서있지도 못하고,,ㅠ",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030190.1.11",
          "form": "헐 그거 진짜 힘들 거 같아요",
          "original_form": "헐 그거 진짜 힘들 거 같아요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030190.1.12",
          "form": "누워있지도 못하다니ㅠㅠㅠㅠㅠㅠ",
          "original_form": "누워있지도 못하다니ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030190.1.13",
          "form": "ㅠㅠㅠㅠ힘들더라구요 진짜 ",
          "original_form": "ㅠㅠㅠㅠ힘들더라구요 진짜 ",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030190.1.14",
          "form": "그래도 2일까지만 그러고 나머지 날은 괜찮아서..",
          "original_form": "그래도 2일까지만 그러고 나머지 날은 괜찮아서..",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030190.1.15",
          "form": "다행이라고 생각해요..ㅎ",
          "original_form": "다행이라고 생각해요..ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030190.1.16",
          "form": "아 마자요 보통 2~3일까지만 아프니까ㅠㅠㅠ",
          "original_form": "아 마자요 보통 2~3일까지만 아프니까ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030190.1.17",
          "form": "정말 다행이죠",
          "original_form": "정말 다행이죠",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030190.1.18",
          "form": "글고보니 전 최근에 생리통으로 두통증상이 생겼어요",
          "original_form": "글고보니 전 최근에 생리통으로 두통증상이 생겼어요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030190.1.19",
          "form": "너무 어이없지 않나용ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 어이없지 않나용ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030190.1.20",
          "form": "3일째 되는 날에 꼭 머리가 아프더라고요 뭐지...",
          "original_form": "3일째 되는 날에 꼭 머리가 아프더라고요 뭐지...",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030190.1.21",
          "form": "헐,, 너무 한거 아닌가요??",
          "original_form": "헐,, 너무 한거 아닌가요??",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030190.1.22",
          "form": "왜 두통까지 오고 난리,,",
          "original_form": "왜 두통까지 오고 난리,,",
          "speaker_id": "1",
          "time": "20210903 22:58"
