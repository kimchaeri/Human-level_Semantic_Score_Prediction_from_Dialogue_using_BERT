      "utterance": [
        {
          "id": "MDRW2100042790.1.1",
          "form": "아니 그냥... 귀찮은거 같던데",
          "original_form": "아니 그냥... 귀찮은거 같던데",
          "speaker_id": "2",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042790.1.2",
          "form": "하려고 하면 할 수 있을걸..?",
          "original_form": "하려고 하면 할 수 있을걸..?",
          "speaker_id": "2",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042790.1.3",
          "form": "우린 오히려 아빠가 이것저것 찾아보고 가입하는 편이고",
          "original_form": "우린 오히려 아빠가 이것저것 찾아보고 가입하는 편이고",
          "speaker_id": "1",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042790.1.4",
          "form": "엄마는 쿠팡 ㅇㅇ",
          "original_form": "엄마는 쿠팡 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042790.1.5",
          "form": "엊그제 머 시도하다가 안하시던데",
          "original_form": "엊그제 머 시도하다가 안하시던데",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042790.1.6",
          "form": "울아빠는 공무원 아니었으면 진작 짤려서",
          "original_form": "울아빠는 공무원 아니었으면 진작 짤려서",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042790.1.7",
          "form": "우리 길바닥에 나앉았을듯",
          "original_form": "우리 길바닥에 나앉았을듯",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042790.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그정도냐구 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그정도냐구 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042790.1.9",
          "form": "인간이.. 믿음직스럽지 못해...(파워디스",
          "original_form": "인간이.. 믿음직스럽지 못해...(파워디스",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042790.1.10",
          "form": "엄마는 알아오긴 해도 네가 해봐(나를 말함",
          "original_form": "엄마는 알아오긴 해도 네가 해봐(나를 말함",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042790.1.11",
          "form": "하는 쪽이고",
          "original_form": "하는 쪽이고",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042790.1.12",
          "form": "ㅇㅎ....",
          "original_form": "ㅇㅎ....",
          "speaker_id": "1",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042790.1.13",
          "form": "결국 내가 고생이군",
          "original_form": "결국 내가 고생이군",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042790.1.14",
          "form": "결제금액이 워낙 커서",
          "original_form": "결제금액이 워낙 커서",
          "speaker_id": "1",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042790.1.15",
          "form": "적립카드 있으면 그런걸로 긁어도 좋은데",
          "original_form": "적립카드 있으면 그런걸로 긁어도 좋은데",
          "speaker_id": "1",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042790.1.16",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042790.1.17",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042790.1.18",
          "form": "왜 짤라먹지",
          "original_form": "왜 짤라먹지",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042790.1.19",
          "form": "나 보냈잖아 근데 그건 왜 짤라먹어!!",
          "original_form": "나 보냈잖아 근데 그건 왜 짤라먹어!!",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042790.1.20",
          "form": "흐으으음",
          "original_form": "흐으으음",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042790.1.21",
          "form": "일단 좀 더 기다려봐야겠여",
          "original_form": "일단 좀 더 기다려봐야겠여",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042790.1.22",
          "form": "월말이면 좀 저렴히 풀리지 않을까 하는 희망...",
          "original_form": "월말이면 좀 저렴히 풀리지 않을까 하는 희망...",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042790.1.23",
          "form": "월말..이면 우리가 가는거 아녀? ㅋㅋㅋㅋㅋ",
          "original_form": "월말..이면 우리가 가는거 아녀? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:39"
        },
        {
          "id": "MDRW2100042790.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그 전엔 살거야...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그 전엔 살거야...",
          "speaker_id": "1",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100042790.1.25",
          "form": "한",
          "original_form": "한",
          "speaker_id": "1",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100042790.1.26",
          "form": "중순 이후쯤...?",
          "original_form": "중순 이후쯤...?",
          "speaker_id": "1",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100042790.1.27",
          "form": "추석땜에 배송 안되려나...",
          "original_form": "추석땜에 배송 안되려나...",
          "speaker_id": "1",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100042790.1.28",
          "form": "ㅋㅋ큐ㅠㅠ 추석 생각하면...",
          "original_form": "ㅋㅋ큐ㅠㅠ 추석 생각하면...",
          "speaker_id": "2",
          "time": "20210907 11:24"
        },
        {
          "id": "MDRW2100042790.1.29",
          "form": "안될거 같은데",
          "original_form": "안될거 같은데",
          "speaker_id": "2",
          "time": "20210907 11:24"
        },
        {
          "id": "MDRW2100042790.1.30",
          "form": "그 대단하던... 물류창고를 잊을 수 업ㄱ어",
          "original_form": "그 대단하던... 물류창고를 잊을 수 업ㄱ어",
          "speaker_id": "2",
          "time": "20210907 11:24"
        },
        {
          "id": "MDRW2100042790.1.31",
          "form": "맞다 중고나라 아니당근당슨",
          "original_form": "맞다 중고나라 아니당근당슨",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100042790.1.32",
          "form": "알람 울려서 봣는데",
          "original_form": "알람 울려서 봣는데",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100042790.1.33",
