      "utterance": [
        {
          "id": "MDRW2100003616.1.1",
          "form": "소래포구가서 새우튀김 10마리 사먹었는데",
          "original_form": "소래포구가서 새우튀김 10마리 사먹었는데",
          "speaker_id": "2",
          "time": "20210821 08:43"
        },
        {
          "id": "MDRW2100003616.1.2",
          "form": "너무 맛있었는데",
          "original_form": "너무 맛있었는데",
          "speaker_id": "2",
          "time": "20210821 08:43"
        },
        {
          "id": "MDRW2100003616.1.3",
          "form": "와우 엄청나게 맛있었나봐요",
          "original_form": "와우 엄청나게 맛있었나봐요",
          "speaker_id": "1",
          "time": "20210821 08:43"
        },
        {
          "id": "MDRW2100003616.1.4",
          "form": "네 맛있어요",
          "original_form": "네 맛있어요",
          "speaker_id": "2",
          "time": "20210821 08:43"
        },
        {
          "id": "MDRW2100003616.1.5",
          "form": "거기가서 다른건 안사먹었어요?",
          "original_form": "거기가서 다른건 안사먹었어요?",
          "speaker_id": "1",
          "time": "20210821 08:43"
        },
        {
          "id": "MDRW2100003616.1.6",
          "form": "그 아삭 소리나게 튀긴",
          "original_form": "그 아삭 소리나게 튀긴",
          "speaker_id": "2",
          "time": "20210821 08:44"
        },
        {
          "id": "MDRW2100003616.1.7",
          "form": "그 작은 게 튀긴거",
          "original_form": "그 작은 게 튀긴거",
          "speaker_id": "2",
          "time": "20210821 08:44"
        },
        {
          "id": "MDRW2100003616.1.8",
          "form": "그것도 샀어요",
          "original_form": "그것도 샀어요",
          "speaker_id": "2",
          "time": "20210821 08:44"
        },
        {
          "id": "MDRW2100003616.1.9",
          "form": "그게 뭐지",
          "original_form": "그게 뭐지",
          "speaker_id": "1",
          "time": "20210821 08:44"
        },
        {
          "id": "MDRW2100003616.1.10",
          "form": "알거같은데",
          "original_form": "알거같은데",
          "speaker_id": "1",
          "time": "20210821 08:44"
        },
        {
          "id": "MDRW2100003616.1.11",
          "form": "그 엄청 작은게 있자나요",
          "original_form": "그 엄청 작은게 있자나요",
          "speaker_id": "2",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.12",
          "form": "게튀김? 먹어본거같은데 뭐지 알것같은데",
          "original_form": "게튀김? 먹어본거같은데 뭐지 알것같은데",
          "speaker_id": "1",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.13",
          "form": "갯벌같은데 있는",
          "original_form": "갯벌같은데 있는",
          "speaker_id": "2",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.14",
          "form": "아아아 알아요",
          "original_form": "아아아 알아요",
          "speaker_id": "1",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.15",
          "form": "그냥 통째로 다먹는",
          "original_form": "그냥 통째로 다먹는",
          "speaker_id": "2",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.16",
          "form": "그런건 얼마해요?",
          "original_form": "그런건 얼마해요?",
          "speaker_id": "1",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.17",
          "form": "그것도 튀겨서 팔더라구요",
          "original_form": "그것도 튀겨서 팔더라구요",
          "speaker_id": "2",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.18",
          "form": "본적은 있는데 사먹어본적이 없어서",
          "original_form": "본적은 있는데 사먹어본적이 없어서",
          "speaker_id": "1",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.19",
          "form": "새우는 10마리 만원",
          "original_form": "새우는 10마리 만원",
          "speaker_id": "2",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.20",
          "form": "게는요?",
          "original_form": "게는요?",
          "speaker_id": "1",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.21",
          "form": "그건 20마리 만원?",
          "original_form": "그건 20마리 만원?",
          "speaker_id": "2",
          "time": "20210821 08:45"
        },
        {
          "id": "MDRW2100003616.1.22",
          "form": "생각보다 가격대가 있구나",
          "original_form": "생각보다 가격대가 있구나",
          "speaker_id": "1",
          "time": "20210821 08:46"
        },
        {
          "id": "MDRW2100003616.1.23",
          "form": "근데 게는 조금만 샀어요",
          "original_form": "근데 게는 조금만 샀어요",
          "speaker_id": "2",
          "time": "20210821 08:46"
        }
      ]
