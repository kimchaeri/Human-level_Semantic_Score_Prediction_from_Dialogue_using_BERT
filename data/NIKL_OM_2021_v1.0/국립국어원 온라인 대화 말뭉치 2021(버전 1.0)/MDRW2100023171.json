      "utterance": [
        {
          "id": "MDRW2100023171.1.1",
          "form": "나쵸 정답입니다",
          "original_form": "나쵸 정답입니다",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023171.1.2",
          "form": "나쵸 정답~!",
          "original_form": "나쵸 정답~!",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023171.1.3",
          "form": "나쵸에 살사소스 찍어먹었너요",
          "original_form": "나쵸에 살사소스 찍어먹었너요",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023171.1.4",
          "form": "와 아싸 ㅎㅎㅎㅎㅎ",
          "original_form": "와 아싸 ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023171.1.5",
          "form": "헉 맛있겠어요",
          "original_form": "헉 맛있겠어요",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023171.1.6",
          "form": "바삭바삭",
          "original_form": "바삭바삭",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023171.1.7",
          "form": "맞아요 바삭바삭 나쵸",
          "original_form": "맞아요 바삭바삭 나쵸",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023171.1.8",
          "form": "살사소스 찍어먹었어요 ㅎㅎ",
          "original_form": "살사소스 찍어먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023171.1.9",
          "form": "이번엔 제가 문제 낼게요",
          "original_form": "이번엔 제가 문제 낼게요",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023171.1.10",
          "form": "ㅋㅋㅋㅋ좋아요",
          "original_form": "ㅋㅋㅋㅋ좋아요",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023171.1.11",
          "form": "식사류입니다",
          "original_form": "식사류입니다",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023171.1.12",
          "form": "시작!",
          "original_form": "시작!",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023171.1.13",
          "form": "최근에 언제 드셨나요?",
          "original_form": "최근에 언제 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023171.1.14",
          "form": "먹은 지 얼마 안 됐어요",
          "original_form": "먹은 지 얼마 안 됐어요",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023171.1.15",
          "form": "반 년도 안 됐습니다",
          "original_form": "반 년도 안 됐습니다",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023171.1.16",
          "form": "반년 안 된 거면 얼마 안 된 건가요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "반년 안 된 거면 얼마 안 된 건가요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023171.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ정확히 기억이 안 나요! 두어 달 전인 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ정확히 기억이 안 나요! 두어 달 전인 것 같아요",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023171.1.18",
          "form": "자주 먹을 수 있는 음식은 아닌가 봐요?",
          "original_form": "자주 먹을 수 있는 음식은 아닌가 봐요?",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023171.1.19",
          "form": "ㅋㅋㅋ아잌 저보고 언제 먹었냐고 물으시길래ㅋㅋㅋ자주 먹을 수 있어요 흔합니다!",
          "original_form": "ㅋㅋㅋ아잌 저보고 언제 먹었냐고 물으시길래ㅋㅋㅋ자주 먹을 수 있어요 흔합니다!",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023171.1.20",
          "form": "가격대는 1인당 어느정도인가요!",
          "original_form": "가격대는 1인당 어느정도인가요!",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023171.1.21",
          "form": "저렴하면 7천 원 ~ 비싸면 만원 초중반대 정도 한답니다",
          "original_form": "저렴하면 7천 원 ~ 비싸면 만원 초중반대 정도 한답니다",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023171.1.22",
          "form": "한식인가요?",
          "original_form": "한식인가요?",
          "speaker_id": "1",
          "time": "20210829 22:34"
        }
      ]
