      "utterance": [
        {
          "id": "MDRW2100007186.1.1",
          "form": "뉴욕에서 먹은건 기억나는거없음",
          "original_form": "뉴욕에서 먹은건 기억나는거없음",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007186.1.2",
          "form": "안먹었음?",
          "original_form": "안먹었음?",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007186.1.3",
          "form": "큰누나랑 둘이서 화이트와인하나시키고 머먹었었는데 둘이서 11만원인가나왓엇",
          "original_form": "큰누나랑 둘이서 화이트와인하나시키고 머먹었었는데 둘이서 11만원인가나왓엇",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007186.1.4",
          "form": "나는 베이글이랑 치즈케이크가 맛있었다",
          "original_form": "나는 베이글이랑 치즈케이크가 맛있었다",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007186.1.5",
          "form": "근데 거기 분위기 겁나좋았는데 나중에 생각해보니",
          "original_form": "근데 거기 분위기 겁나좋았는데 나중에 생각해보니",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.6",
          "form": "와 비싸네 ㅡㅡ 와인을 비싼거 먹은거아님?",
          "original_form": "와 비싸네 ㅡㅡ 와인을 비싼거 먹은거아님?",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.7",
          "form": "11만원이면 겁나싸게 나온듯 ㅇㅇ 와인한병다먹었는데",
          "original_form": "11만원이면 겁나싸게 나온듯 ㅇㅇ 와인한병다먹었는데",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.8",
          "form": "아하 그렇군",
          "original_form": "아하 그렇군",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.9",
          "form": "???먼솔",
          "original_form": "???먼솔",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.10",
          "form": "비싼것같은뎅",
          "original_form": "비싼것같은뎅",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.11",
          "form": "와인한병에 이거저거 에 분위기좋고해서 한국같으면",
          "original_form": "와인한병에 이거저거 에 분위기좋고해서 한국같으면",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.12",
          "form": "뉴욕에 난 언니야랑 갔을때 엄청 궁핍했다",
          "original_form": "뉴욕에 난 언니야랑 갔을때 엄청 궁핍했다",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.13",
          "form": "20만원즘나왔을듯?",
          "original_form": "20만원즘나왔을듯?",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.14",
          "form": "그래도 그런 재미가있지",
          "original_form": "그래도 그런 재미가있지",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.15",
          "form": "ㅇㅇ 그때 큰누나 궁핍스",
          "original_form": "ㅇㅇ 그때 큰누나 궁핍스",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.16",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.17",
          "form": "그거말곤 먹은거 생각안남",
          "original_form": "그거말곤 먹은거 생각안남",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007186.1.18",
          "form": "돈좀 많이 벌어라 여행가서 행복하게",
          "original_form": "돈좀 많이 벌어라 여행가서 행복하게",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007186.1.19",
          "form": "나도 가난하게 다녔어서",
          "original_form": "나도 가난하게 다녔어서",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007186.1.20",
          "form": "주말인데 어디 놀러안갈래?",
          "original_form": "주말인데 어디 놀러안갈래?",
