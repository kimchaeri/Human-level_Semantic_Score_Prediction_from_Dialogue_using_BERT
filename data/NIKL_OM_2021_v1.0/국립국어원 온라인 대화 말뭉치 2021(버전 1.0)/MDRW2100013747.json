        },
        {
          "id": "MDRW2100013747.1.2",
          "form": "가을이 왔어요",
          "original_form": "가을이 왔어요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013747.1.3",
          "form": "작년에비해 빨리온것같아요",
          "original_form": "작년에비해 빨리온것같아요",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013747.1.4",
          "form": "이미",
          "original_form": "이미",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013747.1.5",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013747.1.6",
          "form": "입추 지났어요",
          "original_form": "입추 지났어요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013747.1.7",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013747.1.8",
          "form": "그래도 작년엔 더웠던것같았거든요",
          "original_form": "그래도 작년엔 더웠던것같았거든요",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013747.1.9",
          "form": "2018년엔 8월 24일에도 넘더웠는디",
          "original_form": "2018년엔 8월 24일에도 넘더웠는디",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013747.1.10",
          "form": "이젠 긴팔입어야겠어요",
          "original_form": "이젠 긴팔입어야겠어요",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013747.1.11",
          "form": "그때 아팠죠 제가",
          "original_form": "그때 아팠죠 제가",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013747.1.12",
          "form": "저도요.",
          "original_form": "저도요.",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013747.1.13",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013747.1.14",
          "form": "그렇군여",
          "original_form": "그렇군여",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013747.1.15",
          "form": "에어컨 틀어서 그거하나는 좋았어요",
          "original_form": "에어컨 틀어서 그거하나는 좋았어요",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013747.1.16",
          "form": "무슨 병명이..?",
          "original_form": "무슨 병명이..?",
