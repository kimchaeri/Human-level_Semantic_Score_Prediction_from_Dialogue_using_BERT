      "utterance": [
        {
          "id": "MDRW2100034907.1.1",
          "form": "아아 저도 그런곳에서 먹어보고싶은데",
          "original_form": "아아 저도 그런곳에서 먹어보고싶은데",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100034907.1.2",
          "form": "맛있나요???",
          "original_form": "맛있나요???",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100034907.1.3",
          "form": "판매자 따라서 퀄리티가 차이 많이나요!",
          "original_form": "판매자 따라서 퀄리티가 차이 많이나요!",
          "speaker_id": "1",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100034907.1.4",
          "form": "동네에 유명한 곳은 문어도 크고 맛있더라구요",
          "original_form": "동네에 유명한 곳은 문어도 크고 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100034907.1.5",
          "form": "오오 저희 동네는 음식트럭이 안오더라구요 ㅜㅠ",
          "original_form": "오오 저희 동네는 음식트럭이 안오더라구요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100034907.1.6",
          "form": "막 야채곱창 그런것도 오나요?",
          "original_form": "막 야채곱창 그런것도 오나요?",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100034907.1.7",
          "form": "네네 가끔 순대랑 야채곱창도 오는데",
          "original_form": "네네 가끔 순대랑 야채곱창도 오는데",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100034907.1.8",
          "form": "그런건 안사먹어봤어요",
          "original_form": "그런건 안사먹어봤어요",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100034907.1.9",
          "form": "그런것도 괜찮다던데",
          "original_form": "그런것도 괜찮다던데",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100034907.1.10",
          "form": "도전하기 좀 겁나죠 ㅋㅋ맛 없을까봐ㅠㅠ",
          "original_form": "도전하기 좀 겁나죠 ㅋㅋ맛 없을까봐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100034907.1.11",
          "form": "네네 그리고 잘못사면 냄새도 좀 나더라구요",
          "original_form": "네네 그리고 잘못사면 냄새도 좀 나더라구요",
          "speaker_id": "1",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100034907.1.12",
          "form": "맞네요 ㅠㅠ 하필 둘 다 냄새 나는군요 ㅋㅋ",
          "original_form": "맞네요 ㅠㅠ 하필 둘 다 냄새 나는군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034907.1.13",
          "form": "가격은 어떤편이에요?",
          "original_form": "가격은 어떤편이에요?",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034907.1.14",
          "form": "가격은 식당보다는 저렴한거 같아요!",
          "original_form": "가격은 식당보다는 저렴한거 같아요!",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100034907.1.15",
          "form": "오오 ㅎㅎ 먹어보고싶네요",
          "original_form": "오오 ㅎㅎ 먹어보고싶네요",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034907.1.16",
          "form": "보통 아파트앞쪽으로 오죠 ???",
          "original_form": "보통 아파트앞쪽으로 오죠 ???",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034907.1.17",
          "form": "네네 아파트에 장 설때 많이 와요",
          "original_form": "네네 아파트에 장 설때 많이 와요",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034907.1.18",
          "form": "전 주택 살아서ㅠㅠ 볼수가 없네요",
          "original_form": "전 주택 살아서ㅠㅠ 볼수가 없네요",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034907.1.19",
          "form": "아 ㅠㅠ",
          "original_form": "아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034907.1.20",
          "form": "그래도 타코야끼 맨날 사먹을 수 있어서",
          "original_form": "그래도 타코야끼 맨날 사먹을 수 있어서",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034907.1.21",
          "form": "좋은거같아요 ㅋㅋㅋ",
          "original_form": "좋은거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034907.1.22",
          "form": "여기 언제 나타날지 모르는 트럭을 기다려야해요 ㅋㅋㅋ",
          "original_form": "여기 언제 나타날지 모르는 트럭을 기다려야해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100034907.1.23",
          "form": "맞아요 ㅋㅋ 저희 집쪽은 배달은 많이 되는데 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ 저희 집쪽은 배달은 많이 되는데 ㅋㅋ",
