      "utterance": [
        {
          "id": "MDRW2100017835.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017835.1.2",
          "form": "무슨 책 좋아하세요?",
          "original_form": "무슨 책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017835.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017835.1.4",
          "form": "과학이랑 스릴러 좋아합니다!",
          "original_form": "과학이랑 스릴러 좋아합니다!",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017835.1.5",
          "form": "그리고 시리즈물도 좋아해요~",
          "original_form": "그리고 시리즈물도 좋아해요~",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017835.1.6",
          "form": "오 스릴러!",
          "original_form": "오 스릴러!",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017835.1.7",
          "form": "가장 최근에 본 책 제목이 뭐였나요?",
          "original_form": "가장 최근에 본 책 제목이 뭐였나요?",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017835.1.8",
          "form": "최근에 본 책이라면",
          "original_form": "최근에 본 책이라면",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017835.1.9",
          "form": "용의자 X의 헌신과 편지라는 책을 읽었어요!",
          "original_form": "용의자 X의 헌신과 편지라는 책을 읽었어요!",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017835.1.10",
          "form": "아아 저 용의자 X 봤어요",
          "original_form": "아아 저 용의자 X 봤어요",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017835.1.11",
          "form": "근데 책 말고 영화로 봤어요",
          "original_form": "근데 책 말고 영화로 봤어요",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017835.1.12",
          "form": "아하 개인적으로 책이 좀 더 좋아요!",
          "original_form": "아하 개인적으로 책이 좀 더 좋아요!",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017835.1.13",
          "form": "책이랑 영화랑 줄거리 똑같나요?",
          "original_form": "책이랑 영화랑 줄거리 똑같나요?",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017835.1.14",
          "form": "영화는 중간에 삭제되는게",
          "original_form": "영화는 중간에 삭제되는게",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.15",
          "form": "아 다르군요",
          "original_form": "아 다르군요",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.16",
          "form": "좀 많아서",
          "original_form": "좀 많아서",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.17",
          "form": "2시간안으로 내용을 압축해야하기에",
          "original_form": "2시간안으로 내용을 압축해야하기에",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.18",
          "form": "아아 짧으니까 영화는 ㅠㅠ",
          "original_form": "아아 짧으니까 영화는 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.19",
          "form": "중요한게 날라가고 그럴수도 있어서",
          "original_form": "중요한게 날라가고 그럴수도 있어서",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.20",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.21",
          "form": "책이 좀 더 좋긴합니다!",
          "original_form": "책이 좀 더 좋긴합니다!",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.22",
          "form": "결말은 같나요 ㅠㅠ",
          "original_form": "결말은 같나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.23",
          "form": "그래서 전 책 먼저 읽고 추후에 영화를 보러가곤합니다",
          "original_form": "그래서 전 책 먼저 읽고 추후에 영화를 보러가곤합니다",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.24",
          "form": "네 결말은 같아요",
          "original_form": "네 결말은 같아요",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.25",
          "form": "남주 넘 맘 아팠어요 ㅠㅠ",
          "original_form": "남주 넘 맘 아팠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.26",
          "form": "그렇죠..ㅠㅠ",
          "original_form": "그렇죠..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.27",
          "form": "다르길 바랬는데 ㅠㅠ",
          "original_form": "다르길 바랬는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017835.1.28",
          "form": "저도 책 읽어야하는데ㅠㅠ",
          "original_form": "저도 책 읽어야하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017835.1.29",
          "form": "안 읽은지 백만년 됐네요 ㅠㅠ",
          "original_form": "안 읽은지 백만년 됐네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        }
      ]
    }
  ]
