      "utterance": [
        {
          "id": "MDRW2100027549.1.1",
          "form": "ㅋㅋㅋㅋㅋ근데 왜 바빠요",
          "original_form": "ㅋㅋㅋㅋㅋ근데 왜 바빠요",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027549.1.2",
          "form": "서비스 직업이라서??",
          "original_form": "서비스 직업이라서??",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027549.1.3",
          "form": "ㅋㅋ서비스직 피곤하죠",
          "original_form": "ㅋㅋ서비스직 피곤하죠",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027549.1.4",
          "form": "저도 사무이긴한데",
          "original_form": "저도 사무이긴한데",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027549.1.5",
          "form": "반 서비스라 피곤",
          "original_form": "반 서비스라 피곤",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027549.1.6",
          "form": "어우 사람상대가",
          "original_form": "어우 사람상대가",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027549.1.7",
          "form": "젤힘들어요",
          "original_form": "젤힘들어요",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027549.1.8",
          "form": "99명의 정상과",
          "original_form": "99명의 정상과",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027549.1.9",
          "form": "1명의 비정상",
          "original_form": "1명의 비정상",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027549.1.10",
          "form": "진짜듣기만해도",
          "original_form": "진짜듣기만해도",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027549.1.11",
          "form": "스트레스네요 ㅋㅋ",
          "original_form": "스트레스네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027549.1.12",
          "form": "1명이 99의 스트레스ㅋㅋ",
          "original_form": "1명이 99의 스트레스ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027549.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 남한테 피해주는거 젤싫어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 남한테 피해주는거 젤싫어요",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027549.1.14",
          "form": "어떤 직종이신지 모르겠는데",
          "original_form": "어떤 직종이신지 모르겠는데",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027549.1.15",
          "form": "노인 취객",
          "original_form": "노인 취객",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027549.1.16",
          "form": "진짜 짜증",
          "original_form": "진짜 짜증",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027549.1.17",
          "form": "저울어요 그럼 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저울어요 그럼 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027549.1.18",
          "form": "ㅋㅋ갈수록 덤덤해지는듯요",
          "original_form": "ㅋㅋ갈수록 덤덤해지는듯요",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027549.1.19",
          "form": "아효 또 시작이네 하고",
          "original_form": "아효 또 시작이네 하고",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027549.1.20",
          "form": "그러킨한데 그래도 울어요 ㅠㅠ",
          "original_form": "그러킨한데 그래도 울어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027549.1.21",
          "form": "아직 어리신거도 있을거 같고",
          "original_form": "아직 어리신거도 있을거 같고",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027549.1.22",
          "form": "성품이 여려서도?",
          "original_form": "성품이 여려서도?",
