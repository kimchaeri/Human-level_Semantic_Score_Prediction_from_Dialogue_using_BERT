      "utterance": [
        {
          "id": "MDRW2100014879.1.1",
          "form": "책많이 읽으세요?",
          "original_form": "책많이 읽으세요?",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014879.1.2",
          "form": "최근에는 많이 못 보고 있어요",
          "original_form": "최근에는 많이 못 보고 있어요",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014879.1.3",
          "form": "전 제태크책만 많이읽어요ㅎㅎ",
          "original_form": "전 제태크책만 많이읽어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014879.1.4",
          "form": "오 주식이나 부동산 관련 책 보시는 건가요?",
          "original_form": "오 주식이나 부동산 관련 책 보시는 건가요?",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014879.1.5",
          "form": "저도 그쪽으로 공부 좀 해야는데",
          "original_form": "저도 그쪽으로 공부 좀 해야는데",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014879.1.6",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014879.1.7",
          "form": "코인이요ㅎㅎ",
          "original_form": "코인이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014879.1.8",
          "form": "저는 투자쪽으로는 잼뱅이라ㅎㅎ",
          "original_form": "저는 투자쪽으로는 잼뱅이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014879.1.9",
          "form": "지키는것만 공부해요ㅎㅎ",
          "original_form": "지키는것만 공부해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014879.1.10",
          "form": "코인! 혹시 책 제목이 뭔가요?",
          "original_form": "코인! 혹시 책 제목이 뭔가요?",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014879.1.11",
          "form": "코인은 이론공부였어요ㅎ 엘리언트파동이론이요ㅎ",
          "original_form": "코인은 이론공부였어요ㅎ 엘리언트파동이론이요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014879.1.12",
          "form": "그리고 제태크 책은  밍키언니의 돈계획?",
          "original_form": "그리고 제태크 책은  밍키언니의 돈계획?",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014879.1.13",
          "form": "저도 좀 찾아봐야겠네요",
          "original_form": "저도 좀 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014879.1.14",
          "form": "한번봐보셔요ㅎㅎ",
          "original_form": "한번봐보셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014879.1.15",
          "form": "오디오북 이용해 보신 적 있으세요?",
          "original_form": "오디오북 이용해 보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014879.1.16",
          "form": "제테크는 항상준비해야하는듯요ㅎ",
          "original_form": "제테크는 항상준비해야하는듯요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014879.1.17",
          "form": "전 가끔 자기 전에 듣는데 사실 집중 안 하면 뭐라고 했는지 기억이 잘 안 나더라구요",
          "original_form": "전 가끔 자기 전에 듣는데 사실 집중 안 하면 뭐라고 했는지 기억이 잘 안 나더라구요",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014879.1.18",
          "form": "오디오북 단점이네요ㅎ",
          "original_form": "오디오북 단점이네요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014879.1.19",
          "form": "오디오북은 좋나요?",
          "original_form": "오디오북은 좋나요?",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014879.1.20",
          "form": "저도한번 해볼까해거",
          "original_form": "저도한번 해볼까해거",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014879.1.21",
          "form": "조용한 데서 들으면 좋더라구요",
          "original_form": "조용한 데서 들으면 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014879.1.22",
          "form": "확실히 기계가 읽어주는 것보단 사람이 낭독해주는 게 더 좋아요",
          "original_form": "확실히 기계가 읽어주는 것보단 사람이 낭독해주는 게 더 좋아요",
          "speaker_id": "1",
          "time": "20210825 21:18"
        }
      ]
