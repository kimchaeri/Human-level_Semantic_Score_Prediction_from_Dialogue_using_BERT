      "utterance": [
        {
          "id": "MDRW2100025020.1.1",
          "form": "핫도그 파는 쥬씨도 있잖아요",
          "original_form": "핫도그 파는 쥬씨도 있잖아요",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100025020.1.2",
          "form": "팔팔핫도그인가",
          "original_form": "팔팔핫도그인가",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100025020.1.3",
          "form": "메뉴 추천해주세요 내일먹어봐야지",
          "original_form": "메뉴 추천해주세요 내일먹어봐야지",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100025020.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100025020.1.5",
          "form": "저는 고규마!",
          "original_form": "저는 고규마!",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100025020.1.6",
          "form": "명랑최애메뉴로",
          "original_form": "명랑최애메뉴로",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100025020.1.7",
          "form": "저도 자주먹어본 게 아니라 풀네임이 생각안나는데",
          "original_form": "저도 자주먹어본 게 아니라 풀네임이 생각안나는데",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100025020.1.8",
          "form": "하나만 드실 거 아니죠?",
          "original_form": "하나만 드실 거 아니죠?",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100025020.1.9",
          "form": "일단 통모자랑 고구마붙은 거 드셔야 하구요",
          "original_form": "일단 통모자랑 고구마붙은 거 드셔야 하구요",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100025020.1.10",
          "form": "갈비맛소세지 좋아하시면 갈비핫도그있어요",
          "original_form": "갈비맛소세지 좋아하시면 갈비핫도그있어요",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100025020.1.11",
          "form": "오....!통모짜....고구마...",
          "original_form": "오....!통모짜....고구마...",
          "speaker_id": "2",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100025020.1.12",
          "form": "메모...",
          "original_form": "메모...",
          "speaker_id": "2",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100025020.1.13",
          "form": "그리구",
          "original_form": "그리구",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100025020.1.14",
          "form": "가래떡핫도그도 맛있다던데.....",
          "original_form": "가래떡핫도그도 맛있다던데.....",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100025020.1.15",
          "form": "헐 가래떡 좋아요",
          "original_form": "헐 가래떡 좋아요",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100025020.1.16",
          "form": "저는 핫도그는 탄수화물+탄수화물 조합으론 가지 않는 사람이라 먹어보지 않았습니다",
          "original_form": "저는 핫도그는 탄수화물+탄수화물 조합으론 가지 않는 사람이라 먹어보지 않았습니다",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100025020.1.17",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ 그쵸 핫도그는 고기들어가줘야하는데",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ 그쵸 핫도그는 고기들어가줘야하는데",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100025020.1.18",
          "form": "고구마는 반죽 겉에 붙어있는 것이니 봐주세요",
          "original_form": "고구마는 반죽 겉에 붙어있는 것이니 봐주세요",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100025020.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100025020.1.20",
          "form": "네네네 찾아봐볼게요",
          "original_form": "네네네 찾아봐볼게요",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100025020.1.21",
          "form": "제가 맛집탐방 이런거완전 좋아하는데",
          "original_form": "제가 맛집탐방 이런거완전 좋아하는데",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100025020.1.22",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100025020.1.23",
          "form": "이상하게 프차 이런데는 잘 안가고 배달을 잘 안시켜먹어서",
          "original_form": "이상하게 프차 이런데는 잘 안가고 배달을 잘 안시켜먹어서",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100025020.1.24",
          "form": "오오...",
          "original_form": "오오...",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100025020.1.25",
          "form": "치킨이랑 떡볶이 핫도그 이런거를 자주 안먹어봤어요 ㅋㄱㅋㅋㅋ",
          "original_form": "치킨이랑 떡볶이 핫도그 이런거를 자주 안먹어봤어요 ㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
