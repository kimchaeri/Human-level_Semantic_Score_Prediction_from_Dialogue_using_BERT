      "utterance": [
        {
          "id": "MDRW2100026477.1.1",
          "form": "토마토를 좋아하시나요?",
          "original_form": "토마토를 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026477.1.2",
          "form": "토마토주스 맛있어요",
          "original_form": "토마토주스 맛있어요",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026477.1.3",
          "form": "저도 그렇게 생각해요",
          "original_form": "저도 그렇게 생각해요",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026477.1.4",
          "form": "토마토 소스 파스타는 어떠신가요?",
          "original_form": "토마토 소스 파스타는 어떠신가요?",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026477.1.5",
          "form": "전 크림이 더 좋아요ㅜㅠ",
          "original_form": "전 크림이 더 좋아요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026477.1.6",
          "form": "죄송해요ㅜㅠ",
          "original_form": "죄송해요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026477.1.7",
          "form": "아니에요 전 토마토가 아닌걸요 ㅋㅋㅋㅋ",
          "original_form": "아니에요 전 토마토가 아닌걸요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026477.1.8",
          "form": "음 그럼 토마토 계란 볶음은 좋아하세요?",
          "original_form": "음 그럼 토마토 계란 볶음은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026477.1.9",
          "form": "중국요리에요",
          "original_form": "중국요리에요",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026477.1.10",
          "form": "헐 안먹어봤는데",
          "original_form": "헐 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026477.1.11",
          "form": "벌써 맛있을거같아요",
          "original_form": "벌써 맛있을거같아요",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026477.1.12",
          "form": "배고파요",
          "original_form": "배고파요",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026477.1.13",
          "form": "맛있어요 재료비도 저렴하고",
          "original_form": "맛있어요 재료비도 저렴하고",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026477.1.14",
          "form": "중국 동북지방 밥반찬이에요",
          "original_form": "중국 동북지방 밥반찬이에요",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026477.1.15",
          "form": "반찬이에요??",
          "original_form": "반찬이에요??",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026477.1.16",
          "form": "전 주식으로 먹을수있을것같아요ㅋㅋ",
          "original_form": "전 주식으로 먹을수있을것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026477.1.17",
          "form": "네 밥이랑 먹어요 스크렘블 에그같은 ㅁ사이에요",
          "original_form": "네 밥이랑 먹어요 스크렘블 에그같은 ㅁ사이에요",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026477.1.18",
          "form": "주식으로도 먹을 수 있는데 탄수화물이 없으니",
          "original_form": "주식으로도 먹을 수 있는데 탄수화물이 없으니",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026477.1.19",
          "form": "밥을 먹어야 좀 든든해요",
          "original_form": "밥을 먹어야 좀 든든해요",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026477.1.20",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026477.1.21",
          "form": "짭잘하게 만들어야겠네요",
          "original_form": "짭잘하게 만들어야겠네요",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026477.1.22",
          "form": "설턍을 치면 달달하고 소금치면 좀 짭짤하구",
          "original_form": "설턍을 치면 달달하고 소금치면 좀 짭짤하구",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026477.1.23",
          "form": "제가 먹은건 단 버전이었어요",
          "original_form": "제가 먹은건 단 버전이었어요",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026477.1.24",
          "form": "헉 전 설탕파 가겠습니다",
          "original_form": "헉 전 설탕파 가겠습니다",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026477.1.25",
          "form": "토마토엔 역시 설탕이죠",
          "original_form": "토마토엔 역시 설탕이죠",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026477.1.26",
          "form": "네 ㅋㅋ 너무 달면 좀 그런데 적당하 달아도 밥이랑 먹기조았어요",
          "original_form": "네 ㅋㅋ 너무 달면 좀 그런데 적당하 달아도 밥이랑 먹기조았어요",
          "speaker_id": "2",
          "time": "20210901 22:41"
        }
      ]
    }
