      "utterance": [
        {
          "id": "MDRW2100018277.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018277.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018277.1.3",
          "form": "최근 구매한 물건이 무엇인가요",
          "original_form": "최근 구매한 물건이 무엇인가요",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018277.1.4",
          "form": "선물받고 싶은거 있으세요?",
          "original_form": "선물받고 싶은거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018277.1.5",
          "form": "음 저는 데스크톱을 선물받고싶네요",
          "original_form": "음 저는 데스크톱을 선물받고싶네요",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018277.1.6",
          "form": "데스크톱 좋죠 요즘 뭐가 비싸다고 그런말을 봤었는데 이제 괜찮은지 모르겠네요",
          "original_form": "데스크톱 좋죠 요즘 뭐가 비싸다고 그런말을 봤었는데 이제 괜찮은지 모르겠네요",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018277.1.7",
          "form": "저는 최근에 남방을 구매했어요",
          "original_form": "저는 최근에 남방을 구매했어요",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018277.1.8",
          "form": "날이 쌀쌀해져서 걸칠게 필요하더라구요",
          "original_form": "날이 쌀쌀해져서 걸칠게 필요하더라구요",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018277.1.9",
          "form": "이제 곧 가을이니 저도 자켓같은거 하나 사야할거같네요",
          "original_form": "이제 곧 가을이니 저도 자켓같은거 하나 사야할거같네요",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018277.1.10",
          "form": "어떤색 자켓 생각하세요?",
          "original_form": "어떤색 자켓 생각하세요?",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018277.1.11",
          "form": "검은색으로 생각중인데",
          "original_form": "검은색으로 생각중인데",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018277.1.12",
          "form": "무난한 블루종을 살까",
          "original_form": "무난한 블루종을 살까",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018277.1.13",
          "form": "가디건을 살까 고민중이에요",
          "original_form": "가디건을 살까 고민중이에요",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018277.1.14",
          "form": "자켓이랑 가디건중에 어떤걸 주로 입으세요? 저는 가디건이 훨씬 많아요",
          "original_form": "자켓이랑 가디건중에 어떤걸 주로 입으세요? 저는 가디건이 훨씬 많아요",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018277.1.15",
          "form": "저는 코트나 패딩말고 걸치는걸 별로 안좋아했어서",
          "original_form": "저는 코트나 패딩말고 걸치는걸 별로 안좋아했어서",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018277.1.16",
          "form": "편한 가디건이 좋을거 같네요",
          "original_form": "편한 가디건이 좋을거 같네요",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018277.1.17",
          "form": "선호하는 브랜드 있으세요?저는 없는데 특정 브랜드마누입으시는분들도 더러 계시더라구요",
          "original_form": "선호하는 브랜드 있으세요?저는 없는데 특정 브랜드마누입으시는분들도 더러 계시더라구요",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018277.1.18",
          "form": "티셔츠나 맨투맨같은데는 선호하는 브랜드가있는데",
          "original_form": "티셔츠나 맨투맨같은데는 선호하는 브랜드가있는데",
          "speaker_id": "1",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018277.1.19",
          "form": "자켓이나 가디건은 솔직히",
          "original_form": "자켓이나 가디건은 솔직히",
          "speaker_id": "1",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018277.1.20",
          "form": "가격만 보는거같아요",
          "original_form": "가격만 보는거같아요",
          "speaker_id": "1",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018277.1.21",
          "form": "특정 로고나 레터링을 나타내는게 아니라",
          "original_form": "특정 로고나 레터링을 나타내는게 아니라",
          "speaker_id": "1",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018277.1.22",
          "form": "가격 제일 중요하죠",
          "original_form": "가격 제일 중요하죠",
          "speaker_id": "2",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100018277.1.23",
          "form": "보세나 브랜드나 큰 차이를 못느껴서",
          "original_form": "보세나 브랜드나 큰 차이를 못느껴서",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018277.1.24",
          "form": "가디건은 인터넷으로 사시나요",
          "original_form": "가디건은 인터넷으로 사시나요",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018277.1.25",
          "form": "맞아요 텍갈이도 심하구요 ㅠ",
          "original_form": "맞아요 텍갈이도 심하구요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018277.1.26",
          "form": "아니면 직접 보고 사시나요?",
          "original_form": "아니면 직접 보고 사시나요?",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018277.1.27",
          "form": "네 거의 모든 쇼핑을 인터넷으로 해요ㅋㅋㅋ",
          "original_form": "네 거의 모든 쇼핑을 인터넷으로 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018277.1.28",
          "form": "쇼핑 너무 힘들어요..",
          "original_form": "쇼핑 너무 힘들어요..",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018277.1.29",
          "form": "최대한 후기 열심히 찾아보고 확대샷 주로 보고 사요",
          "original_form": "최대한 후기 열심히 찾아보고 확대샷 주로 보고 사요",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018277.1.30",
          "form": "인터넷쇼핑 어디서 하시나요?",
          "original_form": "인터넷쇼핑 어디서 하시나요?",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018277.1.31",
          "form": "아울렛이나 십일번가 지마켓 이런데에 검색해서 사요!",
          "original_form": "아울렛이나 십일번가 지마켓 이런데에 검색해서 사요!",
          "speaker_id": "2",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100018277.1.32",
          "form": "주로 가격적인 부분만 보고 사시나요?",
          "original_form": "주로 가격적인 부분만 보고 사시나요?",
          "speaker_id": "1",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100018277.1.33",
          "form": "그건 아니지만 비슷한 디자인이라면 조금 더 저렴한걸 구입하죠 대신 후기를 엄청 꼼꼼히 봐요 ㅋㅋㅋㅋ",
          "original_form": "그건 아니지만 비슷한 디자인이라면 조금 더 저렴한걸 구입하죠 대신 후기를 엄청 꼼꼼히 봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018277.1.34",
          "form": "후기 중요하죠",
          "original_form": "후기 중요하죠",
          "speaker_id": "1",
