      "utterance": [
        {
          "id": "MDRW2100043795.1.1",
          "form": "오옹 안녕하세요",
          "original_form": "오옹 안녕하세요",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043795.1.2",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043795.1.3",
          "form": "이야 먹었지요",
          "original_form": "이야 먹었지요",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043795.1.4",
          "form": "콩나물국에",
          "original_form": "콩나물국에",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043795.1.5",
          "form": "쇠고기를 넣어먹었답니다",
          "original_form": "쇠고기를 넣어먹었답니다",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043795.1.6",
          "form": "님도 드셨는지용",
          "original_form": "님도 드셨는지용",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043795.1.7",
          "form": "네 전 볶음밥 먹었어요",
          "original_form": "네 전 볶음밥 먹었어요",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043795.1.8",
          "form": "어떤 음식 좋아하시나요?",
          "original_form": "어떤 음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043795.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043795.1.10",
          "form": "무슨볶음밥이었나요",
          "original_form": "무슨볶음밥이었나요",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043795.1.11",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043795.1.12",
          "form": "김치찜이땽기네요",
          "original_form": "김치찜이땽기네요",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043795.1.13",
          "form": "냠",
          "original_form": "냠",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043795.1.14",
          "form": "김치찜 맛있죠",
          "original_form": "김치찜 맛있죠",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043795.1.15",
          "form": "요리 잘하시나요?",
          "original_form": "요리 잘하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043795.1.16",
          "form": "헉아니요",
          "original_form": "헉아니요",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043795.1.17",
          "form": "저는그",
          "original_form": "저는그",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043795.1.18",
          "form": "죽여주는 김치찜",
          "original_form": "죽여주는 김치찜",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043795.1.19",
          "form": "이라는 식당아시는지요",
          "original_form": "이라는 식당아시는지요",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043795.1.20",
          "form": "거기 김치찜이 정말 최고로 맛있답니다",
          "original_form": "거기 김치찜이 정말 최고로 맛있답니다",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043795.1.21",
          "form": "프랜차이즈인가요??",
          "original_form": "프랜차이즈인가요??",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043795.1.22",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043795.1.23",
          "form": "아마도그럴듯하네요",
          "original_form": "아마도그럴듯하네요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043795.1.24",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043795.1.25",
          "form": "(머쓱",
          "original_form": "(머쓱",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043795.1.26",
          "form": "나중이 찾아볼께요ㅋㅋㅋㅋ",
          "original_form": "나중이 찾아볼께요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043795.1.27",
          "form": "김치찜 또 맛있게 하시는 곳 아시나요??",
          "original_form": "김치찜 또 맛있게 하시는 곳 아시나요??",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043795.1.28",
          "form": "헿",
          "original_form": "헿",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043795.1.29",
          "form": "오오..",
          "original_form": "오오..",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043795.1.30",
          "form": "저희집이요?",
          "original_form": "저희집이요?",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043795.1.31",
          "form": "(머쓱",
          "original_form": "(머쓱",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043795.1.32",
          "form": "어머니가 참 잘하신답니다....",
          "original_form": "어머니가 참 잘하신답니다....",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043795.1.33",
          "form": "하하...핳...",
          "original_form": "하하...핳...",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043795.1.34",
          "form": "오 부럽네요",
          "original_form": "오 부럽네요",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043795.1.35",
          "form": "껄껄",
          "original_form": "껄껄",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043795.1.36",
          "form": "저는 요리를",
          "original_form": "저는 요리를",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043795.1.37",
          "form": "계란말이",
          "original_form": "계란말이",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043795.1.38",
          "form": "잘하네요",
          "original_form": "잘하네요",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043795.1.39",
          "form": "계란말이 만들기 쉽지 않은데",
          "original_form": "계란말이 만들기 쉽지 않은데",
          "speaker_id": "1",
          "time": "20210907 20:34"
        }
      ]
