      "utterance": [
        {
          "id": "MDRW2100044993.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044993.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044993.1.3",
          "form": "최근에 쇼핑하셨나요?",
          "original_form": "최근에 쇼핑하셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044993.1.4",
          "form": "거의 인터넷으로 생필품만 사고 있어요",
          "original_form": "거의 인터넷으로 생필품만 사고 있어요",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044993.1.5",
          "form": "아 저도 인터넷으로 생필품 자주 사요",
          "original_form": "아 저도 인터넷으로 생필품 자주 사요",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044993.1.6",
          "form": "오프라인보다 좋은 가격으로 살 수 있을 때가 많더라구요",
          "original_form": "오프라인보다 좋은 가격으로 살 수 있을 때가 많더라구요",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044993.1.7",
          "form": "마트는 비싸기도 하구요",
          "original_form": "마트는 비싸기도 하구요",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044993.1.8",
          "form": "맞아요 그리고 부피 큰 물건들은 배달이 편하죠",
          "original_form": "맞아요 그리고 부피 큰 물건들은 배달이 편하죠",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044993.1.9",
          "form": "그쵸 거의 배달이예요. 코로나로 더 하지만요",
          "original_form": "그쵸 거의 배달이예요. 코로나로 더 하지만요",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044993.1.10",
          "form": "근데 배달이 편한 반면 상자쓰레기가 너무 많이 나오는 건 단점인 것 같아요",
          "original_form": "근데 배달이 편한 반면 상자쓰레기가 너무 많이 나오는 건 단점인 것 같아요",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044993.1.11",
          "form": "그건 그런데 어쩔수가 없으니",
          "original_form": "그건 그런데 어쩔수가 없으니",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044993.1.12",
          "form": "저는 냉동음식도 자주 배달시키는데",
          "original_form": "저는 냉동음식도 자주 배달시키는데",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044993.1.13",
          "form": "그래서 아이스팩이 너무 많이 모였어요...",
          "original_form": "그래서 아이스팩이 너무 많이 모였어요...",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044993.1.14",
          "form": "환경오염도 많이 되고 안좋긴 해요",
          "original_form": "환경오염도 많이 되고 안좋긴 해요",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044993.1.15",
          "form": "그러니깐요 아이스팩을 그냥 버리긴 그렇고 어디 수거하는 곳이 있다고 들어서 계속 보관은 하고 있는데",
          "original_form": "그러니깐요 아이스팩을 그냥 버리긴 그렇고 어디 수거하는 곳이 있다고 들어서 계속 보관은 하고 있는데",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100044993.1.16",
          "form": "이러다가 집에서 썩을 것 같아요 ㅋㅋ",
          "original_form": "이러다가 집에서 썩을 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100044993.1.17",
          "form": "뉴스에 보니깐 어느 시장에서 그걸 받아서 사용하드라구요",
          "original_form": "뉴스에 보니깐 어느 시장에서 그걸 받아서 사용하드라구요",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044993.1.18",
          "form": "아 그래요??",
          "original_form": "아 그래요??",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044993.1.19",
          "form": "주변 시장에 알아봐야겠네요!!",
          "original_form": "주변 시장에 알아봐야겠네요!!",
