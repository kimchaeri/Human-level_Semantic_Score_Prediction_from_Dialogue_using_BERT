      "utterance": [
        {
          "id": "MDRW2100038111.1.1",
          "form": "흠 나는 딱히 없어",
          "original_form": "흠 나는 딱히 없어",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.2",
          "form": "나는 트렌치코트를 좋아해",
          "original_form": "나는 트렌치코트를 좋아해",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.3",
          "form": "요즘엔 그냥 편한 옷이 좋아",
          "original_form": "요즘엔 그냥 편한 옷이 좋아",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.4",
          "form": "미용과는 담 쌓았어 난",
          "original_form": "미용과는 담 쌓았어 난",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.5",
          "form": "그럼 넌 셔츠 안좋아하니?",
          "original_form": "그럼 넌 셔츠 안좋아하니?",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.6",
          "form": "나는 따뜻해야해",
          "original_form": "나는 따뜻해야해",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.7",
          "form": "셔츠 좋아",
          "original_form": "셔츠 좋아",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.8",
          "form": "나는 포멀한 차림새도 좋아한단다",
          "original_form": "나는 포멀한 차림새도 좋아한단다",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.9",
          "form": "근데 입고 갈 데가 없어",
          "original_form": "근데 입고 갈 데가 없어",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.10",
          "form": "그럼 니트를 좋아하니??",
          "original_form": "그럼 니트를 좋아하니??",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.11",
          "form": "니트 좋아해",
          "original_form": "니트 좋아해",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.12",
          "form": "나는 니트쟁이야",
          "original_form": "나는 니트쟁이야",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.13",
          "form": "니트는 포근하고 좋다",
          "original_form": "니트는 포근하고 좋다",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.14",
          "form": "니트는 폭닥하고 따뜻해",
          "original_form": "니트는 폭닥하고 따뜻해",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.15",
          "form": "우리 같은 니트있잖아",
          "original_form": "우리 같은 니트있잖아",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.16",
          "form": "다음에도 그거 입고 놀자",
          "original_form": "다음에도 그거 입고 놀자",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.17",
          "form": "기분이 좋아지는 옷이야",
          "original_form": "기분이 좋아지는 옷이야",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.18",
          "form": "아 그 옷 알지",
          "original_form": "아 그 옷 알지",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038111.1.19",
          "form": "우리의 미적감각이 비슷한가봐 하하.",
          "original_form": "우리의 미적감각이 비슷한가봐 하하.",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038111.1.20",
          "form": "그옷도 두껍고 아무곳에나 입기 좋아",
          "original_form": "그옷도 두껍고 아무곳에나 입기 좋아",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038111.1.21",
          "form": "맞아. 폭닥해",
          "original_form": "맞아. 폭닥해",
          "speaker_id": "2",
          "time": "20210901 23:21"
        }
      ]
