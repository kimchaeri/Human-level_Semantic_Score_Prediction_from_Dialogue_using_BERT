          "speaker_id": "2",
          "time": "20210830 17:55"
        },
        {
          "id": "MDRW2100036156.1.19",
          "form": "마스크썼는데 뭐 어때용",
          "original_form": "마스크썼는데 뭐 어때용",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100036156.1.20",
          "form": "하긴 그건또그래요..ㅋㅋㅋㅋ",
          "original_form": "하긴 그건또그래요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100036156.1.21",
          "form": "하고싶은거 하고살자!!!",
          "original_form": "하고싶은거 하고살자!!!",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100036156.1.22",
          "form": "다녀와요!!",
          "original_form": "다녀와요!!",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100036156.1.23",
          "form": "아니면 24모 할때 우리학원 따라와요",
