          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100036156.1.11",
          "form": "쫭나네 은우생각하면싀 일단가라앉혀봐요",
          "original_form": "쫭나네 은우생각하면싀 일단가라앉혀봐요",
          "speaker_id": "2",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100036156.1.12",
          "form": "서면댄스학원에서 레벨노래가르쳐준대서",
          "original_form": "서면댄스학원에서 레벨노래가르쳐준대서",
          "speaker_id": "2",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100036156.1.13",
          "form": "고민중이예요",
          "original_form": "고민중이예요",
          "speaker_id": "2",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100036156.1.14",
          "form": "헐 가봐요 name2",
          "original_form": "헐 가봐요 &name2&",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036156.1.15",
          "form": "퀸덤?",
          "original_form": "퀸덤?",
          "speaker_id": "1",
          "time": "20210830 17:54"
        },
        {
          "id": "MDRW2100036156.1.16",
