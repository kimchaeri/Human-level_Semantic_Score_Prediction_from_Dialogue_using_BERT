      "utterance": [
        {
          "id": "MDRW2100021997.1.1",
          "form": "오늘 저 무슨 국가조사원된것 같았어요ㅎ",
          "original_form": "오늘 저 무슨 국가조사원된것 같았어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021997.1.2",
          "form": "왜요~~~",
          "original_form": "왜요~~~",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021997.1.3",
          "form": "최근 5년간 국내 연안 사고발생 현황파악 및 재발방지대책 수립이요ㅠ",
          "original_form": "최근 5년간 국내 연안 사고발생 현황파악 및 재발방지대책 수립이요ㅠ",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021997.1.4",
          "form": "안전관리자라....",
          "original_form": "안전관리자라....",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021997.1.5",
          "form": "에공 ㅜㅁ ㅜ",
          "original_form": "에공 ㅜㅁ ㅜ",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021997.1.6",
          "form": "제목만해도 거창한데",
          "original_form": "제목만해도 거창한데",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021997.1.7",
          "form": "조사하고 뭐하고 한다고 작성한 페이지가 114페이지ㅠ",
          "original_form": "조사하고 뭐하고 한다고 작성한 페이지가 114페이지ㅠ",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021997.1.8",
          "form": "사고가 많긴 하더라구요 뉴스만 봐도",
          "original_form": "사고가 많긴 하더라구요 뉴스만 봐도",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021997.1.9",
          "form": "이건 회사를 다니는건지... 연구원을 하은건지 ㅣ....",
          "original_form": "이건 회사를 다니는건지... 연구원을 하은건지 ㅣ....",
          "speaker_id": "2",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021997.1.10",
          "form": "저희 대표님이 4월에 바뀌셨는데ㅎ",
          "original_form": "저희 대표님이 4월에 바뀌셨는데ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021997.1.11",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021997.1.12",
          "form": "사고에 관해서 민감해요ㅎ",
          "original_form": "사고에 관해서 민감해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021997.1.13",
          "form": "아하  더 신경써야 하군요",
          "original_form": "아하  더 신경써야 하군요",
          "speaker_id": "1",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021997.1.14",
          "form": "그래서 제가 더 일에대해서 신경써야하니ㅎ",
          "original_form": "그래서 제가 더 일에대해서 신경써야하니ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021997.1.15",
          "form": "근데 정작 이런거 조사한다고 본업을 못하고있으니ㅎㅎ",
          "original_form": "근데 정작 이런거 조사한다고 본업을 못하고있으니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021997.1.16",
          "form": "이거 나온것도 저희 상사가 대표님에게 잘보이려고 한것같애요ㅎ",
          "original_form": "이거 나온것도 저희 상사가 대표님에게 잘보이려고 한것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021997.1.17",
          "form": "잘 꾸며보라이건데... 밑에사람은 힘들어요ㅠㅠ",
          "original_form": "잘 꾸며보라이건데... 밑에사람은 힘들어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021997.1.18",
          "form": "흐규 토닥토닥",
          "original_form": "흐규 토닥토닥",
          "speaker_id": "1",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021997.1.19",
          "form": "항상 아랫사람? 들만  힘든거 같아요",
          "original_form": "항상 아랫사람? 들만  힘든거 같아요",
          "speaker_id": "1",
          "time": "20210829 00:43"
        },
        {
          "id": "MDRW2100021997.1.20",
          "form": "윗분들도 힘들껀데ㅎ 왜 그거 있잖아요",
          "original_form": "윗분들도 힘들껀데ㅎ 왜 그거 있잖아요",
          "speaker_id": "2",
          "time": "20210829 00:45"
        },
        {
          "id": "MDRW2100021997.1.21",
          "form": "엄청 눈에보이는 아부?",
          "original_form": "엄청 눈에보이는 아부?",
          "speaker_id": "2",
          "time": "20210829 00:46"
        },
        {
          "id": "MDRW2100021997.1.22",
          "form": "뭔지 알아요 ㅋㅋ",
          "original_form": "뭔지 알아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:46"
        },
        {
          "id": "MDRW2100021997.1.23",
          "form": "딸랑이네여 ㅋ",
          "original_form": "딸랑이네여 ㅋ",
