      "utterance": [
        {
          "id": "MDRW2100013706.1.1",
          "form": "커피좀 먹어야겠다",
          "original_form": "커피좀 먹어야겠다",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013706.1.2",
          "form": "커피가 있나 집에",
          "original_form": "커피가 있나 집에",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013706.1.3",
          "form": "많지~",
          "original_form": "많지~",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013706.1.4",
          "form": "지금 먹으면 잠 안 오는데",
          "original_form": "지금 먹으면 잠 안 오는데",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013706.1.5",
          "form": "난 잘온당",
          "original_form": "난 잘온당",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013706.1.6",
          "form": "그렇군 조금만 마셔라",
          "original_form": "그렇군 조금만 마셔라",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013706.1.7",
          "form": "엄마두 줄까",
          "original_form": "엄마두 줄까",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013706.1.8",
          "form": "아니 난 됐다",
          "original_form": "아니 난 됐다",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013706.1.9",
          "form": "구램 ㅋ 나만 마셔야겠다",
          "original_form": "구램 ㅋ 나만 마셔야겠다",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013706.1.10",
          "form": "살찐다",
          "original_form": "살찐다",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013706.1.11",
          "form": "블랙커피는 살안찜",
          "original_form": "블랙커피는 살안찜",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013706.1.12",
          "form": "그렇구나 근데 맛이 없다",
          "original_form": "그렇구나 근데 맛이 없다",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013706.1.13",
          "form": "아닌뎅 ㅋㅋ 먹다보면 맛있음",
          "original_form": "아닌뎅 ㅋㅋ 먹다보면 맛있음",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013706.1.14",
          "form": "그럼 나도 조금만 줘 봐라",
          "original_form": "그럼 나도 조금만 줘 봐라",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
