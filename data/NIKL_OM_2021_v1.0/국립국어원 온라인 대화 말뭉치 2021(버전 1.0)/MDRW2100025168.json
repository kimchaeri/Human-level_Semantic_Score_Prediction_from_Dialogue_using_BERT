      "utterance": [
        {
          "id": "MDRW2100025168.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100025168.1.2",
          "form": "네 여기는 비가 와요",
          "original_form": "네 여기는 비가 와요",
          "speaker_id": "2",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025168.1.3",
          "form": "그쪽은 비가 오나요?",
          "original_form": "그쪽은 비가 오나요?",
          "speaker_id": "2",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025168.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025168.1.5",
          "form": "많이요",
          "original_form": "많이요",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025168.1.6",
          "form": "요즘 비가 자주와서 날이 많이 시원해진거같아요",
          "original_form": "요즘 비가 자주와서 날이 많이 시원해진거같아요",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025168.1.7",
          "form": "가을이 왔죠",
          "original_form": "가을이 왔죠",
          "speaker_id": "1",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025168.1.8",
          "form": "그러게요~",
          "original_form": "그러게요~",
          "speaker_id": "2",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100025168.1.9",
          "form": "곧 긴팔을 꺼내야겠어요",
          "original_form": "곧 긴팔을 꺼내야겠어요",
          "speaker_id": "2",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100025168.1.10",
          "form": "간절기라",
          "original_form": "간절기라",
          "speaker_id": "1",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100025168.1.11",
          "form": "감기 걸리기 쉽겠어요",
          "original_form": "감기 걸리기 쉽겠어요",
          "speaker_id": "2",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100025168.1.12",
          "form": "무서우요 열날ㅋ가봐",
          "original_form": "무서우요 열날ㅋ가봐",
          "speaker_id": "1",
          "time": "20210901 00:20"
        },
        {
          "id": "MDRW2100025168.1.13",
          "form": "맞아요 또 요즘 코로나때문에 더 그래요",
          "original_form": "맞아요 또 요즘 코로나때문에 더 그래요",
          "speaker_id": "2",
          "time": "20210901 00:23"
        },
        {
          "id": "MDRW2100025168.1.14",
          "form": "병원도 못 가요",
          "original_form": "병원도 못 가요",
          "speaker_id": "1",
          "time": "20210901 07:13"
        },
        {
          "id": "MDRW2100025168.1.15",
          "form": "항상 겉옷 들고 다냐요",
          "original_form": "항상 겉옷 들고 다냐요",
          "speaker_id": "1",
          "time": "20210901 08:43"
        },
        {
          "id": "MDRW2100025168.1.16",
          "form": "추위를 많이 타시나요??",
          "original_form": "추위를 많이 타시나요??",
          "speaker_id": "2",
          "time": "20210901 08:44"
        },
        {
          "id": "MDRW2100025168.1.17",
          "form": "혹시나요",
          "original_form": "혹시나요",
          "speaker_id": "1",
