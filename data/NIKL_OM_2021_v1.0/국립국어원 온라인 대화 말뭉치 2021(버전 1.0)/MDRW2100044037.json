      "utterance": [
        {
          "id": "MDRW2100044037.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044037.1.2",
          "form": "걷기운동해요",
          "original_form": "걷기운동해요",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044037.1.3",
          "form": "걷는게 건강에 제일 좋다고 하더라고요",
          "original_form": "걷는게 건강에 제일 좋다고 하더라고요",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044037.1.4",
          "form": "네 몸에 무리가 가지 않고",
          "original_form": "네 몸에 무리가 가지 않고",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044037.1.5",
          "form": "전신운동이 되니",
          "original_form": "전신운동이 되니",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044037.1.6",
          "form": "제 주변에도 걷기 운동하는 분들 많이 계세요",
          "original_form": "제 주변에도 걷기 운동하는 분들 많이 계세요",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044037.1.7",
          "form": "챙겨하는 편이에요",
          "original_form": "챙겨하는 편이에요",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044037.1.8",
          "form": "평지를 걸으세요 산지를 걸으세요?",
          "original_form": "평지를 걸으세요 산지를 걸으세요?",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044037.1.9",
          "form": "주로 평지요",
          "original_form": "주로 평지요",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044037.1.10",
          "form": "저 아는분은 계단을 걷더라구요",
          "original_form": "저 아는분은 계단을 걷더라구요",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044037.1.11",
          "form": "계단 걷기가 참 좋다고하던데",
          "original_form": "계단 걷기가 참 좋다고하던데",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044037.1.12",
          "form": "다이어트를 위해 시작했는데 몸에 좋아서",
          "original_form": "다이어트를 위해 시작했는데 몸에 좋아서",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044037.1.13",
          "form": "계속 하신대요",
          "original_form": "계속 하신대요",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044037.1.14",
          "form": "부럽네요 나이가 들면 무릎 때문에 겁이 나서 못 해요",
          "original_form": "부럽네요 나이가 들면 무릎 때문에 겁이 나서 못 해요",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044037.1.15",
          "form": "그렇겠어요 무릎에 무리가 가겠네요",
          "original_form": "그렇겠어요 무릎에 무리가 가겠네요",
          "speaker_id": "2",
          "time": "20210907 21:25"
