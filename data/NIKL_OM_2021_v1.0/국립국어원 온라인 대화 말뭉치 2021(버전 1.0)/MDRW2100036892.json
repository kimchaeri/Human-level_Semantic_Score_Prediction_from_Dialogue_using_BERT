      "utterance": [
        {
          "id": "MDRW2100036892.1.1",
          "form": "네! 저 빵 먹었어요",
          "original_form": "네! 저 빵 먹었어요",
          "speaker_id": "2",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100036892.1.2",
          "form": "점심 드셨어요??",
          "original_form": "점심 드셨어요??",
          "speaker_id": "2",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100036892.1.3",
          "form": "네네 ! 전 오리불고기요 ㅎㅎ",
          "original_form": "네네 ! 전 오리불고기요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036892.1.4",
          "form": "어떤 빵이요??!",
          "original_form": "어떤 빵이요??!",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036892.1.5",
          "form": "크루아상이요!",
          "original_form": "크루아상이요!",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036892.1.6",
          "form": "오리불고기ㅠㅠ 밥 먹고싶네요ㅠㅠ",
          "original_form": "오리불고기ㅠㅠ 밥 먹고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036892.1.7",
          "form": "저는 크루아상이 먹구싶네요 .. 후식으로... !!!",
          "original_form": "저는 크루아상이 먹구싶네요 .. 후식으로... !!!",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036892.1.8",
          "form": "밥도 야무지게 볶아서 먹었어요 ㅎㅎ !",
          "original_form": "밥도 야무지게 볶아서 먹었어요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036892.1.9",
          "form": "그쵸!! 저건 후식으로 먹어야하는건데ㅠㅠ",
          "original_form": "그쵸!! 저건 후식으로 먹어야하는건데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036892.1.10",
          "form": "밥 대신 먹었더니 배가 허해요...\n칼로리를 그렇지않을텐데 ㅋㅋㅋㅋ",
          "original_form": "밥 대신 먹었더니 배가 허해요...\n칼로리를 그렇지않을텐데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036892.1.11",
          "form": "ㅋㅋㅋㅋ맞아욬ㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋ맞아욬ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036892.1.12",
          "form": "빵은 칼로리가 항상 배신한답니다...",
          "original_form": "빵은 칼로리가 항상 배신한답니다...",
          "speaker_id": "1",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036892.1.13",
          "form": "정작 공기만 들었는데...",
          "original_form": "정작 공기만 들었는데...",
          "speaker_id": "1",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036892.1.14",
          "form": "공기만 들었다는 거 너무 공감합니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "공기만 들었다는 거 너무 공감합니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036892.1.15",
          "form": "배에 공기만 채웠네요ㅠㅠ",
          "original_form": "배에 공기만 채웠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036892.1.16",
          "form": "저녁을 든든하게 드셔야겠네요!!!",
          "original_form": "저녁을 든든하게 드셔야겠네요!!!",
          "speaker_id": "1",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100036892.1.17",
          "form": "공기만 드셨자나요 ㅎㅎ",
          "original_form": "공기만 드셨자나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100036892.1.18",
          "form": "맞아요 갑자기 제 인생이 허망해졌어요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 갑자기 제 인생이 허망해졌어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100036892.1.19",
          "form": "소중한 한 끼를 이렇게 허하게 날려버리다니...",
          "original_form": "소중한 한 끼를 이렇게 허하게 날려버리다니...",
          "speaker_id": "2",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100036892.1.20",
          "form": "저녁은 꼭 육류가득 든든하게..",
          "original_form": "저녁은 꼭 육류가득 든든하게..",
          "speaker_id": "2",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100036892.1.21",
          "form": "삼겹살 같은걸로 보충하자구여 !",
          "original_form": "삼겹살 같은걸로 보충하자구여 !",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100036892.1.22",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 14:57"
        },
        {
          "id": "MDRW2100036892.1.23",
          "form": "사랑합니다",
          "original_form": "사랑합니다",
          "speaker_id": "2",
          "time": "20210831 15:00"
        },
        {
          "id": "MDRW2100036892.1.24",
          "form": "금겹살이 돼도 포기할 수 없어요ㅠ",
          "original_form": "금겹살이 돼도 포기할 수 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:00"
        },
        {
          "id": "MDRW2100036892.1.25",
          "form": "오늘도 집에 신문지 깔아야겠어요 ㅎㅎ",
          "original_form": "오늘도 집에 신문지 깔아야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100036892.1.26",
          "form": "아 진짜 고기 집에서 먹으면",
          "original_form": "아 진짜 고기 집에서 먹으면",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100036892.1.27",
          "form": "기름때문에 넘 힘들어요...",
          "original_form": "기름때문에 넘 힘들어요...",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100036892.1.28",
          "form": "근데 포기할 수 없죠 ㅎㅎ",
          "original_form": "근데 포기할 수 없죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100036892.1.29",
          "form": "그래서 베이킹 소다도 샀어요 ㅋㅋㅋ 기름 없애려구..",
          "original_form": "그래서 베이킹 소다도 샀어요 ㅋㅋㅋ 기름 없애려구..",
          "speaker_id": "1",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036892.1.30",
          "form": "밖에서 먹으면 편한데 요즘은 그 식당에 있는 시간이 너무 불안하죠ㅠㅠ ㅋㅋㅋ",
          "original_form": "밖에서 먹으면 편한데 요즘은 그 식당에 있는 시간이 너무 불안하죠ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:06"
        }
      ]
