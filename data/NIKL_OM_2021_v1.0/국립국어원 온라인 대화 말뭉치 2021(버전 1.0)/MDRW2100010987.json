      "utterance": [
        {
          "id": "MDRW2100010987.1.1",
          "form": "나 요즘 일박이일 보는데",
          "original_form": "나 요즘 일박이일 보는데",
          "speaker_id": "2",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100010987.1.2",
          "form": "엄청 재미있어",
          "original_form": "엄청 재미있어",
          "speaker_id": "2",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100010987.1.3",
          "form": "일박이일 알지.",
          "original_form": "일박이일 알지.",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010987.1.4",
          "form": "일박이일동안 연예인들이 예능을 찍잖아.",
          "original_form": "일박이일동안 연예인들이 예능을 찍잖아.",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010987.1.5",
          "form": "맞아 거기 나오는 김선호가 잘생겼어",
          "original_form": "맞아 거기 나오는 김선호가 잘생겼어",
          "speaker_id": "2",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010987.1.6",
          "form": "잘생겨서 좋아하는구너",
          "original_form": "잘생겨서 좋아하는구너",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010987.1.7",
          "form": "맞아 잘생겼야",
          "original_form": "맞아 잘생겼야",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010987.1.8",
          "form": "그리고 김종민 가끔 웃기더라.",
          "original_form": "그리고 김종민 가끔 웃기더라.",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010987.1.9",
          "form": "맞아 요즘 김종민 엄청 웃겨",
          "original_form": "맞아 요즘 김종민 엄청 웃겨",
          "speaker_id": "2",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010987.1.10",
          "form": "연정훈은 의외였어ㅡ 항상 한가인의 남편으로만 알았겨든.",
          "original_form": "연정훈은 의외였어ㅡ 항상 한가인의 남편으로만 알았겨든.",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010987.1.11",
          "form": "맞아 아무래도 연기하는거도 본적없어",
          "original_form": "맞아 아무래도 연기하는거도 본적없어",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010987.1.12",
          "form": "사실 연정훈에겐 딱히 관심이 없어.",
          "original_form": "사실 연정훈에겐 딱히 관심이 없어.",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010987.1.13",
          "form": "좀 진지한 사람인줄 알았는데",
          "original_form": "좀 진지한 사람인줄 알았는데",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010987.1.14",
          "form": "나도야. 신기하다.",
          "original_form": "나도야. 신기하다.",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010987.1.15",
          "form": "생각보다 재미있더라",
          "original_form": "생각보다 재미있더라",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010987.1.16",
          "form": "연정훈은 언제 그렇게 뜬걸까?",
          "original_form": "연정훈은 언제 그렇게 뜬걸까?",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010987.1.17",
          "form": "근데 약간 많이 아저씨같긴해",
          "original_form": "근데 약간 많이 아저씨같긴해",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010987.1.18",
          "form": "음 우리가 어릴때",
          "original_form": "음 우리가 어릴때",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010987.1.19",
          "form": "아무래도 야저씨니까.",
          "original_form": "아무래도 야저씨니까.",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010987.1.20",
          "form": "인기가 많았대!",
          "original_form": "인기가 많았대!",
          "speaker_id": "2",
          "time": "20210823 23:56"
        }
      ]
    }
