      "utterance": [
        {
          "id": "MDRW2100043301.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043301.1.2",
          "form": "안녕하세요?요즘은 어떤 음악 들으세요?",
          "original_form": "안녕하세요?요즘은 어떤 음악 들으세요?",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043301.1.3",
          "form": "음악은 항상 렌덤으로 틀어놔요ㅋ",
          "original_form": "음악은 항상 렌덤으로 틀어놔요ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043301.1.4",
          "form": "아 저는 발라드 좋아해요 혹시 좋아하는 장르 있으신가요?",
          "original_form": "아 저는 발라드 좋아해요 혹시 좋아하는 장르 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043301.1.5",
          "form": "저도 발라드 좋아해요",
          "original_form": "저도 발라드 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043301.1.6",
          "form": "가수는 누구 좋아하세요?",
          "original_form": "가수는 누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043301.1.7",
          "form": "전 아이유요ㅋ",
          "original_form": "전 아이유요ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043301.1.8",
          "form": "아 아이유 좋아하시는구나~아이유 어떤면이 좋으세요?",
          "original_form": "아 아이유 좋아하시는구나~아이유 어떤면이 좋으세요?",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043301.1.9",
          "form": "목소리 넘좋아요\r\n연기도 잘하고",
          "original_form": "목소리 넘좋아요\r\n연기도 잘하고",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043301.1.10",
          "form": "아 그렇죠 얼굴도 넘 이쁘고",
          "original_form": "아 그렇죠 얼굴도 넘 이쁘고",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043301.1.11",
          "form": "나의 아저씨에서 연기 좋았다고 하더라구요",
          "original_form": "나의 아저씨에서 연기 좋았다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043301.1.12",
          "form": "아 맞아요 넘잘햇어요",
          "original_form": "아 맞아요 넘잘햇어요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043301.1.13",
          "form": "저는 보보경심려 봤어요 아이유작품",
          "original_form": "저는 보보경심려 봤어요 아이유작품",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043301.1.14",
          "form": "아 내용이 특이햇져ㅋㅋ",
          "original_form": "아 내용이 특이햇져ㅋㅋ",
