      "utterance": [
        {
          "id": "MDRW2100012314.1.1",
          "form": "운동 좋아하시나요",
          "original_form": "운동 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012314.1.2",
          "form": "요즘 관심가지기 시작했어요!",
          "original_form": "요즘 관심가지기 시작했어요!",
          "speaker_id": "2",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012314.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012314.1.4",
          "form": "저도 홈트 해보려구요",
          "original_form": "저도 홈트 해보려구요",
          "speaker_id": "1",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012314.1.5",
          "form": "아무래도 요즘은 밖에서 하기가 어렵죠",
          "original_form": "아무래도 요즘은 밖에서 하기가 어렵죠",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012314.1.6",
          "form": "맞아요 헬스장도 찝찝하고",
          "original_form": "맞아요 헬스장도 찝찝하고",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012314.1.7",
          "form": "홈트는 집에 기구가 있으신가요?",
          "original_form": "홈트는 집에 기구가 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012314.1.8",
          "form": "철봉이랑 푸시업바있네요ㅎㅎ",
          "original_form": "철봉이랑 푸시업바있네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100012314.1.9",
          "form": "근력 운동 위주네요!",
          "original_form": "근력 운동 위주네요!",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012314.1.10",
          "form": "저는 요즘 유산소만 1~2시간 씩 하고 있어요",
          "original_form": "저는 요즘 유산소만 1~2시간 씩 하고 있어요",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012314.1.11",
          "form": "체력 좀 기르려구요ㅠ",
          "original_form": "체력 좀 기르려구요ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012314.1.12",
          "form": "오 유산소 어떤운동하시나요",
          "original_form": "오 유산소 어떤운동하시나요",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012314.1.13",
          "form": "러닝해요!",
          "original_form": "러닝해요!",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012314.1.14",
          "form": "뛰는 게 은근히 재밌더라고요",
          "original_form": "뛰는 게 은근히 재밌더라고요",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012314.1.15",
          "form": "오 러닝 동네 도시나요",
          "original_form": "오 러닝 동네 도시나요",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012314.1.16",
          "form": "집 근처에 공원이 있어서 그쪽으로 가요",
          "original_form": "집 근처에 공원이 있어서 그쪽으로 가요",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012314.1.17",
          "form": "그래도 꽤 오래뛰시네요",
          "original_form": "그래도 꽤 오래뛰시네요",
          "speaker_id": "1",
