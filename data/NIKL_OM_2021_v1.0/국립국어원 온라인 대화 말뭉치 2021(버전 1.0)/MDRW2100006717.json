      "utterance": [
        {
          "id": "MDRW2100006717.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100006717.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100006717.1.3",
          "form": "기억에 남는곳있낭ㅅ",
          "original_form": "기억에 남는곳있낭ㅅ",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100006717.1.4",
          "form": "나요",
          "original_form": "나요",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100006717.1.5",
          "form": "저는 디즈니 랜드요! 정말 인상적이에요",
          "original_form": "저는 디즈니 랜드요! 정말 인상적이에요",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100006717.1.6",
          "form": "오 어느국가요?",
          "original_form": "오 어느국가요?",
          "speaker_id": "1",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100006717.1.7",
          "form": "미국에 있는거요",
          "original_form": "미국에 있는거요",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100006717.1.8",
          "form": "아 미국에 가보셨군요",
          "original_form": "아 미국에 가보셨군요",
          "speaker_id": "1",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100006717.1.9",
          "form": "넹 교환학생 때 지금 아니면 못 올지도 몰라! 하면서 갔어요",
          "original_form": "넹 교환학생 때 지금 아니면 못 올지도 몰라! 하면서 갔어요",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100006717.1.10",
          "form": "저 로망 여행지예요",
          "original_form": "저 로망 여행지예요",
          "speaker_id": "1",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100006717.1.11",
          "form": "미국이요? 어느 도시가 좋으세요?",
          "original_form": "미국이요? 어느 도시가 좋으세요?",
          "speaker_id": "2",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100006717.1.12",
          "form": "제가 nba 팬이라 농구경기를 꼭 보고싶어요 ㅎㅎ",
          "original_form": "제가 nba 팬이라 농구경기를 꼭 보고싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100006717.1.13",
          "form": "그러면 응원하는팀 홈 구장? 가시고 싶으신 건가요?",
          "original_form": "그러면 응원하는팀 홈 구장? 가시고 싶으신 건가요?",
          "speaker_id": "2",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100006717.1.14",
          "form": "네 ㅎㅎ 또 햄버거도 먹어보고싶어요",
          "original_form": "네 ㅎㅎ 또 햄버거도 먹어보고싶어요",
          "speaker_id": "1",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006717.1.15",
          "form": "스테이크도 드세요 ㅋㅋㅋ",
          "original_form": "스테이크도 드세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:10"
        },
        {
          "id": "MDRW2100006717.1.16",
          "form": "아.소고기 좋아하죠...양고기도..",
          "original_form": "아.소고기 좋아하죠...양고기도..",
          "speaker_id": "1",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100006717.1.17",
          "form": "뉴욕에 엄청 유명한 스테이크집 있다는데",
          "original_form": "뉴욕에 엄청 유명한 스테이크집 있다는데",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006717.1.18",
          "form": "돈이 없어서 못 갔네요ㅠㅠ",
          "original_form": "돈이 없어서 못 갔네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006717.1.19",
          "form": "햄버거는 드셨나요?",
          "original_form": "햄버거는 드셨나요?",
          "speaker_id": "1",
          "time": "20210908 08:17"
        }
      ]
