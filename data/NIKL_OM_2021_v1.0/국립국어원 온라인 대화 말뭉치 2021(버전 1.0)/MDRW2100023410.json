      "utterance": [
        {
          "id": "MDRW2100023410.1.1",
          "form": "저는 반지의제왕 팬이라 촬영지 한번 가보고 싶어요",
          "original_form": "저는 반지의제왕 팬이라 촬영지 한번 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023410.1.2",
          "form": "님은요??",
          "original_form": "님은요??",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023410.1.3",
          "form": "거기가 뉴질랜드죠?",
          "original_form": "거기가 뉴질랜드죠?",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023410.1.4",
          "form": "일본여행가면 어떤거 보고 오시는지 궁금하네요",
          "original_form": "일본여행가면 어떤거 보고 오시는지 궁금하네요",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023410.1.5",
          "form": "저는 대만가보고싶어용",
          "original_form": "저는 대만가보고싶어용",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023410.1.6",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023410.1.7",
          "form": "오 대만",
          "original_form": "오 대만",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023410.1.8",
          "form": "오사카성도 갔었구",
          "original_form": "오사카성도 갔었구",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023410.1.9",
          "form": "드라마로만 봤는데",
          "original_form": "드라마로만 봤는데",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023410.1.10",
          "form": "대나무숲도 갔었구",
          "original_form": "대나무숲도 갔었구",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023410.1.11",
          "form": "대나무숲!",
          "original_form": "대나무숲!",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023410.1.12",
          "form": "대만 넘 좋은것같아요 한국이랑 비슷해소ㅋㅋㅋㅋ",
          "original_form": "대만 넘 좋은것같아요 한국이랑 비슷해소ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023410.1.13",
          "form": "가보신적 있으셔요?",
          "original_form": "가보신적 있으셔요?",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023410.1.14",
          "form": "아녀아녀ㅠㅠ",
          "original_form": "아녀아녀ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023410.1.15",
          "form": "가보고싶어요",
          "original_form": "가보고싶어요",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023410.1.16",
          "form": "예스진지라고 한국인들이 많이가는 대만 여행 코스가 있던데",
          "original_form": "예스진지라고 한국인들이 많이가는 대만 여행 코스가 있던데",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023410.1.17",
          "form": "거기 가봐야지~ 했다가 코로나가 터져서",
          "original_form": "거기 가봐야지~ 했다가 코로나가 터져서",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023410.1.18",
          "form": "못갔어요ㅠㅠ",
          "original_form": "못갔어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023410.1.19",
          "form": "패키지 여행으로 가시는 편이세요?",
          "original_form": "패키지 여행으로 가시는 편이세요?",
          "speaker_id": "2",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023410.1.20",
          "form": "아하 ㅠㅠ",
          "original_form": "아하 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023410.1.21",
          "form": "패키지로도 가보고",
          "original_form": "패키지로도 가보고",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023410.1.22",
          "form": "자유로도 가봤는데",
          "original_form": "자유로도 가봤는데",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023410.1.23",
          "form": "장단점이 다 있는것같아용",
          "original_form": "장단점이 다 있는것같아용",
          "speaker_id": "1",
          "time": "20210830 01:44"
        }
      ]
