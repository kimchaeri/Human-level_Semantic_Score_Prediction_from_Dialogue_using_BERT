      "utterance": [
        {
          "id": "MDRW2100018654.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018654.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018654.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018654.1.4",
          "form": "저도 여행 좋아해요 코로나때문에 최근엔 못갔지만 ㅠㅠ",
          "original_form": "저도 여행 좋아해요 코로나때문에 최근엔 못갔지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018654.1.5",
          "form": "어디 다녀와보셨어요?",
          "original_form": "어디 다녀와보셨어요?",
          "speaker_id": "1",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018654.1.6",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018654.1.7",
          "form": "저는 해외는 못가보고",
          "original_form": "저는 해외는 못가보고",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018654.1.8",
          "form": "국내 부산 가봤어요",
          "original_form": "국내 부산 가봤어요",
          "speaker_id": "2",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018654.1.9",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018654.1.10",
          "form": "저는 강릉 속초 부산 여수 강화 가평 요런 곳들 가봤어요!",
          "original_form": "저는 강릉 속초 부산 여수 강화 가평 요런 곳들 가봤어요!",
          "speaker_id": "1",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018654.1.11",
          "form": "여수 어땠어요",
          "original_form": "여수 어땠어요",
          "speaker_id": "2",
          "time": "20210827 10:03"
        },
        {
          "id": "MDRW2100018654.1.12",
          "form": "밤바다 보셨나요ㅎ",
          "original_form": "밤바다 보셨나요ㅎ",
          "speaker_id": "2",
          "time": "20210827 10:03"
        },
        {
          "id": "MDRW2100018654.1.13",
          "form": "여수는 진짜 짧게다녀와서 딱히 별거 못느끼구왔어요ㅠ",
          "original_form": "여수는 진짜 짧게다녀와서 딱히 별거 못느끼구왔어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:03"
        },
        {
          "id": "MDRW2100018654.1.14",
          "form": "그럼 어디가 좋으셨나요",
          "original_form": "그럼 어디가 좋으셨나요",
          "speaker_id": "2",
          "time": "20210827 10:03"
        },
        {
          "id": "MDRW2100018654.1.15",
          "form": "저는 강릉 좋아해요",
          "original_form": "저는 강릉 좋아해요",
          "speaker_id": "1",
          "time": "20210827 10:03"
        },
        {
          "id": "MDRW2100018654.1.16",
          "form": "강릉가면 주로 뭐하시나요",
          "original_form": "강릉가면 주로 뭐하시나요",
          "speaker_id": "2",
          "time": "20210827 10:04"
        },
        {
          "id": "MDRW2100018654.1.17",
          "form": "바다 보고 카페가고..",
          "original_form": "바다 보고 카페가고..",
          "speaker_id": "1",
          "time": "20210827 10:04"
        },
        {
          "id": "MDRW2100018654.1.18",
          "form": "힐링여햄사고오시는구나",
          "original_form": "힐링여햄사고오시는구나",
          "speaker_id": "2",
          "time": "20210827 10:05"
        },
        {
          "id": "MDRW2100018654.1.19",
          "form": "와 뭐했었지 근 2년 여행을 못갔더니 기억이 멈췄어요",
          "original_form": "와 뭐했었지 근 2년 여행을 못갔더니 기억이 멈췄어요",
          "speaker_id": "1",
          "time": "20210827 10:05"
        },
        {
          "id": "MDRW2100018654.1.20",
          "form": "코로나의 피해ㅠ",
          "original_form": "코로나의 피해ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:05"
        },
        {
          "id": "MDRW2100018654.1.21",
          "form": "게하 어때요?",
          "original_form": "게하 어때요?",
          "speaker_id": "2",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018654.1.22",
          "form": "저는 게하를 가본적이 없어서",
          "original_form": "저는 게하를 가본적이 없어서",
          "speaker_id": "2",
          "time": "20210827 10:07"
        }
      ]
