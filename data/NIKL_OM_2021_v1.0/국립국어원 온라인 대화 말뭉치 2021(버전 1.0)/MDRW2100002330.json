      "utterance": [
        {
          "id": "MDRW2100002330.1.1",
          "form": "열라면 나는 두번 정도 먹었는데",
          "original_form": "열라면 나는 두번 정도 먹었는데",
          "speaker_id": "1",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002330.1.2",
          "form": "그때마다 다음날 힘들어서 ㅜㅜ 스프를 반만 넣어 ㅋㅋㅋ",
          "original_form": "그때마다 다음날 힘들어서 ㅜㅜ 스프를 반만 넣어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002330.1.3",
          "form": "순두부를 넣어봐야겠다 ㅋㅋㅋ",
          "original_form": "순두부를 넣어봐야겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002330.1.4",
          "form": "열라면 최근에 먹었는데 맛있더라",
          "original_form": "열라면 최근에 먹었는데 맛있더라",
          "speaker_id": "2",
          "time": "20210820 09:44"
        },
        {
          "id": "MDRW2100002330.1.5",
          "form": "라면 자주 안먹는데",
          "original_form": "라면 자주 안먹는데",
          "speaker_id": "2",
          "time": "20210820 09:44"
        },
        {
          "id": "MDRW2100002330.1.6",
          "form": "진짜 맵더라 ㅜㅜ",
          "original_form": "진짜 맵더라 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002330.1.7",
          "form": "이름값 하는 라면 ㅋㅋㅋ",
          "original_form": "이름값 하는 라면 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002330.1.8",
          "form": "이름값ㅋㅋㅋㅋㅋ가격도 싸고",
          "original_form": "이름값ㅋㅋㅋㅋㅋ가격도 싸고",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002330.1.9",
          "form": "한 500원인가",
          "original_form": "한 500원인가",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002330.1.10",
          "form": "오 진짜???",
          "original_form": "오 진짜???",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002330.1.11",
          "form": "낱개에?? 싸다 ㅋㅋㅋㅋ",
          "original_form": "낱개에?? 싸다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002330.1.12",
          "form": "맞아 저렴해 ㅎㅎ",
          "original_form": "맞아 저렴해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002330.1.13",
          "form": "파기름 내서 라면 끓이면 더 맛있어",
          "original_form": "파기름 내서 라면 끓이면 더 맛있어",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002330.1.14",
          "form": "오 ㅋㅋㅋㅋ 뭔가 백종원님이 좋아할 법한",
          "original_form": "오 ㅋㅋㅋㅋ 뭔가 백종원님이 좋아할 법한",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002330.1.15",
          "form": "해먹어봐야지 ㅋㅋㅋ",
          "original_form": "해먹어봐야지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002330.1.16",
          "form": "백종원ㅎㅎㅎ 맞네 파기름 좋아하시잖아",
          "original_form": "백종원ㅎㅎㅎ 맞네 파기름 좋아하시잖아",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002330.1.17",
          "form": "오늘 저녁메뉴 뭐야?",
          "original_form": "오늘 저녁메뉴 뭐야?",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002330.1.18",
          "form": "아직 안정했는데 뭔가 국물요리로 먹을거같아 ㅋㅋㅋㅋ",
          "original_form": "아직 안정했는데 뭔가 국물요리로 먹을거같아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002330.1.19",
          "form": "더운데 국물이 땡긴다",
          "original_form": "더운데 국물이 땡긴다",
          "speaker_id": "1",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002330.1.20",
          "form": "에어컨 켜고 먹으니까 ㅋㅋㅋㅋ",
          "original_form": "에어컨 켜고 먹으니까 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002330.1.21",
          "form": "오 난 마라훠궈탕면 컵라면 먹을까봐",
          "original_form": "오 난 마라훠궈탕면 컵라면 먹을까봐",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002330.1.22",
          "form": "유통기한이 다음달 까지라서 빨리먹어야해",
          "original_form": "유통기한이 다음달 까지라서 빨리먹어야해",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002330.1.23",
          "form": "아아 ㅋㅋㅋㅋ 아직 못먹어본거다",
          "original_form": "아아 ㅋㅋㅋㅋ 아직 못먹어본거다",
          "speaker_id": "1",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002330.1.24",
          "form": "오늘 저녁 마라네 ㅋㅋㅋ 마라 땡긴다",
          "original_form": "오늘 저녁 마라네 ㅋㅋㅋ 마라 땡긴다",
          "speaker_id": "1",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002330.1.25",
          "form": "저번에 한번 먹었는데 괜찮더라ㅋㅋㅋ",
          "original_form": "저번에 한번 먹었는데 괜찮더라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002330.1.26",
          "form": "이제 3개남았어",
          "original_form": "이제 3개남았어",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002330.1.27",
          "form": "오오 ㅋㅋㅋㅋ 거의 다 먹었네",
          "original_form": "오오 ㅋㅋㅋㅋ 거의 다 먹었네",
          "speaker_id": "1",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100002330.1.28",
          "form": "마라샹궈 남은거에 육수 부어서 마라탕 해먹던데",
          "original_form": "마라샹궈 남은거에 육수 부어서 마라탕 해먹던데",
