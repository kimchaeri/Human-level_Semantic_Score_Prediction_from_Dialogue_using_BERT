      "utterance": [
        {
          "id": "MDRW2100007794.1.1",
          "form": "디다",
          "original_form": "디다",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007794.1.2",
          "form": "일어나서 계속 청소하고",
          "original_form": "일어나서 계속 청소하고",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007794.1.3",
          "form": "빨래하고",
          "original_form": "빨래하고",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007794.1.4",
          "form": "신발도 세탁하고 ㅋㅋㅋㄱ",
          "original_form": "신발도 세탁하고 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007794.1.5",
          "form": "이제 설거지할라함",
          "original_form": "이제 설거지할라함",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007794.1.6",
          "form": "이요~신발까정~",
          "original_form": "이요~신발까정~",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007794.1.7",
          "form": "멋지구만 언니 ㅋㅋㅋㅋ난 name3이 실내화 사야됨 ㅋㅋㅋ",
          "original_form": "멋지구만 언니 ㅋㅋㅋㅋ난 &name3&이 실내화 사야됨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.8",
          "form": "ㅋㅋㅋㅋㅋㄱ 샌들 시커멓다",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 샌들 시커멓다",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.9",
          "form": "빨기 구찮아서 버렸아여 ㅋㅋㅋ",
          "original_form": "빨기 구찮아서 버렸아여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.10",
          "form": "실내화 진짜",
          "original_form": "실내화 진짜",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.11",
          "form": "한달에\r\n한번씩 세탁하니 너무 더러움",
          "original_form": "한달에\r\n한번씩 세탁하니 너무 더러움",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.12",
          "form": "그래 난 크록스라....나름 비싸서 ㅋㅋㅋㄱㄱ",
          "original_form": "그래 난 크록스라....나름 비싸서 ㅋㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.13",
          "form": "담주에 진짜 바쁠예정이구만요 ㅋㅋㅋㅋ",
          "original_form": "담주에 진짜 바쁠예정이구만요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.14",
          "form": "버리지도 못하고",
          "original_form": "버리지도 못하고",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.15",
          "form": "팔뚝 떨어져나가겠음",
          "original_form": "팔뚝 떨어져나가겠음",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.16",
          "form": "실내화 크록스여요??",
          "original_form": "실내화 크록스여요??",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007794.1.17",
          "form": "엉 흰크록스",
          "original_form": "엉 흰크록스",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007794.1.18",
          "form": "것도 괘찮겠네여 ㅋㅋㅋ",
          "original_form": "것도 괘찮겠네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007794.1.19",
          "form": "유치원때부터 계속 그거신김 \r\n사이즈만 변경해가 ㅋㅋㅋ",
          "original_form": "유치원때부터 계속 그거신김 \r\n사이즈만 변경해가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007794.1.20",
          "form": "일반 실내화 신기니 불편하다고 해서",
          "original_form": "일반 실내화 신기니 불편하다고 해서",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007794.1.21",
          "form": "난 쿠팡서 싼거 사야지 ㅋㅋㅋ",
          "original_form": "난 쿠팡서 싼거 사야지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007794.1.22",
          "form": "신기고 버리믄 되긋다",
          "original_form": "신기고 버리믄 되긋다",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007794.1.23",
          "form": "그래가 버렸어요 ㅋㅋㅋ",
          "original_form": "그래가 버렸어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007794.1.24",
          "form": "잘했다 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "잘했다 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007794.1.25",
          "form": "설거지하고 오꺼\r\nㅋㅋㅋ",
          "original_form": "설거지하고 오꺼\r\nㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100007794.1.26",
          "form": "name1언니랑은 안맞네 ㅋㅋㅋㅋ",
          "original_form": "&name1&언니랑은 안맞네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100007794.1.27",
          "form": "빨리 하고 오ㅓ여 ㅋㅋㅋ",
          "original_form": "빨리 하고 오ㅓ여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100007794.1.28",
          "form": "했다",
          "original_form": "했다",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007794.1.29",
          "form": "이제 애들 데리고 반도가서 눈깜술 할라고 ㅋㅋㄱㅋ",
          "original_form": "이제 애들 데리고 반도가서 눈깜술 할라고 ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007794.1.30",
          "form": "오~빠르데이 ㅋㅋ",
          "original_form": "오~빠르데이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100007794.1.31",
          "form": "진짜 name1언니 최고임~",
          "original_form": "진짜 &name1&언니 최고임~",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100007794.1.32",
          "form": "울 애들 엄마를 바꿔주고싶네 ㅋㅋㅋㅋㅋ",
          "original_form": "울 애들 엄마를 바꿔주고싶네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100007794.1.33",
          "form": "언니도 백조는 늠 멀다 ㅋㅋㅋ",
          "original_form": "언니도 백조는 늠 멀다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100007794.1.34",
          "form": "노는만큼 화내는 빈도도 높다",
          "original_form": "노는만큼 화내는 빈도도 높다",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007794.1.35",
          "form": "그래 백조는 진짜 차타고 댕겨야된다",
          "original_form": "그래 백조는 진짜 차타고 댕겨야된다",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007794.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ그건 사람이면 어쩔수 없음 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그건 사람이면 어쩔수 없음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007794.1.37",
          "form": "풍2가 마지노선임 ㅋㅋㅋ",
          "original_form": "풍2가 마지노선임 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007794.1.38",
          "form": "것도 정문 가까워서 딱인데 ㅋㅋㅋ",
          "original_form": "것도 정문 가까워서 딱인데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007794.1.39",
          "form": "밑에 동보다 반도가 더 가까운 ㅋㅋㅋ",
          "original_form": "밑에 동보다 반도가 더 가까운 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007794.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래 반도가 젤 가깝다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래 반도가 젤 가깝다",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100007794.1.41",
          "form": "밑에 동도 엘베타고 가야되는데",
          "original_form": "밑에 동도 엘베타고 가야되는데",
          "speaker_id": "1",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007794.1.42",
          "form": "눈감술 하다가 그 돌에 박지말고요~",
          "original_form": "눈감술 하다가 그 돌에 박지말고요~",
          "speaker_id": "2",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007794.1.43",
          "form": "반도는 네모난 돌 윽시 많아가 ㅋㅋㅋ",
          "original_form": "반도는 네모난 돌 윽시 많아가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007794.1.44",
          "form": "정강이 나갑니다 ㅋㅋㅋ",
          "original_form": "정강이 나갑니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007794.1.45",
