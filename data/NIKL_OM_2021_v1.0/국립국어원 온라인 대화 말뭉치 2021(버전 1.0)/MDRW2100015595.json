      "utterance": [
        {
          "id": "MDRW2100015595.1.1",
          "form": "우리집 음쓰처리기 삼",
          "original_form": "우리집 음쓰처리기 삼",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015595.1.2",
          "form": "오늘 엄마한테 그얘기 했었는데",
          "original_form": "오늘 엄마한테 그얘기 했었는데",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015595.1.3",
          "form": "name1네 음쓰그거 샀다고ㅋㅋ",
          "original_form": "&name1&네 음쓰그거 샀다고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015595.1.4",
          "form": "설치함?!",
          "original_form": "설치함?!",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015595.1.5",
          "form": "어때??",
          "original_form": "어때??",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015595.1.6",
          "form": "설치했어?",
          "original_form": "설치했어?",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015595.1.7",
          "form": "설치하는거라해야하나",
          "original_form": "설치하는거라해야하나",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015595.1.8",
          "form": "그냥 전원 꼽으면 끝",
          "original_form": "그냥 전원 꼽으면 끝",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015595.1.9",
          "form": "오 싱크대에 설치하는건줄",
          "original_form": "오 싱크대에 설치하는건줄",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015595.1.10",
          "form": "음쓰 버리기 귀찮은데",
          "original_form": "음쓰 버리기 귀찮은데",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015595.1.11",
          "form": "그런것도 있던데 그거 불법아닌가?",
          "original_form": "그런것도 있던데 그거 불법아닌가?",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015595.1.12",
          "form": "난 그거 샀단줄ㅋㅋ",
          "original_form": "난 그거 샀단줄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015595.1.13",
          "form": "다른것도 있어?!?",
          "original_form": "다른것도 있어?!?",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015595.1.14",
          "form": "그냥 쓰레기통처럼 생김",
          "original_form": "그냥 쓰레기통처럼 생김",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015595.1.15",
          "form": "분쇄해주는거야?",
          "original_form": "분쇄해주는거야?",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015595.1.16",
          "form": "아니 분쇄하는거가 스마트카라인데 그거살까 린클살까 하다가 린클 삼",
          "original_form": "아니 분쇄하는거가 스마트카라인데 그거살까 린클살까 하다가 린클 삼",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015595.1.17",
          "form": "그건 어떤방식이야?",
          "original_form": "그건 어떤방식이야?",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015595.1.18",
          "form": "미생물이 음식물 쓰레기를 처리한다는데 처음에 무슨 가루를 넣고 시작해",
          "original_form": "미생물이 음식물 쓰레기를 처리한다는데 처음에 무슨 가루를 넣고 시작해",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015595.1.19",
          "form": "과학적이네 신기",
          "original_form": "과학적이네 신기",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015595.1.20",
          "form": "그 가루에 미생물이 음식물 쓰레기를 흙으로 만든데",
          "original_form": "그 가루에 미생물이 음식물 쓰레기를 흙으로 만든데",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015595.1.21",
          "form": "그게 엄청 빨리 되는거야?",
          "original_form": "그게 엄청 빨리 되는거야?",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015595.1.22",
          "form": "생각보다 빠르게되지는 않아 오늘 넣으면 잘되면 내일 가루가 되는데, 잘 안되는 것들은 오래걸려",
          "original_form": "생각보다 빠르게되지는 않아 오늘 넣으면 잘되면 내일 가루가 되는데, 잘 안되는 것들은 오래걸려",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015595.1.23",
          "form": "헐 엄청 비싸네",
          "original_form": "헐 엄청 비싸네",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015595.1.24",
          "form": "그니까 ㅋㅋㅋ 가격생각하면 잘써야되는데 아무거나 막 못버려",
          "original_form": "그니까 ㅋㅋㅋ 가격생각하면 잘써야되는데 아무거나 막 못버려",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015595.1.25",
          "form": "진짜 신기하다",
          "original_form": "진짜 신기하다",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015595.1.26",
          "form": "방금 찾아봄",
          "original_form": "방금 찾아봄",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015595.1.27",
          "form": "저렇게 없어진다고?!!!",
          "original_form": "저렇게 없어진다고?!!!",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015595.1.28",
          "form": "린클찾아봤나? ㅋㅋ 진짜 흙이 되긴함 막 그거 비료로 쓰던데",
          "original_form": "린클찾아봤나? ㅋㅋ 진짜 흙이 되긴함 막 그거 비료로 쓰던데",
          "speaker_id": "1",
