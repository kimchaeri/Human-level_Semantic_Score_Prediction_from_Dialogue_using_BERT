      "utterance": [
        {
          "id": "MDRW2100010091.1.1",
          "form": "초등학생때라 정확하게 기억은 안나는데",
          "original_form": "초등학생때라 정확하게 기억은 안나는데",
          "speaker_id": "2",
          "time": "20210823 14:10"
        },
        {
          "id": "MDRW2100010091.1.2",
          "form": "자유형 정도 했을 걸?",
          "original_form": "자유형 정도 했을 걸?",
          "speaker_id": "2",
          "time": "20210823 14:10"
        },
        {
          "id": "MDRW2100010091.1.3",
          "form": "근데너수영장안좋아하잖아",
          "original_form": "근데너수영장안좋아하잖아",
          "speaker_id": "1",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100010091.1.4",
          "form": "깊은물싫어하지않았음?",
          "original_form": "깊은물싫어하지않았음?",
          "speaker_id": "1",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100010091.1.5",
          "form": "내가??",
          "original_form": "내가??",
          "speaker_id": "2",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100010091.1.6",
          "form": "아 깊은 물 보는 것만 좋아함",
          "original_form": "아 깊은 물 보는 것만 좋아함",
          "speaker_id": "2",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100010091.1.7",
          "form": "어렸을 때 죽을 뻔해서 그런가 봐",
          "original_form": "어렸을 때 죽을 뻔해서 그런가 봐",
          "speaker_id": "2",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100010091.1.8",
          "form": "??아 수영배울때?",
          "original_form": "??아 수영배울때?",
          "speaker_id": "1",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100010091.1.9",
          "form": "죽을뻔함?",
          "original_form": "죽을뻔함?",
          "speaker_id": "1",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100010091.1.10",
          "form": "ㅇㅇ 키가 120이 안되었을 때 3m? 4m 물에 빠져서",
          "original_form": "ㅇㅇ 키가 120이 안되었을 때 3m? 4m 물에 빠져서",
          "speaker_id": "2",
          "time": "20210823 14:13"
        },
        {
          "id": "MDRW2100010091.1.11",
          "form": "죽을 뻔 함",
          "original_form": "죽을 뻔 함",
          "speaker_id": "2",
          "time": "20210823 14:13"
        },
        {
          "id": "MDRW2100010091.1.12",
          "form": "옆에 있던 애 잡았는데 그 친구가 놓으라고 내 손을 때리더라",
          "original_form": "옆에 있던 애 잡았는데 그 친구가 놓으라고 내 손을 때리더라",
          "speaker_id": "2",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100010091.1.13",
          "form": "그 친구 옆의 어른은 나 발견도 못함",
          "original_form": "그 친구 옆의 어른은 나 발견도 못함",
          "speaker_id": "2",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100010091.1.14",
          "form": "어른있었는데도?",
          "original_form": "어른있었는데도?",
          "speaker_id": "1",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100010091.1.15",
          "form": "응 아무도 안도와줌",
          "original_form": "응 아무도 안도와줌",
          "speaker_id": "2",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100010091.1.16",
          "form": "그어른은왜몰랐는데",
          "original_form": "그어른은왜몰랐는데",
          "speaker_id": "1",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100010091.1.17",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "1",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100010091.1.18",
          "form": "몰라 그때 사람이 적은 것도 아니었는데 아무 조치도 안 취하던데",
          "original_form": "몰라 그때 사람이 적은 것도 아니었는데 아무 조치도 안 취하던데",
          "speaker_id": "2",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100010091.1.19",
          "form": "그수영장사람들이상한데",
          "original_form": "그수영장사람들이상한데",
          "speaker_id": "1",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100010091.1.20",
          "form": "애가빠져서있으면적어도",
          "original_form": "애가빠져서있으면적어도",
          "speaker_id": "1",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100010091.1.21",
          "form": "직원이라도부르던가",
          "original_form": "직원이라도부르던가",
          "speaker_id": "1",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100010091.1.22",
          "form": "그니까 애가 죽을랑 말랑하는데",
          "original_form": "그니까 애가 죽을랑 말랑하는데",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010091.1.23",
          "form": "누가구해줌?",
          "original_form": "누가구해줌?",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010091.1.24",
          "form": "엄마가 구해주심",
          "original_form": "엄마가 구해주심",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010091.1.25",
          "form": "와 이게 말이 되나",
          "original_form": "와 이게 말이 되나",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010091.1.26",
          "form": "아무리그래도너무심한데",
          "original_form": "아무리그래도너무심한데",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010091.1.27",
          "form": "수영장 밖에 계시다가 느낌이 이상해서 봤는데 내가 허우적거리고 있어서 달려오셨대",
          "original_form": "수영장 밖에 계시다가 느낌이 이상해서 봤는데 내가 허우적거리고 있어서 달려오셨대",
          "speaker_id": "2",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010091.1.28",
          "form": "주변에있던어른은뭐라고그럼",
          "original_form": "주변에있던어른은뭐라고그럼",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010091.1.29",
          "form": "그냥모른척함?",
          "original_form": "그냥모른척함?",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010091.1.30",
          "form": "거기서부터 기억이 안나",
          "original_form": "거기서부터 기억이 안나",
          "speaker_id": "2",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010091.1.31",
          "form": "왠지 모르겠는데 그냥 애가 손을 쳐서 화가 났다 까지만 기억남",
          "original_form": "왠지 모르겠는데 그냥 애가 손을 쳐서 화가 났다 까지만 기억남",
          "speaker_id": "2",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010091.1.32",
          "form": "무서움보다 화가 더 쎘나봐",
          "original_form": "무서움보다 화가 더 쎘나봐",
          "speaker_id": "2",
          "time": "20210823 14:20"
        }
      ]
    }
