      "utterance": [
        {
          "id": "MDRW2100027190.1.1",
          "form": "난 어제 동생 씻겼어",
          "original_form": "난 어제 동생 씻겼어",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027190.1.2",
          "form": "우리 털뭉치",
          "original_form": "우리 털뭉치",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027190.1.3",
          "form": "씻는 거 너무 싫어해서 박박 화내길래.. 진정시키면서 씻겼어",
          "original_form": "씻는 거 너무 싫어해서 박박 화내길래.. 진정시키면서 씻겼어",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027190.1.4",
          "form": "귀엽겠당",
          "original_form": "귀엽겠당",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027190.1.5",
          "form": "강아지?",
          "original_form": "강아지?",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027190.1.6",
          "form": "고양이!",
          "original_form": "고양이!",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027190.1.7",
          "form": "엄청 물 닿는거 싫어 했겠네",
          "original_form": "엄청 물 닿는거 싫어 했겠네",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027190.1.8",
          "form": "응.. 근데 착해!!",
          "original_form": "응.. 근데 착해!!",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027190.1.9",
          "form": "가족이네",
          "original_form": "가족이네",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027190.1.10",
          "form": "이름은?",
          "original_form": "이름은?",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027190.1.11",
          "form": "ㅎㅎ 그건 비밀",
          "original_form": "ㅎㅎ 그건 비밀",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027190.1.12",
          "form": "특이해서?",
          "original_form": "특이해서?",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027190.1.13",
          "form": "응!!!!",
          "original_form": "응!!!!",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027190.1.14",
          "form": "궁금하다",
          "original_form": "궁금하다",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027190.1.15",
          "form": "너는 뭐 키워??",
          "original_form": "너는 뭐 키워??",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027190.1.16",
          "form": "암것도 안키움",
          "original_form": "암것도 안키움",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027190.1.17",
          "form": "너라고 해도 되나..",
          "original_form": "너라고 해도 되나..",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027190.1.18",
          "form": "막해 ㅋㅋ",
          "original_form": "막해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027190.1.19",
          "form": "타자빨리치고 좋치",
          "original_form": "타자빨리치고 좋치",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027190.1.20",
          "form": "식물은?",
          "original_form": "식물은?",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027190.1.21",
          "form": "다죽더라고 ㅠ",
          "original_form": "다죽더라고 ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027190.1.22",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027190.1.23",
          "form": "난 선인장 기르기 좋아해서 선인장 많이 기르는데!!",
          "original_form": "난 선인장 기르기 좋아해서 선인장 많이 기르는데!!",
          "speaker_id": "2",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027190.1.24",
          "form": "쑥쑥큼?",
          "original_form": "쑥쑥큼?",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027190.1.25",
          "form": "식물 기르기 어려우면 선인장 좋아!",
          "original_form": "식물 기르기 어려우면 선인장 좋아!",
          "speaker_id": "2",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027190.1.26",
          "form": "응!!! 엄청 자라",
          "original_form": "응!!! 엄청 자라",
          "speaker_id": "2",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027190.1.27",
          "form": "엄청 길어져",
