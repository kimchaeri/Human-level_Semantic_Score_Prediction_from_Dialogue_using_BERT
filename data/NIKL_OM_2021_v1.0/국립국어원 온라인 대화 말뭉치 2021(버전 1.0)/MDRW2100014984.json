        },
        {
          "id": "MDRW2100014984.1.2",
          "form": "아뇨 저 괜찮은데",
          "original_form": "아뇨 저 괜찮은데",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014984.1.3",
          "form": "name2님이 스트레스 있어보이셔서",
