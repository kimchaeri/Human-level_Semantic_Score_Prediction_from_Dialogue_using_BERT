      "utterance": [
        {
          "id": "MDRW2100018807.1.1",
          "form": "잡긴 잡았는데",
          "original_form": "잡긴 잡았는데",
          "speaker_id": "2",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018807.1.2",
          "form": "ㅇㅇㅇ엉",
          "original_form": "ㅇㅇㅇ엉",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018807.1.3",
          "form": "잡고 나니 그 시간대에 백신을 맞아야 한다는게 생각남",
          "original_form": "잡고 나니 그 시간대에 백신을 맞아야 한다는게 생각남",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018807.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018807.1.5",
          "form": "어차피백신공결처리잖아",
          "original_form": "어차피백신공결처리잖아",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018807.1.6",
          "form": "우리 아직 공지 안뜸",
          "original_form": "우리 아직 공지 안뜸",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018807.1.7",
          "form": "아 아직도?",
          "original_form": "아 아직도?",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018807.1.8",
          "form": "학교에뜬게뭐야",
          "original_form": "학교에뜬게뭐야",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018807.1.9",
          "form": "대면도모른다며",
          "original_form": "대면도모른다며",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018807.1.10",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018807.1.11",
          "form": "왜 과에 맡기냐고",
          "original_form": "왜 과에 맡기냐고",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018807.1.12",
          "form": "아 다 과로 미룸?",
          "original_form": "아 다 과로 미룸?",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018807.1.13",
          "form": "과에서는공지언제준다는데",
          "original_form": "과에서는공지언제준다는데",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018807.1.14",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018807.1.15",
          "form": "아마 신청 끝나고 할 것 같음",
          "original_form": "아마 신청 끝나고 할 것 같음",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018807.1.16",
          "form": "ㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018807.1.17",
          "form": "멀리사는애들배려하나도안하네",
          "original_form": "멀리사는애들배려하나도안하네",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018807.1.18",
          "form": "그니까 애들 난리났음",
          "original_form": "그니까 애들 난리났음",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018807.1.19",
          "form": "학교가일을아예안하네",
          "original_form": "학교가일을아예안하네",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018807.1.20",
          "form": "그리고수도권이면비대면해야할텐데",
          "original_form": "그리고수도권이면비대면해야할텐데",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018807.1.21",
          "form": "학교측에서강제해야하는거아님?",
          "original_form": "학교측에서강제해야하는거아님?",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018807.1.22",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018807.1.23",
          "form": "학교는 일을 하라 돈만 받아먹지 말고 일을 해라",
          "original_form": "학교는 일을 하라 돈만 받아먹지 말고 일을 해라",
          "speaker_id": "2",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018807.1.24",
          "form": "돈이라도적게받으면몰라",
          "original_form": "돈이라도적게받으면몰라",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018807.1.25",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210827 12:40"
        }
      ]
