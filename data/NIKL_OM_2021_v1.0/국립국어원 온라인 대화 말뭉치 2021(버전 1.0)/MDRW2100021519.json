      "utterance": [
        {
          "id": "MDRW2100021519.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100021519.1.2",
          "form": "아직 안먹었어요ㅠㅠ",
          "original_form": "아직 안먹었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100021519.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100021519.1.4",
          "form": "배 안 고프세요? 전 아까 먹었어요",
          "original_form": "배 안 고프세요? 전 아까 먹었어요",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100021519.1.5",
          "form": "아직은 안고파용ㅎㅎ 점심을 많이 먹어가지궁!",
          "original_form": "아직은 안고파용ㅎㅎ 점심을 많이 먹어가지궁!",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100021519.1.6",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100021519.1.7",
          "form": "골드키위 2 자몽 1/2 귀리쉐이크요",
          "original_form": "골드키위 2 자몽 1/2 귀리쉐이크요",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100021519.1.8",
          "form": "엇 귀리 쉐이크용?",
          "original_form": "엇 귀리 쉐이크용?",
          "speaker_id": "2",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100021519.1.9",
          "form": "요거트에다 귀리 넣어먹는건가요?",
          "original_form": "요거트에다 귀리 넣어먹는건가요?",
          "speaker_id": "2",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100021519.1.10",
          "form": "아뇨 그냥 시판 귀리 가루에 물과 꿀 넣어서 먹었어요",
          "original_form": "아뇨 그냥 시판 귀리 가루에 물과 꿀 넣어서 먹었어요",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100021519.1.11",
          "form": "아하 저도 귀리밥 자주해먹어서",
          "original_form": "아하 저도 귀리밥 자주해먹어서",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100021519.1.12",
          "form": "집에 귀리가 많아요ㅎㅎㅎ",
          "original_form": "집에 귀리가 많아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100021519.1.13",
          "form": "맛있겠네용!",
          "original_form": "맛있겠네용!",
          "speaker_id": "2",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100021519.1.14",
          "form": "전 보리 안 좋아해서요",
          "original_form": "전 보리 안 좋아해서요",
          "speaker_id": "1",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100021519.1.15",
          "form": "엇 오류 나네요",
          "original_form": "엇 오류 나네요",
          "speaker_id": "1",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100021519.1.16",
          "form": "밥에 맨날 보리 넣거든요",
          "original_form": "밥에 맨날 보리 넣거든요",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100021519.1.17",
          "form": "귀리 밥 짓기 반나절정도 전에 물에 미리 불려놓으면",
          "original_form": "귀리 밥 짓기 반나절정도 전에 물에 미리 불려놓으면",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100021519.1.18",
          "form": "부드럽고 맛있어용!",
          "original_form": "부드럽고 맛있어용!",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100021519.1.19",
          "form": "너무 질기다 싶으시면 이 방법도 추천드려용",
          "original_form": "너무 질기다 싶으시면 이 방법도 추천드려용",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100021519.1.20",
          "form": "그렇군요  귀리밥 먹게되면 해봐야 겠어요",
          "original_form": "그렇군요  귀리밥 먹게되면 해봐야 겠어요",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100021519.1.21",
          "form": "ㅎㅎ 귀리도 국산 있으니까 국산으로 드시면 더 좋아용 고소하달까요",
          "original_form": "ㅎㅎ 귀리도 국산 있으니까 국산으로 드시면 더 좋아용 고소하달까요",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100021519.1.22",
          "form": "국산이 더 고소한가봐요? 현재 캐나다꺼 먹고 있어서요 귀리는 이쪽이 많더라구요",
          "original_form": "국산이 더 고소한가봐요? 현재 캐나다꺼 먹고 있어서요 귀리는 이쪽이 많더라구요",
          "speaker_id": "1",
          "time": "20210830 07:21"
        }
      ]
