        {
          "id": "MDRW2100032182.1.1",
          "form": "장은 어디서 보세요",
          "original_form": "장은 어디서 보세요",
          "speaker_id": "2",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100032182.1.2",
          "form": "그냥 왠만한 건 동네 큰 마트에서 사는 편이에요",
          "original_form": "그냥 왠만한 건 동네 큰 마트에서 사는 편이에요",
          "speaker_id": "1",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100032182.1.3",
          "form": "이마트 가요 저도",
          "original_form": "이마트 가요 저도",
          "speaker_id": "2",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032182.1.4",
          "form": "한 번 가면 얼마어치나 사오세요?",
          "original_form": "한 번 가면 얼마어치나 사오세요?",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032182.1.5",
          "form": "저희 둘이 살아서 한 5만정도?",
          "original_form": "저희 둘이 살아서 한 5만정도?",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032182.1.6",
          "form": "되는거 같아요",
          "original_form": "되는거 같아요",
          "speaker_id": "2",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032182.1.7",
          "form": "너무 많이 사는 것보단 그때그때 사는 게 좋더라구요 ㅎㅎ",
          "original_form": "너무 많이 사는 것보단 그때그때 사는 게 좋더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100032182.1.8",
          "form": "네 상해요 많이 사면 ㅋㅋ",
          "original_form": "네 상해요 많이 사면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032182.1.9",
          "form": "맞아요ㅋㅋ 처치곤란",
          "original_form": "맞아요ㅋㅋ 처치곤란",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032182.1.10",
          "form": "밀키트 자주 사요 그래서",
          "original_form": "밀키트 자주 사요 그래서",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032182.1.11",
          "form": "한번에 똭",
          "original_form": "한번에 똭",
          "speaker_id": "2",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032182.1.12",
          "form": "밀키트 편하고 좋더라구요",
          "original_form": "밀키트 편하고 좋더라구요",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032182.1.13",
          "form": "네 짬뽕순두부",
          "original_form": "네 짬뽕순두부",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032182.1.14",
          "form": "맛있던데",
          "original_form": "맛있던데",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032182.1.15",
