      "utterance": [
        {
          "id": "MDRW2100033183.1.1",
          "form": "나 오늘 초콜릿 먹었다",
          "original_form": "나 오늘 초콜릿 먹었다",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033183.1.2",
          "form": "헐 맛있겠다?",
          "original_form": "헐 맛있겠다?",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033183.1.3",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033183.1.4",
          "form": "밀크초콜릿?",
          "original_form": "밀크초콜릿?",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033183.1.5",
          "form": "이마트 노브랜드 다크 초콜릿 머금",
          "original_form": "이마트 노브랜드 다크 초콜릿 머금",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033183.1.6",
          "form": "그거 의외로 안써서 맛있엉",
          "original_form": "그거 의외로 안써서 맛있엉",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033183.1.7",
          "form": "추천함",
          "original_form": "추천함",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033183.1.8",
          "form": "우왕 담에 먹어볼게",
          "original_form": "우왕 담에 먹어볼게",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033183.1.9",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033183.1.10",
          "form": "오늘 저녁은 뭐 먹을거야?",
          "original_form": "오늘 저녁은 뭐 먹을거야?",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033183.1.11",
          "form": "나는.... 엄마가 오늘 백신맞았대서 족발먹재",
          "original_form": "나는.... 엄마가 오늘 백신맞았대서 족발먹재",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100033183.1.12",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100033183.1.13",
          "form": "무슨 연관인진 모르겠지만 주문함",
          "original_form": "무슨 연관인진 모르겠지만 주문함",
          "speaker_id": "2",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100033183.1.14",
          "form": "너는 저녁 뭐먹엉",
          "original_form": "너는 저녁 뭐먹엉",
          "speaker_id": "2",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100033183.1.15",
          "form": "나는 마라탕 먹을까 생각 중",
          "original_form": "나는 마라탕 먹을까 생각 중",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100033183.1.16",
          "form": "마라탕 먹고싶은데...가족들이 먹으려나?",
          "original_form": "마라탕 먹고싶은데...가족들이 먹으려나?",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100033183.1.17",
          "form": "음...... 괜찮을것같은뎅",
          "original_form": "음...... 괜찮을것같은뎅",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100033183.1.18",
          "form": "가족들이 싫어해?",
          "original_form": "가족들이 싫어해?",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100033183.1.19",
          "form": "싫어하는건 아닌데 좋아하지도 않아",
          "original_form": "싫어하는건 아닌데 좋아하지도 않아",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100033183.1.20",
          "form": "음.... 애매하다",
          "original_form": "음.... 애매하다",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033183.1.21",
          "form": "그래도 시켜먹는걸 제안~",
          "original_form": "그래도 시켜먹는걸 제안~",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033183.1.22",
          "form": "오키오키 설득해봐야지!!",
          "original_form": "오키오키 설득해봐야지!!",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033183.1.23",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "2",
