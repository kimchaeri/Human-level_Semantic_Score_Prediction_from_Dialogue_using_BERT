      "utterance": [
        {
          "id": "MDRW2100005956.1.1",
          "form": "둘다 내가좋아하는 전이에요",
          "original_form": "둘다 내가좋아하는 전이에요",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100005956.1.2",
          "form": "저도요 ㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100005956.1.3",
          "form": "김치전 특히 좋아해요 ㅠㅠㅋㅋㅋ",
          "original_form": "김치전 특히 좋아해요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100005956.1.4",
          "form": "김치는 치킨만큼 사랑하죠!!",
          "original_form": "김치는 치킨만큼 사랑하죠!!",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100005956.1.5",
          "form": "캬 맞죠",
          "original_form": "캬 맞죠",
          "speaker_id": "1",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100005956.1.6",
          "form": "먹을때가 행복해요",
          "original_form": "먹을때가 행복해요",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005956.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005956.1.8",
          "form": "맛집 혹은 배달 등등해서 먹을때가",
          "original_form": "맛집 혹은 배달 등등해서 먹을때가",
          "speaker_id": "1",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005956.1.9",
          "form": "진짜 제일 좋죠..",
          "original_form": "진짜 제일 좋죠..",
          "speaker_id": "1",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005956.1.10",
          "form": "당연하죠!!!",
          "original_form": "당연하죠!!!",
          "speaker_id": "2",
          "time": "20210907 17:46"
        },
        {
          "id": "MDRW2100005956.1.11",
          "form": "먹기위해 사니깐요",
          "original_form": "먹기위해 사니깐요",
          "speaker_id": "2",
          "time": "20210907 17:46"
        },
        {
          "id": "MDRW2100005956.1.12",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005956.1.13",
          "form": "저는 모두의마블 이벤트 당첨되서 쿨라타먹어요",
          "original_form": "저는 모두의마블 이벤트 당첨되서 쿨라타먹어요",
          "speaker_id": "2",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005956.1.14",
          "form": "공짜 맛있네욤",
          "original_form": "공짜 맛있네욤",
          "speaker_id": "2",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005956.1.15",
          "form": "오.. 부럽네요 ㅋㅋㅋ",
          "original_form": "오.. 부럽네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100005956.1.16",
          "form": "처음으로 당첨 된거라서 혹시나 해서 내밀었는데 공짜네요",
          "original_form": "처음으로 당첨 된거라서 혹시나 해서 내밀었는데 공짜네요",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100005956.1.17",
          "form": "ㅋㅋㅋㅋㅋ 공짜는 뭐든지 진리죠..ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 공짜는 뭐든지 진리죠..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005956.1.18",
          "form": "맞앙요ㅋㅋㅋ",
          "original_form": "맞앙요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100005956.1.19",
          "form": "낼도  시도 해볼꺼에요",
          "original_form": "낼도  시도 해볼꺼에요",
          "speaker_id": "2",
          "time": "20210907 18:09"
        },
        {
          "id": "MDRW2100005956.1.20",
          "form": "제가 늦게 시작해서 알게 된 순간부터",
          "original_form": "제가 늦게 시작해서 알게 된 순간부터",
