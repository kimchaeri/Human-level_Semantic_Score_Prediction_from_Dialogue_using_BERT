      "utterance": [
        {
          "id": "MDRW2100022428.1.1",
          "form": "유튭보시나요",
          "original_form": "유튭보시나요",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022428.1.2",
          "form": "네 어떤거 보세요?",
          "original_form": "네 어떤거 보세요?",
          "speaker_id": "2",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100022428.1.3",
          "form": "여행유튭이요!",
          "original_form": "여행유튭이요!",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022428.1.4",
          "form": "오 누구보세요?",
          "original_form": "오 누구보세요?",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022428.1.5",
          "form": "저는 신아로미님 요새 자주보고있어요",
          "original_form": "저는 신아로미님 요새 자주보고있어요",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022428.1.6",
          "form": "그리고 여락이들! 예전에 많이 봤네요",
          "original_form": "그리고 여락이들! 예전에 많이 봤네요",
          "speaker_id": "2",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022428.1.7",
          "form": "여락이들! 북한근처간거 ㅎㅎ",
          "original_form": "여락이들! 북한근처간거 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022428.1.8",
          "form": "와 그거는 안 봤나봐요 어우",
          "original_form": "와 그거는 안 봤나봐요 어우",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022428.1.9",
          "form": "북한근처에서 뭐 하나요?",
          "original_form": "북한근처에서 뭐 하나요?",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022428.1.10",
          "form": "관광이요ㅋㅋㅋ 중국쪽에서",
          "original_form": "관광이요ㅋㅋㅋ 중국쪽에서",
          "speaker_id": "1",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022428.1.11",
          "form": "오 ㅋㅋㅋㅋㅋㅋ 재밌겠네요",
          "original_form": "오 ㅋㅋㅋㅋㅋㅋ 재밌겠네요",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022428.1.12",
          "form": "심심했는데 봐야겠네요",
          "original_form": "심심했는데 봐야겠네요",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022428.1.13",
          "form": "여락이들 처음 뜬게 시베리아가는거 였는데",
          "original_form": "여락이들 처음 뜬게 시베리아가는거 였는데",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022428.1.14",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022428.1.15",
          "form": "오 첫영상부터 보셨군요",
          "original_form": "오 첫영상부터 보셨군요",
          "speaker_id": "1",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100022428.1.16",
          "form": "네 당연하죠~",
          "original_form": "네 당연하죠~",
          "speaker_id": "2",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100022428.1.17",
          "form": "여행 유튜브 보면 참 대신 여행가는 것 같고 힐링되는 것 같아요",
          "original_form": "여행 유튜브 보면 참 대신 여행가는 것 같고 힐링되는 것 같아요",
          "speaker_id": "2",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100022428.1.18",
          "form": "코로나라서 더 못돌아다니니까 좋더라고요 대신 만족할 수 있고",
          "original_form": "코로나라서 더 못돌아다니니까 좋더라고요 대신 만족할 수 있고",
          "speaker_id": "2",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100022428.1.19",
          "form": "요즘 대리만족 중이에여",
          "original_form": "요즘 대리만족 중이에여",
          "speaker_id": "1",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100022428.1.20",
          "form": "그쵸 ㅎㅎ",
          "original_form": "그쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100022428.1.21",
          "form": "혹시 다른 건 뭐 보시는거 있나요?",
          "original_form": "혹시 다른 건 뭐 보시는거 있나요?",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100022428.1.22",
          "form": "빠니보틀이요!",
          "original_form": "빠니보틀이요!",
          "speaker_id": "1",
          "time": "20210829 14:37"
        }
      ]
