      "utterance": [
        {
          "id": "MDRW2100023862.1.1",
          "form": "이따 운동가시나요",
          "original_form": "이따 운동가시나요",
          "speaker_id": "2",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100023862.1.2",
          "form": "고민중",
          "original_form": "고민중",
          "speaker_id": "1",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100023862.1.3",
          "form": "다녀오세요",
          "original_form": "다녀오세요",
          "speaker_id": "2",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100023862.1.4",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023862.1.5",
          "form": "전 오늘 휴식이에요",
          "original_form": "전 오늘 휴식이에요",
          "speaker_id": "2",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023862.1.6",
          "form": "ㅎㅎ 저도그럼",
          "original_form": "ㅎㅎ 저도그럼",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023862.1.7",
          "form": "아녜요 운동을 해줘야해요",
          "original_form": "아녜요 운동을 해줘야해요",
          "speaker_id": "2",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023862.1.8",
          "form": "같이해요",
          "original_form": "같이해요",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023862.1.9",
          "form": "전 이따 홈트하려구요",
          "original_form": "전 이따 홈트하려구요",
          "speaker_id": "2",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100023862.1.10",
          "form": "전 플랭크하려고요",
          "original_form": "전 플랭크하려고요",
          "speaker_id": "1",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100023862.1.11",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100023862.1.12",
          "form": "10초 할 수 있어요",
          "original_form": "10초 할 수 있어요",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023862.1.13",
          "form": "전 일분은 해요",
          "original_form": "전 일분은 해요",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023862.1.14",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023862.1.15",
          "form": "여덟번씩해요",
          "original_form": "여덟번씩해요",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023862.1.16",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023862.1.17",
          "form": "하다보면 늘거예요",
          "original_form": "하다보면 늘거예요",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023862.1.18",
          "form": "요즘 너무 많이먹어요",
          "original_form": "요즘 너무 많이먹어요",
