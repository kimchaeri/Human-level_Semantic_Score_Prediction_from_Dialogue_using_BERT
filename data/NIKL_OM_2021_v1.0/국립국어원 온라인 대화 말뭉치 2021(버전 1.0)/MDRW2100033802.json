      "utterance": [
        {
          "id": "MDRW2100033802.1.1",
          "form": "집안일 자주하세요?",
          "original_form": "집안일 자주하세요?",
          "speaker_id": "2",
          "time": "20210825 03:45"
        },
        {
          "id": "MDRW2100033802.1.2",
          "form": "따로 살아서 제가 하는 편이에요",
          "original_form": "따로 살아서 제가 하는 편이에요",
          "speaker_id": "1",
          "time": "20210825 03:46"
        },
        {
          "id": "MDRW2100033802.1.3",
          "form": "집안 일 하시나요",
          "original_form": "집안 일 하시나요",
          "speaker_id": "1",
          "time": "20210825 03:46"
        },
        {
          "id": "MDRW2100033802.1.4",
          "form": "매일해요",
          "original_form": "매일해요",
          "speaker_id": "2",
          "time": "20210825 03:46"
        },
        {
          "id": "MDRW2100033802.1.5",
          "form": "자취하시는군요",
          "original_form": "자취하시는군요",
          "speaker_id": "2",
          "time": "20210825 03:46"
        },
        {
          "id": "MDRW2100033802.1.6",
          "form": "가장 하기 싫은  집안일은 무엇인가요",
          "original_form": "가장 하기 싫은  집안일은 무엇인가요",
          "speaker_id": "1",
          "time": "20210825 03:47"
        },
        {
          "id": "MDRW2100033802.1.7",
          "form": "음식물처리요",
          "original_form": "음식물처리요",
          "speaker_id": "2",
          "time": "20210825 03:47"
        },
        {
          "id": "MDRW2100033802.1.8",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210825 03:47"
        },
        {
          "id": "MDRW2100033802.1.9",
          "form": "제때 제때 안 버려주면 벌레 꼬이고",
          "original_form": "제때 제때 안 버려주면 벌레 꼬이고",
          "speaker_id": "1",
          "time": "20210825 03:47"
        },
        {
          "id": "MDRW2100033802.1.10",
          "form": "룸메이트랑 생활한적있으세요?",
          "original_form": "룸메이트랑 생활한적있으세요?",
          "speaker_id": "2",
          "time": "20210825 03:47"
        },
        {
          "id": "MDRW2100033802.1.11",
          "form": "냄새도 나고 그렇더라고요",
          "original_form": "냄새도 나고 그렇더라고요",
          "speaker_id": "1",
          "time": "20210825 03:47"
        },
        {
          "id": "MDRW2100033802.1.12",
          "form": "아니요. 룸메이트랑 생활해 보신 적 있으신가요",
          "original_form": "아니요. 룸메이트랑 생활해 보신 적 있으신가요",
          "speaker_id": "1",
          "time": "20210825 03:48"
        },
        {
          "id": "MDRW2100033802.1.13",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210825 03:48"
        },
        {
          "id": "MDRW2100033802.1.14",
          "form": "언제 독립하셨어요?",
          "original_form": "언제 독립하셨어요?",
          "speaker_id": "2",
          "time": "20210825 03:48"
        },
        {
          "id": "MDRW2100033802.1.15",
          "form": "독립은 아니고 주택인데 가족이랑 층을 따로 살아요",
          "original_form": "독립은 아니고 주택인데 가족이랑 층을 따로 살아요",
          "speaker_id": "1",
          "time": "20210825 03:48"
        },
        {
          "id": "MDRW2100033802.1.16",
          "form": "가족이랑 사시나요?",
          "original_form": "가족이랑 사시나요?",
          "speaker_id": "1",
          "time": "20210825 03:49"
        },
        {
          "id": "MDRW2100033802.1.17",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "2",
          "time": "20210825 03:49"
        },
        {
          "id": "MDRW2100033802.1.18",
          "form": "네 같이 살아요",
          "original_form": "네 같이 살아요",
          "speaker_id": "2",
          "time": "20210825 03:49"
        },
        {
          "id": "MDRW2100033802.1.19",
          "form": "그러시구나  가족이랑 사는 거 어떠신가요",
          "original_form": "그러시구나  가족이랑 사는 거 어떠신가요",
          "speaker_id": "1",
          "time": "20210825 03:49"
        },
        {
          "id": "MDRW2100033802.1.20",
          "form": "나쁘지않아요",
          "original_form": "나쁘지않아요",
          "speaker_id": "2",
          "time": "20210825 03:50"
        }
      ]
