      "utterance": [
        {
          "id": "MDRW2100002697.1.1",
          "form": "오늘 너무 더워 진짜",
          "original_form": "오늘 너무 더워 진짜",
          "speaker_id": "2",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.2",
          "form": "여기는 맑아!",
          "original_form": "여기는 맑아!",
          "speaker_id": "1",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.3",
          "form": "샤워 필수야 오늘은",
          "original_form": "샤워 필수야 오늘은",
          "speaker_id": "2",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.4",
          "form": "맞아 초가을인데 아직 더워",
          "original_form": "맞아 초가을인데 아직 더워",
          "speaker_id": "1",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.5",
          "form": "땀이 날 정도의 날씨라는거구나!",
          "original_form": "땀이 날 정도의 날씨라는거구나!",
          "speaker_id": "1",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.6",
          "form": "입추가 지나서 시원할 줄 알았는대",
          "original_form": "입추가 지나서 시원할 줄 알았는대",
          "speaker_id": "2",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.7",
          "form": "속았어",
          "original_form": "속았어",
          "speaker_id": "2",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.8",
          "form": "나도 아이스아메리카노를 마시는데 금방 녹아버렸어.",
          "original_form": "나도 아이스아메리카노를 마시는데 금방 녹아버렸어.",
          "speaker_id": "1",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.9",
          "form": "누구에게 속았니?",
          "original_form": "누구에게 속았니?",
          "speaker_id": "1",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.10",
          "form": "조상님의 지혜",
          "original_form": "조상님의 지혜",
          "speaker_id": "2",
          "time": "20210820 14:52"
        },
        {
          "id": "MDRW2100002697.1.11",
          "form": "거짓 지혜였어",
          "original_form": "거짓 지혜였어",
          "speaker_id": "2",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002697.1.12",
          "form": "오. 친구. 아무도 널 속이지 않았어.",
          "original_form": "오. 친구. 아무도 널 속이지 않았어.",
          "speaker_id": "1",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002697.1.13",
          "form": "오. 친구. 너를 더 소중히 여기렴.",
          "original_form": "오. 친구. 너를 더 소중히 여기렴.",
          "speaker_id": "1",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002697.1.14",
          "form": "ㅠㅠ혹시 더위먹었니?",
          "original_form": "ㅠㅠ혹시 더위먹었니?",
          "speaker_id": "2",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002697.1.15",
          "form": "아무래도 방금 좀 덥게 잤더니 피곤하네.",
          "original_form": "아무래도 방금 좀 덥게 잤더니 피곤하네.",
          "speaker_id": "1",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002697.1.16",
          "form": "날씨가 계속 오락가락 하는구나.",
          "original_form": "날씨가 계속 오락가락 하는구나.",
          "speaker_id": "1",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002697.1.17",
          "form": "오능 밤에 또 비가올까?",
          "original_form": "오능 밤에 또 비가올까?",
          "speaker_id": "1",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002697.1.18",
          "form": "흠 그건 하늘만이 아실 것 같아",
          "original_form": "흠 그건 하늘만이 아실 것 같아",
          "speaker_id": "2",
          "time": "20210820 14:54"
        },
        {
          "id": "MDRW2100002697.1.19",
          "form": "요즘 날씨가 너어무 오락가락하더라",
          "original_form": "요즘 날씨가 너어무 오락가락하더라",
          "speaker_id": "2",
          "time": "20210820 14:54"
        },
        {
          "id": "MDRW2100002697.1.20",
          "form": "마른하늘에 비도내려",
          "original_form": "마른하늘에 비도내려",
          "speaker_id": "2",
          "time": "20210820 14:54"
        },
        {
          "id": "MDRW2100002697.1.21",
          "form": "기상청은 믿지 않는더야?",
          "original_form": "기상청은 믿지 않는더야?",
          "speaker_id": "1",
          "time": "20210820 14:54"
        },
        {
          "id": "MDRW2100002697.1.22",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002697.1.23",
          "form": "날씨가 오락가락하긴해",
          "original_form": "날씨가 오락가락하긴해",
          "speaker_id": "1",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002697.1.24",
          "form": "완전 불신해",
          "original_form": "완전 불신해",
          "speaker_id": "2",
          "time": "20210820 14:55"
        }
      ]
    }
  ]
