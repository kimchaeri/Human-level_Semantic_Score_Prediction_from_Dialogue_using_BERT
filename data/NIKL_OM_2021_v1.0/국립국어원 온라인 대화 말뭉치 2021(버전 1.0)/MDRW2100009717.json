      "utterance": [
        {
          "id": "MDRW2100009717.1.1",
          "form": "오늘도 비가 오네요 ...",
          "original_form": "오늘도 비가 오네요 ...",
          "speaker_id": "1",
          "time": "20210823 08:10"
        },
        {
          "id": "MDRW2100009717.1.2",
          "form": "그쵸 태풍 온다더니",
          "original_form": "그쵸 태풍 온다더니",
          "speaker_id": "2",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100009717.1.3",
          "form": "비가 많이 와서 놀랐어요",
          "original_form": "비가 많이 와서 놀랐어요",
          "speaker_id": "2",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100009717.1.4",
          "form": "네 하루종일 오더라구요 ㅠㅠ",
          "original_form": "네 하루종일 오더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100009717.1.5",
          "form": "전 오늘 재택이어서 나갈일 없었는데",
          "original_form": "전 오늘 재택이어서 나갈일 없었는데",
          "speaker_id": "2",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100009717.1.6",
          "form": "내일은 다시 나가야해서 걱정이네요 비 많이 오면ㅠㅠ",
          "original_form": "내일은 다시 나가야해서 걱정이네요 비 많이 오면ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100009717.1.7",
          "form": "내일도 온다니까 조심하세요",
          "original_form": "내일도 온다니까 조심하세요",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100009717.1.8",
          "form": "전 일 갈때 바지도 다 접어서 갔어요,, 젖어버릴까봐요",
          "original_form": "전 일 갈때 바지도 다 접어서 갔어요,, 젖어버릴까봐요",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100009717.1.9",
          "form": "내일의 제 모습 아닐지..",
          "original_form": "내일의 제 모습 아닐지..",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100009717.1.10",
          "form": "버스타고 다니는데 진짜 비오면 힘들어요ㅠㅠ",
          "original_form": "버스타고 다니는데 진짜 비오면 힘들어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100009717.1.11",
          "form": "아 버스면 정말 답이 없죠",
          "original_form": "아 버스면 정말 답이 없죠",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100009717.1.12",
          "form": "버스 탈 때도 우산 접어야 하고.. 막",
          "original_form": "버스 탈 때도 우산 접어야 하고.. 막",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100009717.1.13",
          "form": "그쵸 차라리 눈오는게 좋아요ㅠㅠㅠ",
          "original_form": "그쵸 차라리 눈오는게 좋아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100009717.1.14",
          "form": "비오는 날은 집에만 있을땐 진짜 좋은데",
          "original_form": "비오는 날은 집에만 있을땐 진짜 좋은데",
          "speaker_id": "2",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100009717.1.15",
          "form": "네!! 비는 보는 것만 좋은 ㅎㅎ",
          "original_form": "네!! 비는 보는 것만 좋은 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100009717.1.16",
          "form": "빗소리 들을땐 좋아요 안에서",
          "original_form": "빗소리 들을땐 좋아요 안에서",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100009717.1.17",
          "form": "왠지 분우ㅣ기도 있고ㅎㅎ",
          "original_form": "왠지 분우ㅣ기도 있고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100009717.1.18",
          "form": "맞아여 !! 이거 하나만 좋고",
          "original_form": "맞아여 !! 이거 하나만 좋고",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100009717.1.19",
          "form": "밖에 나갈 땐 정말 싫어요",
          "original_form": "밖에 나갈 땐 정말 싫어요",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100009717.1.20",
          "form": "그쵸 그래서 저도 비오는 날은",
          "original_form": "그쵸 그래서 저도 비오는 날은",
          "speaker_id": "2",
          "time": "20210824 11:43"
        },
        {
          "id": "MDRW2100009717.1.21",
          "form": "약속 취소해요",
          "original_form": "약속 취소해요",
          "speaker_id": "2",
          "time": "20210824 11:43"
        },
        {
          "id": "MDRW2100009717.1.22",
          "form": "ㅋㅋ ㅋㅋ 그게 올바른 선택 같아요!",
          "original_form": "ㅋㅋ ㅋㅋ 그게 올바른 선택 같아요!",
          "speaker_id": "1",
