      "utterance": [
        {
          "id": "MDRW2100028839.1.1",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028839.1.2",
          "form": "넵 맞았습니다",
          "original_form": "넵 맞았습니다",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028839.1.3",
          "form": "2차까지요?",
          "original_form": "2차까지요?",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028839.1.4",
          "form": "아뇨 1차 지난주에 맞았어요",
          "original_form": "아뇨 1차 지난주에 맞았어요",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028839.1.5",
          "form": "다맞으셨어요?",
          "original_form": "다맞으셨어요?",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028839.1.6",
          "form": "저도 저번주 토요일에 1차 맞았어요",
          "original_form": "저도 저번주 토요일에 1차 맞았어요",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100028839.1.7",
          "form": "아 전 금요일 오후요! ㅋㅋ",
          "original_form": "아 전 금요일 오후요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100028839.1.8",
          "form": "많이 아프셨어요?",
          "original_form": "많이 아프셨어요?",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100028839.1.9",
          "form": "1차맞고 어떠셨어요?",
          "original_form": "1차맞고 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100028839.1.10",
          "form": "전 팔이 좀 아프고",
          "original_form": "전 팔이 좀 아프고",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100028839.1.11",
          "form": "맞은날 밤에 살짝 오한 나길래 약먹고 잤어요",
          "original_form": "맞은날 밤에 살짝 오한 나길래 약먹고 잤어요",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028839.1.12",
          "form": "전 첫날은 좀 아프다가 둘쨋날 팔 못들었는데 귀신같이 멀쩡해지더라고요 아직까진 약 안먹고 괜찮아요",
          "original_form": "전 첫날은 좀 아프다가 둘쨋날 팔 못들었는데 귀신같이 멀쩡해지더라고요 아직까진 약 안먹고 괜찮아요",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028839.1.13",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028839.1.14",
          "form": "문제는 2차인가봐요ㅜㅜ",
          "original_form": "문제는 2차인가봐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028839.1.15",
          "form": "다들아파서ㅜㅜ",
          "original_form": "다들아파서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028839.1.16",
          "form": "아 걱정돼요!!! 더 아프면 안되는데요ㅜ",
          "original_form": "아 걱정돼요!!! 더 아프면 안되는데요ㅜ",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028839.1.17",
          "form": "그럼9일에 맞으시겠네요",
          "original_form": "그럼9일에 맞으시겠네요",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028839.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028839.1.19",
          "form": "잘아시네요",
          "original_form": "잘아시네요",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028839.1.20",
          "form": "예방주사가 이렇게 무서운거라니",
          "original_form": "예방주사가 이렇게 무서운거라니",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028839.1.21",
          "form": "그래도 코로나가 더 무서우니까요ㅜㅠ",
          "original_form": "그래도 코로나가 더 무서우니까요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028839.1.22",
          "form": "무섭긴하죠",
          "original_form": "무섭긴하죠",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028839.1.23",
          "form": "마스크 언제벗나요ㅜㅜ",
          "original_form": "마스크 언제벗나요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028839.1.24",
          "form": "그니까요ㅜㅜ 대신 위생적이긴하죠? 타인의 입냄새도 안맡고",
          "original_form": "그니까요ㅜㅜ 대신 위생적이긴하죠? 타인의 입냄새도 안맡고",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028839.1.25",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028839.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028839.1.27",
          "form": "근데 답답해요",
          "original_form": "근데 답답해요",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028839.1.28",
          "form": "맞아요 회사에서도 계속 끼니깡ㆍ",
          "original_form": "맞아요 회사에서도 계속 끼니깡ㆍ",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028839.1.29",
          "form": "치아에도 안좋대요",
          "original_form": "치아에도 안좋대요",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028839.1.30",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028839.1.31",
          "form": "회사에서 너무 갑갑하더라구요",
          "original_form": "회사에서 너무 갑갑하더라구요",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028839.1.32",
          "form": "회사에서 점심은 어떻게 드세요?",
          "original_form": "회사에서 점심은 어떻게 드세요?",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028839.1.33",
          "form": "코로나땜시",
          "original_form": "코로나땜시",
