      "utterance": [
        {
          "id": "MDRW2100007953.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007953.1.2",
          "form": "키우진 않고 임보를 해요!",
          "original_form": "키우진 않고 임보를 해요!",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100007953.1.3",
          "form": "헤어질때 슬프잫아여",
          "original_form": "헤어질때 슬프잫아여",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100007953.1.4",
          "form": "그건 글치만 제가 키울 여건이 안되네요ㅜㅜ",
          "original_form": "그건 글치만 제가 키울 여건이 안되네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100007953.1.5",
          "form": "저보다 좋은 가족들을 만나게 해 주는데 의의를 두고있어요 ㅋㅋㅋㅋ",
          "original_form": "저보다 좋은 가족들을 만나게 해 주는데 의의를 두고있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100007953.1.6",
          "form": "name1님은 반려동물이랑 같이 사시나요?",
          "original_form": "&name1&님은 반려동물이랑 같이 사시나요?",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100007953.1.7",
          "form": "아니오",
          "original_form": "아니오",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100007953.1.8",
          "form": "아하 아쉽네요~ 예쁜 아이들이 참 많은데",
          "original_form": "아하 아쉽네요~ 예쁜 아이들이 참 많은데",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100007953.1.9",
          "form": "케어 시간이 안되사요",
          "original_form": "케어 시간이 안되사요",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100007953.1.10",
          "form": "하긴 그렇죠 역시 반려동물은 돈많은 백수가 함께해야 ㅋㅋㅋㅋㅋ",
          "original_form": "하긴 그렇죠 역시 반려동물은 돈많은 백수가 함께해야 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100007953.1.11",
          "form": "ㅎㅎ 맞아요",
          "original_form": "ㅎㅎ 맞아요",
          "speaker_id": "1",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100007953.1.12",
          "form": "저는 그래서 길냥이들 구조하는 데 전력을 다하는 중이에요 ㅋㅋㅋㅋ",
          "original_form": "저는 그래서 길냥이들 구조하는 데 전력을 다하는 중이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100007953.1.13",
          "form": "무섭지 않아요?",
          "original_form": "무섭지 않아요?",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100007953.1.14",
          "form": "야옹이들은 무섭진 않아요!",
          "original_form": "야옹이들은 무섭진 않아요!",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100007953.1.15",
          "form": "길냥이들은 커다란 아이들도 없고 다들 못목어가지고ㅜ",
          "original_form": "길냥이들은 커다란 아이들도 없고 다들 못목어가지고ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100007953.1.16",
          "form": "아무래도 들고양이다 보니 거칠잖아요",
          "original_form": "아무래도 들고양이다 보니 거칠잖아요",
          "speaker_id": "1",
          "time": "20210822 15:52"
        }
      ]
