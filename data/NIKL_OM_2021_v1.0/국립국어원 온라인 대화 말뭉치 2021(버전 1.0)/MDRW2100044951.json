      "utterance": [
        {
          "id": "MDRW2100044951.1.1",
          "form": "매운거 좋아하시나보네요 전 맵찔인데 좋아는 해요!!",
          "original_form": "매운거 좋아하시나보네요 전 맵찔인데 좋아는 해요!!",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044951.1.2",
          "form": "닭발 오돌뼈 좋아요!!!",
          "original_form": "닭발 오돌뼈 좋아요!!!",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044951.1.3",
          "form": "맵찔이는 아닌데 그렇다고 막 잘먹지는 못해요 ㅎㅎ",
          "original_form": "맵찔이는 아닌데 그렇다고 막 잘먹지는 못해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044951.1.4",
          "form": "저는 뼈있는거 못먹어여ㅠ",
          "original_form": "저는 뼈있는거 못먹어여ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044951.1.5",
          "form": "오 닭발은 무뼈도 있긴 한데 안드시겠죠!!",
          "original_form": "오 닭발은 무뼈도 있긴 한데 안드시겠죠!!",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044951.1.6",
          "form": "네 그냥 김이랑 밥만 줘도 맛있어요!",
          "original_form": "네 그냥 김이랑 밥만 줘도 맛있어요!",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044951.1.7",
          "form": "날치알 넣으면 더 굿",
          "original_form": "날치알 넣으면 더 굿",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044951.1.8",
          "form": "김가루 와 밥을 2천원 주고 ㅋㅋ 사먹고",
          "original_form": "김가루 와 밥을 2천원 주고 ㅋㅋ 사먹고",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044951.1.9",
          "form": "김 좋아하세요??",
          "original_form": "김 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044951.1.10",
          "form": "어쩔수 없을때가 있어요 ㅋㅋㅋㅋ",
          "original_form": "어쩔수 없을때가 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044951.1.11",
          "form": "김 좋아하졍 ㅎㅎ 근데 그게 있어여 맛이죻ㅎ",
          "original_form": "김 좋아하졍 ㅎㅎ 근데 그게 있어여 맛이죻ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044951.1.12",
          "form": "ㅋㅋㅋ 맞아여 집에 밥이 없을때가 있어서 시킵니다",
          "original_form": "ㅋㅋㅋ 맞아여 집에 밥이 없을때가 있어서 시킵니다",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044951.1.13",
          "form": "그리고 집에걸로 하면 그맛이 안나여",
          "original_form": "그리고 집에걸로 하면 그맛이 안나여",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044951.1.14",
          "form": "맞아여!!!",
          "original_form": "맞아여!!!",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044951.1.15",
          "form": "돈주고 사먹어야 제맛",
          "original_form": "돈주고 사먹어야 제맛",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044951.1.16",
          "form": "과일 좋아하세요??",
          "original_form": "과일 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044951.1.17",
          "form": "네 ㅎㅎ 좋아해여 지금 비싸서 절제중",
          "original_form": "네 ㅎㅎ 좋아해여 지금 비싸서 절제중",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044951.1.18",
          "form": "여름과일 많이 드셨겠네요!!",
          "original_form": "여름과일 많이 드셨겠네요!!",
