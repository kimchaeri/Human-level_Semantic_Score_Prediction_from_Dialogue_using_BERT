      "utterance": [
        {
          "id": "MDRW2100041501.1.1",
          "form": "안녕하세요! 제가 있는 곳은 아침부터 비가 오는데 하임 님 계신 곳은 어떤가요?",
          "original_form": "안녕하세요! 제가 있는 곳은 아침부터 비가 오는데 하임 님 계신 곳은 어떤가요?",
          "speaker_id": "1",
          "time": "20210906 07:47"
        },
        {
          "id": "MDRW2100041501.1.2",
          "form": "저는 지금 서울인데 여기는 맑아요",
          "original_form": "저는 지금 서울인데 여기는 맑아요",
          "speaker_id": "2",
          "time": "20210906 07:48"
        },
        {
          "id": "MDRW2100041501.1.3",
          "form": "남부지방인가요?",
          "original_form": "남부지방인가요?",
          "speaker_id": "2",
          "time": "20210906 07:48"
        },
        {
          "id": "MDRW2100041501.1.4",
          "form": "네네 부럽네요 얼른 날씨가 개면 좋겠어요",
          "original_form": "네네 부럽네요 얼른 날씨가 개면 좋겠어요",
          "speaker_id": "1",
          "time": "20210906 07:48"
        },
        {
          "id": "MDRW2100041501.1.5",
          "form": "거기도 아침은 조금 쌀쌀한가요?",
          "original_form": "거기도 아침은 조금 쌀쌀한가요?",
          "speaker_id": "1",
          "time": "20210906 07:48"
        },
        {
          "id": "MDRW2100041501.1.6",
          "form": "저도 곧 대구로 돌아가는데 비오려나요",
          "original_form": "저도 곧 대구로 돌아가는데 비오려나요",
          "speaker_id": "2",
          "time": "20210906 07:49"
        },
        {
          "id": "MDRW2100041501.1.7",
          "form": "지금 실내라서 잘 모르겠어요",
          "original_form": "지금 실내라서 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210906 07:49"
        },
        {
          "id": "MDRW2100041501.1.8",
          "form": "음 대구는 안 온다고 들었던 것 같은데",
          "original_form": "음 대구는 안 온다고 들었던 것 같은데",
          "speaker_id": "1",
          "time": "20210906 07:49"
        },
        {
          "id": "MDRW2100041501.1.9",
          "form": "아 그러셨구나",
          "original_form": "아 그러셨구나",
          "speaker_id": "1",
          "time": "20210906 07:49"
        },
        {
          "id": "MDRW2100041501.1.10",
          "form": "갑자기 가을이 된 거 같아요",
          "original_form": "갑자기 가을이 된 거 같아요",
          "speaker_id": "1",
          "time": "20210906 07:49"
        },
        {
          "id": "MDRW2100041501.1.11",
          "form": "그쵸? 이제 9월이니까",
          "original_form": "그쵸? 이제 9월이니까",
          "speaker_id": "2",
          "time": "20210906 07:50"
        },
        {
          "id": "MDRW2100041501.1.12",
          "form": "완연한 가을이네요",
          "original_form": "완연한 가을이네요",
          "speaker_id": "2",
          "time": "20210906 07:50"
        },
        {
          "id": "MDRW2100041501.1.13",
          "form": "시간이 너무 빠른 것 같아요",
          "original_form": "시간이 너무 빠른 것 같아요",
          "speaker_id": "1",
          "time": "20210906 07:51"
        },
        {
          "id": "MDRW2100041501.1.14",
          "form": "name1님 사시는 곳은 원래 비가 많이 오는 지방인가요?",
          "original_form": "&name1&님 사시는 곳은 원래 비가 많이 오는 지방인가요?",
          "speaker_id": "2",
          "time": "20210906 07:51"
        },
        {
          "id": "MDRW2100041501.1.15",
          "form": "아니요! 그건 아닌데 어제부터 갑자기 비가 오더라구요",
          "original_form": "아니요! 그건 아닌데 어제부터 갑자기 비가 오더라구요",
          "speaker_id": "1",
          "time": "20210906 07:51"
        },
        {
          "id": "MDRW2100041501.1.16",
          "form": "아하, 그랬군요",
          "original_form": "아하, 그랬군요",
          "speaker_id": "2",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100041501.1.17",
          "form": "진짜 시간 빨리 가요",
          "original_form": "진짜 시간 빨리 가요",
          "speaker_id": "2",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100041501.1.18",
          "form": "그쵸ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100041501.1.19",
          "form": "이번 해는 코로나라 더 그랬어요",
          "original_form": "이번 해는 코로나라 더 그랬어요",
          "speaker_id": "2",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100041501.1.20",
          "form": "이러다 금방 또 춥겠죠",
          "original_form": "이러다 금방 또 춥겠죠",
          "speaker_id": "1",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100041501.1.21",
          "form": "집안에 박혀있으니",
          "original_form": "집안에 박혀있으니",
          "speaker_id": "2",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100041501.1.22",
          "form": "아 맞아요 괜히 한 거 없이 시간만 가는 기분",
          "original_form": "아 맞아요 괜히 한 거 없이 시간만 가는 기분",
          "speaker_id": "1",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100041501.1.23",
          "form": "맞아요, 봄가을이 너무 짧아요 ㅜ.ㅜ",
          "original_form": "맞아요, 봄가을이 너무 짧아요 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100041501.1.24",
          "form": "맞아요 이제 4계절 아닌 것 같아요ㅜㅜ",
          "original_form": "맞아요 이제 4계절 아닌 것 같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100041501.1.25",
          "form": "거의 2계절 메인에 봄가을은 피처링 수준",
          "original_form": "거의 2계절 메인에 봄가을은 피처링 수준",
          "speaker_id": "2",
          "time": "20210906 07:53"
        },
        {
          "id": "MDRW2100041501.1.26",
          "form": "ㅋㅋㅋㅋㅋ그러니까요",
          "original_form": "ㅋㅋㅋㅋㅋ그러니까요",
          "speaker_id": "1",
          "time": "20210906 07:53"
        },
        {
          "id": "MDRW2100041501.1.27",
