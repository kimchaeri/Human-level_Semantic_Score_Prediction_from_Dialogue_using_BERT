      "utterance": [
        {
          "id": "MDRW2100007482.1.1",
          "form": "name1님은 일하세요?",
          "original_form": "&name1&님은 일하세요?",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007482.1.2",
          "form": "아녀 지금은 백수 ㅋㅋㅋㅋ",
          "original_form": "아녀 지금은 백수 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007482.1.3",
          "form": "name2님은요?? 하고계실듯",
          "original_form": "&name2&님은요?? 하고계실듯",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007482.1.4",
          "form": "전 직장인이요",
          "original_form": "전 직장인이요",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007482.1.5",
          "form": "그럼 구직활동하고 계세요?",
          "original_form": "그럼 구직활동하고 계세요?",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007482.1.6",
          "form": "네근데 \r\n 알바만하고살고싶은 솔직헌심정",
          "original_form": "네근데 \r\n 알바만하고살고싶은 솔직헌심정",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007482.1.7",
          "form": "직장너무어려워요",
          "original_form": "직장너무어려워요",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007482.1.8",
          "form": "요즘 취업 넘 힘들죠",
          "original_form": "요즘 취업 넘 힘들죠",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007482.1.9",
          "form": "알바는 잘구해지나요?",
          "original_form": "알바는 잘구해지나요?",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007482.1.10",
          "form": "알바는언제든지 괜찮죠",
          "original_form": "알바는언제든지 괜찮죠",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007482.1.11",
          "form": "제가 머리쓰는것도 몸쓰는것도 시원찮아서 직장 못다닐거같아요..ㅋㅋㅋ",
          "original_form": "제가 머리쓰는것도 몸쓰는것도 시원찮아서 직장 못다닐거같아요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007482.1.12",
          "form": "코로나로 힘들텐데 그래도 잘구해지나보네요",
          "original_form": "코로나로 힘들텐데 그래도 잘구해지나보네요",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007482.1.13",
          "form": "에이 잘하실것 같은데요 부딪혀봐야알죠",
          "original_form": "에이 잘하실것 같은데요 부딪혀봐야알죠",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007482.1.14",
          "form": "딱히잘하는게없는것도 너무 치명적인거같아요..ㅋㅋㅋ",
          "original_form": "딱히잘하는게없는것도 너무 치명적인거같아요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007482.1.15",
          "form": "재주라고는 그림밖에없었는데 예체능으로 못가서",
          "original_form": "재주라고는 그림밖에없었는데 예체능으로 못가서",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007482.1.16",
          "form": "오 그림 잘그리세요?",
          "original_form": "오 그림 잘그리세요?",
          "speaker_id": "2",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007482.1.17",
          "form": "그럼 이모티콘 이런거 만들어보세요",
          "original_form": "그럼 이모티콘 이런거 만들어보세요",
          "speaker_id": "2",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007482.1.18",
          "form": "ㅋㅋㅋ그것도좋죠 의욕이안날뿐 ..",
          "original_form": "ㅋㅋㅋ그것도좋죠 의욕이안날뿐 ..",
          "speaker_id": "1",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007482.1.19",
          "form": "좋아하는일이면  더잘하실수 있을듯",
          "original_form": "좋아하는일이면  더잘하실수 있을듯",
          "speaker_id": "2",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007482.1.20",
          "form": "아님 캘리그라피? 요런거 알바하셔도 되겠어요 건당 돈받고",
          "original_form": "아님 캘리그라피? 요런거 알바하셔도 되겠어요 건당 돈받고",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007482.1.21",
          "form": "네 근데 그런거 어디서 팔지 싶은것도있어요",
          "original_form": "네 근데 그런거 어디서 팔지 싶은것도있어요",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007482.1.22",
          "form": "전공생이면 알음알음할거같은데",
          "original_form": "전공생이면 알음알음할거같은데",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007482.1.23",
          "form": "필요로 하는곳에가서 그려주고오는거죠",
          "original_form": "필요로 하는곳에가서 그려주고오는거죠",
          "speaker_id": "2",
          "time": "20210822 02:43"
        },
        {
          "id": "MDRW2100007482.1.24",
          "form": "요즘 식당 메뉴판 이런거 많더라구요",
          "original_form": "요즘 식당 메뉴판 이런거 많더라구요",
