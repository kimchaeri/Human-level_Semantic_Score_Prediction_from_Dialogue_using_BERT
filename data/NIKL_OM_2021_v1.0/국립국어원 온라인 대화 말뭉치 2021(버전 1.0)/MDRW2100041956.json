      "utterance": [
        {
          "id": "MDRW2100041956.1.1",
          "form": "갑자기 반려동뭉",
          "original_form": "갑자기 반려동뭉",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041956.1.2",
          "form": "나 반려동물 없음",
          "original_form": "나 반려동물 없음",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041956.1.3",
          "form": "하지만 워너비",
          "original_form": "하지만 워너비",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041956.1.4",
          "form": "나도 워너비.. . ....",
          "original_form": "나도 워너비.. . ....",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041956.1.5",
          "form": "하지만 키우기는",
          "original_form": "하지만 키우기는",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041956.1.6",
          "form": "내가 그 생명을 책임 못질것 같아서 ㅠㅠ",
          "original_form": "내가 그 생명을 책임 못질것 같아서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041956.1.7",
          "form": "하 인정요....",
          "original_form": "하 인정요....",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041956.1.8",
          "form": "영원히 못키울지도",
          "original_form": "영원히 못키울지도",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041956.1.9",
          "form": "어렸을때 강아지는 키웠었어",
          "original_form": "어렸을때 강아지는 키웠었어",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041956.1.10",
          "form": "패키니즈인가? 하는",
          "original_form": "패키니즈인가? 하는",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041956.1.11",
          "form": "오오오오오",
          "original_form": "오오오오오",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041956.1.12",
          "form": "오 신기",
          "original_form": "오 신기",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041956.1.13",
          "form": "구피도 키웠었고 달팽이도",
          "original_form": "구피도 키웠었고 달팽이도",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041956.1.14",
          "form": "언니 뭔가",
          "original_form": "언니 뭔가",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041956.1.15",
          "form": "많은 친구들을 키웠군",
          "original_form": "많은 친구들을 키웠군",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041956.1.16",
          "form": "이것저것 키워봤지!",
          "original_form": "이것저것 키워봤지!",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041956.1.17",
          "form": "달팽이는 진짜 알을 많이 낳아",
          "original_form": "달팽이는 진짜 알을 많이 낳아",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041956.1.18",
          "form": "진짜 많이 낳아... 진짜 많이..",
          "original_form": "진짜 많이 낳아... 진짜 많이..",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041956.1.19",
          "form": "잡학다식하군",
          "original_form": "잡학다식하군",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041956.1.20",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041956.1.21",
          "form": "그정도여?",
          "original_form": "그정도여?",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041956.1.22",
          "form": "융...",
          "original_form": "융...",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041956.1.23",
          "form": "보다보면 징그러",
          "original_form": "보다보면 징그러",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041956.1.24",
          "form": "...난 민달팽이밖에모룸 ㅜ",
          "original_form": "...난 민달팽이밖에모룸 ㅜ",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041956.1.25",
          "form": "으앙",
          "original_form": "으앙",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041956.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041956.1.27",
          "form": "달팽이 밥으로 약간 애호박이랑",
          "original_form": "달팽이 밥으로 약간 애호박이랑",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041956.1.28",
          "form": "당근 상추",
          "original_form": "당근 상추",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041956.1.29",
          "form": "배추",
          "original_form": "배추",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041956.1.30",
          "form": "달걀껍질 이런거 먹였어",
          "original_form": "달걀껍질 이런거 먹였어",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041956.1.31",
          "form": "근데 먹는거에 따라 똥색이 달라진다!!!",
          "original_form": "근데 먹는거에 따라 똥색이 달라진다!!!",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041956.1.32",
          "form": "오!!!!",
          "original_form": "오!!!!",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041956.1.33",
          "form": "그거 실제로 봄???",
          "original_form": "그거 실제로 봄???",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041956.1.34",
          "form": "...신기하다ㅎ",
          "original_form": "...신기하다ㅎ",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041956.1.35",
          "form": "웅!!!!!!!! 당근먹으면 주황똥 싸",
          "original_form": "웅!!!!!!!! 당근먹으면 주황똥 싸",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041956.1.36",
          "form": "나도 함 보고 싶음",
          "original_form": "나도 함 보고 싶음",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041956.1.37",
          "form": "남의 배변에 관심이 많은 사람. . . ...",
          "original_form": "남의 배변에 관심이 많은 사람. . . ...",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041956.1.38",
          "form": "뭐라고요..?!",
          "original_form": "뭐라고요..?!",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041956.1.39",
