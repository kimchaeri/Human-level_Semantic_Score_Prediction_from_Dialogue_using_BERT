      "utterance": [
        {
          "id": "MDRW2100013047.1.1",
          "form": "어제 모르는사람이 종교에 대해서 물음",
          "original_form": "어제 모르는사람이 종교에 대해서 물음",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013047.1.2",
          "form": "마지막에 혼자남을까봐 모르는사람과 대화함 ㅋㅋㅋㅋ",
          "original_form": "마지막에 혼자남을까봐 모르는사람과 대화함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100013047.1.3",
          "form": "근데 기독교였음",
          "original_form": "근데 기독교였음",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100013047.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전도 안했어요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전도 안했어요??",
          "speaker_id": "2",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100013047.1.5",
          "form": "난 종교는 그래서 몬하겠어요 ㅋㅋㅋㅋ",
          "original_form": "난 종교는 그래서 몬하겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100013047.1.6",
          "form": "아니 전도는 안하고",
          "original_form": "아니 전도는 안하고",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013047.1.7",
          "form": "본인은 기독교인데 독실하지않다",
          "original_form": "본인은 기독교인데 독실하지않다",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013047.1.8",
          "form": "교회도 잘안가고",
          "original_form": "교회도 잘안가고",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013047.1.9",
          "form": "성경책도 안읽고",
          "original_form": "성경책도 안읽고",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013047.1.10",
          "form": "그럼..종교가 없는거 아녀요??",
          "original_form": "그럼..종교가 없는거 아녀요??",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100013047.1.11",
          "form": "ㅋㅋㅋㅋㅋ기독교인척 허는거 아녀요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ기독교인척 허는거 아녀요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100013047.1.12",
          "form": "교회에 오빠 만나러가야제 ㅋㅋㅋㅋ",
          "original_form": "교회에 오빠 만나러가야제 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100013047.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱ 대학교때 친구따라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱ 대학교때 친구따라",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013047.1.14",
          "form": "간식준다고 했다가 갔다가",
          "original_form": "간식준다고 했다가 갔다가",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013047.1.15",
          "form": "문화상품권 준다고 꼬여서 갔다대",
          "original_form": "문화상품권 준다고 꼬여서 갔다대",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013047.1.16",
          "form": "나는 불교에 가까운 무교 라고 했더니",
          "original_form": "나는 불교에 가까운 무교 라고 했더니",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013047.1.17",
          "form": "종교믿는사람 어떠냐고",
          "original_form": "종교믿는사람 어떠냐고",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013047.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ부처님오신날엔 절에 밥먹으로 가겠는데~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ부처님오신날엔 절에 밥먹으로 가겠는데~",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013047.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 그러이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 그러이",
          "speaker_id": "1",
          "time": "20210825 11:44"
        },
        {
          "id": "MDRW2100013047.1.20",
          "form": "갑자기 종교얘기로 훅 들어와서",
          "original_form": "갑자기 종교얘기로 훅 들어와서",
          "speaker_id": "1",
          "time": "20210825 11:44"
        },
        {
          "id": "MDRW2100013047.1.21",
          "form": "전도하나 싶었음",
          "original_form": "전도하나 싶었음",
          "speaker_id": "1",
          "time": "20210825 11:45"
        },
        {
          "id": "MDRW2100013047.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ전도한다고 넘어갈 언니 아니잖아여 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전도한다고 넘어갈 언니 아니잖아여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100013047.1.23",
          "form": "종교는 진짜 좀 무서움 ㅋㅋㅋㅋ",
          "original_form": "종교는 진짜 좀 무서움 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100013047.1.24",
          "form": "어 그래서 계속 나는 무교에 가까운 불교라고 얘기함",
          "original_form": "어 그래서 계속 나는 무교에 가까운 불교라고 얘기함",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013047.1.25",
          "form": "절에 가면 마음이 편하다고 하니까 어디든 가면 마음이 편하다고",
          "original_form": "절에 가면 마음이 편하다고 하니까 어디든 가면 마음이 편하다고",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013047.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그카믄 바로 차단 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그카믄 바로 차단 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013047.1.27",
          "form": "첨에 일부러 그래 밑밥깐거 아녀요 ㅋㅋㅋ",
          "original_form": "첨에 일부러 그래 밑밥깐거 아녀요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013047.1.28",
          "form": "독실하지 않다캄서 ㅋㅋㅋ",
          "original_form": "독실하지 않다캄서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013047.1.29",
          "form": "그럴지도 모르지",
          "original_form": "그럴지도 모르지",
          "speaker_id": "1",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013047.1.30",
          "form": "처음부터 차단할까봐",
          "original_form": "처음부터 차단할까봐",
          "speaker_id": "1",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013047.1.31",
          "form": "그러니깐요~",
          "original_form": "그러니깐요~",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013047.1.32",
          "form": "얼마나 머리잘쓰는데요 ㅋㅋㅋㅋㅋ",
          "original_form": "얼마나 머리잘쓰는데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013047.1.33",
          "form": "니같군 ㅋㅋㅋ",
          "original_form": "니같군 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100013047.1.34",
          "form": "우리 새언니는 요새 교회 안 가던데",
          "original_form": "우리 새언니는 요새 교회 안 가던데",
          "speaker_id": "1",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100013047.1.35",
          "form": "코로나 터지고 한 번도 안 갔다고",
          "original_form": "코로나 터지고 한 번도 안 갔다고",
          "speaker_id": "1",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100013047.1.36",
          "form": "대구 도 교회 좀 그만 갔으면 좋겠다",
          "original_form": "대구 도 교회 좀 그만 갔으면 좋겠다",
          "speaker_id": "1",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100013047.1.37",
          "form": "아~새언니 기독교라했져??",
          "original_form": "아~새언니 기독교라했져??",
          "speaker_id": "2",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013047.1.38",
          "form": "교회는진짜",
          "original_form": "교회는진짜",
          "speaker_id": "2",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013047.1.39",
          "form": "내돈 내가미 왜가는동;;",
          "original_form": "내돈 내가미 왜가는동;;",
          "speaker_id": "2",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013047.1.40",
          "form": "그사람들은 우리가 불쌍하긋져??",
          "original_form": "그사람들은 우리가 불쌍하긋져??",
          "speaker_id": "2",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013047.1.41",
          "form": "다행히 11 조는 안 한단다",
          "original_form": "다행히 11 조는 안 한단다",
          "speaker_id": "1",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013047.1.42",
          "form": "지옥간다고 ㅋㅋㅋ",
          "original_form": "지옥간다고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013047.1.43",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013047.1.44",
          "form": "처음에 name3 언니 만났을 때도 그랬지",
          "original_form": "처음에 &name3& 언니 만났을 때도 그랬지",
          "speaker_id": "1",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013047.1.45",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013047.1.46",
          "form": "안 친했을 때",
          "original_form": "안 친했을 때",
          "speaker_id": "1",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013047.1.47",
          "form": "가까운데 있었네요 ㅋㅋㅋㅋ",
          "original_form": "가까운데 있었네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013047.1.48",
          "form": "교회 얘기하려고 해서 당황했다",
          "original_form": "교회 얘기하려고 해서 당황했다",
          "speaker_id": "1",
