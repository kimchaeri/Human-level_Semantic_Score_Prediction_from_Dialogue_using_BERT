      "utterance": [
        {
          "id": "MDRW2100032437.1.1",
          "form": "포케랑 양식 생각 중이에요",
          "original_form": "포케랑 양식 생각 중이에요",
          "speaker_id": "2",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032437.1.2",
          "form": "아 전 뭐 먹을지 모르겠어요",
          "original_form": "아 전 뭐 먹을지 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032437.1.3",
          "form": "맨날 메뉴고민돼요ㅜ",
          "original_form": "맨날 메뉴고민돼요ㅜ",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032437.1.4",
          "form": "배달이에요, 집밥이에요?",
          "original_form": "배달이에요, 집밥이에요?",
          "speaker_id": "2",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100032437.1.5",
          "form": "어제 배달먹어서 집밥먹어야될거 같아요",
          "original_form": "어제 배달먹어서 집밥먹어야될거 같아요",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032437.1.6",
          "form": "하임님은 배달인가요?",
          "original_form": "하임님은 배달인가요?",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032437.1.7",
          "form": "오늘은 나가서 먹을 것 같아요",
          "original_form": "오늘은 나가서 먹을 것 같아요",
          "speaker_id": "2",
          "time": "20210905 17:33"
        },
        {
          "id": "MDRW2100032437.1.8",
          "form": "여행중이라서ㅎㅎ",
          "original_form": "여행중이라서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:33"
        },
        {
          "id": "MDRW2100032437.1.9",
          "form": "여행지에서 먹는 음식은 다맛있죠ㅜ",
          "original_form": "여행지에서 먹는 음식은 다맛있죠ㅜ",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032437.1.10",
          "form": "지금 나갈준비중이세요?",
          "original_form": "지금 나갈준비중이세요?",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032437.1.11",
          "form": "어머니가 꿈나라셔서 좀이따 나갈듯요ㅎㅎ",
          "original_form": "어머니가 꿈나라셔서 좀이따 나갈듯요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100032437.1.12",
          "form": "그럼 맛집 검색은 끝내신건가요?ㅎㅎ",
          "original_form": "그럼 맛집 검색은 끝내신건가요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100032437.1.13",
          "form": "네네, 후보는 온리원!",
          "original_form": "네네, 후보는 온리원!",
          "speaker_id": "2",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100032437.1.14",
          "form": "종각역쪽에 있대요",
          "original_form": "종각역쪽에 있대요",
          "speaker_id": "2",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100032437.1.15",
          "form": "맛있겠어요ㅜㅜㅜ",
          "original_form": "맛있겠어요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100032437.1.16",
          "form": "숙소 주변인가요?",
          "original_form": "숙소 주변인가요?",
          "speaker_id": "1",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100032437.1.17",
          "form": "네넹, 도보 10분 이내래요",
          "original_form": "네넹, 도보 10분 이내래요",
          "speaker_id": "2",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100032437.1.18",
          "form": "좋네요 너무 멀면 배고프죠",
          "original_form": "좋네요 너무 멀면 배고프죠",
          "speaker_id": "1",
          "time": "20210905 17:40"
        },
        {
          "id": "MDRW2100032437.1.19",
          "form": "맞아요, 가깝고 오래 안기다리는 맛집이 좋아요",
          "original_form": "맞아요, 가깝고 오래 안기다리는 맛집이 좋아요",
          "speaker_id": "2",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100032437.1.20",
          "form": "저는 이제 밥먹으러 가려구요 ㅎ",
          "original_form": "저는 이제 밥먹으러 가려구요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 17:52"
        }
      ]
