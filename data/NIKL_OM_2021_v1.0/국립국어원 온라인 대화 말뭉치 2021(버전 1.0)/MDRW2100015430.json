      "utterance": [
        {
          "id": "MDRW2100015430.1.1",
          "form": "유툽 보시나요",
          "original_form": "유툽 보시나요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015430.1.2",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015430.1.3",
          "form": "가장 많이 보는 건 넷플이지만ㅎㅎ",
          "original_form": "가장 많이 보는 건 넷플이지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015430.1.4",
          "form": "오 뭐보시나요",
          "original_form": "오 뭐보시나요",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100015430.1.5",
          "form": "음 요즘 정주행 중인거는 워킹데드요!",
          "original_form": "음 요즘 정주행 중인거는 워킹데드요!",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100015430.1.6",
          "form": "혹시 뭐 보시는 거 있으세요?",
          "original_form": "혹시 뭐 보시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100015430.1.7",
          "form": "킹덤이요!",
          "original_form": "킹덤이요!",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100015430.1.8",
          "form": "오 킹덤! 아신전까지 다 보신거에요?",
          "original_form": "오 킹덤! 아신전까지 다 보신거에요?",
          "speaker_id": "2",
          "time": "20210827 01:08"
        },
        {
          "id": "MDRW2100015430.1.9",
          "form": "넵 다봤어요ㅎㅎ",
          "original_form": "넵 다봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100015430.1.10",
          "form": "오오...!! 너무 재밌죠ㅠ 사실 아신전은 약간 루즈한 감이 없지 않았는데ㅋㅋ",
          "original_form": "오오...!! 너무 재밌죠ㅠ 사실 아신전은 약간 루즈한 감이 없지 않았는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100015430.1.11",
          "form": "오맞아요 살짝 잔잔ㅎㅎ",
          "original_form": "오맞아요 살짝 잔잔ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:03"
        },
        {
          "id": "MDRW2100015430.1.12",
          "form": "ㅋㅋㅋㅋ호랑이가 고라니 물어뜯을때가 젤 무서웠어요...",
          "original_form": "ㅋㅋㅋㅋ호랑이가 고라니 물어뜯을때가 젤 무서웠어요...",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100015430.1.13",
          "form": "그거 말고는 다소 다들 잔잔..",
          "original_form": "그거 말고는 다소 다들 잔잔..",
          "speaker_id": "2",
          "time": "20210829 13:40"
        },
        {
          "id": "MDRW2100015430.1.14",
          "form": "마지막 쿠키도 볼만ㅎㅎ",
          "original_form": "마지막 쿠키도 볼만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100015430.1.15",
          "form": "아 맞아요ㅋㅋㅋ 쿠키도 볼만했어요",
          "original_form": "아 맞아요ㅋㅋㅋ 쿠키도 볼만했어요",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100015430.1.16",
          "form": "혹시 다른 플랫폼 이용하시는 거 있으세요?",
          "original_form": "혹시 다른 플랫폼 이용하시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100015430.1.17",
          "form": "유튭넷플 거의쓰네요ㅎㅎ",
          "original_form": "유튭넷플 거의쓰네요ㅎㅎ",
