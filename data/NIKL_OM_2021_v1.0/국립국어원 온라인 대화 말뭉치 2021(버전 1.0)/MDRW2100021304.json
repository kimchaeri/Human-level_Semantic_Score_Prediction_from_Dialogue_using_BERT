        {
          "id": "MDRW2100021304.1.1",
          "form": "스포츠 조아하시나요",
          "original_form": "스포츠 조아하시나요",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021304.1.2",
          "form": "뵬로요 올림픽은 좀 봤어요",
          "original_form": "뵬로요 올림픽은 좀 봤어요",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021304.1.3",
          "form": "오 저도 여자배구!",
          "original_form": "오 저도 여자배구!",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021304.1.4",
          "form": "멋지더라구요",
          "original_form": "멋지더라구요",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021304.1.5",
          "form": "일본전 보셨나요..",
          "original_form": "일본전 보셨나요..",
          "speaker_id": "1",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021304.1.6",
          "form": "네 그럼요",
          "original_form": "네 그럼요",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021304.1.7",
          "form": "지고있었는데 역전..",
          "original_form": "지고있었는데 역전..",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021304.1.8",
          "form": "ㅇㅇ 엄청났어요",
          "original_form": "ㅇㅇ 엄청났어요",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021304.1.9",
          "form": "양궁도 대박이었죠",
          "original_form": "양궁도 대박이었죠",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021304.1.10",
          "form": "네 ㅎㅎ 재방봐도 짜릿",
          "original_form": "네 ㅎㅎ 재방봐도 짜릿",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021304.1.11",
          "form": "안산선수는진짜 실력이..",
          "original_form": "안산선수는진짜 실력이..",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021304.1.12",
          "form": "멘탈도 대단",
          "original_form": "멘탈도 대단",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021304.1.13",
          "form": "국가대표 뽑히기도 엄청 힘들더라구요",
