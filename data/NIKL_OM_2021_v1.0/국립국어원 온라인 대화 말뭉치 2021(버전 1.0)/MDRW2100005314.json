          "original_form": "와",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005314.1.7",
          "form": "닭 가 스묘ㅏㄹ",
          "original_form": "닭 가 스묘ㅏㄹ",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005314.1.8",
          "form": "안보내져요",
          "original_form": "안보내져요",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005314.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005314.1.10",
          "form": "와 는 아이스크림은 아니겠죠",
          "original_form": "와 는 아이스크림은 아니겠죠",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005314.1.11",
          "form": "영어로 얘기해요",
          "original_form": "영어로 얘기해요",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005314.1.12",
          "form": "닭쮜쮜",
          "original_form": "닭쮜쮜",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005314.1.13",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.14",
          "form": "더이상해졌음",
          "original_form": "더이상해졌음",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.15",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.16",
          "form": "랑 현미밥",
          "original_form": "랑 현미밥",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.17",
          "form": "상추 냠냠",
          "original_form": "상추 냠냠",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.18",
          "form": "요즘 운동이랑 식단 하시는구나",
          "original_form": "요즘 운동이랑 식단 하시는구나",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.19",
          "form": "햤어요",
          "original_form": "햤어요",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.20",
          "form": "그쵸..",
          "original_form": "그쵸..",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.21",
          "form": "인샹이",
          "original_form": "인샹이",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.22",
          "form": "팍팍해요",
          "original_form": "팍팍해요",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.23",
          "form": "지금 냄비에",
          "original_form": "지금 냄비에",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.24",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.25",
          "form": "뼈해장국이잇는ㄷ",
          "original_form": "뼈해장국이잇는ㄷ",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.26",
          "form": "그러겠네요...",
          "original_form": "그러겠네요...",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.27",
          "form": "이게 말이도ㅡ는지",
          "original_form": "이게 말이도ㅡ는지",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.28",
          "form": "자다가 나와서 먹는거 아녜요?",
          "original_form": "자다가 나와서 먹는거 아녜요?",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.29",
          "form": "..",
          "original_form": "..",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.30",
          "form": "가끔 그랴요",
          "original_form": "가끔 그랴요",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.31",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.32",
          "form": "진짜였네",
          "original_form": "진짜였네",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.33",
          "form": "그래서 양치를 빨리",
          "original_form": "그래서 양치를 빨리",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.34",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.35",
          "form": "햐야하죠",
          "original_form": "햐야하죠",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005314.1.37",
          "form": "양치는 나자신과의 약속이니까요",
          "original_form": "양치는 나자신과의 약속이니까요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.38",
          "form": "한입만충",
          "original_form": "한입만충",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.39",
          "form": "다도ㅑㅅ어요",
          "original_form": "다도ㅑㅅ어요",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.40",
          "form": "더이상 먹지않겠다",
          "original_form": "더이상 먹지않겠다",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.41",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.42",
          "form": "하지만 방금 양차하고",
          "original_form": "하지만 방금 양차하고",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.43",
          "form": "아몬ㄷ",
          "original_form": "아몬ㄷ",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.44",
          "form": "그래서 전 충치가생겼어요",
          "original_form": "그래서 전 충치가생겼어요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.45",
          "form": "먹엇어요",
          "original_form": "먹엇어요",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.46",
          "form": "윽...",
          "original_form": "윽...",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.47",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.48",
          "form": "저 견과류싫어해요",
          "original_form": "저 견과류싫어해요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.49",
          "form": "아니 왜 충치가",
          "original_form": "아니 왜 충치가",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.50",
          "form": "샹겨요",
          "original_form": "샹겨요",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.51",
          "form": "견과류 맛있는데",
          "original_form": "견과류 맛있는데",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.52",
          "form": "지키지 못할 약속은 하지않아서",
          "original_form": "지키지 못할 약속은 하지않아서",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.53",
          "form": "호두도 안드세요",
          "original_form": "호두도 안드세요",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.54",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.55",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.56",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.57",
          "form": "아 갸웃긴",
          "original_form": "아 갸웃긴",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005314.1.58",
          "form": "호두 간지용으로 주머니에 넣고다녀요",
          "original_form": "호두 간지용으로 주머니에 넣고다녀요",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005314.1.59",
          "form": "절그럭절그록",
          "original_form": "절그럭절그록",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005314.1.60",
          "form": "와..",
          "original_form": "와..",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005314.1.61",
          "form": "그거 멋있다",
          "original_form": "그거 멋있다",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005314.1.62",
          "form": "무슨 영화에서 봤는데",
          "original_form": "무슨 영화에서 봤는데",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005314.1.63",
          "form": "멋있더라구요",
          "original_form": "멋있더라구요",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005314.1.64",
          "form": "설정으로 해주세요",
          "original_form": "설정으로 해주세요",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005314.1.65",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005314.1.66",
          "form": "컨셉충같겠다",
          "original_form": "컨셉충같겠다",
