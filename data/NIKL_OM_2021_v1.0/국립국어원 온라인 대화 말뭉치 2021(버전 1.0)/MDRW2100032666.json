        {
          "id": "MDRW2100032666.1.1",
          "form": "네 다음에 기회되면 한번 보시면",
          "original_form": "네 다음에 기회되면 한번 보시면",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032666.1.2",
          "form": "좋을것같아요ㅋㅋ",
          "original_form": "좋을것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032666.1.3",
          "form": "ㅎㅎ 네 기회되면 다음에 한번 봐볼게요",
          "original_form": "ㅎㅎ 네 기회되면 다음에 한번 봐볼게요",
          "speaker_id": "2",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032666.1.4",
          "form": "플랫폼 쓰세요?",
          "original_form": "플랫폼 쓰세요?",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032666.1.5",
          "form": "네 저는 왓챠써요",
          "original_form": "네 저는 왓챠써요",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032666.1.6",
          "form": "왓챠좋나요?!!",
          "original_form": "왓챠좋나요?!!",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032666.1.7",
          "form": "네 그냥 나쁘지않아요",
          "original_form": "네 그냥 나쁘지않아요",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032666.1.8",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032666.1.9",
          "form": "저는 웨이브써요",
          "original_form": "저는 웨이브써요",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032666.1.10",
          "form": "웨이브좋나요?",
          "original_form": "웨이브좋나요?",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032666.1.11",
          "form": "네 국내 드라마많이 보시면",
          "original_form": "네 국내 드라마많이 보시면",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032666.1.12",
          "form": "웨이브도 좋으실것같아요ㅋㅋ",
          "original_form": "웨이브도 좋으실것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032666.1.13",
          "form": "아 웨이브에는 국내 드라마가 많나요 ?",
          "original_form": "아 웨이브에는 국내 드라마가 많나요 ?",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032666.1.14",
          "form": "어 네 제가 느끼기에는!",
          "original_form": "어 네 제가 느끼기에는!",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032666.1.15",
          "form": "다음에 한번 써봐야겠네요 ㅎㅎ",
          "original_form": "다음에 한번 써봐야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032666.1.16",
          "form": "도움이된다면 좋겠어요! ㅋㅋ",
          "original_form": "도움이된다면 좋겠어요! ㅋㅋ",
