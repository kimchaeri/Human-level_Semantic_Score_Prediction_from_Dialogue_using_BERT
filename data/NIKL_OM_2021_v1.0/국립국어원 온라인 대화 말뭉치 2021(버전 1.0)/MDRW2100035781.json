      "utterance": [
        {
          "id": "MDRW2100035781.1.1",
          "form": "안녕하세여?",
          "original_form": "안녕하세여?",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100035781.1.2",
          "form": "안녕하세요.\r\n독립하고싶으신가요?",
          "original_form": "안녕하세요.\r\n독립하고싶으신가요?",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100035781.1.3",
          "form": "반반이영",
          "original_form": "반반이영",
          "speaker_id": "2",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100035781.1.4",
          "form": "가족간의 스트레스가 있으셔서 그런가요? 반반인가요?",
          "original_form": "가족간의 스트레스가 있으셔서 그런가요? 반반인가요?",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100035781.1.5",
          "form": "집에 혼자있는게 무서워서요",
          "original_form": "집에 혼자있는게 무서워서요",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100035781.1.6",
          "form": "그러면 가족 중에 누구를 데려가고싶나요?",
          "original_form": "그러면 가족 중에 누구를 데려가고싶나요?",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100035781.1.7",
          "form": "하지만 스으레스 맥스일때가 꽤있어서..",
          "original_form": "하지만 스으레스 맥스일때가 꽤있어서..",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100035781.1.8",
          "form": "그러면 혼자사셔야하는거아닌지",
          "original_form": "그러면 혼자사셔야하는거아닌지",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100035781.1.9",
          "form": "무슨 의도애요?",
          "original_form": "무슨 의도애요?",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100035781.1.10",
          "form": "독립하게되면 가족 중에 누구랑 같이살고싶나요",
          "original_form": "독립하게되면 가족 중에 누구랑 같이살고싶나요",
          "speaker_id": "1",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100035781.1.11",
          "form": "그건 독립이 아니에요",
          "original_form": "그건 독립이 아니에요",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100035781.1.12",
          "form": "숙제입니다 알아오세요.",
          "original_form": "숙제입니다 알아오세요.",
          "speaker_id": "1",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100035781.1.13",
