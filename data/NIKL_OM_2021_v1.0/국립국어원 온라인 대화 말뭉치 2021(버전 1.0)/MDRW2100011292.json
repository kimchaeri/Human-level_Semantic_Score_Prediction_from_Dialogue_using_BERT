        },
        {
          "id": "MDRW2100011292.1.2",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20210824 10:50"
        },
        {
          "id": "MDRW2100011292.1.3",
          "form": "영화좋아하세요ㆍ",
          "original_form": "영화좋아하세요ㆍ",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011292.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011292.1.5",
          "form": "네~ 영화좋아해요#",
          "original_form": "네~ 영화좋아해요#",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100011292.1.6",
          "form": "최근데 모가디슈랑 씽크홀봤네요~",
          "original_form": "최근데 모가디슈랑 씽크홀봤네요~",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100011292.1.7",
          "form": "누누님은요~?",
          "original_form": "누누님은요~?",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100011292.1.8",
          "form": "오 잼난거다보셨네요",
          "original_form": "오 잼난거다보셨네요",
          "speaker_id": "1",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011292.1.9",
          "form": "저는 그거두개를 보고싶어요ㅋㅋㅋ",
          "original_form": "저는 그거두개를 보고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011292.1.10",
          "form": "오옹ㅋㅋㅋ 최근에 휴가여서 어디갈데는 없고 영화만 본걱같아욬ㅋ",
          "original_form": "오옹ㅋㅋㅋ 최근에 휴가여서 어디갈데는 없고 영화만 본걱같아욬ㅋ",
          "speaker_id": "2",
          "time": "20210824 12:24"
        },
        {
          "id": "MDRW2100011292.1.11",
          "form": "최근에 보신 영화있으신가요~?",
          "original_form": "최근에 보신 영화있으신가요~?",
          "speaker_id": "2",
          "time": "20210824 12:24"
        },
        {
          "id": "MDRW2100011292.1.12",
          "form": "저는 기억이없네요ㅋㅋㅋ 하도오래돼서",
          "original_form": "저는 기억이없네요ㅋㅋㅋ 하도오래돼서",
          "speaker_id": "1",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011292.1.13",
          "form": "인생영화는 무엇인가요?",
          "original_form": "인생영화는 무엇인가요?",
          "speaker_id": "1",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011292.1.14",
          "form": "음 7번방의 선물?",
          "original_form": "음 7번방의 선물?",
          "speaker_id": "2",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100011292.1.15",
          "form": "진짜 펑펑 울었던걱 같아요ㅠ",
          "original_form": "진짜 펑펑 울었던걱 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100011292.1.16",
          "form": "아 뭔지알아요",
          "original_form": "아 뭔지알아요",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011292.1.17",
          "form": "저도그거영화관에서 봤어요!",
          "original_form": "저도그거영화관에서 봤어요!",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011292.1.18",
          "form": "오옹~ 누누님은 인생영화뭐에요~?",
          "original_form": "오옹~ 누누님은 인생영화뭐에요~?",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100011292.1.19",
          "form": "전 인터스텔라요ㅋㅋㅋ",
          "original_form": "전 인터스텔라요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100011292.1.20",
          "form": "인터스텔라 추천은 많이 받았는데 한번도 못봤네영ㅋㅋ",
          "original_form": "인터스텔라 추천은 많이 받았는데 한번도 못봤네영ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100011292.1.21",
          "form": "재밌나용?!",
          "original_form": "재밌나용?!",
          "speaker_id": "2",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100011292.1.22",
          "form": "네네 재밌답니다ㅋㅋㅋ",
          "original_form": "네네 재밌답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:17"
        }
      ]
