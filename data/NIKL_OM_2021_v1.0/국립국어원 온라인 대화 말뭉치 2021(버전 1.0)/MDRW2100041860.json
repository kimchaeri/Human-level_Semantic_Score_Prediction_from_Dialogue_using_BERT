      "utterance": [
        {
          "id": "MDRW2100041860.1.1",
          "form": "이모가 준거야 소중히 써",
          "original_form": "이모가 준거야 소중히 써",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041860.1.2",
          "form": "안잃어버리구 잘 쓰고 있어요 ㅋㅋ 조심할게요",
          "original_form": "안잃어버리구 잘 쓰고 있어요 ㅋㅋ 조심할게요",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041860.1.3",
          "form": "물도 자주 마시고 그래야 해",
          "original_form": "물도 자주 마시고 그래야 해",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041860.1.4",
          "form": "환절기라 되게 춥더라구요 밤에 문열고있으면",
          "original_form": "환절기라 되게 춥더라구요 밤에 문열고있으면",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041860.1.5",
          "form": "완전 감기걸릴가ㅓ같아요 ㅠ",
          "original_form": "완전 감기걸릴가ㅓ같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041860.1.6",
          "form": "감기 조심해야지 비오는날 전이면 진짜 엄청 춥다",
          "original_form": "감기 조심해야지 비오는날 전이면 진짜 엄청 춥다",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041860.1.7",
          "form": "그러니까요 ㅠㅠ 맨날 밤에 문닫고자는",
          "original_form": "그러니까요 ㅠㅠ 맨날 밤에 문닫고자는",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041860.1.8",
          "form": "엄마두 요즘 대추차 마셔",
          "original_form": "엄마두 요즘 대추차 마셔",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100041860.1.9",
          "form": "아 그거 아빠가 얘기했어요",
          "original_form": "아 그거 아빠가 얘기했어요",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100041860.1.10",
          "form": "뭐라고 하든?",
          "original_form": "뭐라고 하든?",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100041860.1.11",
          "form": "대추 껍질 많다고 ㅋㅋㅋㅋㅋ",
          "original_form": "대추 껍질 많다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100041860.1.12",
          "form": "엄마가 갈아서 그래1",
          "original_form": "엄마가 갈아서 그래1",
          "speaker_id": "2",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100041860.1.13",
          "form": "그거 대추 그냥 갈아버리셨다면서요 ㅋㅋㅋㅋㅋ",
          "original_form": "그거 대추 그냥 갈아버리셨다면서요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041860.1.14",
          "form": "어쩔수 없었어",
          "original_form": "어쩔수 없었어",
          "speaker_id": "2",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041860.1.15",
          "form": "어떻게 하는지 몰라서",
          "original_form": "어떻게 하는지 몰라서",
          "speaker_id": "2",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041860.1.16",
          "form": "ㅎㅎ 그거 대추맛 많이 나서 속 안좋다고 아빠가 그랬어요",
          "original_form": "ㅎㅎ 그거 대추맛 많이 나서 속 안좋다고 아빠가 그랬어요",
          "speaker_id": "1",
          "time": "20210906 17:15"
        },
        {
          "id": "MDRW2100041860.1.17",
          "form": "이번에는 채로 걸러서 괜찮아",
          "original_form": "이번에는 채로 걸러서 괜찮아",
          "speaker_id": "2",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041860.1.18",
          "form": "너 대추 싫어하잖아",
          "original_form": "너 대추 싫어하잖아",
          "speaker_id": "2",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041860.1.19",
          "form": "대추 덜싫어하게 된거면 다행인데",
          "original_form": "대추 덜싫어하게 된거면 다행인데",
          "speaker_id": "2",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041860.1.20",
          "form": "엄마가 만들어준거니까 맛있게 먹어",
          "original_form": "엄마가 만들어준거니까 맛있게 먹어",
          "speaker_id": "2",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041860.1.21",
          "form": "ㅈ",
          "original_form": "ㅈ",
          "speaker_id": "2",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041860.1.22",
          "form": "집에 언제오냐",
          "original_form": "집에 언제오냐",
          "speaker_id": "2",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041860.1.23",
          "form": "건강 챙기게 맛있는거 해줄게",
          "original_form": "건강 챙기게 맛있는거 해줄게",
          "speaker_id": "2",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041860.1.24",
          "form": "대추차랑",
          "original_form": "대추차랑",
          "speaker_id": "2",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041860.1.25",
          "form": "보리차는 잘먹으면서",
          "original_form": "보리차는 잘먹으면서",
          "speaker_id": "2",
          "time": "20210906 17:18"
        }
      ]
    }
  ]
