          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028658.1.32",
          "form": "아진짜웃기내",
          "original_form": "아진짜웃기내",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028658.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028658.1.34",
