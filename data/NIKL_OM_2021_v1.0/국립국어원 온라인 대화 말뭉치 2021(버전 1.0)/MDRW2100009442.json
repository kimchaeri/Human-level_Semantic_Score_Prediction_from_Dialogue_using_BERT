      "utterance": [
        {
          "id": "MDRW2100009442.1.1",
          "form": "개강은 언제인가요?",
          "original_form": "개강은 언제인가요?",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009442.1.2",
          "form": "저는 졸업을해서 개강이 없어요",
          "original_form": "저는 졸업을해서 개강이 없어요",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009442.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009442.1.4",
          "form": "주제가 학교관련이라...",
          "original_form": "주제가 학교관련이라...",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009442.1.5",
          "form": "저도 어른입니다.. ㅋㅋㅋ",
          "original_form": "저도 어른입니다.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009442.1.6",
          "form": "뭐 최근데 배우시는거  있나여?",
          "original_form": "뭐 최근데 배우시는거  있나여?",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009442.1.7",
          "form": "데이터 쪽 공부하고 있어요",
          "original_form": "데이터 쪽 공부하고 있어요",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009442.1.8",
          "form": "name1님은 학교다닐때 동아리활동 하셨었나요",
          "original_form": "&name1&님은 학교다닐때 동아리활동 하셨었나요",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009442.1.9",
          "form": "저는 아싸라... 동아리는... \r\n아.. 사회인되서 일본어 동아리요.. ㅎㅎ",
          "original_form": "저는 아싸라... 동아리는... \r\n아.. 사회인되서 일본어 동아리요.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009442.1.10",
          "form": "동아리 활동하신거있으신가봐여?",
          "original_form": "동아리 활동하신거있으신가봐여?",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009442.1.11",
          "form": "아니요 저는 해본 동아리 없어요",
          "original_form": "아니요 저는 해본 동아리 없어요",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009442.1.12",
          "form": "일본어 동아리 하셨구나. 일본어에 관심 있으셨어요?",
          "original_form": "일본어 동아리 하셨구나. 일본어에 관심 있으셨어요?",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009442.1.13",
          "form": "밥벌이였엇죠.. ㅎㅎㅎㅎ",
          "original_form": "밥벌이였엇죠.. ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009442.1.14",
          "form": "어릴때 일본어에 빠져서.. 일어만 팠던거 가아요..",
          "original_form": "어릴때 일본어에 빠져서.. 일어만 팠던거 가아요..",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009442.1.15",
          "form": "디비하시는건가요ㅡ",
          "original_form": "디비하시는건가요ㅡ",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009442.1.16",
          "form": "그럼 학교다닐때 전공도 일본어쪽이셨나요?",
          "original_form": "그럼 학교다닐때 전공도 일본어쪽이셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009442.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009442.1.18",
          "form": "반도체 전공인데.. 그냥 취미로 일본어하다가..\r\n일본계에서 일하게되었네요..",
          "original_form": "반도체 전공인데.. 그냥 취미로 일본어하다가..\r\n일본계에서 일하게되었네요..",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009442.1.19",
          "form": "디비도 계속 공부하셔야되죠?",
          "original_form": "디비도 계속 공부하셔야되죠?",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009442.1.20",
          "form": "계속 공부할께 생기네요",
          "original_form": "계속 공부할께 생기네요",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009442.1.21",
          "form": "그럼 학교다닐때 축제는 좀 다니셨나요?",
          "original_form": "그럼 학교다닐때 축제는 좀 다니셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009442.1.22",
          "form": "학생때는 공부아님 땡땡이만 친듯... ㅋㅋ",
          "original_form": "학생때는 공부아님 땡땡이만 친듯... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009442.1.23",
          "form": "모범생이엿나요?",
          "original_form": "모범생이엿나요?",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009442.1.24",
          "form": "그냥저냥 지각안하는 학생이였어요",
          "original_form": "그냥저냥 지각안하는 학생이였어요",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009442.1.25",
          "form": "축제는 새내기때 많이다녔구요",
          "original_form": "축제는 새내기때 많이다녔구요",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009442.1.26",
          "form": "전공으로 취직하신거면 공부 잘하셧을듯..",
          "original_form": "전공으로 취직하신거면 공부 잘하셧을듯..",
