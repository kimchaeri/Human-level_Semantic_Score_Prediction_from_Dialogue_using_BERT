        },
        {
          "id": "MDRW2100005819.1.2",
          "form": "닌텐도게임",
          "original_form": "닌텐도게임",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005819.1.3",
          "form": "너는",
          "original_form": "너는",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005819.1.4",
          "form": "펀치니들",
          "original_form": "펀치니들",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005819.1.5",
          "form": "닌텐도 형부가 빠졌대",
          "original_form": "닌텐도 형부가 빠졌대",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005819.1.6",
          "form": "젤다..",
          "original_form": "젤다..",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005819.1.7",
          "form": "젤다 나는 오빠",
          "original_form": "젤다 나는 오빠",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005819.1.8",
          "form": "빌려줌",
          "original_form": "빌려줌",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005819.1.9",
          "form": "명작이지..",
          "original_form": "명작이지..",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005819.1.10",
          "form": "ㅋㅋㅋㅋ재밌나바",
          "original_form": "ㅋㅋㅋㅋ재밌나바",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100005819.1.11",
          "form": "링피트 하고 몸져누웠자너... 존힘드러... 나루토 처럼 달리느라 힘들었어",
          "original_form": "링피트 하고 몸져누웠자너... 존힘드러... 나루토 처럼 달리느라 힘들었어",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100005819.1.12",
          "form": "어 그거 진짜잼짘ㅋㅋㅋ 힘들곸ㅋㅋㅋㅋㅋ",
          "original_form": "어 그거 진짜잼짘ㅋㅋㅋ 힘들곸ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100005819.1.13",
          "form": "진짜힘들어...와 근력도없어서 난리남",
          "original_form": "진짜힘들어...와 근력도없어서 난리남",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100005819.1.14",
          "form": "자꾸 조이라는데 힘이안들어가요 선생님..",
          "original_form": "자꾸 조이라는데 힘이안들어가요 선생님..",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100005819.1.15",
          "form": "운동부하 넘 쎈거아니냐",
          "original_form": "운동부하 넘 쎈거아니냐",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100005819.1.16",
          "form": "그랬나봐..",
          "original_form": "그랬나봐..",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100005819.1.17",
          "form": "ㅋㅋㅋㅋ어제 땅끄홈트했어",
          "original_form": "ㅋㅋㅋㅋ어제 땅끄홈트했어",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100005819.1.18",
          "form": "오 어때 안하다햐도 할만해?",
          "original_form": "오 어때 안하다햐도 할만해?",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100005819.1.19",
          "form": "응 칼소폭",
          "original_form": "응 칼소폭",
          "speaker_id": "2",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100005819.1.20",
          "form": "핵핵핵매운맛이거",
          "original_form": "핵핵핵매운맛이거",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100005819.1.21",
          "form": "안뛰는버전있어서항만해",
          "original_form": "안뛰는버전있어서항만해",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100005819.1.22",
          "form": "ㅋㅋㅋ말만들어도 힘든뎅..",
          "original_form": "ㅋㅋㅋ말만들어도 힘든뎅..",
          "speaker_id": "1",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100005819.1.23",
          "form": "관절 소중",
          "original_form": "관절 소중",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100005819.1.24",
          "form": "난 펀치니들할거야..주말",
          "original_form": "난 펀치니들할거야..주말",
          "speaker_id": "1",
