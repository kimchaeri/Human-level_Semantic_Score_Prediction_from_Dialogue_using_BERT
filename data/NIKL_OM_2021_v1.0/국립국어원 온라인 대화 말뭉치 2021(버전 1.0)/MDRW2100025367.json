      "utterance": [
        {
          "id": "MDRW2100025367.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025367.1.2",
          "form": "여기는 아침에 조금씩 오더라구오",
          "original_form": "여기는 아침에 조금씩 오더라구오",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025367.1.3",
          "form": "거기는 비오나요",
          "original_form": "거기는 비오나요",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025367.1.4",
          "form": "네 많이여",
          "original_form": "네 많이여",
          "speaker_id": "1",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025367.1.5",
          "form": "비오면 신발 젖어서 싫어요",
          "original_form": "비오면 신발 젖어서 싫어요",
          "speaker_id": "2",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025367.1.6",
          "form": "어떤 날씨 좋아하시나요",
          "original_form": "어떤 날씨 좋아하시나요",
          "speaker_id": "2",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025367.1.7",
          "form": "구름낀날이요",
          "original_form": "구름낀날이요",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025367.1.8",
          "form": "저도 햇빛 싫어서 약간 흐린날이 좋아요",
          "original_form": "저도 햇빛 싫어서 약간 흐린날이 좋아요",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025367.1.9",
          "form": "어떤 계절 좋아하세요",
          "original_form": "어떤 계절 좋아하세요",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025367.1.10",
          "form": "봄이요",
          "original_form": "봄이요",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025367.1.11",
          "form": "저는 가을이 좋아요 선선하니",
          "original_form": "저는 가을이 좋아요 선선하니",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025367.1.12",
          "form": "내일 일기예보도 비가 온데요",
          "original_form": "내일 일기예보도 비가 온데요",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025367.1.13",
          "form": "또요?",
          "original_form": "또요?",
          "speaker_id": "1",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025367.1.14",
          "form": "듣기로는 비가 많이온데요",
          "original_form": "듣기로는 비가 많이온데요",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025367.1.15",
          "form": "저번에 번개가 뒷산 나무에 떨어져서 불이 났었네요",
          "original_form": "저번에 번개가 뒷산 나무에 떨어져서 불이 났었네요",
          "speaker_id": "2",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025367.1.16",
          "form": "바쁘신가봐요",
          "original_form": "바쁘신가봐요",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100025367.1.17",
          "form": "학",
          "original_form": "학",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025367.1.18",
          "form": "그런일이",
          "original_form": "그런일이",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025367.1.19",
          "form": "소방차가 6대왔어요",
          "original_form": "소방차가 6대왔어요",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025367.1.20",
          "form": "다행히 인명피해는 없었나봐요",
          "original_form": "다행히 인명피해는 없었나봐요",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025367.1.21",
          "form": "다행이네여",
          "original_form": "다행이네여",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
