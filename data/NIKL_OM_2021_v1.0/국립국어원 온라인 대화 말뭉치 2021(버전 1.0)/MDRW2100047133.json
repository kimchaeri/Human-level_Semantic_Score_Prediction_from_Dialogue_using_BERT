        {
          "id": "MDRW2100047133.1.1",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100047133.1.2",
          "form": "아니요 없습니다",
          "original_form": "아니요 없습니다",
          "speaker_id": "2",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100047133.1.3",
          "form": "저는 없고 부모님은 불교요",
          "original_form": "저는 없고 부모님은 불교요",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100047133.1.4",
          "form": "저도 없고 무교입니다",
          "original_form": "저도 없고 무교입니다",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100047133.1.5",
          "form": "어릴때친구랑같이 교회만요",
          "original_form": "어릴때친구랑같이 교회만요",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100047133.1.6",
          "form": "저도 어릴때 교회는 다녔지요",
          "original_form": "저도 어릴때 교회는 다녔지요",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100047133.1.7",
          "form": "달란트받았죠",
          "original_form": "달란트받았죠",
          "speaker_id": "1",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100047133.1.8",
          "form": "과자세트 받고 좋았죠",
          "original_form": "과자세트 받고 좋았죠",
          "speaker_id": "2",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100047133.1.9",
          "form": "그쵸 현금이었죠",
          "original_form": "그쵸 현금이었죠",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100047133.1.10",
          "form": "교회가면 친구도 있고 즐겁긴 했죠",
          "original_form": "교회가면 친구도 있고 즐겁긴 했죠",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100047133.1.11",
          "form": "또래니까",
          "original_form": "또래니까",
          "speaker_id": "1",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100047133.1.12",
          "form": "주말엔 교회가야 놀수가 있죠",
          "original_form": "주말엔 교회가야 놀수가 있죠",
