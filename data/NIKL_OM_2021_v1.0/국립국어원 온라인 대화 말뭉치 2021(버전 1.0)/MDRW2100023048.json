      "utterance": [
        {
          "id": "MDRW2100023048.1.1",
          "form": "여태까지 나온 힌트가 다 콩가루같은 기분인데",
          "original_form": "여태까지 나온 힌트가 다 콩가루같은 기분인데",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100023048.1.2",
          "form": "그릇ㄴㄴ\r\n인원수ㄴㄴ\r\n해산물 소량에 밀가루\r\n간식",
          "original_form": "그릇ㄴㄴ\r\n인원수ㄴㄴ\r\n해산물 소량에 밀가루\r\n간식",
          "speaker_id": "2",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100023048.1.3",
          "form": "먹고싶은건 맞으신거죠",
          "original_form": "먹고싶은건 맞으신거죠",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100023048.1.4",
          "form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋ콩가루 힌트 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋ콩가루 힌트 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100023048.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100023048.1.6",
          "form": "진짜 존재하는 음식이예요!",
          "original_form": "진짜 존재하는 음식이예요!",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100023048.1.7",
          "form": "진짜 먹고싶은거 맞아요",
          "original_form": "진짜 먹고싶은거 맞아요",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100023048.1.8",
          "form": "name1 괴롭히려고 고른 거 아니예요",
          "original_form": "&name1& 괴롭히려고 고른 거 아니예요",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100023048.1.9",
          "form": "연어...샌드위치",
          "original_form": "연어...샌드위치",
          "speaker_id": "1",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100023048.1.10",
          "form": "인가요?",
          "original_form": "인가요?",
          "speaker_id": "1",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100023048.1.11",
          "form": "와 참신하다",
          "original_form": "와 참신하다",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100023048.1.12",
          "form": "하지만 아닙니다 정답 처리해주고싶어요",
          "original_form": "하지만 아닙니다 정답 처리해주고싶어요",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100023048.1.13",
          "form": "너무 마음에 드는 답이예요 맛있겠네요",
          "original_form": "너무 마음에 드는 답이예요 맛있겠네요",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100023048.1.14",
          "form": "가산점 10점",
          "original_form": "가산점 10점",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100023048.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 감사합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 감사합니다",
          "speaker_id": "1",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100023048.1.16",
          "form": "저 초성찬스 쓰겠습니다 너무 길어지네요",
          "original_form": "저 초성찬스 쓰겠습니다 너무 길어지네요",
          "speaker_id": "1",
          "time": "20210829 21:08"
        },
        {
