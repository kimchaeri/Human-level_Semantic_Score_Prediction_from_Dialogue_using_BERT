      "utterance": [
        {
          "id": "MDRW2100010036.1.1",
          "form": "저도 방법을 알려주세요ㅎ 책많이 읽는법",
          "original_form": "저도 방법을 알려주세요ㅎ 책많이 읽는법",
          "speaker_id": "1",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010036.1.2",
          "form": "독서해본지가 언젠지",
          "original_form": "독서해본지가 언젠지",
          "speaker_id": "1",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010036.1.3",
          "form": "ㅋㅋㅋㅋ 현실적으로는",
          "original_form": "ㅋㅋㅋㅋ 현실적으로는",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010036.1.4",
          "form": "한페이지당 천원ㅋㅋㅋㅋ?ㅋㅋ",
          "original_form": "한페이지당 천원ㅋㅋㅋㅋ?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010036.1.5",
          "form": "읽으면 돈줘요?ㅎ",
          "original_form": "읽으면 돈줘요?ㅎ",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010036.1.6",
          "form": "그런 꿀직장이 있나요?ㅋㅋ",
          "original_form": "그런 꿀직장이 있나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010036.1.7",
          "form": "아뇨 책 많이 읽는 방법이요ㅋㅋㅋ",
          "original_form": "아뇨 책 많이 읽는 방법이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010036.1.8",
          "form": "근데 연구라서 많이 읽는 편이니",
          "original_form": "근데 연구라서 많이 읽는 편이니",
          "speaker_id": "2",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010036.1.9",
          "form": "진짜 읽으면 돈주는 직업이네요",
          "original_form": "진짜 읽으면 돈주는 직업이네요",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010036.1.10",
          "form": "색다른 직업이네요 ㅎ 신선한데요~",
          "original_form": "색다른 직업이네요 ㅎ 신선한데요~",
          "speaker_id": "1",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010036.1.11",
          "form": "ㅎㅎ 평범하다고 생각했는데ㅋㅋㅋ",
          "original_form": "ㅎㅎ 평범하다고 생각했는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010036.1.12",
          "form": "제 입장에서는 원자력이 색다르게 느껴져요ㅋ",
          "original_form": "제 입장에서는 원자력이 색다르게 느껴져요ㅋ",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010036.1.13",
          "form": "업무 스트레스는 없어요?",
          "original_form": "업무 스트레스는 없어요?",
          "speaker_id": "1",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010036.1.14",
          "form": "에구 그냥 불쑥불쑥 스트레스 천지지요",
          "original_form": "에구 그냥 불쑥불쑥 스트레스 천지지요",
          "speaker_id": "2",
          "time": "20210823 13:41"
        },
        {
          "id": "MDRW2100010036.1.15",
          "form": "일단 내맘대로 결과가 안 나오니ㅠ",
          "original_form": "일단 내맘대로 결과가 안 나오니ㅠ",
          "speaker_id": "2",
          "time": "20210823 13:42"
        },
        {
          "id": "MDRW2100010036.1.16",
          "form": "결과는 어떤걸 내야되나요? 보고서?",
          "original_form": "결과는 어떤걸 내야되나요? 보고서?",
          "speaker_id": "1",
          "time": "20210823 13:43"
        },
        {
          "id": "MDRW2100010036.1.17",
          "form": "그렇지요 연구보고서ㅎㅎ",
          "original_form": "그렇지요 연구보고서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 13:52"
        },
        {
          "id": "MDRW2100010036.1.18",
          "form": "보고서 얘기만 들어도 스트레스가ㅎㅎ",
          "original_form": "보고서 얘기만 들어도 스트레스가ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100010036.1.19",
          "form": "ㅎㅎ 회사에 보고서 없으세요?",
          "original_form": "ㅎㅎ 회사에 보고서 없으세요?",
          "speaker_id": "2",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100010036.1.20",
          "form": "아 그럼 회계나 좀 명확한 업무시려나",
          "original_form": "아 그럼 회계나 좀 명확한 업무시려나",
          "speaker_id": "2",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100010036.1.21",
          "form": "예전엔 있었는데 다른업무로 바꿨어요",
          "original_form": "예전엔 있었는데 다른업무로 바꿨어요",
          "speaker_id": "1",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010036.1.22",
          "form": "케이블 경로보고 분석업무요",
          "original_form": "케이블 경로보고 분석업무요",
          "speaker_id": "1",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010036.1.23",
          "form": "아 그럼 원전 가까운 곳에 거주하셔야하나? o.o?",
          "original_form": "아 그럼 원전 가까운 곳에 거주하셔야하나? o.o?",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010036.1.24",
          "form": "아니요ㅎ 현장근무아니고 걍데이터로ㅎ",
          "original_form": "아니요ㅎ 현장근무아니고 걍데이터로ㅎ",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010036.1.25",
          "form": "한수원 아니고ㅋ",
          "original_form": "한수원 아니고ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010036.1.26",
          "form": "작은 회사예요",
          "original_form": "작은 회사예요",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010036.1.27",
          "form": "그 원전 하나를 가동 하냐 마냐 얘기가 있던거 같은데",
          "original_form": "그 원전 하나를 가동 하냐 마냐 얘기가 있던거 같은데",
          "speaker_id": "2",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010036.1.28",
          "form": "쉬운 일이 없네요.",
          "original_form": "쉬운 일이 없네요.",
          "speaker_id": "2",
          "time": "20210823 14:00"
        },
        {
          "id": "MDRW2100010036.1.29",
          "form": "그니까요 먹고살기 힘드네요ㅎㅎ",
          "original_form": "그니까요 먹고살기 힘드네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 14:00"
        }
      ]
