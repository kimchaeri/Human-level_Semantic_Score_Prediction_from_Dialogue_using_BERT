      "utterance": [
        {
          "id": "MDRW2100038125.1.1",
          "form": "흠 송강?",
          "original_form": "흠 송강?",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038125.1.2",
          "form": "송강도 한물갔어?",
          "original_form": "송강도 한물갔어?",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038125.1.3",
          "form": "아 그친구 잘생겼도라",
          "original_form": "아 그친구 잘생겼도라",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038125.1.4",
          "form": "넷플릭스에서 인기더라 송강",
          "original_form": "넷플릭스에서 인기더라 송강",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.5",
          "form": "아니야 아직은 대세같아",
          "original_form": "아니야 아직은 대세같아",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.6",
          "form": "미스터 홈?",
          "original_form": "미스터 홈?",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.7",
          "form": "아 크러쉬랑 누가 사귄다던데",
          "original_form": "아 크러쉬랑 누가 사귄다던데",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.8",
          "form": "스윗홈? 아무튼 그 드라마에서 인상깊데 나오나봐",
          "original_form": "스윗홈? 아무튼 그 드라마에서 인상깊데 나오나봐",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.9",
          "form": "니가 말하려던건 스위트홈인것같아",
          "original_form": "니가 말하려던건 스위트홈인것같아",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.10",
          "form": "아 너 가수 조이를 말하는구나",
          "original_form": "아 너 가수 조이를 말하는구나",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.11",
          "form": "너 넷플릭스에 빠삭하구나",
          "original_form": "너 넷플릭스에 빠삭하구나",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.12",
          "form": "나는 조이의 팬이야",
          "original_form": "나는 조이의 팬이야",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.13",
          "form": "비록 그 음색은 내취향이 아니지만...",
          "original_form": "비록 그 음색은 내취향이 아니지만...",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.14",
          "form": "조이는 예쁘고 구ㅏ여워",
          "original_form": "조이는 예쁘고 구ㅏ여워",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.15",
          "form": "얼굴이 귀여워",
          "original_form": "얼굴이 귀여워",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.16",
          "form": "크러쉬는 노래는 잘하지만",
          "original_form": "크러쉬는 노래는 잘하지만",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.17",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.18",
          "form": "잘생긴건 아닌것같아",
          "original_form": "잘생긴건 아닌것같아",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038125.1.19",
          "form": "크러쉬는 얼굴이 취향이 어냐",
          "original_form": "크러쉬는 얼굴이 취향이 어냐",
          "speaker_id": "2",
          "time": "20210901 23:29"
