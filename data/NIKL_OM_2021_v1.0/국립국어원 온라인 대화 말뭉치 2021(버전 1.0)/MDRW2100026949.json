      "utterance": [
        {
          "id": "MDRW2100026949.1.1",
          "form": "그럼..불도 안 들어오잖아요ㅜㅜㅜ",
          "original_form": "그럼..불도 안 들어오잖아요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026949.1.2",
          "form": "그래서 안쪽을 열어서",
          "original_form": "그래서 안쪽을 열어서",
          "speaker_id": "1",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026949.1.3",
          "form": "오른쪽 귀로 올라가는 선을",
          "original_form": "오른쪽 귀로 올라가는 선을",
          "speaker_id": "1",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026949.1.4",
          "form": "잘라버릴까 생각중이에요",
          "original_form": "잘라버릴까 생각중이에요",
          "speaker_id": "1",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026949.1.5",
          "form": "아닠ㅋ 과연 능력자ㅋㅋ",
          "original_form": "아닠ㅋ 과연 능력자ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:37"
        },
        {
          "id": "MDRW2100026949.1.6",
          "form": "아직 계획만 ㅋㅋ",
          "original_form": "아직 계획만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:37"
        },
        {
          "id": "MDRW2100026949.1.7",
          "form": "아니면 안쪽에 울리는 거 빼버려도 ㅋㅋㅋ",
          "original_form": "아니면 안쪽에 울리는 거 빼버려도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:37"
        },
        {
          "id": "MDRW2100026949.1.8",
          "form": "딴사람들은 건전지를 뺄때",
          "original_form": "딴사람들은 건전지를 뺄때",
          "speaker_id": "2",
          "time": "20210902 01:38"
        },
        {
          "id": "MDRW2100026949.1.9",
          "form": "개조할 생각을 하시다니ㄷㄷㅡ",
          "original_form": "개조할 생각을 하시다니ㄷㄷㅡ",
          "speaker_id": "2",
          "time": "20210902 01:38"
        },
        {
          "id": "MDRW2100026949.1.10",
          "form": "일단 내일 성공 구매하시고",
          "original_form": "일단 내일 성공 구매하시고",
          "speaker_id": "1",
          "time": "20210902 01:39"
        },
        {
          "id": "MDRW2100026949.1.11",
          "form": "코레일 역사 내",
          "original_form": "코레일 역사 내",
          "speaker_id": "1",
          "time": "20210902 01:39"
        },
        {
          "id": "MDRW2100026949.1.12",
          "form": "스토리웨이 편의점을",
          "original_form": "스토리웨이 편의점을",
          "speaker_id": "1",
          "time": "20210902 01:39"
        },
        {
          "id": "MDRW2100026949.1.13",
          "form": "잘 찾아다니십셔",
          "original_form": "잘 찾아다니십셔",
          "speaker_id": "1",
          "time": "20210902 01:39"
        },
        {
          "id": "MDRW2100026949.1.14",
          "form": "거기를 집중공략해야 하는군요!!",
          "original_form": "거기를 집중공략해야 하는군요!!",
          "speaker_id": "2",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100026949.1.15",
          "form": "넵넵!",
          "original_form": "넵넵!",
          "speaker_id": "2",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100026949.1.16",
          "form": "근데 일반인이 코레일 역사 알기 어려운데",
          "original_form": "근데 일반인이 코레일 역사 알기 어려운데",
          "speaker_id": "1",
          "time": "20210902 01:44"
        },
        {
          "id": "MDRW2100026949.1.17",
          "form": "간단하게 1호선 : 서울역~청량리 제외구간",
          "original_form": "간단하게 1호선 : 서울역~청량리 제외구간",
          "speaker_id": "1",
          "time": "20210902 01:44"
        },
        {
          "id": "MDRW2100026949.1.18",
          "form": "근데 또 서울역 청량리는 기차역 있어서",
          "original_form": "근데 또 서울역 청량리는 기차역 있어서",
          "speaker_id": "1",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100026949.1.19",
          "form": "시청~제기동 뺀 1호선 역",
          "original_form": "시청~제기동 뺀 1호선 역",
          "speaker_id": "1",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100026949.1.20",
          "form": "아니ㅎㅎ 뭐죠 이거암호인가요",
          "original_form": "아니ㅎㅎ 뭐죠 이거암호인가요",
          "speaker_id": "2",
          "time": "20210902 01:46"
        },
        {
          "id": "MDRW2100026949.1.21",
          "form": "전 그냥 서울역 가면 될줄ㅜㅜㅜ",
          "original_form": "전 그냥 서울역 가면 될줄ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 01:46"
        },
        {
          "id": "MDRW2100026949.1.22",
          "form": "서울역 가시면 돼요 사실ㅋㅋ",
          "original_form": "서울역 가시면 돼요 사실ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:48"
        },
        {
          "id": "MDRW2100026949.1.23",
          "form": "근데 혹시 거기 재고 없을까봐요",
          "original_form": "근데 혹시 거기 재고 없을까봐요",
          "speaker_id": "1",
          "time": "20210902 01:48"
        },
        {
          "id": "MDRW2100026949.1.24",
          "form": "아ㅎ 넹넹 일단 얘기해주신거 캡쳐했어요ㅎ",
          "original_form": "아ㅎ 넹넹 일단 얘기해주신거 캡쳐했어요ㅎ",
          "speaker_id": "2",
          "time": "20210902 01:49"
        },
        {
          "id": "MDRW2100026949.1.25",
          "form": "ㅋㅋ성공 구매하실거라고",
          "original_form": "ㅋㅋ성공 구매하실거라고",
          "speaker_id": "1",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026949.1.26",
          "form": "믿습니다",
          "original_form": "믿습니다",
          "speaker_id": "1",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026949.1.27",
          "form": "라이언역장 군단에 동참하세요!",
          "original_form": "라이언역장 군단에 동참하세요!",
