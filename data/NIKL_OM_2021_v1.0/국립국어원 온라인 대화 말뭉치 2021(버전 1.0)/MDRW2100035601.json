      "utterance": [
        {
          "id": "MDRW2100035601.1.1",
          "form": "다리는 되게 튼튼함",
          "original_form": "다리는 되게 튼튼함",
          "speaker_id": "1",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100035601.1.2",
          "form": "ㅇ글켔지 ㅋㅋ",
          "original_form": "ㅇ글켔지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100035601.1.3",
          "form": "사고나면 폐쇄해야하니까",
          "original_form": "사고나면 폐쇄해야하니까",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100035601.1.4",
          "form": "응 그리고 중국 다리처럼 유리 이런것도 아니고",
          "original_form": "응 그리고 중국 다리처럼 유리 이런것도 아니고",
          "speaker_id": "1",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100035601.1.5",
          "form": "무섭당ㅋㅋ",
          "original_form": "무섭당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100035601.1.6",
          "form": "견고하게 지어진게 느껴짐",
          "original_form": "견고하게 지어진게 느껴짐",
          "speaker_id": "1",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100035601.1.7",
          "form": "중국처럼 인구넘치는나라아니니까",
          "original_form": "중국처럼 인구넘치는나라아니니까",
          "speaker_id": "2",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100035601.1.8",
          "form": "신경써줬음해ㅋㅋ",
          "original_form": "신경써줬음해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100035601.1.9",
          "form": "오늘 사람은 대빵 많았음",
          "original_form": "오늘 사람은 대빵 많았음",
          "speaker_id": "1",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100035601.1.10",
          "form": "사람 많으니 더흔들린거같기도?",
          "original_form": "사람 많으니 더흔들린거같기도?",
          "speaker_id": "1",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100035601.1.11",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100035601.1.12",
          "form": "유료화되니까 긍갑다",
          "original_form": "유료화되니까 긍갑다",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100035601.1.13",
          "form": "ㅇㅇ 모든이의 마음은 같아 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 모든이의 마음은 같아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100035601.1.14",
          "form": "마자 나라도 근처면 갔을껄",
          "original_form": "마자 나라도 근처면 갔을껄",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100035601.1.15",
          "form": "지자체의 야심이 느껴짐",
          "original_form": "지자체의 야심이 느껴짐",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035601.1.16",
          "form": "관광 울산으로 지역경제를 살려보잣",
          "original_form": "관광 울산으로 지역경제를 살려보잣",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100035601.1.17",
          "form": "웅 그래야제 고래도 다시온다든데",
          "original_form": "웅 그래야제 고래도 다시온다든데",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035601.1.18",
          "form": "근데 솔직히 솔직히 돈주고 볼정도는 아니라는ㅜㅜ",
          "original_form": "근데 솔직히 솔직히 돈주고 볼정도는 아니라는ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035601.1.19",
          "form": "ㅋ 그냥 구실이고 데이트가는정도 아니것어",
          "original_form": "ㅋ 그냥 구실이고 데이트가는정도 아니것어",
          "speaker_id": "2",
          "time": "20210829 21:30"
        }
      ]
