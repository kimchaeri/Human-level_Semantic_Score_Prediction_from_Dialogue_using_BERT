      "utterance": [
        {
          "id": "MDRW2100019179.1.1",
          "form": "황정민 새 영화나왔던데!",
          "original_form": "황정민 새 영화나왔던데!",
          "speaker_id": "2",
          "time": "20210827 18:18"
        },
        {
          "id": "MDRW2100019179.1.2",
          "form": "오 제목이 뭐였죠?",
          "original_form": "오 제목이 뭐였죠?",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100019179.1.3",
          "form": "포스터 본 거 같은데 제목을 모르겠네요",
          "original_form": "포스터 본 거 같은데 제목을 모르겠네요",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100019179.1.4",
          "form": "인질이래요ㅋㅋㅋㅋ유퀴즈에 나와서 얘기하더라고요",
          "original_form": "인질이래요ㅋㅋㅋㅋ유퀴즈에 나와서 얘기하더라고요",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100019179.1.5",
          "form": "아 영화 안본지가 너무 오래되서",
          "original_form": "아 영화 안본지가 너무 오래되서",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100019179.1.6",
          "form": "그거 보러 가볼까봐요ㅠㅠ",
          "original_form": "그거 보러 가볼까봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100019179.1.7",
          "form": "최근에 영화 본 적 있으세요?",
          "original_form": "최근에 영화 본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100019179.1.8",
          "form": "저는 집 영화관을 쓰고 있습니다",
          "original_form": "저는 집 영화관을 쓰고 있습니다",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100019179.1.9",
          "form": "(티비)ㅋㅋ",
          "original_form": "(티비)ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100019179.1.10",
          "form": "오늘은 이탈리안잡 봤어요 티비서 해주더라구요",
          "original_form": "오늘은 이탈리안잡 봤어요 티비서 해주더라구요",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100019179.1.11",
          "form": "오 처음 들어보는 영화네요",
          "original_form": "오 처음 들어보는 영화네요",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100019179.1.12",
          "form": "무슨 내용인가요?",
          "original_form": "무슨 내용인가요?",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100019179.1.13",
          "form": "제이슨 스타뎀 나오는 영화에요!!!사기꾼들 사기치고 작존치고 하는 얘기요",
          "original_form": "제이슨 스타뎀 나오는 영화에요!!!사기꾼들 사기치고 작존치고 하는 얘기요",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100019179.1.14",
          "form": "재밌을거 같네요!!",
          "original_form": "재밌을거 같네요!!",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100019179.1.15",
          "form": "제일 재밌던 영화 있으신가요?",
          "original_form": "제일 재밌던 영화 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100019179.1.16",
          "form": "최근에는 이탈리안잡이요!!!",
          "original_form": "최근에는 이탈리안잡이요!!!",
          "speaker_id": "2",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100019179.1.17",
          "form": "ㅋㅋㅋ최근에 봤던게 그냥 재일 기억에 많이 남는건가",
          "original_form": "ㅋㅋㅋ최근에 봤던게 그냥 재일 기억에 많이 남는건가",
          "speaker_id": "2",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100019179.1.18",
          "form": "아 그거도 인상깊었다",
          "original_form": "아 그거도 인상깊었다",
          "speaker_id": "2",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100019179.1.19",
          "form": "핵소고지!",
          "original_form": "핵소고지!",
          "speaker_id": "2",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100019179.1.20",
          "form": "오 핵소고지 재밌나요?",
          "original_form": "오 핵소고지 재밌나요?",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100019179.1.21",
          "form": "제목만 들어봤는데!",
          "original_form": "제목만 들어봤는데!",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100019179.1.22",
          "form": "음 되게 심장졸이는데 감동적이고 슬퍼요",
          "original_form": "음 되게 심장졸이는데 감동적이고 슬퍼요",
          "speaker_id": "2",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100019179.1.23",
          "form": "가족들이랑 다같이 앉아서 봤는데 다들 좋은 영화라고 동의했던..",
          "original_form": "가족들이랑 다같이 앉아서 봤는데 다들 좋은 영화라고 동의했던..",
          "speaker_id": "2",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100019179.1.24",
          "form": "다들 좋았다고 하니 궁금하네요!!",
          "original_form": "다들 좋았다고 하니 궁금하네요!!",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100019179.1.25",
          "form": "나중에 봐야겠어요ㅎㅎ",
          "original_form": "나중에 봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100019179.1.26",
          "form": "저도 하마 추천해주세뇨",
          "original_form": "저도 하마 추천해주세뇨",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100019179.1.27",
          "form": "하나 추천!",
          "original_form": "하나 추천!",
          "speaker_id": "2",
          "time": "20210829 16:25"
        }
      ]
    }
  ]
