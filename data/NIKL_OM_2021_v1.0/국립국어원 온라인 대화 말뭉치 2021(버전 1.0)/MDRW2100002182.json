      "utterance": [
        {
          "id": "MDRW2100002182.1.1",
          "form": "넷플릭스 가입되셨나요?",
          "original_form": "넷플릭스 가입되셨나요?",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002182.1.2",
          "form": "네네 보시는 컨텐츠 있으세요?",
          "original_form": "네네 보시는 컨텐츠 있으세요?",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002182.1.3",
          "form": "요즘 알고있지만 드라마에 빠져산답니다\r\n혹시보셨나요?",
          "original_form": "요즘 알고있지만 드라마에 빠져산답니다\r\n혹시보셨나요?",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002182.1.4",
          "form": "아 안보지만 한소희를 엄청 좋아해서",
          "original_form": "아 안보지만 한소희를 엄청 좋아해서",
          "speaker_id": "2",
          "time": "20210820 01:02"
        },
        {
          "id": "MDRW2100002182.1.5",
          "form": "클립은 본적있어요",
          "original_form": "클립은 본적있어요",
          "speaker_id": "2",
          "time": "20210820 01:02"
        },
        {
          "id": "MDRW2100002182.1.6",
          "form": "한소희 너무 예쁘죠",
          "original_form": "한소희 너무 예쁘죠",
          "speaker_id": "2",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002182.1.7",
          "form": "맞아요 진짜예쁜듯!\r\n실제로 연예인본적있으세요?",
          "original_form": "맞아요 진짜예쁜듯!\r\n실제로 연예인본적있으세요?",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002182.1.8",
          "form": "음 저는 조인성 실제로 본적있어요",
          "original_form": "음 저는 조인성 실제로 본적있어요",
          "speaker_id": "2",
          "time": "20210820 01:04"
        },
        {
          "id": "MDRW2100002182.1.9",
          "form": "우와.... 멋있죠? 누가봐도 조인성이죠?ㅋㅋ",
          "original_form": "우와.... 멋있죠? 누가봐도 조인성이죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:05"
        },
        {
          "id": "MDRW2100002182.1.10",
          "form": "네ㅋㅋㅋㅋㅋ생각보다 크더라구요",
          "original_form": "네ㅋㅋㅋㅋㅋ생각보다 크더라구요",
          "speaker_id": "2",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002182.1.11",
          "form": "얼굴도 작고",
          "original_form": "얼굴도 작고",
          "speaker_id": "2",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002182.1.12",
          "form": "직접보신 연예인 있으세요?",
          "original_form": "직접보신 연예인 있으세요?",
          "speaker_id": "2",
          "time": "20210820 01:10"
        },
        {
          "id": "MDRW2100002182.1.13",
          "form": "집이 영화관 근처에살아서 지나가다 연예인들 종종봤는데 김혜수가 정말 멋있었어요",
          "original_form": "집이 영화관 근처에살아서 지나가다 연예인들 종종봤는데 김혜수가 정말 멋있었어요",
          "speaker_id": "1",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002182.1.14",
          "form": "얼굴커보이는데 하나도 안크고 멋진언니? 크!",
          "original_form": "얼굴커보이는데 하나도 안크고 멋진언니? 크!",
          "speaker_id": "1",
          "time": "20210820 01:13"
        },
        {
          "id": "MDRW2100002182.1.15",
          "form": "와 김혜수가 관객으로 온걸 본거에요??",
          "original_form": "와 김혜수가 관객으로 온걸 본거에요??",
          "speaker_id": "2",
          "time": "20210820 01:15"
        },
        {
          "id": "MDRW2100002182.1.16",
          "form": "심지어 유해진하고 같이 있던걸봤답니다 ㅎㅎㅎ",
          "original_form": "심지어 유해진하고 같이 있던걸봤답니다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002182.1.17",
          "form": "두분 잘어울렸다는!ㅋㅋ",
          "original_form": "두분 잘어울렸다는!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002182.1.18",
          "form": "앗ㅋㅋㅋㅋㅋㅋ그시절에 보셨군요...",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋ그시절에 보셨군요...",
          "speaker_id": "2",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002182.1.19",
          "form": "ㅋㅋㅋ여자가봐도 넘 멋있었어요!\r\n최근에 전지현 등장한 좀비 영화 넷플릭스로봤는데 3편 기대하고있습니다!",
          "original_form": "ㅋㅋㅋ여자가봐도 넘 멋있었어요!\r\n최근에 전지현 등장한 좀비 영화 넷플릭스로봤는데 3편 기대하고있습니다!",
          "speaker_id": "1",
          "time": "20210820 01:19"
        },
        {
          "id": "MDRW2100002182.1.20",
          "form": "아 그 아신전인가 그건가요??",
          "original_form": "아 그 아신전인가 그건가요??",
          "speaker_id": "2",
          "time": "20210820 01:21"
        },
        {
          "id": "MDRW2100002182.1.21",
          "form": "오 네네 꼭 보세요 재미나요\r\n왜 한국판좀비가 등장했는지 ㅎㅎㅎㅎ",
          "original_form": "오 네네 꼭 보세요 재미나요\r\n왜 한국판좀비가 등장했는지 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 01:22"
        },
        {
          "id": "MDRW2100002182.1.22",
          "form": "아 넷플릭스는 항상 보고싶은건 많은데 미루다가 못보는것 같아요ㅜ",
          "original_form": "아 넷플릭스는 항상 보고싶은건 많은데 미루다가 못보는것 같아요ㅜ",
          "speaker_id": "2",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002182.1.23",
          "form": "찜해두고 나중에 꼭 볼게요!",
          "original_form": "찜해두고 나중에 꼭 볼게요!",
          "speaker_id": "2",
          "time": "20210820 01:23"
        }
      ]
    }
  ]
