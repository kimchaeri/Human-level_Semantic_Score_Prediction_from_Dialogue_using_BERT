      "utterance": [
        {
          "id": "MDRW2100027188.1.1",
          "form": "혹시 가족중에 군대 다녀오신 분 있나요?!",
          "original_form": "혹시 가족중에 군대 다녀오신 분 있나요?!",
          "speaker_id": "2",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100027188.1.2",
          "form": "저는 음 저랑 여동생 뿐이라ㅜ",
          "original_form": "저는 음 저랑 여동생 뿐이라ㅜ",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100027188.1.3",
          "form": "혹시 오빠나 남동생?",
          "original_form": "혹시 오빠나 남동생?",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100027188.1.4",
          "form": "아까 오빠 있다고 하셨던가요?",
          "original_form": "아까 오빠 있다고 하셨던가요?",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100027188.1.5",
          "form": "전 남동생 있어요!",
          "original_form": "전 남동생 있어요!",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100027188.1.6",
          "form": "군대 다녀온...ㅋㅋㅋ",
          "original_form": "군대 다녀온...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100027188.1.7",
          "form": "남동생 전역한지 오래됐나요? ㅋㅋ",
          "original_form": "남동생 전역한지 오래됐나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100027188.1.8",
          "form": "둘다 30대다보니...ㅋㅋㅋㅋ",
          "original_form": "둘다 30대다보니...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100027188.1.9",
          "form": "민방위?도 끝났어옄ㅋㅋㅋㅋ",
          "original_form": "민방위?도 끝났어옄ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100027188.1.10",
          "form": "ㅋㅋ 글쿤요 육군 나오셨으나요?",
          "original_form": "ㅋㅋ 글쿤요 육군 나오셨으나요?",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100027188.1.11",
          "form": "네네 ㅋㅋㅋ 취사병이었어요!!",
          "original_form": "네네 ㅋㅋㅋ 취사병이었어요!!",
          "speaker_id": "2",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100027188.1.12",
          "form": "갔다오니 철좀 들던가요 ㅋㅋ",
          "original_form": "갔다오니 철좀 들던가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100027188.1.13",
          "form": "오 신기하다 취사병 요리를 원래좀 하셨나 어캐뽑히신거지 ㅎ",
          "original_form": "오 신기하다 취사병 요리를 원래좀 하셨나 어캐뽑히신거지 ㅎ",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100027188.1.14",
          "form": "원래 요리 좀 좋아했ㄴ느데ㅋㅋㅋ",
          "original_form": "원래 요리 좀 좋아했ㄴ느데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100027188.1.15",
          "form": "군대에서 뭐라도 배워오고 싶다고 자기가 손들었대요",
          "original_form": "군대에서 뭐라도 배워오고 싶다고 자기가 손들었대요",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100027188.1.16",
          "form": "전역하고도 알바를 다 요식업 쪽에서 하더라고요",
          "original_form": "전역하고도 알바를 다 요식업 쪽에서 하더라고요",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100027188.1.17",
          "form": "오 근데 진짜 배워왔나요?",
          "original_form": "오 근데 진짜 배워왔나요?",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027188.1.18",
          "form": "요리를 ㅎㅎ",
          "original_form": "요리를 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027188.1.19",
          "form": "칼은 잘쓰려나 채썰고 ㅋㅋ",
          "original_form": "칼은 잘쓰려나 채썰고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100027188.1.20",
          "form": "아 그런거 잘해요 ㅋㅋㅋㅋ",
          "original_form": "아 그런거 잘해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100027188.1.21",
          "form": "제가 진짜 요리 못하고...",
          "original_form": "제가 진짜 요리 못하고...",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100027188.1.22",
          "form": "채썰고 이런건 익혀왔더라구요",
          "original_form": "채썰고 이런건 익혀왔더라구요",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100027188.1.23",
          "form": "취사병이 다른 보직보다 힘들대요",
          "original_form": "취사병이 다른 보직보다 힘들대요",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100027188.1.24",
          "form": "그죠 밥먹는건 비와도 눈와도 안쉬잖아요ㅜㅜ",
          "original_form": "그죠 밥먹는건 비와도 눈와도 안쉬잖아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100027188.1.25",
          "form": "그럼 요리가 아니고 재료손질만 배워 온건가요 ㅋㄱ",
          "original_form": "그럼 요리가 아니고 재료손질만 배워 온건가요 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100027188.1.26",
          "form": "아 요리도 배우긴 했어욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 요리도 배우긴 했어욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100027188.1.27",
          "form": "근데 일반 요리?랑 단체급식요리랑 좀 다르대요",
          "original_form": "근데 일반 요리?랑 단체급식요리랑 좀 다르대요",
