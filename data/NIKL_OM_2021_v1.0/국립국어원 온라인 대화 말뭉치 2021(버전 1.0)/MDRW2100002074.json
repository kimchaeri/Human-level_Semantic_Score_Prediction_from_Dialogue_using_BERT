      "utterance": [
        {
          "id": "MDRW2100002074.1.1",
          "form": "가족때문에 집에 아예 못 키워?",
          "original_form": "가족때문에 집에 아예 못 키워?",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002074.1.2",
          "form": "그래서 넘 아쉬운..ㅠㅠ",
          "original_form": "그래서 넘 아쉬운..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002074.1.3",
          "form": "진짜 키우고 싶은데",
          "original_form": "진짜 키우고 싶은데",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002074.1.4",
          "form": "그럼 지인들 반려동물만 만나겠네",
          "original_form": "그럼 지인들 반려동물만 만나겠네",
          "speaker_id": "2",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002074.1.5",
          "form": "알러지 그것도 문제지",
          "original_form": "알러지 그것도 문제지",
          "speaker_id": "2",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002074.1.6",
          "form": "자취하면 키우고 싶기도 한데",
          "original_form": "자취하면 키우고 싶기도 한데",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002074.1.7",
          "form": "근데 그러면 혼자 있는 시간이 기니까 너무 미안해서 그것두 모르겠어",
          "original_form": "근데 그러면 혼자 있는 시간이 기니까 너무 미안해서 그것두 모르겠어",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002074.1.8",
          "form": "하긴 가족들이랑 다같이 키워야 맘 편한데",
          "original_form": "하긴 가족들이랑 다같이 키워야 맘 편한데",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002074.1.9",
          "form": "혼자 키우는 건 죄책감 생길것 같더라구요",
          "original_form": "혼자 키우는 건 죄책감 생길것 같더라구요",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002074.1.10",
          "form": "분리불안도 생길테고",
          "original_form": "분리불안도 생길테고",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002074.1.11",
          "form": "그니까ㅠㅠ만약 키우면 고양이 한 마리 키우고 싶긴 한데",
          "original_form": "그니까ㅠㅠ만약 키우면 고양이 한 마리 키우고 싶긴 한데",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002074.1.12",
          "form": "가능할진 모르겠는..ㅎㅎ",
          "original_form": "가능할진 모르겠는..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002074.1.13",
          "form": "냥줍을 바랄 수 밖에 없겠어요",
          "original_form": "냥줍을 바랄 수 밖에 없겠어요",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002074.1.14",
          "form": "집 근처에 가끔 만나는 길냥이 있는데",
          "original_form": "집 근처에 가끔 만나는 길냥이 있는데",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002074.1.15",
          "form": "너무 귀여워요ㅋㅋㅋ",
          "original_form": "너무 귀여워요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002074.1.16",
          "form": "길 고양이들 오손도손 모여있는 거 귀엽겠다",
          "original_form": "길 고양이들 오손도손 모여있는 거 귀엽겠다",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002074.1.17",
          "form": "아는 척 해요?",
          "original_form": "아는 척 해요?",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002074.1.18",
          "form": "아는척 해요!",
          "original_form": "아는척 해요!",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002074.1.19",
          "form": "근데 너무 웃긴게 만나면 배 까고 드러눕는데",
          "original_form": "근데 너무 웃긴게 만나면 배 까고 드러눕는데",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002074.1.20",
          "form": "와우 똑똑하다",
          "original_form": "와우 똑똑하다",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002074.1.21",
          "form": "고양이가 아니라 개냥이네요",
          "original_form": "고양이가 아니라 개냥이네요",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002074.1.22",
          "form": "그쵸 진짜 순해요",
          "original_form": "그쵸 진짜 순해요",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002074.1.23",
          "form": "사진도 있는데 너무 귀엽고..",
          "original_form": "사진도 있는데 너무 귀엽고..",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002074.1.24",
          "form": "어떻게 길 생활 하는건지 안타깝네 적응된건가",
          "original_form": "어떻게 길 생활 하는건지 안타깝네 적응된건가",
          "speaker_id": "2",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002074.1.25",
          "form": "사진 못 봐서 아쉬워요",
          "original_form": "사진 못 봐서 아쉬워요",
          "speaker_id": "2",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002074.1.26",
          "form": "그쵸 못 보여줘서 아쉬운 마음ㅠㅠ",
          "original_form": "그쵸 못 보여줘서 아쉬운 마음ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002074.1.27",
          "form": "그래도 사람들이 많이 챙겨주는 거 같더라구요",
          "original_form": "그래도 사람들이 많이 챙겨주는 거 같더라구요",
          "speaker_id": "1",
          "time": "20210819 23:50"
        }
      ]
    }
