      "utterance": [
        {
          "id": "MDRW2100044790.1.1",
          "form": "요즘 보는 프로그램 있으신가요?",
          "original_form": "요즘 보는 프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044790.1.2",
          "form": "유퀴즈 봐요",
          "original_form": "유퀴즈 봐요",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044790.1.3",
          "form": "오 저도 잘보고 있어요",
          "original_form": "오 저도 잘보고 있어요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044790.1.4",
          "form": "최근 양궁선수 나온거 보셨나요",
          "original_form": "최근 양궁선수 나온거 보셨나요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044790.1.5",
          "form": "네 봤어요ㅋㅋ 다들 귀엽고 재밌으시더라구요",
          "original_form": "네 봤어요ㅋㅋ 다들 귀엽고 재밌으시더라구요",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044790.1.6",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044790.1.7",
          "form": "드라마도 보시나요?",
          "original_form": "드라마도 보시나요?",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044790.1.8",
          "form": "요새 슬의생 보고 있어요",
          "original_form": "요새 슬의생 보고 있어요",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044790.1.9",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044790.1.10",
          "form": "네 재밌어요ㅋㅋ 주에 1번만 해서 좀 감질나긴 하네요",
          "original_form": "네 재밌어요ㅋㅋ 주에 1번만 해서 좀 감질나긴 하네요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044790.1.11",
          "form": "뭐 보시는거 있나요?",
          "original_form": "뭐 보시는거 있나요?",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044790.1.12",
          "form": "저는 보이스4 보고있습니다",
          "original_form": "저는 보이스4 보고있습니다",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044790.1.13",
          "form": "보신적 있으신가요?",
          "original_form": "보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044790.1.14",
          "form": "아뇨ㅋㅋ 시리즈 재밌다고 듣긴 했어요",
          "original_form": "아뇨ㅋㅋ 시리즈 재밌다고 듣긴 했어요",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044790.1.15",
          "form": "1만 보고 4재밌다길래 시작했는데 재밌네요",
          "original_form": "1만 보고 4재밌다길래 시작했는데 재밌네요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044790.1.16",
          "form": "무서운거 잘보시나요?",
          "original_form": "무서운거 잘보시나요?",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044790.1.17",
          "form": "네 평범하게 보는 편이에요ㅋㅋ",
          "original_form": "네 평범하게 보는 편이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044790.1.18",
          "form": "귀신 나오는것도 보세요??",
          "original_form": "귀신 나오는것도 보세요??",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044790.1.19",
          "form": "저는 못봐요ㅎㅎ",
          "original_form": "저는 못봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044790.1.20",
          "form": "보긴 하는데 재미있어 하는 편은 아니에요",
          "original_form": "보긴 하는데 재미있어 하는 편은 아니에요",
