      "utterance": [
        {
          "id": "MDRW2100010230.1.1",
          "form": "코로나라서 모든종교단체들은 다 온라인하더라구요",
          "original_form": "코로나라서 모든종교단체들은 다 온라인하더라구요",
          "speaker_id": "1",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100010230.1.2",
          "form": "맞아요 그것도 신기했어요ㅎㅎ",
          "original_form": "맞아요 그것도 신기했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010230.1.3",
          "form": "이제 절와도 비빔밥 못먹어요ㅜ",
          "original_form": "이제 절와도 비빔밥 못먹어요ㅜ",
          "speaker_id": "1",
          "time": "20210823 15:54"
        },
        {
          "id": "MDRW2100010230.1.4",
          "form": "엇 아쉬워요ㅠㅠㅠ템플스테이 하고 싶었는데",
          "original_form": "엇 아쉬워요ㅠㅠㅠ템플스테이 하고 싶었는데",
          "speaker_id": "2",
          "time": "20210823 15:54"
        },
        {
          "id": "MDRW2100010230.1.5",
          "form": "2주 격리까지 세트로 해야하는데 ..",
          "original_form": "2주 격리까지 세트로 해야하는데 ..",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010230.1.6",
          "form": "가능한가모르겠어요 ㅋㅋ",
          "original_form": "가능한가모르겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010230.1.7",
          "form": "채식 절밥이 건강하고 좋긴했는데",
          "original_form": "채식 절밥이 건강하고 좋긴했는데",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010230.1.8",
          "form": "네 한번 먹어봤는데 맛있더라구요...절에 있으면 살찔수도ㅎㅎ",
          "original_form": "네 한번 먹어봤는데 맛있더라구요...절에 있으면 살찔수도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100010230.1.9",
          "form": "집에서 해먹어야죠뭐",
          "original_form": "집에서 해먹어야죠뭐",
          "speaker_id": "1",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010230.1.10",
          "form": "ㅋㅋ 절은 도량이 넓으니까 화장실도 멀리걸어가고",
          "original_form": "ㅋㅋ 절은 도량이 넓으니까 화장실도 멀리걸어가고",
          "speaker_id": "1",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010230.1.11",
          "form": "빠질수도 있어요",
          "original_form": "빠질수도 있어요",
          "speaker_id": "1",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010230.1.12",
          "form": "ㅋㅋ그맛이 안나요ㅠ자연의 맛ㅋㅋ",
          "original_form": "ㅋㅋ그맛이 안나요ㅠ자연의 맛ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010230.1.13",
          "form": "배달음식보다는 나을수있겠네요",
          "original_form": "배달음식보다는 나을수있겠네요",
          "speaker_id": "1",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100010230.1.14",
          "form": "오..108배같은것도 궁금하구...",
          "original_form": "오..108배같은것도 궁금하구...",
          "speaker_id": "2",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010230.1.15",
          "form": "아 그럼 더 배고파지죠..",
          "original_form": "아 그럼 더 배고파지죠..",
          "speaker_id": "1",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010230.1.16",
          "form": "유툽에 절하는방법 있을거같아요",
          "original_form": "유툽에 절하는방법 있을거같아요",
          "speaker_id": "1",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010230.1.17",
          "form": "아 그런것도 있구나ㅋㅋㅋ랜선으로 템플스테이ㅎㅎ",
          "original_form": "아 그런것도 있구나ㅋㅋㅋ랜선으로 템플스테이ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010230.1.18",
          "form": "절은 산이니까 산나물캐와서 먹는게 좋은거같아요",
          "original_form": "절은 산이니까 산나물캐와서 먹는게 좋은거같아요",
          "speaker_id": "1",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100010230.1.19",
          "form": "온라인이 대세라..무시할수없는거같아요",
          "original_form": "온라인이 대세라..무시할수없는거같아요",
          "speaker_id": "1",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100010230.1.20",
          "form": "맞아요 그래보여요 신선하구..",
          "original_form": "맞아요 그래보여요 신선하구..",
          "speaker_id": "2",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100010230.1.21",
          "form": "밥은 온라인으로 봐도 충족이 안되니..",
          "original_form": "밥은 온라인으로 봐도 충족이 안되니..",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010230.1.22",
          "form": "배달해야죠뭐",
          "original_form": "배달해야죠뭐",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010230.1.23",
          "form": "ㅋㅋㅋ아니면 사찰음식이라두..",
          "original_form": "ㅋㅋㅋ아니면 사찰음식이라두..",
          "speaker_id": "2",
          "time": "20210823 16:02"
        }
      ]
    }
