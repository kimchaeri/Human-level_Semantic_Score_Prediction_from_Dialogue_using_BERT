      "utterance": [
        {
          "id": "MDRW2100036360.1.1",
          "form": "전 못먹을꺼같지만",
          "original_form": "전 못먹을꺼같지만",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100036360.1.2",
          "form": "눈알인줄 모르고",
          "original_form": "눈알인줄 모르고",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100036360.1.3",
          "form": "근데 엄청난 서비스아니에요!?!?!",
          "original_form": "근데 엄청난 서비스아니에요!?!?!",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100036360.1.4",
          "form": "이게 뭐냐고 했는데...ㅠㅠㅋㅋㅋㅋ",
          "original_form": "이게 뭐냐고 했는데...ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100036360.1.5",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100036360.1.6",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100036360.1.7",
          "form": "제가 그날 좀 많이 먹었나봐요ㅋㅋㅋ",
          "original_form": "제가 그날 좀 많이 먹었나봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100036360.1.8",
          "form": "참치회도좋아하시능군요!!!",
          "original_form": "참치회도좋아하시능군요!!!",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100036360.1.9",
          "form": "네네 좋아해용ㅎㅎㅎ",
          "original_form": "네네 좋아해용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100036360.1.10",
          "form": "전 회는 무슨맛인지잘모르겟어요 ㅠㅠ 다먹긴하는데 ㅋㅋㅋ",
          "original_form": "전 회는 무슨맛인지잘모르겟어요 ㅠㅠ 다먹긴하는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100036360.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100036360.1.12",
          "form": "저도 맛 분간은 잘 못합니다ㅋㅋ",
          "original_form": "저도 맛 분간은 잘 못합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100036360.1.13",
          "form": "너무안주스러워여 ㅋㅋㅋㅋ그냥 술안주",
          "original_form": "너무안주스러워여 ㅋㅋㅋㅋ그냥 술안주",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100036360.1.14",
          "form": "회에서는 술을 빠질수가 없죠흐흐",
          "original_form": "회에서는 술을 빠질수가 없죠흐흐",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100036360.1.15",
          "form": "ㅋㅋㅋㅋ 전 회에는 소맥이나 소주 ...ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 전 회에는 소맥이나 소주 ...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100036360.1.16",
          "form": "아 약간 소독되는 느낌을 좋아하시는구나",
