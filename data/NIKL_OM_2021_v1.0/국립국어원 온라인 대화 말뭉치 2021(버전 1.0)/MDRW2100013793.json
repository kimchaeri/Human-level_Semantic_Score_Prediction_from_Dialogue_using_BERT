      "utterance": [
        {
          "id": "MDRW2100013793.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013793.1.2",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "2",
          "time": "20210825 15:58"
        },
        {
          "id": "MDRW2100013793.1.3",
          "form": "단무지없이 자장면 먹기 vs 김치없이 라면먹기 어떠세요?",
          "original_form": "단무지없이 자장면 먹기 vs 김치없이 라면먹기 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013793.1.4",
          "form": "김치없이라면 먹기",
          "original_form": "김치없이라면 먹기",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013793.1.5",
          "form": "ㅋㅋㅋ저도요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013793.1.6",
          "form": "저는원래 김치없이 라면을 먹거든요!",
          "original_form": "저는원래 김치없이 라면을 먹거든요!",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013793.1.7",
          "form": "재밋다요",
          "original_form": "재밋다요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013793.1.8",
          "form": "또또요",
          "original_form": "또또요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013793.1.9",
          "form": "호박고구마vs밤고구마는 어떠세요?",
          "original_form": "호박고구마vs밤고구마는 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013793.1.10",
          "form": "밤고구마요",
          "original_form": "밤고구마요",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013793.1.11",
          "form": "저랑 반대시네요ㅎ",
          "original_form": "저랑 반대시네요ㅎ",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100013793.1.12",
          "form": "약간 설잊은?",
          "original_form": "약간 설잊은?",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100013793.1.13",
          "form": "맛을 좋아해요",
          "original_form": "맛을 좋아해요",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100013793.1.14",
          "form": "딱딱한거 좋아해요",
          "original_form": "딱딱한거 좋아해요",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100013793.1.15",
          "form": "오늘 저녁 뭐먹을거에요?",
          "original_form": "오늘 저녁 뭐먹을거에요?",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100013793.1.16",
          "form": "아직 고민중이랍니다!",
          "original_form": "아직 고민중이랍니다!",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100013793.1.17",
          "form": "저녁뭐드시나요?",
          "original_form": "저녁뭐드시나요?",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100013793.1.18",
          "form": "술마시고싶은데  ....",
          "original_form": "술마시고싶은데  ....",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013793.1.19",
          "form": "저녁때  보고요",
          "original_form": "저녁때  보고요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013793.1.20",
          "form": "반주안주 생각중이요",
          "original_form": "반주안주 생각중이요",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013793.1.21",
          "form": "곱창어떤가요",
          "original_form": "곱창어떤가요",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100013793.1.22",
          "form": "아님 닭발?",
          "original_form": "아님 닭발?",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100013793.1.23",
          "form": "둘다 너무 좋으다",
          "original_form": "둘다 너무 좋으다",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100013793.1.24",
          "form": "소주파? 맥주파?",
          "original_form": "소주파? 맥주파?",
          "speaker_id": "2",
          "time": "20210825 17:49"
        }
      ]
