      "utterance": [
        {
          "id": "MDRW2100017294.1.1",
          "form": "여행 많이 다녀보셨나요?!",
          "original_form": "여행 많이 다녀보셨나요?!",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017294.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100017294.1.3",
          "form": "코로나 전까지는 애들 데리고 여기저기 다녔는데..",
          "original_form": "코로나 전까지는 애들 데리고 여기저기 다녔는데..",
          "speaker_id": "2",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100017294.1.4",
          "form": "지금은 가까운곳만 잠깐 가는정도요",
          "original_form": "지금은 가까운곳만 잠깐 가는정도요",
          "speaker_id": "2",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100017294.1.5",
          "form": "주로 어디로 다니세요?",
          "original_form": "주로 어디로 다니세요?",
          "speaker_id": "1",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100017294.1.6",
          "form": "저는 살고있는곳이 동해쪽이라 한두시간이면 바닷가지역이라 가끔가는편이에요",
          "original_form": "저는 살고있는곳이 동해쪽이라 한두시간이면 바닷가지역이라 가끔가는편이에요",
          "speaker_id": "1",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100017294.1.7",
          "form": "저도 포항이나 드라이브삼아 가요 ㅋㅋㅋ",
          "original_form": "저도 포항이나 드라이브삼아 가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100017294.1.8",
          "form": "부산은 사람이 너무 많고..",
          "original_form": "부산은 사람이 너무 많고..",
          "speaker_id": "2",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100017294.1.9",
          "form": "진짜 운전하기힘들어요ㅠ",
          "original_form": "진짜 운전하기힘들어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100017294.1.10",
          "form": "남부지방 사시나봐요!",
          "original_form": "남부지방 사시나봐요!",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100017294.1.11",
          "form": "전중부라 강릉 속초 자주가요ㅋㅋㅋㅋ",
          "original_form": "전중부라 강릉 속초 자주가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100017294.1.12",
          "form": "넵넵~어딜가도 요즘은 차안이.가장 안전한듯요..ㅜㅜ",
          "original_form": "넵넵~어딜가도 요즘은 차안이.가장 안전한듯요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100017294.1.13",
          "form": "씁쓸해요~",
          "original_form": "씁쓸해요~",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100017294.1.14",
          "form": "속초에 닭강정 유명허죠??",
          "original_form": "속초에 닭강정 유명허죠??",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100017294.1.15",
          "form": "네네 근데 요즘은 해변가에 포토존도 많아지고",
          "original_form": "네네 근데 요즘은 해변가에 포토존도 많아지고",
          "speaker_id": "1",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100017294.1.16",
          "form": "여행보다는 투어하러 많이 오더라고요ㅋㅋㅋ",
          "original_form": "여행보다는 투어하러 많이 오더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:03"
        },
        {
          "id": "MDRW2100017294.1.17",
          "form": "여행도 정말 사람 없는곳 찾아서 조심조심 ㅋㅋㅋㅋㅋ",
          "original_form": "여행도 정말 사람 없는곳 찾아서 조심조심 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100017294.1.18",
          "form": "어쩌다 이런 시대가ㅠㅠ",
          "original_form": "어쩌다 이런 시대가ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 10:08"
        },
        {
          "id": "MDRW2100017294.1.19",
          "form": "예전이 너무 그리워요~",
          "original_form": "예전이 너무 그리워요~",
          "speaker_id": "2",
          "time": "20210827 10:09"
        },
        {
          "id": "MDRW2100017294.1.20",
          "form": "그러니까요ㅠ",
          "original_form": "그러니까요ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:13"
        },
        {
          "id": "MDRW2100017294.1.21",
          "form": "요즘은 여행을 가도 계획 많이 세우기보다는",
          "original_form": "요즘은 여행을 가도 계획 많이 세우기보다는",
          "speaker_id": "1",
          "time": "20210827 10:13"
        },
        {
          "id": "MDRW2100017294.1.22",
          "form": "거의 호텔이나 당일로 많이 가잖아요",
          "original_form": "거의 호텔이나 당일로 많이 가잖아요",
          "speaker_id": "1",
          "time": "20210827 10:14"
        },
        {
          "id": "MDRW2100017294.1.23",
          "form": "맞아요~근데 숙박 너무 비싸서ㅠㅠ",
          "original_form": "맞아요~근데 숙박 너무 비싸서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100017294.1.24",
          "form": "저도 올 휴가 애들때문에 풀빌라 진짜 손떨면서 예약했어요ㅠ",
          "original_form": "저도 올 휴가 애들때문에 풀빌라 진짜 손떨면서 예약했어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100017294.1.25",
          "form": "근데..얼마 못놀고ㅠ 눈물이 ㅋㅋㅋㅋ",
          "original_form": "근데..얼마 못놀고ㅠ 눈물이 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100017294.1.26",
          "form": "애들 있으면 또 시설 좋은데 가줘야하자나요ㅠㅠㅠ",
          "original_form": "애들 있으면 또 시설 좋은데 가줘야하자나요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100017294.1.27",
          "form": "풀빌라 엄청 비싸던데.... 그냥 자는시간 빼고 물에서 놀아야하는 수줔",
          "original_form": "풀빌라 엄청 비싸던데.... 그냥 자는시간 빼고 물에서 놀아야하는 수줔",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100017294.1.28",
          "form": "진짜 지금도 눈물이 날것같아여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 지금도 눈물이 날것같아여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100017294.1.29",
          "form": "그래도 다녀왔다는거에ㅠㅠ",
          "original_form": "그래도 다녀왔다는거에ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100017294.1.30",
          "form": "열심히 돈벌어야죠ㅠ ㅋㅋㅋ",
          "original_form": "열심히 돈벌어야죠ㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100017294.1.31",
          "form": "아이들 있으면 그래도 여행 자주 다니시겠네요!",
          "original_form": "아이들 있으면 그래도 여행 자주 다니시겠네요!",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100017294.1.32",
          "form": "집에만 있으면 답답해해서",
          "original_form": "집에만 있으면 답답해해서",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100017294.1.33",
          "form": "어쩔 수 없이 나가긴 해야 해요ㅠ",
          "original_form": "어쩔 수 없이 나가긴 해야 해요ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100017294.1.34",
          "form": "최대한 사람 없는 곳 조용한 곳으로 가요ㅠ",
          "original_form": "최대한 사람 없는 곳 조용한 곳으로 가요ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:30"
        }
      ]
    }
  ]
