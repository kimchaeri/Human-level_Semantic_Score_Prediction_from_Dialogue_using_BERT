          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006147.1.3",
          "form": "비많이오죠?",
          "original_form": "비많이오죠?",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006147.1.4",
          "form": "네 갑자기 주룩주룩",
          "original_form": "네 갑자기 주룩주룩",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006147.1.5",
          "form": "오네요ㅜ",
          "original_form": "오네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006147.1.6",
          "form": "많이 습하지 않나요",
          "original_form": "많이 습하지 않나요",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006147.1.7",
          "form": "네 더 안좋아요ㅋㅋ",
          "original_form": "네 더 안좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006147.1.8",
          "form": "찐득찐득",
          "original_form": "찐득찐득",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006147.1.9",
          "form": "춥자않은데 찐득해서 보일러켜게되네요",
          "original_form": "춥자않은데 찐득해서 보일러켜게되네요",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006147.1.10",
          "form": "맞아요 저희는 제습기 ㅎㅎ",
          "original_form": "맞아요 저희는 제습기 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006147.1.11",
          "form": "제습기 지금도 씽씽 돌아가고있겠네요",
          "original_form": "제습기 지금도 씽씽 돌아가고있겠네요",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006147.1.12",
          "form": "저는 곱슬이라",
          "original_form": "저는 곱슬이라",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006147.1.13",
          "form": "비올때마다 머리난리나네요",
          "original_form": "비올때마다 머리난리나네요",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006147.1.14",
          "form": "어유 귀찮으시겠어요",
          "original_form": "어유 귀찮으시겠어요",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006147.1.15",
          "form": "네 원래도 부스스한데",
          "original_form": "네 원래도 부스스한데",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006147.1.16",
          "form": "해그리드가 된답니다",
          "original_form": "해그리드가 된답니다",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006147.1.17",
          "form": "비가 빨리 그쳐야겠군요",
          "original_form": "비가 빨리 그쳐야겠군요",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006147.1.18",
          "form": "네그리고 지성이라",
          "original_form": "네그리고 지성이라",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006147.1.19",
          "form": "잘 마르지도 않더라구요",
          "original_form": "잘 마르지도 않더라구요",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006147.1.20",
          "form": "장마 진짜 싫어하시겠어요",
          "original_form": "장마 진짜 싫어하시겠어요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006147.1.21",
          "form": "비오는건 싫지않은데 그런단점도 있긴해요",
          "original_form": "비오는건 싫지않은데 그런단점도 있긴해요",
