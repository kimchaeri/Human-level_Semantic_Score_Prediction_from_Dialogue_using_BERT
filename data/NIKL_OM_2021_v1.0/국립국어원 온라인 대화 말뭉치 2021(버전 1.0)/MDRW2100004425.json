        {
          "id": "MDRW2100004425.1.2",
          "form": "안녕 name1야",
          "original_form": "안녕 &name1&야",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004425.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004425.1.4",
          "form": "너 배달 자주 시켜먹니?",
          "original_form": "너 배달 자주 시켜먹니?",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004425.1.5",
          "form": "나 오늘 진짜 오랜만에 커피 마셨다!",
          "original_form": "나 오늘 진짜 오랜만에 커피 마셨다!",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004425.1.6",
          "form": "아니 어쩐일로?",
          "original_form": "아니 어쩐일로?",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004425.1.7",
          "form": "응 자주 시켜...좀 부끄럽지만",
          "original_form": "응 자주 시켜...좀 부끄럽지만",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004425.1.8",
          "form": "동생이 시킨다고 그래서 나도 낼름 하나 시켰지",
          "original_form": "동생이 시킨다고 그래서 나도 낼름 하나 시켰지",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004425.1.9",
          "form": "갑자기 비가 너무 와서",
          "original_form": "갑자기 비가 너무 와서",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004425.1.10",
          "form": "배달기사님한테 넘 죄송했어",
          "original_form": "배달기사님한테 넘 죄송했어",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004425.1.11",
          "form": "ㅠㅠㅜㅠㅠㅠ",
          "original_form": "ㅠㅠㅜㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004425.1.12",
          "form": "그리고 나 요리를 잘 못해서",
          "original_form": "그리고 나 요리를 잘 못해서",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004425.1.13",
          "form": "그런 주제에 편식도 심해서",
          "original_form": "그런 주제에 편식도 심해서",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004425.1.14",
          "form": "다양하게 먹고싶어서 배달 자주 시켜",
          "original_form": "다양하게 먹고싶어서 배달 자주 시켜",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004425.1.15",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004425.1.16",
          "form": "의외다",
          "original_form": "의외다",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004425.1.17",
          "form": "집에 있으면 집밥 해먹을줄알았는데",
          "original_form": "집에 있으면 집밥 해먹을줄알았는데",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004425.1.18",
          "form": "나도 만약 자취안했으면 배민 깔지도않았을듯",
          "original_form": "나도 만약 자취안했으면 배민 깔지도않았을듯",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004425.1.19",
          "form": "근데 평소엔 다른 음료 마셨을텐데",
          "original_form": "근데 평소엔 다른 음료 마셨을텐데",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004425.1.20",
          "form": "이번엔 커피시켰네?",
          "original_form": "이번엔 커피시켰네?",
          "speaker_id": "2",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004425.1.21",
          "form": "응. 원래는 건강때문에 15일, 30일에만 당 들어간 커피 마시는데",
          "original_form": "응. 원래는 건강때문에 15일, 30일에만 당 들어간 커피 마시는데",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004425.1.22",
          "form": "최근에 힘들어서 당이 필요했어",
          "original_form": "최근에 힘들어서 당이 필요했어",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004425.1.23",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004425.1.24",
          "form": "그랬구나",
          "original_form": "그랬구나",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004425.1.25",
          "form": "나는 그냥 습관처럼 마시는데",
          "original_form": "나는 그냥 습관처럼 마시는데",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004425.1.26",
          "form": "중독된듯",
          "original_form": "중독된듯",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004425.1.27",
          "form": "하루 한 잔 정도는 괜찮으니까~",
          "original_form": "하루 한 잔 정도는 괜찮으니까~",
          "speaker_id": "1",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004425.1.28",
          "form": "액상과당을 너무 섭취하지만 않으면 되지",
          "original_form": "액상과당을 너무 섭취하지만 않으면 되지",
          "speaker_id": "1",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004425.1.29",
          "form": "억지로 먹고 싶은 거 참는것도 스트레스더라",
          "original_form": "억지로 먹고 싶은 거 참는것도 스트레스더라",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004425.1.30",
          "form": "마셔마셔",
          "original_form": "마셔마셔",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004425.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004425.1.32",
          "form": "저녁 뭐 먹을거야?",
          "original_form": "저녁 뭐 먹을거야?",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004425.1.33",
          "form": "나 이제 대충 일끝냄",
          "original_form": "나 이제 대충 일끝냄",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004425.1.34",
          "form": "저녁에 동생이 볶음밥 해준댔음",
          "original_form": "저녁에 동생이 볶음밥 해준댔음",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004425.1.35",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004425.1.36",
          "form": "동생 착하네",
          "original_form": "동생 착하네",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004425.1.37",
          "form": "그냥 야채볶음밥?",
          "original_form": "그냥 야채볶음밥?",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004425.1.38",
          "form": "점심에 먹다 남은 삼겹살로 볶아준대",
          "original_form": "점심에 먹다 남은 삼겹살로 볶아준대",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004425.1.39",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210821 17:22"
        }
      ]
