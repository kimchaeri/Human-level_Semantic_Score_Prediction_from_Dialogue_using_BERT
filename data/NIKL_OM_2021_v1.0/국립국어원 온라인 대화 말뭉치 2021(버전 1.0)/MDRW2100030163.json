      "utterance": [
        {
          "id": "MDRW2100030163.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030163.1.2",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030163.1.3",
          "form": "어디가 제일 좋으셨어요?",
          "original_form": "어디가 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030163.1.4",
          "form": "중학생때 일본 여행 갔던게 기억이 나네요!",
          "original_form": "중학생때 일본 여행 갔던게 기억이 나네요!",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030163.1.5",
          "form": "일찍 해외가셨네요!",
          "original_form": "일찍 해외가셨네요!",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030163.1.6",
          "form": "전 직장들어가서 처음 중국",
          "original_form": "전 직장들어가서 처음 중국",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030163.1.7",
          "form": "오! 저는 중국 한번도 안가봤는데 어떤가요?",
          "original_form": "오! 저는 중국 한번도 안가봤는데 어떤가요?",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030163.1.8",
          "form": "패키지로 북경인데 옛날이긴한데 더럽고 정신없고ㅋㅋ",
          "original_form": "패키지로 북경인데 옛날이긴한데 더럽고 정신없고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030163.1.9",
          "form": "홍콩 자유여행은 좋았어요ㅎㅎ이제 다시 못가지만요ㅜ",
          "original_form": "홍콩 자유여행은 좋았어요ㅎㅎ이제 다시 못가지만요ㅜ",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030163.1.10",
          "form": "아ㅏ 홍콩..ㅜㅜ 그쵸 요즘 또 그래서...",
          "original_form": "아ㅏ 홍콩..ㅜㅜ 그쵸 요즘 또 그래서...",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030163.1.11",
          "form": "그러니까요ㅜㅜ 코로나 끝남 어디가고싶으세요?",
          "original_form": "그러니까요ㅜㅜ 코로나 끝남 어디가고싶으세요?",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030163.1.12",
          "form": "저는 일본어 잘하는 친구가 있어서  친구랑 일본 자유 여행 계획을 실행하고 싶네요",
          "original_form": "저는 일본어 잘하는 친구가 있어서  친구랑 일본 자유 여행 계획을 실행하고 싶네요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030163.1.13",
          "form": "도쿄로 갈 것 같아요!",
          "original_form": "도쿄로 갈 것 같아요!",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030163.1.14",
          "form": "아 언어 잘하는 친구있음 진짜 여행 열배는 더 좋아지는거같아요",
          "original_form": "아 언어 잘하는 친구있음 진짜 여행 열배는 더 좋아지는거같아요",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030163.1.15",
          "form": "맞아요 사기 당할 일도 없고 말도 잘 풀리니까요",
          "original_form": "맞아요 사기 당할 일도 없고 말도 잘 풀리니까요",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030163.1.16",
          "form": "홍콩 자유여행이면 영어 쓰셨나요?",
          "original_form": "홍콩 자유여행이면 영어 쓰셨나요?",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030163.1.17",
          "form": "그때 현지에 친구가 살고있었어요ㅎㅎ",
          "original_form": "그때 현지에 친구가 살고있었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:59"
        }
      ]
