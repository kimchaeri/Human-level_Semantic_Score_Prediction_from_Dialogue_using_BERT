      "utterance": [
        {
          "id": "MDRW2100041441.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100041441.1.2",
          "form": "아뇨 지금은 안 키우고 전에 햄스터 키웠어요!",
          "original_form": "아뇨 지금은 안 키우고 전에 햄스터 키웠어요!",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100041441.1.3",
          "form": "아하! 햄스터 귀엽죠~",
          "original_form": "아하! 햄스터 귀엽죠~",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100041441.1.4",
          "form": "햄스터 집 꾸미는 유튜브 영상 많이 봤어요",
          "original_form": "햄스터 집 꾸미는 유튜브 영상 많이 봤어요",
          "speaker_id": "2",
          "time": "20210905 23:57"
        },
        {
          "id": "MDRW2100041441.1.5",
          "form": "아 햄스터 집 꾸미는 거 너무 귀엽죠...ㅠㅠㅠㅋㅋㅋㅋㅋ 저도 가끔 힐링하고 싶을 때 봐요",
          "original_form": "아 햄스터 집 꾸미는 거 너무 귀엽죠...ㅠㅠㅠㅋㅋㅋㅋㅋ 저도 가끔 힐링하고 싶을 때 봐요",
          "speaker_id": "1",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100041441.1.6",
          "form": "ㅋㅋㅋ뽀짝한 햄스터들 돌아다니는것두 넘 기여웡용",
          "original_form": "ㅋㅋㅋ뽀짝한 햄스터들 돌아다니는것두 넘 기여웡용",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041441.1.7",
          "form": "맞아요ㅋㅋㅋㅋ name2 님은 키우고 있는 반려동물 있으신가요?",
          "original_form": "맞아요ㅋㅋㅋㅋ &name2& 님은 키우고 있는 반려동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041441.1.8",
          "form": "저도 옛날에 키워봤던건",
          "original_form": "저도 옛날에 키워봤던건",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100041441.1.9",
          "form": "거북이랑 기니피그 고슴도치 키워봤어요",
          "original_form": "거북이랑 기니피그 고슴도치 키워봤어요",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100041441.1.10",
          "form": "헐 거북이 너무 귀엽죠...",
          "original_form": "헐 거북이 너무 귀엽죠...",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100041441.1.11",
          "form": "거북이가 생각보다 빠른거 알고 계신가요...ㅋㅋㅋ",
          "original_form": "거북이가 생각보다 빠른거 알고 계신가요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100041441.1.12",
          "form": "전 거북이 키우면서 거북이가 빠르구나를 첨 알았어요",
          "original_form": "전 거북이 키우면서 거북이가 빠르구나를 첨 알았어요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100041441.1.13",
          "form": "엌ㅋㅋㅋㅋㅋ 거북이 은근 빠르더라구요",
          "original_form": "엌ㅋㅋㅋㅋㅋ 거북이 은근 빠르더라구요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100041441.1.14",
          "form": "ㅋㅋㅋㅋ아시는군요",
          "original_form": "ㅋㅋㅋㅋ아시는군요",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100041441.1.15",
          "form": "친구네가 거북이를 키웠는데 자주가서 봐가지고 알게 됐어요ㅋㅋ",
          "original_form": "친구네가 거북이를 키웠는데 자주가서 봐가지고 알게 됐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:46"
        }
      ]
