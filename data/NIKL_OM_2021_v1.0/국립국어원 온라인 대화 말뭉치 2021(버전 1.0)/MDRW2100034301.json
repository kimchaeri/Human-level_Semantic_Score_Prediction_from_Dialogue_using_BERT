      "utterance": [
        {
          "id": "MDRW2100034301.1.1",
          "form": "저 500개끝났어요",
          "original_form": "저 500개끝났어요",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034301.1.2",
          "form": "엊그제^^",
          "original_form": "엊그제^^",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034301.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034301.1.4",
          "form": "즐거웠어요... 진짜 열심히 하셨네요",
          "original_form": "즐거웠어요... 진짜 열심히 하셨네요",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034301.1.5",
          "form": "저는 첫날 100개 하자마자 질려서ㅋㅋㅋㅋ",
          "original_form": "저는 첫날 100개 하자마자 질려서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034301.1.6",
          "form": "아직도 130개예요",
          "original_form": "아직도 130개예요",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034301.1.7",
          "form": "분발하셔야겠어요ㅎ",
          "original_form": "분발하셔야겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034301.1.8",
          "form": "대화잘되는 분 몇명이랑  쭉하는게 낫더라구요",
          "original_form": "대화잘되는 분 몇명이랑  쭉하는게 낫더라구요",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100034301.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ아ㅜ 분발하겟어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아ㅜ 분발하겟어요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034301.1.10",
          "form": "그런 거 같아요 반절 이상은 연락이 다시 안 오고 이래서",
          "original_form": "그런 거 같아요 반절 이상은 연락이 다시 안 오고 이래서",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034301.1.11",
          "form": "빨리빨리 진행시키는 분들이랑 하심 금방하실수있을거예요",
          "original_form": "빨리빨리 진행시키는 분들이랑 하심 금방하실수있을거예요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034301.1.12",
          "form": "길게대화말고 업무적??으로",
          "original_form": "길게대화말고 업무적??으로",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034301.1.13",
          "form": "아 맞아요ㅋㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034301.1.14",
          "form": "근데 잘 맞는 분들이랑은",
          "original_form": "근데 잘 맞는 분들이랑은",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034301.1.15",
          "form": "이벤트 신경 안 쓰고",
          "original_form": "이벤트 신경 안 쓰고",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034301.1.16",
          "form": "단답형이 젤 좋더라구요",
          "original_form": "단답형이 젤 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034301.1.17",
          "form": "그건그래요ㅎ",
          "original_form": "그건그래요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034301.1.18",
          "form": "길게 대화하면서 재미 보는 거 같아요",
          "original_form": "길게 대화하면서 재미 보는 거 같아요",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034301.1.19",
          "form": "수다삼매경",
          "original_form": "수다삼매경",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034301.1.20",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034301.1.21",
          "form": "저도몇분그랬어요",
          "original_form": "저도몇분그랬어요",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034301.1.22",
          "form": "단답형이 실용적(?)이긴 하죠",
          "original_form": "단답형이 실용적(?)이긴 하죠",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034301.1.23",
          "form": "걍 동시간대 여러명하고하면 왔다갔다하면서",
          "original_form": "걍 동시간대 여러명하고하면 왔다갔다하면서",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100034301.1.24",
          "form": "할수있으니 날잡고 낼해보세요ㅎ",
          "original_form": "할수있으니 날잡고 낼해보세요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034301.1.25",
          "form": "ㅋㅋㅋㅋ알겠습니다!! 치킨이랑 아이스크림 맛있게 드세요",
          "original_form": "ㅋㅋㅋㅋ알겠습니다!! 치킨이랑 아이스크림 맛있게 드세요",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034301.1.26",
          "form": "전이미 7개다받고 네페5만원 하나 남았네요ㅎ",
          "original_form": "전이미 7개다받고 네페5만원 하나 남았네요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034301.1.27",
          "form": "와 오만원 대박이네요",
          "original_form": "와 오만원 대박이네요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034301.1.28",
          "form": "한방에끝낼려고했는데 50개남기고 하루일찍주는바람에",
          "original_form": "한방에끝낼려고했는데 50개남기고 하루일찍주는바람에",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034301.1.29",
          "form": "받은 건 쓰셨어요?",
          "original_form": "받은 건 쓰셨어요?",
