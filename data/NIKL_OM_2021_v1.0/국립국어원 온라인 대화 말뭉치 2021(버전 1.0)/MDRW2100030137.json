      "utterance": [
        {
          "id": "MDRW2100030137.1.1",
          "form": "까짓꺼 그냥 10년 더 입다가",
          "original_form": "까짓꺼 그냥 10년 더 입다가",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030137.1.2",
          "form": "빵꾸나면 그 때 취향 맞게 장만하지 뭐",
          "original_form": "빵꾸나면 그 때 취향 맞게 장만하지 뭐",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100030137.1.3",
          "form": "요즘 들은 늙어도 꾸역꾸역 캐주얼 입던데",
          "original_form": "요즘 {censored}들은 늙어도 꾸역꾸역 캐주얼 입던데",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030137.1.4",
          "form": "걔네보다 나은 나라고 못하랴 싶어",
          "original_form": "걔네보다 나은 나라고 못하랴 싶어",
          "speaker_id": "1",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030137.1.5",
          "form": "맞아 옷  유행따라가는게",
          "original_form": "맞아 옷  유행따라가는게",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030137.1.6",
          "form": "제일 부질없어",
          "original_form": "제일 부질없어",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030137.1.7",
          "form": "좋은 재질의 옷 한벌사서",
          "original_form": "좋은 재질의 옷 한벌사서",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030137.1.8",
          "form": "몇십년 입는게 더 중요해",
          "original_form": "몇십년 입는게 더 중요해",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030137.1.9",
          "form": "다행히 유행타는 옷은 거의 없다",
          "original_form": "다행히 유행타는 옷은 거의 없다",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030137.1.10",
          "form": "지이이인짜 베이직한 옷들 입는 편이었는데",
          "original_form": "지이이인짜 베이직한 옷들 입는 편이었는데",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030137.1.11",
          "form": "나도 밀라논나처럼 20년은 채울까봐",
          "original_form": "나도 밀라논나처럼 20년은 채울까봐",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030137.1.12",
          "form": "적고보니 베이직파였는데 뭘 걱정했나 싶네",
          "original_form": "적고보니 베이직파였는데 뭘 걱정했나 싶네",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030137.1.13",
          "form": "촴내",
          "original_form": "촴내",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030137.1.14",
          "form": "좋다좋다 나랑같이 옷한벌",
          "original_form": "좋다좋다 나랑같이 옷한벌",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030137.1.15",
          "form": "오래입기 같이하자",
          "original_form": "오래입기 같이하자",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030137.1.16",
          "form": "만원 2만원짜리 옷 100벌사는거",
          "original_form": "만원 2만원짜리 옷 100벌사는거",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100030137.1.17",
          "form": "하지말고",
          "original_form": "하지말고",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100030137.1.18",
          "form": "ㅋㅋㅋㅋ좋아좋아",
          "original_form": "ㅋㅋㅋㅋ좋아좋아",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100030137.1.19",
          "form": "20만원 사서 100년 입자",
          "original_form": "20만원 사서 100년 입자",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100030137.1.20",
          "form": "그러자 그러자",
          "original_form": "그러자 그러자",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030137.1.21",
          "form": "난 그래서 밀라논나가 와이셔츠 70년..?된거",
          "original_form": "난 그래서 밀라논나가 와이셔츠 70년..?된거",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030137.1.22",
          "form": "잘 관리해서 지금도 입길래",
          "original_form": "잘 관리해서 지금도 입길래",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030137.1.23",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030137.1.24",
          "form": "나도 좀 좋은 원단으로 만든 와이셔츠",
          "original_form": "나도 좀 좋은 원단으로 만든 와이셔츠",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030137.1.25",
          "form": "고심해서 비싸더라도 살까싶어",
          "original_form": "고심해서 비싸더라도 살까싶어",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030137.1.26",
          "form": "그것도 괜춘하다",
          "original_form": "그것도 괜춘하다",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030137.1.27",
          "form": "가격값 하는 원단인지 꼭꼭꼭 잘 살펴보고",
          "original_form": "가격값 하는 원단인지 꼭꼭꼭 잘 살펴보고",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030137.1.28",
          "form": "그래서 지금 눈에 불켜고",
          "original_form": "그래서 지금 눈에 불켜고",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030137.1.29",
          "form": "와이셔츠 찾는중",
          "original_form": "와이셔츠 찾는중",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030137.1.30",
          "form": "구김 어떤지도 꼭 봐봐",
          "original_form": "구김 어떤지도 꼭 봐봐",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030137.1.31",
          "form": "셔츠한장없는 사람",
          "original_form": "셔츠한장없는 사람",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030137.1.32",
          "form": "구김 심하면 빡쳐서 손이 잘 안 가",
          "original_form": "구김 심하면 빡쳐서 손이 잘 안 가",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030137.1.33",
          "form": "응 후기 많은걸로",
          "original_form": "응 후기 많은걸로",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030137.1.34",
          "form": "사려고",
          "original_form": "사려고",
          "speaker_id": "2",
          "time": "20210903 22:54"
        }
      ]
