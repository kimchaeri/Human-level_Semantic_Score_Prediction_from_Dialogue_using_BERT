      "utterance": [
        {
          "id": "MDRW2100021805.1.1",
          "form": "네 일단 막 줄 기다려서먹을 정도는 아닌데",
          "original_form": "네 일단 막 줄 기다려서먹을 정도는 아닌데",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021805.1.2",
          "form": "찾아가보려 노력은 해요 ㅋㅋㅋ",
          "original_form": "찾아가보려 노력은 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021805.1.3",
          "form": "어떤메뉴요?",
          "original_form": "어떤메뉴요?",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100021805.1.4",
          "form": "뭐 이런저런ㅋㅋㅋ 메뉴는 다양하게",
          "original_form": "뭐 이런저런ㅋㅋㅋ 메뉴는 다양하게",
          "speaker_id": "2",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100021805.1.5",
          "form": "저는 돈카츠조아애요",
          "original_form": "저는 돈카츠조아애요",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100021805.1.6",
          "form": "ㅋㅋㅋㅋ크레이지카츠 추천해주실거져",
          "original_form": "ㅋㅋㅋㅋ크레이지카츠 추천해주실거져",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100021805.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 치킨도조아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 치킨도조아해요",
          "speaker_id": "1",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100021805.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ치킨은 어디가 맛집이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ치킨은 어디가 맛집이에요",
          "speaker_id": "2",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100021805.1.9",
          "form": "치킨은 교촌 좋아해여",
          "original_form": "치킨은 교촌 좋아해여",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100021805.1.10",
          "form": "그쵸 허니콤보요?",
          "original_form": "그쵸 허니콤보요?",
          "speaker_id": "2",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100021805.1.11",
          "form": "허콤에 레드소스 국룰이죠ㅎㅎ",
          "original_form": "허콤에 레드소스 국룰이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100021805.1.12",
          "form": "마자여 허콤 조아요ㅋㅋㅋㅋ양이 적은게 최고의 단점이지만 ㅠㅠ",
          "original_form": "마자여 허콤 조아요ㅋㅋㅋㅋ양이 적은게 최고의 단점이지만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100021805.1.13",
          "form": "살짝 물리기도해요",
          "original_form": "살짝 물리기도해요",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100021805.1.14",
          "form": "마자요 넉넉하면 레콤이랑 허콤 같이 시키고싶아요",
          "original_form": "마자요 넉넉하면 레콤이랑 허콤 같이 시키고싶아요",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100021805.1.15",
          "form": "전 지금 신라면 볶음면 먹는데 매콤하니 맛잇네요",
          "original_form": "전 지금 신라면 볶음면 먹는데 매콤하니 맛잇네요",
