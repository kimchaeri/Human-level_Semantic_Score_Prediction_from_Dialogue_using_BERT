      "utterance": [
        {
          "id": "MDRW2100017540.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017540.1.2",
          "form": "티비 자주 보세요?",
          "original_form": "티비 자주 보세요?",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017540.1.3",
          "form": "티비는 아니구요",
          "original_form": "티비는 아니구요",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017540.1.4",
          "form": "유튭이나 넷플로 봐요",
          "original_form": "유튭이나 넷플로 봐요",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017540.1.5",
          "form": "아 넷플릭스 뭐 보세요?",
          "original_form": "아 넷플릭스 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017540.1.6",
          "form": "음 요즘음 체인지데이즈 돌싱글즈요 ㅋㅋㅋㅋ 미드도 보고",
          "original_form": "음 요즘음 체인지데이즈 돌싱글즈요 ㅋㅋㅋㅋ 미드도 보고",
          "speaker_id": "2",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100017540.1.7",
          "form": "오 ㅋㅋ 돌싱글즈가 그건가요? 커플 바꿔서 연애해보는?",
          "original_form": "오 ㅋㅋ 돌싱글즈가 그건가요? 커플 바꿔서 연애해보는?",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017540.1.8",
          "form": "ㅋㅋㅋ 그데 체인지데이즈고요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 그데 체인지데이즈고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017540.1.9",
          "form": "돌싱글즈는 ㅋㅋㅋㅋ 이혼남녀들 데이트프로에욬ㅋㅋ",
          "original_form": "돌싱글즈는 ㅋㅋㅋㅋ 이혼남녀들 데이트프로에욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017540.1.10",
          "form": "앗 거꾸로 알고 있었네요 ㅋㅋㅋ",
          "original_form": "앗 거꾸로 알고 있었네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017540.1.11",
          "form": "커플 박살 나는 프로인가요?",
          "original_form": "커플 박살 나는 프로인가요?",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017540.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 체인지데이즈...어케될지모르겟네요 기존커플 다 깨질지 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 체인지데이즈...어케될지모르겟네요 기존커플 다 깨질지 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017540.1.13",
          "form": "둘다 재밌어요! 추천ㅋㅋㅋ",
          "original_form": "둘다 재밌어요! 추천ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100017540.1.14",
          "form": "요즘 볼거 진짜 없는데 그거 봐야겠어요",
          "original_form": "요즘 볼거 진짜 없는데 그거 봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017540.1.15",
          "form": "리얼프로 느낌이에요 아니면 예능 느낌?",
          "original_form": "리얼프로 느낌이에요 아니면 예능 느낌?",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017540.1.16",
          "form": "ㅋㅋㅋㅋㅋ 약간 리얼",
          "original_form": "ㅋㅋㅋㅋㅋ 약간 리얼",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017540.1.17",
          "form": "걍 대충 켜놓고보기 좋은듯ㅋㅋㅋ",
          "original_form": "걍 대충 켜놓고보기 좋은듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100017540.1.18",
          "form": "요즘 나혼산이랑 그런 거 논란 많아서 신뢰가 떨어졌어요",
          "original_form": "요즘 나혼산이랑 그런 거 논란 많아서 신뢰가 떨어졌어요",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100017540.1.19",
          "form": "그런 진정성 논란만 없어도 진작 봤을텐데..",
          "original_form": "그런 진정성 논란만 없어도 진작 봤을텐데..",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100017540.1.20",
          "form": "아아 ㅋㅋㅋ저거 대본아냐? 하고 의심을 좀 하긴 했는데 그래도 찐모먼트인 부분이 많아서 ㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋ저거 대본아냐? 하고 의심을 좀 하긴 했는데 그래도 찐모먼트인 부분이 많아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100017540.1.21",
          "form": "저도 그런 의심 중인데...ㅋㅋㅋㅋ",
          "original_form": "저도 그런 의심 중인데...ㅋㅋㅋㅋ",
