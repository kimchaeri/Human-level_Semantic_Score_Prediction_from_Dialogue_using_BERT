      "utterance": [
        {
          "id": "MDRW2100015119.1.1",
          "form": "드라마",
          "original_form": "드라마",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.2",
          "form": "그 어느날 만난 하루였나",
          "original_form": "그 어느날 만난 하루였나",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.3",
          "form": "그 김혜윤 씨 나오는거 봤어요",
          "original_form": "그 김혜윤 씨 나오는거 봤어요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.4",
          "form": "와 너무 유치한데 김혜윤 씨 귀여워서 다봤어요",
          "original_form": "와 너무 유치한데 김혜윤 씨 귀여워서 다봤어요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.6",
          "form": "어하루봤구나",
          "original_form": "어하루봤구나",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.7",
          "form": "어쩌다 만난 하루 보셨나요",
          "original_form": "어쩌다 만난 하루 보셨나요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.8",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.9",
          "form": "그거 진짜 러블리하더라구요",
          "original_form": "그거 진짜 러블리하더라구요",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.10",
          "form": "혜윤이 너무 귀여워요",
          "original_form": "혜윤이 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015119.1.11",
          "form": "개연성은 없지만 해윤 보는 재미로 봤던",
          "original_form": "개연성은 없지만 해윤 보는 재미로 봤던",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015119.1.12",
          "form": "혜윤 씨",
          "original_form": "혜윤 씨",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015119.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015119.1.14",
          "form": "그거랑 또 뭐봤지",
          "original_form": "그거랑 또 뭐봤지",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015119.1.15",
          "form": "그 여름마다 너목들 보고 그럽니다",
          "original_form": "그 여름마다 너목들 보고 그럽니다",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015119.1.16",
          "form": "여름 국룰이죠",
          "original_form": "여름 국룰이죠",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015119.1.17",
          "form": "너목들",
          "original_form": "너목들",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015119.1.18",
          "form": "이종석 눈물 떨구며 키스하는 장면 정말 명장면이죠",
          "original_form": "이종석 눈물 떨구며 키스하는 장면 정말 명장면이죠",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015119.1.19",
          "form": "민준국의 죽일거다..",
          "original_form": "민준국의 죽일거다..",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015119.1.20",
          "form": "를 생각한 나 쓰레기인가요",
          "original_form": "를 생각한 나 쓰레기인가요",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015119.1.21",
          "form": "ㄹㅇ 그게ㅜ어케 1화일까요",
          "original_form": "ㄹㅇ 그게ㅜ어케 1화일까요",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015119.1.22",
          "form": "앗 그렇게 따지면 제가 더 쓰레기 같..",
          "original_form": "앗 그렇게 따지면 제가 더 쓰레기 같..",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015119.1.23",
          "form": "ㅋㅋㅋㅋ 다행읻ㆍ",
          "original_form": "ㅋㅋㅋㅋ 다행읻ㆍ",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015119.1.24",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015119.1.25",
          "form": "혼자보다 같이 썩은게 낫죠",
          "original_form": "혼자보다 같이 썩은게 낫죠",
