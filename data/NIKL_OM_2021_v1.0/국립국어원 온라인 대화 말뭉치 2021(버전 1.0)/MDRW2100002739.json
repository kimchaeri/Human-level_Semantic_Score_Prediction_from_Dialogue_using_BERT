      "utterance": [
        {
          "id": "MDRW2100002739.1.1",
          "form": "이제 가을이 오려나봐요",
          "original_form": "이제 가을이 오려나봐요",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002739.1.2",
          "form": "정말요 ㅎ 하늘이 참 예뻐요",
          "original_form": "정말요 ㅎ 하늘이 참 예뻐요",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002739.1.3",
          "form": "네~~아침 저녁은 약간 쌀쌀하더라고요",
          "original_form": "네~~아침 저녁은 약간 쌀쌀하더라고요",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002739.1.4",
          "form": "여름이 끝이 없을것같았는데 가을이 오는군요",
          "original_form": "여름이 끝이 없을것같았는데 가을이 오는군요",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002739.1.5",
          "form": "그니까요~~갑자기 훅 날씨가 바뀐것 같아요",
          "original_form": "그니까요~~갑자기 훅 날씨가 바뀐것 같아요",
          "speaker_id": "1",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002739.1.6",
          "form": "절기에 관심이 없었는데 입추지나니 날씨가 바뀌네요",
          "original_form": "절기에 관심이 없었는데 입추지나니 날씨가 바뀌네요",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002739.1.7",
          "form": "오~그렇네요 입추가 지나서 그런가봐요 ㅎ",
          "original_form": "오~그렇네요 입추가 지나서 그런가봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002739.1.8",
          "form": "ㅎㅎ 벌써 저는 겨울이 걱정입니다 ㅋ 추위를 많이 타서요",
          "original_form": "ㅎㅎ 벌써 저는 겨울이 걱정입니다 ㅋ 추위를 많이 타서요",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002739.1.9",
          "form": "저도요ㅠㅠ 난방비 걱정이 벌써 드네요",
          "original_form": "저도요ㅠㅠ 난방비 걱정이 벌써 드네요",
          "speaker_id": "1",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002739.1.10",
          "form": "난방비.. 맞아요 ㅠ 오늘은 반팔로 출근하셨나요?",
          "original_form": "난방비.. 맞아요 ㅠ 오늘은 반팔로 출근하셨나요?",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002739.1.11",
          "form": "네~위에는 반팔 아래는 긴바지 좀 얇은거요",
          "original_form": "네~위에는 반팔 아래는 긴바지 좀 얇은거요",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002739.1.12",
          "form": "아~^^ 다음주에는 우산 챙겨다니세요\r\n비예보가 있네요",
          "original_form": "아~^^ 다음주에는 우산 챙겨다니세요\r\n비예보가 있네요",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002739.1.13",
          "form": "네~ 오늘도 날씨가 흐려요",
          "original_form": "네~ 오늘도 날씨가 흐려요",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002739.1.14",
          "form": "아하~ 흐리군요",
          "original_form": "아하~ 흐리군요",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002739.1.15",
          "form": "네~왠지 비올것 같아요 거기는요?",
          "original_form": "네~왠지 비올것 같아요 거기는요?",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002739.1.16",
          "form": "여기는 괜찮아요 ㅎㅎ 맑아요",
          "original_form": "여기는 괜찮아요 ㅎㅎ 맑아요",
