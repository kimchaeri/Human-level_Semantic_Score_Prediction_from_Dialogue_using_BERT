      "utterance": [
        {
          "id": "MDRW2100033865.1.1",
          "form": "어머 집안일 하며 어찌보셨어요",
          "original_form": "어머 집안일 하며 어찌보셨어요",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033865.1.2",
          "form": "멀티가 다되시고 대단하세요 항시 놀라요",
          "original_form": "멀티가 다되시고 대단하세요 항시 놀라요",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033865.1.3",
          "form": "어제 저녁반찬하면서",
          "original_form": "어제 저녁반찬하면서",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033865.1.4",
          "form": "이어폰끼고 풀장착후 보셨죠 ㅎㅎ",
          "original_form": "이어폰끼고 풀장착후 보셨죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033865.1.5",
          "form": "폰옆에 가져다놓고 ㅋㄱ후딱ㅋ",
          "original_form": "폰옆에 가져다놓고 ㅋㄱ후딱ㅋ",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033865.1.6",
          "form": "넹ㅋ",
          "original_form": "넹ㅋ",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033865.1.7",
          "form": "저도 그래야 하는데 전 2개를 못해서",
          "original_form": "저도 그래야 하는데 전 2개를 못해서",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033865.1.8",
          "form": "네 진심 놀랍니다",
          "original_form": "네 진심 놀랍니다",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033865.1.9",
          "form": "주부9단 입니다",
          "original_form": "주부9단 입니다",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033865.1.10",
          "form": "저도 자막읽는거까진 잘안되서ㅠㅋㅋ",
          "original_form": "저도 자막읽는거까진 잘안되서ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033865.1.11",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033865.1.12",
          "form": "한국 드라마죠 ㅎㅎ",
          "original_form": "한국 드라마죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033865.1.13",
          "form": "다시앞에땡기고 땡기고 보면서 밥차리고 했었죠ㅋ",
          "original_form": "다시앞에땡기고 땡기고 보면서 밥차리고 했었죠ㅋ",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033865.1.14",
          "form": "집안일할땐 자막 불가능이죠",
          "original_form": "집안일할땐 자막 불가능이죠",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033865.1.15",
          "form": "네 와 밥은 뭐드셨어요?",
          "original_form": "네 와 밥은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100033865.1.16",
          "form": "오늘은 비가와서 수제비, 칼국수 먹으로 갔다왔어요~~",
          "original_form": "오늘은 비가와서 수제비, 칼국수 먹으로 갔다왔어요~~",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033865.1.17",
          "form": "name2님은 식사하셨나요?!",
          "original_form": "&name2&님은 식사하셨나요?!",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033865.1.18",
          "form": "와 잘하셨어요",
          "original_form": "와 잘하셨어요",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033865.1.19",
          "form": "아뇨 오늘 공모주 하는날이라",
          "original_form": "아뇨 오늘 공모주 하는날이라",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033865.1.20",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033865.1.21",
          "form": "계속 바빠서 굶었어요",
          "original_form": "계속 바빠서 굶었어요",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100033865.1.22",
          "form": "점심으로 샌드위치 먹으려고요",
          "original_form": "점심으로 샌드위치 먹으려고요",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100033865.1.23",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100033865.1.24",
          "form": "피곤할땐 이따 아이도 케어하셔야 하니 꼭 드셔야해요",
          "original_form": "피곤할땐 이따 아이도 케어하셔야 하니 꼭 드셔야해요",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100033865.1.25",
          "form": "청소 빨해만 하고 암것도 못했네요",
          "original_form": "청소 빨해만 하고 암것도 못했네요",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100033865.1.26",
          "form": "저도 2시쯤 동태를 살피고 어디로 청약넣을까 생각중이예요",
          "original_form": "저도 2시쯤 동태를 살피고 어디로 청약넣을까 생각중이예요",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100033865.1.27",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100033865.1.28",
          "form": "아직 안하셨군요",
          "original_form": "아직 안하셨군요",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100033865.1.29",
          "form": "그래도 부지런하시네요 ..!\r\n청소하고 빨래까지",
          "original_form": "그래도 부지런하시네요 ..!\r\n청소하고 빨래까지",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100033865.1.30",
          "form": "이게 집안일에 주식에 힘드네요",
          "original_form": "이게 집안일에 주식에 힘드네요",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100033865.1.31",
          "form": "아니예요",
          "original_form": "아니예요",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100033865.1.32",
          "form": "오늘 정신이 혼미해요 ㅋㅋ",
          "original_form": "오늘 정신이 혼미해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100033865.1.33",
          "form": "몇일전에 코스닥 코스피 너무 떨어져서...",
