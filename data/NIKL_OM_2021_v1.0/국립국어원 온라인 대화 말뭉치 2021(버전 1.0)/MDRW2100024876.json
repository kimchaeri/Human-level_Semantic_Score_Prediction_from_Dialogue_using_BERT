      "utterance": [
        {
          "id": "MDRW2100024876.1.1",
          "form": "혹시 직장인이신가요?",
          "original_form": "혹시 직장인이신가요?",
          "speaker_id": "1",
          "time": "20210831 20:08"
        },
        {
          "id": "MDRW2100024876.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024876.1.3",
          "form": "아르바이트를 해요",
          "original_form": "아르바이트를 해요",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024876.1.4",
          "form": "아르바이트는 재미있으신가요?",
          "original_form": "아르바이트는 재미있으신가요?",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100024876.1.5",
          "form": "아르바이트는 매일 안가고 주3일만 가서 갈만해요",
          "original_form": "아르바이트는 매일 안가고 주3일만 가서 갈만해요",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100024876.1.6",
          "form": "적당히 일하는건 나이와 상관없이 괜잔은것 같아요",
          "original_form": "적당히 일하는건 나이와 상관없이 괜잔은것 같아요",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100024876.1.7",
          "form": "맞아요!\r\n돈도 벌고 할일이 있는건 좋은것 같아요ㅎㅎ",
          "original_form": "맞아요!\r\n돈도 벌고 할일이 있는건 좋은것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 09:27"
        },
        {
          "id": "MDRW2100024876.1.8",
          "form": "일하세요?",
          "original_form": "일하세요?",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100024876.1.9",
          "form": "네ㅎ 과외해요ㅎㅎ",
          "original_form": "네ㅎ 과외해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100024876.1.10",
          "form": "과목은요?",
          "original_form": "과목은요?",
          "speaker_id": "2",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100024876.1.11",
          "form": "초중 수학 국어 과학이요ㅎ",
          "original_form": "초중 수학 국어 과학이요ㅎ",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100024876.1.12",
          "form": "와! 능력있는 선생님이시네요",
          "original_form": "와! 능력있는 선생님이시네요",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100024876.1.13",
          "form": "직장에 묶여 있지않으니 스트레스는 덜하신가요?",
          "original_form": "직장에 묶여 있지않으니 스트레스는 덜하신가요?",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100024876.1.14",
          "form": "소수라 별 스트레스는  안받이요ㅎㅎㅎ",
          "original_form": "소수라 별 스트레스는  안받이요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100024876.1.15",
          "form": "이직할 필요도 없고 좋으시겠어요",
          "original_form": "이직할 필요도 없고 좋으시겠어요",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100024876.1.16",
          "form": "근데 많이 벌지는 못해서ㅜㅜ",
          "original_form": "근데 많이 벌지는 못해서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 14:51"
        }
      ]
