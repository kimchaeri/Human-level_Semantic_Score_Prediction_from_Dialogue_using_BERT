        },
        {
          "id": "MDRW2100029544.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029544.1.3",
          "form": "책 읽는거 좋아하세요?",
          "original_form": "책 읽는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029544.1.4",
          "form": "좋아합니다ㅎㅎ",
          "original_form": "좋아합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029544.1.5",
          "form": "최근에 어떤 책 읽으셨어요?",
          "original_form": "최근에 어떤 책 읽으셨어요?",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029544.1.6",
          "form": "하고 싶은 일하면서 살면 왜 안돼요?",
          "original_form": "하고 싶은 일하면서 살면 왜 안돼요?",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029544.1.7",
          "form": "라는 에세이 책을 읽었어요",
          "original_form": "라는 에세이 책을 읽었어요",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029544.1.8",
          "form": "ㅋㄴ님은 어떤 책을 읽으셨나요",
          "original_form": "ㅋㄴ님은 어떤 책을 읽으셨나요",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029544.1.9",
          "form": "저는 신세계에서라는 소설 읽었어요",
          "original_form": "저는 신세계에서라는 소설 읽었어요",
          "speaker_id": "1",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100029544.1.10",
          "form": "오오 소설을 좋아하시나봐요",
          "original_form": "오오 소설을 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100029544.1.11",
          "form": "어떤 장르인가요?",
          "original_form": "어떤 장르인가요?",
          "speaker_id": "2",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100029544.1.12",
          "form": "판타지에요",
          "original_form": "판타지에요",
          "speaker_id": "1",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029544.1.13",
          "form": "이세계물인가요?",
          "original_form": "이세계물인가요?",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029544.1.14",
          "form": "이세계라고 하기에는 좀 애매한데... 초능력을 다루는 사람들 얘기에요",
          "original_form": "이세계라고 하기에는 좀 애매한데... 초능력을 다루는 사람들 얘기에요",
          "speaker_id": "1",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029544.1.15",
          "form": "오오 초능력도 좋죠",
          "original_form": "오오 초능력도 좋죠",
          "speaker_id": "2",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029544.1.16",
          "form": "몰입도 쉽게할수있구",
          "original_form": "몰입도 쉽게할수있구",
          "speaker_id": "2",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100029544.1.17",
          "form": "초반에는 떡밥 까느라 좀 지루했는데",
          "original_form": "초반에는 떡밥 까느라 좀 지루했는데",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029544.1.18",
          "form": "후반으로 가면 갈수록 진국입니다",
          "original_form": "후반으로 가면 갈수록 진국입니다",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029544.1.19",
          "form": "고된 시간을 버텨내셨네요ㅎㅎ",
          "original_form": "고된 시간을 버텨내셨네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029544.1.20",
          "form": "속독하는 편이신가요?",
          "original_form": "속독하는 편이신가요?",
          "speaker_id": "2",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029544.1.21",
          "form": "네 주변에서 읽는 속도 빠르다는 말 자주 들었었어요",
          "original_form": "네 주변에서 읽는 속도 빠르다는 말 자주 들었었어요",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029544.1.22",
          "form": "부럽네요ㅎㅎ 저는 많이 느려서 스스로도 답답하거든요",
          "original_form": "부럽네요ㅎㅎ 저는 많이 느려서 스스로도 답답하거든요",
          "speaker_id": "2",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029544.1.23",
          "form": "그런 분들도 많더라구요",
          "original_form": "그런 분들도 많더라구요",
          "speaker_id": "1",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100029544.1.24",
          "form": "타고난거 같아요 그런건",
          "original_form": "타고난거 같아요 그런건",
          "speaker_id": "1",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100029544.1.25",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
