      "utterance": [
        {
          "id": "MDRW2100007396.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007396.1.2",
          "form": "졸업한지 꽤 됐어요 ㅋㅋ",
          "original_form": "졸업한지 꽤 됐어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007396.1.3",
          "form": "학교 생활..",
          "original_form": "학교 생활..",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007396.1.4",
          "form": "동아리 활동 하셨나요?",
          "original_form": "동아리 활동 하셨나요?",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007396.1.5",
          "form": "네 특별히 활동적인 건 아니지만",
          "original_form": "네 특별히 활동적인 건 아니지만",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007396.1.6",
          "form": "교육봉사같은 거 했었어요",
          "original_form": "교육봉사같은 거 했었어요",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007396.1.7",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007396.1.8",
          "form": "저도 해보고 싶은 생각은 있었는데",
          "original_form": "저도 해보고 싶은 생각은 있었는데",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007396.1.9",
          "form": "실행까지 못 이어졌네요",
          "original_form": "실행까지 못 이어졌네요",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007396.1.10",
          "form": "저는 좀 활동적인 동아리도 해보고 싶었는데",
          "original_form": "저는 좀 활동적인 동아리도 해보고 싶었는데",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007396.1.11",
          "form": "산악동아리 들어갔는데 술만 마시더라구요ㅋㅋ",
          "original_form": "산악동아리 들어갔는데 술만 마시더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007396.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007396.1.13",
          "form": "산악동아리면",
          "original_form": "산악동아리면",
          "speaker_id": "2",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007396.1.14",
          "form": "술 엄청 잘 먹을 것 같아요",
          "original_form": "술 엄청 잘 먹을 것 같아요",
          "speaker_id": "2",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007396.1.15",
          "form": "다들 ㅋㅋㅋ",
          "original_form": "다들 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007396.1.16",
          "form": "맞아요 근데 저는 못 마셔서 힘들었어요ㅋㅋㅋ",
          "original_form": "맞아요 근데 저는 못 마셔서 힘들었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007396.1.17",
          "form": "동아리 하신 거나 해보고 싶으셨던 거 있으세요?",
          "original_form": "동아리 하신 거나 해보고 싶으셨던 거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007396.1.18",
          "form": "저는 노래부르는 동아리였는데",
          "original_form": "저는 노래부르는 동아리였는데",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007396.1.19",
          "form": "시초는 민중가요 노래패라 ㅋㅋㅋㅋ",
          "original_form": "시초는 민중가요 노래패라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007396.1.20",
          "form": "민중가요랑 대중가요 둘 다 했어요 ㅋㅋㅋ",
          "original_form": "민중가요랑 대중가요 둘 다 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007396.1.21",
          "form": "와 되게 멋있다",
          "original_form": "와 되게 멋있다",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007396.1.22",
          "form": "노래도 잘하시겠네요",
          "original_form": "노래도 잘하시겠네요",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007396.1.23",
          "form": "그냥 대학교 가면 꼭 노래부르는 동아리에 들어가야겠다!! 생각했어서... ㅋㅋㅋ",
          "original_form": "그냥 대학교 가면 꼭 노래부르는 동아리에 들어가야겠다!! 생각했어서... ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007396.1.24",
          "form": "교육봉사는 어린 학생들 대상이었어요??",
          "original_form": "교육봉사는 어린 학생들 대상이었어요??",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007396.1.25",
          "form": "사랑도 명예도 이름도 남김없이 한평생 나가자던 뜨거운 맹세~",
          "original_form": "사랑도 명예도 이름도 남김없이 한평생 나가자던 뜨거운 맹세~",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007396.1.26",
          "form": "어르신들 야학이랑 중고등학생 멘토링요",
          "original_form": "어르신들 야학이랑 중고등학생 멘토링요",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007396.1.27",
          "form": "와 많이 하셨네요",
          "original_form": "와 많이 하셨네요",
          "speaker_id": "2",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007396.1.28",
          "form": "멋지세요",
          "original_form": "멋지세요",
          "speaker_id": "2",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007396.1.29",
          "form": "야학봉사하다가 알게된 언니가 멘토링 프로그램 소개해줘서 그것도 하게된거예요ㅎㅎ",
          "original_form": "야학봉사하다가 알게된 언니가 멘토링 프로그램 소개해줘서 그것도 하게된거예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:38"
        }
      ]
