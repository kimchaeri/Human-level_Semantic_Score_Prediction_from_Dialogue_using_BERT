      "utterance": [
        {
          "id": "MDRW2100026853.1.1",
          "form": "날씨가 좋네요...",
          "original_form": "날씨가 좋네요...",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026853.1.2",
          "form": "그러게요....매미 우는 소리가",
          "original_form": "그러게요....매미 우는 소리가",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026853.1.3",
          "form": "저희는 아까 멈췄어요",
          "original_form": "저희는 아까 멈췄어요",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026853.1.4",
          "form": "아직도 들리긴 하지만요",
          "original_form": "아직도 들리긴 하지만요",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026853.1.5",
          "form": "모고보러가는데 비 왕창내려서",
          "original_form": "모고보러가는데 비 왕창내려서",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026853.1.6",
          "form": "곤란했달지~~~~",
          "original_form": "곤란했달지~~~~",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.7",
          "form": "으아 교복 젖었나여",
          "original_form": "으아 교복 젖었나여",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.8",
          "form": "저는 아까 학식 먹으러 가는데",
          "original_form": "저는 아까 학식 먹으러 가는데",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.9",
          "form": "비가 갑자기 왕창 쏟아져서",
          "original_form": "비가 갑자기 왕창 쏟아져서",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.10",
          "form": "교복은 괜찮았는데 양말이 젖었어요...",
          "original_form": "교복은 괜찮았는데 양말이 젖었어요...",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.11",
          "form": "비가 와장창",
          "original_form": "비가 와장창",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.12",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.13",
          "form": "신발이 다 젖어버리는 바람에",
          "original_form": "신발이 다 젖어버리는 바람에",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.14",
          "form": "건조기에 처박아뒀어요",
          "original_form": "건조기에 처박아뒀어요",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.15",
          "form": "뽀송뽀송해졌달지",
          "original_form": "뽀송뽀송해졌달지",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.16",
          "form": "과격하시네요",
          "original_form": "과격하시네요",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.17",
          "form": "그래도 요즘 정말",
          "original_form": "그래도 요즘 정말",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.18",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.19",
          "form": "가을이라는게 체감되는 것 같아요",
          "original_form": "가을이라는게 체감되는 것 같아요",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.20",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026853.1.21",
          "form": "맞아요 밤이 시원하니깐요",
          "original_form": "맞아요 밤이 시원하니깐요",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026853.1.22",
          "form": "아까 학교돌아오는데 비내려서 그런가",
          "original_form": "아까 학교돌아오는데 비내려서 그런가",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026853.1.23",
          "form": "좀 추웠음",
          "original_form": "좀 추웠음",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026853.1.24",
          "form": "에어컨 틀면 이젠 춥더라그여",
          "original_form": "에어컨 틀면 이젠 춥더라그여",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026853.1.25",
          "form": "에어컨 틀고 샤워했다가",
          "original_form": "에어컨 틀고 샤워했다가",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026853.1.26",
          "form": "얼어죽는줄",
          "original_form": "얼어죽는줄",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026853.1.27",
          "form": "아니 아직도 에어컨 트는 사람이 있다니",
          "original_form": "아니 아직도 에어컨 트는 사람이 있다니",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026853.1.28",
          "form": "그게 더 신기한데요",
          "original_form": "그게 더 신기한데요",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026853.1.29",
          "form": "저희반애들 담요가지고 다님",
          "original_form": "저희반애들 담요가지고 다님",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026853.1.30",
          "form": "?왜요 낮엔 더운디",
          "original_form": "?왜요 낮엔 더운디",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026853.1.31",
          "form": "요즘 그렇게 안덥지 않아요??",
          "original_form": "요즘 그렇게 안덥지 않아요??",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026853.1.32",
          "form": "헐~~ 학교에서 에어컨 안 틀어줘요?",
          "original_form": "헐~~ 학교에서 에어컨 안 틀어줘요?",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026853.1.33",
          "form": "애들 춥다고 하던데 요즘",
          "original_form": "애들 춥다고 하던데 요즘",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026853.1.34",
          "form": "에어컨을 틀기에는 ..  좀 추운편이죠",
          "original_form": "에어컨을 틀기에는 ..  좀 추운편이죠",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026853.1.35",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "1",
          "time": "20210902 00:44"
        }
      ]
