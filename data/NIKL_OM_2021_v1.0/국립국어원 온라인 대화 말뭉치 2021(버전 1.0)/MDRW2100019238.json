      "utterance": [
        {
          "id": "MDRW2100019238.1.1",
          "form": "코인 투자 하세요?",
          "original_form": "코인 투자 하세요?",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019238.1.2",
          "form": "주식은해여",
          "original_form": "주식은해여",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019238.1.3",
          "form": "결혼은 꼭해야 할까요?",
          "original_form": "결혼은 꼭해야 할까요?",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019238.1.4",
          "form": "ㅡ음 결혼",
          "original_form": "ㅡ음 결혼",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019238.1.5",
          "form": "필수는아니라고보아여",
          "original_form": "필수는아니라고보아여",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019238.1.6",
          "form": "저도 그렇게 생각해요",
          "original_form": "저도 그렇게 생각해요",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019238.1.7",
          "form": "아이를 계획하지 않으면 굳이",
          "original_form": "아이를 계획하지 않으면 굳이",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019238.1.8",
          "form": "결혼할 필요없다고 생각해요",
          "original_form": "결혼할 필요없다고 생각해요",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019238.1.9",
          "form": "그렇져",
          "original_form": "그렇져",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019238.1.10",
          "form": "아이계획니 없더라도",
          "original_form": "아이계획니 없더라도",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019238.1.11",
          "form": "결혼은 선택이니",
          "original_form": "결혼은 선택이니",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019238.1.12",
          "form": "미래에도 결혼제도는 유지될까요?",
          "original_form": "미래에도 결혼제도는 유지될까요?",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019238.1.13",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019238.1.14",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019238.1.15",
          "form": "저는 사라질수도 있다고 생각해요",
          "original_form": "저는 사라질수도 있다고 생각해요",
          "speaker_id": "1",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019238.1.16",
          "form": "음 저는 과거에도 그렇고 지금도 그렇고",
          "original_form": "음 저는 과거에도 그렇고 지금도 그렇고",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019238.1.17",
          "form": "결혼이라는것이 한 가정과가정일수도있지만",
          "original_form": "결혼이라는것이 한 가정과가정일수도있지만",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019238.1.18",
          "form": "크게보면 국가 혹은 회사단위에서도 이루어지고있기에",
          "original_form": "크게보면 국가 혹은 회사단위에서도 이루어지고있기에",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019238.1.19",
          "form": "사라지진않을거같아여",
          "original_form": "사라지진않을거같아여",
          "speaker_id": "2",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019238.1.20",
          "form": "프랑스는 결혼하지 않고 동거 상태에서 출생한 자녀에게도",
          "original_form": "프랑스는 결혼하지 않고 동거 상태에서 출생한 자녀에게도",
          "speaker_id": "1",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019238.1.21",
          "form": "결혼한 가정에서 출생한 자녀와 동일한 법적 혜택을 누릴 수  있게 한대요",
          "original_form": "결혼한 가정에서 출생한 자녀와 동일한 법적 혜택을 누릴 수  있게 한대요",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019238.1.22",
          "form": "물론 국가마더 다르지요",
          "original_form": "물론 국가마더 다르지요",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019238.1.23",
          "form": "하지만 아직까지는 유교적관념이 남아있고",
          "original_form": "하지만 아직까지는 유교적관념이 남아있고",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019238.1.24",
          "form": "유교의 영향을 받은 국가에서는",
          "original_form": "유교의 영향을 받은 국가에서는",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019238.1.25",
          "form": "결혼이란게 의미를 가진다고 생각해여 조는",
          "original_form": "결혼이란게 의미를 가진다고 생각해여 조는",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019238.1.26",
          "form": "그렇구나 재미있는 주제 같아요 결혼은",
          "original_form": "그렇구나 재미있는 주제 같아요 결혼은",
          "speaker_id": "1",
          "time": "20210827 16:42"
        }
      ]
