      "utterance": [
        {
          "id": "MDRW2100037555.1.1",
          "form": "제가 그 전엔 장거리 연애를 엄청 오래해서",
          "original_form": "제가 그 전엔 장거리 연애를 엄청 오래해서",
          "speaker_id": "2",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100037555.1.2",
          "form": "한달에 한두번 정도를 6년만났었거든요",
          "original_form": "한달에 한두번 정도를 6년만났었거든요",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100037555.1.3",
          "form": "ㅋㅋㅋㅋㅋ너무 힘든 연애였어요",
          "original_form": "ㅋㅋㅋㅋㅋ너무 힘든 연애였어요",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100037555.1.4",
          "form": "어머... 장거리진짜 .....",
          "original_form": "어머... 장거리진짜 .....",
          "speaker_id": "1",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100037555.1.5",
          "form": "한달에 두번은 너무했다ㅜㅜㅜ",
          "original_form": "한달에 두번은 너무했다ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100037555.1.6",
          "form": "적어도 주1회ㅜㅜ",
          "original_form": "적어도 주1회ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100037555.1.7",
          "form": "그쵸 주1회는 만나야 연애 느낌들어요",
          "original_form": "그쵸 주1회는 만나야 연애 느낌들어요",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037555.1.8",
          "form": "한달1회는 그냥 뭐ㅋㅋㅋ....",
          "original_form": "한달1회는 그냥 뭐ㅋㅋㅋ....",
          "speaker_id": "2",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100037555.1.9",
          "form": "주변 보니까",
          "original_form": "주변 보니까",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100037555.1.10",
          "form": "긴 연애 후에 하는",
          "original_form": "긴 연애 후에 하는",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100037555.1.11",
          "form": "연애는 되게 잘 지내더라구요!",
          "original_form": "연애는 되게 잘 지내더라구요!",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100037555.1.12",
          "form": "그러게요ㅋㅋㅋ 지금은 아직 초반이라 잘 몰라도",
          "original_form": "그러게요ㅋㅋㅋ 지금은 아직 초반이라 잘 몰라도",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100037555.1.13",
          "form": "엄청 착한 사람이라서 든든하고 좋아용",
          "original_form": "엄청 착한 사람이라서 든든하고 좋아용",
          "speaker_id": "2",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100037555.1.14",
          "form": "ㅎㅎㅎㅎ제가 다 기분 좋네요",
          "original_form": "ㅎㅎㅎㅎ제가 다 기분 좋네요",
          "speaker_id": "1",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100037555.1.15",
          "form": "알콩달콩",
          "original_form": "알콩달콩",
          "speaker_id": "1",
          "time": "20210901 16:06"
        },
        {
          "id": "MDRW2100037555.1.16",
          "form": "방금 데이트를 마치고 돌아왔습니다 ㅎㅎ",
          "original_form": "방금 데이트를 마치고 돌아왔습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100037555.1.17",
          "form": "오호~~~ 오늘은 어떤데이트 하셨어용",
          "original_form": "오호~~~ 오늘은 어떤데이트 하셨어용",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100037555.1.18",
          "form": "연애세포가 살아나는 기분",
          "original_form": "연애세포가 살아나는 기분",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100037555.1.19",
          "form": "오늘은 오락실에 갔어요",
          "original_form": "오늘은 오락실에 갔어요",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100037555.1.20",
          "form": "농구하고 사격하고 양궁하고 다트 던지고 ㅋㅋ",
          "original_form": "농구하고 사격하고 양궁하고 다트 던지고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100037555.1.21",
          "form": "ㅋㅋㅋㅋ부럽따 부러워~~~",
          "original_form": "ㅋㅋㅋㅋ부럽따 부러워~~~",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100037555.1.22",
          "form": "남친분은 근처 살아요?",
          "original_form": "남친분은 근처 살아요?",
