      "utterance": [
        {
          "id": "MDRW2100040279.1.1",
          "form": "그런가요?ㅎㅎ",
          "original_form": "그런가요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040279.1.2",
          "form": "ㅋㅋㅋ같이 뭐 보러 많이 다니셨겠네요",
          "original_form": "ㅋㅋㅋ같이 뭐 보러 많이 다니셨겠네요",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040279.1.3",
          "form": "네 데이트하면서 전시회나 공연장 종종 갔죠",
          "original_form": "네 데이트하면서 전시회나 공연장 종종 갔죠",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040279.1.4",
          "form": "어떤 게 젤 기억에 남으시나요",
          "original_form": "어떤 게 젤 기억에 남으시나요",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040279.1.5",
          "form": "다 좋았는데",
          "original_form": "다 좋았는데",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040279.1.6",
          "form": "예술의전당에서 한 피카소 전시회가",
          "original_form": "예술의전당에서 한 피카소 전시회가",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040279.1.7",
          "form": "1시간 넘게 기다린 기억 때문에 강렬하게 남았어요",
          "original_form": "1시간 넘게 기다린 기억 때문에 강렬하게 남았어요",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040279.1.8",
          "form": "앜ㅋ 엄청 사람 많았나보네요",
          "original_form": "앜ㅋ 엄청 사람 많았나보네요",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040279.1.9",
          "form": "여름에 보러 서울 가고 싶었는데 못가서 아쉬워요",
          "original_form": "여름에 보러 서울 가고 싶었는데 못가서 아쉬워요",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040279.1.10",
          "form": "진짜 사람이 많았어요",
          "original_form": "진짜 사람이 많았어요",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040279.1.11",
          "form": "애들 데리고 온 가족단위 관람객도 많고",
          "original_form": "애들 데리고 온 가족단위 관람객도 많고",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100040279.1.12",
          "form": "아 역시 엄청 인기가 많았군여...",
          "original_form": "아 역시 엄청 인기가 많았군여...",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040279.1.13",
          "form": "좀 더 연장해줬으면 했는데 ㅠㅜ",
          "original_form": "좀 더 연장해줬으면 했는데 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040279.1.14",
          "form": "다음에 기회가 있겠죠",
          "original_form": "다음에 기회가 있겠죠",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040279.1.15",
          "form": "코로나+비오는 날씨 때문에 사람들이 몰려서",
          "original_form": "코로나+비오는 날씨 때문에 사람들이 몰려서",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040279.1.16",
          "form": "이번엔 너무 전시회 대기시간이 길었어요",
          "original_form": "이번엔 너무 전시회 대기시간이 길었어요",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040279.1.17",
          "form": "아 입장 관객들도 제한을 했겠네요",
          "original_form": "아 입장 관객들도 제한을 했겠네요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040279.1.18",
          "form": "네 맞아요 전시회 입장 관객수도 제한했어요",
          "original_form": "네 맞아요 전시회 입장 관객수도 제한했어요",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040279.1.19",
          "form": "그래서 전시회장 안에서는 붐비지 않았던 건 좋았습니다",
          "original_form": "그래서 전시회장 안에서는 붐비지 않았던 건 좋았습니다",
          "speaker_id": "1",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100040279.1.20",
          "form": "오 그 한국전쟁 그림 보고싶었는데",
          "original_form": "오 그 한국전쟁 그림 보고싶었는데",
          "speaker_id": "2",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100040279.1.21",
          "form": "볼거 많았죠?",
          "original_form": "볼거 많았죠?",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100040279.1.22",
          "form": "네 유명한 작품들이 많았어요",
          "original_form": "네 유명한 작품들이 많았어요",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100040279.1.23",
          "form": "옛날엔 피카소 그림 잘 이해도 안되고 그랬는데",
          "original_form": "옛날엔 피카소 그림 잘 이해도 안되고 그랬는데",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100040279.1.24",
          "form": "지금 보면 와 악마의 재능이었네 싶고 그래요",
          "original_form": "지금 보면 와 악마의 재능이었네 싶고 그래요",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100040279.1.25",
          "form": "ㅎㅎ시간이 지나면서 해석이 다르게 되는 경우가 있죠",
          "original_form": "ㅎㅎ시간이 지나면서 해석이 다르게 되는 경우가 있죠",
          "speaker_id": "1",
          "time": "20210904 17:45"
        }
      ]
