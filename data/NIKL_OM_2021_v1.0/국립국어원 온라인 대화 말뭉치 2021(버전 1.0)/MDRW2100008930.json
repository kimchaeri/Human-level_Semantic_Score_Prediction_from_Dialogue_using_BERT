      "utterance": [
        {
          "id": "MDRW2100008930.1.1",
          "form": "내일부터 비 쏟아지겄는디",
          "original_form": "내일부터 비 쏟아지겄는디",
          "speaker_id": "2",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100008930.1.2",
          "form": "그래 비오면 미세먼지는 없다",
          "original_form": "그래 비오면 미세먼지는 없다",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100008930.1.3",
          "form": "오늘 다행히 날좋데",
          "original_form": "오늘 다행히 날좋데",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100008930.1.4",
          "form": "엉 담주 주말에 비온다디 날씨 구름으로 바꼈네요",
          "original_form": "엉 담주 주말에 비온다디 날씨 구름으로 바꼈네요",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100008930.1.5",
          "form": "캠핑 취소했다가",
          "original_form": "캠핑 취소했다가",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100008930.1.6",
          "form": "날씨 괜찮아서 다시 예약 ㅋㅋ",
          "original_form": "날씨 괜찮아서 다시 예약 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100008930.1.7",
          "form": "캠핑 가기 좋은날씨군ㅋ",
          "original_form": "캠핑 가기 좋은날씨군ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008930.1.8",
          "form": "일기예보계속바꿤",
          "original_form": "일기예보계속바꿤",
          "speaker_id": "1",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008930.1.9",
          "form": "내일  또 달라지고ㅋㅋ",
          "original_form": "내일  또 달라지고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008930.1.10",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100008930.1.11",
          "form": "제발 비 안오길",
          "original_form": "제발 비 안오길",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100008930.1.12",
          "form": "지금 바람 불어요??",
          "original_form": "지금 바람 불어요??",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100008930.1.13",
          "form": "왜이래 덥노",
          "original_form": "왜이래 덥노",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100008930.1.14",
          "form": "더워서 에어컨 켰다가 껏다",
          "original_form": "더워서 에어컨 켰다가 껏다",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100008930.1.15",
          "form": "지금도 덥네",
          "original_form": "지금도 덥네",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100008930.1.16",
          "form": "문열어놨는데 안시원함",
          "original_form": "문열어놨는데 안시원함",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100008930.1.17",
          "form": "엉 안시원하다",
          "original_form": "엉 안시원하다",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100008930.1.18",
          "form": "애들 자는방은 에어컨 틀어놨음",
          "original_form": "애들 자는방은 에어컨 틀어놨음",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100008930.1.19",
          "form": "투 땀난다",
          "original_form": "투 땀난다",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100008930.1.20",
          "form": "비오네ㅡㅡ",
          "original_form": "비오네ㅡㅡ",
          "speaker_id": "1",
          "time": "20210823 07:54"
        },
        {
          "id": "MDRW2100008930.1.21",
          "form": "빨래 어제했는거 우짜지?",
          "original_form": "빨래 어제했는거 우짜지?",
          "speaker_id": "1",
          "time": "20210823 07:55"
        },
        {
          "id": "MDRW2100008930.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 제습기 돌리슈",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 제습기 돌리슈",
          "speaker_id": "2",
          "time": "20210823 08:29"
        },
        {
          "id": "MDRW2100008930.1.23",
          "form": "비 살짝오네",
          "original_form": "비 살짝오네",
          "speaker_id": "2",
          "time": "20210823 08:29"
        },
        {
          "id": "MDRW2100008930.1.24",
          "form": "난 이제 출근",
          "original_form": "난 이제 출근",
          "speaker_id": "2",
          "time": "20210823 08:29"
        },
        {
          "id": "MDRW2100008930.1.25",
          "form": "빗길 안전운전 하슈",
          "original_form": "빗길 안전운전 하슈",
          "speaker_id": "2",
          "time": "20210823 08:30"
        },
        {
          "id": "MDRW2100008930.1.26",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210823 08:35"
        },
        {
          "id": "MDRW2100008930.1.27",
          "form": "뭐지?ㅋ",
          "original_form": "뭐지?ㅋ",
          "speaker_id": "1",
          "time": "20210823 08:36"
        },
        {
          "id": "MDRW2100008930.1.28",
          "form": "암튼 비오는데 운전싫다ㅋ",
          "original_form": "암튼 비오는데 운전싫다ㅋ",
          "speaker_id": "1",
          "time": "20210823 08:36"
        },
        {
          "id": "MDRW2100008930.1.29",
          "form": "ㅋㅋㅋ 오늘 밤부터 태풍이래요",
          "original_form": "ㅋㅋㅋ 오늘 밤부터 태풍이래요",
          "speaker_id": "2",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100008930.1.30",
          "form": "태풍땜에 전화돌리고있음 어르신들한테",
          "original_form": "태풍땜에 전화돌리고있음 어르신들한테",
          "speaker_id": "1",
          "time": "20210823 10:31"
        },
        {
          "id": "MDRW2100008930.1.31",
          "form": "아하...바쁘구만요",
          "original_form": "아하...바쁘구만요",
          "speaker_id": "2",
          "time": "20210823 10:33"
        },
        {
          "id": "MDRW2100008930.1.32",
          "form": "밤부터 많이온다네 비가",
          "original_form": "밤부터 많이온다네 비가",
          "speaker_id": "1",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100008930.1.33",
          "form": "방송도 막나오노",
          "original_form": "방송도 막나오노",
          "speaker_id": "1",
          "time": "20210823 11:50"
        }
      ]
