      "utterance": [
        {
          "id": "MDRW2100016950.1.1",
          "form": "로잉머신 시작한지는 얼마나 되셨어요??",
          "original_form": "로잉머신 시작한지는 얼마나 되셨어요??",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016950.1.2",
          "form": "반년?쯤 된것같아요",
          "original_form": "반년?쯤 된것같아요",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016950.1.3",
          "form": "실내자전거는 언제부터 타셧어요?",
          "original_form": "실내자전거는 언제부터 타셧어요?",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016950.1.4",
          "form": "오래하셨네요 꾸준함이 중요한 것 같아요 운동은",
          "original_form": "오래하셨네요 꾸준함이 중요한 것 같아요 운동은",
          "speaker_id": "1",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016950.1.5",
          "form": "자전거살까 로잉머신살까 하다가 로잉머신삿거든요 ㅋㅋㅋㅋ",
          "original_form": "자전거살까 로잉머신살까 하다가 로잉머신삿거든요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016950.1.6",
          "form": "자전거보다 더 나은 것 같으세요??",
          "original_form": "자전거보다 더 나은 것 같으세요??",
          "speaker_id": "1",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016950.1.7",
          "form": "음 솔직히 운동효과는 자전거가 더 나은것같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "음 솔직히 운동효과는 자전거가 더 나은것같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016950.1.8",
          "form": "아 그래요? ㅋㅋㅋㅋ",
          "original_form": "아 그래요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016950.1.9",
          "form": "로잉머신 저렴한걸로 삿더니",
          "original_form": "로잉머신 저렴한걸로 삿더니",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016950.1.10",
          "form": "유압식이라서",
          "original_form": "유압식이라서",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016950.1.11",
          "form": "빨리빨리 촥촥 당겨야하는데",
          "original_form": "빨리빨리 촥촥 당겨야하는데",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016950.1.12",
          "form": "좀 느려요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "좀 느려요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016950.1.13",
          "form": "ㅋㅋㅋㅋㅋ 그래도 힘들 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 그래도 힘들 것 같아요",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016950.1.14",
          "form": "전신운동이 된다는 면에선 좋은것 같아요 팔다리 다쓰거든요",
          "original_form": "전신운동이 된다는 면에선 좋은것 같아요 팔다리 다쓰거든요",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016950.1.15",
          "form": "하긴 자전거는 다리만 써서 상체는 운동이 안 될 것같아요",
          "original_form": "하긴 자전거는 다리만 써서 상체는 운동이 안 될 것같아요",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016950.1.16",
          "form": "그래도 운동효과는 자전거가 더 좋은것같아요 ㅋㅋㅋㅋ",
          "original_form": "그래도 운동효과는 자전거가 더 좋은것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
