      "utterance": [
        {
          "id": "MDRW2100003191.1.1",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210820 20:19"
        },
        {
          "id": "MDRW2100003191.1.2",
          "form": "저 잘생긴 사람이 이상형입니다",
          "original_form": "저 잘생긴 사람이 이상형입니다",
          "speaker_id": "2",
          "time": "20210820 20:22"
        },
        {
          "id": "MDRW2100003191.1.3",
          "form": "이상형이 어떻게 되시나요!!",
          "original_form": "이상형이 어떻게 되시나요!!",
          "speaker_id": "2",
          "time": "20210820 20:22"
        },
        {
          "id": "MDRW2100003191.1.4",
          "form": "전 몸 좋은사람이요!",
          "original_form": "전 몸 좋은사람이요!",
          "speaker_id": "1",
          "time": "20210820 20:23"
        },
        {
          "id": "MDRW2100003191.1.5",
          "form": "오 얼굴과 몸이 합쳐지면 더 좋을 거 같네요",
          "original_form": "오 얼굴과 몸이 합쳐지면 더 좋을 거 같네요",
          "speaker_id": "2",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100003191.1.6",
          "form": "성격 부분은요?",
          "original_form": "성격 부분은요?",
          "speaker_id": "2",
          "time": "20210820 20:24"
        },
        {
          "id": "MDRW2100003191.1.7",
          "form": "과묵하고 다정한사람이요ㅋ",
          "original_form": "과묵하고 다정한사람이요ㅋ",
          "speaker_id": "1",
          "time": "20210820 20:26"
        },
        {
          "id": "MDRW2100003191.1.8",
          "form": "저도 다정한사람 좋아요",
          "original_form": "저도 다정한사람 좋아요",
          "speaker_id": "2",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100003191.1.9",
          "form": "그리고 순한사람이요",
          "original_form": "그리고 순한사람이요",
          "speaker_id": "2",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003191.1.10",
          "form": "이상형에 가까운 사람을 본적있으세요?",
          "original_form": "이상형에 가까운 사람을 본적있으세요?",
          "speaker_id": "1",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003191.1.11",
          "form": "음 아쉽게도 아니요 ㅠ 외적인거랑은 어렵죠",
          "original_form": "음 아쉽게도 아니요 ㅠ 외적인거랑은 어렵죠",
          "speaker_id": "2",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003191.1.12",
          "form": "연예인 중에 누가 제일 이상형에 가까우세요?",
          "original_form": "연예인 중에 누가 제일 이상형에 가까우세요?",
          "speaker_id": "2",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003191.1.13",
          "form": "어렸을때 부터 쭉 공유 요ㅋㅋ",
          "original_form": "어렸을때 부터 쭉 공유 요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003191.1.14",
          "form": "오 공유!!!!!! 많은 사람의 이상형이죠",
          "original_form": "오 공유!!!!!! 많은 사람의 이상형이죠",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003191.1.15",
          "form": "전 요즘 외모는 차은우요 하하",
          "original_form": "전 요즘 외모는 차은우요 하하",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003191.1.16",
          "form": "오 전 몸만 보면 비도 좋아해요ㅋㅋ",
          "original_form": "오 전 몸만 보면 비도 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003191.1.17",
          "form": "정말 몸 좋은 연예인들을 좋아하시는군요",
          "original_form": "정말 몸 좋은 연예인들을 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003191.1.18",
          "form": "혹시 김우빈씨도????",
          "original_form": "혹시 김우빈씨도????",
          "speaker_id": "2",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003191.1.19",
          "form": "헉.. 광대승천... 운동하는 남자는 다 사랑스럽습니다ㅋㅋ",
          "original_form": "헉.. 광대승천... 운동하는 남자는 다 사랑스럽습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003191.1.20",
          "form": "알죠 알죠 몸이 좋으면 흐믓합니다",
