      "utterance": [
        {
          "id": "MDRW2100020811.1.1",
          "form": "후참잘이라고 아세요?",
          "original_form": "후참잘이라고 아세요?",
          "speaker_id": "1",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020811.1.2",
          "form": "후참잘이요?",
          "original_form": "후참잘이요?",
          "speaker_id": "2",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020811.1.3",
          "form": "후추 참깨 잘?",
          "original_form": "후추 참깨 잘?",
          "speaker_id": "2",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020811.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020811.1.5",
          "form": "후라이드참잘하는집이라고 있어요",
          "original_form": "후라이드참잘하는집이라고 있어요",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020811.1.6",
          "form": "줄여서 후참이라고 하거나 후참잘이라고 해요",
          "original_form": "줄여서 후참이라고 하거나 후참잘이라고 해요",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020811.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020811.1.8",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020811.1.9",
          "form": "후참잘",
          "original_form": "후참잘",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020811.1.10",
          "form": "이름값 하나요?",
          "original_form": "이름값 하나요?",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020811.1.11",
          "form": "네 ㅋㅋㅋ 근데 양념도 맛있어요",
          "original_form": "네 ㅋㅋㅋ 근데 양념도 맛있어요",
          "speaker_id": "1",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020811.1.12",
          "form": "제 생각엔 다른 브랜드보다 저렴한 편 같아요",
          "original_form": "제 생각엔 다른 브랜드보다 저렴한 편 같아요",
          "speaker_id": "1",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020811.1.13",
          "form": "그렇군요 저렴하다니",
          "original_form": "그렇군요 저렴하다니",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100020811.1.14",
          "form": "주위에 있으먄 한번 먹어봐야겠어요",
          "original_form": "주위에 있으먄 한번 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100020811.1.15",
          "form": "배민에서 이번주까지만 할인쿠폰 뿌리고 있을걸요?",
          "original_form": "배민에서 이번주까지만 할인쿠폰 뿌리고 있을걸요?",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020811.1.16",
          "form": "3천원인가 ㅎㅎ",
          "original_form": "3천원인가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020811.1.17",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100020811.1.18",
          "form": "좋은 정보 감사합니다",
          "original_form": "좋은 정보 감사합니다",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100020811.1.19",
          "form": "이번주엔 이미 치킨을 먹어서 또 먹을진 모르겠지만",
          "original_form": "이번주엔 이미 치킨을 먹어서 또 먹을진 모르겠지만",
          "speaker_id": "2",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100020811.1.20",
          "form": "기회된다면 시도해볼게요",
          "original_form": "기회된다면 시도해볼게요",
          "speaker_id": "2",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100020811.1.21",
          "form": "네네~ 포장할인은 상시로 하니까 그것도 찾아봐용~",
          "original_form": "네네~ 포장할인은 상시로 하니까 그것도 찾아봐용~",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100020811.1.22",
          "form": "전 처갓집양념보다 여기 양념치킨이 더 맛있더라구요",
          "original_form": "전 처갓집양념보다 여기 양념치킨이 더 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100020811.1.23",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100020811.1.24",
          "form": "매운편인가요 단 편인가요?",
          "original_form": "매운편인가요 단 편인가요?",
          "speaker_id": "2",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100020811.1.25",
          "form": "달아요 ㅋㅋ",
          "original_form": "달아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100020811.1.26",
          "form": "근데 튀김옷 자체가 매콤해서 약간 매콤달콤이라고 하는 게 맞을 것 같아요",
          "original_form": "근데 튀김옷 자체가 매콤해서 약간 매콤달콤이라고 하는 게 맞을 것 같아요",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100020811.1.27",
          "form": "아 글쿤여",
          "original_form": "아 글쿤여",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100020811.1.28",
          "form": "튀김옷 자체가 매콤한게 특이하네요",
          "original_form": "튀김옷 자체가 매콤한게 특이하네요",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100020811.1.29",
          "form": "매콤달콤 단맵은 안질리죠",
          "original_form": "매콤달콤 단맵은 안질리죠",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100020811.1.30",
          "form": "맛있을것같아요",
          "original_form": "맛있을것같아요",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100020811.1.31",
          "form": "그래서 후라이드 시켜도 좀 매콤하더라구요 ㅋㅋㅋ",
          "original_form": "그래서 후라이드 시켜도 좀 매콤하더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100020811.1.32",
          "form": "아이들 입맛엔 좀 매울 거 같아요",
          "original_form": "아이들 입맛엔 좀 매울 거 같아요",
          "speaker_id": "1",
          "time": "20210828 17:02"
        }
      ]
    }
