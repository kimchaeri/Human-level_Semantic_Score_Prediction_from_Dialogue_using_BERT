        },
        {
          "id": "MDRW2100005934.1.5",
          "form": "고양이 두마리여",
          "original_form": "고양이 두마리여",
          "speaker_id": "2",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005934.1.6",
          "form": "님은여",
          "original_form": "님은여",
          "speaker_id": "2",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005934.1.7",
          "form": "안키우는데 고양이 좋아해요",
          "original_form": "안키우는데 고양이 좋아해요",
          "speaker_id": "1",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005934.1.8",
          "form": "도전해보세요",
          "original_form": "도전해보세요",
          "speaker_id": "2",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100005934.1.9",
          "form": "9가 힘들고 1이 좋아요",
          "original_form": "9가 힘들고 1이 좋아요",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100005934.1.10",
          "form": "그게뭐에오",
          "original_form": "그게뭐에오",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100005934.1.11",
          "form": "키우는 고양이 종이뭐예요?",
          "original_form": "키우는 고양이 종이뭐예요?",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100005934.1.12",
          "form": "90프로가 힘들고 10프로만 좋아요",
          "original_form": "90프로가 힘들고 10프로만 좋아요",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100005934.1.13",
          "form": "고등어랑 턱시도여",
          "original_form": "고등어랑 턱시도여",
