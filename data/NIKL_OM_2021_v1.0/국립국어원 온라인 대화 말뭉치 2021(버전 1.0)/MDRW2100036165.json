        },
        {
          "id": "MDRW2100036165.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210830 18:31"
        },
        {
          "id": "MDRW2100036165.1.3",
          "form": "저녁 잘드셧어요?",
          "original_form": "저녁 잘드셧어요?",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100036165.1.4",
          "form": "넵ㅎㅎ 맛났내영",
          "original_form": "넵ㅎㅎ 맛났내영",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100036165.1.5",
          "form": "님은 저녁 드셨어요?",
          "original_form": "님은 저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100036165.1.6",
          "form": "먹고잇어요..",
          "original_form": "먹고잇어요..",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100036165.1.7",
          "form": "맨날 피자먹고 싶네요",
          "original_form": "맨날 피자먹고 싶네요",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036165.1.8",
          "form": "하지만 안되죠",
          "original_form": "하지만 안되죠",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036165.1.9",
          "form": "피자 맛없어요",
          "original_form": "피자 맛없어요",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036165.1.10",
          "form": "저는 피자가 제일 좋네요",
          "original_form": "저는 피자가 제일 좋네요",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036165.1.11",
          "form": "저는 싫어요",
          "original_form": "저는 싫어요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036165.1.12",
          "form": "그럼 저 주세여",
          "original_form": "그럼 저 주세여",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036165.1.13",
          "form": "싫은데",
          "original_form": "싫은데",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100036165.1.14",
          "form": "이따 맥주 드실생각 있나요?",
          "original_form": "이따 맥주 드실생각 있나요?",
          "speaker_id": "1",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100036165.1.15",
          "form": "느끼해서 먹어야할것같아요",
          "original_form": "느끼해서 먹어야할것같아요",
          "speaker_id": "2",
