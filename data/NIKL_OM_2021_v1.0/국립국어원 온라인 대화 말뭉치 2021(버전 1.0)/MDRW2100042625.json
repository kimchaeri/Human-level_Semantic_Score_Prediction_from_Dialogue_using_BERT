      "utterance": [
        {
          "id": "MDRW2100042625.1.1",
          "form": "안그래도 보고 있다",
          "original_form": "안그래도 보고 있다",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042625.1.2",
          "form": "개는 훌륭하다 하는데",
          "original_form": "개는 훌륭하다 하는데",
          "speaker_id": "1",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042625.1.3",
          "form": "엉 성악가부부가 의뢰인",
          "original_form": "엉 성악가부부가 의뢰인",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042625.1.4",
          "form": "그래 이번편은 괜찮아 강아지",
          "original_form": "그래 이번편은 괜찮아 강아지",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042625.1.5",
          "form": "다 행동에 문제 있는 강아지",
          "original_form": "다 행동에 문제 있는 강아지",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042625.1.6",
          "form": "우리도 솔직히 어렸을때 강아지 잘 못키웠잖아",
          "original_form": "우리도 솔직히 어렸을때 강아지 잘 못키웠잖아",
          "speaker_id": "1",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042625.1.7",
          "form": "맞다 말을 못하니 더 그런듯하다",
          "original_form": "맞다 말을 못하니 더 그런듯하다",
          "speaker_id": "2",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042625.1.8",
          "form": "어 맨날 간식도 많이 주고",
          "original_form": "어 맨날 간식도 많이 주고",
          "speaker_id": "1",
          "time": "20210906 23:53"
        },
        {
          "id": "MDRW2100042625.1.9",
          "form": "귀여우니 안줄수가 없었다",
          "original_form": "귀여우니 안줄수가 없었다",
          "speaker_id": "2",
          "time": "20210906 23:54"
        },
        {
          "id": "MDRW2100042625.1.10",
          "form": "맞다 그렁 그렁 한 눈으로 쳐다보니",
          "original_form": "맞다 그렁 그렁 한 눈으로 쳐다보니",
          "speaker_id": "1",
          "time": "20210906 23:54"
        },
        {
          "id": "MDRW2100042625.1.11",
          "form": "어 그래도 사람 먹는 음식은 주면 안됨",
          "original_form": "어 그래도 사람 먹는 음식은 주면 안됨",
          "speaker_id": "2",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100042625.1.12",
          "form": "맞아 진짜 조심해야한다",
          "original_form": "맞아 진짜 조심해야한다",
          "speaker_id": "1",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100042625.1.13",
          "form": "강아지 사료나 간식만",
          "original_form": "강아지 사료나 간식만",
          "speaker_id": "2",
          "time": "20210906 23:55"
        }
