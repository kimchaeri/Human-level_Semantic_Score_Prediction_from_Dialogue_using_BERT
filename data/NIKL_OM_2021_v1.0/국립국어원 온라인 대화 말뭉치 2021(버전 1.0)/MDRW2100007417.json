      "utterance": [
        {
          "id": "MDRW2100007417.1.1",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007417.1.2",
          "form": "바닐라를 제일 좋아합니다 name1님은요?",
          "original_form": "바닐라를 제일 좋아합니다 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007417.1.3",
          "form": "저는 약간 샤베트류",
          "original_form": "저는 약간 샤베트류",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007417.1.4",
          "form": "요거트 이런거",
          "original_form": "요거트 이런거",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007417.1.5",
          "form": "마트아이스크림 어떤거 좋아해요",
          "original_form": "마트아이스크림 어떤거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007417.1.6",
          "form": "저 올 여름은 젤루좋아 좋아서 자주 사먹었어요",
          "original_form": "저 올 여름은 젤루좋아 좋아서 자주 사먹었어요",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.7",
          "form": "오오오 샤베트",
          "original_form": "오오오 샤베트",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.8",
          "form": "저는 그 다이어트 아이스크림 위주로 사먹어서",
          "original_form": "저는 그 다이어트 아이스크림 위주로 사먹어서",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.9",
          "form": "그 아는데",
          "original_form": "그 아는데",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.10",
          "form": "스키니 피그 이런데서 아이스크림 사먹었어요",
          "original_form": "스키니 피그 이런데서 아이스크림 사먹었어요",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.11",
          "form": "그 통ㅇ니ㅣ든거",
          "original_form": "그 통ㅇ니ㅣ든거",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.12",
          "form": "맞아 그거요",
          "original_form": "맞아 그거요",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.13",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.14",
          "form": "맛있어요?",
          "original_form": "맛있어요?",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.15",
          "form": "그거 바닐라맛이 맛있어요",
          "original_form": "그거 바닐라맛이 맛있어요",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007417.1.16",
          "form": "녹차맛도 먹어봤는데 좀",
          "original_form": "녹차맛도 먹어봤는데 좀",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007417.1.17",
          "form": "흑임자맛도 맛있어요",
          "original_form": "흑임자맛도 맛있어요",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007417.1.18",
          "form": "음 ㅎㅎ 별로였구나",
          "original_form": "음 ㅎㅎ 별로였구나",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007417.1.19",
          "form": "흑임자?",
          "original_form": "흑임자?",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007417.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007417.1.21",
          "form": "맛이 진한가요?",
          "original_form": "맛이 진한가요?",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007417.1.22",
          "form": "네네네 진해요",
          "original_form": "네네네 진해요",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007417.1.23",
          "form": "근데 호불호 좀 갈릴 것 같어요",
          "original_form": "근데 호불호 좀 갈릴 것 같어요",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007417.1.24",
          "form": "오 좋다 진한맛",
