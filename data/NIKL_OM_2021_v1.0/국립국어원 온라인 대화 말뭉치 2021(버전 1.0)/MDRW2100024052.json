      "utterance": [
        {
          "id": "MDRW2100024052.1.1",
          "form": "name2님 안녕하세요",
          "original_form": "&name2&님 안녕하세요",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024052.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024052.1.3",
          "form": "주제가 종교네요!! 종교 있으신가요?",
          "original_form": "주제가 종교네요!! 종교 있으신가요?",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024052.1.4",
          "form": "네~ 기독교인데",
          "original_form": "네~ 기독교인데",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024052.1.5",
          "form": "다니지는 않습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "다니지는 않습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024052.1.6",
          "form": "name2님은요~?",
          "original_form": "&name2&님은요~?",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024052.1.7",
          "form": "전 종교가 없는데 요즘 세상이 흉흉한걸 보면서",
          "original_form": "전 종교가 없는데 요즘 세상이 흉흉한걸 보면서",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024052.1.8",
          "form": "전지전능한 존재가 있으면 좋을 것 같다고 생각해요ㅎㅋㅋㅋ",
          "original_form": "전지전능한 존재가 있으면 좋을 것 같다고 생각해요ㅎㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024052.1.9",
          "form": "아아 ㅠㅠ 공감입니다..",
          "original_form": "아아 ㅠㅠ 공감입니다..",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024052.1.10",
          "form": "그래서 어벤저스 같은 히어로물이 항상 인기가 좋은걸까요 ㅎㅎ name2님처럼 생각하시는분들이 많을거같네요",
          "original_form": "그래서 어벤저스 같은 히어로물이 항상 인기가 좋은걸까요 ㅎㅎ &name2&님처럼 생각하시는분들이 많을거같네요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024052.1.11",
          "form": "ㅠㅜ맞아요",
          "original_form": "ㅠㅜ맞아요",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024052.1.12",
          "form": "내 힘으로 안될때 누가 도와주길 원해서일까요ㅎ",
          "original_form": "내 힘으로 안될때 누가 도와주길 원해서일까요ㅎ",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024052.1.13",
          "form": "흑 다들  ㅋㅋㅋㅋ ㅠㅠㅠ 휴 힘겹게 사는거 같네요",
          "original_form": "흑 다들  ㅋㅋㅋㅋ ㅠㅠㅠ 휴 힘겹게 사는거 같네요",
          "speaker_id": "1",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024052.1.14",
          "form": "우울한 얘기일지도 모르겠지만",
          "original_form": "우울한 얘기일지도 모르겠지만",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024052.1.15",
          "form": "저는 요즘 사람들 살아가는거만으로도 엄청 대단하다고 생각해요",
          "original_form": "저는 요즘 사람들 살아가는거만으로도 엄청 대단하다고 생각해요",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024052.1.16",
          "form": "종교나 종교인에 거부감은 없으세요?",
          "original_form": "종교나 종교인에 거부감은 없으세요?",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024052.1.17",
          "form": "주변에 비종교인이 더 적어서 딱히 거부감은 없는 것 같아요!",
          "original_form": "주변에 비종교인이 더 적어서 딱히 거부감은 없는 것 같아요!",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100024052.1.18",
          "form": "근데 저는 뭔가 원초적으로 신이 없다고 느껴서 종교를 안믿는건데",
          "original_form": "근데 저는 뭔가 원초적으로 신이 없다고 느껴서 종교를 안믿는건데",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024052.1.19",
          "form": "전 기독교신앙은 있지만 동시에 맹목적 신앙인에 대해 비판적인편이에요 ㅠㅠ 안타까워서",
          "original_form": "전 기독교신앙은 있지만 동시에 맹목적 신앙인에 대해 비판적인편이에요 ㅠㅠ 안타까워서",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024052.1.20",
          "form": "아~~~",
          "original_form": "아~~~",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024052.1.21",
          "form": "신이 있으면 좋겠다고 생각은 해요",
          "original_form": "신이 있으면 좋겠다고 생각은 해요",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024052.1.22",
          "form": "하긴 신이 있다고 납득시켜줄 만한게 없었죠 그치않나요",
          "original_form": "하긴 신이 있다고 납득시켜줄 만한게 없었죠 그치않나요",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024052.1.23",
          "form": "ㅎㅎ 저는 신은 있다고 생각해요",
          "original_form": "ㅎㅎ 저는 신은 있다고 생각해요",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024052.1.24",
          "form": "전 그냥 태초의 우주엔 정말 아무것도 없는게 타당하다고 느꼈는데",
          "original_form": "전 그냥 태초의 우주엔 정말 아무것도 없는게 타당하다고 느꼈는데",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024052.1.25",
          "form": "사실 신이라는게",
          "original_form": "사실 신이라는게",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024052.1.26",
          "form": "실제로있으면 인간이 생각할수있는",
          "original_form": "실제로있으면 인간이 생각할수있는",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024052.1.27",
          "form": "범주가 아닐테니까",
          "original_form": "범주가 아닐테니까",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024052.1.28",
          "form": "그래서 신이니까(?)ㅋㅋㅋ 나약한 인간에겐 신이 필요하단 생각도 들어요",
          "original_form": "그래서 신이니까(?)ㅋㅋㅋ 나약한 인간에겐 신이 필요하단 생각도 들어요",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024052.1.29",
          "form": "꽤..깊게 생각을 하셨던거같아요..!",
          "original_form": "꽤..깊게 생각을 하셨던거같아요..!",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024052.1.30",
          "form": "괜찮은 접근이시네요...",
          "original_form": "괜찮은 접근이시네요...",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024052.1.31",
          "form": "ㅋㅋㅋㅋ옛날에 중학교때 종교인 친구랑 싸웠거든요",
          "original_form": "ㅋㅋㅋㅋ옛날에 중학교때 종교인 친구랑 싸웠거든요",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024052.1.32",
          "form": "앜ㅋㅋㅋㅋㅋㅋ그러셨어요 ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ그러셨어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024052.1.33",
          "form": "친구가 빅뱅을 안믿어서 서로에게 화났는데(!)",
          "original_form": "친구가 빅뱅을 안믿어서 서로에게 화났는데(!)",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024052.1.34",
          "form": "최다 싸움 주제죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "최다 싸움 주제죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024052.1.35",
          "form": "오호...",
          "original_form": "오호...",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024052.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 지금생각해보면 그 친구는 중학생이었는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 지금생각해보면 그 친구는 중학생이었는데",
          "speaker_id": "2",
