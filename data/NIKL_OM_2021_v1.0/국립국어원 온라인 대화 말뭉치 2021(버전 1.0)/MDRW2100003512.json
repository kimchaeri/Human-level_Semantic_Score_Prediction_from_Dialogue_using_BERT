      "utterance": [
        {
          "id": "MDRW2100003512.1.1",
          "form": "가족여행가야지",
          "original_form": "가족여행가야지",
          "speaker_id": "1",
          "time": "20210821 00:08"
        },
        {
          "id": "MDRW2100003512.1.2",
          "form": "뭔여행이야",
          "original_form": "뭔여행이야",
          "speaker_id": "2",
          "time": "20210821 00:08"
        },
        {
          "id": "MDRW2100003512.1.3",
          "form": "코로나 생기고 간적 없잖아",
          "original_form": "코로나 생기고 간적 없잖아",
          "speaker_id": "1",
          "time": "20210821 00:09"
        },
        {
          "id": "MDRW2100003512.1.4",
          "form": "4단계야 가긴 아딜가",
          "original_form": "4단계야 가긴 아딜가",
          "speaker_id": "2",
          "time": "20210821 00:09"
        },
        {
          "id": "MDRW2100003512.1.5",
          "form": "집에있어",
          "original_form": "집에있어",
          "speaker_id": "2",
          "time": "20210821 00:09"
        },
        {
          "id": "MDRW2100003512.1.6",
          "form": "다른집 보면 잘만 다니더만",
          "original_form": "다른집 보면 잘만 다니더만",
          "speaker_id": "1",
          "time": "20210821 00:09"
        },
        {
          "id": "MDRW2100003512.1.7",
          "form": "그건 다른집이고",
          "original_form": "그건 다른집이고",
          "speaker_id": "2",
          "time": "20210821 00:09"
        },
        {
          "id": "MDRW2100003512.1.8",
          "form": "가자 좋은데 찾았는데",
          "original_form": "가자 좋은데 찾았는데",
          "speaker_id": "1",
          "time": "20210821 00:10"
        },
        {
          "id": "MDRW2100003512.1.9",
          "form": "코로나 끝나면",
          "original_form": "코로나 끝나면",
          "speaker_id": "2",
          "time": "20210821 00:10"
        },
        {
          "id": "MDRW2100003512.1.10",
          "form": "이게 언제 끝나는데",
          "original_form": "이게 언제 끝나는데",
          "speaker_id": "1",
          "time": "20210821 00:10"
        },
        {
          "id": "MDRW2100003512.1.11",
          "form": "벌써 오래됫어",
          "original_form": "벌써 오래됫어",
          "speaker_id": "1",
          "time": "20210821 00:10"
        },
        {
          "id": "MDRW2100003512.1.12",
          "form": "아님 2차까지 다 접종하고 나섶",
          "original_form": "아님 2차까지 다 접종하고 나섶",
          "speaker_id": "2",
          "time": "20210821 00:11"
        },
        {
          "id": "MDRW2100003512.1.13",
          "form": "나는 9월 말에 접종인데",
          "original_form": "나는 9월 말에 접종인데",
          "speaker_id": "1",
          "time": "20210821 00:11"
        },
        {
          "id": "MDRW2100003512.1.14",
          "form": "그때 예약하면 가는거지?",
          "original_form": "그때 예약하면 가는거지?",
          "speaker_id": "1",
          "time": "20210821 00:11"
        },
        {
          "id": "MDRW2100003512.1.15",
          "form": "그때도 보긴 해야지",
          "original_form": "그때도 보긴 해야지",
          "speaker_id": "2",
          "time": "20210821 00:12"
        },
        {
          "id": "MDRW2100003512.1.16",
          "form": "나가자 나가자 가자 가자",
          "original_form": "나가자 나가자 가자 가자",
          "speaker_id": "1",
          "time": "20210821 00:12"
        },
        {
          "id": "MDRW2100003512.1.17",
          "form": "놀러가자",
          "original_form": "놀러가자",
          "speaker_id": "1",
          "time": "20210821 00:12"
        },
        {
          "id": "MDRW2100003512.1.18",
          "form": "몰라 그때 나오는거 보고",
          "original_form": "몰라 그때 나오는거 보고",
          "speaker_id": "2",
          "time": "20210821 00:12"
        },
        {
          "id": "MDRW2100003512.1.19",
          "form": "2천명이 넘어",
          "original_form": "2천명이 넘어",
          "speaker_id": "2",
          "time": "20210821 00:12"
        }
      ]
