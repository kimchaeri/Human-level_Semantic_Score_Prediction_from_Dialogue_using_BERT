        {
          "id": "MDRW2100038494.1.1",
          "form": "엄마 어제 인터넷쇼핑으로 만두 주문했다 ㅎㅎ",
          "original_form": "엄마 어제 인터넷쇼핑으로 만두 주문했다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038494.1.2",
          "form": "웬 만두",
          "original_form": "웬 만두",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038494.1.3",
