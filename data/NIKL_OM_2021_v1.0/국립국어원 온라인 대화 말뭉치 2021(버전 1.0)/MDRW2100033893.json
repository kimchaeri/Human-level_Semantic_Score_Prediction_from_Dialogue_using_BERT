        {
          "id": "MDRW2100033893.1.1",
          "form": "보쌈 좋아요 보쌈 김치를 좋아해서요",
          "original_form": "보쌈 좋아요 보쌈 김치를 좋아해서요",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033893.1.2",
          "form": "name1 님은요",
          "original_form": "&name1& 님은요",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033893.1.3",
          "form": "ㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100033893.1.4",
          "form": "족발보단 보쌈파입니다",
          "original_form": "족발보단 보쌈파입니다",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100033893.1.5",
          "form": "전 항아리보쌈 좋아해요. 생활의 달인 나왔다고 하더라구요.",
          "original_form": "전 항아리보쌈 좋아해요. 생활의 달인 나왔다고 하더라구요.",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100033893.1.6",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100033893.1.7",
          "form": "항아리보쌈은 뭔가요?",
          "original_form": "항아리보쌈은 뭔가요?",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100033893.1.8",
          "form": "아 보쌈이 항아리 뚜껑 같은데 나와요",
          "original_form": "아 보쌈이 항아리 뚜껑 같은데 나와요",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033893.1.9",
          "form": "박경규의 항아리보쌈 일거에요ㅡ 식당 이름이",
          "original_form": "박경규의 항아리보쌈 일거에요ㅡ 식당 이름이",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033893.1.10",
          "form": "와 ..봤는데",
          "original_form": "와 ..봤는데",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033893.1.11",
          "form": "엄청ㅈ맛있겠어요",
          "original_form": "엄청ㅈ맛있겠어요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033893.1.12",
          "form": "맛있어요!보쌈김치 넘 맛나요",
          "original_form": "맛있어요!보쌈김치 넘 맛나요",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100033893.1.13",
          "form": "오 굿이죠",
          "original_form": "오 굿이죠",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033893.1.14",
          "form": "단무지없이 자장면 먹기 vs 김치없이 라면먹기 어떠세요?",
          "original_form": "단무지없이 자장면 먹기 vs 김치없이 라면먹기 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033893.1.15",
          "form": "김치 없이 라면은 안 될 거 같아요",
          "original_form": "김치 없이 라면은 안 될 거 같아요",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100033893.1.16",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100033893.1.17",
          "form": "저랑 반대시네요",
          "original_form": "저랑 반대시네요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100033893.1.18",
          "form": "야식이 땡기는 밤이네요.\r\n배달 시킬까 하다 빵 있길래 빵 먹었어요.",
          "original_form": "야식이 땡기는 밤이네요.\r\n배달 시킬까 하다 빵 있길래 빵 먹었어요.",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100033893.1.19",
          "form": "어떤빵드셨나요ㅋㅋㅋㅋ",
          "original_form": "어떤빵드셨나요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100033893.1.20",
          "form": "맘모스빵 비슷한거요.",
          "original_form": "맘모스빵 비슷한거요.",
          "speaker_id": "2",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100033893.1.21",
          "form": "아침 드시나요? 전 거의 안 먹어요",
          "original_form": "아침 드시나요? 전 거의 안 먹어요",
          "speaker_id": "2",
          "time": "20210826 10:20"
        },
        {
          "id": "MDRW2100033893.1.22",
          "form": "저는 아침을 꼬박꼬박 먹어요",
          "original_form": "저는 아침을 꼬박꼬박 먹어요",
