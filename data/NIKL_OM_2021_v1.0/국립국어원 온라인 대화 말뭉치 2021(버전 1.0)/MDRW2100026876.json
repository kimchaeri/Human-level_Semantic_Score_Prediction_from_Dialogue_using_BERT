      "utterance": [
        {
          "id": "MDRW2100026876.1.1",
          "form": "저 내일 학교가는데요",
          "original_form": "저 내일 학교가는데요",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026876.1.2",
          "form": "친구가 자가격리걸려서",
          "original_form": "친구가 자가격리걸려서",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026876.1.3",
          "form": "헐?? 어떡해요",
          "original_form": "헐?? 어떡해요",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026876.1.4",
          "form": "정확히말하명 친구 동생이 1학년인데",
          "original_form": "정확히말하명 친구 동생이 1학년인데",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026876.1.5",
          "form": "확진자나와서 동생이 자가격리거든요",
          "original_form": "확진자나와서 동생이 자가격리거든요",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026876.1.6",
          "form": "그래서 친구도 이번주만 빠진대요",
          "original_form": "그래서 친구도 이번주만 빠진대요",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026876.1.7",
          "form": "그래서 그냥 급식 안먹고 공부나 하려구용...",
          "original_form": "그래서 그냥 급식 안먹고 공부나 하려구용...",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026876.1.8",
          "form": "헐랭...아 점심 안 먹는다는 뜻이",
          "original_form": "헐랭...아 점심 안 먹는다는 뜻이",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026876.1.9",
          "form": "그런거였군용",
          "original_form": "그런거였군용",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026876.1.10",
          "form": "네네네네",
          "original_form": "네네네네",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026876.1.11",
          "form": "요즘 점심안먹는 정시러들 잇어서",
          "original_form": "요즘 점심안먹는 정시러들 잇어서",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026876.1.12",
          "form": "딱히 뭐... 그냥 간식싸가지고 가려구요",
          "original_form": "딱히 뭐... 그냥 간식싸가지고 가려구요",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026876.1.13",
          "form": "그래두 밥은 먹지... 한국인은 밥심이자나요",
          "original_form": "그래두 밥은 먹지... 한국인은 밥심이자나요",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026876.1.14",
          "form": "편의점에서 뭐라도 사서 점심에 먹어여",
          "original_form": "편의점에서 뭐라도 사서 점심에 먹어여",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026876.1.15",
          "form": "저는 나나기리 학원에 다니는데요",
          "original_form": "저는 나나기리 학원에 다니는데요",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026876.1.16",
          "form": "그럴까요... 아침에 10분만 일찍 나가도 되는데",
          "original_form": "그럴까요... 아침에 10분만 일찍 나가도 되는데",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.17",
          "form": "등교하기 힘들어요...졸려요...",
          "original_form": "등교하기 힘들어요...졸려요...",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.18",
          "form": "앗..넵!^^",
          "original_form": "앗..넵!^^",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.19",
          "form": "등교 몇시에 해용??",
          "original_form": "등교 몇시에 해용??",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.20",
          "form": "8시 20분까지요",
          "original_form": "8시 20분까지요",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.21",
          "form": "근데 학교가 바로 앞이라서 10분컷가능",
          "original_form": "근데 학교가 바로 앞이라서 10분컷가능",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.22",
          "form": "우와....8시20분",
          "original_form": "우와....8시20분",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.23",
          "form": "대박이다",
          "original_form": "대박이다",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.24",
          "form": "다른 애들에 비하면 통학시간 짧은데",
          "original_form": "다른 애들에 비하면 통학시간 짧은데",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.25",
          "form": "그래도 너무 힘들어용 등교싫어",
          "original_form": "그래도 너무 힘들어용 등교싫어",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026876.1.26",
          "form": "무슨 의미에 대박이죠?? 대 학 생 씨",
          "original_form": "무슨 의미에 대박이죠?? 대 학 생 씨",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026876.1.27",
          "form": "님 정말 대박인거같애요",
          "original_form": "님 정말 대박인거같애요",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026876.1.28",
          "form": "저는 9시 시작인 1교시도",
          "original_form": "저는 9시 시작인 1교시도",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026876.1.29",
          "form": "늦잠자느라 40분에 들어가고 그러는데요",
          "original_form": "늦잠자느라 40분에 들어가고 그러는데요",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026876.1.30",
          "form": "그치만...님도 고딩때는 그럣을거 아니에요",
          "original_form": "그치만...님도 고딩때는 그럣을거 아니에요",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026876.1.31",
          "form": "대학생되면서 체력이 와장창나는거죠",
          "original_form": "대학생되면서 체력이 와장창나는거죠",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026876.1.32",
          "form": "저 지금도 계단 올라갈때 무릎에서 둑뚝소리나는데",
          "original_form": "저 지금도 계단 올라갈때 무릎에서 둑뚝소리나는데",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026876.1.33",
          "form": "...님의미래예요",
          "original_form": "...님의미래예요",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026876.1.34",
          "form": "수능끝나면 운동하려구요...",
          "original_form": "수능끝나면 운동하려구요...",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026876.1.35",
          "form": "운동중요하죠 ..",
          "original_form": "운동중요하죠 ..",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026876.1.36",
          "form": "글고보니 님 학교에 매점 없었나요",
          "original_form": "글고보니 님 학교에 매점 없었나요",
          "speaker_id": "1",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100026876.1.37",
          "form": "매점 없으면 전자렌지도 없겟네여",
          "original_form": "매점 없으면 전자렌지도 없겟네여",
          "speaker_id": "1",
          "time": "20210902 01:32"
        }
      ]
    }
