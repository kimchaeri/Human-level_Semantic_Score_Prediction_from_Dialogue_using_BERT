        },
        {
          "id": "MDRW2100032291.1.2",
          "form": "왜냐면 너거 형부랑 나이 많은 계장님 쫓아내야 name3가 5급달수있거든",
          "original_form": "왜냐면 너거 형부랑 나이 많은 계장님 쫓아내야 &name3&가 5급달수있거든",
          "speaker_id": "1",
          "time": "20210905 14:57"
        },
        {
          "id": "MDRW2100032291.1.3",
          "form": "그래서 name3위에 한명 계장 쫓아냈다 name3 점수줄라고",
          "original_form": "그래서 &name3&위에 한명 계장 쫓아냈다 &name3& 점수줄라고",
          "speaker_id": "1",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032291.1.4",
          "form": "name3가 전라도가",
          "original_form": "&name3&가 전라도가",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032291.1.5",
          "form": "벌써",
          "original_form": "벌써",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032291.1.6",
          "form": "응 전라도",
          "original_form": "응 전라도",
          "speaker_id": "1",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032291.1.7",
          "form": "응 당뇨안고 내려갔다 걔도 전라도인데ㅜ",
          "original_form": "응 당뇨안고 내려갔다 걔도 전라도인데ㅜ",
          "speaker_id": "1",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032291.1.8",
          "form": "심각하메",
          "original_form": "심각하메",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032291.1.9",
          "form": "심각하네",
          "original_form": "심각하네",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032291.1.10",
          "form": "그 팀장이 윗 인사 장난친단다",
          "original_form": "그 팀장이 윗 인사 장난친단다",
          "speaker_id": "1",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100032291.1.11",
          "form": "그래서 지가 대단한줄 아는데",
          "original_form": "그래서 지가 대단한줄 아는데",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032291.1.12",
          "form": "맞나 헐 뭐도",
          "original_form": "맞나 헐 뭐도",
          "speaker_id": "2",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032291.1.13",
          "form": "너거 형부가 들은것만해도 고소장 2개 준비중이란다",
          "original_form": "너거 형부가 들은것만해도 고소장 2개 준비중이란다",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032291.1.14",
          "form": "고소하나정말",
          "original_form": "고소하나정말",
          "speaker_id": "2",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032291.1.15",
          "form": "팀장앞으로",
          "original_form": "팀장앞으로",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032291.1.16",
          "form": "장난친거 다 아니깐ㅜ",
          "original_form": "장난친거 다 아니깐ㅜ",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032291.1.17",
          "form": "법무부 이런데 무섭다ㅜ",
          "original_form": "법무부 이런데 무섭다ㅜ",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032291.1.18",
          "form": "고소가 되긴하나",
          "original_form": "고소가 되긴하나",
          "speaker_id": "2",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032291.1.19",
          "form": "내가 쳐내야 승진하니깐",
          "original_form": "내가 쳐내야 승진하니깐",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032291.1.20",
          "form": "되지 한명은 해임되게 생겼으니 고소하는거고",
          "original_form": "되지 한명은 해임되게 생겼으니 고소하는거고",
          "speaker_id": "1",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100032291.1.21",
          "form": "한명은 승진 탈락시켰으니 고소준비하고",
          "original_form": "한명은 승진 탈락시켰으니 고소준비하고",
          "speaker_id": "1",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100032291.1.22",
          "form": "ㅣ그면 가만있어도 나가겠네",
          "original_form": "ㅣ그면 가만있어도 나가겠네",
          "speaker_id": "2",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100032291.1.23",
          "form": "다 팀장이 뒤에서 장난친것임",
          "original_form": "다 팀장이 뒤에서 장난친것임",
          "speaker_id": "1",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100032291.1.24",
          "form": "고소랑 그거는 상관없다",
          "original_form": "고소랑 그거는 상관없다",
          "speaker_id": "1",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100032291.1.25",
          "form": "개인적으로 힘든거지ㅜ",
          "original_form": "개인적으로 힘든거지ㅜ",
          "speaker_id": "1",
          "time": "20210905 15:00"
        },
        {
          "id": "MDRW2100032291.1.26",
          "form": "그면 고소하면 어떻데되노",
          "original_form": "그면 고소하면 어떻데되노",
          "speaker_id": "2",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032291.1.27",
          "form": "귀찮아지지 재판받으러 가야되고",
          "original_form": "귀찮아지지 재판받으러 가야되고",
          "speaker_id": "1",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032291.1.28",
          "form": "근데 너거 형부 말로는 바보라서 그런상황인지도 모른단다ㅜ",
          "original_form": "근데 너거 형부 말로는 바보라서 그런상황인지도 모른단다ㅜ",
          "speaker_id": "1",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032291.1.29",
          "form": "진짜 고소하는거가",
          "original_form": "진짜 고소하는거가",
          "speaker_id": "2",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032291.1.30",
          "form": "한명은 변호사도 샀단다",
          "original_form": "한명은 변호사도 샀단다",
          "speaker_id": "1",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100032291.1.31",
          "form": "고소한다고 팀장한테 말한거가",
          "original_form": "고소한다고 팀장한테 말한거가",
          "speaker_id": "2",
          "time": "20210905 15:01"
        }
      ]
