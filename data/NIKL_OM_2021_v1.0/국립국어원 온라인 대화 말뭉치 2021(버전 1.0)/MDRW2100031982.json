      "utterance": [
        {
          "id": "MDRW2100031982.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 10:16"
        },
        {
          "id": "MDRW2100031982.1.2",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031982.1.3",
          "form": "아 반려견키우고있어요",
          "original_form": "아 반려견키우고있어요",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031982.1.4",
          "form": "동물키우시나요?",
          "original_form": "동물키우시나요?",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031982.1.5",
          "form": "우와 몇살이에요?",
          "original_form": "우와 몇살이에요?",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031982.1.6",
          "form": "전 없어요 ㅠ",
          "original_form": "전 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031982.1.7",
          "form": "이제 곧 4살되어요!",
          "original_form": "이제 곧 4살되어요!",
          "speaker_id": "2",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031982.1.8",
          "form": "키워보신적두 없으세요?",
          "original_form": "키워보신적두 없으세요?",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031982.1.9",
          "form": "어렸을땐",
          "original_form": "어렸을땐",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031982.1.10",
          "form": "팬더마우스 키워봤어요",
          "original_form": "팬더마우스 키워봤어요",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031982.1.11",
          "form": "누에랑요",
          "original_form": "누에랑요",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031982.1.12",
          "form": "독특하죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "독특하죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031982.1.13",
          "form": "우와 팬더마우스요?",
          "original_form": "우와 팬더마우스요?",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031982.1.14",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031982.1.15",
          "form": "햄스터보다 순해요",
          "original_form": "햄스터보다 순해요",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031982.1.16",
          "form": "이쁘고",
          "original_form": "이쁘고",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031982.1.17",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031982.1.18",
          "form": "애교많게 생겼어여!",
          "original_form": "애교많게 생겼어여!",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031982.1.19",
          "form": "보고오셨어요? ㅎㅎ",
          "original_form": "보고오셨어요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031982.1.20",
          "form": "쪼끄맣고 귀여워요 ㅎㅎ갑자기 키우고싶네용",
          "original_form": "쪼끄맣고 귀여워요 ㅎㅎ갑자기 키우고싶네용",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031982.1.21",
          "form": "반려동물 같이 있는건 너무 행복한 일인거 같아요",
          "original_form": "반려동물 같이 있는건 너무 행복한 일인거 같아요",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031982.1.22",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031982.1.23",
          "form": "멍뭉이 키우시는 name2님이 인생 승자입니다",
          "original_form": "멍뭉이 키우시는 &name2&님이 인생 승자입니다",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031982.1.24",
          "form": "ㅎㅎ애기 넘 이쁘겠어요",
          "original_form": "ㅎㅎ애기 넘 이쁘겠어요",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031982.1.25",
          "form": "헤헤 감사합니다ㅋㅋ 넘귀여워요",
          "original_form": "헤헤 감사합니다ㅋㅋ 넘귀여워요",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031982.1.26",
          "form": "그냥 제눈엔 너무 다 예뻐요",
          "original_form": "그냥 제눈엔 너무 다 예뻐요",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031982.1.27",
          "form": "건강만 했으면 하죠!",
          "original_form": "건강만 했으면 하죠!",
          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031982.1.28",
          "form": "으아 사진보여주세요",
          "original_form": "으아 사진보여주세요",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031982.1.29",
          "form": "왜 사진전송이 안될까유",
          "original_form": "왜 사진전송이 안될까유",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031982.1.30",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031982.1.31",
          "form": "ㅋㅋㅋ저희 반려견 사진찍는거 좋아해서 사진 많은데",
          "original_form": "ㅋㅋㅋ저희 반려견 사진찍는거 좋아해서 사진 많은데",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031982.1.32",
          "form": "아쉽네요!",
          "original_form": "아쉽네요!",
          "speaker_id": "2",
          "time": "20210905 10:35"
        }
      ]
    }
