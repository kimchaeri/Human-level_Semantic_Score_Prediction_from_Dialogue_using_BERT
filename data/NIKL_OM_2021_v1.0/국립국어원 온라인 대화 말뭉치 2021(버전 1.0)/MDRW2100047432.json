        },
        {
          "id": "MDRW2100047432.1.4",
          "form": "넷플 1달무료하고 탈퇴했어요",
          "original_form": "넷플 1달무료하고 탈퇴했어요",
          "speaker_id": "2",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100047432.1.5",
          "form": "지금쿠팡플레이로봐요",
          "original_form": "지금쿠팡플레이로봐요",
          "speaker_id": "2",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100047432.1.6",
          "form": "쿠팡플레이보세요?",
          "original_form": "쿠팡플레이보세요?",
          "speaker_id": "2",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100047432.1.7",
          "form": "엇?아니요",
          "original_form": "엇?아니요",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100047432.1.8",
          "form": "쿠팡플레이도 볼 거 많나요?",
          "original_form": "쿠팡플레이도 볼 거 많나요?",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100047432.1.9",
          "form": "그냥 그것도 한달무료라ㅎㅎ",
          "original_form": "그냥 그것도 한달무료라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100047432.1.10",
          "form": "해봤어요",
          "original_form": "해봤어요",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100047432.1.11",
          "form": "아항ㅋㅋ 저도 무료체험\r\n신청해봐야겠네요ㅋㅋ",
          "original_form": "아항ㅋㅋ 저도 무료체험\r\n신청해봐야겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100047432.1.12",
          "form": "쿠팡플레이로는 뭐 보셨어요?",
          "original_form": "쿠팡플레이로는 뭐 보셨어요?",
          "speaker_id": "1",
