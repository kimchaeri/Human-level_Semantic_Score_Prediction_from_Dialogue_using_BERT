      "utterance": [
        {
          "id": "MDRW2100029644.1.1",
          "form": "어렵지 않나요?",
          "original_form": "어렵지 않나요?",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029644.1.2",
          "form": "그러겠죠??",
          "original_form": "그러겠죠??",
          "speaker_id": "1",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029644.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029644.1.4",
          "form": "백신 2차까지 맞은 사람들은 예외로 한다고 한거 같던데",
          "original_form": "백신 2차까지 맞은 사람들은 예외로 한다고 한거 같던데",
          "speaker_id": "1",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029644.1.5",
          "form": "전 1차만",
          "original_form": "전 1차만",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029644.1.6",
          "form": "저희 가족은 2차까지 완료했어요",
          "original_form": "저희 가족은 2차까지 완료했어요",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029644.1.7",
          "form": "근데 친척중에 아직 1차만 맞은 사람들이 있어서",
          "original_form": "근데 친척중에 아직 1차만 맞은 사람들이 있어서",
          "speaker_id": "1",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029644.1.8",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029644.1.9",
          "form": "작년부터 계속 못 모이니까 속상하네요",
          "original_form": "작년부터 계속 못 모이니까 속상하네요",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029644.1.10",
          "form": "까먹겠어요",
          "original_form": "까먹겠어요",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029644.1.11",
          "form": "가족들과 같이  살고 있나요 아니면 혼자사나요",
          "original_form": "가족들과 같이  살고 있나요 아니면 혼자사나요",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029644.1.12",
          "form": "같이요",
          "original_form": "같이요",
          "speaker_id": "2",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029644.1.13",
          "form": "그럼 일단 가족들은 항상 보니까 좋네요..",
          "original_form": "그럼 일단 가족들은 항상 보니까 좋네요..",
          "speaker_id": "1",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029644.1.14",
          "form": "저도 가족들과 같이 살아서 혼자있는 외로움은 없어요",
          "original_form": "저도 가족들과 같이 살아서 혼자있는 외로움은 없어요",
