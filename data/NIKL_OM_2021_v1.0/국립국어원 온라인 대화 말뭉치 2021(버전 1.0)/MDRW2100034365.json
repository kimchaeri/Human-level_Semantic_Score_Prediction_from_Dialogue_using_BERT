      "utterance": [
        {
          "id": "MDRW2100034365.1.1",
          "form": "수록곡인가요?",
          "original_form": "수록곡인가요?",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100034365.1.2",
          "form": "타이틀인데 좀 된 노래에요",
          "original_form": "타이틀인데 좀 된 노래에요",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100034365.1.3",
          "form": "노래 못 부르는 저를 노래방에서 가수로 만들어주는 노래에요 ㅋㅋㅋ 꼭 들어보세요",
          "original_form": "노래 못 부르는 저를 노래방에서 가수로 만들어주는 노래에요 ㅋㅋㅋ 꼭 들어보세요",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100034365.1.4",
          "form": "그럼 체이스미 아닌가요?",
          "original_form": "그럼 체이스미 아닌가요?",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100034365.1.5",
          "form": "헉 그건가봐요 ㅌㅋㅋㅋ",
          "original_form": "헉 그건가봐요 ㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100034365.1.6",
          "form": "아ㅋㅋㅋ체이스미 좋죠ㅠ",
          "original_form": "아ㅋㅋㅋ체이스미 좋죠ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100034365.1.7",
          "form": "넘 좋더라구여 ㅋㅋㅋ",
          "original_form": "넘 좋더라구여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100034365.1.8",
          "form": "좀 빠른 템포를 좋아해서요",
          "original_form": "좀 빠른 템포를 좋아해서요",
          "speaker_id": "1",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100034365.1.9",
          "form": "마자요ㅠ",
          "original_form": "마자요ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100034365.1.10",
          "form": "타이틀 거의 좋더라구요ㅠ",
          "original_form": "타이틀 거의 좋더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100034365.1.11",
          "form": "어떤 노래 좋아하세요?",
          "original_form": "어떤 노래 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 00:36"
        },
        {
          "id": "MDRW2100034365.1.12",
          "form": "저는 부를때는 발라드가 좋구요\n들을때는 신나는게 좋아요ㅋㅋ",
          "original_form": "저는 부를때는 발라드가 좋구요\n들을때는 신나는게 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100034365.1.13",
          "form": "최근에 나온건데 비커즈도 좋더라구요 드림캐쳐꺼요",
          "original_form": "최근에 나온건데 비커즈도 좋더라구요 드림캐쳐꺼요",
          "speaker_id": "2",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100034365.1.14",
          "form": "아 제목만 들어봣어요",
          "original_form": "아 제목만 들어봣어요",
          "speaker_id": "1",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100034365.1.15",
          "form": "드림캐쳐 노래는 다 괜찮은거같아요 ㅋㅋㅋ",
          "original_form": "드림캐쳐 노래는 다 괜찮은거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100034365.1.16",
          "form": "마자요 다 좋아요\n좀 마이너 느낌이긴하지만ㅋㅋ",
          "original_form": "마자요 다 좋아요\n좀 마이너 느낌이긴하지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100034365.1.17",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100034365.1.18",
          "form": "소수만 듣는 약간ㅋㅋㅋ",
          "original_form": "소수만 듣는 약간ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100034365.1.19",
          "form": "혹시 이달의소녀도 아시나요?",
          "original_form": "혹시 이달의소녀도 아시나요?",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100034365.1.20",
          "form": "마자요 그래서 음원차트에 잘 못오르나봐요ㅠ",
          "original_form": "마자요 그래서 음원차트에 잘 못오르나봐요ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100034365.1.21",
          "form": "이달소 들어만봤어요!",
          "original_form": "이달소 들어만봤어요!",
          "speaker_id": "1",
          "time": "20210828 00:39"
        }
      ]
    }
