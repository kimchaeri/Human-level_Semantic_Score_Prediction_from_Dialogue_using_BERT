      "utterance": [
        {
          "id": "MDRW2100046513.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211026 19:32"
        },
        {
          "id": "MDRW2100046513.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211026 19:32"
        },
        {
          "id": "MDRW2100046513.1.3",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "1",
          "time": "20211026 19:32"
        },
        {
          "id": "MDRW2100046513.1.4",
          "form": "운동 하시는거 있으세요?",
          "original_form": "운동 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20211026 19:32"
        },
        {
          "id": "MDRW2100046513.1.5",
          "form": "실내에서 싸이클 즐겨 타요",
          "original_form": "실내에서 싸이클 즐겨 타요",
          "speaker_id": "1",
          "time": "20211026 19:33"
        },
        {
          "id": "MDRW2100046513.1.6",
          "form": "와 저도 자전거 타는 거 좋아해요 ㅎㅎ",
          "original_form": "와 저도 자전거 타는 거 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211026 19:33"
        },
        {
          "id": "MDRW2100046513.1.7",
          "form": "저는 실외지만요",
          "original_form": "저는 실외지만요",
          "speaker_id": "2",
          "time": "20211026 19:33"
        },
        {
          "id": "MDRW2100046513.1.8",
          "form": "자전거가 정직한 운동같아요 ㅎㅎ",
          "original_form": "자전거가 정직한 운동같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211026 19:33"
        },
        {
          "id": "MDRW2100046513.1.9",
          "form": "그쵸 ㅎㅎ",
          "original_form": "그쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211026 19:34"
        },
        {
          "id": "MDRW2100046513.1.10",
          "form": "요즘 실외  자전거는 조금 춥지 않나요",
          "original_form": "요즘 실외  자전거는 조금 춥지 않나요",
          "speaker_id": "1",
          "time": "20211026 19:34"
        },
        {
          "id": "MDRW2100046513.1.11",
          "form": "하체가 튼튼해야지 건강하다니까요",
          "original_form": "하체가 튼튼해야지 건강하다니까요",
          "speaker_id": "2",
          "time": "20211026 19:34"
        },
        {
          "id": "MDRW2100046513.1.12",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20211026 19:34"
        },
        {
          "id": "MDRW2100046513.1.13",
          "form": "요즘 저녁에 타니 춥더라고요",
          "original_form": "요즘 저녁에 타니 춥더라고요",
          "speaker_id": "2",
          "time": "20211026 19:34"
        },
        {
          "id": "MDRW2100046513.1.14",
          "form": "주말엔 낮에 탔는데 ㅎㅎ 가을 구경도 하고 좋았어요",
          "original_form": "주말엔 낮에 탔는데 ㅎㅎ 가을 구경도 하고 좋았어요",
          "speaker_id": "2",
          "time": "20211026 19:34"
        },
        {
          "id": "MDRW2100046513.1.15",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20211026 19:35"
        },
        {
          "id": "MDRW2100046513.1.16",
          "form": "보통 싸이클 어느정도 타세요?",
          "original_form": "보통 싸이클 어느정도 타세요?",
          "speaker_id": "2",
          "time": "20211026 19:35"
        },
        {
          "id": "MDRW2100046513.1.17",
          "form": "저도 실내 싸이클 사고싶어서 당근에서 구경중이에요",
          "original_form": "저도 실내 싸이클 사고싶어서 당근에서 구경중이에요",
          "speaker_id": "2",
          "time": "20211026 19:35"
        },
        {
          "id": "MDRW2100046513.1.18",
          "form": "퇴근하고 30분정도요",
          "original_form": "퇴근하고 30분정도요",
          "speaker_id": "1",
          "time": "20211026 19:35"
        },
        {
          "id": "MDRW2100046513.1.19",
          "form": "유산소로 딱 적당하네요!",
          "original_form": "유산소로 딱 적당하네요!",
          "speaker_id": "2",
          "time": "20211026 19:35"
        },
        {
          "id": "MDRW2100046513.1.20",
          "form": "근데 안하는 날이 더 많단는건 비밀이에요",
          "original_form": "근데 안하는 날이 더 많단는건 비밀이에요",
          "speaker_id": "1",
          "time": "20211026 19:36"
        },
        {
          "id": "MDRW2100046513.1.21",
          "form": "ㅋㅋㅋㅋㅋ 한다는 게 중요한거죠!",
          "original_form": "ㅋㅋㅋㅋㅋ 한다는 게 중요한거죠!",
          "speaker_id": "2",
          "time": "20211026 19:36"
        }
      ]
