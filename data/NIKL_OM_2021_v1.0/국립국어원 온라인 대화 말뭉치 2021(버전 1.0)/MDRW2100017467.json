        },
        {
          "id": "MDRW2100017467.1.2",
          "form": "어떤걸 주로 만들어 드시나요?",
          "original_form": "어떤걸 주로 만들어 드시나요?",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017467.1.3",
          "form": "요리는 못해요",
          "original_form": "요리는 못해요",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017467.1.4",
          "form": "저도 요리는 못 하겠어요ㅠㅠ",
          "original_form": "저도 요리는 못 하겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017467.1.5",
          "form": "밀키트 잘 나오니",
          "original_form": "밀키트 잘 나오니",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100017467.1.6",
          "form": "까요",
          "original_form": "까요",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100017467.1.7",
          "form": "약간 밀키트보다 배달을 시키게 돼요ㅋㅋ",
          "original_form": "약간 밀키트보다 배달을 시키게 돼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100017467.1.8",
          "form": "편하죠",
          "original_form": "편하죠",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100017467.1.9",
          "form": "밀키트도 일단 요리를 해야하니까요",
          "original_form": "밀키트도 일단 요리를 해야하니까요",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100017467.1.10",
          "form": "그른가요",
          "original_form": "그른가요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100017467.1.11",
          "form": "요리한 뒤 뒷감당도 귀찮구여",
          "original_form": "요리한 뒤 뒷감당도 귀찮구여",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100017467.1.12",
          "form": "설거지도 귀찮아서",
          "original_form": "설거지도 귀찮아서",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100017467.1.13",
          "form": "밀키트도 설거지는 나오니까요",
          "original_form": "밀키트도 설거지는 나오니까요",
          "speaker_id": "1",
          "time": "20210826 23:22"
