        {
          "id": "MDRW2100006362.1.1",
          "form": "책 자주 읽으시나요?",
          "original_form": "책 자주 읽으시나요?",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006362.1.2",
          "form": "가끔ㅋㅋ",
          "original_form": "가끔ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006362.1.3",
          "form": "자주 보시나요?",
          "original_form": "자주 보시나요?",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006362.1.4",
          "form": "저도 가끔..ㅋㅋ",
          "original_form": "저도 가끔..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006362.1.5",
          "form": "가장 최근에 어떤거 보셨어요?",
          "original_form": "가장 최근에 어떤거 보셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006362.1.6",
          "form": "분노의 돌파구라고 아세요?",
          "original_form": "분노의 돌파구라고 아세요?",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006362.1.7",
          "form": "소설이에요..",
          "original_form": "소설이에요..",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100006362.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100006362.1.9",
          "form": "에세이.같은 건데요 노벨상 수상자가 썼어요",
          "original_form": "에세이.같은 건데요 노벨상 수상자가 썼어요",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006362.1.10",
          "form": "아..읽어봐야겠어요",
          "original_form": "아..읽어봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006362.1.11",
          "form": "전 미비포유 그 시리즈 보그 있어요",
          "original_form": "전 미비포유 그 시리즈 보그 있어요",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006362.1.12",
          "form": "그건 뭘까요?",
          "original_form": "그건 뭘까요?",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006362.1.13",
          "form": "그 영화나온건데 사지마비 환자와 사랑에 빠지는",
          "original_form": "그 영화나온건데 사지마비 환자와 사랑에 빠지는",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006362.1.14",
          "form": "아.. 제가 영화를 안봐서 몰랐네요",
          "original_form": "아.. 제가 영화를 안봐서 몰랐네요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006362.1.15",
          "form": "저도 사실 비극이라... ㅋㅋ아닌가..해피일까요",
          "original_form": "저도 사실 비극이라... ㅋㅋ아닌가..해피일까요",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006362.1.16",
          "form": "애매하긴 해요",
          "original_form": "애매하긴 해요",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006362.1.17",
          "form": "영화는 비극인가요?",
          "original_form": "영화는 비극인가요?",
