      "utterance": [
        {
          "id": "MDRW2100029215.1.1",
          "form": "좋아하는 배달 음식 있으세요?",
          "original_form": "좋아하는 배달 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029215.1.2",
          "form": "교촌치킨 허니콤보요!",
          "original_form": "교촌치킨 허니콤보요!",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029215.1.3",
          "form": "하 교촌 허니콤보 맛있죠ㅠㅠ",
          "original_form": "하 교촌 허니콤보 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029215.1.4",
          "form": "전 떡볶이 주로 배달시키는거 같아요",
          "original_form": "전 떡볶이 주로 배달시키는거 같아요",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029215.1.5",
          "form": "엽떡? 신전? 어디꺼요",
          "original_form": "엽떡? 신전? 어디꺼요",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029215.1.6",
          "form": "신전이용!",
          "original_form": "신전이용!",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029215.1.7",
          "form": "저도 신전 좋아해요 ㅋㅋ",
          "original_form": "저도 신전 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100029215.1.8",
          "form": "어묵 튀김 좋아해서 꼭 시켜요 ㅋㅋ",
          "original_form": "어묵 튀김 좋아해서 꼭 시켜요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100029215.1.9",
          "form": "하 역시 신전은 어묵 튀김 필수죠ㅜ",
          "original_form": "하 역시 신전은 어묵 튀김 필수죠ㅜ",
          "speaker_id": "1",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029215.1.10",
          "form": "저번에 감자면 나온 것두 드셔보셨나용",
          "original_form": "저번에 감자면 나온 것두 드셔보셨나용",
          "speaker_id": "1",
          "time": "20210903 12:02"
        },
        {
          "id": "MDRW2100029215.1.11",
          "form": "엥 그런것도 있나요",
          "original_form": "엥 그런것도 있나요",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029215.1.12",
          "form": "요즘 시켜먹은지 오래되가지고 ㅠ",
          "original_form": "요즘 시켜먹은지 오래되가지고 ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029215.1.13",
          "form": "한 2달전에 출시됐을거에요!",
          "original_form": "한 2달전에 출시됐을거에요!",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029215.1.14",
          "form": "담에 신전 드시게되면 한 번 같이 먹어보세요",
          "original_form": "담에 신전 드시게되면 한 번 같이 먹어보세요",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029215.1.15",
          "form": "중국당면 좋아하신다면,,,!",
          "original_form": "중국당면 좋아하신다면,,,!",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029215.1.16",
          "form": "오 감사합니다",
          "original_form": "오 감사합니다",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100029215.1.17",
          "form": "배떡로제 드셔보셧나요",
          "original_form": "배떡로제 드셔보셧나요",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100029215.1.18",
          "form": "당연히 먹어봤습니다,,,,!! 맛있더라구요ㅜ",
          "original_form": "당연히 먹어봤습니다,,,,!! 맛있더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100029215.1.19",
          "form": "그니까요...완전...",
          "original_form": "그니까요...완전...",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100029215.1.20",
          "form": "우유빙수랑 얼음빙수 중엔",
          "original_form": "우유빙수랑 얼음빙수 중엔",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100029215.1.21",
          "form": "뭘 선호하시나요",
          "original_form": "뭘 선호하시나요",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100029215.1.22",
          "form": "저는 우유얼음이요~!!",
          "original_form": "저는 우유얼음이요~!!",
          "speaker_id": "1",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100029215.1.23",
