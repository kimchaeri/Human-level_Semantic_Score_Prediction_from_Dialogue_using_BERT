          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020819.1.25",
          "form": "하고",
          "original_form": "하고",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020819.1.26",
          "form": "ㅇㅇ타타라스나도",
          "original_form": "ㅇㅇ타타라스나도",
          "speaker_id": "2",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020819.1.27",
          "form": "무슨 엔진 문제고",
          "original_form": "무슨 엔진 문제고",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020819.1.28",
          "form": "ㅇㅇ 법 이런거 깐깐하게 따지고",
          "original_form": "ㅇㅇ 법 이런거 깐깐하게 따지고",
          "speaker_id": "1",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020819.1.29",
          "form": "사진기 막 발명하는시기고",
          "original_form": "사진기 막 발명하는시기고",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020819.1.30",
          "form": "수메르는 약간",
          "original_form": "수메르는 약간",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020819.1.31",
          "form": "학자도시?",
          "original_form": "학자도시?",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020819.1.32",
          "form": "ㅇㅇ 게임 설정상",
          "original_form": "ㅇㅇ 게임 설정상",
          "speaker_id": "1",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020819.1.33",
          "form": "아카데미가 거기에 있을걸",
          "original_form": "아카데미가 거기에 있을걸",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020819.1.34",
          "form": "만나는 수메르인이 거의 교수아니면 대학원생이야",
          "original_form": "만나는 수메르인이 거의 교수아니면 대학원생이야",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020819.1.35",
          "form": "리사도 수메르 출신이잖아",
          "original_form": "리사도 수메르 출신이잖아",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
