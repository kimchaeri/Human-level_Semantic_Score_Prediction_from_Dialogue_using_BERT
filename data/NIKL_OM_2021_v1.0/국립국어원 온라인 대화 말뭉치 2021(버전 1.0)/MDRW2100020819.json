      "utterance": [
        {
          "id": "MDRW2100020819.1.1",
          "form": "그럼 캐릭터 육성을 다른걸 더 넣기라도 해야댐",
          "original_form": "그럼 캐릭터 육성을 다른걸 더 넣기라도 해야댐",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020819.1.2",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020819.1.3",
          "form": "성유물을 더 올리던지",
          "original_form": "성유물을 더 올리던지",
          "speaker_id": "1",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020819.1.4",
          "form": "만렙 확장이 싫으면",
          "original_form": "만렙 확장이 싫으면",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020819.1.5",
          "form": "폰타인?떡밥",
          "original_form": "폰타인?떡밥",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020819.1.6",
          "form": "겁나 뿌리더레",
          "original_form": "겁나 뿌리더레",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020819.1.7",
          "form": "던데",
          "original_form": "던데",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020819.1.8",
          "form": "인게임에서",
          "original_form": "인게임에서",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020819.1.9",
          "form": "ㅇㅇ 다음 도시가",
          "original_form": "ㅇㅇ 다음 도시가",
          "speaker_id": "1",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020819.1.10",
          "form": "폰타인인가 수메르인가 그래",
          "original_form": "폰타인인가 수메르인가 그래",
          "speaker_id": "1",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020819.1.11",
          "form": "타타라스나도 폰타인출신있고",
          "original_form": "타타라스나도 폰타인출신있고",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020819.1.12",
          "form": "외국인 출입 금지시킨거 치곤",
          "original_form": "외국인 출입 금지시킨거 치곤",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020819.1.13",
          "form": "외국인들 잘만 돌아다니던데",
          "original_form": "외국인들 잘만 돌아다니던데",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020819.1.14",
          "form": "먹거리 어벤져스인가",
          "original_form": "먹거리 어벤져스인가",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020819.1.15",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020819.1.16",
          "form": "몬드나 리월보다",
          "original_form": "몬드나 리월보다",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020819.1.17",
          "form": "걔네도 각각 몬드 리월 폰타인 출신같던데",
          "original_form": "걔네도 각각 몬드 리월 폰타인 출신같던데",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020819.1.18",
          "form": "오히려 외국인 더 많은듯",
          "original_form": "오히려 외국인 더 많은듯",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020819.1.19",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020819.1.20",
          "form": "다음번은 폰타인인가봐",
          "original_form": "다음번은 폰타인인가봐",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020819.1.21",
          "form": "컨셉이 약간",
          "original_form": "컨셉이 약간",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020819.1.22",
          "form": "스팀펑크 느낌이던데",
          "original_form": "스팀펑크 느낌이던데",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020819.1.23",
          "form": "좀 스팀펑크?",
          "original_form": "좀 스팀펑크?",
          "speaker_id": "2",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020819.1.24",
