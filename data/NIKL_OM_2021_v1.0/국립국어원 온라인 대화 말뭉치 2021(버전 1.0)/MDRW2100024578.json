      "utterance": [
        {
          "id": "MDRW2100024578.1.1",
          "form": "그러게요 시원하긴하네요",
          "original_form": "그러게요 시원하긴하네요",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100024578.1.2",
          "form": "이따 퇴근해야하는데",
          "original_form": "이따 퇴근해야하는데",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100024578.1.3",
          "form": "걱정이네요",
          "original_form": "걱정이네요",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100024578.1.4",
          "form": "빗길 싫어여..",
          "original_form": "빗길 싫어여..",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100024578.1.5",
          "form": "맞아요ㅠ 지금 비 많이와요ㅠ",
          "original_form": "맞아요ㅠ 지금 비 많이와요ㅠ",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100024578.1.6",
          "form": "하루종일 많이오네요",
          "original_form": "하루종일 많이오네요",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100024578.1.7",
          "form": "누가 차로 나 데리러 왔으면 좋겠어요",
          "original_form": "누가 차로 나 데리러 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024578.1.8",
          "form": "그러게요.. 비오는날 출퇴근 최악이에요",
          "original_form": "그러게요.. 비오는날 출퇴근 최악이에요",
          "speaker_id": "1",
          "time": "20210831 15:58"
        },
        {
          "id": "MDRW2100024578.1.9",
          "form": "걸을수가 없죠...",
          "original_form": "걸을수가 없죠...",
          "speaker_id": "1",
          "time": "20210831 15:59"
        },
        {
          "id": "MDRW2100024578.1.10",
          "form": "아아악 30분남았는게 한게없네요 그래도 퇴근은 하고싶어요 비를 뚫고 집으로",
          "original_form": "아아악 30분남았는게 한게없네요 그래도 퇴근은 하고싶어요 비를 뚫고 집으로",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024578.1.11",
          "form": "뚫고지나가요~~",
          "original_form": "뚫고지나가요~~",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024578.1.12",
          "form": "뚫고 지나가요~~~",
          "original_form": "뚫고 지나가요~~~",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024578.1.13",
          "form": "비와서 더 집중이 안되는걸수도있어요ㅠ",
          "original_form": "비와서 더 집중이 안되는걸수도있어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024578.1.14",
          "form": "뭐를??? 비를~~~~",
          "original_form": "뭐를??? 비를~~~~",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024578.1.15",
          "form": "휴 늦장마...",
          "original_form": "휴 늦장마...",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024578.1.16",
          "form": "요즘 모기도 기승이더라구요",
          "original_form": "요즘 모기도 기승이더라구요",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024578.1.17",
          "form": "마자요 그렇다면서요? 저는 다행히 아직은 안물렸는데 걱정이네요",
          "original_form": "마자요 그렇다면서요? 저는 다행히 아직은 안물렸는데 걱정이네요",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024578.1.18",
          "form": "저 엄청 물렸어요 ㅠㅠ 모기 박멸 원츄ㅠㅠㅠ",
          "original_form": "저 엄청 물렸어요 ㅠㅠ 모기 박멸 원츄ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024578.1.19",
          "form": "헐 벌써요? 정말.. 차라리 확 추워지면 모기들 사라지려나요? 이것도 모르겠네ㅠ",
          "original_form": "헐 벌써요? 정말.. 차라리 확 추워지면 모기들 사라지려나요? 이것도 모르겠네ㅠ",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024578.1.20",
          "form": "겨울이 좋은 이유 중 하나가 모기 없는거죠",
          "original_form": "겨울이 좋은 이유 중 하나가 모기 없는거죠",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024578.1.21",
          "form": "마자요 ㅋㅋㅋㅋ겨울좋아용 빨리 겨울되서 기모후드 입고싶어요",
          "original_form": "마자요 ㅋㅋㅋㅋ겨울좋아용 빨리 겨울되서 기모후드 입고싶어요",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024578.1.22",
          "form": "역시나 바지가 비에 홀딱...",
          "original_form": "역시나 바지가 비에 홀딱...",
          "speaker_id": "2",
          "time": "20210831 18:34"
        },
        {
          "id": "MDRW2100024578.1.23",
          "form": "오늘 비오는데 이사하는 집있네여 ㅠㅠ안타깝다ㅜㅜ",
          "original_form": "오늘 비오는데 이사하는 집있네여 ㅠㅠ안타깝다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 18:34"
        },
        {
          "id": "MDRW2100024578.1.24",
          "form": "와 이제아침엔 정말 선선하네요",
          "original_form": "와 이제아침엔 정말 선선하네요",
          "speaker_id": "1",
