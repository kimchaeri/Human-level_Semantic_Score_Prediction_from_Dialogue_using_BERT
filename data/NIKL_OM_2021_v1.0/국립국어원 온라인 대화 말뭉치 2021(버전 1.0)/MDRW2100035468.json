      "utterance": [
        {
          "id": "MDRW2100035468.1.1",
          "form": "여긴좋아요 ㅎㅎ거긴 그래요?",
          "original_form": "여긴좋아요 ㅎㅎ거긴 그래요?",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100035468.1.2",
          "form": "오랜만에 해가 나서 ㅜㅜ",
          "original_form": "오랜만에 해가 나서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100035468.1.3",
          "form": "빨래 아직도 돌리고 있어요",
          "original_form": "빨래 아직도 돌리고 있어요",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035468.1.4",
          "form": "시험 잘보셨어요?",
          "original_form": "시험 잘보셨어요?",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035468.1.5",
          "form": "오늘 오전에 비왔는데 .날이 안좋았죠",
          "original_form": "오늘 오전에 비왔는데 .날이 안좋았죠",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100035468.1.6",
          "form": "고생하셨어요 ㅎㅎ",
          "original_form": "고생하셨어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100035468.1.7",
          "form": "비가왔다가 또 안왔다가 또 비가 많이왔다가 적게왔다가ㅜㅜ",
          "original_form": "비가왔다가 또 안왔다가 또 비가 많이왔다가 적게왔다가ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100035468.1.8",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100035468.1.9",
          "form": "정말 거긴 날씨 엉망이네요",
          "original_form": "정말 거긴 날씨 엉망이네요",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100035468.1.10",
          "form": "요새 계속 날씨가 안좋아서",
          "original_form": "요새 계속 날씨가 안좋아서",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100035468.1.11",
          "form": "빨래도 산더미 ㅡㅡ",
          "original_form": "빨래도 산더미 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100035468.1.12",
          "form": "지금은 좀 낫지만 여전히 안말라요",
          "original_form": "지금은 좀 낫지만 여전히 안말라요",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100035468.1.13",
          "form": "제습기 있으세요?",
          "original_form": "제습기 있으세요?",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035468.1.14",
          "form": "나갈때 선선해서 땀은 안나던데 그거 말곤 참 좋진 않아요",
          "original_form": "나갈때 선선해서 땀은 안나던데 그거 말곤 참 좋진 않아요",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035468.1.15",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035468.1.16",
          "form": "에어컨 틀어요",
          "original_form": "에어컨 틀어요",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035468.1.17",
          "form": "저희는 요새 제습기로 다 말려요ㅜㅜ",
          "original_form": "저희는 요새 제습기로 다 말려요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035468.1.18",
          "form": "그럼 보송해요 ㅋㅋ",
          "original_form": "그럼 보송해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035468.1.19",
          "form": "에어컨은 전기세가 무서워서 ㅋㅋ",
          "original_form": "에어컨은 전기세가 무서워서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035468.1.20",
          "form": "아 그럼 좋은데",
          "original_form": "아 그럼 좋은데",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100035468.1.21",
          "form": "에어컨은 추워서 ㅋㅋ안말라요",
          "original_form": "에어컨은 추워서 ㅋㅋ안말라요",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100035468.1.22",
          "form": "집만 보송",
          "original_form": "집만 보송",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100035468.1.23",
          "form": "수건이 한장도 없어요 ㅋㅋ",
          "original_form": "수건이 한장도 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100035468.1.24",
          "form": "어제 빨래 못하고 오늘 했는데 안말라서 ㅜㅜ",
          "original_form": "어제 빨래 못하고 오늘 했는데 안말라서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100035468.1.25",
          "form": "오늘 오전에 수건을 다쓰고 ㅡㅡ",
          "original_form": "오늘 오전에 수건을 다쓰고 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100035468.1.26",
          "form": "건조기에 돌리고 있어요 ㅋㅋ",
          "original_form": "건조기에 돌리고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100035468.1.27",
          "form": "오 그래도 건조기 있으셔서 다행이네요 ㅎㅎ",
          "original_form": "오 그래도 건조기 있으셔서 다행이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035468.1.28",
          "form": "저희도 미룰때까지 미루다 얼마전에 한꺼번에 막 돌렸네요 ㅎㅎ",
          "original_form": "저희도 미룰때까지 미루다 얼마전에 한꺼번에 막 돌렸네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100035468.1.29",
          "form": "네 ㅋㅋ다들 미루다 한꺼번에 어쩔수 없는 날씨예요",
          "original_form": "네 ㅋㅋ다들 미루다 한꺼번에 어쩔수 없는 날씨예요",
          "speaker_id": "2",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100035468.1.30",
          "form": "주말에 큰일 하셨네요ㅜㅜ",
          "original_form": "주말에 큰일 하셨네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100035468.1.31",
          "form": "비가 왔다안왔다하는데 요즘비는 맞기가 찜찜해요 ㅋㅋㅋ",
          "original_form": "비가 왔다안왔다하는데 요즘비는 맞기가 찜찜해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100035468.1.32",
          "form": "맞아요 ㅋㅋ빗속에 뭐가 들었나 몰라요 ㅎㅎ",
          "original_form": "맞아요 ㅋㅋ빗속에 뭐가 들었나 몰라요 ㅎㅎ",
