      "utterance": [
        {
          "id": "MDRW2100046933.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20211107 04:35"
        },
        {
          "id": "MDRW2100046933.1.2",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20211108 00:31"
        },
        {
          "id": "MDRW2100046933.1.3",
          "form": "뭐하고 계세요?",
          "original_form": "뭐하고 계세요?",
          "speaker_id": "2",
          "time": "20211108 00:32"
        },
        {
          "id": "MDRW2100046933.1.4",
          "form": "넷플릭스 보고있습니당",
          "original_form": "넷플릭스 보고있습니당",
          "speaker_id": "1",
          "time": "20211107 13:10"
        },
        {
          "id": "MDRW2100046933.1.5",
          "form": "name2님은 뭐하시나요?",
          "original_form": "&name2&님은 뭐하시나요?",
          "speaker_id": "1",
          "time": "20211107 13:10"
        },
        {
          "id": "MDRW2100046933.1.6",
          "form": "저는 여행유튜브 봐요 ㅎㅎㅎㅎ",
          "original_form": "저는 여행유튜브 봐요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211107 13:39"
        },
        {
          "id": "MDRW2100046933.1.7",
          "form": "넷플릭스 어떤 거 보세요?",
          "original_form": "넷플릭스 어떤 거 보세요?",
          "speaker_id": "2",
          "time": "20211107 13:39"
        },
        {
          "id": "MDRW2100046933.1.8",
          "form": "오티스의 비밀공작소요",
          "original_form": "오티스의 비밀공작소요",
          "speaker_id": "1",
          "time": "20211107 13:46"
        },
        {
          "id": "MDRW2100046933.1.9",
          "form": "좋아하시는 유튜버 있으세요?",
          "original_form": "좋아하시는 유튜버 있으세요?",
          "speaker_id": "1",
          "time": "20211107 13:47"
        },
        {
          "id": "MDRW2100046933.1.10",
          "form": "전 빠니보틀 채널 자주봐요!",
          "original_form": "전 빠니보틀 채널 자주봐요!",
          "speaker_id": "2",
          "time": "20211107 14:11"
        },
        {
          "id": "MDRW2100046933.1.11",
          "form": "헉 저도 그거 볼라했는데 재밌어요?",
          "original_form": "헉 저도 그거 볼라했는데 재밌어요?",
          "speaker_id": "2",
          "time": "20211107 14:11"
        },
        {
          "id": "MDRW2100046933.1.12",
          "form": "아 빠니보틍 저도 알아요ㅋㅋ",
          "original_form": "아 빠니보틍 저도 알아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211107 14:33"
        },
        {
          "id": "MDRW2100046933.1.13",
          "form": "네 진짜 재밌어요 ㅋㅋ",
          "original_form": "네 진짜 재밌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211107 14:33"
        },
        {
          "id": "MDRW2100046933.1.14",
          "form": "오 봐야겠다 ㅎㅎ",
          "original_form": "오 봐야겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211107 14:34"
        },
        {
          "id": "MDRW2100046933.1.15",
          "form": "또 추천해줄 거 있으세요?",
          "original_form": "또 추천해줄 거 있으세요?",
          "speaker_id": "2",
          "time": "20211107 14:34"
        },
        {
          "id": "MDRW2100046933.1.16",
          "form": "종이의집이요 ㅋㅋㅋ",
          "original_form": "종이의집이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211107 14:46"
        },
        {
          "id": "MDRW2100046933.1.17",
          "form": "오 봐볼게요!",
          "original_form": "오 봐볼게요!",
          "speaker_id": "2",
          "time": "20211107 14:55"
        },
        {
          "id": "MDRW2100046933.1.18",
          "form": "한국 거는 안보세요?",
          "original_form": "한국 거는 안보세요?",
          "speaker_id": "2",
          "time": "20211107 14:56"
        },
        {
          "id": "MDRW2100046933.1.19",
          "form": "오징어게임 봤어요!",
          "original_form": "오징어게임 봤어요!",
          "speaker_id": "1",
          "time": "20211107 15:29"
        },
        {
          "id": "MDRW2100046933.1.20",
          "form": "오징어 게임 안 본 사람 찾기가 더 힘들더라구요 ㅋㅋㅋ",
          "original_form": "오징어 게임 안 본 사람 찾기가 더 힘들더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211107 15:30"
        },
        {
          "id": "MDRW2100046933.1.21",
          "form": "저도 오늘 넷플릭스 보는 날로 정하고 새벽까지 봐야겠어요 ㅎㅎ",
          "original_form": "저도 오늘 넷플릭스 보는 날로 정하고 새벽까지 봐야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211107 15:30"
        },
        {
          "id": "MDRW2100046933.1.22",
          "form": "즐거운 일요일 보내세요!",
          "original_form": "즐거운 일요일 보내세요!",
          "speaker_id": "2",
          "time": "20211107 15:30"
        }
      ]
