      "utterance": [
        {
          "id": "MDRW2100031219.1.1",
          "form": "최애 치킨메뉴 무엇인가용",
          "original_form": "최애 치킨메뉴 무엇인가용",
          "speaker_id": "2",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100031219.1.2",
          "form": "저는 후라이드요",
          "original_form": "저는 후라이드요",
          "speaker_id": "1",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100031219.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100031219.1.4",
          "form": "저두요 양념은 찍어먹으면 되지만 양념은 한가지밖에 못먹자나요@",
          "original_form": "저두요 양념은 찍어먹으면 되지만 양념은 한가지밖에 못먹자나요@",
          "speaker_id": "2",
          "time": "20210904 19:44"
        },
        {
          "id": "MDRW2100031219.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:44"
        },
        {
          "id": "MDRW2100031219.1.6",
          "form": "어디 치킨주로 드세요?",
          "original_form": "어디 치킨주로 드세요?",
          "speaker_id": "1",
          "time": "20210904 19:44"
        },
        {
          "id": "MDRW2100031219.1.7",
          "form": "앗 근데 저는 구운치킨이 제일 좋아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 근데 저는 구운치킨이 제일 좋아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:46"
        },
        {
          "id": "MDRW2100031219.1.8",
          "form": "앞 답변과 말이 안맞죠 ㅋㅋㅋㅋ",
          "original_form": "앞 답변과 말이 안맞죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031219.1.9",
          "form": "굽네요?",
          "original_form": "굽네요?",
          "speaker_id": "1",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031219.1.10",
          "form": "굽네랑 교촌 둘다 좋아요",
          "original_form": "굽네랑 교촌 둘다 좋아요",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031219.1.11",
          "form": "꺅",
          "original_form": "꺅",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031219.1.12",
          "form": "그리고 집앞에 오븐마루라고 있거든요",
          "original_form": "그리고 집앞에 오븐마루라고 있거든요",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031219.1.13",
          "form": "ㅎㅎ 구운 치킨도 여러소스 먹을수있죠",
          "original_form": "ㅎㅎ 구운 치킨도 여러소스 먹을수있죠",
          "speaker_id": "1",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031219.1.14",
          "form": "그것도 좋아요",
          "original_form": "그것도 좋아요",
          "speaker_id": "2",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031219.1.15",
          "form": "오븐마루는 저렴한가요?",
          "original_form": "오븐마루는 저렴한가요?",
          "speaker_id": "1",
          "time": "20210904 19:47"
        },
        {
          "id": "MDRW2100031219.1.16",
          "form": "네! 일반 배달 치킨보다 저렴해요",
          "original_form": "네! 일반 배달 치킨보다 저렴해요",
          "speaker_id": "2",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031219.1.17",
          "form": "그리고 바로 집앞이라 배달비가 없어서 좋아요 ㅋㅋㅋ",
          "original_form": "그리고 바로 집앞이라 배달비가 없어서 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031219.1.18",
          "form": "왕복 십분도 안걸리거든요",
          "original_form": "왕복 십분도 안걸리거든요",
          "speaker_id": "2",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031219.1.19",
          "form": "그쵸 배달비도 은근 부담이예요",
          "original_form": "그쵸 배달비도 은근 부담이예요",
          "speaker_id": "1",
          "time": "20210904 19:48"
        },
        {
          "id": "MDRW2100031219.1.20",
          "form": "요즘은 배달비도 막 5000원씩 받는곳도 있더라구요",
          "original_form": "요즘은 배달비도 막 5000원씩 받는곳도 있더라구요",
          "speaker_id": "1",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031219.1.21",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "2",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031219.1.22",
          "form": "ㅠㅠㅠㅠㅠㅠ배달비 진짜 너무 비싸요",
          "original_form": "ㅠㅠㅠㅠㅠㅠ배달비 진짜 너무 비싸요",
          "speaker_id": "2",
          "time": "20210904 19:49"
        },
        {
          "id": "MDRW2100031219.1.23",
          "form": "먹고 싶을때 포기하는 이유중 절반은 배달비죠",
          "original_form": "먹고 싶을때 포기하는 이유중 절반은 배달비죠",
          "speaker_id": "2",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031219.1.24",
          "form": "그래서 저도 주로 포장해서 먹어요",
          "original_form": "그래서 저도 주로 포장해서 먹어요",
          "speaker_id": "1",
          "time": "20210904 19:50"
        },
        {
          "id": "MDRW2100031219.1.25",
          "form": "근데 집앞에 음식점이 몇개 없어요ㅜ",
          "original_form": "근데 집앞에 음식점이 몇개 없어요ㅜ",
