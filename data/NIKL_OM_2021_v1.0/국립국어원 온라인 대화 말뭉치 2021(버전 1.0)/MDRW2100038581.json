      "utterance": [
        {
          "id": "MDRW2100038581.1.1",
          "form": "ㅠㅠ아쉽지만 다음 결승전 남아서",
          "original_form": "ㅠㅠ아쉽지만 다음 결승전 남아서",
          "speaker_id": "2",
          "time": "20210903 07:13"
        },
        {
          "id": "MDRW2100038581.1.2",
          "form": "응원하려구요ㅋㅋㅋㅋ",
          "original_form": "응원하려구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:13"
        },
        {
          "id": "MDRW2100038581.1.3",
          "form": "결승전끝나구나면 새로시작하나요 프로그램 끝나진않죵?ㅇㅁㅇ",
          "original_form": "결승전끝나구나면 새로시작하나요 프로그램 끝나진않죵?ㅇㅁㅇ",
          "speaker_id": "1",
          "time": "20210903 10:01"
        },
        {
          "id": "MDRW2100038581.1.4",
          "form": "공식 답 나온게 없어서ㅜ",
          "original_form": "공식 답 나온게 없어서ㅜ",
          "speaker_id": "2",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100038581.1.5",
          "form": "시즌제 갔으면 좋겠는데..",
          "original_form": "시즌제 갔으면 좋겠는데..",
          "speaker_id": "2",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100038581.1.6",
          "form": "탈락한 팀들 너무 아쉬워서",
          "original_form": "탈락한 팀들 너무 아쉬워서",
          "speaker_id": "2",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100038581.1.7",
          "form": "요즘인기봐서는 시즌제갈거같기도해요\r\n라스에서도 거기 나오시는분 나오셔서 그프로그램 언급하구 그러더라구요ㄱㅋㅋㅋㅋ",
          "original_form": "요즘인기봐서는 시즌제갈거같기도해요\r\n라스에서도 거기 나오시는분 나오셔서 그프로그램 언급하구 그러더라구요ㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100038581.1.8",
          "form": "아아 맞아요!!!",
          "original_form": "아아 맞아요!!!",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100038581.1.9",
          "form": "박선영씨!",
          "original_form": "박선영씨!",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100038581.1.10",
          "form": "완전 mvp예요",
          "original_form": "완전 mvp예요",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100038581.1.11",
          "form": "악ㅋㄲㅋ이름 들으니 저도바로알겠네요ㅋㅋㅋ",
          "original_form": "악ㅋㄲㅋ이름 들으니 저도바로알겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:09"
        },
        {
          "id": "MDRW2100038581.1.12",
          "form": "다른거 보시는건 없으세요??",
          "original_form": "다른거 보시는건 없으세요??",
          "speaker_id": "2",
          "time": "20210903 14:15"
        },
        {
          "id": "MDRW2100038581.1.13",
          "form": "관심있는 연예인이나",
          "original_form": "관심있는 연예인이나",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100038581.1.14",
          "form": "제가아이돌좋아한다는말은했었죠?!!! \r\n마구섞이니헷갈립니다ㅋㅋㅋㅋ",
          "original_form": "제가아이돌좋아한다는말은했었죠?!!! \r\n마구섞이니헷갈립니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100038581.1.15",
          "form": "가수덕질주로합니당ㅋㅋㅋ",
          "original_form": "가수덕질주로합니당ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100038581.1.16",
          "form": "집에틀어박혀서 티비보고싶어요ㄷ",
          "original_form": "집에틀어박혀서 티비보고싶어요ㄷ",
          "speaker_id": "1",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100038581.1.17",
          "form": "퇴근하고싶습니다",
          "original_form": "퇴근하고싶습니다",
          "speaker_id": "1",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100038581.1.18",
          "form": "두시부터 퇴근...!!ㅋㅋㅋㅋ",
          "original_form": "두시부터 퇴근...!!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100038581.1.19",
          "form": "저도 한때 덕질했었는데",
          "original_form": "저도 한때 덕질했었는데",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100038581.1.20",
          "form": "지금은 그 열정이 어디로 갔는지 모르겠어요ㅜㅜ",
          "original_form": "지금은 그 열정이 어디로 갔는지 모르겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100038581.1.21",
          "form": "ㅋㅋㅋㅋ저도예전같지않아요\r\n나이들어덕질하는게쉬운게아닌거같아오\r\n체력디ㅡ안따라줘요",
          "original_form": "ㅋㅋㅋㅋ저도예전같지않아요\r\n나이들어덕질하는게쉬운게아닌거같아오\r\n체력디ㅡ안따라줘요",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100038581.1.22",
          "form": "그리고ㅋㅋㅋㅋㅋㅋㅋ약간 홍대병이랑 비슷한데",
          "original_form": "그리고ㅋㅋㅋㅋㅋㅋㅋ약간 홍대병이랑 비슷한데",
          "speaker_id": "2",
          "time": "20210904 08:52"
        },
        {
          "id": "MDRW2100038581.1.23",
          "form": "좀 떠서 팬유입 많아지거나",
          "original_form": "좀 떠서 팬유입 많아지거나",
          "speaker_id": "2",
          "time": "20210904 08:52"
        },
        {
          "id": "MDRW2100038581.1.24",
          "form": "활동진짜 많이하면 마음이 차갑게..",
          "original_form": "활동진짜 많이하면 마음이 차갑게..",
          "speaker_id": "2",
          "time": "20210904 08:52"
        },
        {
          "id": "MDRW2100038581.1.25",
          "form": "식는거같아요 왜그런지모르지만ㅋㅋㅋㅋ",
          "original_form": "식는거같아요 왜그런지모르지만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 08:52"
        },
        {
          "id": "MDRW2100038581.1.26",
          "form": "그런맘들수있어여ㅋㄱㅋㅋ",
          "original_form": "그런맘들수있어여ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100038581.1.27",
          "form": "그리고 빵 떠서 알려졌응 좋겠는데",
          "original_form": "그리고 빵 떠서 알려졌응 좋겠는데",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100038581.1.28",
          "form": "그 반대의 마음도 들 때..",
          "original_form": "그 반대의 마음도 들 때..",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100038581.1.29",
          "form": "찾아보고 덕질하는 재미가 있어야하는데",
          "original_form": "찾아보고 덕질하는 재미가 있어야하는데",
          "speaker_id": "2",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100038581.1.30",
          "form": "내가 찾지 않아도 뭘 해도 소식이 보이고",
          "original_form": "내가 찾지 않아도 뭘 해도 소식이 보이고",
          "speaker_id": "2",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100038581.1.31",
          "form": "진짜 하루할당량을 넘어서게 덕질정보가 들어오다보니가",
          "original_form": "진짜 하루할당량을 넘어서게 덕질정보가 들어오다보니가",
          "speaker_id": "2",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100038581.1.32",
          "form": "좀 피로해지더라고요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "좀 피로해지더라고요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100038581.1.33",
          "form": "아마도 제 마음이 그렇게 좋아하진 않았겠죠",
          "original_form": "아마도 제 마음이 그렇게 좋아하진 않았겠죠",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100038581.1.34",
          "form": "아 그게 정답일까요 결론이 딱맞는거같아요\r\n공감 확 되네요ㅎㅅㅎ \r\n주말인데 티비만ㅅ이보구계신가용",
          "original_form": "아 그게 정답일까요 결론이 딱맞는거같아요\r\n공감 확 되네요ㅎㅅㅎ \r\n주말인데 티비만ㅅ이보구계신가용",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100038581.1.35",
          "form": "덕질얘기에 이어서,  주말인데 티비많이보고계신가요!",
          "original_form": "덕질얘기에 이어서,  주말인데 티비많이보고계신가요!",
          "speaker_id": "1",
          "time": "20210904 13:05"
        }
      ]
    }
