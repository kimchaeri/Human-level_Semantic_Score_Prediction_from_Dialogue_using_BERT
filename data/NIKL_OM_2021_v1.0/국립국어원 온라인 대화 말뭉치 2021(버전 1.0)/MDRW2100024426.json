      "utterance": [
        {
          "id": "MDRW2100024426.1.1",
          "form": "메타버스 앱 사용해보셨나요",
          "original_form": "메타버스 앱 사용해보셨나요",
          "speaker_id": "2",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100024426.1.2",
          "form": "아니요 해보셨나요?",
          "original_form": "아니요 해보셨나요?",
          "speaker_id": "1",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100024426.1.3",
          "form": "네이버 제페토 앱 써봤어요",
          "original_form": "네이버 제페토 앱 써봤어요",
          "speaker_id": "2",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100024426.1.4",
          "form": "아 어떤가요 재밌나요?",
          "original_form": "아 어떤가요 재밌나요?",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100024426.1.5",
          "form": "재미는없지만 해볼만해요",
          "original_form": "재미는없지만 해볼만해요",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024426.1.6",
          "form": "아바타 꾸미고 월드맵 다니구",
          "original_form": "아바타 꾸미고 월드맵 다니구",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024426.1.7",
          "form": "이게 성공할지 어떨지",
          "original_form": "이게 성공할지 어떨지",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100024426.1.8",
          "form": "전망 괜찮을까요?",
          "original_form": "전망 괜찮을까요?",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100024426.1.9",
          "form": "정부 공기업 사기업 모두 관심두고있어서",
          "original_form": "정부 공기업 사기업 모두 관심두고있어서",
          "speaker_id": "2",
          "time": "20210901 06:55"
        },
        {
          "id": "MDRW2100024426.1.10",
          "form": "괜찮을것같아요",
          "original_form": "괜찮을것같아요",
          "speaker_id": "2",
          "time": "20210901 06:55"
        },
        {
          "id": "MDRW2100024426.1.11",
          "form": "뉴스에 많이 나오더라구요",
          "original_form": "뉴스에 많이 나오더라구요",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100024426.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 14:19"
        },
        {
          "id": "MDRW2100024426.1.13",
          "form": "회사에서 메타버스 행사 이벤트를 했어서",
          "original_form": "회사에서 메타버스 행사 이벤트를 했어서",
          "speaker_id": "2",
          "time": "20210901 14:19"
        },
        {
          "id": "MDRW2100024426.1.14",
          "form": "참여해봤는데 조작법이 어렵더라구오",
          "original_form": "참여해봤는데 조작법이 어렵더라구오",
          "speaker_id": "2",
          "time": "20210901 14:19"
        },
        {
          "id": "MDRW2100024426.1.15",
          "form": "전 현실공간도 벅차서ㅠ",
          "original_form": "전 현실공간도 벅차서ㅠ",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100024426.1.16",
          "form": "맞아요 저도 ㅋㅋ",
          "original_form": "맞아요 저도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:22"
        },
        {
          "id": "MDRW2100024426.1.17",
          "form": "회사 행사아니면 해볼기회가 없었을것같아요",
          "original_form": "회사 행사아니면 해볼기회가 없었을것같아요",
          "speaker_id": "2",
          "time": "20210901 14:22"
        },
        {
          "id": "MDRW2100024426.1.18",
          "form": "근데 또 발전한다니 기웃거리게되요ㅋ",
          "original_form": "근데 또 발전한다니 기웃거리게되요ㅋ",
          "speaker_id": "1",
          "time": "20210901 15:53"
        }
      ]
