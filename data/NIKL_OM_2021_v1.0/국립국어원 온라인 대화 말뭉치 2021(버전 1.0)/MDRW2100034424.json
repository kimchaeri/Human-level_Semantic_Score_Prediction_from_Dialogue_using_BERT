      "utterance": [
        {
          "id": "MDRW2100034424.1.1",
          "form": "저는 도자캣",
          "original_form": "저는 도자캣",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100034424.1.2",
          "form": "키스미모어",
          "original_form": "키스미모어",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100034424.1.3",
          "form": "듣고있어용!",
          "original_form": "듣고있어용!",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100034424.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100034424.1.5",
          "form": "앗 저는 지금 중국노래듣고있어요~",
          "original_form": "앗 저는 지금 중국노래듣고있어요~",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100034424.1.6",
          "form": "노래 제목이 뭔가용?",
          "original_form": "노래 제목이 뭔가용?",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100034424.1.7",
          "form": "\"이유는 없어\" 라는 곡인데 유툽에 검색해보셔요 추천해요 노래넘좋아여~",
          "original_form": "\"이유는 없어\" 라는 곡인데 유툽에 검색해보셔요 추천해요 노래넘좋아여~",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100034424.1.8",
          "form": "오호 중국음악은 잘 안들어봤는데",
          "original_form": "오호 중국음악은 잘 안들어봤는데",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100034424.1.9",
          "form": "추천 감사해요!",
          "original_form": "추천 감사해요!",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100034424.1.10",
          "form": "히히 좋은건 공유해야죠~",
          "original_form": "히히 좋은건 공유해야죠~",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100034424.1.11",
          "form": "키스미모어 듣고왔는데 노래좋네요!",
          "original_form": "키스미모어 듣고왔는데 노래좋네요!",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100034424.1.12",
          "form": "취향에 맞으신다니 다행이에요ㅎㅎ",
          "original_form": "취향에 맞으신다니 다행이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100034424.1.13",
          "form": "ㅋㅋㅋ 아 유투브도 자주보시나요?",
          "original_form": "ㅋㅋㅋ 아 유투브도 자주보시나요?",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100034424.1.14",
          "form": "네네! 근데 주로 동물 동영상 많이 봐용",
          "original_form": "네네! 근데 주로 동물 동영상 많이 봐용",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100034424.1.15",
          "form": "아하!! 저는 주로 asmr 듣는거같아요 ㅎㅎ",
          "original_form": "아하!! 저는 주로 asmr 듣는거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100034424.1.16",
          "form": "주무시기 전에 보시나용?ㅎㅎ",
          "original_form": "주무시기 전에 보시나용?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100034424.1.17",
          "form": "넹  ㅋㅋㅋ 근데 약간 평정심이 필요할때도 틀어서 듣곤해요",
          "original_form": "넹  ㅋㅋㅋ 근데 약간 평정심이 필요할때도 틀어서 듣곤해요",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100034424.1.18",
          "form": "아하ㅠㅠ 저는 asmr 보면",
          "original_form": "아하ㅠㅠ 저는 asmr 보면",
          "speaker_id": "2",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100034424.1.19",
          "form": "바로 잠들어서",
          "original_form": "바로 잠들어서",
          "speaker_id": "2",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100034424.1.20",
          "form": "한번도 집중해서 본적이 없어요ㅋㅋ",
          "original_form": "한번도 집중해서 본적이 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100034424.1.21",
          "form": "앜ㅋㅋㅋㅋ 오 효과만땅인가봐여",
          "original_form": "앜ㅋㅋㅋㅋ 오 효과만땅인가봐여",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100034424.1.22",
          "form": "전고인물이라 30분넘게봐야 잠오더라구요 ㅠ",
          "original_form": "전고인물이라 30분넘게봐야 잠오더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100034424.1.23",
          "form": "30분이나 버티시다니 대단해요ㅋㅋㅋ",
          "original_form": "30분이나 버티시다니 대단해요ㅋㅋㅋ",
          "speaker_id": "2",
