      "utterance": [
        {
          "id": "MDRW2100011050.1.1",
          "form": "주변에 결혼한 친구 있니?",
          "original_form": "주변에 결혼한 친구 있니?",
          "speaker_id": "2",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011050.1.2",
          "form": "너 연애한 적 앴오?",
          "original_form": "너 연애한 적 앴오?",
          "speaker_id": "1",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011050.1.3",
          "form": "음 친구는 아니고 결혼한 선배는 있어",
          "original_form": "음 친구는 아니고 결혼한 선배는 있어",
          "speaker_id": "1",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011050.1.4",
          "form": "음 아니 없어!",
          "original_form": "음 아니 없어!",
          "speaker_id": "2",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011050.1.5",
          "form": "다들 정말 어른이 된 거 같더라.",
          "original_form": "다들 정말 어른이 된 거 같더라.",
          "speaker_id": "1",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011050.1.6",
          "form": "아 연애한 적 없구나!",
          "original_form": "아 연애한 적 없구나!",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011050.1.7",
          "form": "연애를 하지 않는 데 특별한 이유가 있내?",
          "original_form": "연애를 하지 않는 데 특별한 이유가 있내?",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011050.1.8",
          "form": "음 나는 혼자사는것이 편해",
          "original_form": "음 나는 혼자사는것이 편해",
          "speaker_id": "2",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011050.1.9",
          "form": "그리고 주변에 못생긴남자들만 가득해",
          "original_form": "그리고 주변에 못생긴남자들만 가득해",
          "speaker_id": "2",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011050.1.10",
          "form": "연애시잘에도 혼자 살 수 잇어",
          "original_form": "연애시잘에도 혼자 살 수 잇어",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011050.1.11",
          "form": "아아 넌 얼굴이 중요하구나",
          "original_form": "아아 넌 얼굴이 중요하구나",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011050.1.12",
          "form": "나는 주변에 결혼생각있는 사람들을 만났어",
          "original_form": "나는 주변에 결혼생각있는 사람들을 만났어",
          "speaker_id": "2",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011050.1.13",
          "form": "맞아. 얼굴은 연애에 있어 중요한 조건이지.",
          "original_form": "맞아. 얼굴은 연애에 있어 중요한 조건이지.",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011050.1.14",
          "form": "당연하지 얼굴 뜯어먹고 살아야해",
          "original_form": "당연하지 얼굴 뜯어먹고 살아야해",
          "speaker_id": "2",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011050.1.15",
          "form": "어머. 다음에 그 이야기 더 들려주렴.",
          "original_form": "어머. 다음에 그 이야기 더 들려주렴.",
          "speaker_id": "1",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011050.1.16",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "2",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011050.1.17",
          "form": "결혼이라니 어른이다.",
          "original_form": "결혼이라니 어른이다.",
          "speaker_id": "1",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011050.1.18",
          "form": "나도 이야기를 듣자말자 갑자기 어른처럼 느꺄쟜어",
          "original_form": "나도 이야기를 듣자말자 갑자기 어른처럼 느꺄쟜어",
          "speaker_id": "2",
          "time": "20210824 00:10"
        }
      ]
