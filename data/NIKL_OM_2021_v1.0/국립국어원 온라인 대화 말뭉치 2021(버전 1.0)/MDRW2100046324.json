        },
        {
          "id": "MDRW2100046324.1.2",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 09:11"
        },
        {
          "id": "MDRW2100046324.1.3",
          "form": "엄청 일찍 일어나셨네요!",
          "original_form": "엄청 일찍 일어나셨네요!",
          "speaker_id": "2",
          "time": "20211020 09:11"
        },
        {
          "id": "MDRW2100046324.1.4",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211021 00:26"
        },
        {
          "id": "MDRW2100046324.1.5",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20211020 13:41"
        },
        {
          "id": "MDRW2100046324.1.6",
          "form": "저는 닭볶음탕 먹어서 매우 행복하네요.....",
          "original_form": "저는 닭볶음탕 먹어서 매우 행복하네요.....",
          "speaker_id": "2",
          "time": "20211020 13:41"
        },
        {
          "id": "MDRW2100046324.1.7",
          "form": "저는 백반요",
          "original_form": "저는 백반요",
          "speaker_id": "1",
          "time": "20211020 14:08"
        },
        {
          "id": "MDRW2100046324.1.8",
          "form": "사실 백반이 최곤데",
          "original_form": "사실 백반이 최곤데",
          "speaker_id": "2",
          "time": "20211020 14:10"
        },
        {
          "id": "MDRW2100046324.1.9",
          "form": "저희 회사 근처에 아직 맛있는 백반집을 못찾아서",
          "original_form": "저희 회사 근처에 아직 맛있는 백반집을 못찾아서",
          "speaker_id": "2",
          "time": "20211020 14:10"
        },
        {
          "id": "MDRW2100046324.1.10",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 14:10"
        },
        {
          "id": "MDRW2100046324.1.11",
          "form": "슬푸네요",
          "original_form": "슬푸네요",
          "speaker_id": "2",
          "time": "20211020 14:10"
        },
        {
          "id": "MDRW2100046324.1.12",
          "form": "저런 ㅋㅋ",
          "original_form": "저런 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 14:10"
        },
        {
          "id": "MDRW2100046324.1.13",
          "form": "백반은 메인메뉴없이",
          "original_form": "백반은 메인메뉴없이",
          "speaker_id": "2",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.14",
          "form": "반찬만 나오나용?",
          "original_form": "반찬만 나오나용?",
          "speaker_id": "2",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.15",
          "form": "안타깝네요",
          "original_form": "안타깝네요",
          "speaker_id": "1",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.16",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.17",
          "form": "허허 그래도 다른 맛있는 음식점들이 많아서 위안이 되네요 ㅋㅋㅋ",
          "original_form": "허허 그래도 다른 맛있는 음식점들이 많아서 위안이 되네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.18",
          "form": "제육이나 된장",
          "original_form": "제육이나 된장",
          "speaker_id": "1",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.19",
          "form": "제육이랑 된장이면 충분하죠",
          "original_form": "제육이랑 된장이면 충분하죠",
          "speaker_id": "2",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.20",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.21",
          "form": "그죠 ㅋ",
          "original_form": "그죠 ㅋ",
          "speaker_id": "1",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.22",
          "form": "한식은 정말 최고인 것 같아요",
          "original_form": "한식은 정말 최고인 것 같아요",
          "speaker_id": "2",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.23",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.24",
          "form": "맨날 먹어도 안질려...",
          "original_form": "맨날 먹어도 안질려...",
          "speaker_id": "2",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211020 14:11"
        },
        {
          "id": "MDRW2100046324.1.26",
          "form": "ㅎㅎ 전 음식때문에 다른 나라에서 못살듯요\r\n",
          "original_form": "ㅎㅎ 전 음식때문에 다른 나라에서 못살듯요\r\n",
