      "utterance": [
        {
          "id": "MDRW2100011218.1.1",
          "form": "백신은 맞으셨나요",
          "original_form": "백신은 맞으셨나요",
          "speaker_id": "1",
          "time": "20210824 08:14"
        },
        {
          "id": "MDRW2100011218.1.2",
          "form": "네 맞았어요",
          "original_form": "네 맞았어요",
          "speaker_id": "2",
          "time": "20210824 08:15"
        },
        {
          "id": "MDRW2100011218.1.3",
          "form": "얀센",
          "original_form": "얀센",
          "speaker_id": "2",
          "time": "20210824 08:15"
        },
        {
          "id": "MDRW2100011218.1.4",
          "form": "맞으셨어요?",
          "original_form": "맞으셨어요?",
          "speaker_id": "2",
          "time": "20210824 08:15"
        },
        {
          "id": "MDRW2100011218.1.5",
          "form": "저는 모더나 1차만 맞았어요",
          "original_form": "저는 모더나 1차만 맞았어요",
          "speaker_id": "1",
          "time": "20210824 08:21"
        },
        {
          "id": "MDRW2100011218.1.6",
          "form": "언제 맞으셨어요",
          "original_form": "언제 맞으셨어요",
          "speaker_id": "1",
          "time": "20210824 08:21"
        },
        {
          "id": "MDRW2100011218.1.7",
          "form": "저는 얀센 6월인가",
          "original_form": "저는 얀센 6월인가",
          "speaker_id": "2",
          "time": "20210824 08:27"
        },
        {
          "id": "MDRW2100011218.1.8",
          "form": "그때쯤 맞은것같아요",
          "original_form": "그때쯤 맞은것같아요",
          "speaker_id": "2",
          "time": "20210824 08:27"
        },
        {
          "id": "MDRW2100011218.1.9",
          "form": "7월인가 헷갈리네",
          "original_form": "7월인가 헷갈리네",
          "speaker_id": "2",
          "time": "20210824 08:27"
        },
        {
          "id": "MDRW2100011218.1.10",
          "form": "부작용 없으셨나요 ㅎㅎ",
          "original_form": "부작용 없으셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 08:42"
        },
        {
          "id": "MDRW2100011218.1.11",
          "form": "미열있고 나른한정돚였어요",
          "original_form": "미열있고 나른한정돚였어요",
          "speaker_id": "2",
          "time": "20210824 08:45"
        },
        {
          "id": "MDRW2100011218.1.12",
          "form": "그래도 얀센은 1회 접종이라 좋아요",
          "original_form": "그래도 얀센은 1회 접종이라 좋아요",
          "speaker_id": "2",
          "time": "20210824 08:45"
        },
        {
          "id": "MDRW2100011218.1.13",
          "form": "전 아스피린 22알 먹었다는 사람 봤었는데",
          "original_form": "전 아스피린 22알 먹었다는 사람 봤었는데",
          "speaker_id": "1",
          "time": "20210824 08:47"
        },
        {
          "id": "MDRW2100011218.1.14",
          "form": "다행이시네요",
          "original_form": "다행이시네요",
          "speaker_id": "1",
          "time": "20210824 08:47"
        },
        {
          "id": "MDRW2100011218.1.15",
          "form": "22알이면 괜찮은건가 ㄷㄷ",
          "original_form": "22알이면 괜찮은건가 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210824 08:48"
        },
        {
          "id": "MDRW2100011218.1.16",
          "form": "2차접종은 언제세요?",
          "original_form": "2차접종은 언제세요?",
          "speaker_id": "2",
          "time": "20210824 08:48"
        },
        {
          "id": "MDRW2100011218.1.17",
          "form": "저는 9월6일이요",
          "original_form": "저는 9월6일이요",
          "speaker_id": "1",
          "time": "20210824 08:54"
        },
        {
          "id": "MDRW2100011218.1.18",
          "form": "으 아니 월욜이라서ㅜ",
          "original_form": "으 아니 월욜이라서ㅜ",
          "speaker_id": "1",
          "time": "20210824 08:54"
        },
        {
          "id": "MDRW2100011218.1.19",
          "form": "아직 좀 남았네요",
          "original_form": "아직 좀 남았네요",
          "speaker_id": "2",
          "time": "20210824 08:59"
        },
        {
          "id": "MDRW2100011218.1.20",
          "form": "별탈없이 지나가기를 바랍니다",
          "original_form": "별탈없이 지나가기를 바랍니다",
          "speaker_id": "2",
          "time": "20210824 09:00"
        },
        {
          "id": "MDRW2100011218.1.21",
          "form": "그때 얀센 따로 신청하신건가요",
          "original_form": "그때 얀센 따로 신청하신건가요",
          "speaker_id": "1",
          "time": "20210824 09:10"
        },
        {
          "id": "MDRW2100011218.1.22",
          "form": "네 예비역이랑 민방위만 신청 가능하다고해서",
          "original_form": "네 예비역이랑 민방위만 신청 가능하다고해서",
          "speaker_id": "2",
          "time": "20210824 09:11"
        },
        {
          "id": "MDRW2100011218.1.23",
          "form": "신청하고 접종했죠",
          "original_form": "신청하고 접종했죠",
          "speaker_id": "2",
          "time": "20210824 09:11"
        },
        {
          "id": "MDRW2100011218.1.24",
          "form": "저는 잔여백신 50대 60대꺼 맞은거라 제 나이대보다는 조금 빨리 맞아요 ㅎ",
          "original_form": "저는 잔여백신 50대 60대꺼 맞은거라 제 나이대보다는 조금 빨리 맞아요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 09:13"
        }
      ]
