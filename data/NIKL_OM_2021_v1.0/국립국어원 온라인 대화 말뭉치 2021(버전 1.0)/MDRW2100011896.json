      "utterance": [
        {
          "id": "MDRW2100011896.1.1",
          "form": "안녕하세요~ 영화관 가는거 좋아하시나요?",
          "original_form": "안녕하세요~ 영화관 가는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011896.1.2",
          "form": "아니요 님은요?",
          "original_form": "아니요 님은요?",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011896.1.3",
          "form": "근데 허광한 영화는 보고싶어요",
          "original_form": "근데 허광한 영화는 보고싶어요",
          "speaker_id": "2",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100011896.1.4",
          "form": "전 좋아해요! 허광한이 영화제목인가요?",
          "original_form": "전 좋아해요! 허광한이 영화제목인가요?",
          "speaker_id": "1",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100011896.1.5",
          "form": "대만배우예요",
          "original_form": "대만배우예요",
          "speaker_id": "2",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100011896.1.6",
          "form": "우와 대만배우는 어떻게 알게되셨나요?",
          "original_form": "우와 대만배우는 어떻게 알게되셨나요?",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011896.1.7",
          "form": "상견니라는 드라마때문에요",
          "original_form": "상견니라는 드라마때문에요",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011896.1.8",
          "form": "상견니 영화관에서 상영한다고하네요",
          "original_form": "상견니 영화관에서 상영한다고하네요",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011896.1.9",
          "form": "그렇구나 한번 찾아봐야겠어요",
          "original_form": "그렇구나 한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011896.1.10",
          "form": "어떤 장르죠?",
          "original_form": "어떤 장르죠?",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011896.1.11",
          "form": "대만영화 말할수없는 비밀이랑 비슷한 느낌 청춘 로맨스 타임슬립",
          "original_form": "대만영화 말할수없는 비밀이랑 비슷한 느낌 청춘 로맨스 타임슬립",
          "speaker_id": "2",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011896.1.12",
          "form": "아 저도 좋아하는 장르네요\r\n타임슬립물 많이 봤는데 재밌어요",
          "original_form": "아 저도 좋아하는 장르네요\r\n타임슬립물 많이 봤는데 재밌어요",
          "speaker_id": "1",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011896.1.13",
          "form": "상견니 좋아하는 여자분들이 많아요",
          "original_form": "상견니 좋아하는 여자분들이 많아요",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011896.1.14",
          "form": "혹시 주인공이 잘생겼나요?",
          "original_form": "혹시 주인공이 잘생겼나요?",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011896.1.15",
          "form": "네 잘생겼어요~제 스타일입니다^^",
          "original_form": "네 잘생겼어요~제 스타일입니다^^",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011896.1.16",
          "form": "앜ㅋㅋ 그렇구나 찾아보니까 시원 청량하게 생기셨네요",
          "original_form": "앜ㅋㅋ 그렇구나 찾아보니까 시원 청량하게 생기셨네요",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011896.1.17",
          "form": "웃는게 예뻐요~",
