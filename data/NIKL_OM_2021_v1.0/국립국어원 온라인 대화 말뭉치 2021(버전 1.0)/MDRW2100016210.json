      "utterance": [
        {
          "id": "MDRW2100016210.1.1",
          "form": "좋아하는 책 있으세요?",
          "original_form": "좋아하는 책 있으세요?",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016210.1.2",
          "form": "책 많이 읽으세요?",
          "original_form": "책 많이 읽으세요?",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016210.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016210.1.4",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016210.1.5",
          "form": "저는 음 지금 생각나는 책은 딱히 없는데",
          "original_form": "저는 음 지금 생각나는 책은 딱히 없는데",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016210.1.6",
          "form": "와 정말 없네요",
          "original_form": "와 정말 없네요",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016210.1.7",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016210.1.8",
          "form": "죽은시인의 사회 재미있게 읽었어요",
          "original_form": "죽은시인의 사회 재미있게 읽었어요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.9",
          "form": "웃지.마세요",
          "original_form": "웃지.마세요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.10",
          "form": "아네",
          "original_form": "아네",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.11",
          "form": "왜요 재밌어서 웃었는데요",
          "original_form": "왜요 재밌어서 웃었는데요",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.12",
          "form": "책 많이 읽으신다면 추천 좀 해주세요",
          "original_form": "책 많이 읽으신다면 추천 좀 해주세요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.13",
          "form": "한강 시집이요",
          "original_form": "한강 시집이요",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.14",
          "form": "우리가 빛의속도로 갈 수 없다면 도 강추입니다",
          "original_form": "우리가 빛의속도로 갈 수 없다면 도 강추입니다",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.15",
          "form": "시인 이름이 한강이에요?",
          "original_form": "시인 이름이 한강이에요?",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.17",
          "form": "오 후자는 들어본거 같아요",
          "original_form": "오 후자는 들어본거 같아요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.18",
          "form": "설마 한강 작가 모르세요?",
          "original_form": "설마 한강 작가 모르세요?",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.19",
          "form": "네 몰라요",
          "original_form": "네 몰라요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.20",
          "form": "레알?",
          "original_form": "레알?",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.21",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.23",
          "form": "어메이징하세요",
          "original_form": "어메이징하세요",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.24",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.25",
          "form": "관심이 없어서요",
          "original_form": "관심이 없어서요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016210.1.26",
          "form": "..",
          "original_form": "..",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.27",
          "form": "한강 유명한 상받아서 유명해요",
          "original_form": "한강 유명한 상받아서 유명해요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.28",
          "form": "제가 아는 시인이 아마 손에 꼽을걸요",
          "original_form": "제가 아는 시인이 아마 손에 꼽을걸요",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.29",
          "form": "작품들도 다 유명하고요ㅛ",
          "original_form": "작품들도 다 유명하고요ㅛ",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.30",
          "form": "올해는 책 좀 읽어야겠네요",
          "original_form": "올해는 책 좀 읽어야겠네요",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.31",
          "form": "원랴 작가예요",
          "original_form": "원랴 작가예요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.32",
          "form": "유명한 작품 하나만 알려주세요",
          "original_form": "유명한 작품 하나만 알려주세요",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.33",
          "form": "그러세요 룸메분 본받으시거요",
          "original_form": "그러세요 룸메분 본받으시거요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.34",
          "form": "소년이 온다랑 채식주의자요",
          "original_form": "소년이 온다랑 채식주의자요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.35",
          "form": "둘 다 아직 안 읽어봤지만",
          "original_form": "둘 다 아직 안 읽어봤지만",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.36",
          "form": "좋은 평이 많아요",
          "original_form": "좋은 평이 많아요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.37",
          "form": "스토킹하세요 ? 제가 룸메 있는거 어케아셨죠",
          "original_form": "스토킹하세요 ? 제가 룸메 있는거 어케아셨죠",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016210.1.38",
          "form": "채식주의자가 한강 작가 꺼 였나요?",
          "original_form": "채식주의자가 한강 작가 꺼 였나요?",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016210.1.39",
          "form": "아까 얘기하셨잖아요",
          "original_form": "아까 얘기하셨잖아요",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016210.1.40",
          "form": "채식주의자는 들어봤어요",
          "original_form": "채식주의자는 들어봤어요",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016210.1.41",
          "form": "훌륭한 친구분이 룸메시라고",
          "original_form": "훌륭한 친구분이 룸메시라고",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016210.1.42",
          "form": "그렇겠죠 제일 유명하니가요",
          "original_form": "그렇겠죠 제일 유명하니가요",
          "speaker_id": "1",
