      "utterance": [
        {
          "id": "MDRW2100032917.1.1",
          "form": "오늘 바람이 많이부네요",
          "original_form": "오늘 바람이 많이부네요",
          "speaker_id": "2",
          "time": "20210906 12:58"
        },
        {
          "id": "MDRW2100032917.1.2",
          "form": "날씨 어떤가요",
          "original_form": "날씨 어떤가요",
          "speaker_id": "2",
          "time": "20210906 12:58"
        },
        {
          "id": "MDRW2100032917.1.3",
          "form": "여기도 바람 불고 있어요",
          "original_form": "여기도 바람 불고 있어요",
          "speaker_id": "1",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100032917.1.4",
          "form": "어제까지 화창하고 시원한 바람이었는데",
          "original_form": "어제까지 화창하고 시원한 바람이었는데",
          "speaker_id": "1",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100032917.1.5",
          "form": "오늘은 흐리고 추운 바람입니다",
          "original_form": "오늘은 흐리고 추운 바람입니다",
          "speaker_id": "1",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100032917.1.6",
          "form": "비온다더니 정말 곧 올거같네요",
          "original_form": "비온다더니 정말 곧 올거같네요",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100032917.1.7",
          "form": "네 비는 조금만 왔으면 좋겠는데",
          "original_form": "네 비는 조금만 왔으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100032917.1.8",
          "form": "바람보니 태풍지나 가나 싶기도 합니다",
          "original_form": "바람보니 태풍지나 가나 싶기도 합니다",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100032917.1.9",
          "form": "바람이 태풍급이긴 해요ㅋㅋ",
          "original_form": "바람이 태풍급이긴 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100032917.1.10",
          "form": "그러게 말입니다",
          "original_form": "그러게 말입니다",
          "speaker_id": "1",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100032917.1.11",
          "form": "비도 이제 한두방울씩 떨어집니다",
          "original_form": "비도 이제 한두방울씩 떨어집니다",
          "speaker_id": "1",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100032917.1.12",
          "form": "내일까지 온다는 예보가 있네요",
          "original_form": "내일까지 온다는 예보가 있네요",
          "speaker_id": "1",
          "time": "20210906 17:14"
        },
        {
          "id": "MDRW2100032917.1.13",
          "form": "비오네요",
          "original_form": "비오네요",
          "speaker_id": "2",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100032917.1.14",
          "form": "그치고나면 좀 서늘해지겠어요",
          "original_form": "그치고나면 좀 서늘해지겠어요",
          "speaker_id": "2",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100032917.1.15",
          "form": "비가 올때마다 조금씩 서늘해지면서 겨울로 다가가겠지요",
          "original_form": "비가 올때마다 조금씩 서늘해지면서 겨울로 다가가겠지요",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100032917.1.16",
          "form": "이제는 점점 옷을 따뜻하게 잘 챙겨입어야 하는 계절이 되고 있네요",
          "original_form": "이제는 점점 옷을 따뜻하게 잘 챙겨입어야 하는 계절이 되고 있네요",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100032917.1.17",
          "form": "가을 옷 후딱 꺼내입어야겠어요",
          "original_form": "가을 옷 후딱 꺼내입어야겠어요",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100032917.1.18",
          "form": "봄 가을이 자꾸 짧아져서",
          "original_form": "봄 가을이 자꾸 짧아져서",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100032917.1.19",
          "form": "옷정리를 잘 해야합니다",
          "original_form": "옷정리를 잘 해야합니다",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100032917.1.20",
          "form": "환절기에 감기 걸리기 쉬운데",
          "original_form": "환절기에 감기 걸리기 쉬운데",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100032917.1.21",
          "form": "요즘은 감기 걸리면 눈치가 보이니까요",
          "original_form": "요즘은 감기 걸리면 눈치가 보이니까요",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100032917.1.22",
          "form": "감기환자가 줄긴했다네요 마스크쓰고나선",
          "original_form": "감기환자가 줄긴했다네요 마스크쓰고나선",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100032917.1.23",
          "form": "저도 작년부터 감기 걸리지 않고 있습니다",
          "original_form": "저도 작년부터 감기 걸리지 않고 있습니다",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100032917.1.24",
          "form": "아무래도 마스크와 손세정제 덕이 크겠지요",
          "original_form": "아무래도 마스크와 손세정제 덕이 크겠지요",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100032917.1.25",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100032917.1.26",
          "form": "위생의 중요성..!",
          "original_form": "위생의 중요성..!",
          "speaker_id": "2",
          "time": "20210906 18:02"
        }
      ]
    }
