      "utterance": [
        {
          "id": "MDRW2100020866.1.1",
          "form": "많이 틀렸어?",
          "original_form": "많이 틀렸어?",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020866.1.2",
          "form": "ㅇㅇ 처음이라 그런지 좀 ㅎㅎ",
          "original_form": "ㅇㅇ 처음이라 그런지 좀 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020866.1.3",
          "form": "어려운 문제도 많이 풀어보고 익숙해져야 하니깐",
          "original_form": "어려운 문제도 많이 풀어보고 익숙해져야 하니깐",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020866.1.4",
          "form": "ㅇㅇ 매번 쉬운 문제만 풀고 다 맞다가 멘붕온듯",
          "original_form": "ㅇㅇ 매번 쉬운 문제만 풀고 다 맞다가 멘붕온듯",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020866.1.5",
          "form": "ㅎㅎ 중고등학교 생각하면 응용 문제는 필수지",
          "original_form": "ㅎㅎ 중고등학교 생각하면 응용 문제는 필수지",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020866.1.6",
          "form": "머리 썼더니 머리가 지끈지끈하대 ㅋ",
          "original_form": "머리 썼더니 머리가 지끈지끈하대 ㅋ",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020866.1.7",
          "form": "평소에 얼마나 안썼으면",
          "original_form": "평소에 얼마나 안썼으면",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020866.1.8",
          "form": "만점왕 응용도 어려워하는데 쎈수학 풀면 ㅋㅋ",
          "original_form": "만점왕 응용도 어려워하는데 쎈수학 풀면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020866.1.9",
          "form": "머리 지끈 정도가 아니라 뱅글뱅글 돌지",
          "original_form": "머리 지끈 정도가 아니라 뱅글뱅글 돌지",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020866.1.10",
          "form": "이 위에 응용은 어느 정도 수준인지 한 번 보고 싶네 ㅎㅎ",
          "original_form": "이 위에 응용은 어느 정도 수준인지 한 번 보고 싶네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020866.1.11",
          "form": "지난 학기는 응용으로 한 번 풀어봐도 괜찮을듯",
          "original_form": "지난 학기는 응용으로 한 번 풀어봐도 괜찮을듯",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020866.1.12",
          "form": "문제집만 너무 많으면 애가 질릴까봐 ㅋ",
          "original_form": "문제집만 너무 많으면 애가 질릴까봐 ㅋ",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020866.1.13",
          "form": "암튼 자꾸 부딪혀 봐야해 그래야 실력도 늘어",
          "original_form": "암튼 자꾸 부딪혀 봐야해 그래야 실력도 늘어",
          "speaker_id": "2",
          "time": "20210828 13:42"
        }
