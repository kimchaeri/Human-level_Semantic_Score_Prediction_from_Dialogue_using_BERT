      "utterance": [
        {
          "id": "MDRW2100041865.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100041865.1.2",
          "form": "많이 시원해졌네요",
          "original_form": "많이 시원해졌네요",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100041865.1.3",
          "form": "네 엄청요",
          "original_form": "네 엄청요",
          "speaker_id": "1",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100041865.1.4",
          "form": "거기도 비오나요?",
          "original_form": "거기도 비오나요?",
          "speaker_id": "1",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100041865.1.5",
          "form": "네 조금씩 오네요",
          "original_form": "네 조금씩 오네요",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041865.1.6",
          "form": "비많이와요?",
          "original_form": "비많이와요?",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041865.1.7",
          "form": "네 여기는 꽤요",
          "original_form": "네 여기는 꽤요",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041865.1.8",
          "form": "그래도 지금은 그나마 잠잠해졌어요",
          "original_form": "그래도 지금은 그나마 잠잠해졌어요",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041865.1.9",
          "form": "추석 다되어가니 많이 시원해졌네요",
          "original_form": "추석 다되어가니 많이 시원해졌네요",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041865.1.10",
          "form": "이러다 겨울 온다 하겠어요",
          "original_form": "이러다 겨울 온다 하겠어요",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041865.1.11",
          "form": "증말요 !!!!!",
          "original_form": "증말요 !!!!!",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041865.1.12",
          "form": "초가을 느낌 낭낭합니다",
          "original_form": "초가을 느낌 낭낭합니다",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041865.1.13",
          "form": "단풍구경 가고 싶어요",
          "original_form": "단풍구경 가고 싶어요",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041865.1.14",
          "form": "아 저두요",
          "original_form": "아 저두요",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041865.1.15",
          "form": "곧 가을이면",
          "original_form": "곧 가을이면",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041865.1.16",
          "form": "금방 피겠죠",
          "original_form": "금방 피겠죠",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041865.1.17",
          "form": "가을단풍이 예쁘던데",
          "original_form": "가을단풍이 예쁘던데",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041865.1.18",
          "form": "못가본지 엄청 오래되었어요",
          "original_form": "못가본지 엄청 오래되었어요",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041865.1.19",
          "form": "짱 예브죠!!!!",
          "original_form": "짱 예브죠!!!!",
          "speaker_id": "1",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041865.1.20",
          "form": "은행은 싫지만...",
          "original_form": "은행은 싫지만...",
          "speaker_id": "1",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041865.1.21",
          "form": "네 은행은 냄새가 ㅜㅜㅜ",
          "original_form": "네 은행은 냄새가 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041865.1.22",
          "form": "냄새많이나요",
          "original_form": "냄새많이나요",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041865.1.23",
          "form": "ㅠ 은행 정말 싫어요",
