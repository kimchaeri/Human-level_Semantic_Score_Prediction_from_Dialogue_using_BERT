      "utterance": [
        {
          "id": "MDRW2100015095.1.1",
          "form": "반갑습니당",
          "original_form": "반갑습니당",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015095.1.2",
          "form": "똑똑똑",
          "original_form": "똑똑똑",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015095.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015095.1.4",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015095.1.5",
          "form": "네에!!",
          "original_form": "네에!!",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015095.1.6",
          "form": "맞춰보세요!",
          "original_form": "맞춰보세요!",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015095.1.7",
          "form": "히히ㅣ",
          "original_form": "히히ㅣ",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015095.1.8",
          "form": "떡볶이?",
          "original_form": "떡볶이?",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015095.1.9",
          "form": "헐.",
          "original_form": "헐.",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015095.1.10",
          "form": "천재",
          "original_form": "천재",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015095.1.11",
          "form": "어떻게 아셨어요?",
          "original_form": "어떻게 아셨어요?",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015095.1.12",
          "form": "ㅋㅋㅋ떡볶이 좋아하는 사람들 많이 만나서요",
          "original_form": "ㅋㅋㅋ떡볶이 좋아하는 사람들 많이 만나서요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015095.1.13",
          "form": "저도 좋아하고요",
          "original_form": "저도 좋아하고요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015095.1.14",
          "form": "으헉",
          "original_form": "으헉",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015095.1.15",
          "form": "짜장면 좋아하시죠?",
          "original_form": "짜장면 좋아하시죠?",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015095.1.16",
          "form": "정답!!",
          "original_form": "정답!!",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015095.1.17",
          "form": "흠 싫어하진 않아요ㅋㅋ",
          "original_form": "흠 싫어하진 않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015095.1.18",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015095.1.19",
          "form": "군만두",
          "original_form": "군만두",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015095.1.20",
          "form": "짬뽕",
          "original_form": "짬뽕",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015095.1.21",
          "form": "짜장면",
          "original_form": "짜장면",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015095.1.22",
          "form": "배고프네요",
          "original_form": "배고프네요",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015095.1.23",
          "form": "ㅋㅋㅋ오늘 뭐 드셨어요?",
          "original_form": "ㅋㅋㅋ오늘 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015095.1.24",
          "form": "저.. 라면 햄버거요",
          "original_form": "저.. 라면 햄버거요",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015095.1.25",
          "form": "저녁에...",
          "original_form": "저녁에...",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015095.1.26",
          "form": "와인이랑 돼지고기",
          "original_form": "와인이랑 돼지고기",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015095.1.27",
          "form": "먹을겁니다",
          "original_form": "먹을겁니다",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015095.1.28",
          "form": "오 그 조합으로 드시는 분은 처음 봤어요",
          "original_form": "오 그 조합으로 드시는 분은 처음 봤어요",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015095.1.29",
          "form": "어떤 와인 좋아하세요?",
          "original_form": "어떤 와인 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015095.1.30",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015095.1.31",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015095.1.32",
          "form": "쉬라즈요",
          "original_form": "쉬라즈요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015095.1.33",
          "form": "포도품종 쉬라즈",
          "original_form": "포도품종 쉬라즈",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015095.1.34",
          "form": "왕좋아해요",
          "original_form": "왕좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015095.1.35",
          "form": "제가 종류는 잘 모르겠네요ㅋㅋ",
          "original_form": "제가 종류는 잘 모르겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015095.1.36",
          "form": "술 잘 드시나요?",
          "original_form": "술 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015095.1.37",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015095.1.38",
          "form": "한잔이 딱",
          "original_form": "한잔이 딱",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015095.1.39",
          "form": "좋습니다",
          "original_form": "좋습니다",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015095.1.40",
          "form": "ㅋㅋㅋㅋ전 한잔이라도 제대로 마시고 싶어요ㅠ",
          "original_form": "ㅋㅋㅋㅋ전 한잔이라도 제대로 마시고 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015095.1.41",
          "form": "너무 못마셔서....",
          "original_form": "너무 못마셔서....",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015095.1.42",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015095.1.43",
          "form": "주량이 어떻게 되세요?",
          "original_form": "주량이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015095.1.44",
          "form": "전 대충 한모금이요",
          "original_form": "전 대충 한모금이요",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015095.1.45",
          "form": "그정도면 다 취하는 것 같아요ㅋㅋ",
          "original_form": "그정도면 다 취하는 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015095.1.46",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015095.1.47",
          "form": "그럼 이슬톡톡은요?",
          "original_form": "그럼 이슬톡톡은요?",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015095.1.48",
          "form": "그건 옛날에 한번 먹어봤는데 취하기 보단 제가 탄산을 못 마셔서요",
          "original_form": "그건 옛날에 한번 먹어봤는데 취하기 보단 제가 탄산을 못 마셔서요",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015095.1.49",
          "form": "아아..",
          "original_form": "아아..",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015095.1.50",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015095.1.51",
          "form": "논알코올 와인도 있습니다",
          "original_form": "논알코올 와인도 있습니다",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015095.1.52",
          "form": "네... 근데 보통 도수가 너무 세던데요ㅠ",
          "original_form": "네... 근데 보통 도수가 너무 세던데요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015095.1.53",
          "form": "걍 먹지 말아야죠 다음 생에나 즐기려고요",
          "original_form": "걍 먹지 말아야죠 다음 생에나 즐기려고요",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015095.1.54",
          "form": "도수낮은 것도 있어요",
          "original_form": "도수낮은 것도 있어요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015095.1.55",
          "form": "다음생애요?",
          "original_form": "다음생애요?",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015095.1.56",
          "form": "마음아파요",
          "original_form": "마음아파요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015095.1.57",
          "form": "ㅋㅋㅋㅋㅋ 그쵸....",
          "original_form": "ㅋㅋㅋㅋㅋ 그쵸....",
          "speaker_id": "2",
          "time": "20210825 22:22"
        }
      ]
