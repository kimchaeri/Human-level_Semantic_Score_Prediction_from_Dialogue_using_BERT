      "utterance": [
        {
          "id": "MDRW2100022607.1.1",
          "form": "저녁메뉴정하셨어요?",
          "original_form": "저녁메뉴정하셨어요?",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022607.1.2",
          "form": "아직 고민중인데 김치볶음밥 먹을까 생각중이에요",
          "original_form": "아직 고민중인데 김치볶음밥 먹을까 생각중이에요",
          "speaker_id": "1",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022607.1.3",
          "form": "저녁메뉴 정하셨나요?",
          "original_form": "저녁메뉴 정하셨나요?",
          "speaker_id": "1",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100022607.1.4",
          "form": "아녀ㅠ 정한건아닌데 집에잇능걸로 해먹어야할듯해여ㅠ",
          "original_form": "아녀ㅠ 정한건아닌데 집에잇능걸로 해먹어야할듯해여ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022607.1.5",
          "form": "직접 요리하세요?",
          "original_form": "직접 요리하세요?",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022607.1.6",
          "form": "요리라긴뭔가 거창한거같고 ㅋㅋ 그냥 국찌개정도ㅠ",
          "original_form": "요리라긴뭔가 거창한거같고 ㅋㅋ 그냥 국찌개정도ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022607.1.7",
          "form": "국, 찌개 하시는거면 잘하시는거죠!",
          "original_form": "국, 찌개 하시는거면 잘하시는거죠!",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022607.1.8",
          "form": "저는 국 만드는게 젤 어렵더라구요ㅠㅠ",
          "original_form": "저는 국 만드는게 젤 어렵더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022607.1.9",
          "form": "맛은 보장못합니다 ㅋㅋㅋ",
          "original_form": "맛은 보장못합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022607.1.10",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022607.1.11",
          "form": "전연두살짝 마무리 ㅋㅋ!",
          "original_form": "전연두살짝 마무리 ㅋㅋ!",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022607.1.12",
          "form": "오 꼭 필요하죠ㅎㅎ",
          "original_form": "오 꼭 필요하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022607.1.13",
          "form": "간만 대충 맞으면될꺼같아요ㅠㅠ",
          "original_form": "간만 대충 맞으면될꺼같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022607.1.14",
          "form": "찌개 어떤거 자주 하세요?",
          "original_form": "찌개 어떤거 자주 하세요?",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022607.1.15",
          "form": "차돌된장 !",
          "original_form": "차돌된장 !",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022607.1.16",
          "form": "이건 기본들어가는재료가잇으니 ㅋㅋ차돌에힘으로 그냥끓여도 괜찮더라구요!!",
          "original_form": "이건 기본들어가는재료가잇으니 ㅋㅋ차돌에힘으로 그냥끓여도 괜찮더라구요!!",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022607.1.17",
          "form": "국을 자주해요~ 콩나물귝 어묵국 김치국 김치어묵국 김치콩나물국 ㅋㅋㅋㅋ 이런식으로",
